<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: turne.xml,v 1.16 2022/08/30 06:23:24 revo Exp $">
<kap>
  <rad>turne</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="turne.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="vojagx.0i">Voja&gcirc;i</ref> a&ubreve;
      rondvoja&gcirc;i de loko al loko prezentante spekta&jcirc;ojn, arta&jcirc;ojn:
      <ekz>
        malgranda teatra trupo <tld/>as, ludante &ccirc;iutage la saman opereton en alia urbo
        <fnt>
          <bib>KrB</bib>
          <lok>Muzika terminaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu el la Moskvaj teatroj <tld/>anta en Leningrado
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 27a, p.
        289a</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>is kun sia nacia teamo en malproksimaj urboj de nia lando
        <fnt>
          <bib>Monato</bib>
          <aut>Lena Karpunina</aut>
          <vrk><url ref="https://www.monato.be/2006/008836.php"
              >La revo pri cirko</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gast.0i.evento">gasti<sncref/></ref>,
      <ref cel="vojagx.rond0i">rondvoja&gcirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_t;&c_u;&c_r;&c_n;&c_e;</ind></trd>
  <trd lng="ca">fer una <ind>gira</ind></trd>
  <trd lng="cs">hr&aacute;t na turn&eacute;</trd>
  <trd lng="de">auf Tournee gehen</trd>
  <trd lng="en">tour<klr tip="amb"> (theatrical)</klr></trd>
  <trd lng="fr">faire une <ind>tourn&eacute;e</ind><klr>
  (spectacles)</klr></trd>
  <trd lng="hu">turn&eacute;zik</trd>
  <trdgrp lng="nl">
     <trd>een <ind>tournee</ind> maken</trd>,
     <trd>op <ind>tournee</ind> gaan</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>by&cacute; w <ind>turnee</ind></trd>,
    <trd>mie&cacute; <ind>turnee</ind></trd>,
    <trd>odbywa&cacute; <ind>turnee</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>fazer uma turn&ecirc;</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol;
  <ind>&c_t;&c_u;&c_r;&c_n;&c_je;</ind></trd>
  <trd lng="sk">hra&tcaron; na turn&eacute;</trd>
</drv>

<drv mrk="turne.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEA</uzo>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="vojagx.0o">Voja&gcirc;o</ref> de artista grupo por prezenti spekta&jcirc;ojn en
      diversaj lokoj:
      <ekz>
        la sukceso de la festivaloj kura&gcirc;igis la organizadon de <tld/>oj de unuopaj
        teatraj trupoj en aliaj landoj
        <fnt>
          <bib>EeP</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        en &ccirc;iu urbo la sama scenaro, post la lasta prezento de la <tld/>o junulo frapas
        la pordon de la lo&gcirc;io
        <fnt>
          <bib>KrB</bib>
          <lok>Muzika terminaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni menciu interalie s-ron <nom>Jan Duda</nom>, &ccirc;e&hcirc;a violonisto, kiu kun &Ccirc;e&hcirc;a Nacia Simfonia
        Orkestro faris <tld/>on en pli ol 20 urboj tra Japanio
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470;3</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_r;&c_n;&c_e;</trd>
  <trd lng="ca">gira</trd>
  <trdgrp lng="cs">
    <trd>turn&eacute;</trd>,
    <trd>um&ecaron;leck&yacute; z&aacute;jezd</trd>
  </trdgrp>
  <trd lng="de">Tournee</trd>
  <trd lng="en">tour<klr tip="amb"> (theatrical)</klr></trd>
  <trd lng="fr">tourn&eacute;e<klr> (spectacles)</klr></trd>
  <trdgrp lng="hu">
    <trd>turn&eacute;</trd>,
    <trd>el&odblac;ad&oacute;k&ouml;r&uacute;t</trd>
  </trdgrp>
  <trd lng="nl">tournee</trd>
  <trd lng="pl">turnee</trd>
  <trd lng="pt">turn&ecirc;</trd>
  <trd lng="ru">&c_t;&c_u;&c_r;&c_n;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>turn&eacute;</trd>,
    <trd>umeleck&yacute; z&aacute;jazd</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_u;&c_r;&c_n;&c_je;</trd>,
    <trd>&c_gu;&c_a;&c_s;&c_t;&c_r;&c_o;&c_l;&c_mol;</trd>,
    <trd>&c_gu;&c_a;&c_s;&c_t;&c_r;&c_o;&c_l;&c_mol;&c_n;&c_a; &c_p;&c_o;&c_ji;&c_z;&c_d;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
