<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kvadrat</rad>/o
</kap>
<drv mrk="kvadra1.0o">
  <kap><tld/>o</kap>
  <snc mrk="kvadra1.0o.figuro">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 27</lok></fnt>
    <dif>
      Ortangula <ref tip="super" cel="romb.0o.figuro">rombo</ref>;
      alternative kaj samsence:
      <ref cel="regul.0a.plurlatero">regula</ref>
      <ref tip="super" cel="later.kvar0o">kvarlatero</ref>.
    </dif>
    <trd lng="hu">n&eacute;gyzet <klr
tip="ind">(n&eacute;gysz&ouml;g)</klr></trd>
    <trd lng="nl">vierkant</trd>
    <trdgrp lng="pl">
      <trd>kwadrat<klr tip="ind"> (czworok&aogonek;t)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="kvadra1.0o.algebra">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 10</lok></fnt>
    <dif>
      La dua <ref tip="super" cel="potenc.0o.per_entjero">potenco</ref>;
      nombro multiplikita per si mem:
      <ekz>
        <frm>3&quadrat; = 9</frm> <klr>(legu: tri kvadrate estas na&ubreve;,
        a&ubreve;: tri je kvadrato estas na&ubreve;)</klr>;
      </ekz>
      <ekz>
        la areo de <tld/>o <sncref ref="kvadra1.0o.figuro"/> estas
        la <tld/>o <sncref ref="kvadra1.0o.algebra"/> de &gcirc;ia
        <ref cel="later.0o.streko">latero</ref>.
      </ekz>
    </dif>
    <trd lng="hu">n&eacute;gyzet <klr tip="ind">(m&aacute;sodik
hatv&aacute;ny)</klr></trd>
    <trd lng="nl">kwadraat</trd>
    <trdgrp lng="pl">
      <trd>kwadrat<klr tip="ind"> (druga pot&eogonek;ga)</klr></trd>,
      <trd>druga pot&eogonek;ga<klr tip="ind"> (liczby)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_k;&c_v;&c_a;&c_d;&c_r;&c_a;&c_t;</trd>
  <trd lng="de">Quadrat</trd>
  <trd lng="en">square</trd>
  <trd lng="es">cuadrado<klr> (geometr&iacute;a y matem&aacute;ticas)</klr></trd>
  <trd lng="fr">carr&eacute;<klr tip="ind"> (math., subst.)</klr></trd>
  <trd lng="ru">&c_k;&c_v;&c_a;&c_d;&c_r;&c_a;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>druhá mocnina</trd>,
    <trd>štvorec</trd>
  </trdgrp>
  <trd lng="sv">kvadrat</trd>
</drv>

<drv mrk="kvadra1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Havanta formon de <ref cel="kvadra1.0o.figuro">kvadrato</ref>:
      <ekz>
        <tld/>a folio, skatolo.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>carr&eacute;<klr tip="ind"> (adj.)</klr></trd>
    </trdgrp>
    <trd lng="hu">n&eacute;gyzetes</trd>
    <trd lng="nl">vierkant</trd>
    <trd lng="ru">&c_k;&c_v;&c_a;&c_d;&c_r;&c_a;&c_t;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <ref cel="metr.kvadrata0o"><tld/>a metro</ref>,
      </ekz>
      <ekz>
        mejlo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">MAT</uzo>
        <tld/>a ekvacio <klr>(duagrada)</klr>
        <fnt><bib>PV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">MAT</uzo>
        <tld/>a <ref cel="radik.0o.per">radiko</ref>
        <fnt><bib>PV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>square</trd>,
      <trd>quadratic</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>carr&eacute;<klr tip="ind"> (adj.)</klr></trd>,
      <trd>quadratique</trd>
    </trdgrp>
    <trd lng="hu">n&eacute;gyzet-</trd>
    <trd lng="nl">kwadraat</trd>
    <trdgrp lng="pl">
      <trd>kwadratowy</trd>,
      <trd>drugiego stopnia</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_v;&c_a;&c_d;&c_r;&c_a;&c_t;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_k;&c_v;&c_a;&c_d;&c_r;&c_a;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kvadra1.0a.matrico">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>EKV</bib><lok>&para;270</lok></fnt>
    <dif>
      <klr>(p.p. <ref cel="matric.n_p0o">(n,p)-matrico</ref>)</klr>
      Tia, ke <frm><k>n</k> = <k>p</k></frm>.
    </dif>
    <trd lng="de">quadratisch<klr tip="ind"> (-e Matrix)</klr></trd>
    <trd lng="en">square<klr tip="ind"> (matrix)</klr></trd>
    <trd lng="fr"><klr tip="amb">(matrice) </klr>carr&eacute;e</trd>
    <trd lng="hu">n&eacute;gyzetes <klr
tip="ind">(m&aacute;trix)</klr></trd>
    <trd lng="pl"><klr tip="amb">(macierz) </klr>kwadratowa</trd>
    <trd lng="ru">&c_k;&c_v;&c_a;&c_d;&c_r;&c_a;&c_t;&c_n;&c_a;&c_ja;<klr tip="amb"> (&c_m;&c_a;&c_t;&c_r;&c_i;&c_c;&c_a;)</klr></trd>
  </snc>
  <trd lng="be">&c_k;&c_v;&c_a;&c_d;&c_r;&c_a;&c_t;&c_n;&c_y;</trd>
  <trdgrp lng="de">
    <trd>quadratisch</trd>,
    <trd>Quadrat-</trd>
  </trdgrp>
  <trd lng="en">square</trd>
  <trdgrp lng="sk">
    <trd>druhého stupňa</trd>,
    <trd>kvadratický</trd>,
    <trd>štvorcový</trd>
  </trdgrp>
  <trd lng="sv">kvadratisk</trd>
</drv>

<drv mrk="kvadra1.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="kvadra1.0igi.forme">
    <dif>
      Igi ion <tld/>a:
      <ekz>
        <tld/>igi konstru&scirc;tonon;
      </ekz>
      <ekz>
        <tld/>igi batalionon.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; &c_f;&c_o;&c_r;&c_m;&c_u; <ind>&c_k;&c_v;&c_a;&c_d;&c_r;&c_a;&c_t;&c_a;</ind></trd>,
      <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_k;&c_v;&c_a;&c_d;&c_r;&c_a;&c_t;&c_n;&c_y;&c_m;</ind></trd>
    </trdgrp>
    <trd lng="de">Quadrat formen</trd>
    <trdgrp lng="fr">
      <trd>&eacute;quarrir<klr tip="ind"> (rendre carr&eacute;)</klr></trd>,
      <trd>rendre <ind>carr&eacute;</ind></trd>
    </trdgrp>
    <trd lng="hu">n&eacute;gysz&ouml;ges&iacute;t</trd>
    <trd lng="nl">vierkant maken</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_i;&c_d;&c_a;&c_t;&c_mol; &c_f;&c_o;&c_r;&c_m;&c_u; <ind>&c_k;&c_v;&c_a;&c_d;&c_r;&c_a;&c_t;&c_a;</ind></trd>,
      <trd>&c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; <ind>&c_k;&c_v;&c_a;&c_d;&c_r;&c_a;&c_t;&c_n;&c_y;&c_m;</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="kvadra1.0igi.potencigi">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <ref tip="super" cel="potenc.0igi.algebra">Potencigi</ref>
      per <frm>2</frm>.
    </dif>
    <trd lng="be">&c_u;&c_z;&c_v;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol; &c_u; <ind>&c_k;&c_v;&c_a;&c_d;&c_r;&c_a;&c_t;</ind></trd>
    <trd lng="de">quadrieren</trd>
    <trd lng="en">square</trd>
    <trd lng="fr">&eacute;lever au <ind>carr&eacute;</ind></trd>
    <trd lng="hu">n&eacute;gyzetre emel</trd>
    <trd lng="pl">podnosi&cacute; do kwadratu</trd>
    <trd lng="ru">&c_v;&c_o;&c_z;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol; &c_v; <ind>&c_k;&c_v;&c_a;&c_d;&c_r;&c_a;&c_t;</ind></trd>
    <trd lng="sv">kvadrera</trd>
  </snc>
  <trdgrp lng="sk">
    <trd>umocniť na druhú</trd>,
    <trd>utvoriť štvorec</trd>
  </trdgrp>
</drv>

<drv mrk="kvadra1.0umi0i">
  <kap><tld/>umi, <var><kap><tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Desegni <tld/>ojn sur io, <tld/>streki:
      <ekz>
        <tld/>umi paperfolion.
      </ekz>
    </dif>
    <trd lng="be">&c_l;&c_ib;&c_n;&c_je;&c_ib;&c_c;&c_mol; <klr tip="amb">(&c_k;&c_v;&c_a;&c_d;&c_r;&c_a;&c_t;&c_a;&c_m;&c_ib;)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_l;&c_i;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol; <klr tip="ind">(&c_k;&c_v;&c_a;&c_d;&c_r;&c_a;&c_t;&c_a;&c_m;&c_i;)</klr></trd>,
      <trd>&c_g;&c_r;&c_a;&c_f;&c_i;&c_t;&c_mol; <klr tip="amb">(&c_k;&c_v;&c_a;&c_d;&c_r;&c_a;&c_t;&c_a;&c_m;&c_i;)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="fr">quadriller<klr tip="ind"> (dessiner des
carr&eacute;s)</klr></trd>
  <trd lng="hu">kock&aacute;sra vonalk&aacute;z</trd>
  <trd lng="nl">vierkanten aanbrengen</trd>
  <trd lng="sk">štvorčekovať</trd>
</drv>

<drv mrk="kvadra1.0ita">
  <kap><tld/>ita</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Havanta <tld/>forman ornamon:
      <ekz>
        pantalono el <tld/>ita &scirc;tofo<fnt>B</fnt>.
      </ekz>
    </dif>
    <rim>
      Se la ornamo konsistas el <tld/>e kruci&gcirc;antaj linioj, oni
      povas diri <ref cel="krad.0ita">kradita</ref>; se la tutaj
      <tld/>areoj estas diverskoloraj, oni povas paroli pri
      <ref tip="vid" cel="sxak.0tabulo">&scirc;aktabula</ref>
      ornamo.
    </rim>
    <trdgrp lng="be">
      <trd>&c_k;&c_l;&c_je;&c_c;&c_ib;&c_s;&c_t;&c_y;</trd>,
      <trd>&c_k;&c_l;&c_ja;&c_t;&c_ch;&c_a;&c_s;&c_t;&c_y;</trd>
    </trdgrp>
    <trd lng="de">kariert</trd>
    <trd lng="en">checkered</trd>
    <trd lng="fr">&agrave; <ind>carreaux</ind></trd>
    <trd lng="hu">kock&aacute;s</trd>
    <trd lng="nl">geruit</trd>
    <trd lng="ru">&c_k;&c_l;&c_je;&c_t;&c_ch;&c_a;&c_t;&c_y;&c_j;</trd>
  </snc>
  <trd lng="sk">štvorčekovaný</trd>
</drv>

<drv mrk="kvadra1.0egala">
  <kap><tld/>egala</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(p.p. elemento <frm><k>x</k></frm>
      de <ref cel="monoid.0o">monoido</ref> kun
      multiplike signata operacio <frm>*</frm>)</klr>
      Tia, ke <frm><k>x</k>*<k>x</k> = <k>x</k></frm>; alidire: egala
      al &gcirc;ia <ref cel="kvadra1.0o.algebra"><tld/>o</ref>:
      <ekz>
        kiu ajn estas entjero <frm><k>n</k></frm>,
        la <ref cel="potenc.noa0o">n-a potenco</ref>
        de <tld/>egala elemento egalas al &gcirc;i mem;
      </ekz>
      <ekz>
        <ref cel="projek.vektora0o">vektora projekcio</ref> estas
        <tld/>egala endomorfio;
      </ekz>
      <ekz>
        <tld/>egala matrico;
      </ekz>
      <ekz>
        en la aro de kompleksoj <tld/>egalaj estas nur
        <frm>0</frm> kaj <frm>1</frm>.
      </ekz>
    </dif>
    <rim>
      Estas strange, ke en niaj fontoj troveblas nek tiu termino, nek
      la ekvivalenta internacia formo <ctl>idempotenta</ctl> (etimologie
      interpretebla kiel <ctl>egala al (&ccirc;iuj) siaj potencoj</ctl>),
      dum plurloke troveblas <ctl>nilpotenta</ctl>.
    </rim>
  </snc>
  <trd lng="be">&c_ib;&c_d;&c_e;&c_m;&c_p;&c_a;&c_t;&c_e;&c_n;&c_t;&c_n;&c_y;</trd>
  <trd lng="de">idempotent</trd>
  <trd lng="en">idempotent</trd>
  <trd lng="fr">idempotent</trd>
  <trd lng="hu">idempotens</trd>
  <trd lng="pl">idempotentny</trd>
  <trd lng="ru">&c_i;&c_d;&c_je;&c_m;&c_p;&c_o;&c_t;&c_je;&c_n;&c_t;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">idempotentný</trd>
</drv>
</art>
