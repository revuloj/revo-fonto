<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>1</ofc>
   <rad>morfin</rad>/o
</kap>
<drv mrk="morfin.0o">
   <kap><tld/>o</kap>
   <snc mrk="morfin.0o.MED">
     <uzo tip="fak">MED</uzo>
     <dif>
       Opia <ref tip="super" cel="medika1.0o.MED">medikamento</ref>
       uzata kiel
       dormigilo kaj dolorkvietigilo precipe per subha&ubreve;taj en&scirc;prucigoj.
     </dif>
   </snc>
  <trdgrp lng="be">
     <trd>&c_m;&c_a;&c_r;&c_f;&c_ib;&c_n;</trd>,
     <trd>&c_m;&c_o;&c_r;&c_f;&c_ib;&c_j;</trd>
   </trdgrp>
  <trd lng="bg">&c_m;&c_o;&c_r;&c_f;&c_i;&c_n;</trd>
  <trd lng="ca">morfina</trd>
  <trdgrp lng="de">
     <trd>Morphium</trd>,
     <trd>Morphin</trd>
   </trdgrp>
  <trdgrp lng="en">
     <trd>morphine</trd>,
     <trd>morphia</trd>
   </trdgrp>
  <trd lng="fa">&a_m;&a_r;&a_f;&f_y;&a_n;</trd>
  <trd lng="fr">morphine</trd>
  <trd lng="hu">morfium</trd>
  <trd lng="nl">morfine</trd>
  <trd lng="pt">morfina</trd>
  <trdgrp lng="ru">
     <trd>&c_m;&c_o;&c_r;&c_f;&c_i;&c_n;</trd>,
     <trd>&c_m;&c_o;&c_r;&c_f;&c_i;&c_j;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>alkaloid ópia</trd>,
    <trd>morfium</trd>,
    <trd>morfín</trd>
  </trdgrp>
  <trd lng="sv">morfin</trd>
</drv>
</art>
