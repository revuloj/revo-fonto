<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>morfin</rad>/o
</kap>
<drv mrk="morfin.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Opia <ref tip="super" cel="medika1.0o">medikamento</ref> uzata kiel
      dormigilo kaj dolorkvietigilo precipe per subha&ubreve;taj en&scirc;prucigoj.
    </dif>
  </snc>
  <trd lng="de">Morphium</trd>
  <trd lng="nl">morfine</trd>
  <trd lng="sv">morfin</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_r;&c_f;&c_i;&c_n;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_f;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
