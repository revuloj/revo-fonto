<drv mrk="ring.euxklida0o">
  <kap>e&ubreve;klida <tld/>o</kap>
  <snc mrk="ring.euxklida0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <ref cel="komut.0eca.ringo">Komuteca</ref>,
      <ref cel="integr.0a.MAT">integra</ref>
      <ref tip="super" cel="ring.0o.MAT"><tld/>o</ref>
      <frm am="bb R"><g>R</g></frm>,
      konsiderata kune kun tia bildigo <frm am="phi">&phi;</frm>, ke al &ccirc;iu
      nenula elemento <frm am="a"><k>a</k></frm> &gcirc;i asocias entjeron
      <frm am="phi(a) ge 0">&phi;(<k>a</k>) &#x2265; 0</frm>, kaj ke
      <frm am="phi(a) le phi(b)">&phi;(<k>a</k>) &#x2264; &phi;(<k>b</k>)</frm>,
      se <frm am="a"><k>a</k></frm> estas divizoro de
      <frm am="b"><k>b</k></frm>, kun
      la
      aldona kondi&ccirc;o, ke por &ccirc;iu paro
      <frm am="(a,b)">(<k>a</k>,<k>b</k>)</frm> kun nenula
      <frm am="b"><k>b</k></frm> ekzistas
      almena&ubreve; unu tia paro <frm am="(q,r)">(<k>q</k>,<k>r</k>)</frm>, ke <frm am="a=b xx                   q+r"><k>a</k> = <k>b</k>&#xD7;<k>q</k>+<k>r</k></frm> kaj
      <frm am="phi(a) le phi(b)">&phi;(<k>a</k>) &#x2264; &phi;(<k>b</k>)</frm>:
      <ekz>
        la <tld/>o de entjeroj estas e&ubreve;klida <tld/>o
        <klr>(<frm am="phi(a) = |a|">&phi;(<k>a</k>) =
        |<k>a</k>|</frm>)</klr>;
      </ekz>
      <ekz>
        &ccirc;iu <ref cel="ring.polinom0o">polinom<tld/>o</ref>
        estas e&ubreve;klida <tld/>o, konsiderante la bildigon
        <frm am="phi">&phi;</frm>, kiu al polinomo asocias &gcirc;ian gradon;
      </ekz>
      <ekz>
        &ccirc;iu e&ubreve;klida <tld/>o estas
        <ref cel="ideal.cxef0a.MAT">&ccirc;efideala</ref>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="euxkli.0o">E&ubreve;klido</ref>,
      <ref cel="divid.euxklida0o">e&ubreve;klida divido</ref>,
      <ref cel="kvocie.0o.euxklida">kvociento</ref>,
      <ref cel="rest.0o.euxklida">resto</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_w;&c_k;&c_l;&c_ib;&c_d;&c_a;&c_v;&c_a; &c_k;&c_o;&c_l;&c_c;&c_a;</trd>
  <trd lng="de">Euklidischer <ind>Ring</ind></trd>
  <trd lng="en">Euclidean <ind>ring</ind></trd>
  <trd lng="fr">anneau euclidien</trd>
  <trd lng="hu">euklideszi <ind>gy&udblac;r&udblac;</ind></trd>
  <trd lng="nl">Euclidische ring</trd>
  <trd lng="pl">pier&sacute;cie&nacute; euklidesowy</trd>
  <trd lng="ru">&c_je;&c_v;&c_k;&c_l;&c_i;&c_d;&c_o;&c_v;&c_o; <ind>&c_k;&c_o;&c_l;&c_mol;&c_c;&c_o;</ind></trd>
</drv>

