<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tineed.xml,v 1.14 2022/03/28 14:44:11 revo Exp $">
<kap>
  <rad>tineed</rad>/oj <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tineed.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="tineed.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref tip="malprt" cel="lepido1.0oj">lepidopteroj</ref>
      (<trd lng="la">Tineidae</trd>):
      <ekz>
        <tld/>oj estas familio de lepidopteroj al kiu apartenas <klr>[&#x2026;]</klr>
        vesto-<ref tip="prt" cel="tine.0o">tineo</ref>
        <klr>[&#x2026;]</klr> Atlasa tineo, la plej granda specio de tineo <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=294277">Tineedoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_p;&c_r;&c_a;&c_w;&c_d;&c_n;&c_y;&c_ja; <ind>&c_m;&c_o;&c_l;&c_ib;</ind> <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">tineids</trd>
  <trd lng="cs">moli</trd>
  <trd lng="de"><klr>(Echte)</klr> Motten<klr tip="ind">
  (Familie)</klr></trd>
  <trd lng="fr">mites</trd>
  <trd lng="hu">val&oacute;di <ind>molyok</ind><klr tip="ind">
  (csal&aacute;d)</klr></trd>
  <trd lng="nl">motten<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pl">molowate</trd>
  <trdgrp lng="pt">
    <trd>tine&iacute;deos</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_i;&c_je; <ind>&c_m;&c_o;&c_l;&c_i;</ind>
  <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">mole</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x43E;&#x43B;&#x435;&#x432;&#x456;</trd>,
    <trd>&#x441;&#x43F;&#x440;&#x430;&#x432;&#x436;&#x43D;&#x456; &#x43C;&#x43E;&#x43B;&#x456;</trd>
  </trdgrp>
</drv>
</art>
