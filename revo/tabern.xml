<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>tabernakl</rad>/o <fnt>Z</fnt>
    </kap>

    <drv mrk="tabern.0o">
      <kap><tld/>o</kap>
      <snc mrk="tabern.0o.BIB">
        <uzo tip="fak">BIB</uzo>
        <dif>
          &Ccirc;e la antikvaj Hebreoj, portebla templo &dash; tendo en kiu
          estis konservata la kesto de atesto:
          <ekz>
            &ccirc;ar nubo de la Eternulo estis super la <tld/>o dum la
            tago, kaj fajro estis dum la nokto super &gcirc;i, anta&ubreve;
            la okuloj de &ccirc;iuj Izraelidoj en &ccirc;iuj iliaj mar&scirc;oj
            <fnt><bib>MT</bib>, <lok>&Eli; 40:38</lok></fnt>.
          </ekz>
        </dif>
        <trd lng="hu">frigys&aacute;tor</trd>
        <trd lng="ru">&c_s;&c_k;&c_i;&c_n;&c_i;&c_ja;</trd>
      </snc>

      <snc mrk="tabern.0o.KRI">
        <uzo tip="fak">KRI</uzo>
        <dif>
          &Scirc;ranketo en <ref tip="malprt" cel="altar.0ejo">altarejo</ref>,
          en kiu oni konservas konsekritajn e&ubreve;karistia&jcirc;ojn.
        </dif>
        <ref tip="vid" cel="hosti.0ujo">hostiujo</ref>
        <trdgrp lng="hu">
          <trd>tabern&aacute;kulum</trd>,
          <trd>szents&eacute;gh&aacute;z</trd>
        </trdgrp>
        <trd
lng="ru">&c_d;&c_a;&c_r;&c_o;&c_h;&c_r;&c_a;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_c;&c_a;</trd>
      </snc>

      <trdgrp lng="be">
        <trd>&c_t;&c_a;&c_b;&c_e;&c_r;&c_n;&c_a;&c_k;&c_l;&c_mol;</trd>,
        <trd>&c_t;&c_a;&c_b;&c_e;&c_r;&c_n;&c_a;&c_k;&c_u;&c_l;</trd>
        <trd>&c_s;&c_k;&c_ib;&c_n;&c_ib;&c_ja;</trd>,
       
<trd>&c_d;&c_a;&c_r;&c_a;&c_z;&c_a;&c_h;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_c;&c_a;</trd>
      </trdgrp>
      <trd lng="de">Tabernakel</trd>
      <trd lng="nl">tabernakel</trd>
      <trdgrp lng="pt">
        <trd>tabern&aacute;culo</trd>
      </trdgrp>
      <trd lng="sv">tabernakel</trd>
      <trd lng="fr">tabernacle</trd>
    </drv>
  </art>
