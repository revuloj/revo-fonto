<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tabernakl</rad>/o <fnt>Z</fnt>
</kap>
<drv mrk="tabern.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;e antikvaj Hebreoj, tendo sub kiu estis konservata la kesto de
      atesto.
    </dif>
  </snc>
  <snc>
    <uzo tip="fak">KRI</uzo>
    <dif>
      &Scirc;ranketo, en kiu estas konservata E&ubreve;karistio.
    </dif>
  </snc>
  <trd lng="de">Tabernakel</trd>
  <trd lng="nl">tabernakel</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_a;&c_r;&c_o;&c_n;&c_o;&c_s;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_d;&c_a;&c_r;&c_o;&c_h;&c_r;&c_a;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="sv">tabernakel</trd>
</drv>
</art>
