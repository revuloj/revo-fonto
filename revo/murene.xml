<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: murene.xml,v 1.14 2024/02/06 09:23:18 revo Exp $">
<kap>
  <rad>murened</rad>/oj
</kap>
<drv mrk="murene.0oj">
  <kap><tld/>oj</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="angil.0oformaj">angiloformaj fi&scirc;oj</ref>
      (<trd lng="la">Muraenidae</trd>):
      <ekz>
        la <tld/>oj vivas ka&scirc;itaj en kavernoj, rokfendoj a&ubreve; koralejoj, kiujn ili forlasas nur foje
        nokte pro &ccirc;asado
        na&gcirc;ante per serpentaj movoj de la tuta korpo, dum la tago ili montras maksimume la kapon.
      </ekz>
    </dif>
    <ref tip="prt" cel="muren.0o">mureno</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_u;&c_r;&c_e;&c_n;&c_a;&c_v;&c_y;&c_ja;</trd>,
    <trd>&c_m;&c_u;&c_r;&c_e;&c_n;&c_y; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  </trdgrp>
  <trd lng="ca">mur&egrave;nids</trd>
  <trd lng="cs">mur&eacute;novit&eacute; <klr>(&ccaron;ele&dcaron;)</klr></trd>
  <trd lng="de">Mur&auml;nen<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="fr">Mur&eacute;nid&eacute;s</trd>
  <trd lng="hu">mur&eacute;naf&eacute;l&eacute;k</trd>
  <trdgrp lng="nl">
    <trd>murenen<klr tip="ind"> (fam.)</klr></trd>,
    <trd>moeralen<klr tip="ind"> (fam.)</klr></trd>
  </trdgrp>
  <trd lng="pl">murenowate</trd>
  <trd lng="pt">muren&iacute;deos</trd>
  <trd lng="ro">muraenidae</trd>
  <trd lng="ru">&c_u;&c_g;&c_r;&c_jo;&c_v;&c_y;&c_je; <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">mur&eacute;novit&eacute;</trd>
</drv>
</art>
