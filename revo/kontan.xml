<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kontant</rad>/a
</kap>
<drv mrk="kontan.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Tuj pagata:
      <ekz>
        pagi per <tld/>a mono;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        akcepti belajn parolojn por <tld/>a mono<fnt>Z</fnt>
        <klr>(kiel vera&jcirc;oj)</klr>;
      </ekz>
      <ekz>
        vendi, a&ccirc;eti <tld/>e.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kredit.0e">kredite</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">contant</trd>
  <trd lng="ru">&c_n;&c_a;&c_l;&c_i;&c_ch;&c_n;&c_y;&c_j; <klr>(&c_o; &c_d;&c_je;&c_n;&c_mol;&c_g;&c_a;&c_h;)</klr></trd>
</drv>
</art>
