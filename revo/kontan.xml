<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kontan.xml,v 1.20 2021/07/12 09:33:01 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kontant</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kontan.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="kontan.0a.EKON">
    <dif>
      Tuj pagata:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        akcepti belajn parolojn por <tld/>a mono<fnt>Z</fnt>
        <klr>(kiel vera&jcirc;oj)</klr>;
      </ekz>
      <ekz>
        akiri <tld/>an monon en alia lando
        <fnt>
        <aut>R. Rotsaert</aut>,
        <vrk>Pagi ene de la E&ubreve;ropa Unio kostos malpli</vrk>,
        <bib>Monato</bib>,
        <lok>2002:2, p. 10a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_ja;&c_w;&c_n;&c_y; <klr>(&c_p;&c_r;&c_a; &c_g;&c_r;&c_o;&c_sh;&c_y;)</klr></trd>
  <trd lng="bg">&c_b;&c_r;&c_o;&c_j;</trd>
  <trdgrp lng="cs">
    <trd>hotov&ecaron; plat&iacute;c&iacute;</trd>,
    <trd>okam&zcaron;it&ecaron; placen&yacute;</trd>
  </trdgrp>
  <trd lng="de">bar</trd>
  <trdgrp lng="en">
    <trd>paid in cash</trd>,
    <trd>cash</trd>
  </trdgrp>
  <trd lng="fa">&a_n;&a_q;&a_d;</trd>
  <trd lng="fr">comptant<klr> (adj.)</klr></trd>
  <trd lng="nl">contant<klr tip="ind"> (bn.)</klr></trd>
  <trd lng="pl">got&oacute;wk&aogonek; p&lstroke;atny</trd>     
  <trd lng="ro">numerar pl&abreve;tite</trd>
  <trd lng="ru">&c_n;&c_a;&c_l;&c_i;&c_ch;&c_n;&c_y;&c_j; <klr>(&c_o; &c_d;&c_je;&c_n;&c_mol;&c_g;&c_a;&c_h;)</klr></trd>
  <trd lng="sk">platiaci v hotovosti</trd>
</drv>

<drv mrk="kontan.0o">
  <kap><tld/>o</kap>
  <snc mrk="kontan.0o.EKON">
    <dif>
      Tuj disponebla mono:
      <ekz>
        li <klr>...</klr> ur&gcirc;e bezonas <tld/>on
        <fnt>
          <aut>Lao She, trad. Wang Chongfang</aut>,
          <vrk><url
          ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0026/">
          Kamelo &Scirc;jangzi</url></vrk>,
          <lok>1988</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_ja;&c_w;&c_n;&c_y;&c_ja; &c_g;&c_r;&c_o;&c_sh;&c_y;</trd>
  <trd lng="fr">argent comptant</trd>
  <trd lng="pl">got&oacute;wka</trd>     
  <trd lng="ro">numerar</trd>
</drv>

<drv mrk="kontan.0e">
  <kap><tld/>e</kap>
  <snc mrk="kontan.0e.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      En a&ccirc;eto, ne prokrastante inter akiro kaj pago.
    </dif>
    <refgrp tip="ant">
      <ref cel="kredit.0e">kredite</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_ja;&c_w;&c_n;&c_y;&c_m;&c_ib; <klr>(&c_p;&c_r;&c_a; &c_p;&c_l;&c_a;&c_c;&c_jo;&c_zh;)</klr></trd>
  <trd lng="bg">&c_b;&c_r;&c_o;&c_j;</trd>
  <trd lng="de">bar</trd>
  <trdgrp lng="en">
    <trd>paid in cash</trd>,
    <trd>paid immediately</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_n;&a_q;&a_d;</trd>,
    <trd>&a_n;&a_q;&a_d;&a_A;&a_fathatan;</trd>
  </trdgrp>
  <trd lng="fr">comptant<klr> (adv.)</klr></trd>
  <trd lng="nl">contant<klr tip="ind"> (bw.)</klr></trd>
  <trdgrp lng="pl">
    <trd>got&oacute;wk&aogonek; p&lstroke;atne</trd>,
    <trd>w <ind>got&oacute;wce</ind></trd>
  </trdgrp>   
  <trd lng="ro">pl&abreve;tit &icirc;n bani</trd>
</drv>
</art>
