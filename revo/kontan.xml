<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kontant</rad>/a
</kap>
<drv mrk="kontan.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Tuj pagata:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        akcepti belajn parolojn por <tld/>a mono<fnt>Z</fnt>
        <klr>(kiel vera&jcirc;oj)</klr>;
      </ekz>
      <ekz>
        akiri <tld/>an monon en alia lando<fnt><aut>Roland Rotsaert</aut>:
        <vrk>Pagi ene de la E&ubreve;ropa Unio kostos malpli</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 2a, p. 10a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">bar</trd>
  <trdgrp lng="fr">
    <trd>contant<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="nl">contant</trd>
  <trd lng="ru">&c_n;&c_a;&c_l;&c_i;&c_ch;&c_n;&c_y;&c_j; <klr>(&c_o; &c_d;&c_je;&c_n;&c_mol;&c_g;&c_a;&c_h;)</klr></trd>
</drv>

<drv mrk="kontan.0e">
  <kap><tld/>e</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      En a&ccirc;eto, ne prokrastante inter akiro kaj pago.
    </dif>
    <refgrp tip="ant">
      <ref cel="kredit.0e">kredite</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>contant<klr> (adv.)</klr></trd>
  </trdgrp>
</drv>
</art>
