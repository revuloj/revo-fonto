<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>cit</rad>/i
    </kap>
    <drv mrk="cit.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          La&ubreve;vorte ripeti, skribe a&ubreve; parole, vorton, frazon
          a&ubreve; tekston, fragmenton de iu verko:
          <ekz>
            la supre <tld/>ita
            vorto.
          </ekz>
        </dif>
        <trd lng="de">zitieren</trd>
      </snc>
      <snc>
        <dif>
          Aparte signi, montri iun a&ubreve; ion, mencii:
          <ekz>
            neeble estas
            <tld/>i &ccirc;iujn nomojn;
          </ekz><ekz>
            oni <tld/>as lin pro lia boneco;
          </ekz><ekz>
            mi povas <tld/>i al vi interesan fakton.
          </ekz>
        </dif>
        <trdgrp lng="de">
           <trd>vorstellen</trd>,
           <trd>erw&auml;hnen</trd>
        </trdgrp>
      </snc>
      <trd lng="en">quote</trd>
      <trd lng="fr">citer</trd>
      <trd lng="ru">&c_c;&c_i;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </drv>
    <drv mrk="cit.0ajxo">
      <kap><tld/>(a&jcirc;)o</kap>
      <snc>
        <dif>
          La ripetita teksto. 
        </dif>
      </snc>
      <trd lng="de">Zitat</trd>
      <trd lng="fr">citation</trd>
      <trd lng="ru">&c_c;&c_i;&c_t;&c_a;&c_t;&c_a;</trd>
    </drv>
    <drv mrk="cit.re0i">
      <kap>re<tld/>i<fnt>Z</fnt></kap>
      <snc>
        <dif>
          La&ubreve;te, la&ubreve;vorte kaj
          iom mekanike ripeti ion parkere lernitan:
          <ekz>
            re<tld/>i lecionon
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="deklam.0i">deklami</ref>.
        </refgrp>
      </snc>
     <trdgrp lng="de">
        <trd>vortragen</trd>,
        <trd>rezitieren</trd>
     </trdgrp>
      <trd lng="fr">r&eacute;citer</trd>
    </drv>
    <drv mrk="cit.0iloj">
      <kap><tld/>iloj</kap>
      <uzo tip="fak">TIP</uzo>
      <snc>
        <dif>
          <ref tip="malprt" cel="interp3.0o">Interpunkciaj</ref>
          <ref cel="sign.0o.KOMP">signoj</ref> uzataj por limsigni
          <tld/>a&jcirc;on:
          <ekz>
            malfermi, fermi la <tld/>ilojn</ekz>.
        </dif>
      </snc>
      <trdgrp lng="de">
         <trd>Anf&uuml;hrungszeichen</trd>,
         <trd>Anf&uuml;hrungsstriche</trd>,
         <trd>G&auml;nsef&uuml;&szlig;chen</trd>
      </trdgrp>
      <trd lng="fr">guillemets</trd>
      <trd lng="ru">&c_k;&c_a;&c_v;&c_y;&c_ch;&c_k;&c_i;</trd>
    </drv>
  </art>
