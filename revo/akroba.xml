<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: akroba.xml,v 1.32 2025/09/12 18:00:21 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>akrobat</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="akroba.0o">
  <kap><tld/>o</kap>
  <snc mrk="akroba.0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="sub" cel="danc.sxnur0isto">&Scirc;nurdancisto</ref> kaj 
      <ref tip="sub" cel="gimnas1.0isto">gimnastikisto</ref>:
      <ekz>
        poste aperis luksaj <nac>carrucae</nac> <klr>(&ccirc;aroj)</klr>, 
        plenaj de <tld/>oj, dancistoj, dancistinoj,
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili evoluigis grandegan lertecon de la anta&ubreve;aj membroj kaj fari&gcirc;is veraj
        <tld/>oj de trapezo
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        kelnerino rapidanta kun <tld/>a strukturo el fumantaj teleroj
        <fnt>
          <bib>ChB</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        financ-<tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2010/010078.php">&Scirc;uldoj kaj kulpoj</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="cirk.0isto">cirkisto</ref>,
       <ref cel="jxongl.0isto">&jcirc;onglisto</ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#26434;&#25216; <pr>z&aacute;j&igrave;</pr></trd>,
    <trd>&#38620;&#25216; <pr>z&aacute;j&igrave;</pr></trd>,
    <trd>&#34920;&#28436;&#29305;&#25216;&#32773; <pr>bi&#462;oy&#462;nt&egrave;j&igrave;zh&ecaron;</pr></trd>,
    <trd>&#26434;&#25216;&#28436;&#21592; <pr>z&aacute;j&igrave;y&#462;nyu&aacute;n</pr></trd>,
    <trd>&#38620;&#25216;&#28436;&#21729; <pr>z&aacute;j&igrave;y&#462;nyu&aacute;n</pr></trd>,
    <trd>&#20840;&#27494;&#34892; <pr>qu&aacute;nw&#468;h&aacute;ng</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_k;&c_r;&c_a;&c_b;&c_a;&c_t;</trd>
  <trd lng="bg">&c_a;&c_k;&c_r;&c_o;&c_b;&c_a;&c_t;</trd>
  <trd lng="ca">acr&ograve;bata</trd>
  <trdgrp lng="cs">
    <trd>akrobat</trd>,
    <trd>cirkusov&yacute; um&ecaron;lec</trd>,
    <trd>varietn&iacute; um&ecaron;lec</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Zirkust&auml;nzer</trd>,
     <trd>Akrobat</trd>
  </trdgrp>
  <trd lng="en">acrobat</trd>
  <trd lng="es">acr&oacute;bata</trd>
  <trd lng="fr">acrobate</trd>
  <trd lng="he">&alef;&#1463;&qof;&resh;&vav;&#1465;&bet;&#1468;&#1464;&tet;</trd>
  <trd lng="hu">akrobata</trd>
  <trdgrp lng="ja">
    <trd>&#26354;&#33464;&#24107;  <pr>&#12365;&#12423;&#12367;&#12370;&#12356;&#12375;</pr></trd>,
    <trd>&#36605;&#26989;&#24107;  <pr>&#12363;&#12427;&#12431;&#12374;&#12375;</pr></trd>
  </trdgrp>
  <trd lng="nl">acrobaat</trd>
  <trdgrp lng="pt">
    <trd>acrobata</trd>,
    <trd>acr&oacute;bata</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_k;&c_r;&c_o;&c_b;&c_a;&c_t;</trd>
  <trd lng="sk">akrobat</trd>
  <trd lng="sv">akrobat</trd>
  <trd lng="uk">&c_a;&c_k;&c_r;&c_o;&c_b;&c_a;&c_t;</trd>
</drv>

<drv mrk="akroba.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="akroba.0i.TEA">
    <dif>
      Plenumi <tld/>a&jcirc;ojn:
      <ekz>
        li <tld/>is sur&scirc;nure
        <fnt>
          <aut>F. Ko&zcaron;&iacute;k, trad. J. Karen kaj J. Vondrou&scaron;ek</aut>,
          <vrk>La plej eminenta inter Pierotoj</vrk>,
          <lok>2001</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mensloge kaj akrabate <tld/>is <klr>[&#8230;]</klr> ver-tordaj avert-asertoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2003/008208.html">Du mil tri</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="jxongl.0i">&jcirc;ongli</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_j;&c_m;&c_a;&c_c;&c_c;&c_a;
  <ind>&c_a;&c_k;&c_r;&c_a;&c_b;&c_a;&c_t;&c_y;&c_k;&c_a;&c_j;</ind></trd>
  <trd lng="ca">fer acrob&agrave;cies</trd>
  <trd lng="es">hacer acrobacias</trd>
  <trd lng="fr">faire l'<ind>acrobate</ind></trd>
  <trd lng="he">&lamed;&ayin;&shin;&vav;&tav; &alef;&qof;&resh;&vav;&bet;&tet;&yod;&qof;&he;</trd>
</drv>

<drv mrk="akroba.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="akroba.0ajxo.TEA">
    <dif>
      Malfacila gimnastika&jcirc;o:
      <ekz>
        oni vidis <tld/>a&jcirc;on de precipe lertaj &leftquot;aerartistoj&rightquot;
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>zirkusreife &Uuml;bung</trd>,
      <trd>Akrobatik</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#29305;&#25216;&#34920;&#28436; <pr>t&egrave;j&igrave;bi&#462;oy&#462;n</pr></trd>,
    <trd>&#27494;&#25171; <pr>w&#468;d&#462;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="akroba.0ajxo.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Videbla, impresa
      <ref tip="vid" cel="lert.0ajxo.KOMUNE">lerta&jcirc;o</ref>:
      <ekz>
        kiam ia popolo ricevas skriban literaturon, ni jam tian blindan lingvan
        <tld/>a&jcirc;on ne renkontas
        <fnt>
          <bib>LR</bib>
          <lok>4. Fonetiko</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>&Uuml;bung</trd>,
      <trd>Akrobatik</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_r;&c_a;&c_b;&c_a;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="bg">&c_a;&c_k;&c_r;&c_o;&c_b;&c_a;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="ca">acrob&agrave;cia</trd>
  <trd lng="en">acrobatics</trd>
  <trd lng="es">acrobacia</trd>
  <trd lng="fr">acrobatie</trd>
  <trd lng="he">&alef;&#1463;&qof;&#1456;&resh;&vav;&#1465;&bet;&#1468;&#1464;&tet;&#1460;&yod;&qof;&#1464;&he;</trd>
  <trdgrp lng="hu">
    <trd>akobata mutatv&aacute;ny</trd>,
    <trd>akrobatika</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#26354;&#33464;  <pr>&#12365;&#12423;&#12367;&#12370;&#12356;</pr></trd>,
    <trd>&#36605;&#26989;  <pr>&#12363;&#12427;&#12431;&#12374;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_k;&c_r;&c_o;&c_b;&c_a;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">akrobacia</trd>
  <trd lng="uk">&c_a;&c_k;&c_r;&c_o;&c_b;&c_a;&c_t;&c_i;&c_ch;&c_n;&c_i;&c_j; &c_p;&c_r;&c_i;&c_j;&c_o;&c_m;</trd>
</drv>
</art>
