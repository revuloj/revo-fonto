<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>cirkvit</rad>/o
</kap>
<drv mrk="cirkvi.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">ELE</uzo>
    <dif>
      La tuto de kondukiloj, aparatoj kaj ma&scirc;inoj, tra kiuj trafluas
      la elektra kurento.
    </dif>
    <trd lng="be">&c_l;&c_a;&c_n;&c_c;&c_u;&c_g; <klr tip="amb">(&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_y;)</klr></trd>
    <trd lng="el">&kappa;&ypsilon_ton;&kappa;&lambda;&omega;&my;&alfa;</trd>
    <trd lng="en">circuit</trd>
    <trdgrp lng="de">
      <trd>Schaltung</trd>,
      <trd>Schaltkreis</trd>
    </trdgrp>
    <trd lng="es">circuito</trd>
    <trd lng="fr">circuit</trd>
    <trd lng="hu">&aacute;ramk&ouml;r</trd>
    <trd lng="nl">circuit</trd>
    <trd lng="pt">circuito</trd>
    <trd lng="ru">&c_c;&c_je;&c_p;&c_mol; <klr>(&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja;)</klr></trd>
    <trd lng="sk"><klr>(elektrick&yacute;)</klr> obvod</trd>
    <trd lng="sv"><klr>(elektrisk) </klr>krets</trd>
  </snc>
  <snc mrk="cirkvi.0o.grafeo">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <klr>(en <ref cel="grafe.orientita0o">orientita grafeo</ref>)</klr>
      Tia <ref tip="super" cel="cikl.0o.grafeo">ciklo</ref>, ke la
      fina rando de &ccirc;iu termo koincidas kun la komenca de la sekvanta
      termo.
    </dif>
    <trd lng="de">Zyklusprogression</trd>
    <trd lng="en">circuit</trd>
    <trd lng="fr">circuit<klr tip="ind"> (dans un graphe)</klr></trd>
    <trd lng="pl">cykl<klr tip="ind"> (w grafie)</klr></trd>
    <trd lng="ru">&c_k;&c_o;&c_n;&c_t;&c_u;&c_r;<klr tip="ind"> (&c_v; &c_g;&c_r;&c_a;&c_f;&c_je;)</klr></trd>
  </snc>
</drv>
</art>
