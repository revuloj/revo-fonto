<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: neptuni.xml,v 1.22 2024/01/29 15:21:35 revo Exp $">
<kap>
  <rad>neptuni</rad>/o
</kap>
<drv mrk="neptuni.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Np</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="93">elemento</ref> kun
      atomnumero 93, atompezo 237,0482; 
      <ref tip="super" cel="urani.trans0o">transuranio</ref>, kies &ccirc;iuj 
      <ref tip="super" cel="izotop.0o">izotopoj</ref> 
      de tiu elemento estas radioaktivaj; aktinoido:
      <ekz>
        <tld/>o estas ar&gcirc;ent-kolora radioaktiva elemento, kaj kromprodukto de nukleaj reaktoroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=28850">Neptunio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_e;&c_p;&c_t;&c_u;&c_n;&c_ib;&c_j;</trd>
  <trd lng="bg">&c_n;&c_je;&c_p;&c_t;&c_u;&c_n;&c_i;&c_j;</trd>
  <trd lng="br">neptuniom</trd>
  <trd lng="ca">neptuni</trd>
  <trd lng="cs">neptunium</trd>
  <trd lng="de">Neptunium</trd>
  <trd lng="en">neptunium</trd>
  <trd lng="es">neptunio</trd>
  <trd lng="fr">neptunium</trd>
  <trd lng="hu">nept&uacute;nium</trd>
  <trd lng="id">neptunium</trd>
  <trd lng="it">nettunio</trd>
  <trd lng="la">Neptunium</trd>
  <trd lng="nl">neptunium</trd>
  <trd lng="pl">neptun</trd>
  <trd lng="pt">net&uacute;nio</trd>
  <trd lng="ru">&c_n;&c_je;&c_p;&c_t;&c_u;&c_n;&c_i;&c_j;</trd>
  <trd lng="sk">nept&uacute;nium</trd>
  <trd lng="sv">neptunium</trd>
  <trd lng="uk">&#x43D;&#x435;&#x43F;&#x442;&#x443;&#x43D;&#x456;&#x439;</trd>
  <trdgrp lng="zh">
    <trd>&#x93BF; <pr>n&#xE1;</pr></trd>,
    <trd>&#x954E; <pr>n&#xE1;</pr></trd>,
    <trd>&#x933C; <pr>n&#xE0;i</pr></trd>
  </trdgrp>
</drv>
</art>
