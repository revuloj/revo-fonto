<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>neptuni</rad>/o
</kap>
<drv mrk="neptuni.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 93, atompezo 237,0482, simbolo Np. <ref tip="super"
      cel="urani.trans0o">Transuranio</ref>. &Ccirc;iuj izotopoj de tiu
elemento
      estas radioaktivaj.
    </dif>
  </snc>
  <trd lng="de">Neptunium</trd>
  <trd lng="en">neptunium</trd>
  <trd lng="hu">nept&uacute;nium</trd>
  <trd lng="nl">neptunium</trd>
  <trd lng="ru">&c_n;&c_je;&c_p;&c_t;&c_u;&c_n;&c_i;&c_j;</trd>
</drv>
</art>
