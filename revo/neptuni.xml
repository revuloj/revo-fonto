<art mrk="$Id$">
<kap>
<rad>neptuni</rad>/o
</kap>
<drv mrk="neptuni.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
     &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
atomnumero 93, atompezo 237,0482, simbolo Np. <ref tip="super"
cel="urani.trans0o">Transuranio</ref>. &Ccirc;iuj izotopoj de tiu elemento
estas radioaktivaj.
    </dif>
  </snc>
<trd lng="de">Neptunium</trd>
<trd lng="nl">neptunium</trd>
</drv>
</art>

