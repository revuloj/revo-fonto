<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>radioaktiv</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="radioaktiv.0a">
  <kap><tld/>a</kap>
  <snc mrk="radioaktiv.0a.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Elsendanta <ref tip="vid" cel="radi.0o">radiojn</ref>
      pro disfalado de
      <ref tip="vid" cel="nukle.0o.FIZ">nukleoj</ref> de kelkaj
      &hcirc;emiaj elementoj:
    </dif>
    <ref tip="vid" cel="radium.0o">radiumo</ref>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_d;&c_y;&c_je;&c_a;&c_k;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>
  <trd lng="de">radioaktiv</trd>
  <trd lng="en">radioactive</trd>
  <trd lng="es">radiactivo</trd>
  <trd lng="fr">radioactif</trd>
  <trd lng="hu">radioakt&iacute;v</trd>
  <trd lng="nl">radioactief</trd>
  <trd lng="pt">radioativo</trd>
  <trd lng="ru">&c_r;&c_a;&c_d;&c_i;&c_o;&c_a;&c_k;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="radioaktiv.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="radioaktiv.0eco.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Kvalito esti <tld/>a.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_d;&c_y;&c_je;&c_a;&c_k;&c_t;&c_y;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="de">Radioaktivit&auml;t</trd>
  <trd lng="en">radioactivity</trd>
  <trd lng="es">radiactividad</trd>
  <trd lng="fr">radioactivit&eacute;</trd>
  <trd lng="hu">radioaktivit&aacute;s</trd>
  <trd lng="nl">radioactiviteit</trd>
  <trd lng="pt">radioatividade</trd>
  <trd lng="ru">&c_r;&c_a;&c_d;&c_i;&c_o;&c_a;&c_k;&c_t;&c_i;&c_v;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>

</art>
