<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <rad>momant</rad>/o
</kap>
<drv mrk="momant.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">FIZ</uzo>
   <snc>
     <dif>
       Vektora produto de io per levilbrako:
       <ekz>
         <tld/>o de forto;
       </ekz>
       <ekz>
         <tld/>o de movokvanto.
       </ekz>
     </dif>
   </snc>
   <snc>
     <dif>
       Produto de polusa intenso per distanco a&ubreve;
       per produtoj de distancoj inter polusoj:
       <ekz>
         magneta <tld/>o;
       </ekz>
       <ekz>
         dupolusa <tld/>o;
       </ekz>
       <ekz>
         kvarpolusa <tld/>o.
       </ekz>
     </dif>
   </snc>
   <snc>
     <dif>
       Produto de io per funkcio de distanco:
       <ekz>
         inerci<tld/>o (produto de maso per la kvadrato
         de &gcirc;ia distanco for de akso).
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_m;&c_o;&c_m;&c_a;&c_n;&c_t; 
<klr>(&c_f;&c_ib;&c_z;&c_y;&c_ch;&c_n;&c_y;, 
&c_m;&c_a;&c_t;&c_e;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;)</klr></trd>
   <trd lng="hu">nyomat&eacute;k<klr tip="ind"> (fizika)</klr></trd>
   <trd lng="nl">moment<klr tip="ind"> (fysica)</klr></trd>
   <trd lng="pt">momento <klr>(f&iacute;s.)</klr></trd>
   <trd lng="fr">moment<klr tip="ind"> (phys., m&eacute;c.)</klr></trd>
   <trd lng="ru">&c_m;&c_o;&c_m;&c_je;&c_n;&c_t; <klr 
tip="amb">(&c_f;&c_i;&c_z;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;, 
&c_m;&c_a;&c_t;&c_je;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;)</klr></trd>
</drv>

<drv mrk="momant.noa0o">
   <kap>n-a <tld/>o</kap>
   <uzo tip="fak">MAT</uzo>
   <fnt><bib>EKV</bib><lok>&para;285,
   &leftquot;momanto de la ordo n&rightquot;</lok></fnt>
   <snc>
     <dif>
       <klr>(de <ref cel="variab.hazarda0o">hazarda variablo</ref>
       <frm>X</frm>)</klr>
       La <ref cel="ekspek.0o">ekspekto</ref> de
       <frm>X<sup><k>n</k></sup></frm>;
       simb. <frm>E(X<sup><k>n</k></sup>)</frm>:
       <ekz>
         <ind>n-a centra <tld/>o</ind>
         <klr>(<frm>E((X-E(X))<sup><k>n</k></sup>)</frm>)</klr>.
         <trd lng="de">n-tes zentrales <ind>Moment</ind></trd>
         <trd lng="en">n-th central <ind>moment</ind></trd>
         <trd lng="fr">moment centr&eacute; d'ordre n</trd>
         <trdgrp lng="pl">
           <trd>moment centralny rz&eogonek;du n<klr tip="ind"> 
(zmiennej losowej)</klr></trd>,
           <trd>n-ty moment centralny<klr tip="ind"> (zmiennej 
losowej)</klr></trd>
         </trdgrp>
         <trd lng="ru">n-&c_j; 
&c_c;&c_je;&c_n;&c_t;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; 
<ind>&c_m;&c_o;&c_m;&c_je;&c_n;&c_t;</ind></trd>
       </ekz>
     </dif>
   </snc>
   <trd lng="de">n-tes <ind>Moment</ind></trd>
   <trd lng="en">n-th <ind>moment</ind></trd>
   <trd lng="fr">moment d'ordre n</trd>
   <trdgrp lng="pl">
     <trd>moment rz&eogonek;du n<klr tip="ind"> (zmiennej 
losowej)</klr></trd>,
     <trd>n-ty moment<klr tip="ind"> (zmiennej losowej)</klr></trd>
   </trdgrp>
   <trd lng="ru">n-&c_j; <ind>&c_m;&c_o;&c_m;&c_je;&c_n;&c_t;</ind></trd>
</drv>
</art>
