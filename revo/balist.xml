    <trd>haj&iacute;t&oacute;g&eacute;p</trd>,
    <trd>baliszta</trd>
  </trdgrp>
  <trd lng="nl">katapult</trd>
  <trd lng="pl">balista</trd>
  <trd lng="ru">&c_b;&c_a;&c_l;&c_l;&c_i;&c_s;&c_t;&c_a;</trd>
  <trd lng="sv">katapult</trd>
</drv>
