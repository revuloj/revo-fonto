<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>homeopati</rad>/o
</kap>
<drv mrk="homeop.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Kuraca sistemo, konsistanta en aplikado al malsanuloj
      de malgrandegaj dozoj da medikamentoj, produktantaj &ccirc;e sana
      individuo la simptomojn de malsaneco analoga al tiu, kiun oni
      volas kuraci. 
    </dif>
  </snc>
  <trd lng="ru">&c_g;&c_o;&c_m;&c_je;&c_o;&c_p;&c_a;&c_t;&c_i;&c_ja;</trd>
</drv>
<drv mrk="homeop.0a">
  <kap><tld/>a</kap>
  <snc num="1">
    <dif>
      Rilata al <tld/>o: <ekz><tld/>a metodo.</ekz>
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Malgrandega: <ekz><tld/>a dozo.</ekz>
    </dif>
  </snc>
  <trd lng="ru">&c_g;&c_o;&c_m;&c_je;&c_o;&c_p;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
<!--
$Log$
-->
</vortaro>
