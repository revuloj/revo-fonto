<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>grat</rad>/i
</kap>
<drv mrk="grat.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Froti malforte kaj supra&jcirc;e per akra a&ubreve; raspa objekto:
      <ekz>
        <tld/>i per ungo la ha&ubreve;ton, kiam &gcirc;i jukas;
      </ekz>
      <ekz>
        &ccirc;evalo <tld/>as la teron per sia hufo;
      </ekz>
      <ekz>
        hundo <tld/>as &ccirc;e l' pordo;
      </ekz>
      <ekz>
        la malliberulo <tld/>is sur la muro versojn kaj bildojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i <klr>(ludi gratante)</klr> mandolinon, gitaron;
      </ekz>
      <ekz>en unu sako du katoj, &ccirc;iam mordoj kaj <tld/>oj<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        inter la dornoj li ricevis kelkajn fortajn <tld/>ojn<fnt>Z</fnt>
        <klr>(<tld/>vundojn)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="skrap.0i">skrapi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_u;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_r;&c_a;&c_p;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_a;&c_b;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_e;&c_b;&c_c;&c_ib;</trd>
  </trdgrp>
  <trd lng="de">kratzen</trd>
  <trd lng="en">scratch</trd>
  <trdgrp lng="fr">
    <trd>gratter<klr tip="ind"> (racler)</klr></trd>,
    <trd>racler<klr tip="ind"> (gratter)</klr></trd></trdgrp> 
  <trd lng="nl">krassen</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_s;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_je;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_c;&c_a;&c_r;&c_a;&c_p;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="grat.0vundi">
  <kap><tld/>vundi</kap>
  <snc>
    <dif>
      Vundi <tld/>ante:
      <ekz>
        <tld/>vundi al iu la viza&gcirc;on;
      </ekz>
      <ekz>
        <ind><tld/>vundo</ind>.
        <trdgrp lng="fr">
          <trd>griffure<klr tip="ind"> (&eacute;gratignure)</klr></trd>,
          <trd>&eacute;gratignure</trd>,
          <trd>&eacute;raflure<klr tip="ind"> (&eacute;gratignure)</klr></trd></trdgrp> 
      <trd lng="nl">schram</trd>
</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_r;&c_a;&c_p;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_p;&c_a;&c_r;&c_a;&c_n;&c_ib;&c_c;&c_mol;)</klr></trd>
  <trdgrp lng="fr">
    <trd>griffer<klr tip="ind"> (&eacute;gratigner)</klr></trd>,
    <trd>&eacute;gratigner</trd>,
    <trd>&eacute;rafler<klr tip="ind"> (&eacute;gratigner)</klr></trd></trdgrp> 
  <trd lng="nl">schrammen</trd>
  <trd lng="ru">&c_p;&c_o;&c_c;&c_a;&c_r;&c_a;&c_p;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>

