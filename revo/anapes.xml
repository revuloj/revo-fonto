<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>anapest</rad>/o
</kap>
<drv mrk="anapes.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="pied.vers0o">Verspiedo</ref> konsistanta
      el tri silaboj, el kiuj la du unuaj estas mallongaj
      a&ubreve; senakcentaj, kaj la tria longa a&ubreve; akcenta; simb.
      /&ypsilon;&ypsilon;&dash;/:
      <ekz>
        &leftquot;apostol'&rightquot; konsistigas <tld/>on.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_a;&c_p;&c_e;&c_s;&c_t;</trd>
  <trd lng="de">Anap&auml;st</trd>
  <trd lng="fr">anapeste</trd>
  <trd lng="hu">anapesztus</trd>
  <trd lng="nl">anapest</trd>
  <trd lng="pt">anapesto</trd>
  <trd lng="ru">&c_a;&c_n;&c_a;&c_p;&c_je;&c_s;&c_t;</trd>
</drv>

<drv mrk="anapes.n0o">
  <kap>n-<tld/>o</kap>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="metr.0o.POE">Metro</ref>, &ccirc;efe konsistanta el
      n <tld/>oj; simb. /&dash;&ypsilon;&ypsilon;/<sup>n</sup>:
      <ekz>
        <ind>tri<tld/>o</ind>.
        <trd lng="fr">trim&egrave;tre anapestique</trd>
        <trd lng="pt">trianap&#233;stico</trd>
      </ekz>
    </dif>
    <trd lng="pt">n-<ind>anap&#233;stico</ind></trd>
  </snc>
</drv>
</art>
