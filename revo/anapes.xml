<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: anapes.xml,v 1.21 2021/02/28 16:37:32 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>anapest</rad>/o
</kap>
<drv mrk="anapes.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="pied.vers0o">Verspiedo</ref> konsistanta
      el tri silaboj, el kiuj la du unuaj estas mallongaj
      a&ubreve; senakcentaj, kaj la tria longa a&ubreve; akcenta; simb.
      /&ypsilon;&ypsilon;&#x2010;/:
      <ekz>
        &leftquot;apostol'&rightquot; konsistigas <tld/>on.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_a;&c_p;&c_e;&c_s;&c_t;</trd>
  <trd lng="bg">&c_a;&c_n;&c_a;&c_p;&c_je;&c_s;&c_t;</trd>
  <trd lng="cs">anapest</trd>
  <trd lng="de">Anap&auml;st</trd>
  <trdgrp lng="en">
    <trd>anapest</trd>,
    <trd>anaepest</trd>,
    <trd>antidactylus</trd>
  </trdgrp>
  <trd lng="fr">anapeste</trd>
  <trdgrp lng="he">
    <trd>&#x5D0;&#x5E0;&#x5E4;&#x5E1;&#x5D8;&#x5D5;</trd>,
    <trd>&#x5DE;&#x5E9;&#x5E7;&#x5DC; &#x5E7;&#x5E6;&#x5E8;-&#x5E7;&#x5E6;&#x5E8;-&#x5D0;&#x5E8;&#x5D5;&#x5DA;</trd>
  </trdgrp>
  <trd lng="hu">anapesztus</trd>
  <trd lng="nl">anapest</trd>
  <trd lng="pl">anapest</trd>
  <trd lng="pt">anapesto</trd>
  <trd lng="ru">&c_a;&c_n;&c_a;&c_p;&c_je;&c_s;&c_t;</trd>
  <trd lng="sk">anapest</trd>
</drv>

<drv mrk="anapes.n0o">
  <kap>n-<tld/>o</kap>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="metr.0o.POE">Metro</ref>, &ccirc;efe konsistanta
      el
      n <tld/>oj; simb. /&#x2010;&ypsilon;&ypsilon;/<sup>n</sup>:
      <ekz>
        <ind>tri<tld/>o</ind>.
        <trd lng="fr">trim&egrave;tre anapestique</trd>
        <trd lng="pt">trianap&eacute;stico</trd>
      </ekz>
    </dif>
    <trd lng="pt">n-<ind>anap&eacute;stico</ind></trd>
    <trd lng="pl">metrum</trd>
  </snc>
</drv>
</art>
