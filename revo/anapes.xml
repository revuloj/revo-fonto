<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: anapes.xml,v 1.27 2024/09/13 09:29:43 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>anapest</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="anapes.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POE</uzo>
  <snc mrk="anapes.0o.verspiedo">
    <dif>
      <ref tip="super" cel="pied.vers0o">Verspiedo</ref> konsistanta
      el tri silaboj, el kiuj la du unuaj estas mallongaj
      a&ubreve; senakcentaj, kaj la tria longa a&ubreve; akcenta; simb.
      /&ypsilon;&ypsilon;&#8208;/:
      <ekz>
        &leftquot;apostol'&rightquot; konsistigas <tld/>on;
      </ekz>
      <ekz>
        klasika formo, tiu plejofte uzata de Horacio: kvarversaj strofoj el du
        sammetrikaj versoj, konsistantaj el du <ref cel="jamb.0o">jambo</ref>j, poste <tld/>o, poste
        ankora&ubreve;
        jambo, la tria verso konsistas el kvar jamboj plus lasta mallonga silabo, kaj
        la lasta verso el du <ref cel="daktil1.0o">daktilo</ref>j kaj du <ref
        cel="sponde.0o">spondeo</ref>j
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2012/010377.php"
              >Nuntempo en antikva vesto</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_a;&c_p;&c_e;&c_s;&c_t;</trd>
  <trd lng="bg">&c_a;&c_n;&c_a;&c_p;&c_je;&c_s;&c_t;</trd>
  <trd lng="cs">anapest</trd>
  <trd lng="de">Anap&auml;st</trd>
  <trdgrp lng="en">
    <trd>anapest</trd>,
    <trd>anaepest</trd>,
    <trd>antidactylus</trd>
  </trdgrp>
  <trd lng="fr">anapeste</trd>
  <trdgrp lng="he">
    <trd>&alef;&nun;&pe;&samekh;&tet;&vav;</trd>,
    <trd>&mem;&shin;&qof;&lamed;
    &qof;&tsadi;&resh;-&qof;&tsadi;&resh;-&alef;&resh;&vav;&fkaf;</trd>
  </trdgrp>
  <trd lng="hu">anapesztus</trd>
  <trd lng="ja">&#30701;&#30701;&#38263;&#26684;  <pr>&#12383;&#12435;&#12383;&#12435;&#12385;&#12423;&#12358;&#12363;&#12367;</pr></trd>
  <trd lng="nl">anapest</trd>
  <trd lng="pl">anapest</trd>
  <trd lng="pt">anapesto</trd>
  <trd lng="ro">anapest</trd>
  <trd lng="ru">&c_a;&c_n;&c_a;&c_p;&c_je;&c_s;&c_t;</trd>
  <trd lng="sk">anapest</trd>
  <trd lng="uk">&c_a;&c_n;&c_a;&c_p;&c_je;&c_s;&c_t;</trd>
</drv>

<drv mrk="anapes.n0o">
  <kap>n-<tld/>o</kap>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="metr.0o.POE">Metro</ref>, &ccirc;efe konsistanta
      el
      n <tld/>oj; simb. <frm>/&#8208;&ypsilon;&ypsilon;/<sup>n</sup></frm>:
      <ekz>
        <ind>tri<tld/>o</ind>.
        <trd lng="fr">trim&egrave;tre anapestique</trd>
        <trd lng="pt">trianap&eacute;stico</trd>
      </ekz>
    </dif>
    <trd lng="pt">n-<ind>anap&eacute;stico</ind></trd>
    <trd lng="pl">metrum</trd>
    <trd lng="ro">m&abreve;sur&abreve; metric&abreve;</trd>
  </snc>
<trd lng="be">&c_m;&c_e;&c_t;&c_a;&c_r; &c_z; n <ind>&c_a;&c_n;&c_a;&c_p;&c_e;&c_s;&c_t;&c_a;&c_w;</ind></trd>
</drv>
</art>
