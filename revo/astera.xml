<drv mrk="astera.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Asteraceae</trd>), kies floroj estas
      dense kunigitaj kvaza&ubreve;e en kapeto.
    </dif>
  </snc>
  <trd lng="de">Korbbl&uuml;tengew&auml;chse</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_o;&c_zh;&c_n;&c_o;&c_c;&c_v;&c_je;&c_t;&c_n;&c_y;&c_je;</trd>,
    <trd>&c_a;&c_s;&c_t;&c_r;&c_o;&c_v;&c_y;&c_je;</trd>
  </trdgrp>
  <trd lng="sv">korgblommiga v&auml;xter</trd>
</drv>
</art>

