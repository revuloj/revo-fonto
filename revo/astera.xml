<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: astera.xml,v 1.21 2023/10/28 08:33:09 revo Exp $">
<kap>
  <rad>asterac</rad>/oj
</kap>
<drv mrk="astera.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="astera.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref>
      de <ref tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj, kies floroj estas dense kunigitaj kvaza&ubreve;e en kapeto
      (<trd lng="la">Asteraceae</trd>):
      <ekz>
        topinamburo <klr>[&#x2026;]</klr> altas 1&#x202F;-&#x202F;3&#x202F;m, apartenas al la familio de
        <tld/>oj
        <klr>[kaj]</klr> simile al helianto &gcirc;i havas grandajn foliojn kaj hele
        flavajn florojn
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2012/010553.php">Topinamburo &ccirc;u herba&ccirc;o?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="kompoz1.0oj">kompozitoj</ref>
    <trd lng="zh">&#x83CA;&#x79D1; <pr>j&#xFA;k&#x113;</pr></trd>
</snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_s;&c_t;&c_r;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#x2019;&c_ja;)</klr></trd>,
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_a;&c_n;&c_a;&c_k;&c_v;&c_je;&c_t;&c_n;&c_y;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>aster&agrave;cies</trd>,
    <trd>compostes</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hv&ecaron;zdicovit&eacute;</trd>,
    <trd>slo&zcaron;nokv&ecaron;t&eacute; <klr>(&ccaron;ele&dcaron;)</klr></trd>
  </trdgrp>
  <trd lng="de">Korbbl&uuml;tengew&auml;chse</trd>
  <trd lng="en">asteraceae</trd>
  <trd lng="fr">ast&eacute;rac&eacute;es</trd>
  <trd lng="hu">f&eacute;szkesvir&aacute;gzat&uacute;ak</trd>
  <trd lng="it">asteracee</trd>
  <trd lng="ja">&#x30AD;&#x30AF;&#x79D1;  <pr>&#x30AD;&#x30AF;&#x304B;</pr></trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_o;&c_zh;&c_n;&c_o;&c_c;&c_v;&c_je;&c_t;&c_n;&c_y;&c_je;</trd>,
    <trd>&c_a;&c_s;&c_t;&c_r;&c_o;&c_v;&c_y;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hviezdicovit&eacute;</trd>,
    <trd>zlo&zcaron;nokvet&eacute;</trd>
  </trdgrp>
  <trd lng="sv">korgblommiga v&auml;xter</trd>
  <trdgrp lng="uk">
    <trd>&c_a;&c_j;&c_s;&c_t;&c_r;&#x43E;&c_v;&c_ib;</trd>,
    <trd>&c_s;&#x43A;&#x43B;&c_a;&c_d;&#x43D;&#x43E;&c_c;&c_v;&c_ib;&c_t;&c_ib;</trd>
  </trdgrp>
</drv>
</art>

