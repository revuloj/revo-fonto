<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: monarh1.xml,v 1.20 2020/10/05 22:11:01 revo Exp $">
    <kap>
      <ofc>8</ofc>
      <rad>monar&hcirc;i</rad>/o, 
      <var><kap><ofc>8</ofc> <rad>monarki</rad>/o
      <fnt><bib>PV</bib></fnt></kap></var></kap>
    <drv mrk="monarh1.0o">
      <kap><tld/>o, <var><kap>monarkio <fnt><lok>Akademio de Esperanto,
      <url
      ref="http://www.akademio-de-esperanto.org/aktoj/aktoj2/8oa.html">8a
      Oficiala Aldono</url>, II.A.1</lok></fnt></kap></var></kap>
      <snc mrk="monarh1.0o.JUR">
        <uzo tip="fak">JUR</uzo>
        <dif>
          Politika <ref tip="super" cel="regxim.0o.POL">re&gcirc;imo</ref>
          de regno kies <ref cel="suvere.0o">suvereno</ref> estas
          persono (la <ref tip="prt" cel="monarh.0o">monarko</ref>):
          <ekz>
            absoluta, konstitucia, elekta, hereda <tld/>o.
          </ekz>
        </dif>
        <trdgrp lng="fa">
          <trd>&f_p;&a_A;&a_d;&a_sh;&a_A;&a_h;&f_y;</trd>,
          <trd>&a_s;&a_l;&a_T;&a_n;&a_t;</trd>
        </trdgrp>
      </snc>
      <snc mrk="monarh1.0o.POL">
        <uzo tip="fak">POL</uzo>
        <dif>
          Regno regata de monarko:
          <ekz>
            la &scirc;tato Vatikano estas absoluta <tld/>o elekta.
          </ekz>
        </dif>
        <trdgrp lng="fa">
          <trd>&f_k;&a_sh;&a_w;&a_r; <ind>&f_p;&a_A;&a_d;&a_sh;&a_A;&a_h;&f_y;</ind></trd>,
          <trd>&f_k;&a_sh;&a_w;&a_r; <ind>&a_s;&a_l;&a_T;&a_n;&a_t;&f_y;</ind></trd>
        </trdgrp>
      </snc>
      <trd lng="be">&c_m;&c_a;&c_n;&c_a;&c_r;&c_h;&c_ib;&c_ja;</trd>
      <trd lng="bg">&c_m;&c_o;&c_n;&c_a;&c_r;&c_h;&c_i;&c_ja;</trd>
      <trd lng="ca">monarquia</trd>
      <trd lng="de">Monarchie</trd>
      <trd lng="en">monarchy</trd>
      <trd lng="hu">monarchia</trd>
      <trd lng="nl">monarchie</trd>
      <trd lng="pl">monarchia</trd>
      <trd lng="pt">monarquia</trd>
      <trd lng="ro">monarhie</trd>
      <trd lng="ru">&c_m;&c_o;&c_n;&c_a;&c_r;&c_h;&c_i;&c_ja;</trd>
    </drv>
  </art>
