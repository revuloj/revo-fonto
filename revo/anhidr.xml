<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: anhidr.xml,v 1.18 2023/10/12 18:05:09 revo Exp $">
<kap>
  <rad>anhidr</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="anhidr.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Ne enhavanta akvon:
      <ekz>
        <tld/>aj silikatoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=328788">56 Meleto</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        dunatria fosfato <klr>[estas&#x2026;]</klr> dubaza salo de fosfata acido kaj natria
        hidroksido, konata plurforme tiel kiel <tld/>a, du-, hepta-, okta- kaj
        dodeka-hidrata, &ccirc;iuj estas akvosolveblaj pulvoroj, kaj la <tld/>a salo estas
        tre higroskopa
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=531459">Dunatria fosfato</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_z;&c_v;&c_o;&c_d;&c_n;&c_y;</trd>
  <trd lng="bg">&c_b;&c_je;&c_z;&c_v;&c_o;&c_d;&c_je;&c_n;</trd>
  <trd lng="cs">bezvod&yacute;</trd>
  <trd lng="de">wasserfrei</trd>
  <trdgrp lng="en">
    <trd>anhydrous</trd>,
    <trd>waterless</trd>
  </trdgrp>
  <trd lng="fr">anhydre</trd>
  <trd lng="he">&#x5E0;&#x5B0;&#x5D8;&#x5D5;&#x5BC;&#x5DC; &#x5DE;&#x5B7;&#x5D9;&#x5B4;&#x5DD;</trd>
  <trd lng="ja">&#x7121;&#x6C34;&#x306E;  <pr>&#x3080;&#x3059;&#x3044;&#x306E;</pr></trd>
  <trd lng="nl">watervrij</trd>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_v;&c_o;&c_d;&c_n;&c_y;&c_j; <klr tip="amb">(&c_h;&c_i;&c_m;.)</klr></trd>
  <trdgrp lng="sk">
    <trd>bezvod&yacute; <klr>(chem.)</klr></trd>,
    <trd>bezvod&yacute;</trd>
  </trdgrp>
  <trd lng="uk">&#x431;&#x435;&#x437;&#x432;&#x43E;&#x434;&#x43D;&#x438;&#x439;</trd>
</drv>
</art>
