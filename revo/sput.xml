<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sput</rad>/i
</kap>
<drv mrk="sput.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sput.0i.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Amase el&jcirc;eti tra la bu&scirc;o
      <klr>(sangon, muka&jcirc;on ks, esceptante salivon.
      <ref tip="vid" cel="kracx.0i">kra&ccirc;i</ref>)</klr>.
    </dif>
    <trd lng="be">&c_h;&c_a;&c_r;&c_k;&c_a;&c_c;&c_mol; 
      <klr>(&c_k;&c_r;&c_y;&c_v;&c_jo;&c_ju;, 
      &c_s;&c_mol;&c_l;&c_ib;&c_z;&c_mol;&c_z;&c_ju;)</klr>
    </trd>
    <trd lng="es">esputar<klr tip="amb"> (excepto saliva)</klr></trd>
    <trd lng="hu">k&ouml;p</trd>
    <trd lng="ru">&c_h;&c_a;&c_r;&c_k;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="sput.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      El&jcirc;eti amase el ia truo:
      <ekz>
        la vulkano el<tld/>is lafon;
      </ekz>
      <ekz>
        la kanonoj el<tld/>is morton;
      </ekz>
      <ekz>
        <tld/>i insultojn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="erupci.0i">erupcii</ref>,
      <ref cel="sxpruc.0igiel0igi">&scirc;prucigi</ref>,
      <ref cel="vom.0i">vomi</ref>
    </refgrp>
    <trd lng="be">&c_v;&c_y;&c_v;&c_ja;&c_r;&c_g;&c_a;&c_c;&c_mol;</trd> 
    <trd lng="hu">ok&aacute;d</trd>
    <trd lng="ru">&c_i;&c_z;&c_v;&c_je;&c_r;&c_g;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="de">
    <trd>spucken</trd>,
    <trd>speien</trd>
  </trdgrp>
  <trd lng="en">spit</trd>
  <trd lng="fr">cracher</trd>
  <trdgrp lng="nl">
    <trd>spugen</trd>,
    <trd>spuwen</trd>
  </trdgrp>
</drv>
</art>
