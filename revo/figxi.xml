<art mrk="$Id: figxi.xml,v 1.13 2023/11/09 20:48:32 revo Exp $">
<kap>
  <rad>Fi&gcirc;i</rad>/o
  <fnt><bib>RLN</bib></fnt>
</kap>

<drv mrk="figxi.0o">
  <kap>
    <tld/>o
  </kap>
  <mlg kod="ISO-3166">FJ</mlg>
  <snc mrk="figxi.0o.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Insulara, <ref tip="malprt" cel="oceani.O0o">oceania</ref>
      <ref tip="lst" cel="sxtat.0o" lst="voko:&scirc;tatoj">&scirc;tato</ref>
      kun &ccirc;efurbo Suvo, Respubliko <tld/>o:
      <ekz>
        pluraj landoj, kiuj anta&ubreve;e estis kolonioj de Britio, inkluzivas en siaj flagoj
        la britan en miniaturo (A&ubreve;stralio, Nov-Zelando, <tld/>o ktp)
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2004/008271.html">Flagoj: fenestro al vasta mondo</url></vrk>
              <bib>Monato</bib>
              <lok>2004/04, p. 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        la marakva krokodilo <klr>[&#x2026;]</klr> kapablas na&gcirc;i kelkcent kilometrojn en la
        maro kaj fojfoje aperis e&ccirc; en Nov-Kaledonio kaj <tld/>o
        <fnt>
          <aut>&Scirc;andor Monostori</aut>
          <vrk><url ref="&Monato;/2015/011466.php">Bani kun krokodiloj</url></vrk>
          <bib>Monato</bib>
          <lok>2015/06, p. 8</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_F;&c_ib;&#x301;&c_d;&c_zh;&c_y;</trd>
    <trdgrp lng="bg">
      <trd>&c_F;&c_i;&c_d;&c_zh;&c_i;</trd>,
      <trd>&c_R;&c_je;&c_p;&c_u;&c_b;&c_l;&c_i;&c_k;&c_a; <ind>&c_F;&c_i;&c_d;&c_zh;&c_i;</ind></trd>
    </trdgrp>
    <trd lng="de">Fidschi</trd>
    <trdgrp lng="en">
      <trd>Fiji</trd>,
      <trd>Republic of <ind>Fiji</ind></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>Fidji</trd>,
      <trd>R&eacute;publique des Fidji</trd>
    </trdgrp>
    <trd lng="id">Fiji</trd>
    <trd lng="nl">Fiji</trd>
    <trdgrp lng="pl">
      <trd>Fid&zdot;i</trd>,
      <trd>Republika Wysp <ind>Fid&zdot;i</ind></trd>
    </trdgrp>
    <trd lng="pt">Fiji</trd>
    <trd lng="ro">Fiji</trd>
    <trdgrp lng="zh">
    <trd>&#x6590;&#x6D4E; <pr>f&#x11B;ij&#xEC;</pr></trd>,
    <trd>&#x6590;&#x6D4E;&#x7FA4;&#x5C9B; <pr>f&#x11B;ij&#xEC;q&#xFA;nd&#x1CE;o</pr></trd>
  </trdgrp>
</snc>
</drv>

