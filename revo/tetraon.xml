<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tetraon.xml,v 1.16 2020/09/07 06:10:46 revo Exp $">
<kap>
  <rad>tetraon</rad>/o,
  <var><kap><rad var="a">tetra</rad>/o</kap></var>
</kap>
<drv mrk="tetraon.0o">
  <kap><tld/>o,
    <var><kap><tld var="a"/>o</kap></var>
  </kap>
  <snc mrk="tetraon.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio 
      <ref tip="malprt" cel="fazane.0oj">fazanedoj</ref>
      (<trd lng="la">Tetrao</trd>), de kokoformaj birdoj,
      kies vosto konsistas el 18 longaj kaj lar&gcirc;aj plumoj, kiujn
      &gcirc;i etendas ventumilforme dum la pari&gcirc;a ceremonio, kaj
      al kiu apartenas la <ref tip="sub" cel="urogal.0o">urogalo</ref>:
      <ekz>
        linko <klr>[&#8230;]</klr> man&gcirc;as &ccirc;efe malgrandajn bestojn kaj birdojn kiel 
        leporoj, musleporoj, kapreoloj, <tld var="a"/>oj kaj lagopoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=9085">Linko</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Auerh&uuml;hner</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_je;&c_c;&c_ja;&c_r;&c_u;&c_k;</trd>,
    <trd>&c_k;&c_a;&c_s;&c_a;&c_ch;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>gall fer</trd>,
    <trd>gall salvatge</trd>,
    <trd>g&egrave;nere Tetrao</trd>
  </trdgrp>
  <trd lng="cs">tet&rcaron;ev</trd>
  <trdgrp lng="fr">
    <trd>coq de bruy&egrave;re</trd>,
    <trd>t&eacute;tras</trd>
  </trdgrp>
  <trd lng="hu">fajd</trd>
  <trd lng="nl">korhoen</trd>
  <trdgrp lng="pt">
    <trd>tetraz</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_t;&c_je;&c_r;&c_je;&c_v;</trd>
  <trd lng="sk">tetrov</trd>
  <trd lng="sv">tetrao</trd>
</drv>
</art>
