<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tetraon</rad>/o
</kap>
<drv mrk="tetraon.0o">
  <kap><tld/>o</kap>
  <snc mrk="tetraon.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="tetraon1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Tetrao</trd>).
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_je;&c_c;&c_ja;&c_r;&c_u;&c_k;</trd>,
    <trd>&c_k;&c_a;&c_s;&c_a;&c_ch;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>coq de bruy&egrave;re</trd>,
    <trd>t&eacute;tras</trd>
  </trdgrp>
  <trd lng="hu">fajd</trd>
  <trd lng="nl">korhoen</trd>
  <trdgrp lng="pt">
    <trd>tetraz</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_t;&c_je;&c_r;&c_je;&c_v;</trd>
</drv>
</art>
