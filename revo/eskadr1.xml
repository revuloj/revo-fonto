<vortaro>
<art mrk="$Id$">
<kap>
  <rad>eskadron</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="eskadr1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Parto de kavaleria <ref tip="malprt" cel="regime.0o">regimento</ref>, komandata de oficiro havanta rangon
      inter kapitano kaj subkolonelo.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="skadr.0o">skadro</ref>,
    <ref cel="skadro.0o">skadrono</ref>.
  </refgrp>
  <trd lng="de">Schwadron</trd>
  <trd lng="fr">escadron</trd>
  <trd lng="nl">escadron</trd>
  <trd lng="ru">&c_e;&c_s;&c_k;&c_a;&c_d;&c_r;&c_o;&c_n;</trd>
</drv>
</art>
