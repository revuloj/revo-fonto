<art mrk="$Id: riboz.xml,v 1.6 2023/08/19 13:27:08 revo Exp $">
<kap>
  <rad>riboz</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="riboz.0o">
  <kap><tld/>o</kap>
  <snc mrk="riboz.0o.KEM">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="super" cel="suker.0o.KUI">Sukero</ref> trovebla
      en vegeta&jcirc;aj kaj bestaj &ccirc;elaroj, kaj anka&ubreve; en <ref tip="malprt"
      cel="acid.ribonuklea0o">RNA</ref>
      (C<sub>5</sub>H<sub>10</sub>O<sub>5</sub>):
      <ekz>
        kvankam la pli vaste konataj sukeroj, glukozo kaj fruktozo,
        havas en &ccirc;iu molekulo ses karbon-atomojn, <tld/>o havas nur
        kvin
        <fnt>
          <aut>S. Norvell</aut>,
          <vrk>Riboj</vrk>,
          <bib>Monato</bib>,
          <lok>2000:11, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ar">&a_r;&a_y;&a_b;&a_w;&a_z;</trd>
  <trd lng="be">&c_r;&c_y;&c_b;&c_o;&c_z;&c_a;</trd>
