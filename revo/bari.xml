<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bari.xml,v 1.28 2025/02/22 20:25:27 revo Exp $">
  <kap>
    <rad>bari</rad>/o <fnt><bib>PVEF</bib></fnt>,
    <var><kap><rad var="um">barium</rad>/o <fnt><bib>JED</bib></fnt></kap></var>
  </kap>
<drv mrk="bari.0o">
  <kap><tld/>o, <var><kap><tld var="um"/>o</kap></var></kap>
  <uzo tip="fak">KEM</uzo>
  <mlg kod="IUPAC">Ba</mlg>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="56">elemento</ref> kun
      atomnumero 56, atompezo 137,34;
      <ref tip="super" cel="metal.teralkala0o">teralkala metalo</ref>
      ar&gcirc;ente blanka, fandebla anta&ubreve; la ru&gcirc;i&gcirc;o; denseco 3,78:
      <ekz>
        <tld/>o estas venena
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=24496">Bario</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        radiumo estas metalo, ni kredas, <klr>[&#8230;]</klr> &ccirc;ar &gcirc;iaj saloj estas similaj al
        la saloj de <tld var="um"/>o, blanka metalo, kaj bonekonata elemento
        <fnt>
          <aut>Sir William Ramsay</aut>
          <vrk><url ref="https://www.gutenberg.org/files/38240/38240-h/vol1/e03.htm"
              >Radiumo</url></vrk>
          <lok>The Esperantist, n-ro 3, Jan. 1904</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Humphry Davy</nom> malkovris la elementojn natrio, kalio, <tld/>o, stroncio, kalcio
        kaj magnezio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=46473">Humphry Davy</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_r;&c_y;&c_j;</trd>
  <trd lng="bg">&c_b;&c_a;&c_r;&c_i;&c_j;</trd>
  <trd lng="br">bariom</trd>
  <trd lng="ca">bari</trd>
  <trd lng="cs">baryum</trd>
  <trd lng="de">Barium</trd>
  <trd lng="en">barium</trd>
  <trd lng="es">bario</trd>
  <trd lng="fr">baryum</trd>
  <trd lng="he">&bet;&resh;&yod;&vav;&fmem;</trd>
  <trd lng="hu">b&aacute;rium</trd>
  <trd lng="id">barium</trd>
  <trd lng="it">bario</trd>
  <trd lng="ja">&#12496;&#12522;&#12454;&#12512;</trd>
  <trd lng="la">Barium</trd>
  <trd lng="nl">barium</trd>
  <trd lng="pl">bar</trd>
  <trd lng="pt">b&aacute;rio</trd>
  <trd lng="ru">&c_b;&c_a;&c_r;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>b&aacute;rium</trd>,
    <trd>b&aacute;ryum</trd>
  </trdgrp>
  <trd lng="sv">barium</trd>
  <trd lng="uk">&c_b;&c_a;&c_r;&c_ib;&c_j;</trd>
</drv>
</art>
