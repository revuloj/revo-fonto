<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>kapsul</rad>/o
</kap>
<drv mrk="kapsul.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Skatoletforma objekto.
    </dif>
  </snc>
  <snc>
    <dif>
      Metala ujeto kun formo de duonsfero.
    </dif>
  </snc>
  <snc>
    <dif>
      Ujeto el gelateno a&ubreve; oblato, uzata por enteni malbongustajn
      medikamentojn.
    </dif>
  </snc>
  <snc mrk="kapsul.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      La <ref tip="super" cel="perika.0o">perikarpo</ref> de
      <ref tip="malprt" cel="frukt.malferm0o">malfermfruktoj</ref>.
    </dif>
  </snc>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
