<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ene.xml,v 1.22 2023/11/07 20:34:10 revo Exp $">
<kap>
  <rad>Ene</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="ene.0o">
  <kap><tld/>o</kap>
  <snc mrk="ene.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latina.
    </dif>
  </snc>
  <snc mrk="ene.0o.PERS">
    <uzo tip="fak">PERS</uzo>
    <dif>
      Rolulo de Iliado, &Ccirc;efrolulo de <ref tip="vid" cel="ene.0ado"><tld/>ado</ref>, gvidanto de
      fu&gcirc;antaj
      trojanoj al Italujo:
      <ekz>
        Vergilio montris per la persono de <tld/>o la kura&gcirc;on de
        kompatema
        filo kaj la sagacon de brava kaj lerta kapitano
        <fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro 25a, p.
        181a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x30A2;&#x30A4;&#x30CD;&#x30A4;&#x30A2;&#x30B9;</trd>,
    <trd>&#x30A2;&#x30A8;&#x30CD;&#x30A2;&#x30B9;</trd>
  </trdgrp>
  <trd lng="lat">Aeneas</trd>
  <trd lng="be">&c_E;&c_n;&c_e;&c_j;</trd>
  <trd lng="ca">Enees</trd>
  <trdgrp lng="cs">
    <trd>Aeneas</trd>,
    <trd>Ainei&aacute;s</trd>
  </trdgrp>
  <trd lng="de">Aeneas</trd>
  <trd lng="en">Aeneas</trd>
  <trd lng="es">Eneas</trd>
  <trd lng="fr">&Eacute;n&eacute;e</trd>
  <trd lng="hu">Aeneas</trd>
  <trd lng="it">Enea</trd>
  <trd lng="pl">Eneasz</trd>
  <trd lng="pt">En&eacute;ias</trd>
  <trd lng="ru">&c_E;&c_n;&c_je;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>Aeneas</trd>,
    <trd>Ainei&aacute;s</trd>
  </trdgrp>
  <trd lng="uk">&c_Je;&#x43D;&c_je;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x57C3;&#x6D85;&#x963F;&#x65AF; <pr>&#x101;ini&#xE8;&#x101;s&#x12B;</pr></trd>,
    <trd>&#x4E9A;&#x5C3C;&#x65AF; <pr>y&#xE0;n&#xED;s&#x12B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ene.0ado">
  <kap><tld/>ado</kap>
  <uzo tip="fak">BELE</uzo>
  <snc mrk="ene.0ado.BELE">
    <dif>
      Latina epopeo de <ref tip="vid" cel="vergili.0o">Vergilio</ref>, prezentanta la fu&gcirc;on de
      <tld/>o kaj aliaj venkitaj trojanoj, kaj ilian
      instali&gcirc;on en Italujo kiel prapatroj de Romianoj:
      <ekz>
        Schiller <klr>...</klr> tradukas la duan kaj kvaran
        kantojn de la <tld/>ado en moderna ritmo kun rimoj
        <fnt>
          <aut>K. Kalocsay</aut>
          <vrk><url ref="&DonH;Revuoj/nlr/nlr45/metriko.html">
          La klasika metriko kaj Esperanto</url></vrk>
          <lok>La Nica Literatura Revuo, 1959-05 &gcirc;is 06,
          (4/5), p. 164a-171a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_E;&c_n;&c_e;&c_ib;&c_d;&c_a;</trd>
  <trd lng="de">Aeneis</trd>
  <trd lng="en">Aeneid</trd>
  <trd lng="es">Eneida</trd>
  <trd lng="fr">&Eacute;n&eacute;ide</trd>
  <trd lng="ja">&#x300E;<ind>&#x30A2;&#x30A8;&#x30CD;&#x30FC;&#x30A4;&#x30B9;</ind>&#x300F;</trd>
  <trd lng="lat">Aeneis</trd>
  <trd lng="hu">Aeneis</trd>
  <trd lng="it">Eneide</trd>
  <trd lng="ca">Eneida</trd>
  <trd lng="pl">Eneida</trd>
  <trd lng="pt">Eneida</trd>
  <trd lng="ru">&c_E;&c_n;&c_je;&c_i;&c_d;&c_a;</trd>
  <trd lng="uk">&c_Je;&#x43D;&c_je;&c_ji;&c_d;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x4F0A;&#x5C3C;&#x4F0A;&#x5FB7; <pr>y&#x12B;n&#xED;y&#x12B;d&#xE9;</pr></trd>,
    <trd>&#x57C3;&#x6D85;&#x963F;&#x65AF;&#x7D00; <pr>&#x101;ini&#xE8;&#x101;s&#x12B;j&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>

