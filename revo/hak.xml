<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc> <rad>hak</rad>/i
</kap>
<drv mrk="hak.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="hak.0i.trancxi">
    <dif>
      Frapi por tran&ccirc;i per batoj de diversformaj pezaj iloj:
      <ekz>
        <tld/>i arbon, bovokorpon;
      </ekz>
      <ekz>
        de<tld/>i <klr>(faligi <tld/>ante)</klr> kverkon;
      </ekz>
      <ekz>
        <tld/>i per glavo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i pajlon, viandon <klr>(distran&ccirc;i en
        pecetojn)</klr>.
      </ekz>
    </dif>
    <trd lng="de">hacken</trd>
  </snc>
  <snc mrk="hak.0i.maldelikate">
    <dif>
      Maldelikate tran&ccirc;i kvaza&ubreve; per batado de <tld/>ilo:
      <ekz>
        <ind>el<tld/>i sian vojon</ind> tra arbaro
        <klr>(<ref cel="bat.tra0i">trabati</ref>)</klr>;
        <trd lng="fr">se <ind>frayer</ind> un chemin</trd>
      </ekz>
      <ekz>
        vojo tra<tld/>ita en &scirc;tonegoj;
      </ekz>
      <ekz>
        <ind>&ccirc;irka&ubreve;<tld/>i</ind> <klr>(malkrudigi, prepare
        formi)</klr> statuon;
        <trdgrp lng="fr">
          <trd>d&eacute;grossir<klr tip="ind"> (tailler sommairement)</klr></trd>
        </trdgrp>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind><tld/>ita</ind> vo&ccirc;o, prononco, parolo
        <klr>(spasme interrompita, ne interligita)</klr>;
        <trdgrp lng="fr">
          <trd><klr tip="ind">(discours, style) </klr>hach&eacute;</trd>,
          <trd><klr tip="ind">(discours, style) </klr>heurt&eacute;</trd>,
          <trd><klr tip="ind">(discours, style) </klr>saccad&eacute;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        li diris per glacie malvarma, akre <tld/>ata tono<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>ita stilo <klr>(tro mallongfraza, sen sufi&ccirc;a
        interligiteco)</klr>;
      </ekz>
      <ekz>
        <ind><tld/>maniere paroli</ind>.
        <trdgrp lng="fr">
          <trd>parler de fa&ccedil;on saccad&eacute;e</trd>,
          <trd>b&eacute;gayer<klr tip="ind"> (fig., dire de mni&egrave;re hach&eacute;)</klr></trd>,
          <trd>hoqueter<klr tip="ind"> (fig., dire de mni&egrave;re hach&eacute;)</klr></trd>
          </trdgrp> 
        <trd lng="nl">hakkelen</trd>
      </ekz>
    </dif>
  </snc>
  <snc mrk="hak.0i.difekti">
    <dif>
      Difekti a&ubreve; detrui kvaza&ubreve; per batado de <tld/>ilo:
      <ekz>
        la hajlo <tld/>is la rikoltojn, la grenojn;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        troa laboro <ind>sub<tld/>i</ind>s lian farton
        <klr>(<ref cel="fos.sub0i">subfosi</ref>)</klr>.
        <trdgrp lng="fr">
          <trd>miner<klr tip="ind"> (saper)</klr></trd>,
          <trd>saper</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <adm>
    * "El~i sian vojon", "tra~i": referenci al enkondukota "trabori".
    * Fari apartan sencon por "~ita vocxo, stilo, ks".
    * Pripensi la uzon de FIG.
    <aut>MB</aut>
  </adm>
  <trd lng="en">hack</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;couper<klr tip="ind"> (en frappant avec un instrument tranchant)</klr></trd>,
    <trd>hacher<klr tip="ind"> (couper, couper en petits morceaux, interrompre, d&eacute;chiqueter)</klr></trd>,
    <trd>gr&ecirc;ler<klr tip="ind"> (d&eacute;vaster par la gr&ecirc;le)</klr></trd>,
    <trd>tailler<klr tip="ind"> (en frappant avec un instrument tranchant)</klr></trd>,
    <trd>trancher<klr tip="ind"> (en frappant avec un instrument tranchant)</klr></trd>,
    <trd>couper<klr tip="ind"> (&agrave; la hache)</klr></trd>,
    <trd>abattre<klr tip="ind"> (un arbre)</klr></trd>
  </trdgrp>
  <trd lng="nl">hakken</trd>
  <trd lng="ru">&c_r;&c_u;&c_b;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="hak.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="H"/>ilbato:
      <ekz>
        unu <tld/>o kverkon ne faligas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>o post <tld/>o estas la plej efika atako
        <klr>(atingi celon per ripetitaj penoj kaj provoj)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de"><klr tip="ind">(Axt-)</klr>Schlag</trd>
  <trd lng="fr">coup<klr tip="ind"> (de hache)</klr></trd>
  <trdgrp lng="nl">
    <trd>bijlslag</trd>,
    <trd>slag<klr tip="ind"> (met bijl)</klr>
    </trd>
  </trdgrp>
</drv>

<drv mrk="hak.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        <ind><mll tip="kom"><tld/>ado de ligno</mll> donas lignerojn</ind>
        <klr>(oni devas akcepti sen surprizo la konsekvencojn de siaj
        agoj)</klr> <fnt><bib>PrV</bib></fnt>.
        <trd lng="de"><mll tip="kom">Wo ge<ind>hobelt</ind> wird</mll> da
        fallen Sp&auml;ne</trd>
        <trd lng="fr"><mll tip="kom">on ne fait pas d'<ind>omelette</ind></mll> sans casser d'&oelig;ufs</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>action de <ind>hacher</ind></trd>,
    <trd>hachage</trd>,
    <trd>hachement</trd>
  </trdgrp>
  <trd lng="nl">gehak</trd>
  <trd lng="ru">&c_r;&c_u;&c_b;&c_k;&c_a;</trd>
</drv>

<drv mrk="hak.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="hak.0ilo.klingo">
    <dif>
      Instrumento por tran&ccirc;i, konsistanta el lar&gcirc;a, dika, peza
      klingo kun pli-malpli longa tenilo:
      <ekz>
        <tld/>ilo de arb<tld/>isto, &ccirc;arpentisto;
      </ekz>
      <ekz>
        <ind><tld/>ileto</ind> de hejtisto, de ligno&scirc;ufaristo;
        <trd lng="fr">hachette</trd>
      </ekz>
      <ekz>
        <tld/>ilo estas tran&ccirc;a, sed ne cedas la bran&ccirc;o
        <klr>(malgra&ubreve; liaj penoj li ne sukcesas venki la baron)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Axt</trd>,
      <trd>Beil</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>hache</trd>,
      <trd>cogn&eacute;e</trd>,
      <trd>merlin<klr tip="ind"> (hache)</klr></trd>
    </trdgrp>
    <trd lng="nl">bijl</trd>
  </snc>
  <snc mrk="hak.0ilo.diversforma">
    <dif>
      Diversforma fraptran&ccirc;a instrumento por distran&ccirc;i en
      pecetojn:
      <ekz>
        <tld/>ilo por pajlo, viando.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>hachoir</trd>,
      <trd>couperet</trd>,
    </trdgrp>
    <trd lng="nl">hakmes</trd>
  </snc>
  <trd lng="ru">&c_t;&c_o;&c_p;&c_o;&c_r;</trd>
</drv>

<drv mrk="hak.arbo0isto">
  <kap>arbo<tld/>isto</kap>
  <snc>
    <dif>
      Metiisto, kiu faligas arbojn.
    </dif>
  </snc>
  <trd lng="de">Holzf&auml;ller</trd>
  <trd lng="fr">b&ucirc;cheron</trd>
  <trd lng="nl">houthakker</trd>
  <trd lng="ru">&c_d;&c_r;&c_o;&c_v;&c_o;&c_s;&c_je;&c_k;</trd>
</drv>
</art>
