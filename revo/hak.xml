<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hak.xml,v 1.49 2024/01/25 09:24:52 revo Exp $">
<kap>
  <ofc>*</ofc> <rad>hak</rad>/i <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="hak.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="hak.0i.trancxi">
    <dif>
      <ref tip="super" cel="frap.0i.bati">Frapi</ref> per akra ilo por tran&ccirc;e dispartigi:
      <ekz>
        per <tld/>ilo ni hakas
        <fnt><bib>F</bib>, <lok>&FE; 34</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i lignon
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 19:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni senfeligu la bruloferon kaj dis<tld/>u &gcirc;in en pecojn
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 1:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        la amaso mortigu ilin per &scirc;tonoj kaj <tld/>u ilin per siaj glavoj
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 23:47</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>us ilin per mia beko, se ili ne estus tiel grandaj
        <fnt>
          <bib>Fab2</bib>
          <lok>Najbaraj familioj</lok>
        </fnt>!
      </ekz>
      <ekz>
        oni <tld/>is, oni segis, &dash; oni konstruis triferdekulon
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>os al vi la akrajn spronojn en la karnon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni fendis la &scirc;tonegojn per kojnoj <klr>[&#8230;]</klr> kaj <tld/>is obeliskojn
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        malgranda kaverneto <tld/>ita en roko
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 1 (63)</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ita brasiko
        <fnt>
          <bib>SrT</bib>
          <lok>Libro IV.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi sidas hejme kaj <tld/>as fingre al la klavaro de mia komputilo
        <fnt>
          <bib>Kon11</bib>
          <lok>Videoludoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">hacken</trd>
    <trd lng="pt">picar</trd>
    <trdgrp lng="zh">
      <trd>&#20405;&#25130; <pr>q&imacron;nji&eacute;</pr></trd>,
      <trd>&#37604;&#22320; <pr>ch&uacute;d&igrave;</pr></trd>,
      <trd>&#21057;&#30862; <pr>du&ograve;su&igrave;</pr></trd>,
      <trd>&#37917; <pr>ju&eacute;</pr></trd>,
      <trd>&#21128; <pr>p&imacron;</pr></trd>,
      <trd>&#20999;&#30862; <pr>qi&emacron;su&igrave;</pr></trd>,
      <trd>&#30733; <pr>k&#462;n</pr></trd>
    </trdgrp>
  </snc>
  <snc mrk="hak.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Maldelikate diserigi:
      <ekz>
        glacie malvarma, akre <tld/>ata tono
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia plua rakonto estis <tld/>ata de singultoj kaj snufoj
        <fnt>
          <bib>ChC</bib>
          <lok>15</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
<!-- mankas cita&jcirc;aro por tiuj sencoj..., ili eblke ne sufi&ccirc;e diferencas de la &ccirc;efa senco!?
  <snc mrk="hak.0i.maldelikate">
    <dif>
      Maldelikate tran&ccirc;i kvaza&ubreve; per batado de <tld/>ilo:
      <ekz>
        <ind>el<tld/>i sian vojon</ind> tra arbaro
        <klr>(<ref cel="bat.tra0i">trabati</ref>)</klr>;
        <trd lng="es"><ind>trazar</ind>se un camino</trd>
        <trd lng="fr">se <ind>frayer</ind> un chemin</trd>
        <trd lng="hu">utat <ind>v&aacute;g</ind> mag&aacute;nak</trd>
      </ekz>
      <ekz>
        vojo tra<tld/>ita en &scirc;tonegoj;
      </ekz>
      <ekz>
        <ind>&ccirc;irka&ubreve;<tld/>i</ind> <klr>(malkrudigi, prepare
          formi)</klr> statuon;
        <trdgrp lng="fr">
          <trd>d&eacute;grossir<klr tip="ind"> (tailler
          sommairement)</klr></trd>
        </trdgrp>
        <trd lng="hu">lenagyol<klr tip="ind">
        (k&ouml;rbev&aacute;g)</klr></trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind><tld/>ita</ind> vo&ccirc;o, prononco, parolo
        <klr>(spasme interrompita, ne interligita)</klr>;
        <trdgrp lng="es">
          <trd><klr tip="ind">(discurso, estilo)
          </klr>entrecortado</trd>,
          <trd><klr tip="ind">(discurso, estilo)
          </klr>atropelladamente</trd>,
          <trd><klr tip="ind">(discurso, estilo) </klr>a tirones</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd><klr tip="ind">(discours, style) </klr>hach&eacute;</trd>,
          <trd><klr tip="ind">(discours, style) </klr>heurt&eacute;</trd>,
          <trd><klr tip="ind">(discours, style) </klr>saccad&eacute;</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>darabos<klr tip="ind"> (besz&eacute;d,
          st&iacute;lus)</klr></trd>,
          <trd>szaggatott</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <tld/>ita stilo <klr>(tro mallongfraza, sen sufi&ccirc;a
          interligiteco)</klr>;
      </ekz>
      <ekz>
        <ind><tld/>maniere paroli</ind>.
        <trdgrp lng="es">
          <trd>tartamudear<klr tip="ind"> (fig., decir de manera
              entrecortada)</klr></trd>,
          <trd>hablar bruscamente</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>b&eacute;gayer<klr tip="ind"> (fig., dire de mani&egrave;re
              hach&eacute;e)</klr></trd>,
          <trd>hoqueter<klr tip="ind"> (fig., dire de mani&egrave;re
              hach&eacute;e)</klr></trd>,
          <trd>parler de fa&ccedil;on saccad&eacute;e</trd>
        </trdgrp>
        <trd lng="nl">hakkelen</trd>
      </ekz>
    </dif>
    <trd lng="pt">rachar</trd>
  </snc>
  <snc mrk="hak.0i.difekti">
    <dif>
      Difekti a&ubreve; detrui kvaza&ubreve; per batado de <tld/>ilo:
      <ekz>
        la hajlo <tld/>is la rikoltojn, la grenojn;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        troa laboro <ind>sub<tld/>i</ind>s lian farton
        <klr>(<ref cel="fos.sub0i">subfosi</ref>)</klr>.
        <trdgrp lng="es">
          <trd>minar<klr tip="ind"> (socavar)</klr></trd>,
          <trd>socavar</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>miner<klr tip="ind"> (saper)</klr></trd>,
          <trd>saper</trd>
        </trdgrp>
      </ekz>
    </dif>
    <trd lng="pt">machadar</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_b;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_o;&c_s;&c_je;&c_ch;&c_mol;</trd>
    </trdgrp>
  </snc>
  <adm>
    * "El~i sian vojon", "tra~i": referenci al enkondukota
    "trabori".
    * Fari apartan sencon por "~ita vocxo, stilo, ks".
    * Pripensi la uzon de FIG.
    <aut>MB</aut>
  </adm>
  -->
  <trdgrp lng="be">
    <trd>&c_s;&c_ja;&c_ch;&c_y;</trd>,
    <trd>&c_p;&c_l;&c_ja;&c_zh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_o;&c_c;&c_mol; <klr tip="amb">(&c_d;&c_r;&c_o;&c_v;&c_y;)</klr></trd>
  </trdgrp>
  <trd lng="bo">&t_ga;&t_ca;&t_naro;&t_da; &t_ba;&t_ca;&t_da; &t_ga;&t_ca;&t_da; &t_cha;&t_naro;&t_da;&t_tsheg;</trd>
  <trd lng="br">bouc'halia&ntilde;</trd>
  <trd lng="en">hack</trd>
  <trd lng="es">cortar a golpes</trd>
  <trdgrp lng="fr">
    <trd>abattre<klr tip="ind"> (un arbre)</klr></trd>,
    <trd>couper<klr tip="ind"> (&agrave; la hache)</klr></trd>,
    <trd>d&eacute;couper<klr tip="ind"> (en frappant avec un instrument
        tranchant)</klr></trd>,
    <trd>gr&ecirc;ler<klr tip="ind"> (d&eacute;vaster par la
    gr&ecirc;le)</klr></trd>,
    <trd>hacher<klr tip="ind"> (couper, couper en petits morceaux,
    interrompre,
        d&eacute;chiqueter)</klr></trd>,
    <trd>tailler<klr tip="ind"> (en frappant avec un instrument
    tranchant)</klr></trd>,
    <trd>trancher<klr tip="ind"> (en frappant avec un instrument
    tranchant)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>v&aacute;g<klr tip="ind"> (&uuml;t&eacute;ssel)</klr></trd>,
    <trd>csap<klr tip="ind"> (v&aacute;gva)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>abbattere<klr tip="ind"> (di albero)</klr></trd>,
    <trd>mozzare</trd>,
    <trd>spaccare<klr tip="ind"> (di legna)</klr></trd>,
    <trd>troncare</trd>
    </trdgrp>
  <trdgrp lng="ja">
    <trd>&#20999;&#12427;  <pr>&#12365;&#12427;</pr></trd>,
    <trd>&#12383;&#12383;&#12365;&#20999;&#12427;  <pr>&#12383;&#12383;&#12365;&#12365;&#12427;</pr></trd>,
    <trd>&#20999;&#12428;&#20999;&#12428;&#12395;&#12377;&#12427;  <pr>&#12365;&#12428;&#12366;&#12428;&#12395;&#12377;&#12427;</pr></trd>,
    <trd>&#20663;&#12417;&#12427;  <pr>&#12356;&#12383;&#12417;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">hakken</trd>
  <trd lng="pt">cortar com <ind>machado</ind></trd>
  <trd lng="ru">&c_r;&c_u;&c_b;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>naseka&tcaron;</trd>,
    <trd>seka&tcaron;</trd>,
    <trd>&tcaron;a&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nasekat</trd>,
    <trd>sekat sekerou</trd>,
    <trd>t&iacute;t</trd>
  </trdgrp>
  <trd lng="sw">kutema</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_u;&c_b;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_o;&c_t;&c_i; (&c_d;&c_r;&c_o;&c_v;&c_a;)</trd>,
    <trd>&c_n;&c_i;&c_shch;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_b;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_u;&c_b;&c_a;&c_t;&c_i; &c_s;&c_l;&c_o;&c_v;&c_a;</trd>,
    <trd>&c_g;&c_o;&c_v;&c_o;&c_r;&c_i;&c_t;&c_i; &c_u;&c_r;&c_i;&c_v;&c_ch;&c_a;&c_s;&c_t;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="hak.0o">
  <kap><tld/>o</kap>
  <snc mrk="hak.0o.KOMUNE">
    <dif>
      <tld lit="H"/>a bato:
      <ekz>
        levante spontonon <tld/>o-prete
        <fnt>
          <bib>SrT</bib>
          <lok>Libro IX.</lok>
        </fnt>;
      </ekz>
      <ekz>
        ci foriros el la mondo, malgrandulo, anta&ubreve; ol ci <klr>[&#8230;]</klr> gustis &ccirc;iujn la
        pu&scirc;ojn kaj hakojn, atendantaj cin de &ccirc;iuj, kiuj estas pli supraj
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
          <lok>Du blinduloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu <tld/>o kverkon ne faligas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>o post <tld/>o estas la plej efika atako
        <klr>(atingi celon per ripetitaj penoj kaj provoj)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_d;&c_a;&c_r; <klr tip="amb">(&c_s;&c_ja;&c_k;&c_je;&c_r;&c_a;&c_j;)</klr></trd>
  <trd lng="br">taol<klr tip="amb"> (bouc'hal)</klr></trd>
  <trdgrp lng="cs">
    <trd>sek</trd>,
    <trd>seknut&iacute;</trd>
  </trdgrp>
  <trd lng="de"><klr tip="ind">(Axt-)</klr>Schlag</trd>
  <trdgrp lng="es">
    <trd>golpe<klr tip="ind"> (de hacha)</klr></trd>,
    <trd>impacto<klr tip="ind"> (de hacha)</klr></trd>
  </trdgrp>
  <trd lng="fr">coup<klr tip="ind"> (de hache)</klr></trd>
  <trd lng="hu">csap&aacute;s<klr tip="ind">(fejsze-)</klr></trd>
  <trdgrp lng="it"><trd>colpo d'<ind>ascia</ind></trd>,
  <trd>colpo di <ind>scure</ind></trd>,
  <trd>colpo d'<ind>accetta</ind></trd>
  </trdgrp>
  <trd lng="ja">&#19968;&#25731;  <pr>&#12356;&#12385;&#12370;&#12365;</pr></trd>
  <trdgrp lng="nl">
    <trd>bijlslag</trd>,
    <trd>slag<klr tip="ind"> (met bijl)</klr>
    </trd>
  </trdgrp>
  <trd lng="pt">machadada</trd>
  <trd lng="ru">&c_u;&c_d;&c_a;&c_r;<klr tip="amb">
      (&c_r;&c_u;&c_b;&c_ja;&c_shch;&c_i;&c_j;)</klr></trd>
  <trdgrp lng="sk">
    <trd>sek</trd>,
    <trd>seknutie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_u;&c_b;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_u;&c_b;&c_a;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="hak.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="hak.0ado.KOMUNE">
    <dif>
      Ago <tld/>i:
      <ekz>
        <klr>[li]</klr> vivtenas sin <klr>[&#8230;]</klr> per <tld/>ado, karbigado ktp
        <fnt>
          <bib>HsT</bib>
          <lok>Sia propra mastro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind><mll tip="kom"><tld/>ado de ligno</mll> donas
        lignerojn</ind>
        <klr>(iuj aferoj neprigas perdojn)</klr>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="de"><mll tip="kom">Wo ge<ind>hobelt</ind>
        wird</mll>, da
          fallen Sp&auml;ne</trd>
        <trd lng="es"><mll tip="kom">no hacemos una
        <ind>tortilla</ind></mll> sin romper
          los huevos</trd>
        <trd lng="fr"><mll tip="kom">on ne fait pas
        d'<ind>omelette</ind></mll> sans casser
          d'&oelig;ufs</trd>
        <trd lng="hu"><mll tip="kom">Ahol<ind>faragnak</ind></mll>, hullik a
          forg&aacute;cs</trd>
        <trd lng="ru"><ind>&c_l;&c_je;&c_s; &c_r;&c_u;&c_b;&c_ja;&c_t;</ind> &#8208;
          &c_shch;&c_je;&c_p;&c_k;&c_i; &c_l;&c_je;&c_t;&c_ja;&c_t;</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_u;&c_b;&c_k;&c_a; <klr tip="amb">(&c_s;&c_ja;&c_k;&c_je;&c_r;&c_a;&c_j;)</klr></trd>,
    <trd>&c_v;&c_a;&c_l;&c_k;&c_a; <klr tip="amb">(&c_l;&c_je;&c_s;&c_u;)</klr></trd>,
    <trd>&c_v;&c_y;&c_s;&c_je;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_c;&c_ib;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="br">bouc'halerezh</trd>
  <trdgrp lng="cs">
    <trd>h&aacute;kov&aacute;n&iacute;</trd>,
    <trd>sek&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Hacken</trd>
  <trd lng="es">Acci&oacute;n de <ind>cortar</ind></trd>
  <trdgrp lng="fr">
    <trd>action de <ind>hacher</ind></trd>,
    <trd>hachage</trd>,
    <trd>hachement</trd>
  </trdgrp>
  <trd lng="hu">v&aacute;g&aacute;s<klr tip="ind"> (&uuml;t&eacute;ssel)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#20999;&#12427;&#12371;&#12392;  <pr>&#12365;&#12427;&#12371;&#12392;</pr></trd>,
    <trd>&#12383;&#12383;&#12365;&#20999;&#12427;&#12371;&#12392;  <pr>&#12383;&#12383;&#12365;&#12365;&#12427;&#12371;&#12392;</pr></trd>
  </trdgrp>
  <trd lng="nl">gehak</trd>
  <trd lng="ru">&c_r;&c_u;&c_b;&c_k;&c_a;</trd>
  <trd lng="sk">h&aacute;kovanie</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_u;&c_b;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_u;&c_b;&c_a;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="hak.0ilo">
  <kap><ofc>*</ofc><tld/>ilo</kap>
  <snc mrk="hak.0ilo.klingo">
    <dif>
      Instrumento por <tld/>i, konsistanta el lar&gcirc;a, dika, peza
      <ref tip="prt" cel="kling.0o">klingo</ref> padelforma kaj pli-malpli longa tenilo:
      <ekz>
        per <tld/>ilo ni hakas, per segilo ni segas
        <fnt><bib>F</bib>, <lok>&FE; 34</lok></fnt>;
      </ekz>
      <ekz>
        eksvingi&gcirc;os lia mano kun la <tld/>ilo por haki la arbon
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 19:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        por fabriki la bretarojn mi de<tld/>is arbon, kaj ellaboris &gcirc;in per segilo,
        <tld/>ilo, rabotilo
        <fnt>
          <bib>Rob</bib>
          <lok>Parto II</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> batis malrapide, sed malmole per la angulo de la <tld/>ilo sur
        la lignokojnon, kaj &gcirc;i penetris pli kaj pli en la trunkpecon, fendotan en du
        duonojn
        <fnt>
          <bib>VRA</bib>
          <lok>Jan Frideg&aring;rd</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>ileto</ind> de hejtisto, de ligno&scirc;ufaristo;
        <trd lng="br">bouc'halig</trd>
        <trd lng="es">hacha peque&ntilde;a</trd>
        <trd lng="fr">hachette</trd>
        <trdgrp lng="hu">
          <trd>kisbalta</trd>,
          <trd>szekerce</trd>
        </trdgrp>
      </ekz>
      <ekz>
        anta&ubreve;kuris liktoroj, portantaj <tld/>ilojn meze de vergofaskoj
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ilo estas tran&ccirc;a, sed ne cedas la bran&ccirc;o
        <klr>(malgra&ubreve; liaj penoj li ne sukcesas venki la
        baron)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="adz.0o">adzo</ref>,
      <ref cel="topor.0o">toporo</ref>
    </refgrp>
    <trd lng="br">bouc'hal</trd>
    <trdgrp lng="de">
      <trd>Axt</trd>,
      <trd>Beil</trd>
    </trdgrp>
    <trd lng="es">hacha</trd>
    <trdgrp lng="fr">
      <trd>hache</trd>,
      <trd>cogn&eacute;e</trd>,
      <trd>merlin<klr tip="ind"> (hache)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>balta</trd>,
      <trd>fejsze</trd>
    </trdgrp>
    <trdgrp lng="it"><trd>ascia</trd>,
    <trd>scure</trd>,
    <trd>accetta</trd>
    </trdgrp>
    <trd lng="nl">bijl</trd>
    <trd lng="pt">machado</trd>
    <trdgrp lng="zh">
      <trd>&#37383; <pr>f&umacron;</pr></trd>,
      <trd>&#26024; <pr>qi&amacron;ng</pr></trd>,
      <trd>&#26023;&#23376; <pr>f&#468;z&#464;</pr></trd>
    </trdgrp>
  </snc>
  <snc mrk="hak.0ilo.diversforma">
    <dif>
      Diversforma fraptran&ccirc;a instrumento por distran&ccirc;i en
      pecetojn:
      <ekz>
        <tld/>ilo por pajlo, viando.
      </ekz>
    </dif>
    <trd lng="br">mi&ntilde;serez</trd>
    <trdgrp lng="es">
      <trd>cuchilla</trd>,
      <trd>instrumento de corte</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>couperet</trd>,
      <trd>hachoir</trd>
    </trdgrp>
    <trd lng="hu">v&aacute;g&oacute;eszk&ouml;z</trd>
    <trd lng="it">mannaia</trd>
    <trd lng="nl">hakmes</trd>
    <trd lng="pt">instrumento de corte</trd>
  </snc>
  <trd lng="be">&c_s;&c_ja;&c_k;&c_je;&c_r;&c_a;</trd>
  <trd lng="cs">sekyra</trd>
  <trd lng="ja">&#12362;&#12398;</trd>
  <trd lng="pt">machado</trd>
  <trd lng="ru">&c_t;&c_o;&c_p;&c_o;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>sekera</trd>,
    <trd>vala&scaron;ka</trd>
  </trdgrp>
  <trd lng="sw">shoka</trd>
  <trd lng="uk">&c_s;&c_o;&c_k;&c_i;&c_r;&c_a;</trd>
</drv>

<drv mrk="hak.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="hak.cxirkaux0i.KOMUNE">
    <dif>
      <tld lit="H"/>i &ccirc;e la &ccirc;irka&ubreve;o, por
      doni &gcirc;ustan formon:
      <ekz>
        li starigis &scirc;ton<tld/>istojn, por &ccirc;irka&ubreve;<tld/>i
        &scirc;tonojn por la konstruado de la domo de Dio
        <fnt><bib>MT</bib>, <lok>&Kr1; 22:2</lok></fnt>;
      </ekz>
      <ekz>
        la viroj, kiuj revenis el la militiro, rakontis pri
        la belegaj temploj el &ccirc;irka&ubreve;<tld/>itaj multekostaj &scirc;tonoj
        <fnt><bib>Fab4</bib><lok>Filino de la mar&ccirc;a
        re&gcirc;o</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_s;&c_ja;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_ch;&c_e;&c_s;&c_v;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_s;&c_ja;&c_k;&c_je;&c_r;&c_a;&c_j;)</klr></trd>
  </trdgrp>
  <trd lng="br">bouc'hala&ntilde;</trd>
  <trd lng="de">behauen</trd>
  <trdgrp lng="es">
    <trd>tallar</trd>,
    <trd>desbastar<klr> (piedra, madera)</klr></trd>
  </trdgrp>
  <trd lng="fr">tailler<klr tip="ind"> (sur le pourtour)</klr></trd>
  <trd lng="it">intagliare</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_je;&c_s;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_t;&c_je;&c_s;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_o;&c_b;&c_r;&c_u;&c_b;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_b;&c_t;&c_je;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="hak.de0i">
  <kap>de<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="hak.de0i.KOMUNE">
    <dif>
      Forigi per <tld/>ilo, preni <tld/>ante, faligi <tld/>ante:
      <ekz>
        <nom>Abimele&hcirc;</nom> prenis <tld/>ilon en sian manon
        kaj de<tld/>is bran&ccirc;on de arbo
        <fnt><bib>MT</bib>, <lok>&Jug; 9:48</lok></fnt>;
      </ekz>
      <ekz>
        en la tempo de Kristnasko oni de<tld/>is tre junajn arbojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Abio</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_s;&c_ja;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_c;&c_ib;&c_n;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">tenna&ntilde;<klr tip="amb"> (gant ur
  vouc'hal)</klr></trd>
  <trdgrp lng="cs">
    <trd>k&aacute;cet <klr>(strom)</klr></trd>,
    <trd>useknout</trd>,
    <trd>utnout</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>abhacken</trd>,
    <trd>abschlagen</trd>
  </trdgrp>
  <trd lng="es">recortar</trd>
  <trdgrp lng="fr">
    <trd>&ocirc;ter<klr tip="ind"> (d'un coup de hache)</klr></trd>,
    <trd>enlever<klr tip="ind"> (d'un coup de hache)</klr></trd>,
    <trd>couper<klr tip="ind"> (&agrave; la hache pour &ocirc;ter)</klr></trd>,
    <trd>tailler<klr tip="ind"> (&agrave; la hache pour
    &ocirc;ter)</klr></trd>,
    <trd>abattre<klr tip="ind"> (un arbre)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
  <trd>mozzare</trd>,
  <trd>tranciare</trd></trdgrp>
  <trdgrp lng="ja">
    <trd>&#20999;&#12426;&#33853;&#12392;&#12377;  <pr>&#12365;&#12426;&#12362;&#12392;&#12377;</pr></trd>,
    <trd>&#20999;&#12426;&#38626;&#12377;  <pr>&#12365;&#12426;&#12399;&#12394;&#12377;</pr></trd>
  </trdgrp>
  <trd lng="pt">abater<klr tip="amb"> (com machado)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_r;&c_u;&c_b;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_r;&c_u;&c_b;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_s;&c_je;&c_ch;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odseka&tcaron;</trd>,
    <trd>s&tcaron;a&tcaron;</trd>,
    <trd>u&tcaron;a&tcaron;</trd>,
    <trd>zo&tcaron;a&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_r;&c_u;&c_b;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_t;&c_i;&c_n;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_s;&c_ib;&c_k;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#30733;&#19979; <pr>k&#462;nxi&agrave;</pr></trd>,
    <trd>&#26025; <pr>zh&#462;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hak.batal0ilo">
  <kap>batal<tld/>ilo</kap>
  <snc mrk="hak.batal0ilo.ARM">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      <tld lit="H"/>ilforma
      <ref tip="lst" lst="voko:bataliloj" cel="arm.0ilo">armilo</ref>:
      <ekz>
        &ccirc;irka&ubreve; 3,5 jarmiloj anta&ubreve; nun la stepanoj akiris
        modernajn batal<tld/>ilojn, kaj kombininte ilin kun la
        avanta&gcirc;oj de la &ccirc;evalo rapide konkeris vastajn
        teritoriojn en Turano, Irano kaj Hindio oriente, Anatolio
        kaj Balkanio sude, la tuta E&ubreve;ropo &gcirc;is Irlando okcidente
        <fnt>
          <aut>S. Pokrovskij</aut>, <vrk>La granda stepo</vrk>.
          <bib>LOdE</bib>, <lok>2003:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        sed eraras la friponoj, &ccirc;ar la batal<tld/>ilo <tld/>os
        reen, al ilia propra kapo
	<fnt><bib>SatirRak</bib>, <vrk>Pythagoras</vrk></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="haleba.0o">halebardo</ref>
  </snc>
  <trd lng="be">&c_s;&c_ja;&c_k;&c_je;&c_r;&c_a; <klr>(&c_b;&c_a;&c_ja;&c_v;&c_a;&c_ja;)</klr></trd>
  <trd lng="en">pole-ax(e)</trd>
  <trd lng="es">hacha de guerra</trd>
  <trd lng="fr">francisque</trd>
  <trdgrp lng="it">
  <trd>ascia<klr> da battaglia</klr></trd>,
  <trd>ascia<klr> da guerra</klr></trd>,
  <trd>azza</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_k;&c_i;&c_r;&c_a;</trd>,
    <trd>&c_b;&c_o;&c_je;&c_v;&c_o;&c_j; &c_t;&c_o;&c_p;&c_o;&c_r;</trd>
  </trdgrp>
  <trd lng="sk">bojov&aacute; sekera</trd>
  <trd lng="cs">v&aacute;le&ccaron;n&aacute; sekera</trd>
  <trd lng="de">Streitaxt</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_o;&c_j;&c_o;&c_v;&c_a;&nbsp;&c_s;&c_o;&c_k;&c_i;&c_r;&c_a;</trd>,
    <trd>&c_t;&c_o;&c_p;&c_ib;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#37381; <pr>yu&egrave;</pr></trd>,
    <trd>&#25097; <pr>yu&egrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hak.brik0ilo">
  <kap>brik<tld/>ilo</kap>
  <snc mrk="hak.brik0ilo.KOMUNE">
    <dif>
      Ilo per kiu oni dis<tld/>as brikojn, eventuale
      betonon a&ubreve; alian solida&jcirc;on...:
      <ekz>
        <nom>Dongguj</nom> iris al la hejmo de <nom>Bing&ccirc;en</nom> kunportante
        siajn brik<tld/>ilon kaj trulon
        <fnt>
          <aut>Gi Hjuepej</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/epc/epc6104/urbo.html">
          Vizito al la urbo</url></vrk>,
          <lok>El Popola &Ccirc;inio, 1961:4, p. 173a-175a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="cxiz.0ilo">&ccirc;izilo</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Maurerhammer</trd>
  </trdgrp>
  <trd lng="fr">ciseau de ma&ccedil;on</trd>
</drv>

<drv mrk="hak.ligno0isto">
  <kap>
    ligno<tld/>isto, <var><kap>arbo<tld/>isto</kap></var>
  </kap>
  <snc>
    <uzo tip="fak">HOR</uzo>
    <dif>
      <ref tip="lst" lst="voko:metioj" cel="meti.0isto">Metiisto</ref>, kiu faligas
      kaj senbran&ccirc;igas arbojn en la arbaro:
      <ekz>
        ili fari&gcirc;is ligno<tld/>istoj kaj akvoportistoj por la tuta komunumo
        <fnt>
          <bib>MT</bib>
          <lok>Josuo 9:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen por la &ccirc;arpentistoj, por la ligno<tld/>istoj, viaj servantoj, mi destinis
        dudek mil kor'ojn da dra&scirc;ita tritiko
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 2:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        apud granda arbaro vivis malri&ccirc;a ligno<tld/>isto kun siaj edzino kaj du infanoj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ha&ccirc;jo kaj Grenjo</lok>
        </fnt>;
      </ekz>
      <ekz>
        en a&ubreve;tuno regule venadis arbo<tld/>istoj kaj de<tld/>adis kelke da plej grandaj
        arboj
        <fnt>
          <bib>Fab2</bib>
          <lok>Abio</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen venis arbo<tld/>istoj, kaj tiam fari&gcirc;is granda revolucio
        <fnt>
          <bib>Fab1</bib>
          <lok>Fluganta kofro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_y;&c_v;&c_a;&c_s;&c_je;&c_k;</trd>
  <trd lng="br">koataer</trd>
  <trdgrp lng="cs">
    <trd>d&rcaron;eva&rcaron;</trd>,
    <trd>d&rcaron;evorubec</trd>
  </trdgrp>
  <trd lng="de">Holzf&auml;ller</trd>
  <trd lng="es">le&ntilde;ador</trd>
  <trd lng="fr">b&ucirc;cheron</trd>
  <trd lng="hu">fav&aacute;g&oacute;</trd>
  <trd lng="it">boscaiolo</trd>
  <trd lng="ja">&#12365;&#12371;&#12426;</trd>
  <trd lng="nl">houthakker</trd>
  <trd lng="pt">lenhador</trd>
  <trd lng="ru">&c_d;&c_r;&c_o;&c_v;&c_o;&c_s;&c_je;&c_k;</trd>
  <trd lng="sk">drevoruba&ccaron;</trd>
  <trd lng="uk">&c_l;&c_ib;&c_s;&c_o;&c_r;&c_u;&c_b;</trd>
</drv>

