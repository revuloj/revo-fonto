<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sfinkt.xml,v 1.22 2024/03/24 20:21:56 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>sfinkter</rad>/o
</kap>
<drv mrk="sfinkt.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">ANA</uzo>
    <dif>
      Ringforma <ref tip="super" cel="muskol.0o">muskolo</ref>,
      kiu stre&ccirc;i&gcirc;ante fermas iujn tubformajn
      organojn:
      <ekz>
        kiam la veziko pleni&gcirc;as da urino, refleksago malstre&ccirc;as la <tld/>an muskolon de
        la malalta veziko, kaj oni volas urini
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4360">Urina sistemo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_f;&c_ib;&c_n;&c_k;&c_t;&c_a;&c_r;</trd>
  <trd lng="ca">esf&iacute;nter</trd>
  <trdgrp lng="cs">
    <trd>sfinkter</trd>,
    <trd>sv&ecaron;rac&iacute; sval</trd>,
    <trd>sv&ecaron;ra&ccaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Sphinkter</trd>,
    <trd>Ringmuskel</trd>,
    <trd>Schlie&szlig;muskel</trd>
  </trdgrp>
  <trd lng="en">sphincter</trd>
  <trd lng="es">esf&iacute;nter</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_s;&a_f;&a_n;&f_k;&a_t;&a_r;</trd>,
    <trd>&a_ayn;&a_D;&a_l;&a_h;&a_hamza_sure; &a_H;&a_l;&a_q;&a_w;&f_y;</trd>,
    <trd>&a_ayn;&a_D;&a_l;&a_h;&a_hamza_sure; &a_h;&a_m;&zwnj;&f_k;&a_sh;</trd>
  </trdgrp>
  <trd lng="fr">sphincter</trd>
  <trd lng="hu">z&aacute;r&oacute;izom</trd>
  <trd lng="it">sfintere</trd>
  <trd lng="ja">&#x62EC;&#x7D04;&#x7B4B;  <pr>&#x304B;&#x3064;&#x3084;&#x304F;&#x304D;&#x3093;</pr></trd>
  <trdgrp lng="nl">
    <trd>kringspier</trd>,
    <trd>sluitspier</trd>
  </trdgrp>
  <trd lng="pl">zwieracz <klr>(mi&eogonek;sie&nacute;)</klr></trd>
  <trd lng="ru">&c_s;&c_f;&c_i;&c_n;&c_k;&c_t;&c_je;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>sfinkter</trd>,
    <trd>zviera&ccaron; <klr>(sval)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x441;&#x444;&#x456;&#x43D;&#x43A;&#x442;&#x435;&#x440;</trd>,
    <trd>&#x43C;&#x2019;&#x44F;&#x437;-&#x437;&#x430;&#x43C;&#x438;&#x43A;&#x430;&#x447;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x62EC;&#x7D04;&#x808C; <pr>ku&#xF2;yu&#x113;j&#x12B;</pr></trd>,
    <trd>&#x62EC;&#x7EA6;&#x808C; <pr>ku&#xF2;yu&#x113;j&#x12B;</pr></trd>,
    <trd>&#x538B;&#x7F29;&#x5668; <pr>y&#x101;su&#x14D;q&#xEC;</pr></trd>,
    <trd>&#x58D3;&#x7E2E;&#x5668; <pr>y&#x101;su&#x14D;q&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>
