<drv mrk="kultiv.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kultiv.0i.vegetalojn">
    <uzo tip="fak">AGR</uzo>
    <dif>
      <ref tip="super" cel="kresk.0igi.AGR">Kreskigi</ref>
      vegeta&jcirc;ojn prilaborante teron;
      <ref tip="sin" cel="kultur.0i.flegi">kulturi<sncref/></ref>:
      <ekz>
        mi <tld/>as sikomorojn
        <fnt><bib>MT</bib> <lok>&Amo; 7:14</lok></fnt>;
      </ekz><ekz>
        ekzistas vastaj &gcirc;ardenoj en kiuj oni <tld/>as la planton
        por eligi bonodoran rozakvon
        <fnt>
          <aut>M. Karamudini</aut>,
          <vrk><url ref="http://karamudini.blogspot.com">la vivo</url>
          </vrk>,
          <lok>2005-05-14</lok>
        </fnt>;
      </ekz><ekz>
        <tld/>a&jcirc;oj tradiciaj anstata&ubreve;i&gcirc;is per produktoj celataj al eksportado
        <fnt>
          <aut>Gianfranco Cazzaro</aut>
          <vrk><url ref="&Monato;/1994/A940602.php">Morale a&ccirc;eti, konscie subteni</url></vrk>
          <bib>Monato</bib>
          <lok>1994/06, p. 9</lok>
        </fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        vi estas la <tld/>ota&jcirc;o de Dio
        <fnt><bib>NT</bib> <lok>&Ko1; 3:9</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bred.0i">bredi</ref>,
      <ref cel="plant.0i.AGR">planti<sncref/></ref>
    </refgrp>
    <trd lng="de">kultivieren <klr>(Pflanzen)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_y;&c_r;&c_a;&c_shch;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol;<klr> (&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>,
      <trd>&c_k;&c_u;&c_l;&c_mol;&c_t;&c_i;&c_v;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;<klr> (&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="kultiv.0i.teron">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Prilabori teron por kreskigi vegeta&jcirc;ojn;
      <ref tip="sin" cel="kultur.0i.AGR">kulturi<sncref/></ref>:
      <ekz>
        la tero <klr>[&#x2026;]</klr> produktas kreska&jcirc;ojn, <klr>[&#x2026;]</klr>
        pro kiuj &gcirc;i anka&ubreve; estas <tld/>ata
        <fnt><bib>NT</bib> <lok>Hebreoj 6:7</lok></fnt>;
      </ekz><ekz>
        <nom>Noa</nom> komencis ter<tld/>an laboron kaj plantis vinber&gcirc;ardenon
        <fnt><bib>MT</bib> <lok>&Gen; 9:20</lok> </fnt>;
      </ekz><ekz>
        fekundaj <tld/>eblaj grundoj
        <fnt>
          <aut>Gilbert Ledon</aut>
            <vrk><url ref="&Monato;/2000/007018.php">Kulturo de milito</url></vrk>
            <bib>Monato</bib>
            <lok>2000/06, p. 5</lok>
          </fnt>.
      </ekz>
    </dif>
    <trd lng="de">anbauen <klr>(Landw.)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_o;&c_z;&c_d;&c_je;&c_l;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_r;&c_a;&c_b;&c_a;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;<klr> (&c_z;&c_je;&c_m;&c_l;&c_ju;)</klr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>kultivovat <klr>(poln&iacute; plodiny)</klr></trd>,
    <trd>p&ecaron;stit</trd>,
    <trd>p&ecaron;stovat</trd>
  </trdgrp>
      <trd lng="en">cultivate</trd>
  <trd lng="fr">cultiver<klr> (agriculture)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x8015;&#x4F5C;&#x3059;&#x308B;  <pr>&#x3053;&#x3046;&#x3055;&#x304F;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x683D;&#x57F9;&#x3059;&#x308B;  <pr>&#x3055;&#x3044;&#x3070;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x80B2;&#x3066;&#x308B;  <pr>&#x305D;&#x3060;&#x3066;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>cultiveren</trd>,
    <trd>kweken</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kultywowa&cacute;</trd>,
    <trd>uprawia&cacute;<klr> (ziemi&eogonek;, ro&sacute;liny uprawne)</klr></trd>
  </trdgrp>
  <trd lng="pt">cultivar <klr> a terra</klr></trd>
  <trdgrp lng="sk">
    <trd>pestova&tcaron;</trd>,
    <trd>vzdel&aacute;va&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>odla</trd>,
    <trd>bruka<klr> jorden</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_o;&c_b;&c_r;&c_o;&c_b;&c_l;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_k;&c_u;&c_l;&c_mol;&c_t;&c_i;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_r;&c_o;&c_shch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_v;&c_o;&c_d;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x57F9;&#x690D; <pr>p&#xE9;izh&#xED;</pr></trd>,
    <trd>&#x517B;&#x6B96; <pr>y&#x1CE;ngzh&#xED;</pr></trd>,
    <trd>&#x990A;&#x6B96; <pr>y&#x1CE;ngzh&#xED;</pr></trd>,
    <trd>&#x4F43; <pr>ti&#xE1;n</pr></trd>,
    <trd>&#x57F9;&#x517B; <pr>p&#xE9;iy&#x1CE;ng</pr></trd>,
    <trd>&#x57F9;&#x990A; <pr>p&#xE9;iy&#x1CE;ng</pr></trd>,
    <trd>&#x683D;&#x57F9; <pr>z&#x101;ip&#xE9;i</pr></trd>,
    <trd>&#x6811; <pr>sh&#xF9;</pr></trd>,
    <trd>&#x6A39; <pr>sh&#xF9;</pr></trd>,
    <trd>&#x79CD; <pr>zh&#xF2;ng</pr></trd>,
    <trd>&#x7A2E; <pr>zh&#xF2;ng</pr></trd>,
    <trd>&#x8015;&#x79CD; <pr>g&#x113;ngzh&#xF2;ng</pr></trd>,
    <trd>&#x8015;&#x7A2E; <pr>g&#x113;ngzh&#xF2;ng</pr></trd>,
    <trd>&#x754B; <pr>ti&#xE1;n</pr></trd>
  </trdgrp>
</drv>

