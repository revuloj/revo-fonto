<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sardin.xml,v 1.20 2019/06/30 18:21:49 revo Exp $">
<kap>
  <rad>sardin</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="sardin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="fisx.mar0o" lst="voko:fi&scirc;oj">Marfi&scirc;o</ref>,
      vivanta en grandaj svarmoj proksime de la marbordo,
      ofte malgranda konservita dense unu apud alia en skatolo kun oleo;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="klupee.0oj">klupeedoj</ref>
      (<trd lng="la">Sardina</trd>):
      <ekz>
        konsumu <klr>[&#8230;]</klr> pli da grasa fi&scirc;o (salmo, skombro, haringo, an&ccirc;ovo kaj
        <tld/>oj)
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van De Velde</aut>
          <vrk><url ref="&Monato;/2013/010934.php"
              >Sablohorlo&gcirc;o for&scirc;ovas piramidon</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kunpremita kiel enskatoligita <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2011/010541.php"
              >Grincantaj genuoj ... kaj &gcirc;entileco</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/6/6a/Sardina_pilchardus_2011.jpg/320px-Sardina_pilchardus_2011.jpg"
      prm="CC BY-SA 3.0"
      ><tld/>o (Sardina pilchardus)
        <fnt>
          <aut>Â© Citron, propra verko</aut>
          <lok><url ref="&WCW;/File:Sardina_pilchardus_2011.jpg"
              >https://commons.wikimedia.org/wiki/File:Sardina_pilchardus_2011.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_r;&c_d;&c_y;&c_n;&c_a;</trd>
  <trd lng="cs">sardinka</trd>
  <trdgrp lng="de">
    <trd>Sardine</trd>,
    <trd>Pilchard</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>sardine</trd>,
    <trd>pilchard</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>sardina</trd>,
    <trd>pilchard</trd>
  </trdgrp>
  <trd lng="fa">&a_s;&a_A;&a_r;&a_d;&f_y;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>sardine</trd>,
    <trd>pilchard</trd>
  </trdgrp>
  <trd lng="hu">szard&iacute;nia</trd>
  <trd lng="id">sarden</trd>
  <trd lng="it">sardina</trd>
  <trdgrp lng="nl">
    <trd>sardien</trd>,
    <trd>pelser</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>sardyna</trd>,
    <trd>sardynka</trd>,
    <trd>pilchard</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>sardinha</trd>,
    <trd>carapau</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_a;&c_r;&c_d;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">sardinka</trd>
</drv>
</art>
