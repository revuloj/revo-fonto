<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>sardin</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="sardin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="klupe.0edoj">klupeedoj</ref>
      (<trd lng="la">Sardina</trd>), ofte konservitaj ankora&ubreve;
      malgrandaj en oleo.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Sardine</trd>,
    <trd>Pilchard</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>sardine</trd>,
    <trd>pilchard</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>sardina</trd>,
    <trd>pilchard</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>sardine</trd>,
    <trd>pilchard</trd>
  </trdgrp>
  <trd lng="it">sardina</trd>
  <trdgrp lng="nl">
    <trd>sardien</trd>,
    <trd>pelser</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>sardinha</trd>,
    <trd>carapau</trd>
  </trdgrp>
</drv>
</art>
