<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>sardin</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="sardin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="fisx.mar0o" lst="voko:fi&scirc;oj">Marfi&scirc;o</ref>,
      vivanta en grandaj svarmoj proksime de la marbordo,
      ofte malgranda konservita dense unu apud alia en skatolo kun oleo;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="klupee.0oj">klupeedoj</ref>
      (<trd lng="la">Sardina</trd>). 
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_r;&c_d;&c_y;&c_n;&c_a;</trd>
  <trd lng="cs">sardinka</trd>
  <trdgrp lng="de">
    <trd>Sardine</trd>,
    <trd>Pilchard</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>sardine</trd>,
    <trd>pilchard</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>sardina</trd>,
    <trd>pilchard</trd>
  </trdgrp>
  <trd lng="fa">&a_s;&a_A;&a_r;&a_d;&f_y;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>sardine</trd>,
    <trd>pilchard</trd>
  </trdgrp>
  <trd lng="hu">szard&iacute;nia</trd>
  <trd lng="id">sarden</trd>
  <trd lng="it">sardina</trd>
  <trdgrp lng="nl">
    <trd>sardien</trd>,
    <trd>pelser</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>sardyna</trd>,
    <trd>sardynka</trd>,
    <trd>pilchard</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>sardinha</trd>,
    <trd>carapau</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_a;&c_r;&c_d;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">sardinka</trd>
</drv>
</art>
