<drv mrk="luks.sen0ane0a">
   <kap>sen<tld/>a, <var><kap>ne<tld/>a</kap></var></kap>
   <snc>
     <dif>
       Simpla, modesta:
       <ekz>
         &ccirc;ambro sen<tld/>a sed komforta.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_y;</trd>,
     <trd>&c_s;&c_mol;&c_c;&c_ib;&c_p;&c_l;&c_y;</trd>
   </trdgrp>
   <trdgrp lng="de">
      <trd>bescheiden</trd>,
      <trd>einfach</trd>
   </trdgrp>
   <trd lng="fr">simple</trd>
   <trdgrp lng="hu">
      <trd>f&eacute;ny&udblac;z&eacute;st&odblac;l mentes</trd>,
      <trd>szer&eacute;ny</trd>
   </trdgrp>
   <trd lng="nl">sober</trd>
   <trdgrp lng="pl">
     <trd>bez wodotrysk&oacute;w <klr>(bez niepotrzebnych dodatk&oacute;w)</klr></trd>,
     <trd>skromny</trd>
   </trdgrp>
   <trdgrp lng="ro">
     <trd>f&abreve;r&abreve; f&acirc;nt&acirc;ni</trd>,
     <trd>f&abreve;r&abreve; extras inutili</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_o;&c_j;</trd>,
     <trd>&c_b;&c_je;&c_z; <ind>&c_r;&c_o;&c_s;&c_k;&c_o;&c_sh;&c_i;</ind></trd>
   </trdgrp>
</drv>

<drv mrk="luks.mal0a">
   <kap>mal<tld/>a</kap>
   <snc>
     <dif>
       Havanta malri&ccirc;an aspekton.
     </dif>
   </snc>
  <trd lng="be">&c_b;&c_je;&c_d;&c_n;&c_y; <klr>(&c_n;&c_a;
&c_v;&c_y;&c_g;&c_l;&c_ja;&c_d;)</klr></trd>
  <trdgrp lng="cs">
    <trd>jednoduch&yacute;</trd>,
    <trd>skromn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>karg</trd>,
      <trd>&auml;u&szlig;erst bescheiden</trd>
   </trdgrp>
  <trdgrp lng="fr">
      <trd>pauvre</trd>,
      <trd>indigent</trd>
   </trdgrp>
  <trdgrp lng="hu">
      <trd>szeg&eacute;nyes</trd>,
      <trd>nyomor&uacute;s&aacute;gos</trd>
   </trdgrp>
  <trd lng="nl">armelijk</trd>
  <trdgrp lng="pl">
    <trd>prosty <klr>(skromny)</klr></trd>,
    <trd>biedny</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>simplu <klr>(modest)</klr></trd>,
    <trd>mizer</trd>,
    <trd>s&abreve;rac</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_d;&c_n;&c_y;&c_j; &c_n;&c_a; &c_v;&c_i;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>jednoduch&yacute;</trd>,
    <trd>skromn&yacute;</trd>
  </trdgrp>
</drv>
