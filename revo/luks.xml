<drv mrk="luks.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc>
     <dif>
       Elmontrema kaj multekosta pompa brileco en vivmaniero:
       <ekz>
         <tld/>o en vestoj, pri man&gcirc;oj;
       </ekz>
       <ekz>
         re&gcirc;a <tld/>o.
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="komfor">komforto</ref>,
       <ref cel="pomp.0o">pompo</ref>,
       <ref cel="ricx">ri&ccirc;eco</ref>,
       <ref cel="abund.0eco">abundeco</ref>.
     </refgrp>
     <refgrp tip="vid">
       <ref cel="velur.0o">veluro</ref>.
     </refgrp>
   </snc>
  <trdgrp lng="be">
     <trd>&c_l;&c_ju;&c_k;&c_s;</trd>,
     <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_sh;&c_a;</trd>,
     <trd>&c_p;&c_y;&c_sh;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
     <trd>&c_h;&c_a;&c_r;&c_a;&c_s;&c_t;&c_v;&c_o;</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>luxus</trd>,
    <trd>p&rcaron;epych</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Pracht</trd>,
      <trd>Luxus</trd>,
      <trd>Aufwand</trd>,
      <trd>Verschwendung</trd>
   </trdgrp>
  <trd lng="el">&pi;&omikron;&lambda;&ypsilon;&tau;&epsilon_ton;&lambda;&epsilon;&jota;&alfa;</trd>
  <trd lng="en">luxury</trd>
  <trd lng="es">lujo</trd>
  <trdgrp lng="fr"><trd>luxe</trd><trd>faste</trd><trd>somptuosit&eacute;</trd></trdgrp>
  <trdgrp lng="hu">
    <trd>f&eacute;ny&udblac;z&eacute;s</trd>,
    <trd>luxus</trd>
  </trdgrp>
  <trd lng="nl">luxe</trd>
  <trdgrp lng="pl">
    <trd>luksus</trd>,
    <trd>przepych</trd>,
    <trd>zbytek</trd>,
    <trd>splendor</trd>
  </trdgrp>
  <trd lng="pt">luxo</trd>
  <trdgrp lng="ro">
    <trd>lux</trd>,
    <trd>avere</trd>,
    <trd>abuz</trd>
  </trdgrp> 
  <trdgrp lng="ru">
     <trd>&c_r;&c_o;&c_s;&c_k;&c_o;&c_sh;&c_mol;</trd>,
     <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_o;&c_l;&c_je;&c_p;&c_i;&c_je;</trd>,
     <trd>&c_p;&c_y;&c_sh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>luxus</trd>,
    <trd>n&aacute;dhera</trd>,
    <trd>prepych</trd>
  </trdgrp>
  <trd lng="sv">lyx</trd>
</drv>

<drv mrk="luks.0a">
   <kap><tld/>a</kap>
   <snc>
     <dif>
       Elmontranta <tld/>on:
       <ekz>
         <tld/>a la vesto, sed malplena
         la po&scirc;o (<ref tip="vid" cel="murmur">murmuro</ref>)
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         pli bona bran&ccirc;o sennuksa ol ka&gcirc;o plej
         <tld/>a
         (<ref tip="vid" cel="liber.0o">libero</ref>)
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         <tld/>a fenestro, meblaro, lo&gcirc;ejo;
       </ekz>
       <ekz>
         mi ne povas permesi al mi tian <tld/>an malavarecon<fnt>Z</fnt>;
       </ekz>
       <ekz>
         <uzo tip="stl">FIG</uzo>
         floroj en plej <tld/>a florado<fnt>Z</fnt>;
       </ekz>
       <ekz>
         kontra&ubreve;<tld/>a le&gcirc;o;
       </ekz>
       <ekz>
         vivi lar&gcirc;e kaj <tld/>e
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ref tip="sin" cel="flu.super0a">superflua</ref>.
     </dif>
   </snc>
  <trd lng="af">grand</trd>
  <trd lng="am">&#x1273;&#x120B;&#x1245;</trd>
  <trd lng="ar">&#x627;&#x644;&#x643;&#x628;&#x631;&#x649;</trd>
  <trdgrp lng="be">
     <trd>&c_l;&c_ju;&c_k;&c_s;&c_a;&c_v;&c_y;</trd>,
     <trd>&c_l;&c_ju;&c_k;&c_s;&c_u;&c_s;&c_o;&c_v;&c_y;</trd>,
     <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_sh;&c_n;&c_y;</trd>,
     <trd>&c_p;&c_y;&c_sh;&c_n;&c_y;</trd>
   </trdgrp>
  <trd lng="bn">&#x997;&#x9CD;&#x9B0;&#x9CD;&#x9AF;&#x9BE;&#x9A8;&#x9CD;&#x9A1;</trd>
  <trd lng="co">prugettu</trd>
  <trdgrp lng="cs">
    <trd>luxusn&iacute;</trd>,
    <trd>p&rcaron;epychov&yacute;</trd>,
    <trd>skvostn&yacute;</trd>
  </trdgrp>
  <trd lng="da">store</trd>
  <trdgrp lng="de">
      <trd>pr&auml;chtig</trd>,
      <trd>&uuml;ppig</trd>,
      <trd>luxuri&ouml;s</trd>,
      <trd>aufw&auml;ndig</trd>,
      <trd>verschwenderisch</trd>
   </trdgrp>
  <trd lng="el">&pi;&omikron;&lambda;&ypsilon;&tau;&epsilon;&lambda;&eta_ton;&sigma_fina;</trd>
  <trd lng="es">lujoso</trd>
  <trd lng="et">grand</trd>
  <trdgrp lng="fr"><trd>luxueux</trd><trd>fastueux</trd></trdgrp>
  <trd lng="fy">grand</trd>
  <trd lng="gl">gran</trd>
  <trd lng="gu">&#xA97;&#xACD;&#xAB0;&#xABE;&#xAA8;&#xACD;&#xAA1;</trd>
  <trd lng="ha">babban</trd>
  <trd lng="hi">&#x92D;&#x935;&#x94D;&#x92F;</trd>
  <trd lng="hr">veli&#x10D;anstven</trd>
  <trdgrp lng="hu">
    <trd>f&eacute;ny&udblac;z&odblac;</trd>,
    <trd>luxus-</trd>
  </trdgrp>
  <trd lng="hy">&#x574;&#x565;&#x56E;</trd>
  <trd lng="is">st&#xF3;ra</trd>
  <trd lng="ja">&#x58EE;&#x5927;&#x306A;</trd>
  <trd lng="ka">&#x10D2;&#x10E0;&#x10D0;&#x10DC;&#x10D3;&#x10D8;&#x10DD;&#x10D6;&#x10E3;&#x10DA;&#x10D8;</trd>
  <trd lng="km">&#x1792;&#x17C6;</trd>
  <trd lng="ko">&#xADF8;&#xB79C;&#xB4DC;</trd>
  <trd lng="lo">&#xE82;&#xEAD;&#xE87;&#xE9B;&#xEB9;&#xEC8;</trd>
  <trd lng="mr">&#x917;&#x94D;&#x930;&#x901;&#x921;</trd>
  <trd lng="my">&#x1001;&#x1019;&#x103A;&#x1038;&#x1014;&#x102C;&#x1038;&#x1000;&#x103C;&#x102E;&#x1038;&#x1000;&#x103B;&#x101A;&#x103A;</trd>
  <trd lng="ne">&#x92E;&#x939;&#x93E;&#x928;&#x94D;</trd>
  <trd lng="nl">luxueus</trd>
  <trd lng="ny">chachikulu</trd>
  <trd lng="pa">&#xA38;&#xA3C;&#xA3E;&#xA28;&#xA26;&#xA3E;&#xA30;</trd>
  <trdgrp lng="pl">
    <trd>luksusowy</trd>,
    <trd>pompatyczny</trd>,
    <trd>paradny</trd>
  </trdgrp>
  <trd lng="ps">&#x644;&#x648;&#x626;&#x6D0;</trd>
  <trd lng="pt">luxuoso</trd>
  <trdgrp lng="ro">
    <trd>de lux</trd>,
    <trd>luxos</trd>
  </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_r;&c_o;&c_s;&c_k;&c_o;&c_sh;&c_n;&c_y;&c_j;</trd>,
     <trd>&c_p;&c_y;&c_sh;&c_n;&c_y;&c_j;</trd>,
     <trd>&c_k;&c_l;&c_a;&c_s;&c_s;&c_a;
&leftquot;&c_l;&c_ju;&c_k;&c_s;&rightquot;</trd>
   </trdgrp>
  <trd lng="sd">&#x67E;&#x648;&#x67D;&#x64A;</trd>
  <trd lng="si">&#xDB8;&#xDC4;&#xDCF;</trd>
  <trdgrp lng="sk">
