<drv mrk="luks.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Havanta malri&ccirc;an aspekton:
      <ekz>
        Duponto sin direktis al mal<tld/>a trinkejo, kiun li ofte vizitadis
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni metis &scirc;in en la mal<tld/>an liton, delikata tola&jcirc;o ne trovi&gcirc;is en &gcirc;i
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        robo, certe a&ccirc;etita en plej mal<tld/>a magazeno
        <fnt>
          <bib>ChR</bib>
          <lok>Tien kaj reen</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i luis modestan malnovan domon en mal<tld/>a kvartalo
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2012/010361.php">Lisuarte</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="simpl.0a.senornama">simpla<sncref/></ref>
    </refgrp>
   </snc>
  <trd lng="be">&c_b;&c_je;&c_d;&c_n;&c_y; <klr>(&c_n;&c_a;
&c_v;&c_y;&c_g;&c_l;&c_ja;&c_d;)</klr></trd>
