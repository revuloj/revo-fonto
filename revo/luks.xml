  </trdgrp>
  <trd lng="sq">i madh</trd>
  <trd lng="st">kholo</trd>
  <trd lng="sv">lyxig</trd>
  <trd lng="ta">&#xBAA;&#xBC6;&#xBB0;&#xBC1;&#xBAE;&#xBCD;</trd>
  <trd lng="te">&#xC17;&#xC4D;&#xC30;&#xC3E;&#xC02;&#xC21;&#xC4D;</trd>
  <trd lng="th">&#xE22;&#xE34;&#xE48;&#xE07;&#xE43;&#xE2B;&#xE0D;&#xE48;</trd>
  <trd lng="tl">malaki at maganda</trd>
  <trd lng="ur">&#x6AF;&#x631;&#x6CC;&#x646;&#x688;</trd>
  <trd lng="vi">l&#x1EDB;n</trd>
  <trd lng="yi">&#x5D2;&#x5E8;&#x5D5;&#x5D9;&#x5E1;</trd>
  <trd lng="zu">omkhulu</trd>
