<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>lapis</rad>/o
</kap>
<drv mrk="lapis.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Solida ar&gcirc;enta nitrato, uzata kiel ka&ubreve;terizilo, kaj anka&ubreve; nomata:
      infera &scirc;tono.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>H&ouml;llenstein</trd>,
     <trd>Silbernitrat</trd>
  </trdgrp>
  <trd lng="en">lunar caustic</trd>
  <trdgrp lng="nl">
    <trd>lapis</trd>,
    <trd>helse <ind>steen</ind></trd>
  </trdgrp>
  <trd lng="pt">l&aacute;pis infernal</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_ja;&c_p;&c_i;&c_s;</trd>,
    <trd>&c_n;&c_i;&c_t;&c_r;&c_a;&c_t; &c_s;&c_je;&c_r;&c_je;&c_b;&c_r;&c_a;</trd>
  </trdgrp>
</drv>
</art>
