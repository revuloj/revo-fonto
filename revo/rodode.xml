<vortaro>
<art mrk="$Id$">
<kap>
  <rad>rododendr</rad>/o
</kap>
<drv mrk="rodode.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="erikac.0oj">erikacoj</ref>
      (<trd lng="la">Rhododendron</trd>),
      montara kaj ornama planto.
    </dif>
  </snc>
  <bld lok="../bld/rodode.jpg">
    &gcirc;ardene kulturata <tld/>o
  </bld>
  <trd lng="be">&c_r;&c_a;&c_d;&c_a;&c_d;&c_e;&c_n;&c_d;&c_r;&c_a;&c_n;</trd>
  <trd lng="de">Rhododendron</trd>
  <trd lng="en">rhododendron</trd>
  <trdgrp lng="hu">
    <trd>havassz&eacute;pe</trd>,
    <trd>havasir&oacute;zsa</trd>,
    <trd>hangar&oacute;zsa</trd>,
    <trd>rododendron</trd>
  </trdgrp>
  <trd lng="nl">rododendron</trd>
  <trd lng="pt">rododendro</trd>
  <trd
lng="ru">&c_r;&c_o;&c_d;&c_o;&c_d;&c_je;&c_n;&c_d;&c_r;&c_o;&c_n;</trd>
</drv>
</art>
