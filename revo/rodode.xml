<art mrk="$Id: rodode.xml,v 1.21 2019/06/30 18:21:49 revo Exp $">
<kap>
  <rad>rododendr</rad>/o
</kap>
<drv mrk="rodode.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="erikac.0oj">erikacoj</ref>
      (<trd lng="la">Rhododendron</trd>),
      montara kaj ornama planto:
      <ekz>
        domo, &ccirc;irka&ubreve;ata de arboj kaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="https://www.monato.be/2003/007861.html"
              >Sekretaj vortoj de kura&gcirc;a presisto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        plantoj el la t.n. alpaj zonoj, kiuj vere riskas malaperi ka&ubreve;ze de la klimata
        &scirc;an&gcirc;i&gcirc;o: ili inkluzivas edelvejsojn, <tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2012/010754.php"
              >Protektanta biodiversecon</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/rodode.jpg">
    &gcirc;ardene kulturata <tld/>o
  </bld>
  <trd lng="be">&c_r;&c_a;&c_d;&c_a;&c_d;&c_e;&c_n;&c_d;&c_r;&c_a;&c_n;</trd>
  <trd lng="bg">&c_r;&c_o;&c_d;&c_o;&c_d;&c_je;&c_n;&c_d;&c_r;&c_o;&c_n;</trd>
  <trd lng="de">Rhododendron</trd>
  <trd lng="en">rhododendron</trd>
  <trdgrp lng="hu">
    <trd>havassz&eacute;pe</trd>,
    <trd>havasir&oacute;zsa</trd>,
    <trd>hangar&oacute;zsa</trd>,
    <trd>rododendron</trd>
  </trdgrp>
  <trd lng="nl">rododendron</trd>
  <trd lng="pt">rododendro</trd>
  <trd lng="ru">&c_r;&c_o;&c_d;&c_o;&c_d;&c_je;&c_n;&c_d;&c_r;&c_o;&c_n;</trd>
  <trd lng="fr">rhododendron</trd>
  <trd lng="sk">rododendron</trd>
  <trdgrp lng="cs">
    <trd>p&ecaron;ni&scaron;n&iacute;k</trd>,
    <trd>rododendron</trd>
  </trdgrp>
</drv>
</art>
