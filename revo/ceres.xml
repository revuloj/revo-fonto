<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: ceres.xml,v 1.2 2022/11/08 08:41:23 afido Exp $">
    <kap>
      <rad>Ceres</rad>/o
    </kap>

<drv mrk="ceres.0o">
  <kap><tld/>o</kap>
  <snc mrk="ceres.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Roma <ref tip="super" cel="di.0ino">diino</ref> de <ref cel="kultur.ter0o">terkulturo</ref>:
      <ekz>
        la senato aran&gcirc;is solena&jcirc;ojn kaj publikajn pre&gcirc;ojn al Vulkano, Cereso kaj
        Prozerpino
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        Saturno estas la edzo de Reo kaj patro de <tld/>o, Jupitero <klr>[&#8230;]</klr>
        interalie
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=136143"
              >Saturno (dio)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="cereal.0o">cerealo</ref>
    <trd lng="be">&c_C;&c_e;&c_r;&c_e;&c_r;&c_a; <klr>(&c_b;&c_a;&c_g;&c_ib;&c_n;&c_ja;)</klr></trd>
  </snc>
  <snc mrk="ceres.0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="super" cel="planed.nan0o">Nanplanedo</ref> nomita la&ubreve; la diino:
      <ekz>
        Asteroida zono en la <klr>[Asteroida]</klr> zono orbitas objektoj ekde
        malgrandaj, neregule formitaj pecetoj &gcirc;is la <klr>[&#8230;]</klr> preska&ubreve;planedo
        <tld/>o, kies diametro estas &ccirc;irka&ubreve; 945&#8239;km
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=85395"
              >Asteroida zono</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="pluton.0o.AST">Plutono<sncref/></ref>
    <trd lng="be">&c_C;&c_e;&c_r;&c_e;&c_r;&c_a; <klr>(&c_k;&c_a;&c_r;&c_l;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_ja; &c_p;&c_l;&c_ja;&c_n;&c_e;&c_t;&c_a;)</klr></trd>
  </snc>
  <trd lng="de">Ceres</trd>
  <trd lng="en">Ceres</trd>
  <trd lng="fr">C&eacute;r&egrave;s</trd>
</drv>

  </art>

