<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>karob</rad>/o
</kap>
<drv mrk="karob.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Frukto de <tld/>arbo.
    </dif>
 
  </snc>
  <trdgrp lng="de">
    <trd>Johannisbrot</trd>,
    <trd>Karobe</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>carob</trd>,
    <trd>St. John's bread</trd>
  </trdgrp>
</drv>
<drv mrk="karob.0arbo">
  <kap><tld/>arbo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio cesalpiniacoj (<trd lng="la">Ceratonia</trd>),
      el kies semoj oni produktas specon de faruno.
    </dif>
  </snc>
  <trd lng="de">Johannisbrotbaum</trd>
  <trd lng="en">carob tree</trd>
</drv>
</art>

