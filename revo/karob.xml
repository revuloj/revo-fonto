<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>karob</rad>/o
</kap>
<drv mrk="karob.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Frukto de <tld/>arbo.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Johannisbrot</trd>,
    <trd>Karobe</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>carob</trd>,
    <trd>St. John's bread</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>caroube</trd>,
    <trd>carouge</trd>
  </trdgrp>
  <trd lng="hu">szentj&aacute;noskeny&eacute;r</trd>
  <trd lng="nl">carobe</trd>
  <trd lng="ru">&c_r;&c_o;&c_zh;&c_o;&c_k;
<klr>(&c_f;&c_r;&c_u;&c_k;&c_t;)</klr></trd>
</drv>

<drv mrk="karob.0ujo0arbo">
  <kap><tld/>ujo, <tld/>arbo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio cesalpiniacoj (<trd lng="la">Ceratonia</trd>),
      el kies semoj oni produktas specon de faruno.
    </dif>
  </snc>
  <trd lng="de">Johannisbrotbaum</trd>
  <trd lng="en">carob tree</trd>
  <trd lng="fr">caroubier</trd>
  <trd lng="hu">szentj&aacute;noskeny&eacute;rfa</trd>
  <trd lng="nl">johannesbroodboom</trd>
  <trd lng="ru">&c_r;&c_o;&c_zh;&c_k;&c_o;&c_v;&c_o;&c_je;
&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
</drv>
</art>
