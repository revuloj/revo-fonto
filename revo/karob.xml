<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: karob.xml,v 1.20 2023/09/01 13:58:56 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>karob</rad>/o
</kap>
<drv mrk="karob.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="karob.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Frukto de <tld/>arbo:
      <ekz>
        li deziris plenigi sian ventron per la <tld/>oj, kiujn la porkoj
        man&gcirc;is; kaj neniu donis al li
        <fnt><bib>NT</bib>, <lok>&Luk; 15:16</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_zh;&c_o;&c_k; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="ca">garrofa</trd>
  <trd lng="cs">svatojansk&yacute; chl&eacute;b</trd>
  <trdgrp lng="de">
    <trd>Johannisbrot</trd>,
    <trd>Karobe</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>carob</trd>,
    <trd>St. John's bread</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>caroube</trd>,
    <trd>carouge</trd>
  </trdgrp>
  <trd lng="hu">szentj&aacute;noskeny&eacute;r</trd>
  <trd lng="ja">&#x30A4;&#x30CA;&#x30B4;&#x30DE;&#x30E1;</trd>
  <trdgrp lng="nl">
    <trd>carobe</trd>,
    <trd>johannesbroodpit</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>szara&nacute;czyn str&aogonek;kowy</trd>,
    <trd>karob</trd>,
    <trd>chleb &sacute;wi&eogonek;toja&nacute;ski</trd>
  </trdgrp> 
  <trd lng="ro">ro&#x219;cov</trd>
  <trd lng="ru">&c_r;&c_o;&c_zh;&c_o;&c_k; <klr>(&c_f;&c_r;&c_u;&c_k;&c_t;)</klr></trd>
  <trd lng="sk">sv&auml;toj&aacute;nsky chlieb</trd>
  <trd lng="tr">ke&ccedil;i boynuzu</trd>
  <trd lng="uk">&c_r;&c_ib;&c_zh;&#x43A;&c_i; (&c_ji;&c_s;&c_t;&c_ib;&c_v;&#x43D;&c_ib;)</trd>
</drv>

<drv mrk="karob.0ujo">
  <kap><tld/>ujo, <var><kap><tld/>arbo</kap></var></kap>
  <snc mrk="karob.0ujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio cesalpiniacoj (<trd lng="la">Ceratonia</trd>),
      el kies semoj oni produktas specon de faruno:
      <ekz>
        fronte al la 6-eta&gcirc;a konstrua&jcirc;o kie trovi&gcirc;as mia apartamento
        staras <tld/>-arbo preska&ubreve; 100-jara
        <fnt>
        <aut>drls</aut>
        <vrk>Re: Karobo (estis: nomoj de birdoj)</vrk>
        <lok>soc.culture.esperanto, 2005-02-19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_zh;&c_k;&c_o;&c_v;&c_a;&c_je; &c_d;&c_r;&c_e;&c_v;&c_a;</trd>
  <trd lng="ca">garrofer</trd>
  <trd lng="cs">rohovn&iacute;k obecn&yacute;</trd>
  <trd lng="de">Johannisbrotbaum</trd>
  <trd lng="en">carob tree</trd>
  <trd lng="fr">caroubier</trd>
  <trd lng="hu">szentj&aacute;noskeny&eacute;rfa</trd>
  <trd lng="ja">&#x30A4;&#x30CA;&#x30B4;&#x30DE;&#x30E1;</trd>
  <trdgrp lng="nl">
    <trd>johannesbroodboom</trd>,
    <trd>sint-jansbroodboom</trd>
  </trdgrp>
  <trd lng="pl">karobowe drzewo</trd>
  <trd lng="ro">ro&#x219;cov</trd>
  <trd lng="ru">&c_r;&c_o;&c_zh;&c_k;&c_o;&c_v;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  <trd lng="sk">rohovn&iacute;k obecn&yacute;</trd>
  <trd lng="uk">&c_r;&c_ib;&c_zh;&#x43A;&#x43E;&c_v;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&#x43E;</trd>
</drv>
</art>
