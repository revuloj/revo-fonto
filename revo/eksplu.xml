<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: eksplu.xml,v 1.24 2023/11/07 19:22:36 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>ekspluat</rad>/i
</kap>
<drv mrk="eksplu.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Uzadi ion, valorigante &gcirc;in por sia profito:
      <ekz>
        <tld/>i minejon, fervojon, firmon, gazeton;
      </ekz>
      <ekz>
        malbone <tld/>ita kolonio;
      </ekz>
      <ekz>
        <tld/>i bienon, fabrikon;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i sian kapablon;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;i ne scius <tld/>i sian influon al la princo
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 16</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i la publikan scivolemon.
      </ekz>
    </dif>
    <trd lng="de">abbauen <klr tip="ind"> (Rohstoffe)</klr></trd>
    <trdgrp lng="hu">
      <trd>kiakn&aacute;z</trd>,
      <trd>hasznos&iacute;t</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>eksploatowa&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="eksplu.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Troprofiti el ies laboro, ne donante justan kompenson:
      <ekz>
        <tld/>i proletaron;
      </ekz>
      <ekz>
        la premata kaj <tld/>ata popolo
        <fnt><bib>Far1</bib>, <lok>Anta&ubreve;parolo</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i la klientojn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sucx.el0i">elsu&ccirc;i</ref>,
      <ref cel="plum">senplumigi</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="ung.0ego">ungego</ref>.
    </refgrp>
    <trd lng="de">r&uuml;cksichtslos ausnutzen</trd>
    <trd lng="en">take unfair advantage of</trd>
    <trd lng="hu">kizs&aacute;km&aacute;nyol</trd>
    <trdgrp lng="pl">
      <trd>eksploatowa&cacute;</trd>,
      <trd>wykorzystywa&cacute;</trd>,
      <trd>wyzyskiwa&cacute;</trd>,
      <trd>zdziera&cacute;<klr> (pot.)</klr></trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Trouzi:
      <ekz>
        <tld/>i ies paciencon.
      </ekz>
    </dif>
    <trd lng="de">&uuml;berstrapazieren</trd>
    <trd lng="hu">vissza&eacute;l</trd>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_s;&c_p;&c_l;&c_ju;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">explotar</trd>
  <trdgrp lng="cs">
    <trd>od&#x159;&#xED;t</trd>,
    <trd>vyko&#x159;istit</trd>,
    <trd>vyko&#x159;is&#x165;ovat</trd>,
    <trd>vyu&#x17E;&#xED;t</trd>,
    <trd>vyu&#x17E;&#xED;vat</trd>,
    <trd>zneu&#x17E;&#xED;t</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ausnutzen</trd>,
    <trd>ausbeuten</trd>
  </trdgrp>
  <trd lng="en">exploit</trd>
  <trd lng="es">explotar</trd>
  <trd lng="fr">exploiter</trd>
  <trdgrp lng="ja">
    <trd>&#x958B;&#x767A;&#x3059;&#x308B;  <pr>&#x304B;&#x3044;&#x306F;&#x3064;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x3046;&#x307E;&#x304F;&#x5229;&#x7528;&#x3059;&#x308B;  <pr>&#x3046;&#x307E;&#x304F;&#x308A;&#x3088;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x643E;&#x53D6;&#x3059;&#x308B;  <pr>&#x3055;&#x304F;&#x3057;&#x3085;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x9177;&#x4F7F;&#x3059;&#x308B;  <pr>&#x3053;&#x304F;&#x3057;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="pl">wykorzystywa&cacute;</trd>
  <trd lng="pt">explorar</trd>
  <trd lng="ru">&c_e;&c_k;&c_s;&c_p;&c_l;&c_u;&c_a;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>dob&#xFD;va&#x165;</trd>,
    <trd>vykoris&#x165;ova&#x165;</trd>,
    <trd>vyu&#x17E;&#xED;va&#x165;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x435;&#x43A;&#x441;&#x43F;&#x43B;&#x443;&#x430;&#x442;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x438;&#x437;&#x438;&#x441;&#x43A;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x438;&#x43A;&#x43E;&#x440;&#x438;&#x441;&#x442;&#x43E;&#x432;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x434;&#x43E;&#x431;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x52CB;&#x7EE9; <pr>x&#x16B;nj&#x12B;</pr></trd>,
    <trd>&#x6718; <pr>ju&#x101;n</pr></trd>,
    <trd>&#x5F00;&#x77FF; <pr>k&#x101;iku&#xE0;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="eksplu.0egi">
  <kap><tld/>egi<fnt>B</fnt></kap>
  <snc>
    <dif>
      <tld lit="E"/>i 2 kaj 3.
    </dif>
    <trd lng="ca">abusar</trd>
    <trdgrp lng="pl">
      <trd>wykorzystywa&cacute;</trd>,
      <trd>wyzyskiwa&cacute;</trd>,
      <trd>zdziera&cacute;<klr> (pot.)</klr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x643E;&#x53D6;&#x3059;&#x308B;  <pr>&#x3055;&#x304F;&#x3057;&#x3085;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x9177;&#x4F7F;&#x3059;&#x308B;  <pr>&#x3053;&#x304F;&#x3057;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
</drv>
</art>
