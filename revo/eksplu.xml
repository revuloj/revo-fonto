<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: eksplu.xml,v 1.28 2025/11/01 20:28:25 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>ekspluat</rad>/i
<fnt><bib>JED</bib></fnt></kap>
<drv mrk="eksplu.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="eksplu.0i.elcxerpi">
    <dif>
      Uzadi ion, valorigante &gcirc;in por sia profito:
      <ekz>
        <tld/>i minejon, fervojon, firmon, gazeton;
      </ekz>
      <ekz>
        ri&ccirc;a kompanio <klr>[volis]</klr> <tld/>i la grandajn <klr>[&#x2026;]</klr> ri&ccirc;a&jcirc;ojn de
        lia lando
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;i ne scius <tld/>i sian influon al la princo
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 16</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxerp.el0i">el&ccirc;erpi</ref>,
      <ref cel="uz.el0i">eluzi</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_e;&c_k;&c_s;&c_p;&c_l;&c_ju;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_k;&c_a;&c_r;&c_y;&c_s;&c_t;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="ca">explotar</trd>
    <trd lng="de">abbauen <klr tip="ind"> (Rohstoffe)</klr></trd>
    <trdgrp lng="hu">
      <trd>kiakn&aacute;z</trd>,
      <trd>hasznos&iacute;t</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>eksploatowa&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="eksplu.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="profit.tro0i">Troprofiti</ref> el ies laboro,
      ne donante justan kompenson:
      <ekz>
        <tld/>i proletaron;
      </ekz>
      <ekz>
        la premata kaj <tld/>ata popolo
        <fnt><bib>Far1</bib>, <lok>Anta&ubreve;parolo</lok></fnt>;
      </ekz>
      <ekz>
        <nom>Szwejc</nom> <tld/>as la kompatindajn laboristinojn, kiuj <klr>[&#x2026;]</klr> pro granda
        mizero estas devigataj labori por pago plej bagatela
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i la klientojn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sucx.el0i">elsu&ccirc;i</ref>
    </refgrp>
    <trd lng="be">&c_e;&c_k;&c_s;&c_p;&c_l;&c_ju;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="ca">explotar</trd>
    <trd lng="de">r&uuml;cksichtslos ausnutzen</trd>
    <trd lng="en">take unfair advantage of</trd>
    <trd lng="hu">kizs&aacute;km&aacute;nyol</trd>
    <trdgrp lng="pl">
      <trd>eksploatowa&cacute;</trd>,
      <trd>wykorzystywa&cacute;</trd>,
      <trd>wyzyskiwa&cacute;</trd>,
      <trd>zdziera&cacute;<klr> (pot.)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="eksplu.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="uz.tro0i">Trouzi</ref>:
      <ekz>
        <tld/>i ies paciencon;
      </ekz>
      <ekz>
        fripono <klr>[&#x2026;]</klr> rapide <tld/>is la naivon de <nom>Kandid</nom>
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi sciis bone <tld/>i mian situacion
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_e;&c_k;&c_s;&c_p;&c_l;&c_ju;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="ca">
      <trd>explotar</trd>,
      <trd>abusar (de)</trd>
    </trdgrp>
    <trd lng="de">&uuml;berstrapazieren</trd>
    <trd lng="hu">vissza&eacute;l</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>od&rcaron;&iacute;t</trd>,
    <trd>vyko&rcaron;istit</trd>,
    <trd>vyko&rcaron;is&tcaron;ovat</trd>,
    <trd>vyu&zcaron;&iacute;t</trd>,
    <trd>vyu&zcaron;&iacute;vat</trd>,
    <trd>zneu&zcaron;&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ausnutzen</trd>,
    <trd>ausbeuten</trd>
  </trdgrp>
  <trd lng="en">exploit</trd>
  <trd lng="es">explotar</trd>
  <trd lng="fr">exploiter</trd>
  <trdgrp lng="ja">
    <trd>&#x958B;&#x767A;&#x3059;&#x308B;  <pr>&#x304B;&#x3044;&#x306F;&#x3064;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x3046;&#x307E;&#x304F;&#x5229;&#x7528;&#x3059;&#x308B;  <pr>&#x3046;&#x307E;&#x304F;&#x308A;&#x3088;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x643E;&#x53D6;&#x3059;&#x308B;  <pr>&#x3055;&#x304F;&#x3057;&#x3085;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x9177;&#x4F7F;&#x3059;&#x308B;  <pr>&#x3053;&#x304F;&#x3057;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="pl">wykorzystywa&cacute;</trd>
  <trd lng="pt">explorar</trd>
  <trd lng="ru">&c_e;&c_k;&c_s;&c_p;&c_l;&c_u;&c_a;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>dob&yacute;va&tcaron;</trd>,
    <trd>vykoris&tcaron;ova&tcaron;</trd>,
    <trd>vyu&zcaron;&iacute;va&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_je;&c_k;&c_s;&c_p;&c_l;&c_u;&c_a;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_z;&c_i;&c_s;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_k;&c_o;&c_r;&c_i;&c_s;&c_t;&c_o;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_d;&c_o;&c_b;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x52CB;&#x7EE9; <pr>x&umacron;nj&imacron;</pr></trd>,
    <trd>&#x6718; <pr>ju&amacron;n</pr></trd>,
    <trd>&#x5F00;&#x77FF; <pr>k&amacron;iku&agrave;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="eksplu.0egi">
  <kap><tld/>egi</kap>
  <snc>
    <dif>
      Ekspluati <sncref ref="eksplu.0i.EKON"/> a&ubreve; <sncref ref="eksplu.0i.FIG"/>:
      <ekz>
        ruzuloj pretaj <tld/>egi la situacion por pliri&ccirc;i&gcirc;i
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2013/010777.php"
              >&Gcirc;is la tero tremas</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">abusar</trd>
    <trdgrp lng="pl">
      <trd>wykorzystywa&cacute;</trd>,
      <trd>wyzyskiwa&cacute;</trd>,
      <trd>zdziera&cacute;<klr> (pot.)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_s;&c_p;&c_l;&c_ju;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_p;&c_r;&c_a;&c_z;&c_mol;&c_m;&c_je;&c_r;&c_n;&c_a;)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x643E;&#x53D6;&#x3059;&#x308B;  <pr>&#x3055;&#x304F;&#x3057;&#x3085;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x9177;&#x4F7F;&#x3059;&#x308B;  <pr>&#x3053;&#x304F;&#x3057;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
</drv>
</art>
