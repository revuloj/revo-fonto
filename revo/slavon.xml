<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Slavon</rad>/o<fnt>PIV</fnt></kap>
    <drv mrk="slavon.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">LIN</uzo>
      <uzo tip="fak">HIS</uzo>
       <snc>
        <dif>
          Literatura lingvo de la ortodokse kristanaj slavoj kaj
          rumanoj en 9-17 jc, plu uzata kiel lingvo religia
          (kaj tial iam misnomata <ctl>eklezi-slava</ctl>).
        </dif>
      </snc>
      <trdgrp lng="de">
         <trd>Altkirchenslawisch</trd>,
         <trd>Altbulgarisch</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>Slavonic</trd>,
        <trd>Old Church Slavonic</trd></trdgrp>
      <trd lng="fr">Slavon</trd>
      <trdgrp lng="hu">
        <trd>&oacute;szl&aacute;v</trd>,
        <trd>egyh&aacute;zi <ind>szl&aacute;v</ind></trd></trdgrp>
      <trdgrp lng="ru">
        <trd>&c_s;&c_t;&c_a;&c_r;&c_o;&c_s;&c_l;&c_a;&c_v;&c_ja;&c_n;&c_s;&c_k;&c_i;&c_j; <klr>(&c_ja;&c_z;&c_y;&c_k;)</klr></trd>,
        <trd>&c_c;&c_je;&c_r;&c_k;&c_o;&c_v;&c_n;&c_o;-&c_s;&c_l;&c_a;&c_v;&c_ja;&c_n;&c_s;&c_k;&c_i;&c_j;</trd></trdgrp>
    </drv>
  </art>

