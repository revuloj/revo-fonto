<art mrk="$Id: kariof1.xml,v 1.16 2025/04/02 02:02:17 revo Exp $">
<kap>
  <rad>kariofilac</rad>/oj
</kap>
<drv mrk="kariof1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>
      herboj (<trd lng="la">Caryophyllaceae</trd>), al kiu apartenas
      <refgrp tip="sub">
        <ref cel="stelari.0o">stelario</ref>,
        <ref cel="diant.0o">dianto</ref>,
      </refgrp>
      sed ne <ref cel="kariof.0ujo">kariofilujo</ref>:
      <ekz>
        la <tld/>oj <klr>[&#8230;]</klr> konsistas el 1750 specioj en 88 genroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=79915">Kariofilacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_v;&c_a;&c_z;&c_mol;&c_d;&c_z;&c_ib;&c_k;&c_o;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>,
    <trd>&c_ib;&c_s;&c_k;&c_a;&c_r;&c_k;&c_o;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  </trdgrp>
  <trd lng="ca">cariofil&middot;l&agrave;cies</trd>
  <trd lng="de">Nelkengew&auml;chse</trd>
  <trdgrp lng="en">
    <trd>Caryophyllaceae</trd>,
    <trd><ind>pink</ind> family <klr>(of flowering plants)</klr></trd>,
    <trd><ind>carnation</ind> family <klr>(of flowering plants)</klr></trd>
  </trdgrp>
  <trd lng="fr">caryophyllac&eacute;es</trd>
  <trd lng="hu">szegf&udblac;f&eacute;l&eacute;k</trd>
  <trd lng="ja">&#12490;&#12487;&#12471;&#12467;&#31185;  <pr>&#12490;&#12487;&#12471;&#12467;&#12363;</pr></trd>
  <trdgrp lng="nl">
    <trd>muurachtigen</trd>,
    <trd>anjerfamilie</trd>,
    <trd>anjelierachtigen</trd>
  </trdgrp>
  <trd lng="pl">go&zacute;dzikowate</trd>
  <trd lng="pt">cariofil&aacute;ceas</trd>
  <trd lng="ro">cariofilaceele</trd>
  <trd lng="ru">&c_g;&c_v;&c_o;&c_z;&c_d;&c_i;&c_ch;&c_n;&c_y;&c_je;</trd>
  <trd lng="uk">&c_g;&c_v;&c_o;&c_z;&c_d;&c_i;&c_ch;&c_n;&c_ib;</trd>
</drv>
</art>

