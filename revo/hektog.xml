<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hektog.xml,v 1.22 2023/11/15 16:45:18 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>hektogram</rad>/o
</kap>
<drv mrk="hektog.0o">
  <kap>
    <tld/>o
  </kap>
  <mlg kod="SI">hg</mlg>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:mezurunuoj" cel="unu.mezur0o.SCI">Mezurunuo</ref> de maso,
      egala al cent <ref tip="prt" cel="gram.0o">gramoj</ref>:
      <ekz>
        dum la buitia inicado la glutitaj kvantoj estas grandegaj, &gcirc;is pluraj <tld/>oj
        da raspitaj radikoj, kaj tio povas superi 5 gramojn da pura alkaloido
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=148638">Ibogao</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>                        
  <trd lng="be">&c_g;&c_je;&c_k;&c_t;&c_a;&c_g;&c_r;&c_a;&c_m;</trd>
  <trd lng="bg">&c_h;&c_je;&c_k;&c_t;&c_o;&c_g;&c_r;&c_a;&c_m;</trd>
  <trd lng="br">hektogram(ad)</trd>
  <trd lng="de">Hektogramm</trd>
  <trd lng="el">&epsilon;&kappa;&alfa;&tau;&omikron_ton;&gamma;&rho;&alfa;&my;&my;&omikron;</trd>
  <trd lng="en">hectogram</trd>
  <trd lng="es">hect&oacute;gramo</trd>
  <trd lng="fa">&a_h;&f_k;&a_t;&a_w;&f_g;&a_r;&a_m;</trd>
  <trd lng="fr">hectogramme</trd>
  <trd lng="hu">sz&aacute;z <ind>gramm</ind></trd>
  <trd lng="it">ettogrammo</trd>
  <trd lng="ja">&#x30D8;&#x30AF;&#x30C8;&#x30B0;&#x30E9;&#x30E0;</trd>
  <trd lng="nl">hectogram</trd>
  <trd lng="pt">hectograma</trd>
  <trd lng="ru">&c_g;&c_je;&c_k;&c_t;&c_o;&c_g;&c_r;&c_a;&c_m;&c_m;</trd>
  <trd lng="uk">&#x433;&#x435;&#x43A;&#x442;&#x43E;&#x433;&#x440;&#x430;&#x43C;</trd>
  <trdgrp lng="zh">
    <trd>&#x5161; <pr>b&#x1CE;i</pr></trd>,
    <trd>&#x516C;&#x4E24; <pr>g&#x14D;ngli&#x1CE;ng</pr></trd>
  </trdgrp>
</drv>
</art>
