<drv mrk="facil.mal0igi">
   <kap>mal<tld/>igi</kap>
   <gra><vspec>tr</vspec></gra>
   <snc>
     <dif>
       Ka&ubreve;zi pli da peno, aldoni barojn kaj obstaklojn:
       <ekz>
         altaj muroj mal<tld/>igis la eniron
         <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 18a</lok></fnt>;
       </ekz>
      <ekz>
        ondo da pasantoj <klr>[&#8230;]</klr> mal<tld/>igis al &scirc;i la kuradon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        neniam mi vidis &scirc;in tiel gaja: tio pli mal<tld/>igos mian taskon
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Oka</lok>
        </fnt>;
      </ekz>
      <ekz>
        via familio eble mal<tld/>igus tiun edzini&gcirc;on
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekdua</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne deziris mal<tld/>igi vivon de la onklino
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la vivo estas simpla kaj <tld/>a, li pensis, sed la homoj mal<tld/>igas &gcirc;in
        <fnt>
          <bib>Tor</bib>
          <lok>III.</lok>
        </fnt>.
      </ekz>
    </dif>
     <refgrp tip="sub">
       <ref cel="embara.0i">embarasi</ref>,
       <ref cel="gxen.0i">&gcirc;eni</ref>,
       <ref cel="handikap.0i">handikapi</ref>,
       <ref cel="implik.0i">impliki</ref>,
       <ref cel="help.mal0i">malhelpi</ref>,
       <ref cel="oportu.mal0i">maloportuni</ref>,
       <ref cel="obstak.0i">obstakli</ref>
     </refgrp>
   </snc>
   <trd lng="br">diaesaat<klr tip="ind"> (v.k.)</klr></trd>
   <trd lng="ca">dificultar</trd>
   <trdgrp lng="de">
     <trd>erschweren</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>rendre <ind>difficile</ind></trd>,
     <trd>faire <ind>obstacle</ind></trd>,
     <trd>s'<ind>opposer</ind>
     &agrave;<klr tip="amb"> (faire obstacle)</klr></trd>
   </trdgrp>
   <trdgrp lng="id">
     <trd>memper<ind>sukar</ind></trd>,
     <trd>memper<ind>sulit</ind></trd>,
     <trd>memper<ind>susah</ind></trd>
   </trdgrp>
   <trd lng="id"><baz>sukar</baz>menyukarkan</trd>
