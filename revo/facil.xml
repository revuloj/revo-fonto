<drv mrk="facil.0pieda">
   <kap><tld/>pieda</kap>
   <snc>
     <dif>
       <tld lit="F"/>e movanta la piedojn, <ref tip="super" cel="lert.0a.korpo">lerta</ref> en
       irado:
      <ekz>
        dorm' anoncas sin, &#11005; iras la son&gcirc;oj, la belaj, &#11005; <tld/>piede en nin
        <fnt>
          <aut>Georg Heym, trad. Hans-Georg Kaiser</aut>
          <vrk><url ref="http://www.poezio.net/version?poem-id=169&amp;version-id=,337"
              >&Ccirc;iuj pejza&gcirc;oj</url></vrk>
        </fnt>.
      </ekz>
     </dif>
    <refgrp tip="vid">
      <ref cel="lert.0a.korpo">lerta<sncref/></ref>,
      <ref cel="vigl.0a.rapidmova">vigla<sncref/></ref>
    </refgrp>
   </snc>
   <trdgrp lng="br">
     <trd>mibin a droad</trd>,
     <trd>ska&ntilde;v e c'har</trd>
   </trdgrp>
   <trd lng="ca">de cames lleugeres</trd>
