<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>facil</rad>/a <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="facil.0a">
   <kap><ofc>*</ofc><tld/>a</kap>
   <snc mrk="facil.0a.farebla">
     <dif>
       Senpene farebla:
       <ekz>
         <tld/>a laboro<fnt>B</fnt>,
       </ekz>
       <ekz>
         tasko, problemo, lingvo, vojo<fnt>B</fnt>, spiro;
       </ekz>
       <ekz>
         <tld/>aj versoj
         <klr>(kiuj ŝajnas senpene faritaj kaj fluantaj)</klr>.
       </ekz>
     </dif>
     <trdgrp lng="ca">
       <trd>senzill</trd>,
       <trd>simple</trd>
     </trdgrp>
     <trdgrp lng="id">
       <trd>gampang</trd>,
       <trd>mudah</trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>лёгкий</trd>,
       <trd>нетрудный</trd>
     </trdgrp>
   </snc>
   <snc mrk="facil.0a.eltenebla">
     <dif>
       Senpene eltenebla, ne peza, apenaŭ sentebla, neintensa:
       <ekz>
         <tld/>a <klr>(milda)</klr> vento<fnt>Z</fnt>,
         klimato, riproĉo;
       </ekz>
       <ekz>
         <tld/>a <klr>(neforta)</klr> rideto,
         saluto<fnt>Z</fnt>, dormo.
       </ekz>
     </dif>
     <trdgrp lng="ca">
       <trd>lleuger</trd>,
       <trd>tènue</trd>,
       <trd>suau</trd>
     </trdgrp>
     <trdgrp lng="id">
       <trd>lembut</trd>,
       <trd>ringan</trd>
     </trdgrp>
     <trd lng="ru">лёгкий <klr>
     (в переносном
     смысле)</klr></trd>
   </snc>
   <trd lng="be">лёгкі</trd>
   <trd lng="bg">лесно</trd>
   <trdgrp lng="br">
     <trd>aes<klr tip="ind"> (ag.)</klr></trd>,
     <trd>aezet<klr tip="ind"> (ag.)</klr></trd>
   </trdgrp>
   <trd lng="ca">fàcil</trd>
   <trdgrp lng="cs">
     <trd>hladký</trd>,
     <trd>lehký</trd>,
     <trd>nenáhlý</trd>,
     <trd>nenáročný</trd>,
     <trd>snadný</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>einfach</trd>,
     <trd>leicht</trd>
   </trdgrp>
   <trd lng="en">easy</trd>
   <trd lng="es">fácil</trd>
   <trdgrp lng="fr">
     <trd>aisé<klr tip="ind"> (facile)</klr></trd>,
     <trd>facile</trd>,
     <trd>léger</trd>
   </trdgrp>
   <trdgrp lng="fy">
     <trd>maklik</trd>,
     <trd>ienfâldich</trd>
   </trdgrp>
   <trd lng="he">קל</trd>
   <trdgrp lng="hu">
     <trd>könnyű</trd>,
     <trd>könnyed</trd>,
     <trd>enyhe</trd>,
   </trdgrp>
   <trd lng="io">facila</trd>
   <trdgrp lng="nl">
     <trd>gemakkelijk</trd>,
     <trd>eenvoudig</trd>
   </trdgrp>
   <trd lng="oc">aisit</trd>
   <trdgrp lng="pl">
     <trd>lekki</trd>,
     <trd>łatwy</trd>
   </trdgrp>
   <trd lng="pt">fácil</trd>
   <trdgrp lng="sk">
     <trd>snadný</trd>,
     <trd>ľahký</trd>
   </trdgrp>
   <trdgrp lng="sv">
     <trd>enkel</trd>,
     <trd>lätt</trd>
   </trdgrp>
   <trd lng="tr">kolay</trd>
</drv>

<drv mrk="facil.0e">
   <kap><ofc>*</ofc><tld/>e</kap>
   <snc>
     <dif>
       Laŭ <tld/>a maniero:
       <ekz>
         <tld/>e malvarmumi
         <fnt><bib>F</bib>, <lok>Ekzercaro, ¶ 42</lok></fnt>;
       </ekz>
       <ekz>
         <tld/>e forgesi, pardoni, paroli;
       </ekz>
       <ekz>
         Esperanto estas <tld/>e lernebla.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">лёгка</trd>
   <trd lng="bg">лесно</trd>
   <trdgrp lng="br">
     <trd>aes<klr tip="ind"> (adv.)</klr></trd>,
     <trd>aezet<klr tip="ind"> (adv.)</klr></trd>
   </trdgrp>
   <trd lng="ca">fàcilment</trd>
   <trd lng="cs">facile</trd>
   <trdgrp lng="de">
     <trd>einfach</trd>,
     <trd>leicht</trd>
   </trdgrp>
   <trd lng="en">easily</trd>
   <trd lng="es">fácilmente</trd>
   <trd lng="fr">facilement</trd>
   <trdgrp lng="fy">
     <trd>maklik</trd>,
     <trd>ienfâldich</trd>
   </trdgrp>
   <trd lng="he">ב<ind>קלות</ind></trd>
   <trdgrp lng="hu">
     <trd>könnyen</trd>,
     <trd>könnyedén</trd>
   </trdgrp>
   <trd lng="id">dengan <ind>mudah</ind></trd>
   <trd lng="io">facile</trd>
   <trdgrp lng="nl">
     <trd>gemakkelijk</trd>,
     <trd>eenvoudig</trd>
   </trdgrp>
   <trdgrp lng="pl">
     <trd>lekko</trd>,
     <trd>łatwo</trd>,
     <trd>łacno</trd>,
     <trd>z <ind>łatwością</ind></trd>
   </trdgrp>
   <trd lng="pt">facilmente</trd>
   <trdgrp lng="ru">
     <trd>легко</trd>,
     <trd>с лёгкостью</trd>
   </trdgrp>
   <trd lng="sk">ľahko</trd>
   <trdgrp lng="sv">
     <trd>enkelt</trd>,
     <trd>lätt</trd>
   </trdgrp>
   <trd lng="tr">kolayca</trd>
</drv>

<drv mrk="facil.0igi">
   <kap><tld/>igi</kap>
   <gra><vspec>tr</vspec></gra>
   <snc mrk="facil.0igi.farebligi">
     <dif>
       Igi ion <tld/>e farebla:
       <ekz>
         <tld/>igi al iu la laboron.
       </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>аблегчыць</trd>,
       <trd>спрасьціць</trd>
     </trdgrp>
     <trd lng="bg">улесняване на</trd>
     <trd lng="ca">facilitar</trd>
     <trd lng="en">facilitate</trd>
     <trd lng="es">facilitar</trd>
     <trdgrp lng="id">
       <trd>[<ind>mudah</ind>] memudahkan</trd>,
       <trd>[<ind>mudah</ind>] mempermudah</trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>облегчить</trd>,
       <trd>упростить</trd>
     </trdgrp>
     <trdgrp lng="nl">
       <trd>vergemakkelijken</trd>,
       <trd>vereenvoudigen</trd>
     </trdgrp>
     <trdgrp lng="pl">
       <trd>ułatwiać</trd>,
       <trd>upraszczać</trd>
     </trdgrp>
     <trdgrp lng="sv">
       <trd>underlätta</trd>,
       <trd>förenkla</trd>
     </trdgrp>
     <trd lng="pt">facilitar</trd>
   </snc>
   <snc mrk="facil.0igi.mildigi">
     <dif>
       Mildigi, kvietigi:
       <ekz>
         <tld/>igi la koron;
       </ekz>
       <ekz>
         rakontu al mi vian malĝojon, per tio vi eble
         <tld/>igos vian animon<fnt>Z</fnt>.
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="pez">malpezigi</ref>,
       <ref cel="sxargx">senŝarĝigi</ref>
     </refgrp>
     <refgrp tip="vid">
       <ref cel="ricev.0i">ricevi</ref>.
     </refgrp>
     <trdgrp lng="be">
       <trd>зьмякчыць</trd>,
       <trd>аблегчыць <klr>
       (душу)</klr></trd>
     </trdgrp>
     <trd lng="ca">alleugerir</trd>
     <trd lng="en">ease</trd>
     <trd lng="id">[<ind>ringan</ind>] meringankan</trd>
     <trd lng="nl">verzachten</trd>
     <trd lng="pt">aquietar</trd>
     <trdgrp lng="pl">
       <trd>ulżyć</trd>,
       <trd>przynosić <ind>ulgę</ind></trd>,
       <trd>zrzucać kamień <klr> (np. z serca)</klr></trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>смягчить</trd>,
       <trd>облегчить <klr>
       (душу)</klr></trd>
     </trdgrp>
     <trd lng="sv">lätta</trd>
   </snc>
   <trd lng="br">aesaat<klr tip="ind"> (v.k.)</klr></trd>
   <trdgrp lng="cs">
     <trd>usnadnit</trd>,
     <trd>zjednodušit</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>vereinfachen</trd>,
     <trd>erleichtern</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>alléger</trd>,
     <trd>atténuer</trd>,
     <trd>faciliter</trd>,
     <trd>simplifier</trd>,
     <trd>soulager</trd>
   </trdgrp>
   <trd lng="he">לה<ind>קל</ind></trd>
   <trdgrp lng="hu">
     <trd>könnyít</trd>,
     <trd>megkönnyít</trd>
   </trdgrp>
   <trd lng="io">faciligar</trd>
   <trdgrp lng="sk">
     <trd>poľahčiť</trd>,
     <trd>uľahčiť</trd>
   </trdgrp>
   <trdgrp lng="tr">
     <trd>kolaylaştırmak</trd>,
     <trd>basitleştirmek</trd>
   </trdgrp>
</drv>

<drv mrk="facil.0igxi">
   <kap><tld/>iĝi</kap>
   <gra><vspec>ntr</vspec></gra>
   <snc mrk="facil.0igxi.pene">
     <dif>
       Iĝi <tld/>a, malpli peniga ol antaŭe:
       <ekz>
         mi neniam sugestis ke la tasko <tld/>iĝos
         <fnt>
           <aut>K. Enderby</aut>,
           <vrk><url
ref="http://esperanto-ondo.ru/Ind-jaro.htm#1999-intervjuo">
           Ni ne devas esti sen influo...</url></vrk>,
           <lok>La Ondo de Esperanto, 2000:2 (64)</lok>
         </fnt>.
       </ekz>
     </dif>
     <trd lng="fr">devenir <ind>facile</ind></trd>
   </snc>
   <snc mrk="facil.0igxi.malpeze">
     <dif>
       Iĝi <tld/>a, malpli peza, malpli prema:
       <ekz>
         nun tute <tld/>iĝis lia koro
         <fnt>
           <aut>M. Saltykov-Ŝĉedrin, trad. S. Pokrovskij</aut>,
           <vrk>La trompulo gazetisto kaj la kredema leganto</vrk>,
           <lok>La Ondo de Esperanto, 2000:7 (69)</lok>
         </fnt>.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="pez.mal0igxi.KOMUNE">malpeziĝi</ref>,
       <ref cel="sxargx.sen0igxi.KOMUNE">senŝarĝiĝi</ref>
     </refgrp>
     <trd lng="fr">s'<ind>alléger</ind></trd>
   </snc>
   <trd lng="io">facileskar</trd>
</drv>

<drv mrk="facil.mal0a">
   <kap>mal<tld/>a</kap>
   <snc>
     <dif>
       Kaŭzanta penon, peza, malagrable intensa:
       <ekz>
         la brusto leviĝadis per mal<tld/>aj ĝemoj
         <fnt><bib>Marta</bib></fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="bg">трудно</trd>
   <trd lng="br">diaes</trd>
   <trd lng="ca">difícil</trd>
   <trdgrp lng="cs">
     <trd>nesnadný</trd>,
     <trd>obtížný</trd>,
     <trd>těžký <klr>(duševně)</klr></trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>schwer</trd>,
     <trd>schwierig</trd>
   </trdgrp>
   <trd lng="es">difícil</trd>
   <trd lng="fr">difficile</trd>
   <trdgrp lng="id">
     <trd>sukar</trd>,
     <trd>sulit</trd>,
     <trd>susah</trd>
   </trdgrp>
   <trd lng="io">desfacila</trd>
   <trd lng="nl">moeilijk</trd>
   <trd lng="pl">trudny</trd>
   <trd lng="pt">difícil</trd>
   <trdgrp lng="ru">
     <trd>трудный</trd>,
     <trd>сложный<klr tip="ind">
     (трудный)</klr></trd>,
     <trd>затруднительный</trd>
   </trdgrp>
   <trdgrp lng="sk">
     <trd>neľahký</trd>,
     <trd>obtiažny</trd>
   </trdgrp>
   <trd lng="tr">zor</trd>
</drv>

<drv mrk="facil.mal0ajxo">
   <kap>mal<tld/>aĵo</kap>
   <snc>
     <dif>
       Io pene farebla aŭ plenumebla; malhelpaĵo:
       <ekz>
         la mal<tld/>aĵoj kaj problemoj de la arto aperadis tiam
         antaŭ ŝi
         <fnt><bib>Marta</bib></fnt>.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="bar.0o">baro</ref>,
       <ref cel="dorn.0o">dorno</ref>
     </refgrp>
   </snc>
   <trd lng="be">цяжкасьць</trd>
   <trd lng="bg">трудност</trd>
   <trdgrp lng="br">
     <trd>diaester</trd>,
     <trd>diaezamant</trd>
   </trdgrp>
   <trd lng="ca">dificultat</trd>
   <trd lng="de">Schwierigkeit</trd>
   <trd lng="en">difficulty</trd>
   <trd lng="es">dificultad</trd>
   <trdgrp lng="fr">
     <trd>difficulté</trd>,
     <trd>problème<klr tip="ind"> (difficulté)</klr></trd>,
     <trd>complication</trd>
   </trdgrp>
   <trdgrp lng="id">
     <trd>[<ind>sukar</ind>] kesukaran</trd>,
     <trd>[<ind>sulit</ind>] kesulitan</trd>,
     <trd>[<ind>susah</ind>] kesusahan</trd>
   </trdgrp>
   <trd lng="io">desfacilajo</trd>
   <trd lng="nl">moeilijkheid</trd>
   <trd lng="hu">nehézség</trd>
   <trd lng="pl">trudność</trd>
   <trdgrp lng="ru">
     <trd>затруднение
     </trd>,
     <trd>трудность</trd>
   </trdgrp>
   <trd lng="sv">svårighet</trd>
   <trd lng="pt">dificuldade</trd>
   <trdgrp lng="tr">
     <trd>zorluk</trd>,
     <trd>problem</trd>
   </trdgrp>
</drv>

<drv mrk="facil.mal0igi">
   <kap>mal<tld/>igi</kap>
   <gra><vspec>tr</vspec></gra>
   <snc>
     <dif>
       Kaŭzi pli da peno, aldoni barojn kaj obstaklojn:
       <ekz>
         altaj muroj mal<tld/>igis la eniron
         <fnt><bib>Far1</bib>, <lok>ĉapitro 18a</lok></fnt>.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="gxen.0i">ĝeni</ref>,
       <ref cel="help.mal0i">malhelpi</ref>
     </refgrp>
   </snc>
   <trd lng="br">diaesaat<klr tip="ind"> (v.k.)</klr></trd>
   <trd lng="ca">dificultar</trd>
   <trdgrp lng="de">
     <trd>erschweren</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>rendre <ind>difficile</ind></trd>,
     <trd>faire <ind>obstacle</ind></trd>,
     <trd>s'<ind>opposer</ind>
     à<klr tip="amb"> (faire obstacle)</klr></trd>
   </trdgrp>
   <trdgrp lng="id">
     <trd>[<ind>sukar</ind>] menyukarkan</trd>,
     <trd>[<ind>sukar</ind>] mempersukar</trd>,
     <trd>[<ind>sulit</ind>] menyulitkan</trd>,
     <trd>[<ind>sulit</ind>] mempersulit</trd>,
     <trd>[<ind>susah</ind>] menyusahkan</trd>,
     <trd>[<ind>susah</ind>] mempersusah</trd>
   </trdgrp>
   <trd lng="io">desfaciligar</trd>
   <trdgrp lng="nl">
     <trd>bemoeilijken</trd>,
     <trd>moeilijk maken</trd>
   </trdgrp>
   <trd lng="pl">utrudniać</trd>
   <trd lng="pt">dificultar</trd>
   <trd lng="ru">затруднить</trd>
   <trd lng="tr">zorlaştırmak</trd>
</drv>

<drv mrk="facil.0pieda">
   <kap><tld/>pieda</kap>
   <snc>
     <dif>
       <tld lit="F"/>e movanta la piedojn, <ref tip="super"
       cel="lert.0a.korpo">lerta</ref> en irado.
     </dif>
   </snc>
   <trdgrp lng="br">
     <trd>mibin a droad</trd>,
     <trd>skañv e c'har</trd>
   </trdgrp>
   <trd lng="ca">de cames lleugeres</trd>
   <trd lng="cs">mrštný</trd>
   <trd lng="de">leichtfüßig</trd>
   <trd lng="en">easy-going</trd>
   <trdgrp lng="fr">
     <trd>agile</trd>,
     <trd>alerte<klr tip="ind"> (ingambe)</klr></trd>,
     <trd>ingambe</trd>
   </trdgrp>
   <trd lng="he">קל רגליים</trd>
   <trdgrp lng="hu">
     <trd>könnyű léptű</trd>,
     <trd>könnyed járású</trd>
   </trdgrp>
   <trdgrp lng="id">
     <trd>gesit</trd>,
     <trd>lincah</trd>,
     <trd>tangkas</trd>
   </trdgrp>
   <trd lng="nl">lichtvoetig</trd>
   <trdgrp lng="pl">
     <trd>szybkonogi</trd>,
     <trd>lekkostopy</trd>
   </trdgrp>
   <trdgrp lng="pt">
     <trd>lesto</trd>,
     <trd>lépido</trd>,
     <trd>ágil</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>легконогий</trd>,
     <trd>быстроногий</trd>
   </trdgrp>
   <trd lng="sk">ľahkonohý</trd>
</drv>
</art>
