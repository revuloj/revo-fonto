<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: artik.xml,v 1.57 2023/10/28 08:33:09 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>artik</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="artik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="artik.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Loko, kie kunligi&gcirc;as du ostoj a&ubreve; membroj, tiel ke ili
      povas movi&gcirc;i unu rilate al alia:
      <ekz>
        <ind>pied<tld/>o</ind>
        <fnt><bib>Fab2</bib>, <lok>ru&gcirc;aj &scirc;uoj</lok></fnt>;
        <trd lng="br">mell an ufern</trd>
      </ekz>
      <ekz>
        la tendeno <klr>[&#x2026;]</klr> sur la <tld/>o de la femuro
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 32:32</lok>
        </fnt>;
      </ekz>
      <ekz>
        vertebra <tld/>o;
      </ekz>
      <ekz>
        tordu miajn <tld/>ojn, se mi ne diras la veron
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&gcirc;i]</klr> ricevis artriton en unu el la flugil<tld/>oj
        <fnt>
          <bib>Fab4</bib>
          <lok>Sterkskarabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        eleganta piedo, kies <tld/>o estas volvita de blanka banda&gcirc;o
         <fnt>
          <aut>Shi Chengtai</aut>
          <vrk><url ref="&Monato;/1995/A950601.php">La kompensa kiso</url></vrk>
          <bib>Monato</bib>
          <lok>1995/06, p. 27</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">articulaci&ograve;</trd>
    <trdgrp lng="en">
      <trd>joint</trd>,
      <trd>articulation</trd>
    </trdgrp>
    <trd lng="hu">iz&uuml;let</trd>
    <trdgrp lng="it">
      <trd>giuntura</trd>,
      <trd>articolazione</trd>
    </trdgrp>
    <trd lng="pl">staw</trd>
    <trd lng="pt">articula&ccedil;&atilde;o</trd>
    <trd lng="ro">articula&#x21B;ie</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_u;&c_s;&c_t;&c_a;&c_v;</trd>,
      <trd>&c_s;&c_o;&c_ch;&c_l;&c_je;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x6D3B;&#x7BC0; <pr>hu&#xF3;ji&#xE9;</pr></trd>,
    <trd>&#x6D3B;&#x8282; <pr>hu&#xF3;ji&#xE9;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="artik.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Me&hcirc;anika kunligo de du pecoj, kiuj povas movi&gcirc;i
      &ccirc;irka&ubreve; la punkto de kunligo:
      <ekz>
        unu viro sen ia celo stre&ccirc;is la pafarkon, kaj pafe trafis la re&gcirc;on de <nom>Izrael</nom>
        inter la <tld/>oj de la kiraso
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 22:34</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="it">
      <trd>giuntura</trd>,
      <trd>giunzione</trd>,
      <trd>giunto</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>zawiasa</trd>,
      <trd>przegub</trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>balama</trd>,
      <trd>&#x219;arnier&abreve;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x4EBA;&#x5DE5;&#x5173;&#x8282; <pr>r&#xE9;ng&#x14D;nggu&#x101;nji&#xE9;</pr></trd>,
    <trd>&#x4EBA;&#x5DE5;&#x95DC;&#x7BC0; <pr>r&#xE9;ng&#x14D;nggu&#x101;nji&#xE9;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_s;&c_t;&c_a;&c_w;</trd>,
    <trd>&c_s;&c_u;&c_ch;&c_l;&c_ja;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_sh;&c_a;&c_r;&c_n;&c_ib;&c_r;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_t;&c_a;&c_v;&c_a;</trd>
  <trd lng="bo">&#xF5A;&#xF72;&#xF42;&#xF66;&#xF0B;</trd>
  <trdgrp lng="br">
    <trd>koubl</trd>,
    <trd>mell</trd>,
    <trd>kenvell</trd>
  </trdgrp>
  <trd lng="ca">articulaci&oacute;</trd>
  <trd lng="cs">kloub</trd>
  <trd lng="de">Gelenk</trd>
  <trdgrp lng="en">
    <trd>joint</trd>,
    <trd>hinge</trd>
  </trdgrp>
  <trd lng="es">articulaci&oacute;n</trd>
  <trd lng="fr">articulation</trd>
  <trdgrp lng="he">
    <trd>&#x5DE;&#x5E4;&#x5E8;&#x5E7;</trd>,
    <trd>&#x5E4;&#x5E8;&#x5E7;</trd>
  </trdgrp>
  <trd lng="hu">csukl&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x95A2;&#x7BC0;  <pr>&#x304B;&#x3093;&#x305B;&#x3064;</pr></trd>,
    <trd>&#x7D99;&#x624B;  <pr>&#x3064;&#x304E;&#x3066;</pr></trd>
  </trdgrp>
  <trd lng="pl">kolanko<klr> (BOT)</klr></trd>
  <trd lng="ro">genunchi</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_a;&c_r;&c_n;&c_i;&c_r;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_je;&c_n;&c_o;</trd>,
    <trd>&c_s;&c_o;&c_ch;&c_l;&c_je;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>k&lacute;b</trd>,
    <trd>k&lacute;b</trd>,
    <trd>zhyb</trd>,
    <trd>&ccaron;l&aacute;nok</trd>
  </trdgrp>
  <trd lng="sv">led</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_u;&c_g;&#x43B;&#x43E;&c_b;</trd>,
    <trd>&c_z;&c_ch;&#x43B;&c_je;&#x43D;&c_u;&c_v;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_z;&#x2019;&c_jeu;&c_d;&#x43D;&c_a;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="artik.0igi">
  <kap><tld/>igi, <var><kap><tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="artik.0igi.kunligi">
    <dif>
      Kunligi per <tld/>o.
    </dif>
    <trd lng="be">&c_z;&c_l;&c_u;&c_ch;&c_a;&c_c;&c_mol; <klr>(&c_p;&c_r;&c_a;&c_z; &c_sh;&c_a;&c_r;&c_n;&c_ib;&c_r; &c_ib; &c_d;&c_a; &c_g;. &c_p;.)</klr></trd>
    <trdgrp lng="br">
      <trd>koubla&ntilde;<klr tip="ind"> (mella&ntilde;)</klr></trd>,
      <trd>mella&ntilde;<klr tip="ind"> (koubla&ntilde;)</klr></trd>
    </trdgrp>
    <trd lng="ca">articular</trd>
    <trdgrp lng="hu">
      <trd>csukl&oacute;san k&ouml;t</trd>,
      <trd>csukl&oacute;san illeszt</trd>
    </trdgrp>
    <trd lng="pl">&lstroke;&aogonek;czy&cacute; na zawiasy</trd>
    <trd lng="ro">s&abreve; se conecteze la pendul</trd>
    <trd lng="ru">&c_s;&c_o;&c_ch;&c_l;&c_je;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="artik.0igi.FON">
    <uzo tip="stl">ARK</uzo>
    <uzo tip="fak">FON</uzo>
    <dif>
      <ref tip="dif" cel="artikulaci.0i">Artikulacii</ref>:
      <ekz>
        <tld/>igita parolo <klr>(kiu estas disigebla en diferencajn
        precizajn silabojn)</klr>;
      </ekz>
      <ekz>
        la lingvo evoluis ekde la unua <tld/>igita sono &gcirc;is la apero de la skribo
        <fnt>
          <bib>Monato</bib>
          <aut>Mihail Korotkov</aut>
          <vrk><url ref="&Monato;/2011/010218.php">La origino de la homaj lingvoj</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="akcent.0i.skandi">akcenti<sncref/></ref>
    <trd lng="be">&c_a;&c_r;&c_t;&c_y;&c_k;&c_u;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="ca">articular</trd>
    <trd lng="de">artikulieren</trd>
    <trdgrp lng="en">
      <trd>articulate</trd>,
      <trd>utter</trd>
    </trdgrp>
    <trd lng="es">articular</trd>
    <trdgrp lng="hu">
      <trd>artikul&aacute;l</trd>,
      <trd>tagol</trd>
    </trdgrp>
    <trd lng="pl">artyku&lstroke;owa&cacute;</trd>
    <trd lng="ro">pronun&#x21B;a</trd>
    <trd lng="ru">&c_a;&c_r;&c_t;&c_i;&c_k;&c_u;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>skloubit</trd>,
    <trd>spojit kloubem</trd>
  </trdgrp>
  <trd lng="fr">articuler</trd>
  <trd lng="it">articolare</trd>
  <trdgrp lng="ja">
    <trd>&#x95A2;&#x7BC0;&#x3067;&#x3064;&#x306A;&#x3050;  <pr>&#x304B;&#x3093;&#x305B;&#x3064;&#x3067;&#x3064;&#x306A;&#x3050;</pr></trd>,
    <trd>&#x5206;&#x7BC0;&#x3059;&#x308B;  <pr>&#x3076;&#x3093;&#x305B;&#x3064;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="sk">spoji&tcaron; k&lacute;bom</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_ch;&#x43B;&c_je;&#x43D;&#x43E;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&c_z;&#x2019;&c_jeu;&c_d;&#x43D;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="artik.0uloj">
  <kap><tld/>uloj,
  <var><kap><tld/>pieduloj</kap></var></kap>
  <snc mrk="artik.0uloj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="malprt" cel="verteb.sen0uloj">Senvertebraj 
      animaloj</ref>, kies korpo konsistas el ringoformaj 
      segmentoj kaj kies membroj havas plurajn 
      <ref tip="prt" cel="artik.0o.ANA">artikojn</ref>, 
      ekzemple <ref tip="prt" cel="krust.0uloj">krustuloj</ref>, 
      <ref tip="prt" cel="arane.0uloj">araneuloj</ref>, 
      <ref tip="prt" cel="miriap.0oj.ZOO">miriapodoj</ref> 
      kaj <ref tip="prt" cel="insekt.0oj">insektoj</ref>;
      <ref tip="lst" cel="filum.0o" lst="voko:zoologiaj_grupoj">filumo</ref> 
      de la <ref tip="malprt" cel="best.0oj">bestoj</ref>
      (<trd lng="la">Arthropoda</trd>);
      <ref tip="sin" cel="artropod.0o.ZOO">artropodoj</ref>:
      <ekz>
        iuj insektoj kaj aliaj <tld/>uloj en natura stato foje
        malfekundi&gcirc;as
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 7a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x7BC0;&#x80A2;&#x52D5;&#x7269; <pr>ji&#xE9;zh&#x12B;d&#xF2;ngw&#xF9;</pr></trd>,
    <trd>&#x8282;&#x80A2;&#x52A8;&#x7269; <pr>ji&#xE9;zh&#x12B;d&#xF2;ngw&#xF9;</pr></trd>,
    <trd>&#x7BC0;&#x652F;&#x52D5;&#x7269; <pr>ji&#xE9;zh&#x12B;d&#xF2;ngw&#xF9;</pr></trd>,
    <trd>&#x8282;&#x652F;&#x52A8;&#x7269; <pr>ji&#xE9;zh&#x12B;d&#xF2;ngw&#xF9;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_s;&c_u;&c_s;&c_t;&c_a;&c_v;&c_a;&c_n;&c_o;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="br">artropoded</trd>
  <trd lng="ca">artr&ograve;podes</trd>
  <trd lng="de">Gliederf&uuml;&szlig;er</trd>
  <trd lng="en">arthropods</trd>
  <trd lng="es">artr&oacute;podos</trd>
  <trd lng="fr">arthropodes</trd>
  <trd lng="hu">&iacute;zeltl&aacute;b&uacute;ak</trd>
  <trd lng="it">artropodi</trd>
  <trd lng="pl">stawonogi</trd>
  <trd lng="ro">artropode</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_t;&c_r;&c_o;&c_p;&c_o;&c_d;&c_y;</trd>,
    <trd>&c_ch;&c_l;&c_je;&c_n;&c_i;&c_s;&c_t;&c_o;&c_n;&c_o;&c_g;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="artik.el0igi">
  <kap>el<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="artik.el0igi.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Misloki du <tld/>ajn ostojn en membro, tiel ke ili ne plu normale
      movi&gcirc;as:
      <ekz>
        el<tld/>igita &scirc;ultro
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        unu <klr>[malsanulo]</klr> &gcirc;emis, al kiu oni &jcirc;us 
        remetis el<tld/>igitan piedon
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 1a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_b;&c_ib;&c_c;&c_mol; <klr>(&c_z; &c_s;&c_u;&c_s;&c_t;&c_a;&c_v;&c_a;)</klr></trd>,
    <trd>&c_z;&c_mol;&c_v;&c_ib;&c_h;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>vymknout</trd>,
    <trd>vymykat</trd>,
    <trd>vyvrtnout</trd>
  </trdgrp>
  <trd lng="de">ausrenken</trd>
  <trd lng="en">to dislocate <klr tip="amb">(a joint)</klr></trd>
  <trdgrp lng="fr">
    <trd>d&eacute;boiter<klr tip="amb"> (une articulation)</klr></trd>,
    <trd>d&eacute;mettre<klr tip="amb"> (une articulation)</klr></trd>,
    <trd>luxer</trd>
  </trdgrp>
  <trdgrp lng="it">
     <trd>disarticolare</trd>,
     <trd>lussare</trd>
  </trdgrp>
  <trd lng="pl">nastawia&cacute; ko&sacute;ci</trd>
  <trd lng="ro">a&#x219;eza&#x21B;i oasele</trd>
  <trd lng="sk">vyvrtn&uacute;&tcaron;</trd>
  <trd lng="uk">&c_v;&c_i;&c_v;&c_i;&c_h;&#x43D;&c_u;&c_t;&c_i;</trd>
</drv>

<drv mrk="artik.el0igxi">
  <kap>el<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="artik.el0igxi.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Gliti ekster la normala <tld/>a loko:
      <ekz>
        la femuro de <nom>Jakob</nom> el<tld/>i&gcirc;is, dum li luktis
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 32:25</lok>
        </fnt>;
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_b;&c_ib;&c_c;&c_c;&c_a; <klr>(&c_z; &c_s;&c_u;&c_s;&c_t;&c_a;&c_v;&c_a;)</klr></trd>,
    <trd>&c_z;&c_mol;&c_v;&c_ib;&c_h;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de">sich <ind>ausrenken</ind></trd>
  <trd lng="en">become dislocated</trd>
  <trdgrp lng="fr">
    <trd>se <ind>d&eacute;boiter</ind><klr tip="amb">
(articulation)</klr></trd>,
    <trd>se <ind>d&eacute;mettre</ind><klr tip="amb">
(articulation)</klr></trd>,
    <trd>se <ind>luxer</ind></trd>
    </trdgrp>
    <trdgrp lng="it">
     <trd>disarticolarsi</trd>,
     <trd>lussarsi</trd>
   </trdgrp>
    <trd lng="pl">zwichn&aogonek;&cacute;</trd>  
    <trd lng="ro">entors&abreve;</trd>
</drv>

<drv mrk="artik.krom0uloj">
  <kap>krom<tld/>uloj</kap>
  <snc mrk="artik.krom0uloj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="grup.0o.BIO" lst="voko:zoologiaj_grupoj">Grupo</ref>  
      de <ref tip="malprt" cel="mam.0uloj">mamuloj</ref>
      (<trd lng="la">Xenarthra</trd>) kun kromaj artikoj &ccirc;e la
      brustaj kaj lumbaj vertebroj, ampleksanta la ordojn
      <trd lng="la">Pilosa</trd> kaj <trd lng="la">Cingulata</trd>.
    </dif>
    <refgrp tip="prt">
      <ref cel="dazip.0o">dazipo</ref>,
      <ref cel="bradip.0o">bradipo</ref>,
      <ref cel="mangx.formik0ulo">formikman&gcirc;ulo</ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#x8CA7;&#x9F52;&#x76EE; <pr>p&#xED;nch&#x1D0;m&#xF9;</pr></trd>,
    <trd>&#x8D2B;&#x9F7F;&#x76EE; <pr>p&#xED;nch&#x1D0;m&#xF9;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_n;&c_ja;&c_p;&c_o;&c_w;&c_n;&c_a;&c_z;&c_u;&c_b;&c_y;&c_ja; &c_n;&c_o;&c_v;&c_a;&c_s;&c_mol;&c_v;&c_je;&c_t;&c_n;&c_y;&c_ja;</trd>
  <trd lng="ca">xenartres</trd>
  <trdgrp lng="de">
    <trd>Nebengelenktiere</trd>,
    <trd>Zahnlose <klr tip="ind">[S&auml;ugetierordnung]</klr></trd>
  </trdgrp>
  <trd lng="en">Xenarthra</trd>
  <trd lng="fr">x&eacute;narthres</trd>
  <trd lng="hu">fogh&iacute;jas <ind>eml&odblac;s&ouml;k</ind></trd>
  <trdgrp lng="it">
    <trd>xenartri</trd>,
    <trd>edentati</trd>
  </trdgrp>
  <trd lng="pl">szczerbaki</trd>
  <trd lng="ro">Xenarthra</trd>
  <trd lng="ru">&c_n;&c_je;&c_p;&c_o;&c_l;&c_n;&c_o;&c_z;&c_u;&c_b;&c_y;&c_je;</trd>
</drv>

<drv mrk="artik.pied0o">
  <kap>pied<tld/>o</kap>
  <snc mrk="artik.pied0o.ANA">
  <uzo tip="fak">ANA</uzo>
    <dif>
      Artiko inter <ref cel="krur.0o.genuo_gxis_piedo">kruro</ref>
      kaj <ref cel="pied.0o.ANA">piedo</ref>:
      <ekz>
        la pied<tld/>o de &scirc;ia malgranda piedeto fari&gcirc;adis tute ru&gcirc;a
        <fnt>
          <bib>Fab2</bib>
          <lok>Ru&gcirc;aj &scirc;uoj</lok>
        </fnt>;
      </ekz>
      <ekz>
