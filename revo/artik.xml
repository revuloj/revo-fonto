<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>artik</rad>/o
</kap>
<drv mrk="artik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">ANA</uzo>
    <dif>
      Organa &ccirc;arniro, kuniganta du partojn de la korpo, tiel ke
      amba&ubreve; povas movi&gcirc;i:
      <ekz>
        pied<tld/>o, vertebra <tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>joint</trd>,
      <trd>articulation</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_u;&c_s;&c_t;&c_a;&c_v;</trd>,
      <trd>&c_s;&c_o;&c_ch;&c_l;&c_je;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Mekanika kunligo de du pecoj, kiuj povas movi&gcirc;i
      &ccirc;irka&ubreve; la punkto de kunligo.
    </dif>
  </snc>
  <trd lng="de">Gelenk</trd>
  <trdgrp lng="en">
    <trd>joint</trd>,
    <trd>hinge</trd>
  </trdgrp>
  <trd lng="fr">articulation</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_a;&c_r;&c_n;&c_i;&c_r;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_je;&c_n;&c_o;</trd>,
    <trd>&c_s;&c_o;&c_ch;&c_l;&c_je;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sv">led</trd>
</drv>

<drv mrk="artik.0igi">
  <kap><tld/>(ig)i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kunligi per <tld/>o.
    </dif>
    <trd lng="ru">&c_s;&c_o;&c_ch;&c_l;&c_je;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <uzo tip="fak">FON</uzo>
    <dif>
      Movi la bu&scirc;organojn por nete a&ubreve;digi sonon:
      <ekz>
        <tld/>igita parolo <klr>(kiu estas disigebla en diferencajn
        precizajn silabojn)</klr>.
      </ekz>
    </dif>
    <trd lng="de">artikulieren</trd>
    <trdgrp lng="en">
      <trd>articulate</trd>,
      <trd>utter</trd>
    </trdgrp>
    <trd lng="es">articular</trd>
    <trd lng="ru">&c_a;&c_r;&c_t;&c_i;&c_k;&c_u;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="fr">articuler</trd>
</drv>

<drv mrk="artik.0pieduloj">
  <kap><tld/>pieduloj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Filumo de la <ref tip="super" cel="best.0oj">bestoj</ref>
      (<trd lng="la">Arthropoda</trd>).
    </dif>
  </snc>
  <trd lng="de">Gliederf&uuml;&szlig;er</trd>
  <trd lng="en">arthropods</trd>
  <trd lng="es">artr&oacute;podos</trd>
  <trd lng="fr">arthropodes</trd>
</drv>

<drv mrk="artik.krom0uloj">
  <kap>krom<tld/>uloj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="mam.0uloj">mamuloj</ref>
      (<trd lng="la">Xenarthra</trd>), kun kromaj artikoj &ccirc;e la
      brustaj kaj lumbaj vertebroj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Nebengelenktiere</trd>,
    <trd>Zahnlose <klr tip="ind">(S&auml;getierordnung)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="artik.0igapunkto">
  <kap><tld/>iga punkto</kap>
  <snc>
    <dif>
      Loko en la bu&scirc;o, kie sono estas <tld/>igita.
    </dif>
    <refgrp tip="sin">
      <ref cel="pozici.0o">pozicio</ref>.
    </refgrp>
  </snc>
  <trd lng="en">articulation point</trd>
  <trd lng="es">punto de articulaci&oacute;n</trd>
  <trd lng="fr">point d'articulation</trd>
</drv>
</art>
