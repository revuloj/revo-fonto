<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: averagx.xml,v 1.12 2023/08/30 18:23:38 revo Exp $">
<kap>
  <rad>avera&gcirc;</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="averagx.0o">
  <kap><tld/>o</kap>
  <snc mrk="averagx.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <dif>
      Aritmetika <ref tip="sub" cel="nombr.mez0o.MAT">meznombro</ref>:
      <ekz>
        la <tld/>o de la licencpagoj en la lastaj dek jaroj
        <fnt>
         <vrk><url ref="http://www.eventoj.hu/arkivo/eve-056.htm">Raporto 
         de la UEA-komisiono pri financo al la komitato</url></vrk>,
         <lok>Eventoj, n-ro 0056, 2/junio 1994</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kapa malneta nacia produkto en la ali&gcirc;ontaj landoj malsuperas duonon de la
        EU-avera&gcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Istv&aacute;n Ertl</aut>
          <vrk><url ref="https://www.monato.be/2003/008140.html">Pri orienti&gcirc;o kaj okcidenti&gcirc;o</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        224 libroj <klr>[&#x2026;]</klr> rimarkeble superas la <tld/>on de la anta&ubreve;aj 12 jaroj
        <fnt>
          <bib>LOdE</bib>
          <lok>2004, &#x2116; 2 (112)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_ja;&c_r;&c_e;&c_d;&c_n;&c_ja;&c_je; &c_a;&c_r;&c_y;&c_t;&c_m;&c_e;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_je;</trd>,
    <trd>&c_s;&c_ja;&c_r;&c_e;&c_d;&c_n;&c_ja;&c_ja; &c_v;&c_je;&c_l;&c_ib;&c_ch;&c_y;&c_n;&c_ja;</trd>
  </trdgrp>
  <trd lng="de">Durchschnitt</trd>
  <trd lng="he">&mem;&#x5B0;&mem;&vav;&#x5BC;&tsadi;&#x5B8;&ayin;</trd>
  <trd lng="nl">gemiddelde</trd>
  <trdgrp lng="es">
    <trd>media aritm&eacute;tica</trd>,
    <trd>promedio</trd>
  </trdgrp>
  <trd lng="fr">moyenne</trd>
  <trd lng="pl">&sacute;rednia</trd>
  <trd lng="uk">&c_s;&c_je;&c_r;&c_je;&c_d;&c_n;&c_jeu; &c_a;&c_r;&c_i;&c_f;&c_m;&c_je;&c_t;&c_i;&c_ch;&c_n;&c_je;</trd>
</drv>

<drv mrk="averagx.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Reprezentanta la <tld/>on:
      <ekz>
        TEJO estas reprezentanta <tld/>an nombron de 18 mil membroj
        <fnt>
          <bib>Kon11</bib>
          <lok>Interparolo kun Fernando Maia Jr.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj homoj ne povas koncentri&gcirc;i longe sur la sama tasko
        <fnt>
          <bib>Kon11</bib>
          <lok>La Konsiloj de la oficisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Seamus Finnigan</nom> bone reprezentas la <tld/>an nacian devenon en Britio
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la filmo tuj i&gcirc;is propagandeca, enhavanta &ccirc;ion, pri kio son&gcirc;as <tld/>a
        esperantisto
        <fnt>
          <bib>Kon11</bib>
          <lok>Kino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve; statistiko, la <tld/>a kvanto de uzebla akvo por &ccirc;iu lo&gcirc;anto en Pekino estas
        nur malpli ol 200 kubmetroj jare
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mez.0a">meza</ref>,
      <ref cel="kvalit.mez0a">mezkvalita</ref>,
      <ref cel="moder.0a">modera</ref>
    </refgrp>
  </snc>
  <trd lng="de">durchschnittlich</trd>
  <trd lng="ja">&#x5E73;&#x5747;&#x306E;  [<ind>&#x3078;&#x3044;&#x304D;&#x3093;&#x306E;</ind>]</trd>
</drv>









<drv mrk="averagx.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="averagx.0ulo.homo">
    <dif>
      Ordinara <ref tip="super" cel="hom.0o">homo</ref>:
      <ekz>
        <tld/>ulo eble ne scias <klr>[&#x2026;]</klr> ke akompanas &ccirc;iun satelitan   
        televidprogramon pluraj sonkanaloj
        <fnt>
          <aut>Simono Pejno</aut>:
          <vrk>Radio</vrk>,
          <bib>Monato</bib>
          <lok>n-ro 04/1995</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kvalit.mez0ulo">mezkvalitulo</ref>,
      <ref cel="mediokr.0ulo">mediokrulo</ref>,
      <ref cel="normal.0ulo">normalulo</ref>,
<ref cel="ordina.0ulo">ordinarulo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_ja;&c_r;&c_e;&c_d;&c_n;&c_ib; &c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;</trd>
  <trdgrp lng="de">
    <trd>Durchschnittsmensch</trd>,
    <trd>Durchschnittsb&uuml;rger</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>ciudadano medio</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>doorsnee<klr tip="amb"> (-persoon)</klr></trd>,
    <trd>middelmaat<klr tip="amb"> (persoon)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zwyk&lstroke;y cz&lstroke;owiek</trd>,
    <trd>przeci&eogonek;tny &sacute;miertelnik</trd>
  </trdgrp>
</drv>
</art>

