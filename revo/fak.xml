<drv mrk="fak.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fak.0o.ujo">
    <dif>
      Aparta <ref tip="super" cel="divid.0ajxo">divida&jcirc;o</ref> de 
      <ref tip="malprt" cel="mebl.0o">meblo</ref>, kesto a&ubreve; objekto,
      destinita por enteni a&jcirc;ojn a&ubreve; ulojn, apartigante ilin:
      <ekz>
        li havas plenan &scirc;rankon da ar&gcirc;enta&jcirc;oj, ne kalkulante la enhavon de la sekretaj
        <tld/>oj
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tistino kaj kamentubisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[pro]</klr> teruro <klr>[&#x2026;]</klr> ili ensaltis en la <tld/>on de la
        subfenestra benko
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tistino kaj kamentubisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        tablo kun multaj <tld/>oj super la skriba plata&jcirc;o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la mastro iris al la skribotablo, malfermis unu <tld/>on kaj elprenis monon
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        el la <nom>oleotechium</nom>, t.e. la lasta ban<tld/>o, li eliradis ankora&ubreve; kvaza&ubreve;
        revivigita
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la homoj komencis forlasi la <tld/>ojn, en kiuj ili ari&gcirc;is, por rektigi la
        piedojn kaj babili
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        grandliteraj surskriboj <klr>[indikis]</klr> la opajn <tld/>ojn: Etiletero,
        Celulozo, Albumino <klr>[&#x2026;]</klr>
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        ni trovis en la kel<tld/>o responda al lia apartamento, dokumentojn
        <fnt>
          <bib>ChR</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="uj.0o">ujo</ref>
    <refgrp tip="vid">
      <ref cel="cxel.0o">&ccirc;elo</ref>,
      <ref cel="kest.0o">kesto</ref>,
      <ref cel="kamer.0o">kamero</ref>,
      <ref cel="kupe.0o">kupeo</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_a;&c_d;&c_d;&c_z;&c_je;&c_l;</trd>,
      <trd>&c_a;&c_d;&c_d;&c_z;&c_ja;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_sh;&c_u;&c_f;&c_l;&c_ja;&c_d;&c_a;</trd>,
      <trd>&c_s;&c_e;&c_k;&c_c;&c_y;&c_ja;</trd>
    </trdgrp>
    <trd lng="ca">compartiment</trd>
