<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc><rad>fak</rad>/o</kap>
<drv mrk="fak.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fak.0o.ujo">
    <dif>
      Aparta divida&jcirc;o de meblo, kesto a&ubreve; objekto,
      destinita por enteni a&jcirc;ojn a&ubreve; personojn,
      apartigante ilin:
      <ekz>
        <tld/>oj de skribotablo, de &scirc;ranko, de voja&gcirc;kofro;
      </ekz>
      <ekz>
        la 64 <tld/>oj de
        <ref tip="malprt" cel="sxak.0tabulo">&scirc;aktabulo</ref>;
      </ekz>
      <ekz>
        fum<tld/>o
        de vagonaro.
      </ekz>
    </dif>
    <ref tip="super" cel="uj.0o">ujo;</ref>
    <refgrp tip="vid">
      <ref cel="cxel.0o">&ccirc;elo</ref>,
      <ref cel="kupe.0o">kupeo</ref>,
      <ref cel="sekci.0o">sekcio</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Fach</trd>,
      <trd>Schublade</trd>,
      <trd>Feld</trd>,
      <trd>Abteil</trd>,
      <trd>Abteilung</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>compartiment</trd>,
      <trd>case</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>rekesz</trd>,
      <trd>fakk</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>vak</trd>,
      <trd>compartiment</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_t;&c_d;&c_je;&c_l;</trd>,
      <trd>&c_o;&c_t;&c_d;&c_je;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc mrk="fak.0o.brancxo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Aparta bran&ccirc;o de scienco, arto, industrio, komerco, kc:
      <ekz>
        literatura <tld/>o;
      </ekz>
      <ekz>
        politiko ne estas mia <tld/>o;
      </ekz>
      <ekz>
        divers<tld/>a komerco.
      </ekz>
    </dif>
    <ref tip="vid" cel="profes.0o">profesio.</ref>
    <trdgrp lng="de">
      <trd>Fach</trd>,
      <trd>Bereich</trd>,
      <trd>Gebiet</trd>,
      <trd>Branche</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>discipline</trd>,
      <trd>sp&eacute;cialit&eacute;</trd>,
      <trd>domaine<klr tip="ind"> (de connaissance)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>szakter&uuml;let</trd>,
      <trd>&aacute;gazat</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>vak</trd>,
      <trd>branche</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_p;&c_je;&c_c;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_l;&c_a;&c_s;&c_t;&c_mol; <klr>(&c_d;&c_je;&c_ja;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_i;)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="cs">odbor</trd>
  <trd lng="en">section</trd>
</drv>

<drv mrk="fak.0aro">
  <kap><tld/>aro</kap>
  <fnt><bib>VdE</bib>, <lok></lok></fnt>
  <snc>
    <dif>
      Kesto a&ubreve; skatolo kun <tld/>oj.
    </dif>
  </snc>
  <trd lng="fr">casier<klr tip="ind"> (bo&icirc;te &agrave; cases)</klr></trd>
  <trd lng="hu">rekeszes tart&oacute;</trd>
  <trd lng="ru">&c_ja;&c_shch;&c_i;&c_k; &c_s; &c_o;&c_t;&c_d;&c_je;&c_l;&c_je;&c_n;&c_i;&c_ja;&c_m;&c_i;</trd>
</drv>

<drv mrk="fak.0isto0ulo">
  <kap><tld/>isto, <tld/>ulo<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Tiu, kiu sin okupas pri aparta te&hcirc;nika <tld/>o .
    </dif>
    <refgrp tip="vid">
      <ref cel="spert">spertulo</ref>,
      <ref cel="eksper2.0isto">ekspertizisto</ref>,
      <ref cel="kompet.0ulo">kompetentulo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Fachmann</trd>,
    <trd>Fachfrau</trd>
  </trdgrp>
  <trd lng="fr">sp&eacute;cialiste</trd>
  <trdgrp lng="hu">
    <trd>szakember</trd>,
    <trd>specialista</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vakman</trd>,
    <trd>specialist</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_p;&c_je;&c_c;&c_i;&c_a;&c_l;&c_i;&c_s;&c_t;</trd>
</drv>

<drv mrk="fak.0saltaludo">
  <kap><tld/>salta ludo</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="paradi1.lud0o">Paradizludo</ref>.
    </dif>
  </snc>
  <trd lng="ru">&c_k;&c_l;&c_a;&c_s;&c_s;&c_i;&c_k;&c_i; <klr tip="amb">(&c_i;&c_g;&c_r;&c_a;)</klr></trd>
</drv>

</art>
