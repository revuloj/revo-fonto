<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fak.xml,v 1.29 2021/05/28 17:05:24 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fak</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fak.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fak.0o.ujo">
    <dif>
      Aparta divida&jcirc;o de meblo, kesto a&ubreve; objekto,
      destinita por enteni a&jcirc;ojn a&ubreve; personojn,
      apartigante ilin:
      <ekz>
        <tld/>oj de skribotablo, de &scirc;ranko, de voja&gcirc;kofro;
      </ekz>
      <ekz>
        la 64 <tld/>oj de
        <ref tip="malprt" cel="sxak.0tabulo">&scirc;aktabulo</ref>;
      </ekz>
      <ekz>
        fum<tld/>o de vagonaro.
      </ekz>
    </dif>
    <ref tip="super" cel="uj.0o">ujo;</ref>
    <refgrp tip="vid">
      <ref cel="cxel.0o">&ccirc;elo</ref>,
      <ref cel="kupe.0o">kupeo</ref>,
      <ref cel="sekci.0o">sekcio</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_a;&c_d;&c_d;&c_z;&c_je;&c_l;</trd>,
      <trd>&c_a;&c_d;&c_d;&c_z;&c_ja;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_sh;&c_u;&c_f;&c_l;&c_ja;&c_d;&c_a;</trd>,
      <trd>&c_s;&c_e;&c_k;&c_c;&c_y;&c_ja;</trd>
    </trdgrp>
    <trd lng="ca">compartiment</trd>
    <trdgrp lng="de">
      <trd>Fach</trd>,
      <trd>Schublade</trd>,
      <trd>Feld</trd>,
      <trd>Abteil</trd>,
      <trd>Abteilung</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>compartment</trd>,
      <trd>pigeonhole</trd>,
      <trd>shelf</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>compartimento</trd>,
      <trd>hueco de un casillero</trd>,
      <trd>estante</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>compartiment</trd>,
      <trd>case</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>rekesz</trd>,
      <trd>fakk</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>vak</trd>,
      <trd>compartiment</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>pole</trd>,
      <trd>przegr&oacute;dka</trd>,
      <trd>przedzia&lstroke;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>compartimento</trd>,
      <trd>se&ccedil;&atilde;o</trd>,
      <trd>escaninho</trd>.
      <trd>casa <klr tip="ind">(de tabuleiro)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_t;&c_d;&c_je;&c_l;</trd>,
      <trd>&c_o;&c_t;&c_d;&c_je;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc mrk="fak.0o.brancxo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Aparta bran&ccirc;o de scienco, arto, industrio, komerco, kc:
      <ekz>
        literatura <tld/>o;
      </ekz>
      <ekz>
        politiko ne estas mia <tld/>o;
      </ekz>
      <ekz>
        divers<tld/>a komerco.
      </ekz>
    </dif>
    <ref tip="vid" cel="profes.0o">profesio</ref>
    <trdgrp lng="be">
      <trd>&c_s;&c_p;&c_e;&c_c;&c_y;&c_ja;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_v;&c_o;&c_b;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol; <klr tip="ind">(&c_d;&c_z;&c_je;&c_j;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ib;,
      &c_v;&c_je;&c_d;&c_a;&c_w;)</klr></trd>,
      <trd>&c_s;&c_f;&c_e;&c_r;&c_a; <klr tip="amb">(&c_d;&c_z;&c_je;&c_j;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ib;,
      &c_v;&c_je;&c_d;&c_a;&c_w;)</klr></trd>,
    </trdgrp>
    <trdgrp lng="ca">
      <trd>branca</trd>,
      <trd>especialitat</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Fach</trd>,
      <trd>Bereich</trd>,
      <trd>Gebiet</trd>,
      <trd>Branche</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>branch</trd>,
      <trd>discipline</trd>,
      <trd>specialty</trd>,
      <trd>area of expertise</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>rama</trd>,
      <trd>disciplina</trd>,
      <trd>especialidad</trd>,
      <trd>&aacute;rea de especialidad</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>discipline</trd>,
      <trd>sp&eacute;cialit&eacute;</trd>,
      <trd>domaine<klr tip="ind"> (de connaissance)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>szakter&uuml;let</trd>,
      <trd>&aacute;gazat</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>vak</trd>,
      <trd>branche</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>bran&zdot;a</trd>,
      <trd>dyscyplina</trd>,
      <trd>dziedzina</trd>,
      <trd>fach</trd>,
      <trd>specjalno&sacute;&cacute;</trd>,
      <trd>zaw&oacute;d</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>especialidade</trd>,
      <trd>ramo <klr tip="ind">(especialidade)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_p;&c_je;&c_c;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_l;&c_a;&c_s;&c_t;&c_mol;
      <klr>(&c_d;&c_je;&c_ja;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_i;)</klr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>odbor</trd>,
    <trd>odd&ecaron;len&iacute;</trd>,
    <trd>samostatn&aacute; &ccaron;&aacute;st</trd>
  </trdgrp>
  <trd lng="en">section</trd>
  <trd lng="ne">&#x92E;&#x93E;&#x92E;&#x932;&#x93E;</trd>
  <trdgrp lng="sk">
    <trd>box</trd>,
    <trd>oddelen&yacute; priestor</trd>
  </trdgrp>
</drv>

<drv mrk="fak.0aro">
  <kap><tld/>aro</kap>
  <fnt><bib>VdE</bib></fnt>
  <snc>
    <dif>
      Kesto a&ubreve; skatolo kun <tld/>oj.
    </dif>
  </snc>
  <trd lng="ca">caseller</trd>
  <trdgrp lng="en">
    <trd>pigeonholes</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>casillero</trd>
  </trdgrp>
  <trd lng="fr">casier<klr tip="ind"> (bo&icirc;te &agrave;
  cases)</klr></trd>
  <trd lng="hu">rekeszes tart&oacute;</trd>
  <trdgrp lng="pl">
    <trd>sekretarzyk</trd>,
    <trd>kasetka</trd>
  </trdgrp>
  <trd lng="ru">&c_ja;&c_shch;&c_i;&c_k; &c_s;
  &c_o;&c_t;&c_d;&c_je;&c_l;&c_je;&c_n;&c_i;&c_ja;&c_m;&c_i;</trd>
</drv>

<drv mrk="fak.0ulo">
  <kap><tld/>ulo,
   <var><kap><tld/>isto</kap></var></kap>
  <snc mrk="fak.0ulo.KOMUNE">
    <dif>
      Tiu, kiu sin okupas pri aparta <tld/>o scienca, te&hcirc;nika a&ubreve; arta:
      <ekz>
        artikoloj <klr>[&#x2026;]</klr> el la plumo de elstaraj <tld/>uloj
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006807.html">Fascina gazeto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        tiun aferon devus pridiskuti en zorge organizitaj konferencoj sa&gcirc;aj <tld/>uloj
        <fnt>
          <bib>Monato</bib>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006803.html">Reago: Supersignoj, pro kiu diabla ka&ubreve;zo?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>isto pri socialaj problemoj
        <fnt>
          <vrk><url ref="https://gejaj-petolajhoj.blogspot.com/2017/">Gejaj rakontoj en Esperanto.: 2017</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        sociologoj, psikologoj kaj aliaj <tld/>uloj opinias, ke pli bone estus dialogi
        kun la kontra&ubreve;uloj de la kunsido ol batali kontra&ubreve; ili
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007093.html">Internacia kunsido de financistoj en Prago</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la ekzistanta man&ccirc;ura estas rigardata de la <tld/>uloj kiel rara primara
        materialo por esploro
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007025.html">La Man&ccirc;ura: Formortanta lingvo de malplimulto en &Ccirc;inio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        fakuloj zorgas pri la &ccirc;evaloj el tiu raso
        <fnt>
          <bib>Monato</bib>
          <aut>Jean-Yves Santerre</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006864.html">Dombestoj: &Ccirc;evalrasoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        se iu okaze alvokita <tld/>ulo fine sukcesos instali &ccirc;iujn satelitricevajn
        necesa&jcirc;ojn, <klr>[&#x2026;]</klr> vi &gcirc;uos postfare perfektan ricevadon
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9504-radio.txt">Radio</url></vrk>
          <lok>95/04</lok>
        </fnt>;
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="specia.0isto">specialisto</ref>,
      <ref cel="eksper1.0o">eksperto</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="kompet.0ulo">kompetentulo</ref>,
      <ref cel="scienc.0isto">sciencisto</ref>,
      <ref cel="spert">spertulo</ref>,
      <ref cel="eksper2.0isto">ekspertizisto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_e;&c_c;&c_y;&c_ja;&c_l;&c_ib;&c_s;&c_t;</trd>,
    <trd>&c_a;&c_d;&c_m;&c_y;&c_s;&c_l;&c_o;&c_v;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="ca">especialista</trd>
  <trd lng="cs">odborn&iacute;k</trd>
  <trdgrp lng="de">
    <trd>Fachmann</trd>,
    <trd>Fachfrau</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>expert</trd>,
    <trd>specialist</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>experto</trd>,
    <trd>especialista</trd>
  </trdgrp>
  <trd lng="fr">sp&eacute;cialiste</trd>
  <trdgrp lng="hu">
    <trd>szakember</trd>,
    <trd>specialista</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vakman</trd>,
    <trd>specialist</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>fachowiec</trd>,
    <trd>specjalista</trd>,
    <trd>ekspert</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_p;&c_je;&c_c;&c_i;&c_a;&c_l;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">odborn&iacute;k</trd>
</drv>

<drv mrk="fak.0saltaludo">
  <kap><tld/>salta ludo</kap>
  <snc>
    <ref tip="dif" cel="paradi1.lud0o">paradizludo</ref>
  </snc>
  <trdgrp lng="ca">
    <trd>xarranca</trd>,
    <trd>sambori</trd>
  </trdgrp>
  <trd lng="fr">marelle</trd>
  <trd lng="pl">gra w <ind>klasy</ind></trd>
  <trd lng="ru">&c_k;&c_l;&c_a;&c_s;&c_s;&c_i;&c_k;&c_i; <klr tip="amb">(&c_i;&c_g;&c_r;&c_a;)</klr></trd>
</drv>

<drv mrk="fak.trab0a">
  <kap>trab<tld/>a</kap>
  <snc mrk="fak.trab0a.KON">
    <uzo tip="fak">KON</uzo>
    <dif>
      Konstruita el plekto de traboj, kun pleniga&jcirc;o en la
      interlasitaj 
      fakoj:
      <ekz>
        la du subaj eta&gcirc;oj el nestukita sablo&scirc;tono estas
        dividitaj nur
        per eta&gcirc;aj kornicoj, la tria eta&gcirc;o estas trab<tld/>a
        
        surkonstrua&jcirc;o
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.aliaflanko.de/urbo/badkoenig/badkoenig.html">
          Bad K&ouml;nig invitas...</url></vrk>,
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">d'entramat de fusta</trd>
  <trd lng="de">Fachwerk-</trd>
  <trd lng="en">truss</trd>
  <trd lng="fr">&agrave; <ind>colombage</ind></trd>
  <trd lng="pl">a&zdot;urowy</trd>
</drv>
</art>
