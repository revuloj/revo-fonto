<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sekrec.xml,v 1.29 2023/10/21 06:31:44 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>sekreci</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sekrec.0o">
  <kap><tld/>o</kap>
  <snc mrk="sekrec.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
    Procezo per kiu &ccirc;eloj, precipe de epiteliaj <ref cel="gland.0o">glando</ref>j,
      produktas organikan specifan substancon, pli-malpli likvan, kiu eli&gcirc;as en la
      internan medion (vd <ref tip="vid" cel="endokrin.0a.FIZL">endokrina</ref>)
      a&ubreve; en la eksteran medion, plejofte la&ubreve; kanalo (vd
      <ref tip="vid" cel="ekzokrin.0a.FIZL">ekzokrina</ref>):
      <ekz>
        la kataro estas akompanata de pli forta <tld/>o
        <klr>[&#x2026;]</klr> de la konjunktivo
        <fnt><bib>FK</bib>, <lok>5, kronika katara
        konjunktivito</lok></fnt>;
      </ekz>
      <ekz>
        malvarmega akvo akcelas la <tld/>adon de stre&ccirc;hormonoj, malaltigas la
        sangopremon, igas la ha&ubreve;ton pli elasta
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2009/009629.php">Vintra banado</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekskre.0o">ekskrecio</ref>,
      <ref cel="muk.0o">muko</ref>
    </refgrp>
  </snc>
  <snc mrk="sekrec.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Produktado de <ref tip="vid" cel="sekrec.0ajxo.BOT"><tld/>a&jcirc;o<sncref/></ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_e;&c_k;&c_r;&c_e;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_v;&c_y;&c_d;&c_z;&c_ja;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>odm&ecaron;&scaron;ek</trd>,
    <trd>sekret</trd>,
    <trd>v&yacute;m&ecaron;&scaron;ek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Sekretion</trd>,
    <trd><ofc>*</ofc>Absonderung</trd>,
    <trd>Aussch&uuml;ttung <klr>(Sekretion)</klr></trd>
  </trdgrp>
  <trd lng="en"><ofc>*</ofc>secretion</trd>
  <trd lng="fr"><ofc>*</ofc>s&eacute;cr&eacute;tion</trd>
  <trd lng="hu">v&aacute;lad&eacute;k</trd>
  <trdgrp lng="it">
    <trd>secrezione</trd>,
    <trd>secreto</trd>
  </trdgrp>
  <trd lng="ja">&#x5206;&#x6CCC;&#x7269;  <pr>&#x3076;&#x3093;&#x3074;&#x3064;&#x3076;&#x3064;</pr></trd>
  <trd lng="nl">afscheiding<klr tip="ind"> (v.vocht)</klr></trd>
  <trd lng="pl"><ofc>*</ofc>sekrecja</trd>
  <trd lng="pt">secre&ccedil;&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_k;&c_r;&c_je;&c_c;&c_i;&c_ja;</trd>,
    <trd><ofc>*</ofc>&c_v;&c_y;&c_d;&c_je;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">v&yacute;lu&ccaron;ok <klr>(&zcaron;liaz)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_je;&c_k;&c_r;&c_je;&c_c;&c_ib;&c_ja;</trd>,
    <trd>&c_v;&c_i;&c_d;&c_ib;&c_l;&c_je;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="sekrec.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sekrec.0i.FIZL">
    <dif>
      Fari <ref tip="vid" cel="sekrec.0o.FIZL"><tld/>on</ref>, produkti
      <tld/>a&jcirc;on:
      <ekz>
        glando <tld/>anta hormonojn, salivon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_d;&c_z;&c_ja;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_e;&c_k;&c_r;&c_e;&c_c;&c_y;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">vym&ecaron;&scaron;ovat</trd>
  <trd lng="de">absondern</trd>
  <trd lng="en">secrete</trd>
  <trd lng="fr">s&eacute;cr&eacute;ter</trd>
  <trd lng="hu">kiv&aacute;laszt <klr tip="ind">(v&aacute;lad&eacute;kot)</klr></trd>
  <trd lng="it">secernere</trd>
  <trd lng="ja">&#x5206;&#x6CCC;&#x3059;&#x308B;  <pr>&#x3076;&#x3093;&#x3074;&#x3064;&#x3059;&#x308B;</pr></trd>
  <trd lng="nl">afscheiden</trd>
  <trd lng="pl">wydziela&cacute;</trd>
  <trd lng="pt">secretar</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_d;&c_je;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_je;&c_k;&c_r;&c_je;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">vylu&ccaron;ova&tcaron;</trd>
  <trd lng="uk">&c_v;&c_i;&c_d;&c_ib;&c_l;&c_ja;&c_t;&c_i;</trd>
</drv>

<drv mrk="sekrec.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="sekrec.0ajxo.FIZL">
  <uzo tip="fak">FIZL</uzo>
    <dif>
      Substanco <tld/>ita.
    </dif>
    <refgrp tip="vid">
      <ref cel="ekskre1.0o">ekskremento</ref>
    </refgrp>
  </snc>
  <snc mrk="sekrec.0ajxo.BOT">
  <uzo tip="fak">BOT</uzo>
    <dif>
      Organika substanco, pli-malpli likva, produktata de iuj vegeta&jcirc;aj
      &ccirc;eloj, haroj, &ccirc;elsakoj, glandoj, kanaloj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_d;&c_z;&c_ja;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je; <klr>(&c_r;&c_e;&c_ch;&c_y;&c_v;&c_a;)</klr></trd>,
    <trd>&c_s;&c_a;&c_k;&c_r;&c_e;&c_t; <klr tip="amb">(&c_z;&c_a;&c_l;&c_o;&c_z;&c_y;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Sekret</trd>,
    <trd>Ausscheidung <klr>(Sekret)</klr></trd>
  </trdgrp>
  <trd lng="en">secretion</trd>
  <trd lng="fr">s&eacute;cr&eacute;tion</trd>
  <trd lng="pl">wydzielina</trd>
  <trd lng="pt">secre&ccedil;&atilde;o</trd>
</drv>

<drv mrk="sekrec.el0o">
  <kap>el<tld/>o</kap>
  <snc mrk="sekrec.el0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Procezo per kiu, <tld/>a&jcirc;oj de glando estas elver&scirc;itaj ekster tiun
      glandon la&ubreve; specialaj duktoj, nomataj el<tld/>aj duktoj.
    </dif>
    <rim>
      Ne konfuzu elsekrecion kaj <ref tip="vid" cel="ekskre.0o">ekskrecion</ref>. La unua
      forkondukas <tld/>on
      ekster glandon. La dua forkondukas for&jcirc;eta&jcirc;on ekster la
      organismon.
    </rim>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_v;&c_ja;&c_d;&c_z;&c_je;&c_n;&c_mol;&c_je; <klr>(&c_z; &c_a;&c_g;&c_r;&c_a;&c_n;&c_ib;&c_z;&c_m;&c_a;)</klr></trd>
  <trd lng="de">Ausscheidung</trd>
  <trd lng="fr">excr&eacute;tion</trd>
  <trd lng="pl">wydalanie</trd>
  <trd lng="pt">excre&ccedil;&atilde;o</trd>
</drv>

<drv mrk="sekrec.el0a">
  <kap>el<tld/>a</kap>
  <snc mrk="sekrec.el0a.FIZL">
    <dif>
      Rilatanta a&ubreve; servanta al <ref tip="vid" cel="sekrec.el0o">el<tld/>o</ref>:
      <ekz>
        el<tld/>a dukto.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_v;&c_a;&c_d;&c_n;&c_y;</trd>
  <trd lng="de">Ausscheidungs-</trd>
  <trd lng="fr">excr&eacute;teur</trd>
  <trd lng="pl">wydalniczy</trd>
  <trd lng="pt">excretor</trd>
</drv>

<drv mrk="sekrec.el0i">
  <kap>el<tld/>i</kap>
  <snc mrk="sekrec.el0i.FIZL">
    <dif>
      Forkonduki per <ref tip="vid" cel="sekrec.el0o">el<tld/>o</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_v;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol; <klr>(&c_z; &c_a;&c_g;&c_r;&c_a;&c_n;&c_ib;&c_z;&c_m;&c_a;)</klr></trd>
  <trd lng="cs">vym&ecaron;&scaron;ovat</trd>
  <trd lng="de">ausscheiden</trd>
  <trd lng="fr">excr&eacute;ter</trd>
  <trd lng="pl">wydziela&cacute;</trd>
  <trd lng="pt">excretar</trd>
  <trd lng="sk">vylu&ccaron;ova&tcaron;</trd>
</drv>
</art>
