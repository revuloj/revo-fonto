<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>sekreci</rad>/o
</kap>
<drv mrk="sekrec.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Organika substanco, ordinare flua&jcirc;a, produktata de iaj glandoj;
      eliga&jcirc;o<sup>Z</sup>:
      <ekz>
        la kataro estas akompanata de pli forta <tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Sekret</trd>
  <trd lng="nl">afscheiding<klr tip="ind"> (v.vocht)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_k;&c_r;&c_je;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_v;&c_y;&c_d;&c_je;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="sekrec.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Eligi <tld/>e.
    </dif>
  </snc>
  <trd lng="de">absondern</trd>
  <trd lng="nl">afscheiden</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_d;&c_je;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_je;&c_k;&c_r;&c_je;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
