<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bragx.xml,v 1.20 2023/10/14 08:39:48 revo Exp $">
<kap>
  <rad>bra&gcirc;</rad>/o
</kap>

<drv mrk="bragx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ardaj ligno- a&ubreve; karbopecoj, jam duone konsumitaj kaj ne
      plu &jcirc;etantaj flamon:
      <ekz>
        fajrobrigadistoj alvenas kiam &ccirc;io forbrulis,
        &gcirc;ustatempe por observi la subcindrajn <tld/>ojn
        <fnt><aut>Hamza Baksi&cacute;</aut>:
        <vrk>La fino de la mondo komenci&gcirc;as en Sarajevo</vrk>,
        <bib>Monato</bib><lok>jaro 1993a, numero 10a, p. 8a</lok></fnt>;
      </ekz>
      <ekz>
        sur bretaro vici&gcirc;as jam rusti&gcirc;intaj <tld/>-gladiloj
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2004/008445.html">Kolekti antikva&jcirc;ojn</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        alia forno pli simpla <klr>[&#x2026;]</klr> uzas karbon, kiun oni <tld/>igas por kuiri
        la man&gcirc;a&jcirc;on
        <fnt>
          <bib>Monato</bib>
          <aut>Gbeglo Koffi/pg</aut>
          <vrk><url ref="&Monato;/2006/008903.php">Fornoj kaj formoj</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        respondante per siaj <tld/>aj okuloj
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 9</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="karb.0eto">karbeto</ref>,
      <ref cel="ard.karb0ajxo">karbarda&jcirc;o</ref>
    </refgrp>
    <trd lng="de">Glut</trd>
    <trd lng="fr">braise</trd>
    <trd lng="hu">par&aacute;zs</trd>
    <trd lng="pl">w&eogonek;gle</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_s;&c_k;&c_a;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_je;
      &c_u;&c_g;&c_l;&c_i;</trd>,
      <trd>&c_zh;&c_a;&c_r;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Tiaj samaj pecoj, estingitaj anta&ubreve; plena konsumi&gcirc;o
      kaj uzataj kiel <ref tip="super" cel="brul.0igajxo">bruliga&jcirc;o</ref>
    </dif>
    <refgrp tip="sin">
      <ref cel="karb.0eto">karbeto</ref>,
      <ref cel="karb.ligno0o">lignokarbo</ref>.
    </refgrp>
    <trd lng="de">Holzkohle</trd>
    <trdgrp lng="fr">
      <trd>charbon de bois</trd>
    </trdgrp>
    <trd lng="hu">fasz&eacute;n</trd>    
    <trd lng="pl">w&eogonek;giel drzewny</trd>
    <trdgrp lng="ru">
      <trd>&c_u;&c_g;&c_l;&c_i;</trd>,
      <trd>&c_g;&c_o;&c_l;&c_o;&c_v;&c_je;&c_sh;&c_k;&c_i;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_u;&c_g;&c_a;&c_l;&c_mol;</trd>,
    <trd>&c_v;&c_u;&c_g;&c_o;&c_l;&c_mol;&c_l;&c_je;</trd>,
    <trd>&c_g;&c_a;&c_l;&c_a;&c_v;&c_je;&c_sh;&c_k;&c_ib;</trd>,
    <trd>&c_zh;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="en">charcoal</trd>
  <trd lng="es">brasa</trd>
  <trdgrp lng="ja">
    <trd>&#x304A;&#x304D;</trd>,
    <trd>&#x6D88;&#x3057;&#x70AD;  <pr>&#x3051;&#x3057;&#x305A;&#x307F;</pr></trd>
  </trdgrp>
  <trd lng="uk">&c_zh;&c_a;&c_r;</trd>
</drv>

<drv mrk="bragx.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <dif>
      Metala a&jcirc;ura <ref tip="super" cel="fajr.0ujo">fajrujo</ref>,
      ofte portebla kaj metita sur piedo, uzata por teni ardajn
      <tld/>ojn kaj servi kiel <ref tip="super" cel="hejt">hejtilo</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_ch;&c_a;&c_g;</trd>,
    <trd>&c_zh;&c_a;&c_r;&c_o;&c_w;&c_n;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Brasero</trd>,
    <trd>Glutbecken</trd>
  </trdgrp>
  <trd lng="es">brasero</trd>
  <trd lng="fr">brasero</trd>
  <trdgrp lng="hu">
    <trd>par&aacute;zs&uuml;st</trd>,
    <trd>mang&aacute;l</trd>
  </trdgrp>
  <trd lng="ja">&#x706B;&#x9262;  <pr>&#x3072;&#x3070;&#x3061;</pr></trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_ch;&c_a;&c_g;</trd>,
    <trd>&c_zh;&c_a;&c_r;&c_o;&c_v;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
