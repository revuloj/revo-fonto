<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>bra&gcirc;</rad>/o
</kap>
<drv mrk="bragx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ardaj ligno- a&ubreve; karbopecoj, jam duone konsumitaj kaj ne
      plu &jcirc;etantaj flamon.
    </dif>
    <refgrp tip="sin">
      <ref cel="karb.0eto">karbeto</ref>;
      <ref cel="ard.karb0ajxo">karbarda&jcirc;o</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Tiaj samaj pecoj, estingitaj anta&ubreve; plena konsumi&gcirc;o kaj
      uzataj kiel <ref tip="super" cel="brul.0igajxo">bruliga&jcirc;o</ref>.
    </dif>
    <refgrp tip="sin">
      <ref cel="karb.0eto">karbeto</ref>;
      <ref cel="karb.ligno0o">lignokarbo</ref>.
    </refgrp>
  </snc>
  <trd lng="fr">braise</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_k;&c_a;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_je; &c_u;&c_g;&c_l;&c_i;</trd>,
    <trd>&c_d;&c_r;&c_je;&c_v;&c_je;&c_s;&c_n;&c_y;&c_j; &c_u;&c_g;&c_o;&c_l;&c_mol;</trd>
  </trdgrp>
  <adm>
    Kontroli la rusan tradukon.<aut>MB</aut>
  </adm>
</drv>

<drv mrk="bragx.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <dif>
      Metala a&jcirc;ura <ref tip="super" cel="fajr.0ujo">fajrujo</ref>,
      ofte portebla kaj metita sur piedo, uzata por teni ardajn <tld/>ojn
      kaj servi kiel <ref tip="super" cel="hejt">hejtilo</ref>.
    </dif>
  </snc>
  <trd lng="fr">brasero</trd>
</drv>
</art>
