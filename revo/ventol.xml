<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ventol</rad>/i
</kap>
<drv mrk="ventol.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Refre&scirc;igi la aeron, produktante aerondon:
      <ekz>
        <tld/>i tunelon, varman &ccirc;ambron.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="aer.0umi">aerumi</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_a;&c_v;&c_je;&c_t;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_v;&c_e;&c_n;&c_t;&c_y;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="hu">szell&odblac;ztet</trd>    
    <trdgrp lng="nl">
      <trd>luchten</trd>,
      <trd>ventileren</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_je;&c_n;&c_t;&c_i;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_v;&c_je;&c_t;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Purigi grenon, skuante &gcirc;in en aerondo, kiu forblovas la neta&ubreve;gan
      parton.
    </dif>
    <trd lng="be">&c_p;&c_r;&c_a;&c_v;&c_je;&c_j;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="hu">
      <trd>szelel</trd>,
      <trd>rost&aacute;l</trd>
    </trdgrp>
    <trd lng="nl">wannen</trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_v;&c_je;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="el">&alfa;&epsilon;&rho;&jota_ton;&zeta;&omega;</trd>
  <trd lng="en">ventilate</trd>
  <trdgrp lng="fr">
    <trd>ventiler</trd>, <trd>a&eacute;rer</trd>
  </trdgrp>
  <trd lng="pt">ventilar</trd>
</drv>

<drv mrk="ventol.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Aparato, kun turni&gcirc;antaj aletoj, por <tld/>i ejon.
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_e;&c_n;&c_t;&c_y;&c_l;&c_ja;&c_t;&c_a;&c_r;</trd>,
      <trd>&c_k;&c_u;&c_l;&c_je;&c_r;</trd>
    </trdgrp>
    <trd lng="fr">ventilateur</trd>
    <trd lng="hu">ventil&aacute;tor</trd>    
    <trd lng="nl">ventilator</trd>
    <trd lng="ru">&c_v;&c_je;&c_n;&c_t;&c_i;&c_l;&c_ja;&c_t;&c_o;&c_r;</trd>
  </snc>
  <snc>
    <dif>
      Speco de plata korbo por <tld/>i grenon.
    </dif>
    <trd lng="be">&c_v;&c_je;&c_ja;&c_l;&c_k;&c_a;</trd>
    <trd lng="nl">wan</trd>
    <trd lng="ru">&c_v;&c_je;&c_ja;&c_l;&c_k;&c_a;</trd>
  </snc>
  <trd lng="el">&alfa;&ny;&epsilon;&my;&jota;&sigma;&tau;&eta_ton;&rho;&alfa;&sigma_fina;</trd>
  <trd lng="pt">ventilador</trd>
</drv>
</art>
