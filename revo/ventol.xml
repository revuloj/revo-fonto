<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ventol.xml,v 1.27 2023/10/21 16:50:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ventol</rad>/i
</kap>
<drv mrk="ventol.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ventol.0i.KOMUNE">
    <dif>
      Refre&scirc;igi la aeron, produktante trablovon:
      <ekz>
        <tld/>i tunelon, varman &ccirc;ambron;
      </ekz>
      <ekz>
        nur malmultaj &ccirc;ambroj havis <klr>[&#x2026;]</klr> mallar&gcirc;ajn malferma&jcirc;ojn por <tld/>ado
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la 119-an &ccirc;ambron estas venigita novulo, sen&ccirc;ese balbutanta ion pri valuto en
        <tld/>truo
        <fnt>
          <bib>MkM</bib>
          <lok>13</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aer.0umi">aerumi</ref>,
      <ref cel="vent.0umi">ventumi</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_a;&c_v;&c_je;&c_t;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_v;&c_e;&c_n;&c_t;&c_y;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="bg">&c_i;&c_z;&c_l;&c_a;&c_g;&c_a;&c_m;</trd>
    <trdgrp lng="de">
      <trd>l&uuml;ften</trd>,
      <trd>ventilieren</trd>,
      <trd>Durchzug machen</trd>
    </trdgrp>
    <trd lng="hu">szell&odblac;ztet</trd>
    <trdgrp lng="nl">
      <trd>luchten</trd>,
      <trd>ventileren</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_je;&c_n;&c_t;&c_i;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_v;&c_je;&c_t;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
<!-- mi trovis tiusence nur ekz-ojn kun "ventumi"!
  <snc mrk="ventol.0i.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Purigi grenon, skuante &gcirc;in en trablovo, kiu forblovas la
      neta&ubreve;gan parton.
    </dif>
    <trd lng="be">&c_p;&c_r;&c_a;&c_v;&c_je;&c_j;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="de">
      <trd>windsichten</trd>,
      <trd>die <ind>Spreu</ind> vom Weizen trennen</trd>,
      <trd>worfeln</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>szelel</trd>,
      <trd>rost&aacute;l</trd>
    </trdgrp>
    <trd lng="nl">wannen</trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_v;&c_je;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
-->
  <trdgrp lng="cs">
    <trd>ventilovat</trd>,
    <trd>v&ecaron;trat</trd>
  </trdgrp>
  <trd lng="el">&alfa;&epsilon;&rho;&jota_ton;&zeta;&omega;</trd>
  <trd lng="en">ventilate</trd>
  <trdgrp lng="fr">
    <trd>a&eacute;rer</trd>,
    <trd>vanner</trd>,
    <trd>ventiler</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x63DB;&#x6C17;&#x3059;&#x308B;  <pr>&#x304B;&#x3093;&#x304D;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x98A8;&#x901A;&#x3057;&#x3092;&#x3088;&#x304F;&#x3059;&#x308B;  <pr>&#x304B;&#x305C;&#x3068;&#x304A;&#x3057;&#x3092;&#x3088;&#x304F;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="pt">ventilar</trd>
  <trdgrp lng="sk">
    <trd>prevetra&tcaron;</trd>,
    <trd>ventilova&tcaron;</trd>,
    <trd>vetra&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_je;&c_n;&c_t;&c_i;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_v;&c_ib;&c_t;&c_r;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="ventol.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="ventol.0ilo.KOMUNE">
    <dif>
      Aparato por <tld/>i, ordinare tia kun turni&gcirc;antaj aletoj:
      <ekz>
        <klr>[jen]</klr> turni&gcirc;adis <tld/>ilo lokita en la muro
        <fnt>
          <bib>VaK</bib>
          <lok>Dua &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        pasintece, kiam ajn siroko blovis (kutima fakto en Palermo), la <nom>Qanat</nom>-oj estis
        uzataj kiel <tld/>iloj, kiuj nature kaj efike malvarmigis la aeron
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011265.php">En Palermo subtere</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        povumaj procesoroj varmi&gcirc;as kaj postulas <tld/>ilon
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#x2116; 10</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="vent.0umilo">ventumilo</ref>
    <trdgrp lng="be">
      <trd>&c_v;&c_e;&c_n;&c_t;&c_y;&c_l;&c_ja;&c_t;&c_a;&c_r;</trd>,
      <trd>&c_k;&c_u;&c_l;&c_je;&c_r;</trd>
    </trdgrp>
    <trdgrp lng="bg">
      <trd>&c_f;&c_je;&c_n;</trd>
      <trd>&c_v;&c_je;&c_n;&c_t;&c_i;&c_l;&c_a;&c_t;&c_o;&c_r;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>L&uuml;fter</trd>
      <trd>Ventilator</trd>
    </trdgrp>
    <trd lng="hu">ventil&aacute;tor</trd>
    <trd lng="nl">ventilator</trd>
    <trd lng="ru">&c_v;&c_je;&c_n;&c_t;&c_i;&c_l;&c_ja;&c_t;&c_o;&c_r;</trd>
  </snc>
<!-- mi trovis tiusence nur ekz-ojn kun "ventumilo"!
  <snc mrk="ventol.0ilo.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Speco de plata korbo por <tld/>i grenon.
    </dif>
    <trd lng="be">&c_v;&c_je;&c_ja;&c_l;&c_k;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Worfschaufel</trd>,
      <trd>Sichter</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>szelel&odblac;rosta</trd>,
      <trd>gabonarosta</trd>
    </trdgrp>
    <trd lng="nl">wan</trd>
    <trd lng="ru">&c_v;&c_je;&c_ja;&c_l;&c_k;&c_a;</trd>
  </snc>
-->
  <trdgrp lng="cs">
    <trd>ventil&aacute;tor</trd>,
    <trd>v&ecaron;tr&aacute;k</trd>
  </trdgrp>
  <trd lng="el">&alfa;&ny;&epsilon;&my;&jota;&sigma;&tau;&eta_ton;&rho;&alfa;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>ventilator</trd>,
    <trd>ventilating fan</trd>,
    <trd>fan</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>a&eacute;rateur</trd>,
    <trd>pelle &agrave; vanner</trd>,
    <trd>van</trd>,
    <trd>ventilateur</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x63DB;&#x6C17;&#x88C5;&#x7F6E;  <pr>&#x304B;&#x3093;&#x304D;&#x305D;&#x3046;&#x3061;</pr></trd>,
    <trd>&#x30D9;&#x30F3;&#x30C1;&#x30EC;&#x30FC;&#x30BF;&#x30FC;</trd>,
    <trd>&#x63DB;&#x6C17;&#x6247;  <pr>&#x304B;&#x3093;&#x304D;&#x305B;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="pt">ventilador</trd>
  <trdgrp lng="sk">
    <trd>2.veja&ccaron;ka</trd>,
    <trd>ventil&aacute;tor</trd>,
    <trd>vetern&iacute;k</trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_je;&c_n;&c_t;&c_i;&c_l;&c_ja;&c_t;&c_o;&c_r;</trd>
</drv>
</art>
