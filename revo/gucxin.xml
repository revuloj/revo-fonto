<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gucxin.xml,v 1.6 2024/04/11 07:24:08 revo Exp $">
<kap>
  <rad>gu&ccirc;in</rad>/o
</kap>
<drv mrk="gucxin.0o">
  <kap><tld/>o</kap>
  <snc mrk="gucxin.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Antiva &ccirc;ina
      <ref tip="lst" cel="instru2.kord0o.MUZ"
      lst="voko:kordinstrumentoj">kordinstrumento</ref>,
      citro en nuntempa formo sepkorda, sen ponto kaj fretoj,
      kun milda sonoro kaj kapablo por songlitoj:
      <ekz>
        ni povas eltrovi poemojn pri <tld/>o en la &ccirc;ina antikva poemaro
        <ctl>Libro de Poezio</ctl>, kompilita de Konfuceo
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Gu%C4%89ino">
          gu&ccirc;ino</url>, 2010-12-23</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_c;&c_ib;&c_n;&c_mol; <klr>(&c_k;&c_ib;&c_t;&c_a;&c_j;&c_s;&c_k;&c_ib; &c_m;&c_u;&c_z;&c_y;&c_ch;&c_n;&c_y; &c_ib;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_e;&c_n;&c_t;)</klr></trd>
  <trdgrp lng="ca">
    <trd>qin</trd>,
    <trd>guqin</trd>
  </trdgrp>
  <trd lng="en">guqin</trd>
  <trdgrp lng="fr">
    <trd>guqin<klr> (musique)</klr></trd>,
    <trd>qin<klr> (musique)</klr></trd>
  </trdgrp>
</drv>
</art>

