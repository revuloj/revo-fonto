<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gu&ccirc;in</rad>/o
</kap>
<drv mrk="gucxin.0o">
  <kap><tld/>o</kap>
  <snc mrk="gucxin.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Antiva &ccirc;ina
      <ref tip="super" cel="instru2.kord0o.MUZ">kordinstrumento</ref>,
      citro en nuntempa formo sepkorda, sen ponto kaj fretoj,
      kun milda sonoro kaj kapablo por songlitoj:
      <ekz>
        ni povas eltrovi poemojn pri <tld/>o en la &ccirc;ina antikva poemaro 
        <ctl>Libro de Poezio</ctl>, kompilita de Konfuceo
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Gu%C4%89ino">
          gu&ccirc;ino</url>, 2010-12-23</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>guqin<klr> (musique)</klr></trd>,
    <trd>qin<klr> (musique)</klr></trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
-->
</vortaro>