<vortaro>
  <art mrk="$Id: rig1.xml,v 1.9 2024/04/14 05:46:54 afido Exp $">
    <kap>
      <rad>Rig</rad>/o
    </kap>

<drv mrk="rig1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      La <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&ccirc;efurbo</ref>
      de <ref tip="malprt" cel="latv.L0ujo">Latvujo</ref>:
      <ekz>
        el Litovio la a&ubreve;toj direkti&gcirc;is al ma&scirc;ina muzeo en latva &ccirc;efurbo <tld/>o
        <fnt><aut>Laimius Stra&zcaron;nickas</aut>: 
          <vrk><url ref="&Monato;/2000/007097.php">Per malnovaj a&ubreve;toj &ccirc;irka&ubreve; Balta Maro</url></vrk>,
          <bib>Monato</bib>
          <lok>2000/11, p. 25</lok>
        </fnt>;
      </ekz>
      <ekz>
        samtempe en Vilniuso, <tld/>o kaj Talino, en la lingvoj litova, latva kaj
        estona, estis kantata kanto &leftquot;Baltio jam veki&gcirc;as&rightquot;, populara anta&ubreve; du jardekoj
        <fnt>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2009/009908.php">Vojo al demokratio</url></vrk>
          <bib>Monato</bib>
          <lok>2009/11, p. 10</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_R;&c_y;&c_g;&c_a;</trd>
    <trd lng="de">Riga</trd>
    <trd lng="en">Riga</trd>
    <trd lng="fr">Riga</trd>
    <trd lng="nl">Riga</trd>
    <trd lng="pl">Ryga</trd>
    <trd lng="ro">Riga</trd>
  </snc>
  <trd lng="ja">&#x30EA;&#x30AC;</trd>
  <trd lng="uk">&c_R;&c_i;&c_g;&c_a; (&c_s;&c_t;&#x43E;&#x43B;&c_i;&c_c;&#x44F; &#x41B;&c_a;&c_t;&c_v;&c_ib;&c_ji;)</trd>
  <trd lng="zh">&#x91CC;&#x52A0; <pr>L&#x1D0;ji&#x101;</pr></trd>
</drv>

