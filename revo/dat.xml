<drv mrk="dat.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="dat.0i.KOMUNE">
    <dif>
      Provizi ion per <tld/>o:
      <ekz>
        letero de <nom>Tolstoj</nom> mem, <tld/>ita 27 IV 1894
        <fnt>
          <bib>Zam</bib>
        </fnt>;
      </ekz>
      <ekz>
        raporto, <tld/>ita la 28-an de junio 1922
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        tenu la &ccirc;ekon dum proksimume unu jaro kaj tiam <tld/>u &gcirc;in kaj prezentu &gcirc;in al
        la banko
        <fnt>
          <bib>KrM</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="br">deiziada&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>datovat</trd>,
    <trd>opat&rcaron;it datumem</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Zeitpunkt <ind>festlegen</ind></trd>,
    <trd>Zeitpunkt <ind>ausmachen</ind></trd>,
    <trd>Datum <ind>festlegen</ind></trd>,
    <trd>Datum <ind>ausmachen</ind></trd>,
    <trd>Tag ausmachen</trd>,
    <trd>Termin festlegen</trd>,
    <trd>sich (zeitlich) <ind>verabreden</ind></trd>,
    <trd>datieren</trd>,
    <trd>terminieren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>(set a) <ind>date</ind></trd>,
    <trd>(assign a) <ind>date</ind></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>fechar</trd>,
    <trd>datar</trd>
  </trdgrp>
  <trd lng="fr">dater<klr tip="ind"> (qc)</klr></trd>
  <trd lng="he">&lamed;&tsadi;&yod;&yod;&fnun; &tav;&alef;&resh;&yod;&fkaf;</trd>
  <trdgrp lng="hu">
    <trd>keltez</trd>,
    <trd>dat&aacute;l</trd>,
    <trd>d&aacute;tumoz</trd>
  </trdgrp>
