<drv mrk="dat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="dat.0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Preciza tempo de ia okaza&jcirc;o:
      <ekz>
        <ind>kiun <tld/>on ni havas</ind> hodia&ubreve;
        <fnt><bib>F</bib>, <lok>&FE; 12</lok></fnt>?
        <trd lng="hu">h&aacute;nyadika van?</trd>
      </ekz><ekz>
        la <tld/>o de nova konferenco nun ne povas esti difinita
        <fnt>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2002/007016.php">Balkanio:
          Balkana konferenco certigis 2,4 miliardojn da
          e&ubreve;roj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/08, p. 19</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <klr>[&#x2026;]</klr> balda&ubreve; festontaj sian oran
        edzi&gcirc;ofeston, sed ili ne povis precize rememori la <tld/>on
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur <klr>[&#x2026;]</klr> de&scirc;irebla tagfolio legi&gcirc;is per grandaj
        ciferoj la <tld/>o de l&#x2019; tago, 14a de Decembro 1900
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz><ekz>
        li ne povis memori la jarnumerojn de bataloj, la naski&gcirc;<tld/>ojn
        kaj regadtempojn de iuj personoj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Destino de Atrejo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;atu amikon la&ubreve; la <tld/>o de akiro
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="tag.0o.dato">tago<sncref/></ref>
    </refgrp>
  </snc>
  <rim mrk="dat.0o.RIM.1">
    Ne konfuzu kun <ref tip="vid" cel="datum.0o">datumo</ref>
    <aut>pp</aut>.
  </rim>
  <trd lng="be">&c_d;&c_a;&c_t;&c_a;</trd>
  <trd lng="bo">&t_tsha;&t_drengbu;&t_sa;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trd lng="br">deiziad</trd>
  <trd lng="cs">datum</trd>
  <trdgrp lng="de">
    <trd>Zeitpunkt</trd>,
    <trd>Datum</trd>,
    <trd>Termin</trd>
  </trdgrp>
  <trd lng="en">date</trd>
  <trd lng="es">fecha</trd>
  <trd lng="fr">date</trd>
  <trd lng="he">&tav;&#x5B7;&alef;&#x5B2;&resh;&#x5B4;&yod;&fkaf;</trd>
  <trdgrp lng="hu">
    <trd>d&aacute;tum</trd>,
    <trd>kelt</trd>,
    <trd>id&odblac;pont</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x65E5;&#x4ED8;  <pr>&#x3072;&#x3065;&#x3051;</pr></trd>,
    <trd>&#x5E74;&#x6708;&#x65E5;  <pr>&#x306D;&#x3093;&#x304C;&#x3063;&#x3074;</pr></trd>,
    <trd>&#x671F;&#x65E5;  <pr>&#x304D;&#x3058;&#x3064;</pr></trd>,
    <trd>&#x65E5;&#x53D6;&#x308A;  <pr>&#x3072;&#x3069;&#x308A;</pr></trd>
  </trdgrp>
  <trd lng="nl">datum</trd>
  <trd lng="pl">data</trd>
  <trdgrp lng="pt">
    <trd>data</trd>
  </trdgrp>
  <trd lng="ro">dat&abreve;</trd>
  <trd lng="ru">&c_d;&c_a;&c_t;&c_a;</trd>
  <trd lng="sk">d&aacute;tum</trd>
  <trd lng="sv">datum</trd>
  <trd lng="tr">tarih</trd>
  <trd lng="sk">d&aacute;tum, &uacute;daj</trd>
  <trd lng="sk">d&aacute;tum, &uacute;daj</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_ch;&c_i;&c_s;&#x43B;&#x43E; (&#x43C;&c_ib;&c_s;&#x44F;&c_c;&#x44F;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x65F6;&#x95F4;&#x70B9; <pr>sh&#xED;ji&#x101;ndi&#x1CE;n</pr></trd>,
    <trd>&#x65F6;&#x673A; <pr>sh&#xED;j&#x12B;</pr></trd>,
    <trd>&#x6CE8;&#x660E;&#x65E5;&#x671F; <pr>zh&#xF9;m&#xED;ngr&#xEC;q&#x12B;</pr></trd>,
    <trd>&#x65F6;&#x671F; <pr>sh&#xED;q&#x12B;</pr></trd>
  </trdgrp>
