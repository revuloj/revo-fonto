  <trd lng="ro">data</trd>
  <trd lng="ru">&c_d;&c_a;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">datova&tcaron;</trd>
  <trd lng="sv">datera</trd>
  <trd lng="tr">tarih atmak</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_a;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_r;&#x43E;&c_s;&c_t;&c_a;&c_v;&#x43B;&#x44F;&c_t;&c_i; &c_ch;&c_i;&c_s;&#x43B;&#x43E;</trd>
