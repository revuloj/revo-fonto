<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>klan</rad>/o
</kap>
<drv mrk="klan.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Grupo da individuoj, kiuj konsideras
      sin kiel parencoj unu al aliaj, sed kiuj rekonas ekskluzive
      tiun parencecon per tio, ke ili havas saman totemon. 
    </dif>
    <refgrp tip="sin">
      <ref cel="trib.0o">tribo</ref>,
      <ref cel="gent.0o">gento</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Sippe</trd>,
      <trd>Clan</trd>
    </trdgrp>
    <trd lng="en">clan</trd>
  </snc>
  <snc num="2">
    <dif>
      Partieta&ccirc;o, intrigantaro. 
    </dif>
    <refgrp tip="sin">
      <ref cel="koteri.0o">koterio</ref>,
      <ref cel="sekt.0o">sekto</ref>,
      <ref cel="kamari.0o">kamarilo</ref>.
    </refgrp>
    <trd lng="be">&c_k;&c_l;&c_a;&c_n;</trd>
    <trdgrp lng="de">
      <trd>Kl&uuml;ngel</trd>,
      <trd>Sippschaft</trd>
    </trdgrp>
  </snc>
  <trd lng="ru">&c_k;&c_l;&c_a;&c_n;</trd>
<trd lng="nl">clan</trd>
</drv>
</art>
