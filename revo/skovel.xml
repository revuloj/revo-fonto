<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: skovel.xml,v 1.6 2022/10/03 07:41:52 revo Exp $">
<kap>
  <rad>skovel</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="skovel.0o">
  <kap><tld/>o</kap>
  <snc mrk="skovel.0o.HEJM">
    <dif>
      Tubo<ref tip="super" cel="bros.0o.HEJM">broso</ref>
      kun cilindra hararo kaj verga tenilo por purigi
      ekzemple la internon de boteloj:
      <ekz>
        &gcirc;i estis <klr>[&#8230;]</klr> <tld/>o el malseka, blanka haro
        <fnt>
          <aut>G. K. Chesterton, trad. C. Bean</aut>,
          <vrk>La Naiveco de Pastro Brown</vrk>,
          <lok>1937</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_jo;&c_r;&c_sh;&c_y;&c_k; <klr tip="amb">(&c_sh;&c_ch;&c_o;&c_t;&c_k;&c_a;)</klr></trd>,
    <trd>&c_ch;&c_y;&c_s;&c_mol;&c_c;&c_jo;&c_l;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Rundb&uuml;rste</trd>
  <trdgrp lng="fr">
    <trd>&eacute;couvillon</trd>,
    <trd>goupillon<klr> (brosse)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_jo;&c_r;&c_sh;&c_i;&c_k;</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&#x43D;&#x43D;&c_i;&#x43A; (&c_d;&#x43B;&#x44F; &c_ch;&c_i;&c_shch;&c_je;&#x43D;&#x43D;&#x44F;
    &c_d;&c_i;&#x43C;&#x43E;&c_g;&c_a;&c_r;&#x43D;&c_i;&c_h; &c_t;&c_r;&c_u;&c_b;)</trd>,
    <trd>&c_shch;&c_ib;&c_t;&#x43A;&c_a;</trd>,
    <trd>&c_j;&#x43E;&c_r;&c_zh; (&c_d;&#x43B;&#x44F; &#x43C;&c_i;&c_t;&c_t;&#x44F; &#x43F;&#x43B;&#x44F;&c_sh;&#x43E;&#x43A;)</trd>
  </trdgrp>
</drv>
</art>

