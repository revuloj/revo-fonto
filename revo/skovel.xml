<art mrk="$Id: skovel.xml,v 1.5 2022/03/27 17:06:35 revo Exp $">
<kap>
  <rad>skovel</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="skovel.0o">
  <kap><tld/>o</kap>
  <snc mrk="skovel.0o.HEJM">
    <dif>
      Tubo<ref tip="super" cel="bros.0o.HEJM">broso</ref>
      kun cilindra hararo kaj verga tenilo por purigi
      ekzemple la internon de boteloj:
      <ekz>
        &gcirc;i estis <klr>[&#x2026;]</klr> <tld/>o el malseka, blanka haro
        <fnt>
          <aut>G. K. Chesterton, trad. C. Bean</aut>,
          <vrk>La Naiveco de Pastro Brown</vrk>,
          <lok>1937</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Rundb&uuml;rste</trd>
  <trdgrp lng="fr">
    <trd>&eacute;couvillon</trd>,
    <trd>goupillon<klr> (brosse)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_jo;&c_r;&c_sh;&c_i;&c_k;</trd>
  <trdgrp lng="uk">
    <trd>&#x431;&#x430;&#x43D;&#x43D;&#x438;&#x43A; (&#x434;&#x43B;&#x44F; &#x447;&#x438;&#x449;&#x435;&#x43D;&#x43D;&#x44F; &#x434;&#x438;&#x43C;&#x43E;&#x433;&#x430;&#x440;&#x43D;&#x438;&#x445; &#x442;&#x440;&#x443;&#x431;)</trd>,
    <trd>&#x449;&#x456;&#x442;&#x43A;&#x430;</trd>,
    <trd>&#x439;&#x43E;&#x440;&#x436; (&#x434;&#x43B;&#x44F; &#x43C;&#x438;&#x442;&#x442;&#x44F; &#x43F;&#x43B;&#x44F;&#x448;&#x43E;&#x43A;)</trd>
  </trdgrp>
</drv>
</art>

