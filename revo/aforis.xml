<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>aforism</rad>/o
</kap>
<drv mrk="aforis.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Konciza, sencori&ccirc;a principo, rilata al scienco a&ubreve; arto.
    </dif>
    <refgrp tip="sin">
      <ref cel="maksim.0o">maksimo</ref>,
      <ref cel="prover.0o">proverbo</ref>,
      <ref cel="senten.0o">sentenco</ref>.
    </refgrp>
  </snc>
  <trd lng="fr">aphorisme</trd>
  <trd lng="en">aphorism</trd>
  <trd lng="nl">aforisme</trd>
  <trd lng="pt">aforismo</trd>
  <trd lng="ru">&c_a;&c_f;&c_o;&c_r;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
