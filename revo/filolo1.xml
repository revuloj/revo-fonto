<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>filologi</rad>/o
</kap>
<drv mrk="filolo1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SCI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="scienc.0o.fako">Scienco</ref>, kiu helpe de la
      historio, literaturo, arkeologio, mitologio, popollegendaro, esploras la
      lingvojn kiel organojn de la spirita vivo de la popoloj. 
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_l;&c_ja;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trdgrp lng="de">
     <trd>Philologie</trd>,
     <trd>Sprach- und Literaturwissenschaft</trd></trdgrp>
  <trd lng="en">philology</trd>
  <trd lng="es">filolog&iacute;a</trd>
  <trd lng="fr">philologie</trd>
  <trd lng="hu">filol&oacute;gia</trd>  
  <trdgrp lng="nl">
   <trd>taalkunde</trd>,
   <trd>filologie</trd>
       </trdgrp>
  <trd lng="ru">&c_f;&c_i;&c_l;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
</drv>
<drv mrk="filolo1.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Scienculo, kiu studas la <tld/>on.  
    </dif>
    <ref tip="sin" cel="filolo.0o">filologo</ref>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_l;&c_jo;&c_l;&c_ja;&c_g;</trd>
  <trdgrp lng="de">
     <trd>Philologe</trd>,
     <trd>Sprachforscher</trd>,
     <trd>Literaturforscher</trd>,
     <trd>Sprach- und Literaturforscher</trd>,
     <trd>Sprach- und Literaturlehrer</trd></trdgrp>
  <trd lng="en">philologist</trd>
  <trd lng="es">fil&oacute;logo</trd>
  <trd lng="fr">philologue</trd>
  <trdgrp lng="hu">
    <trd>b&ouml;lcs&eacute;sz</trd>,
    <trd>filol&oacute;gus</trd>
  </trdgrp>
  <trdgrp lng="nl">
   <trd>filoloog</trd>,
   <trd>taalkundige</trd>
       </trdgrp>
  <trd lng="ru">&c_f;&c_i;&c_l;&c_o;&c_l;&c_o;&c_g;</trd>
</drv>
</art>
