<drv mrk="tegucigalp.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      La <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&ccirc;efurbo</ref> de <ref tip="malprt" cel="hondur.H0o">Honduro</ref>:
      <ekz>
        <tld/>o estis fondita en la 16-a jarcento, kiel centro de la mina aktivado de
        oro kaj ar&gcirc;ento
        <fnt>
          <vrk><url ref="https://dhjtyl.blogspot.com/2018/12/tegucigalpo.html"
              >Tegucigalpo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        s-ro <nom>John D. Negroponte</nom>, usona ambasadoro en <tld/>o inter 1981 kaj 1985
        <fnt>
          <vrk><url ref="http://eo.mondediplo.com/article936.html"
              >Le Monde diplomatique' en Esperanto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Tegucigalpa</trd>
    <trd lng="en">Tegucigalpa</trd>
    <trd lng="fr">Tegucigalpa</trd>
  </snc>
