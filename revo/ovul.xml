<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ovul</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="ovul.0o">
  <kap><tld/>o</kap>
  <snc mrk="ovul.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <ref tip="dif" cel="ov.0ocxelo">Ovo&ccirc;elo</ref>
    <trd lng="de">Eizelle</trd>
    <trd lng="hu">petesejt</trd>
    <trd lng="pl">kom&oacute;rka jajowa</trd>
    <trd lng="ru">&c_ja;&c_j;&c_c;&c_je;&c_k;&c_l;&c_je;&c_t;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="ovul.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Sfereca a&ubreve; ovala korpeto trovi&gcirc;anta en la
      <ref tip="malprt" cel="ovari.0o.BOT">ovario</ref> kaj kiu
      &ccirc;e formi&gcirc;o de frukto fari&gcirc;os
      <ref tip="vid" cel="grajn.sem0o">semgrajno</ref>.
    </dif>
    <trd lng="de">Samenanlage</trd>
    <trd lng="hu">magkezdem&eacute;ny</trd>
    <trd lng="ru">&c_s;&c_je;&c_m;&c_ja;&c_p;&c_o;&c_ch;&c_k;&c_a;</trd>
  </snc>
  <trd lng="be">&c_ja;&c_j;&c_c;&c_a;&c_k;&c_l;&c_je;&c_t;&c_k;&c_a;</trd>
  <trd lng="el">&omega;&alfa_ton;&rho;&jota;&omikron;</trd>
  <trd lng="fa">&a_t;&a_kh;&a_m;&f_k;</trd>
  <trd lng="fr">ovule</trd>
  <trd lng="he">&bet;&yod;&tsadi;&yod;&tav;</trd>
  <trd lng="nl">eitje</trd>
  <trd lng="pt">&oacute;vulo</trd>
</drv>

<drv mrk="ovul.0ado">
  <kap><tld/>ado</kap>
  <uzo tip="fak">FIZL</uzo>
  <ref tip="dif" cel="ovol.0ado">ovolado</ref>
  <ekz>
    oni preferu la antikoncipajn rimedojn, kiuj nur
    halt<klr>ig</klr>as la <tld/>adon
    <fnt>
      <aut>E. Pauli</aut>,
      <vrk><url ref="http://www.cfh.ufsc.br/~simpozio/">
      Enciklopedio simpozio</url></vrk>,
      <lok>artikolo &leftquot;antikoncipa&rightquot;, [1999]</lok>
    </fnt>
  </ekz>
  <trd lng="fr">ovulation</trd>
</drv>
</art>
