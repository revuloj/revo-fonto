<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>ovul</rad>/o</kap>
<drv mrk="ovul.0o">
  <kap><tld/>o</kap>
  <snc mrk="ovul.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      &Gcirc;ermo de la embrio, el kiu formi&gcirc;as feto a&ubreve; ovo.
    </dif>
  </snc>
  <snc mrk="ovul.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Sfereca a&ubreve; ovala korpeto trovi&gcirc;anta en la
      <ref tip="malprt" cel="ovari.0o.BOT">ovario</ref> kaj kiu
      &ccirc;e formi&gcirc;o de frukto fari&gcirc;os
      <ref tip="vid" cel="grajn.sem0o">semgrajno</ref>.
    </dif>
  </snc>
  <trd lng="nl">eitje</trd>
  <trd lng="ru">&c_ja;&c_j;&c_c;&c_je;&c_k;&c_l;&c_je;&c_t;&c_k;&c_a;</trd>
</drv>
</art>
