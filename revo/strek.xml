  <kap>voj<tld/>o</kap>
  <snc>
    <dif>
      Vojlinio:
      <ekz>
        mi irigos vin sur rekta voj<tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 4:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne iru la vojon kune kun ili, gardu vian piedon de ilia voj<tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 1:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la&ubreve; la voj<tld/>o de la justo
        <klr>(kp <ref cel="pad.0o">pado</ref> de la virto)</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 8:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        elegante kuradintaj tra la maratonaj voj<tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Husejn Al-Amily</aut>
          <vrk><url ref="&Monato;/2004/008404.html">Elmontri belecon de sporto</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        transdonu la pastron al mia dispono kaj vi fari&gcirc;os liberaj
        &dash; alie via voj<tld/>o fini&gcirc;os &ccirc;i tie
        <fnt>
        <aut>Jack London, trad. Reto Rossetti</aut>,
        <vrk>Dio de liaj prapatroj</vrk>,
        <lok>NLR, 1/4</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="trajek.0o">trajektorio</ref>,
      <ref cel="trak.0o">trako</ref>,
      <ref cel="sulk.voj0o">vojsulko</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_l;&c_ja;&c_h;</trd>,
    <trd>&c_t;&c_r;&c_a;&c_je;&c_k;&c_t;&c_o;&c_r;&c_y;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>cam&iacute;</trd>,
    <trd>sendera</trd>,
    <trd>traject&ograve;ria</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>cesti&ccaron;ka</trd>,
    <trd>p&ecaron;&scaron;ina</trd>,
    <trd>stezka</trd>
  </trdgrp>
  <trd lng="de">Wegstrecke</trd>
  <trdgrp lng="es">
    <trd>camino</trd>,
    <trd>sendero</trd>,
    <trd>trayectoria</trd>
  </trdgrp>
  <trd lng="fr">sentier</trd>
  <trd lng="hu">&uacute;tvonal</trd>
  <trdgrp lng="ja">
    <trd>&#x72ED;&#x3044;&#x9053;  <pr>&#x305B;&#x307E;&#x3044;&#x3069;&#x3046;</pr></trd>,
    <trd>&#x9698;&#x8DEF;  <pr>&#x3042;&#x3044;&#x308D;</pr></trd>
  </trdgrp>
  <trd lng="nl">weggetje</trd>
  <trdgrp lng="pl">
