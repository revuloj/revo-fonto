<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: martinik.xml,v 1.2 2025/01/11 08:31:19 afido Exp $">
    <kap>
      <ofc>10</ofc><rad>Martinik</rad>/o
    </kap>

<drv mrk="martinik.0o">
  <kap><ofc>10</ofc><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:insuloj" cel="insul.0o.GEOG">Insulo</ref> de
      <ref tip="malprt" cel="antil.Malgrandaj_0oj">Malgrandaj Antiloj</ref>, inter Dominiko kaj
      Sankta Lucio:
      <ekz>
        vizito de insulo post insulo! Trinidado, Barbados, <tld/>o
        <fnt>
          <bib>ChV</bib>
          <lok>16</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Martinique</trd>
    <trd lng="en">Martinique</trd>
    <trd lng="fr">Martinique</trd>
  </snc>
<trd lng="be">&c_M;&c_a;&c_r;&c_t;&c_y;&c_n;&c_ib;&c_k;&c_a;</trd>
</drv>

<drv mrk="martinik.0a">
  <kap>
    <ofc>10</ofc><tld lit="m"/>a
  </kap>
  <snc>
    <dif>
      Rilata al <tld/>o, &gcirc;iaj lo&gcirc;antaj kaj ties gvadelupa lingvo.
    </dif>
  </snc>
<trd lng="be">&c_m;&c_a;&c_r;&c_t;&c_y;&c_n;&c_ib;&c_k;&c_a;&c_n;&c_s;&c_k;&c_ib;</trd>
</drv>

<drv mrk="martinik.0ano">
  <kap>
    <ofc>10</ofc> <tld lit="m"/>ano
  </kap>
  <snc>
    <dif>
      Insulano de <tld/>o.
    </dif>
  </snc>
<trd lng="be">&c_m;&c_a;&c_r;&c_t;&c_y;&c_n;&c_ib;&c_k;&c_a;&c_n;&c_je;&c_c;</trd>
</drv>

  </art>

