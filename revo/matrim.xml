<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: matrim.xml,v 1.5 2024/01/25 20:10:35 revo Exp $">
<kap>
    <rad>matrimoni</rad>/o
</kap>
<drv mrk="matrim.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KRI</uzo>
  <uzo tip="fak">JUR</uzo>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="matrim.0o.edzeco">
    <dif>
      <ref tip="dif" cel="edz.0eco">Edzeco</ref>:
      <ekz>
        ordini al la sacerdoteco kandidatojn vivantajn a&ubreve; en fra&ubreve;leco a&ubreve;
        en <tld/>o
        <fnt>
          <vrk>Orientaj ortodoksaj eklezioj</vrk>
          <lok><url ref="&Viki;/Orientaj_ortodoksaj_eklezioj">Vikipedio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        en la tiamaj lernejoj oni akceptis nur la&ubreve;le&gcirc;ajn filojn de
        katolikaj <tld/>oj
        <fnt>
          <aut>Gabriel Garc&iacute;a M&aacute;rquez</aut>
          <vrk>Cent jaroj da soleco</vrk>
          <lok>tradukis el la hispana Fernando de Diego en 1992</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i esti&gcirc;is ne per konvertado, sed per <tld/>o
        <fnt>
          <aut>Sergio Pokrovskij</aut>,
          <vrk>Judoj a&ubreve; hebreoj (Estis: Kial rusoj kungenocidis kun
          serboj?)</vrk>,
          <lok><url ref="http://groups.google.com/d/msg/soc.culture.esperanto/YhdMZHGOoqU/Y5tjgS6ZTj8J">soc.culture.esperanto</url>,
          1999-08-08</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="matrim.0o.edzigxo">
    <dif>
      <ref tip="dif" cel="edz.0igxo">Edzi&gcirc;o</ref>:
      <ekz>
        &gcirc;in oni trinkas en gravaj festoj, kiel <tld/>oj
        <fnt>
          <vrk>Sakuro</vrk>
          <lok><url ref="&Viki;/Sakuro">Vikipedio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ar">&a_z;&a_w;&a_A;&a_j;</trd>
  <trdgrp lng="be">
    <trd>&c_sh;&c_l;&c_ju;&c_b;</trd>,
    <trd>&c_s;&c_u;&c_zh;&c_e;&c_n;&c_s;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">matrimoni</trd>
  <trdgrp lng="de">
    <trd>Matrimonium</trd>,
    <trd>Ehe</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>marriage</trd>,
    <trd>matrimony</trd>,
    <trd>wedlock</trd>
  </trdgrp>
  <trd lng="es">matrimonio</trd>
  <trdgrp lng="fr">
    <trd>mariage</trd>,
    <trd>noces<klr tip="ind"> (mariage)</klr></trd>
  </trdgrp>
  <trd lng="gl">matrimonio</trd>
  <trd lng="hu">h&aacute;zass&aacute;g<klr tip="ind"> (szents&eacute;g)</klr></trd>
  <trd lng="io">mariajo</trd>
  <trd lng="it">matrimonio</trd>
  <trd lng="nl">huwelijk</trd>
  <trd lng="pl">ma&lstroke;&zdot;e&nacute;stwo</trd>
  <trd lng="pt">casamento</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_a;&c_k;</trd>,
    <trd>&c_s;&c_u;&c_p;&c_r;&c_u;&#x301;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="zh">&#x5A5A;&#x59FB; <pr>h&#x16B;ny&#x12B;n</pr></trd>
</drv>
</art>

