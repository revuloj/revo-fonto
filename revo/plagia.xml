<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>5</ofc>
  <rad>plagiat</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="plagia.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Nelojala faro de tiu, kiu prezentas sub sia propra nomo verkon a&ubreve;
      parton de verko, kies a&ubreve;toro li ne estas.
    </dif>
  </snc>
  <trd lng="de">Plagiat</trd>
  <trd lng="nl">plagiaat</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_g;&c_i;&c_a;&c_t;</trd>
</drv>

<drv mrk="plagia.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Prezenti, kiel sian propran, verkon de aliulo.
    </dif>
  </snc>
  <trd lng="de">plagiieren</trd>
  <trd lng="nl">plagi&euml;ren</trd>
  <trd lng="ru">&c_s;&c_o;&c_v;&c_je;&c_r;&c_sh;&c_i;&c_t;&c_mol; &c_p;&c_l;&c_a;&c_g;&c_i;&c_a;&c_t;</trd>
</drv>
</art>
