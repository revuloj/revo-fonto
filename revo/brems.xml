<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
 <rad>brems</rad>/o
 <fnt>N</fnt>
</kap>
<drv mrk="brems.0o">
 <kap><tld/>o</kap>
 <snc>
  <dif>
   Mekanika aparato por malakceli,
   haltigi a&ubreve; reguligi movon a&ubreve; funkciadon de
   ma&scirc;ino: 
   <ekz>aerprema <tld/>o por trajnoj.</ekz>
  </dif>
 </snc>
 <trd lng="de">Bremse</trd>
 <trd lng="en">brake</trd>
 <trd lng="nl">rem</trd>
 <trd lng="ru">&c_t;&c_o;&c_r;&c_m;&c_o;&c_z;</trd>
 <trd lng="tr">fren</trd>
</drv>
<drv mrk="brems.0i">
 <kap><tld/>i</kap>
 <gra><vspec>tr</vspec></gra>
 <snc num="1">
  <dif>
   Haltigi a&ubreve; malrapidigi ion per <tld/>o. 
  </dif>
  <trd lng="tr">fren yapmak</trd>
 </snc>
 <snc num="2">
  <uzo tip="stl">FIG</uzo>
  <dif>
   Moderigi, reteni: 
   <ekz>mi devis <tld/>i tiun junulan riskemon.</ekz>
  </dif>
  <refgrp tip="sin">
   <ref cel="brid.0i">bridi</ref>.
  </refgrp>
 </snc>
 <trd lng="de">bremsen</trd>
 <trd lng="ru">&c_t;&c_o;&c_r;&c_m;&c_o;&c_z;&c_i;&c_t;&c_mol;</trd>
</drv>
<drv mrk="brems.0sxuo">
 <kap><tld/>&scirc;uo</kap>
 <snc>
  <dif>
   Malmola maso, kiu, &ccirc;e <tld/>ado,
   frotas sur la movi&gcirc;an pecon por haltigi a&ubreve;
   plimalrapidigi &gcirc;ian movon.  
  </dif>
 </snc>
 <trdgrp lng="ru">
   <trd>&c_t;&c_o;&c_r;&c_m;&c_o;&c_z;&c_n;&c_o;&c_j; &c_b;&c_a;&c_sh;&c_m;&c_a;&c_k;</trd>,
   <trd>&c_t;&c_o;&c_r;&c_m;&c_o;&c_z;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_l;&c_o;&c_d;&c_k;&c_a;</trd>
 </trdgrp>
</drv>
</art>
