<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
  <rad>brems</rad>/o <fnt><aut>K. Bennemann</aut>, <vrk>Esperanto
  Hand-W&ouml;rterbuch</vrk>, <lok>1923-1926</lok></fnt>
</kap>

<drv mrk="brems.0o">
  <kap><tld/>o</kap>
  <snc mrk="brems.0o.MAS">
    <uzo tip="fak">AUT</uzo>
    <uzo tip="fak">FER</uzo>
    <uzo tip="fak">MAS</uzo>
    <dif>
      Me&hcirc;anika aparato por malakceli,
      haltigi a&ubreve; reguligi movon a&ubreve; funkciadon de
      ma&scirc;ino:
      <ekz>
        aerprema <tld/>o por trajnoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_o;&c_r;&c_m;&c_a;&c_z;</trd>
  <trd lng="bg">&c_s;&c_p;&c_i;&c_r;&c_a;&c_ch;&c_k;&c_a;</trd>
  <trd lng="cs">brzda</trd>
  <trd lng="de">Bremse</trd>
  <trd lng="en">brake</trd>
  <trd lng="es">freno</trd>
  <trd lng="fr">frein</trd>
  <trd lng="hu">f&eacute;k</trd>
  <trd lng="it">freno</trd>
  <trd lng="nl">rem</trd>
  <trd lng="pl">hamulec</trd>
  <trd lng="ro">fr&acirc;n&abreve;</trd>
  <trd lng="ru">&c_t;&c_o;&c_r;&c_m;&c_o;&c_z;</trd>
  <trd lng="sk">brzda</trd>
  <trd lng="tr">fren</trd>
</drv>

<drv mrk="brems.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="brems.0i.MAS">
    <dif>
      <ref tip="malprt" cel="halt.0igi">Haltigi</ref>
      a&ubreve; malrapidigi per <tld/>o.
    </dif>
    <trd lng="pl">hamowa&cacute;</trd>
    <trd lng="tr">fren yapmak</trd>
  </snc>
  <snc mrk="brems.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="vid" cel="moder.0igi.KOMUNE">Moderigi</ref>,
      reteni,
      <ref tip="vid" cel="brid.0i.FIG">bridi</ref>:
      <ekz>
        mi devis <tld/>i tiun junulan riskemon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="buton.0umimalvaste.FRAZ">butonumi malvaste</ref>,
      <ref cel="katen.0i.FIG">kateni<sncref/></ref>
    </refgrp>
  <trdgrp lng="pl">
    <trd>hamowa&cacute;</trd>,
    <trd>spowalnia&cacute;</trd>
  </trdgrp>
 </snc>
  <trd lng="be">&c_t;&c_a;&c_r;&c_m;&c_a;&c_z;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_s;&c_p;&c_i;&c_r;&c_a;&c_m;</trd>
  <trd lng="cs">brzdit</trd>
  <trd lng="de">bremsen</trd>
  <trd lng="es">frenar</trd>
  <trd lng="fr">freiner</trd>
  <trd lng="hu">f&eacute;kez</trd>
  <trd lng="it">frenare</trd>
  <trd lng="ro">a fr&acirc;na</trd>
  <trd lng="ru">&c_t;&c_o;&c_r;&c_m;&c_o;&c_z;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>brzdi&tcaron;</trd>,
    <trd>zahamova&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="brems.0sxuo">
  <kap><tld/>&scirc;uo</kap>
  <snc mrk="brems.0sxuo.MAS">
    <uzo tip="fak">MAS</uzo>
    <dif>
      Malmola maso, kiu, &ccirc;e <tld/>ado,
      frotas sur la movi&gcirc;an pecon por haltigi a&ubreve;
      malrapidigi &gcirc;ian movon.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Bremsklotz</trd>,
    <trd>Bremsbacke</trd>
  </trdgrp>
  <trd lng="fr">patin <klr tip="ind">(de frein)</klr></trd>
  <trd lng="hu">f&eacute;kpofa</trd>
  <trdgrp lng="it">
    <trd>pattino<klr> (dei freni)</klr></trd>,
    <trd>pastiglia<klr> (dei freni)</klr></trd>,
    <trd>ganascia<klr> (dei freni a tamburo)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>klocek <ind>hamulcowy</ind> <klr>(np. przy hamulcach 
    tarczowych)</klr></trd>,
    <trd>ok&lstroke;adzina <ind>hamulca</ind> <klr>(np. przy hamulcach 
    b&eogonek;bnowych)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_r;&c_m;&c_o;&c_z;&c_n;&c_o;&c_j;
    &c_b;&c_a;&c_sh;&c_m;&c_a;&c_k;</trd>,
    <trd>&c_t;&c_o;&c_r;&c_m;&c_o;&c_z;&c_n;&c_a;&c_ja;
    &c_k;&c_o;&c_l;&c_o;&c_d;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
