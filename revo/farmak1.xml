<art mrk="$Id: farmak1.xml,v 1.18 2022/03/18 13:01:59 revo Exp $">
  <kap>
    <rad>farmakope</rad>/o
    <fnt><bib>FK</bib></fnt>
  </kap>

<drv mrk="farmak1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Farmacia libro, entenata normojn kaj regulojn pri la
      medikamentoj:
      <ekz>
        tiu &ccirc;i okulakvo <klr>[&#x2026;]</klr> la&ubreve; la nova A&ubreve;struja <tld/>o <klr>[&#x2026;]</klr> devas esti preparata en la sekvanta maniero
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        Seslingva Vortareto de Medikamentoj la&ubreve; Japana <tld lit="F"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=26681">Okamoto Jo&scirc;icugu</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_r;&c_m;&c_a;&c_k;&c_a;&c_p;&c_e;&c_ja;</trd>
  <trd lng="bg">&c_f;&c_a;&c_r;&c_m;&c_a;&c_k;&c_o;&c_p;&c_je;&c_j;&c_n;&c_a;</trd>
  <trd lng="ca">farmacopea</trd>
  <trd lng="cs">farmakopoea</trd>
  <trdgrp lng="de">
    <trd>Arzneibuch</trd>,
    <trd>Pharmakop&ouml;e</trd>
  </trdgrp>
  <trd lng="es">farmacopea</trd>
  <trd lng="fr">pharmacop&eacute;e</trd>
  <trdgrp lng="hu">
    <trd>farmakopea</trd>,
    <trd>gy&oacute;gyszerk&ouml;nyv</trd></trdgrp>
  <trd lng="nl">farmacopee</trd>
  <trd lng="pl">farmakopea</trd>
  <trd lng="pt">farmacop&eacute;ia</trd>
  <trd lng="ro">farmacopee</trd>
  <trd lng="ru">&c_f;&c_a;&c_r;&c_m;&c_a;&c_k;&c_o;&c_p;&c_je;&c_ja;</trd>
  <trd lng="sk">farmakopoea</trd>
  <trd lng="uk">&#x444;&#x430;&#x440;&#x43C;&#x430;&#x43A;&#x43E;&#x43F;&#x435;&#x44F;</trd>
</drv>
