<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sokl.xml,v 1.25 2023/10/21 07:46:47 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>sokl</rad>/o
</kap>
<drv mrk="sokl.0o">
  <kap><tld/>o</kap>
  <snc mrk="sokl.0o.bazo">
    <dif>
      <ref tip="super" cel="baz.0o.postamento">Bazo</ref> ordinare
      kvarflanka, sur kiu staras statuo, kolono, a&ubreve; konstrua&jcirc;o;
      <ref tip="dif" cel="piedes.0o">piedestalo</ref>:
      <ekz>
        unue oni volis konstrui tiun memoriga&jcirc;on el marmoro, sed pro la
        altaj kostoj oni realigis &gcirc;in per bronzo kun <tld/>o el polurita granito
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2013/011075.php">Poeto dan&gcirc;era</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o estas okangula kaj havas tri &scirc;tupojn, la plej supra parto de la
        <tld/>o konsistas el metalo kaj estas ornamita je la kvar anguloj
        <fnt>
          <vrk><url ref="&Viki;/Malnova_Bach-monumento_en_Lepsiko">Malnova Bach-monumento en
          Lepsiko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="postam.0o">postamento</ref>,
       <ref cel="fundam.0o.KON">fundamento<sncref/></ref>,
       <ref cel="abutme.0o">abutmento</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;'&c_je;&c_d;&c_e;&c_s;&c_t;&c_a;&c_l;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_t;&c_a;&c_m;&c_e;&c_n;&c_t;</trd>,
    <trd>&c_c;&c_o;&c_k;&c_a;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>patka <klr>(elektronky, sloupu)</klr></trd>,
    <trd>podstavec</trd>,
    <trd>sokl</trd>
  </trdgrp>
  <trd lng="de">Sockel</trd>
  <trdgrp lng="en">
    <trd>base</trd>,
    <trd>pedestal</trd>,
    <trd>plinth</trd>,
    <trd>socle</trd>
  </trdgrp>
  <trd lng="es">pedestal</trd>
  <trdgrp lng="fa">
    <trd>&f_p;&a_A;&f_y;&a_h;</trd>,
    <trd>&f_p;&a_A;&a_s;&a_n;&f_g;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>socle</trd>,
    <trd>soubassement</trd>,
  </trdgrp>
  <trdgrp lng="hu">
      <trd>oszlopsz&eacute;k</trd>,
      <trd>piedeszt&aacute;l</trd>,
      <trd>posztamens</trd>,
      <trd>szobortalapzat</trd>,
      <trd>talapzat</trd>
    </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x53F0;&#x5EA7;  <pr>&#12384;&#12356;&#12374;</pr></trd>,
    <trd>&#x53F0;&#x77F3;  <pr>&#12384;&#12356;&#12356;&#12375;</pr></trd>,
    <trd>&#x53F0;  <pr>&#x305F;&#12356;</pr></trd>,
    <trd>&#x53E3;&#x91D1;  <pr>&#x304F;&#12385;&#x304C;&#x306D;</pr></trd>,
    <trd>&#x5CF6;&#x68DA;  <pr>&#12375;&#x307E;&#x305F;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>sokkel</trd>,
    <trd>voetstuk</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>cok&oacute;&lstroke;</trd>,
    <trd>piedesta&lstroke;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_mol;&c_je;&c_d;&c_je;&c_s;&c_t;&c_a;&c_l;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_t;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_c;&c_o;&c_k;&c_o;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pobre&zcaron;n&aacute; plyt&ccaron;ina</trd>,
    <trd>&scaron;elf</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_c;&#x43E;&#x43A;&#x43E;&#x43B;&#x44C;</trd>,
    <trd>&#x43E;&c_s;&#x43D;&#x43E;&c_v;&c_a;</trd>,
    <trd>&#x43F;&c_ib;&c_d;&c_s;&c_t;&c_a;&c_v;&#x43A;&c_a;</trd>,
    <trd>&#x43F;&#8217;&c_jeu;&c_d;&c_je;&c_s;&c_t;&c_a;&#x43B;</trd>
  </trdgrp>
</drv>
</art>
