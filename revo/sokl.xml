<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>sokl</rad>/o
</kap>
<drv mrk="sokl.0o">
  <kap><tld/>o</kap>
  <snc mrk="sokl.0o.bazo">
    <dif>
      Bazo ordinare kvarflanka sur kiu staras kolono,
      statuo a&ubreve; konstrua&jcirc;o.
    </dif>
    <refgrp tip="sin">
      <ref cel="piedes.0o">piedestalo</ref>
    </refgrp>
  </snc>
  <snc mrk="sokl.0o.movebla">
    <dif>
      Translokebla bazo, sur kiu oni starigas statueton, vazon, ks.
    </dif>
    <refgrp tip="sin">
      <ref cel="postam.0o">postamento</ref>
    </refgrp>
  </snc>
  <refgrp tip="vid">
    <ref cel="stabl.0o">stablo</ref>
  </refgrp>
  <trd lng="de">Sockel</trd>
  <trdgrp lng="fr">
    <trd>pi&eacute;destal</trd>,
    <trd>pi&eacute;douche</trd>,
    <trd>socle</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>sokkel</trd>,
    <trd>voetstuk</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_mol;&c_je;&c_d;&c_je;&c_s;&c_t;&c_a;&c_l;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_t;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_c;&c_o;&c_k;&c_o;&c_l;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
