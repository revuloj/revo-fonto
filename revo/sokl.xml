<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>sokl</rad>/o
</kap>
<drv mrk="sokl.0o">
  <kap><tld/>o</kap>
  <snc mrk="sokl.0o.bazo">
    <dif>
      <ref tip="super" cel="baz.0o.postamento">Bazo</ref> ordinare
      kvarflanka, sur kiu staras statuo, kolono, a&ubreve; konstrua&jcirc;o:
      <ekz>
        unue oni volis konstrui tiun memoriga&jcirc;on el marmoro, sed pro la altaj kostoj oni
        realigis &gcirc;in per bronzo kun <tld/>o el polurita granito
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2013/011075.php"
              >Poeto dan&gcirc;era</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="postam.0o">postamento</ref>,
       <ref cel="fundam.0o.KON">fundamento<sncref/></ref>,
       <ref cel="abutme.0o">abutmento</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;'&c_je;&c_d;&c_e;&c_s;&c_t;&c_a;&c_l;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_t;&c_a;&c_m;&c_e;&c_n;&c_t;</trd>,
    <trd>&c_c;&c_o;&c_k;&c_a;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>patka <klr>(elektronky, sloupu)</klr></trd>,
    <trd>podstavec</trd>,
    <trd>sokl</trd>
  </trdgrp>
  <trd lng="de">Sockel</trd>
  <trdgrp lng="en">
    <trd>base</trd>,
    <trd>pedestal</trd>,
    <trd>plinth</trd>
  </trdgrp>
  <trd lng="es">pedestal</trd>
  <trdgrp lng="fr">
    <trd>socle</trd>,
    <trd>soubassement</trd>,
  </trdgrp>
  <trdgrp lng="hu">
      <trd>oszlopsz&eacute;k</trd>,
      <trd>piedeszt&aacute;l</trd>,
      <trd>posztamens</trd>,
      <trd>szobortalapzat</trd>,
      <trd>talapzat</trd>
    </trdgrp>
  <trdgrp lng="nl">
    <trd>sokkel</trd>,
    <trd>voetstuk</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_mol;&c_je;&c_d;&c_je;&c_s;&c_t;&c_a;&c_l;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_t;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_c;&c_o;&c_k;&c_o;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pobre&zcaron;n&aacute; plyt&ccaron;ina</trd>,
    <trd>&scaron;elf</trd>
  </trdgrp>
</drv>
</art>
