<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: oktant.xml,v 1.16 2017/05/26 16:10:21 revo Exp $">
<kap>
  <rad>oktant</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="oktant.0o">
  <kap><tld/>o</kap>
  <fnt><bib>PV</bib></fnt>
  <snc>
    <uzo tip="fak">MAT</uzo>
    <dif>
      Okono de <ref cel="cirkl.0o.linio">cirklo</ref>;
      <ref cel="angul.centra0o">centra angulo</ref>, kiu
      detran&ccirc;as tian arkon:
      <ekz>
        <ref cel="kvadra.0o">kvadranto</ref> konsistas el du <tld/>oj.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Oktant</trd>,
      <trd>Achtelkreis</trd>
    </trdgrp>
    <trd lng="en">octant</trd>
    <trd lng="hu">k&ouml;rnyolcad</trd>
    <trd lng="pl">oktant</trd>
    <trdgrp lng="pt">
      <trd>octante</trd>,
      <trd>oitante</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">AST</uzo>
    <dif>
      Instrumento por mezuri de sur la maro la alton kaj la angulajn
      distancojn inter la astroj.
    </dif>
    <trd lng="ca">octant</trd>
    <trd lng="de">Oktant</trd>
    <trd lng="es">octante</trd>
    <trd lng="hu">okt&aacute;ns</trd>
    <trd lng="pt">oitante</trd>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_t;&c_a;&c_n;&c_t;</trd>
  <trd lng="cs">oktant</trd>
  <trd lng="fr">octant</trd>
  <trd lng="nl">octant</trd>
  <trd lng="ru">&c_o;&c_k;&c_t;&c_a;&c_n;&c_t;</trd>
  <trd lng="sk">oktant</trd>
</drv>
<drv mrk="oktant.O0o">
  <kap><tld lit="O"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
  </kap>
  <mlg>Oct</mlg>
  <snc mrk="oktant.O0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Sud-hemisfera <ref tip="lst" cel="stel.0figuro"
      lst="voko:stelfiguroj">stelfiguro</ref>
      (<trd lng="la">Octans</trd>).
    </dif>
  </snc>
  <trd lng="en">Octans</trd>
  <trd lng="fr">Octant<klr tip="ind"> (constellation)</klr></trd>
</drv>
</art>
