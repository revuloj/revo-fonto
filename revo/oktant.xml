<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>oktant</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="oktant.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MAT</uzo>
    <dif>
      Okono de cirklo.
    </dif>
    <trdgrp lng="de">
      <trd>Oktant</trd>,
      <trd>Achtelkreis</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">AST</uzo>
    <dif>
      Instrumento por mezuri de sur la maro la alton kaj la angulajn
      distancojn inter la astroj.
    </dif>
    <trd lng="de">Oktant</trd>
    <trd lng="nl">octant</trd>
  </snc>
  <trd lng="ru">&c_o;&c_k;&c_t;&c_a;&c_n;&c_t;</trd>
</drv>
</art>
