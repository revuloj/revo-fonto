<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: oktant.xml,v 1.22 2024/01/30 16:09:26 revo Exp $">
<kap>
  <rad>oktant</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="oktant.0o">
  <kap><tld/>o</kap>
  <fnt><bib>PV</bib></fnt>
  <snc>
    <uzo tip="fak">MAT</uzo>
    <dif>
      Okono de <ref cel="cirkl.0o.linio">cirklo</ref>;
      <ref cel="angul.centra0o">centra angulo</ref>, kiu
      detran&ccirc;as tian arkon:
      <ekz>
        <ref cel="kvadra.0o">kvadranto</ref> konsistas el du <tld/>oj.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Oktant</trd>,
      <trd>Achtelkreis</trd>
    </trdgrp>
    <trd lng="en">octant</trd>
    <trd lng="hu">k&ouml;rnyolcad</trd>
    <trd lng="pl">oktant</trd>
    <trdgrp lng="pt">
      <trd>octante</trd>,
      <trd>oitante</trd>
    </trdgrp>
    <trd lng="ro">octant</trd>
  </snc>
  <snc>
    <uzo tip="fak">AST</uzo>
    <dif>
      Instrumento por mezuri de sur la maro la alton kaj la angulajn
      distancojn inter la astroj.
    </dif>
    <trd lng="ca">octant</trd>
    <trd lng="de">Oktant</trd>
    <trd lng="es">octante</trd>
    <trd lng="hu">okt&aacute;ns</trd>
    <trd lng="pl">oktant</trd> 
    <trd lng="pt">oitante</trd>
    <trd lng="ro">octant</trd>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_t;&c_a;&c_n;&c_t;</trd>
  <trd lng="cs">oktant</trd>
  <trd lng="fr">octant</trd>
  <trd lng="nl">octant</trd>
  <trd lng="ru">&c_o;&c_k;&c_t;&c_a;&c_n;&c_t;</trd>
  <trd lng="sk">oktant</trd>
  <trd lng="zh">&#x5366;&#x9650; <pr>gu&#xE0;xi&#xE0;n</pr></trd>
</drv>
<drv mrk="oktant.O0o">
  <kap><tld lit="O"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
  </kap>
  <mlg>Oct</mlg>
  <snc mrk="oktant.O0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Sud-hemisfera <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref>
      (<trd lng="la">Octans</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_A;&c_k;&c_t;&c_a;&c_n;&c_t; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#x2019;&c_je;)</klr></trd>
  <trd lng="en">Octans</trd>
  <trd lng="fr">Octant<klr tip="ind"> (constellation)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x516B;&#x5206;&#x5186;  <pr>&#x3084;&#x3063;&#x3075;&#x3093;&#x3048;&#x3093;</pr></trd>,
    <trd>&#x516B;&#x5206;&#x5100;  <pr>&#x306F;&#x3061;&#x3076;&#x3093;&#x304E;</pr></trd>
  </trdgrp>
  <trd lng="pl">Oktant  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Octantul  <klr>(constela&#x21B;ie)</klr></trd>
  <trd lng="uk">&#x43E;&#x43A;&c_t;&c_a;&#x43D;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#x5357;&#x6781;&#x5EA7; <pr>N&#xE1;nj&#xED;zu&#xF2;</pr></trd>,
    <trd>&#x5357;&#x6975;&#x5EA7; <pr>N&#xE1;nj&#xED;zu&#xF2;</pr></trd>
  </trdgrp>
</drv>
</art>
