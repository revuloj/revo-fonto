<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kvintesenc</rad>/o
</kap>
<drv mrk="kvinte.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Plej subtila <ref tip="sin" cel="esenc.0o">esenca</ref> parto de
substanco.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="baz.0o.principo">bazo</ref>,
    <ref cel="esenc.0o">esenco</ref>,
    <ref cel="fund.0o.esenco">fundo</ref>,
    <ref cel="fundam.0o.FIG">fundamento</ref>,
    <ref cel="kern.0o.esenco">kerno</ref>
  </refgrp>
  <ref tip="vid" cel="ide.0o">ideo</ref>
  
  <trd
lng="be">&c_k;&c_v;&c_ib;&c_n;&c_t;&c_e;&c_s;&c_e;&c_n;&c_c;&c_y;&c_ja;</trd
>
  <trd lng="ca">quinta ess&egrave;ncia</trd>
  <trdgrp lng="de">
     <trd>Quintessenz</trd>,
     <trd>Wesentlichstes</trd>,
     <trd>Hauptsache</trd>,
     <trd>Kern</trd>
  </trdgrp>
  <trd lng="en">quintessence</trd>
  <trd lng="es">quintaesencia</trd>
  <trd lng="fr">quintessence</trd>
  <trdgrp lng="hu">
     <trd>kvintesszencia</trd>,
     <trd>l&eacute;nyege</trd>,
     <trd>veleje</trd>
  </trdgrp>
  <trd lng="nl">kwintessens</trd>
  <trd lng="pl">kwintesencja</trd>
  <trd
lng="ru">&c_k;&c_v;&c_i;&c_n;&c_t;&c_e;&c_s;&c_s;&c_je;&c_n;&c_c;&c_i;&c_ja;
</trd>
</drv>
</art>
