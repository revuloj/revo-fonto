<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&scirc;mink</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="sxmink.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;miri la viza&gcirc;on per farbo.
    </dif>
  </snc>
  <trd lng="de">schminken</trd>
  <trd lng="nl">schminken</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_i;&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_u;&c_m;&c_ja;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_s;&c_i;&c_t;&c_mol; <klr tip="amb">(&c_l;&c_i;&c_c;&c_o;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="sxmink.0o">
  <kap><tld/>o<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Farbo 2.
    </dif>
  </snc>
  <trd lng="de">Schminke</trd>
  <trd lng="nl">schmink</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_i;&c_m;</trd>,
    <trd>&c_r;&c_u;&c_m;&c_ja;&c_n;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_a;&c_d;&c_a;</trd>
  </trdgrp>
</drv>
</art>
