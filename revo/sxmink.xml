<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxmink.xml,v 1.36 2023/12/16 23:24:48 revo Exp $">
<kap>
  <rad>&scirc;mink</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="sxmink.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kolorigi la viza&gcirc;on (ha&ubreve;ton, okulharojn ktp)
      a&ubreve; aliajn partojn de la korpo per tiucela farbo:
      <ekz>
        multe &scirc;i <tld/>as viza&gcirc;on por ka&scirc;i faldetojn
        <fnt>
          <bib>SkandalJozef</bib>
          <lok>Eldiras babilema negrino</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni <tld/>is la ha&ubreve;ton de la aktoro ludanta la
        rolon de Otelo;
      </ekz>
      <ekz>
        kiel pupeton vi povas min <tld/>i
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga
          Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Milena</nom> fortrinkas la kafon, stari&gcirc;as, iras al la spegulo en la
        &ccirc;ambro kaj komencas <tld/>i sin
        <fnt>
          <bib>Monato</bib>
          <aut>Julian Modest</aut>
          <vrk><url ref="&Monato;/2012/010807.php">La
          memmortigo de Tanja</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        nudon kaj friskon <tld/>is kvar altaj bretaroj plenaj je libroj
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 9</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_r;&c_y;&c_m;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_u;&c_m;&c_ja;&c_n;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_f;&c_a;&c_r;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">schminken</trd>
  <trd lng="en">put on <ind>make-up</ind></trd>
  <trd lng="es">maquillar</trd>
  <trd lng="fr">maquiller</trd>
  <trd lng="he">&lamed;&he;&tav;<ind>&alef;&pe;&resh;</ind></trd>
  <trdgrp lng="hu">
    <trd>beken<klr tip="ind"> (arcot)</klr></trd>,
    <trd>sminkel</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>truccare<klr> (imbellettare)</klr></trd>,
    <trd>imbellettare</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x304A;&#12375;&#x308D;&#12356;&#12434;&#22615;&#x308B;  <pr>&#x304A;&#12375;&#x308D;&#12356;&#12434;&#x306C;&#x308B;</pr></trd>,
    <trd>&#21270;&#x7CA7;&#12377;&#x308B;  <pr>&#12369;&#12375;&#12423;&#12358;&#12377;&#x308B;</pr></trd>,
    <trd>&#x30E1;&#x30FC;&#x30AF;&#x30A2;&#x30C3;&#x30D7;&#12377;&#x308B;
    <pr>&#12417;&#x30FC;&#x304F;&#12354;&#12387;&#12407;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">schminken</trd>
  <trd lng="pl">malowa&cacute; si&eogonek;</trd>
  <trdgrp lng="pt">
    <trd>maquilar(-se)</trd>,
    <trd>maquiar(-se)</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_i;&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_u;&c_m;&c_ja;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_s;&c_i;&c_t;&c_mol;
       <klr tip="amb">(&c_l;&c_i;&c_c;&c_o;)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_f;&c_a;&c_r;&c_b;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_u;&c_m;&#8217;&c_ja;&c_n;&c_i;&c_t;&c_i; (&c_o;&c_b;&c_l;&c_i;&c_ch;&c_ch;&c_ja;)</trd>,
    <trd>&c_g;&c_r;&c_i;&c_m;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="sxmink.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxmink.0o.ado">
    <dif>
      <tld lit="&Scirc;"/>ado, la ago kaj arto <tld/>i:
      <ekz>
        ni vizitis tataran teatron, spektis bonegan spektaklon
        <ctl>&Scirc;tona gasto</ctl> preparitan de &Ccirc;elnanoj,
        partoprenis prelegon pri teatra <tld/>o
        <fnt><bib>LOdE</bib></fnt>;
      </ekz>
      <ekz>
        per <tld/>okrajono &scirc;i emfazas la profundon de siaj okuloj kaj
        siajn humidajn molajn lipojn
        <fnt>
          <bib>Monato</bib>
          <aut>Julian Modest</aut>
          <vrk><url ref="&Monato;/2012/010807.php">La
          memmortigo de Tanja</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_g;&c_r;&c_y;&c_m;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
    <trdgrp lng="it">
      <trd>trucco<klr> (il truccare/si)</klr></trd>,
      <trd>il <ind>truccare</ind></trd>,
      <trd>il <ind>truccarsi</ind></trd>,
    </trdgrp>
    <trdgrp lng="pt">
      <trd>maquilagem</trd>,
      <trd>maquiagem</trd>
    </trdgrp>
  </snc>
  <snc mrk="sxmink.0o.desegno">
    <dif>
      <ref tip="dif" cel="sxmink.0ajxo.masko"><tld lit="&Scirc;"/>a&jcirc;o<sncref/></ref>:
      <ekz>
        efekta <tld/>o;
      </ekz>
      <ekz>
        provoka <tld/>o;
      </ekz>
      <ekz>
        &scirc;i &jcirc;etis rigardon en la spegulon por kontroli
        sian <tld/>on;
      </ekz>
      <ekz>
        mi havis treege putinan <tld/>on, miaj vestoj ne estis tre
        dignaj kaj de unu el miaj &scirc;uoj la plandumo pendis nefiksita
        <fnt>
          <aut>T. Menger</aut>,
          <vrk><url ref="http://homepage.mac.com/mkardol/Ne%20Tiam%2C%20Ne%20Nun%2C%20Neniam.pdf">
          Ne tiam &#8208; ne nun &#8208; neniam ...</url></vrk>,
          <lok>1982</lok>
        </fnt>;
      </ekz>
      <ekz>
        fra&ubreve;linoj, precipe hodia&ubreve; zorgu vian <tld/>on, vian
        dekolta&jcirc;on, vian afablecon &gcirc;enerale
        <fnt>
          <aut>L. Beaucaire</aut>,
          <vrk>Kruko kaj Baniko el Bervalo</vrk>,
          <lok>1974</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>batal<tld/>o</ind>.
        <trd lng="en">war paint</trd>
        <trd lng="de">Kriegsbemalung</trd>
        <trd lng="it">colori di guerra</trd>
        <trd lng="pl">maskowanie wojenne</trd>
        <trd lng="pt">pintura de guerra</trd>
        <trd lng="ru">&c_b;&c_o;&c_je;&c_v;&c_a;&c_ja; &c_r;&c_a;&c_s;&c_k;&c_r;&c_a;&c_s;&c_k;&c_a;</trd>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_g;&c_r;&c_y;&c_m;</trd>,
      <trd>&c_m;&c_a;&c_k;&c_ib;&c_ja;&c_zh;</trd>
    </trdgrp>
    <trd lng="de">Make-up</trd>
    <trd lng="it">trucco<klr> (risult. del truccare/si)</klr></trd>
    <trdgrp lng="pt">
      <trd>maquilagem</trd>,
      <trd>maquiagem</trd>
    </trdgrp>
  </snc>
  <snc mrk="sxmink.0o.farbo">
    <dif>
      <ref tip="dif" cel="sxmink.0ajxo.farbo"><tld lit="&Scirc;"/>a&jcirc;o<sncref/></ref>:
      <ekz>
        blanka, ru&gcirc;a <tld/>o;
      </ekz>
      <ekz>
        &scirc;iaj nigraj brovoj kaj okulharoj ne bezonis <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2006/008760.php">En
          fremda lando</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li &scirc;utetis <tld/>on de la vangoj de flaveta fra&ubreve;lino
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_g;&c_r;&c_y;&c_m;</trd>,
      <trd>&c_p;&c_a;&c_m;&c_a;&c_d;&c_a;</trd>,
      <trd>&c_r;&c_u;&c_m;&c_ja;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="de">Schminke</trd>
    <trdgrp lng="en">
      <trd>make-up</trd>,
      <trd>cosmetic</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>maquillaje</trd>,
      <trd>cosm&eacute;tico</trd>
    </trdgrp>
    <trd lng="he">&alef;&yod;&pe;&vav;&resh;</trd>
    <trdgrp lng="hu">
      <trd>arcfest&eacute;k</trd>,
      <trd>smink</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>trucco<klr> (cosmetico)</klr></trd>,
      <trd>belletto</trd>,
      <trd>cosmetico</trd>,
      <trd>tinta<klr> (cosmetico)</klr></trd>
    </trdgrp>
    <trd lng="nl">schmink</trd>
    <trdgrp lng="pl">
      <trd>szminka</trd>,
      <trd>pomadka do ust</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>maquilagem</trd>,
      <trd>maquiagem</trd>
    </trdgrp>
  </snc>
  <trd lng="es">maquillaje</trd>
  <trd lng="fr">maquillage</trd>
  <trdgrp lng="ja">
    <trd>&#x7D05;&#x304A;&#12375;&#x308D;&#12356;  <pr>&#x304F;&#x308C;&#x306A;&#12356;&#x304A;&#12375;&#x308D;&#12356;</pr></trd>,
    <trd>&#x7F8E;&#38996;&#26009;  <pr>&#12403;&#x304C;&#12435;&#x308A;&#12423;&#12358;</pr></trd>,
    <trd>&#21270;&#x7CA7;&#x54C1;  <pr>&#12369;&#12375;&#12423;&#12358;&#12402;&#12435;</pr></trd>,
    <trd>&#x30C9;&#x30FC;&#x30E9;&#x30F3;</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_i;&c_m;</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_u;&c_m;&#8217;&c_ja;&c_n;&c_a;</trd>,
    <trd>&c_g;&c_r;&c_i;&c_m;</trd>,
    <trd>&c_g;&c_u;&c_b;&c_n;&c_a; &c_p;&c_o;&c_m;&c_a;&c_d;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="sxmink.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="sxmink.0ajxo.masko">
    <dif>
      Rezulto de <tld/>ado;
      <ref tip="sin" cel="sxmink.0o.desegno"><tld/>o<sncref/></ref>:
      <ekz>
        estas konsilinde, ke virinoj evitu dikan <tld/>a&jcirc;on,
        &ccirc;ar la <tld/>a graso pliigas la malvarmi&gcirc;on de
        la ha&ubreve;to
        <fnt>
          <aut>Saliko</aut>,
          <vrk>Rezisti la froston</vrk>,
          <bib>Monato</bib>,
          <lok>2002</lok>
        </fnt>;
      </ekz>
      <ekz>
        eble en 1930 la perspektivo de <nom>Schwartz</nom> &scirc;ajnis ne
        moralisma, sed hodia&ubreve; mi perceptas dikan tavolon da
        hipokrito kaj romantika <tld/>a&jcirc;o inter lia bildo kaj
        la homaj kaj sociaj realoj, kiujn li desegnas
        <fnt>
          <aut>S. Johansson</aut>,
          <vrk><url ref="http://esperanto.net/literaturo/roman/libr/annimontmartrerec.html">
          Flue kaj bunte</url></vrk>,
          <lok>[2005?]</lok>
        </fnt>.
      </ekz>
    </dif>
   <trdgrp lng="be">
      <trd>&c_g;&c_r;&c_y;&c_m;</trd>,
      <trd>&c_m;&c_a;&c_k;&c_ib;&c_ja;&c_zh;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Make-up</trd>,
      <trd>Maske <klr>(Schminke)</klr></trd>
    </trdgrp>
    <trd lng="es">maquillaje</trd>
    <trd lng="it">trucco<klr> (risult. del truccare/si)</klr></trd>
  </snc>
  <snc mrk="sxmink.0ajxo.farbo">
    <dif>
      <ref tip="vid" cel="farb.0o.KOL">Farbo</ref>
      uzata por <tld/>i;
      <ref tip="sin" cel="sxmink.0o.farbo"><tld/>o<sncref/></ref>:
      <ekz>
        &scirc;i ne uzis lipru&gcirc;on a&ubreve; <tld/>a&jcirc;on
        <fnt>
          <aut>J. Modest</aut>, <vrk>En la taksio</vrk>,
          <bib>LOdE</bib>, <lok>2001:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i havas koran naivecon, puran animon kaj ne ka&scirc;as
        sian &ccirc;armon sub pudro, ru&gcirc;o kaj diversaj
        <tld/>a&jcirc;oj
        <fnt>
          <aut>J. Baghy</aut>, <vrk>Fraulino Degel</vrk>,
          <lok>en: Migranta plumo, 1929</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_g;&c_r;&c_y;&c_m;</trd>,
      <trd>&c_p;&c_a;&c_m;&c_a;&c_d;&c_a;</trd>,
      <trd>&c_r;&c_u;&c_m;&c_ja;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="de">Schminke</trd>
    <trd lng="es">cosm&eacute;tico</trd>
    <trdgrp lng="it">
      <trd>trucco<klr> (cosmetico)</klr></trd>,
      <trd>belletto</trd>,
      <trd>cosmetico</trd>,
      <trd>tinta<klr> (cosmetico)</klr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>maquillage</trd>,
    <trd>cosm&eacute;tique<klr> (produit)</klr></trd>
  </trdgrp>
  <trd lng="pl">makija&zdot;</trd>
  <trdgrp lng="pt">
     <trd>maquilagem</trd>,
     <trd>maquiagem</trd>
   </trdgrp>
</drv>
</art>
