<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: pune.xml,v 1.4 2022/07/04 22:31:15 afido Exp $">
    <kap>
      <rad>Pune</rad>/o
    </kap>

<drv mrk="pune.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Granda <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      en la okcidento de <ref tip="malprt" cel="barat.B0o">Barato</ref>,
      &ccirc;. 150&#8239;km sudoriente de <ref cel="mumbaj.0o">Mumbajo</ref>:
      <ekz>
        oni nomas <tld/>on la edukada &ccirc;efurbo de Mahara&scirc;tro
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Puneo">Puneo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        okazas en <tld/>o (en gubernio Mahara&scirc;tro de Barato) Esperanto-kurso por
        komencantoj plurfoje dum la jaro
        <fnt>
          <lok><url
          ref="https://uea.facila.org/loke/puneo-barato/">https://uea.facila.org/loke/puneo-barato/</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_P;&c_u;&c_n;&c_a; <klr>(&c_g;&c_o;&c_r;&c_a;&c_d; &c_u; &c_Ib;&c_n;&c_d;&c_y;&c_ib;)</klr></trd>
    <trd lng="de">Pune</trd>
    <trd lng="en">Pune</trd>
    <trd lng="fr">Pune</trd>
    <trd lng="pl">Pune</trd>
    <trd lng="ro">Pune</trd>
    <trd lng="mr">&#2346;&#2369;&#2339;&#2375;</trd>
  </snc>
</drv>

  </art>

