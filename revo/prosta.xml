<kap>
  <ofc>9</ofc><rad>prostat</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="prosta.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">ANA</uzo>
    <dif>
      &Ccirc;e viraj mamuloj inkluzive de homo, <ref tip="super" cel="gland.0o">glando</ref>
      produktanta parto de la <ref cel="sperm.0o">spermo</ref>, 
      trovi&gcirc;anta inter la vezika cerviko kaj uretro:
      <ekz>
        kancero de <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2014/011193.php">Malsama a&gcirc;o, malsamaj riskoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la semoplasmo estas kreata anka&ubreve; per aldonaj seksaj glandoj: flanktestikoj, la semoveziko kaj <tld/>o 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=38954">Spermo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">prostate gland</trd>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_s;&c_t;&c_a;&c_t;&c_a;</trd>
  <trd lng="ca">pr&ograve;stata</trd>
  <trdgrp lng="cs">
    <trd>prostata</trd>,
    <trd>&zcaron;l&aacute;za p&rcaron;edstojn&aacute;</trd>
  </trdgrp>
  <trd lng="de">Prostata</trd>
  <trd lng="fa">&f_p;&a_r;&a_w;&a_s;&a_t;&a_A;&a_t;</trd>
  <trd lng="fr">prostate</trd>
  <trdgrp lng="hu">
    <trd>d&uuml;lmirigy</trd>,
    <trd>prosztata</trd>
  </trdgrp>
  <trd lng="it">prostata</trd>
  <trd lng="ja">&#x524D;&#x7ACB;&#x817A;  <pr>&#x305C;&#x3093;&#x308A;&#x3064;&#x305B;&#x3093;</pr></trd>
  <trd lng="nl">prostaatklier</trd>
  <trdgrp lng="pl">                                        
    <trd>gruczo&lstroke; krokowy</trd>,                       
    <trd>prostata</trd>,                                   
  </trdgrp>
  <trdgrp lng="ru">   <trd>&c_p;&c_r;&c_je;&c_d;&c_s;&c_t;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_ja;
  &c_zh;&c_je;&c_l;&c_je;&c_z;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_a;&c_t;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>predstojn&aacute; &zcaron;&lcaron;aza</trd>,
    <trd>prostata</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x435;&#x440;&#x435;&#x434;&#x43C;&#x456;&#x445;&#x443;&#x440;&#x43E;&#x432;&#x430; &#x437;&#x430;&#x43B;&#x43E;&#x437;&#x430;</trd>,
    <trd>&#x43F;&#x440;&#x43E;&#x441;&#x442;&#x430;&#x442;&#x430;</trd>
