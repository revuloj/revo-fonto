    <kap>
      <rad>Bagdad</rad>/o
    </kap>

    <uzo tip="fak">GEOG</uzo>

    <drv mrk="bagdad.B0o">
      <kap>
        <tld/>o
      </kap>
      <snc>
        <dif>
          Urbo en <ref tip="malprt" cel="irak.0o.HIS">Irako<sncref/></ref>,
          sur la bordoj de <ref cel="tigris.0o">Tigriso</ref>
          (44&ring;26&#x2032;E, 33&ring;20&#x2032;N); la &ccirc;efurbo de la abasida
