<vortaro>
<art mrk="$Id$">
<kap><rad>Bagdad</rad>/o</kap>

<uzo tip="fak">GEOG</uzo>
<drv mrk="bagdad.B0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="urb.0o">Urbo</ref> en <ref tip="malprt"
      cel="irak.0o.HIS">Irako<sncref/></ref>,
      sur la bordoj de <ref cel="tigris.0o">Tigriso</ref>
      (44&ring;26&#8242;E, 33&ring;20&#8242;N); la &ccirc;efurbo de la 
abasida
      <ref cel="kalif.0ujo">&hcirc;alifujo</ref> en
      752&ndash;836 kaj 892&ndash;1258; nun la &ccirc;efurbo de
      <ref tip="malprt" cel="irak.0o.GEOG">Irako<sncref/></ref>.
    </dif>
  </snc>
  <trd lng="be">&c_B;&c_a;&c_g;&c_d;&c_a;&c_d;</trd>
  <trd lng="bg">&c_B;&c_a;&c_g;&c_d;&c_a;&c_d;</trd>
  <trd lng="de">Bagdad</trd>
  <trdgrp lng="en">
    <trd>Bagdad</trd>,
    <trd>Baghdad</trd>
  </trdgrp>
  <trd lng="es">Bagdad</trd>
  <trd lng="fr">Bagdad</trd>
  <trd lng="hu">Bagdad</trd>
  <trd lng="nl">Bagdad</trd>
  <trd lng="pl">Bagdad</trd>
  <trd lng="ru">&c_B;&c_a;&c_g;&c_d;&c_a;&c_d;</trd>
  <trd lng="sk">Bagdad</trd>
  <trd lng="sv">Bagdad</trd>
  <trd lng="uk">&c_B;&c_a;&c_g;&c_d;&c_a;&c_d;</trd>
</drv>
</art>
