<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>krotal</rad>/o
</kap>
<drv mrk="krotal.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="vipure.0oj">vipuredoj</ref>
      (<trd lng="la">Crotalus</trd>), amerikaj venenaj serpentoj kun
      moveblaj kornaj ringoj &ccirc;e la vosto. Per vibrigado de la vosto
      ili ka&ubreve;zas siblan sonon.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_y;&c_m;&c_u;&c_ch;&c_a;&c_ja; &c_z;&c_mol;&c_m;&c_ja;&c_ja;</trd>
  <trd lng="ca">cr&ograve;tal</trd>
  <trd lng="de">Klapperschlange</trd>
  <trd lng="en">rattlesnake</trd>
  <trd lng="es">cr&oacute;talo</trd>
  <trd lng="hu">cs&ouml;rg&odblac;k&iacute;gy&oacute;</trd>
  <trd lng="nl">ratelslang</trd>
  <trdgrp lng="pt">
    <trd>cr&oacute;talo</trd>,
    <trd>cascavel</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_je;&c_m;&c_u;&c_ch;&c_a;&c_ja; &c_z;&c_m;&c_je;&c_ja;</trd>
</drv>
</art>
