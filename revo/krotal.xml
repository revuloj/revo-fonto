<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: krotal.xml,v 1.23 2023/12/03 09:59:02 revo Exp $">
<kap>
  <rad>krotal</rad>/o
</kap>
<drv mrk="krotal.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="vipure.0oj">viperedoj</ref>
      (<trd lng="la">Crotalus</trd>), amerikaj venenaj 
      <ref tip="lst" cel="serpen.0o" lst="voko:serpentoj">serpentoj</ref> 
      kun moveblaj kornaj ringoj &ccirc;e la vosto,
      ka&ubreve;zante siblan sonon per &gcirc;ia vibrigado:
      <ekz>
        Kleopatra revenis en la kripton kaj la&ubreve; tradicio &scirc;i lasis sin
        venenmordi de <tld/>o, simbolo de la re&gcirc;a potenco
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=21340">Kleopatro</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Kvecalkoatlo</nom> estas prezentita kiel granda <tld/>o kun
        plumoj de la sankta birdo kvecalo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=238193">Kecalkoatlo
          (Mezameriko)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="serpen.son0o">*sonserpento</ref>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_y;&c_m;&c_u;&c_ch;&c_a;&c_ja; &c_z;&c_mol;&c_m;&c_ja;&c_ja;</trd>
  <trd lng="bg">&c_g;&c_malmol;&c_r;&c_m;&c_ja;&c_shch;&c_a; &c_z;&c_m;&c_i;&c_ja;</trd>
  <trd lng="ca">cr&ograve;tal</trd>
  <trd lng="de">Klapperschlange</trd>
  <trd lng="en">rattlesnake</trd>
  <trd lng="es">cr&oacute;talo</trd>
  <trdgrp lng="fr">
    <trd>crotale</trd>
     <trd>serpent &agrave; sonette</trd>
  </trdgrp>
  <trd lng="hu">cs&ouml;rg&odblac;k&iacute;gy&oacute;</trd>
  <trd lng="ja">&#x30AC;&#x30E9;&#x30AC;&#x30E9;&#x30D8;&#x30D3;</trd>
  <trd lng="nl">ratelslang</trd>
  <trd lng="pl">w&aogonek;&zdot; grzechotnik</trd> 
  <trdgrp lng="pt">
    <trd>cr&oacute;talo</trd>,
    <trd>cascavel</trd>
  </trdgrp>
  <trd lng="ro">crotalus</trd>
  <trd lng="ru">&c_g;&c_r;&c_je;&c_m;&c_u;&c_ch;&c_a;&c_ja; &c_z;&c_m;&c_je;&c_ja;</trd>
  <trd lng="uk">&c_s;&c_p;&c_r;&c_a;&c_v;&c_zh;&c_n;&c_ib;&c_j; &c_g;&c_r;&c_i;&c_m;&c_u;&c_ch;&c_n;&c_i;&c_k;</trd>
  <trdgrp lng="zh">
    <trd>&#x54CD;&#x5C3E;&#x86C7; <pr>xi&#x1CE;ngw&#x11B;ish&#xE9;</pr></trd>,
    <trd>&#x97FF;&#x5C3E;&#x86C7; <pr>xi&#x1CE;ngw&#x11B;ish&#xE9;</pr></trd>
  </trdgrp>
</drv>
</art>
