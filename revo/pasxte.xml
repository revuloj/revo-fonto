<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pasxte.xml,v 1.19 2023/10/20 14:25:20 revo Exp $">
<kap>
<ofc>4</ofc>
<rad>pa&scirc;tel</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="pasxte.0o">
  <kap><tld/>o</kap>
  <snc mrk="pasxte.0o.ilo">
    <dif>
      <ref tip="super" cel="krajon.0o">Krajono</ref>
      farita el pulvorigita kolorilo firmigita per gumo ks:
      <ekz>
        &scirc;i laboris kiel desegna&jcirc;instruisto <klr>[kaj]</klr> kreas precipe per <klr>[&#x2026;]</klr> <tld/>o kaj karbo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=68293">Katalin Koszt&aacute;ndi</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        dum sekva veturo al Italio <nom>De Nittis</nom> ekuzis <tld/>ojn, kiuj estus grava artilo por li en siaj ceteraj vivjaroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=138531">Giuseppe De Nittis</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la arto de <klr>(fakte per)</klr> pa&scirc;telo apartenas al la nomataj sekaj teknikoj de pentrarto,
        &ccirc;ar diference de la pentrarto je oleo a&ubreve; je akvarelo, &gcirc;i ne uzas ajnan solva&jcirc;on
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3430">Pentrado</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Pastellstift</trd>
  </snc>
  <snc mrk="pasxte.0o.ajxo">
    <dif>
      <ref tip="dif" cel="pasxte.0ajxo">Pa&scirc;tela&jcirc;o</ref>:
      <ekz>
        vico da krajonaj skizoj kaj e&ccirc; <tld/>oj de la sama grandeco kiel la propra bildo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=137732">Henri de Toulouse-Lautrec</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Pastellgem&auml;lde</trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_t;&c_e;&c_l;&c_mol;</trd>
  <trd lng="ca">pastel</trd>
  <trd lng="en">pastel</trd>
  <trd lng="es">pastel</trd>
  <trdgrp lng="fr">
    <trd>pastel</trd>
  </trdgrp>
  <trd lng="hu">pasztell</trd>
  <trdgrp lng="ja">
    <trd>&#x30D1;&#x30B9;&#x30C6;&#x30EB;</trd>,
    <trd>&#x30D1;&#x30B9;&#x30C6;&#x30EB;&#x753B;  <pr>&#x30D1;&#x30B9;&#x30C6;&#x30EB;&#x304C;</pr></trd>
  </trdgrp>
  <trd lng="nl">pastel</trd>
  <trd lng="pl">pastela</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_je;&c_l;&c_mol;</trd>
  <trd lng="uk">&#x443; &#x440;&#x456;&#x437;&#x43D;. &#x437;&#x43D;&#x430;&#x447;. &#x43F;&#x430;&#x441;&#x442;&#x435;&#x43B;&#x44C;</trd>
</drv>

<drv mrk="pasxte.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="bild.0o.prezentita">Bildo</ref>
      farita per pa&scirc;teloj:
      <ekz>
        <nom>Johann Philipp Bach</nom> kreis <klr>[&#x2026;]</klr> pli ol 100 <klr>[&#x2026;]</klr> portretojn (pentra&jcirc;ojn kaj <tld/>a&jcirc;ojn) 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=316166">Johann Philipp Bach</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        La krio estas <klr>[&#x2026;]</klr> donita al &ccirc;iu el la kvar versioj
        de kompona&jcirc;o, kreita kiel pentra&jcirc;oj kaj <tld/>a&jcirc;oj, de norvega 
        ekspresionista artisto <nom>Edvard Munch</nom> inter 1893 kaj 1910
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=612043">La krio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="pasxte.0o.ajxo">pa&scirc;telo<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_t;&c_e;&c_l;&c_mol;</trd>
  <trd lng="de">Pastellgem&auml;lde</trd>
  <trd lng="en">pastel</trd>
  <trdgrp lng="fr">
    <trd>pastel</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>pasztell</trd>,
    <trd>kr&eacute;tarajz</trd>
  </trdgrp>
  <trd lng="nl">pastel</trd>
  <trd lng="pl">pastela</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_je;&c_l;&c_mol;</trd>
</drv>
</art>
