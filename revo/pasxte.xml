<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>pa&scirc;tel</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="pasxte.0o">
  <kap><tld/>o</kap>
  <snc mrk="pasxte.0o.ilo">
    <dif>
      Krajono farita el pulvorigita kolorilo kaj gumo.
    </dif>
    <trd lng="de">Pastellstift</trd>
  </snc>
  <snc mrk="pasxte.0o.ajxo">
    <ref tip="dif" cel="pasxtel.0ajxo">pa&scirc;tela&jcirc;o</ref>
    <trd lng="de">Pastellgem&auml;lde</trd>
  </snc>
  <trd lng="en">pastel</trd>
  <trdgrp lng="fr">
    <trd>pastel</trd>
  </trdgrp>
  <trd lng="nl">pastel</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_je;&c_l;&c_mol;</trd>
</drv>

<drv mrk="pasxte.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Bildo farita per pa&scirc;teloj.
    </dif>
    <refgrp tip="sin">
      <ref cel="pasxte.0o.ajxo">pa&scirc;telo</ref>
    </refgrp>
  </snc>
  <trd lng="de">Pastellgem&auml;lde</trd>
  <trd lng="en">pastel</trd>
  <trdgrp lng="fr">
    <trd>pastel</trd>
  </trdgrp>
  <trd lng="nl">pastel</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_je;&c_l;&c_mol;</trd>
</drv>
</art>
