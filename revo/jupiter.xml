<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>Jupiter</rad>/o</kap>
<drv mrk="jupiter.0o">
  <kap><tld/>o</kap>
  <snc mrk="jupiter.0o.mita">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Romia supera <ref tip="super" cel="di.0o.politeisma">dio</ref>,
      la dio de lumo, tondro kaj fulmo.
    </dif>
  </snc>
  <snc mrk="jupiter.0o.planedo">
    <uzo tip="fak">AST</uzo>
    <dif>
      La kvina (la&ubreve; distanco de la <ref tip="vid"
      cel="sun.0o.nia">Suno</ref>) kaj la plej granda
      <ref tip="super" cel="planed.0o">planedo</ref>
      de la <ref tip="malprt" cel="sun.0sistemo">sunsistemo</ref>.
    </dif>
    <trd lng="zh">&#26408;&#26143;<klr> [M&ugrave;x&#299;ng]</klr></trd>
  </snc>
  <trd lng="de">Jupiter</trd>
  <trd lng="el">&Delta;&jota_acute;&alfa;&sigma_fina;</trd>
  <trd lng="en">Jupiter</trd>
  <trd lng="es">J&uacute;piter</trd>
  <trd lng="fr">Jupiter</trd>
  <trd lng="it">Giove</trd>
  <trd lng="ru">&c_Ju;&c_p;&c_i;&c_t;&c_je;&c_r;</trd>
</drv>
</art>

