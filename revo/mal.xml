<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mal</rad>-
</kap>
<drv mrk="mal.0">
  <kap>
    <ofc>*</ofc><tld/>-
  </kap>
  <snc>
    <dif>
      Prefikso montranta ideon logike kontra&ubreve;an al tiu, kiun montras
      la radiko:
      <ekz>
        <tld/>longa, <tld/>plena, <tld/>utila, <tld/>lumo,
        <tld/>akcepti, <tld/>aperi;
      </ekz>
      <ekz>
        ne<tld/>obeebla le&gcirc;o<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="ig">ig I- D) c) Rim. 2</ref>,
      <ref cel="de">de</ref>,
      <ref cel="dis">dis</ref>,
      <ref cel="sen">sen</ref>.
    </refgrp>
    <rim>
      Kelkfoje <em>mal</em> egalvaloras <em>ne</em>; tamen &gcirc;i
      &gcirc;enerale kaj propre ne entenas negativan, sed pozitivan
      ideon.
    </rim>
  </snc>
  <snc>
    <dif>
      Vortero:
      <ekz>
        <ind><tld/>o</ind> <klr>(kontra&ubreve;o)</klr>,
        <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
      </ekz>
      <ekz>
        <ind><tld/>a</ind> <klr>(kontra&ubreve;a)</klr>,
        <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>
      </ekz>
      <ekz>
        <ind><tld/>e</ind> <klr>(kontra&ubreve;e)</klr>.
        <trdgrp lng="ru">
          <trd>&c_n;&c_a;&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;</trd>,
          <trd>&c_n;&c_a;&c_o;&c_b;&c_o;&c_r;&c_o;&c_t;</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
