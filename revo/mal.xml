<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mal.xml,v 1.24 2024/01/25 20:10:33 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>mal</rad>-
</kap>
<subart>
<drv mrk="mal.0">
  <kap><tld/>-</kap>
  <snc>
    <dif>
      Prefikso montranta ideon logike kontra&ubreve;an al tiu, kiun montras
      la radiko:
      <ekz>
        <tld/>longa, <tld/>plena, <tld/>utila, <tld/>lumo,
        <tld/>akcepti, <tld/>aperi;
      </ekz>
      <ekz>
        ne<tld/>obeebla le&gcirc;o<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ig">ig I- 4- c) Rim. 2</ref>,
      <ref cel="ne.0">ne</ref>,
      <ref cel="dis.0">dis</ref>,
      <ref cel="sen.0_">sen</ref>.
    </refgrp>
    <rim>
      Kelkfoje <em>mal</em> egalvaloras <em>ne</em>; tamen &gcirc;i
      &gcirc;enerale kaj propre ne entenas negativan, sed pozitivan
      ideon.
    </rim>
    </snc>
    <trd lng="be">&c_p;&c_r;&c_e;&c_f;&c_ib;&c_k;&c_s;, &c_ja;&c_k;&c_ib; &c_n;&c_a;&c_d;&c_a;&c_je; &c_s;&c_u;&c_p;&c_r;&c_a;&c_c;&c_mol;&c_l;&c_je;&c_g;&c_l;&c_a;&c_je; &c_p;&c_a;&c_n;&c_ja;&c_c;&c_mol;&c_c;&c_je;</trd>
    <trd lng="es">prefijo formador de <ind>ant&oacute;nimos</ind></trd>
    <trd lng="io">des-</trd>
    <trd lng="nl">on-</trd>
    <trd lng="pl">przedrostek okre&sacute;laj&aogonek;cy przeciwie&nacute;stwo</trd>
    <trd lng="pt">prefixo formador de <ind>ant&ocirc;nimos</ind></trd>
    <trd lng="ro">prefix opus</trd>
</drv>
</subart>

<subart>
<dif>
Samsignifa memstara vortero.
</dif>
<drv mrk="mal.0o">
  <kap><tld/>o</kap>
  <snc>
  <dif>
  Kontra&ubreve;o:
  </dif>
      <ekz>
        rekte la <tld/>on de tio, kion vi deziris!<fnt>Z</fnt>
      </ekz>
  </snc>
  <trdgrp lng="be">
          <trd>&c_s;&c_u;&c_p;&c_r;&c_a;&c_c;&c_mol;&c_l;&c_je;&c_g;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
          <trd>&c_p;&c_r;&c_o;&c_c;&c_ib;&c_l;&c_je;&c_g;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
        </trdgrp>
  <trd lng="cs">opak</trd>
  <trd lng="en">opposite</trd>
  <trd lng="es">contrario<klr> (subst.)</klr></trd>
  <trd lng="fr">contraire<klr> (subst.)</klr></trd>
  <trdgrp lng="hu">
          <trd>ellenkez&odblac;je</trd>,
          <trd>ellent&eacute;te</trd>,
          <trd>ellentettje</trd>
        </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6B63;&#x53CD;&#x5BFE;  <pr>&#x305B;&#x3044;&#x306F;&#x3093;&#x305F;&#x3044;</pr></trd>,
    <trd>&#x9006;  <pr>&#x3055;&#x304B;</pr></trd>
  </trdgrp>
  <trd lng="nl">tegengestelde<klr tip="ind"> (zn.)</klr></trd>
  <trd lng="pl">przeciwie&nacute;stwo</trd>
  <trd lng="ro">contrar</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">opak</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&#x43E;&c_t;&c_i;&#x43B;&c_je;&c_zh;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&#x43F;&c_r;&#x43E;&c_t;&c_i;&#x43B;&c_je;&c_zh;&#x43D;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x76F8;&#x53CD; <pr>xi&#x101;ngf&#x1CE;n</pr></trd>,
    <trd>&#x76F8;&#x80CC; <pr>xi&#x101;ngb&#xE8;i</pr></trd>,
    <trd>&#x53CD;&#x4E49;&#x5B57; <pr>f&#x1CE;ny&#xEC;z&#xEC;</pr></trd>,
    <trd>&#x53CD;&#x7FA9;&#x5B57; <pr>f&#x1CE;ny&#xEC;z&#xEC;</pr></trd>,
    <trd>&#x6070;&#x6070;&#x76F8;&#x53CD; <pr>qi&#xE0;qi&#xE0;xi&#x101;ngf&#x1CE;n</pr></trd>,
    <trd>&#x5BF9;&#x7ACB;&#x9762; <pr>du&#xEC;l&#xEC;mi&#xE0;n</pr></trd>,
    <trd>&#x5C0D;&#x7ACB;&#x9762; <pr>du&#xEC;l&#xEC;mi&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mal.0a">
  <kap><tld/>a</kap>
  <snc>
  <dif>
  Prezentanta kontra&ubreve;econ:
  </dif>
      <ekz>
       kiel do interkonsentigi la du &scirc;ajne <tld/>ajn
       konkludojn?<fnt>Z</fnt>
      </ekz>
  </snc>
  <trdgrp lng="be">
          <trd>&c_s;&c_u;&c_p;&c_r;&c_a;&c_c;&c_mol;&c_l;&c_je;&c_g;&c_l;&c_y;</trd>,
          <trd>&c_p;&c_r;&c_o;&c_c;&c_ib;&c_l;&c_je;&c_g;&c_l;&c_y;</trd>
        </trdgrp>
  <trd lng="cs">opa&ccaron;n&yacute;</trd>
  <trd lng="es">contrario<klr> (adj.)</klr></trd>
  <trd lng="fr">contraire<klr> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>ellenkez&odblac;</trd>,
    <trd>ellent&eacute;tes</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6B63;&#x53CD;&#x5BFE;&#x306E;  <pr>&#x305B;&#x3044;&#x306F;&#x3093;&#x305F;&#x3044;&#x306E;</pr></trd>,
    <trd>&#x9006;&#x306E;  <pr>&#x304E;&#x3083;&#x304F;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">tegengestelde<klr tip="ind"> (bn.)</klr></trd>
  <trdgrp lng="pl">
    <trd>przeciwny</trd>,
    <trd>przeciwstawny</trd>,
    <trd>odwrotny</trd>,
    <trd>opaczny</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>contr&aacute;rio <klr>(subst.)</klr></trd>,
    <trd>ant&ocirc;nimo</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>contrar</trd>,
    <trd>antagonic</trd>,
    <trd>invers</trd>,
    <trd>gre&#x219;it</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">opa&ccaron;n&yacute;</trd>
  <trd lng="uk">&#x43F;&c_r;&#x43E;&c_t;&c_i;&#x43B;&c_je;&c_zh;&#x43D;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x76F8;&#x53CD; <pr>xi&#x101;ngf&#x1CE;n</pr></trd>,
    <trd>&#x76F8;&#x80CC; <pr>xi&#x101;ngb&#xE8;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mal.0e">
  <kap><tld/>e</kap>
  <snc>
  <dif>
  En kontra&ubreve;a senco:
  </dif>
     <ekz>
        vian proponon mi ne for&jcirc;etas, tute <tld/>e!
     </ekz>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_a;&c_d;&c_v;&c_a;&c_r;&c_o;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>naopak</trd>,
    <trd>opa&ccaron;n&ecaron;</trd>
  </trdgrp>
  <trd lng="en">on the contrary</trd>
  <trd lng="es">contrariamente</trd>
  <trdgrp lng="fr">
    <trd>au contraire</trd>,
    <trd>contrairement<klr> (&agrave;)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ellenkez&odblac;leg</trd>,
    <trd>ellent&eacute;tesen</trd>,
    <trd>ellent&eacute;tben</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x53CD;&#x5BFE;&#x306B;  <pr>&#x306F;&#x3093;&#x305F;&#x3044;&#x306B;</pr></trd>,
    <trd>&#x9006;&#x306B;  <pr>&#x304E;&#x3083;&#x304F;&#x306B;</pr></trd>
  </trdgrp>
  <trd lng="nl">integendeel</trd>
  <trdgrp lng="pl">
    <trd>natomiast</trd>,
    <trd>przeciwnie</trd>,
    <trd>za&sacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>ao <ind>contr&aacute;rio</ind></trd>,
    <trd>contrariamente</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>&icirc;n timp ce</trd>,
    <trd>contrar</trd>,
    <trd>&#x219;i</trd>,
    <trd>iar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;</trd>,
    <trd>&c_n;&c_a;&c_o;&c_b;&c_o;&c_r;&c_o;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>naopak</trd>,
    <trd>opa&ccaron;ne</trd>
  </trdgrp>
  <trd lng="uk">&#x43D;&c_a;&c_v;&#x43F;&c_a;&#x43A;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x53CD;&#x5012; <pr>f&#x1CE;nd&#xE0;o</pr></trd>,
    <trd>&#x53CD;&#x4E4B; <pr>f&#x1CE;nzh&#x12B;</pr></trd>
  </trdgrp>
</drv>
</subart>
</art>

