<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>okzal</rad>/o <fnt><bib>PV</bib></fnt> 
</kap>
<drv mrk="okzal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="okzal.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      plurjara herbo kun acidaj
      man&gcirc;eblaj folioj; specio el familio <ref tip="super"
      cel="poligo1.0oj">poligonacoj</ref>
      (<trd lng="la">Rumex acetosa</trd>):
      <ekz>
        silente ni kuleradis la <tld/>supon
        <fnt>
          <aut>S. Lang, trad. G. Berveling</aut>,
          <vrk>La Talpo&ccirc;asisto</vrk>,
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>                     
  <trd lng="be">&c_sh;&c_ch;&c_a;&c_w;&c_je;</trd>
  <trd lng="bg">&c_k;&c_i;&c_s;&c_je;&c_l;&c_je;&c_c;</trd>
  <trd lng="br">tri&ntilde;chin</trd>
  <trd lng="de">Sauerampfer</trd>
  <trd lng="en">sorrel</trd>
  <trd lng="fa">&a_t;&a_r;&a_sh;&f_k; &a_b;&a_A;&a_gh;&f_y;</trd>
  <trd lng="fr">oseille</trd>
  <trd lng="hu">mezei <ind>s&oacute;ska</ind></trd>
  <trd lng="nl">klaverzuring</trd>
  <trd lng="pt">azeda</trd>
  <trd lng="ru">&c_shch;&c_a;&c_v;&c_je;&c_l;&c_mol;</trd>
</drv>
</art>
