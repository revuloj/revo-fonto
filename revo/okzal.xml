<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>okzal</rad>/o
</kap>
<drv mrk="okzal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio el familio <ref tip="super" cel="poligo1.0oj">poligonacoj</ref>
      (<trd lng="la">Rumex acetosa</trd>), plurjara herbo kun acidaj
      man&gcirc;eblaj folioj.
    </dif>
  </snc>                     
  <trd lng="be">&c_sh;&c_ch;&c_a;&c_w;&c_je;</trd>
  <trd lng="br">tri&ntilde;chin</trd>
  <trd lng="de">Sauerampfer</trd>  
  <trd lng="fr">oseille</trd>
  <trd lng="hu">mezei <ind>s&oacute;ska</ind></trd>
  <trd lng="nl">klaverzuring</trd>
  <trd lng="ru">&c_shch;&c_a;&c_v;&c_je;&c_l;&c_mol;</trd>
</drv>
</art>
