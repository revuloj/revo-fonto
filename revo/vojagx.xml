<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>voja&gcirc;</rad>/i
    </kap>

    <drv mrk="vojagx.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
        <dif>
          Iri, veturi por atingi malproksiman urbon a&ubreve; landon:
          <ekz>
            mi <tld/>as en Hispanujo
            <fnt><bib>F</bib>, <lok>&FE; 26</lok></fnt>;
          </ekz><ekz>
            mi <tld/>as en Hispanujon
            <fnt><bib>F</bib>, <lok>&FE; 26</lok></fnt>;
          </ekz><ekz>
            <tld/>i eksterlanden;
          </ekz><ekz>
            neniam tra<tld/>ita lando<fnt>Z</fnt>.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="ekskur">ekskursi</ref>,
          <ref cel="migr.0i">migri</ref>
        </refgrp>
      </snc>
      <trdgrp lng="be">
        <trd>&c_p;&c_a;&c_d;&c_a;&c_r;&c_o;&c_zh;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_v;&c_a;&c_n;&c_d;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
      </trdgrp>
      <trd lng="br">beaji&ntilde;</trd>
      <trd lng="cs">cestovat</trd>
      <trd lng="de">reisen</trd>
      <trd lng="el">&tau;&alfa;&xi;&jota;&delta;&epsilon;&ypsilon_ton;&omega;</trd>
      <trd lng="en">travel</trd>
      <trd lng="es">viajar</trd>
      <trd lng="fr">voyager</trd>
      <trdgrp lng="he">
        <trd>&lamed;&nun;&samekh;&vav;&ayin;</trd>,
        <trd>&lamed;&tet;&yod;&yod;&lamed;</trd>
      </trdgrp>
      <trd lng="hu">utazik</trd>
      <trd lng="nl">reizen</trd>
      <trd lng="pt">viajar</trd>
      <trd
lng="ru">&c_p;&c_u;&c_t;&c_je;&c_sh;&c_je;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </drv>

    <drv mrk="vojagx.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Iom longa irado de iu loko al alia:
          <ekz>
            plezura, turisma, eksterlanda <tld/>o;
          </ekz><ekz>
            <ctl><tld lit="V"/>o al Kazohinio</ctl>
            <klr>(romano de Szathm&aacute;ri S&aacute;ndor)</klr>
            <fnt><aut>Szathm&aacute;ri S&aacute;ndor</aut>,
              <vrk>Voja&gcirc;o al Kazohinio. &dash; Parizo: SAT, 1958</vrk></fnt>;
          </ekz><ekz>
            kion spertas afrikano post <tld/>o en eksterlandon
            <fnt>
              <aut>Wolfram ROHLOFF</aut>:
              <vrk>&Ccirc;u vi rimarkos komunajn trajtojn?</vrk>.
              <bib>Monato</bib></fnt>;
          </ekz><ekz>
            vi forgesas tiun memorindan <tld/>on en Parizon <klr>...</klr>
            <fnt>
              <aut>Henri Vallienne</aut>: <vrk>Kastelo de Prelongo</vrk>.
              <lok>Inko Tyres&ouml;, 2002</lok>.
            </fnt>;
          </ekz><ekz>
            li senprokraste komencis prepari sin al <tld/>o en Moskvon
            <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 18a</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <rim>
        Kiel kutime &ccirc;e la direkta akuzativo, eblas diri kaj
        <ekz>
          li multe voja&gcirc;is en la Oriento,
        </ekz>
        kaj
        <ekz>
          li multe voja&gcirc;is en la Orienton;
        </ekz>
        tamen la senco estas ne tute sama: en la unua ekzemplo temas
        pri multaj voja&gcirc;oj ene de la Oriento (supozeble, sen reveni en
        sian hejmon en E&ubreve;ropo); do, <ctl>la Oriento</ctl> tiam estas
        loka cirkonstanco.  En la dua okazo temas pri multaj voja&gcirc;oj el
        ekster <ctl>la Oriento</ctl>, do tiam <ctl>la Oriento</ctl>
        estas cela cirkonstanco.
      </rim>
      <trd lng="br">beaj</trd>
      <trd lng="de">Reise</trd>
      <trd lng="es">viaje</trd>
      <trd lng="fr">voyage</trd>
      <trd lng="hu">utaz&aacute;s</trd>
      <trdgrp lng="ru">
        <trd>&c_p;&c_u;&c_t;&c_je;&c_sh;&c_je;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>,
        <trd>&c_p;&c_o;&c_je;&c_z;&c_d;&c_k;&c_a;</trd>
      </trdgrp>
    </drv>

    <drv mrk="vojagx.esplor0i">
      <kap>esplor<tld/>i</kap>
      <snc mrk="vojagx.esplor0i.GEOG">
        <uzo tip="fak">GEOG</uzo>
        <dif>
          <ref tip="super" cel="vojagx.0i"><tld lit="V"/>i</ref> en anta&ubreve;e
          nekonata lando por ekkoni &gcirc;ian naturan kaj homan aspekton:
          <ekz>
            la grandaj esplor<tld/>oj, kiuj konigos al E&ubreve;ropo la Amerikojn
            <fnt>
              <aut>Donald Broadribb</aut>,
              <vrk>La Purgatorio vidita de dektriajarcentulo (Dante Alighieri,
                Purgatorio)</vrk>,
              <bib>Monato</bib>,
              <lok>jaro 2002a, numero 5a, p. 27a</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="br">ergerzhet</trd>
      <trd lng="de">Entdeckungsreise</trd>
      <trd lng="fr">explorer<klr> (g&eacute;og.)</klr></trd>
      <trd lng="hu">felfedez&odblac;&uacute;t</trd>
      <trd
        lng="ru">&c_p;&c_u;&c_t;&c_je;&c_sh;&c_je;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;<klr
          tip="amb"> (&c_g;&c_je;&c_o;&c_g;&c_r;.)</klr></trd>
    </drv>

    <drv mrk="vojagx.kun0i">
      <kap>kun<tld/>i</kap>
      <snc>
        <dif>
          Kune, grupe, interhelpe <ref tip="super"
            cel="vojagx.0i"><tld/>i</ref>:
          <ekz>
            de tiam li i&gcirc;is mia <ind>kun<tld/>anto</ind>
            <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 2a, p. 19a</lok></fnt>.
            <trd lng="br">kenveajour</trd>
            <trd lng="de">Mitreisender</trd>
          </ekz>
        </dif>
        <ref tip="vid" cel="akompa.0i.1">akompani <sncref/></ref>
      </snc>
      <trd lng="de">zusammen <ind>reisen</ind></trd>
      <trdgrp lng="fr">
        <trd>accompagner<klr> (en voyage)</klr></trd>,
        <trd>suivre<klr> (en voyage)</klr></trd>
      </trdgrp>
      <trd lng="hu">egy&uuml;tt <ind>utazik</ind></trd>
      <trd lng="ru">&c_p;&c_u;&c_t;&c_je;&c_sh;&c_je;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;
        &c_v;&c_m;&c_je;&c_s;&c_t;&c_je;</trd>
    </drv>

    <drv mrk="vojagx.rond0i">
      <kap>rond<tld/>i</kap>
      <snc>
        <dif>
          Vizite <ref tip="super" cel="vojagx.0i"><tld/>i</ref>, haltante en
          pluraj lokoj kaj revenonte al la deirejo:
          <ekz>
            La&ubreve; &scirc;an&gcirc;i&gcirc;o de la sezonoj diversaj fi&scirc;oj
            rond<tld/>as la insularon
            <fnt><aut>Hori Yasuo</aut>, <vrk>La man&gcirc;ota fi&scirc;o estas ankora&ubreve; en
                la ... bazaro</vrk>, <bib>Monato</bib>, <lok>jaro 2002a, numero
                10a, p. 17a</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="de">Rundreise</trd>
      <trdgrp lng="fr">
        <trd>faire un <ind>circuit</ind></trd>,
        <trd>faire un <ind>tour</ind><klr> (voyage)</klr></trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>k&ouml;rbeutazik</trd>,
        <trd>k&ouml;rutat tesz</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; <ind>&c_t;&c_u;&c_r;&c_n;&c_je;</ind></trd>
      </trdgrp>
    </drv>
  </art>
