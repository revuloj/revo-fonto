<drv mrk="vojagx.0isto">
  <kap>
    <tld/>isto
  </kap>
  <snc>
    <dif>
      Iu, kies precipa okupo estas <tld/>i:
      <ekz>
        <ind>komerca <tld/>isto</ind>
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Deka</lok>
        </fnt>;
        <trd lng="de">Handelsreisender</trd>
      </ekz>
      <ekz>
        E&ubreve;ropo <klr>[&#8230;]</klr> en ok tagoj, <klr>[&#8230;]</klr> oni povas tion fari, kiel la
        granda <tld/>isto
        <fnt>
          <bib>Fab3</bib>
          <lok>Post jarmiloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la konata <tld/>isto-esploranto <nom>Jaques Thevenot</nom>, konati&gcirc;inte kun la mirinda
        facileco de la tatarlingva strukturo, &mdash; proponis <klr>[&#8230;]</klr> modifi ilian
        gramatikon la&ubreve; la modelo de la lingvo tatara
        <fnt>
          <bib>Zam</bib>
          <lok>Aglutina karaktero de esperanto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="turist.0o">turisto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_a;&c_r;&c_o;&c_zh;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_v;&c_a;&c_n;&c_d;&c_r;&c_o;&c_w;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_v;&c_a;&c_n;&c_d;&c_r;&c_o;&c_v;&c_je;&c_c;</trd>,
    <trd>&c_v;&c_a;&c_ja;&c_zh;&c_o;&c_r;</trd>
  </trdgrp>
  <trd lng="de">Reisender <klr>(beruflich)</klr></trd>
  <trd lng="en">voyager</trd>
</drv>

<drv mrk="vojagx.esplor0i">
  <kap>esplor<tld/>i</kap>
  <snc mrk="vojagx.esplor0i.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="super" cel="vojagx.0i"><tld lit="V"/>i</ref> en
      anta&ubreve;e nekonata lando por ekkoni &gcirc;ian naturan kaj homan
      aspekton:
      <ekz>
        la grandaj esplor<tld/>oj konigos al E&ubreve;ropo la
        Amerikojn
        <fnt>
          <aut>D. Broadribb</aut>,
          <vrk>La Purgatorio vidita de dektriajarcentulo</vrk>,
          <bib>Monato</bib>,
          <lok>2002:5, p. 27a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_a;&c_r;&c_o;&c_zh;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol; <klr>(&c_z; &c_d;&c_a;&c_s;&c_mol;&c_l;&c_je;&c_d;&c_ch;&c_y;&c_m;&c_ib; &c_m;&c_e;&c_t;&c_a;&c_m;&c_ib;)</klr></trd>
  <trd lng="br">ergerzhet</trd>
  <trd lng="de">entdeckungsreisen</trd>
  <trd lng="fr">explorer<klr> (g&eacute;og.)</klr></trd>
  <trd lng="hu">felfedez&odblac;&uacute;t</trd>
  <trd lng="id"><baz>eksplorasi</baz>mengeksplorasi</trd>
  <trd lng="nl">een <ind>ontdekkingsreis</ind> maken</trd>
  <trdgrp lng="pl">
    <trd>eksplorowa&cacute;</trd>,
    <trd>poszukiwa&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_u;&c_t;&c_je;&c_sh;&c_je;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;<klr tip="amb"> (&c_g;&c_je;&c_o;&c_g;&c_r;.)</klr></trd>
</drv>

<drv mrk="vojagx.kun0i">
