<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Ob</rad>/o
</kap>

<drv mrk="ob.O0o">
  <kap><tld/>o</kap> <fnt>PIV</fnt>
  <snc mrk="ob.O0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Rivero en la Okcidenta <ref tip="malprt"
      cel="siberi.S0o">Siberio</ref>, naski&gcirc;anta el kunfluo de la <ref
      cel="altaj.A0o">Altajaj</ref> riveroj Katuno kaj Bijo, kaj fluanta
      en la <ref cel="mar.KaraaM0o">Karaan Maron</ref>.
    </dif>
  </snc>
  <trd lng="de">Ob</trd>
  <trd lng="en">Ob</trd>
  <trd lng="es">Obi</trd>
  <trdgrp lng="fr">
    <trd>Ob</trd>,
    <trd>Obi</trd>
  </trdgrp>
  <trd lng="hu">Ob</trd>
  <trd lng="nl">Ob</trd>
  <trdgrp lng="pt">
    <trd>Ob</trd>,
    <trd>Obi</trd>
  </trdgrp>
  <trd lng="ru">&c_r;. <ind>&c_O;&c_b;&c_mol;</ind></trd>
</drv>

<drv mrk="ob.0aGolfo">
  <kap><tld/>a Golfo</kap>
  <snc mrk="ob.0aGolfo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Golfo en la <ref tip="malprt" cel="mar.KaraaM0o">Karaa Maro</ref>,
      kiun formas la elfluejo de <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Obbusen</trd>
  <trd lng="fr">golfe de l'Ob</trd>
  <trd lng="hu">Ob-&ouml;b&ouml;l</trd>
  <trd lng="ru">&c_O;&c_b;&c_s;&c_k;&c_a;&c_ja; &c_g;&c_u;&c_b;&c_a;</trd>
</drv>
</art>
