<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Ob</rad>/o
    </kap>
    <drv mrk="ob.0o">
      <kap><tld/>o
        <fnt>PIV</fnt>
      </kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
	<dif>
          Rivero en la
          Okcidenta <ref tip="malprt" cel="siberi.0o">Siberio</ref>,
          naski&gcirc;anta el kunfluo de la <ref cel="altaj.0o">Altajaj</ref> 
          riveroj Katuno kaj Bijo, kaj fluanta en la
          <ref cel="kara.0aMaro">Karaan maron</ref>.
	</dif>
      </snc>
      <trd lng="en">Ob</trd>
      <trd lng="nl">Ob</trd>
      <trd lng="ru">&c_r;. <ind>&c_O;&c_b;&c_mol;</ind></trd>
    </drv>
    <drv mrk="ob.0aGolfo">
      <kap><tld/>a Golfo</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
	<dif>
          Golfo en la <ref tip="malprt" cel="kara.0aMaro">Karaa maro</ref>,
          kiun formas la elfluejo de <tld/>o.
	</dif>
      </snc>
      <trd lng="ru">&c_O;&c_b;&c_s;&c_k;&c_a;&c_ja; &c_g;&c_u;&c_b;&c_a;</trd>
    </drv>
  </art>

