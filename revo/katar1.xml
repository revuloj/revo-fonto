<vortaro>
<art mrk="$Id$">
<kap>
  <rad>katar</rad>/o
</kap>

<drv mrk="katar1.0o">
  <kap>
    <tld lit="K"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="katar1.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la <ref tip="malprt" cel="arab.A0aDuoninsulo">Araba 
      Duoninsulo</ref>.
      ISO-3166 kodo: QA.
      &Gcirc;i limas kun
      <ref tip="vid" cel="arab.SaudaA0ujo">Sauda Arabujo</ref>
      kaj <ref tip="vid" cel="pers.P0aGolfo.GEOG">Persa Golfo</ref>.
    </dif>
    <trdgrp lng="pl">
      <trd>&qaf;&Ta;&ra;</trd>,
      <trd>&dal;&waw;&lam;&ta_marbuta; &qaf;&Ta;&ra;</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd><klr tip="amb">Pa&nacute;stwo </klr>Katar</trd>
    </trdgrp>
  </snc>
</drv>

