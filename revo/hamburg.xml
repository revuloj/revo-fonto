<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hamburg.xml,v 1.23 2023/11/15 16:45:17 revo Exp $">
<kap>
  <rad>Hamburg</rad>/o <fnt><bib>DL</bib></fnt>
</kap>

<drv mrk="hamburg.H0o">
  <kap><tld/>o</kap>
  <snc mrk="hamburg.H0o.urbo">
    <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
    <dif>
      Havena <ref tip="lst" cel="urb.0o" lst="voko:urboj">urbo</ref> 
      en norda <ref tip="malprt" cel="german1.G0ujo">Germanujo</ref> (9&#x2DA;58'E,
      53&#x2DA;33'N), situanta &ccirc;e la rivero <ref tip="vid" cel="elb.E0o">Elbo</ref>, regurbo de la
      samnoma federacia lando:
      <ekz>
        la venontan diman&ccirc;on mi veturos <tld/>on
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Fritz Schwarze</nom> el <tld/>o, la&ubreve; metio hartondisto
        <klr>[&#x2026;]</klr> tendumis kun sia familio
        <fnt>
          <aut>B. Golden</aut>
          <vrk><url ref="http://www.autodidactproject.org/other/golden_kosma.html">
          Kosma Ruba&jcirc;o</url></vrk>
          <lok>Monda Karuselo, 1977:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis nur tri monatojn docento pri arto en Hamburgo, &ccirc;ar li
        desegnis
        &leftquot;senliman linion&rightquot; &ccirc;irka&ubreve; la arta altlernejo
        <fnt><aut>Walter Klag</aut>,
        <vrk><url ref="&Monato;/2000/006938.php">Friedensreich Hundertwasser 1928 -
        2000</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/07, p. 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni perceptas la sintenon de <nom>Lapenna</nom> kontra&ubreve; la tiama
        UEA: estis fendo, estigita
        de la estraraj kaj komitataj elektoj en <tld/>o en 1974
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2007/009192.php">Konata vo&ccirc;o ree sonas</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Hamburg</trd>
    <trd lng="es">Hamburgo</trd>
    <trd lng="lat">Hamburgum</trd>
  </snc>
<!-- momente ne valoras distingi tion kuntekste de vortaro (alie en
Leksikono....)
  kiam montri&gcirc;is en ekzemploj, ke Esperantistoj konsekvence distingas,
  oni remetu tiun sencon...
  <snc mrk="hamburg.H0o.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Unu el la federaciaj &scirc;tatoj de <ref tip="malprt"
      cel="german1.G0ujo">Germanujo</ref>, kies
      regurbo kaj plej granda urbo estas
      <tld/>o<sncref ref="hamburg.H0o.urbo"/>.
    </dif>
    <trd lng="de"><klr>(Stadtstaat) </klr>Hamburg</trd>
  </snc>
-->
  <trd lng="be">&c_G;&c_a;&c_m;&c_b;&c_u;&c_r;&c_g;</trd>
  <trd lng="de">Hamburg</trd>
  <trd lng="en">Hamburg</trd>
  <trd lng="fr">Hambourg</trd>
  <trd lng="hu">Hamburg</trd>
  <trd lng="it">Amburgo</trd>
  <trd lng="ja">&#x30CF;&#x30F3;&#x30D6;&#x30EB;&#x30AF;</trd>
  <trd lng="nl">Hamburg</trd>
  <trd lng="pl">Hamburg</trd>
  <trd lng="pt">Hamburgo</trd>
  <trd lng="ru">&c_G;&c_a;&c_m;&c_b;&c_u;&c_r;&c_g;</trd>
  <trd lng="uk">&c_G;&c_a;&#x43C;&c_b;&c_u;&c_r;&c_gu;</trd>
  <trd lng="zh">&#x6C49;&#x5821;&#x5E02; <pr>h&#xE0;nb&#x1CE;osh&#xEC;</pr></trd>
</drv>

<drv mrk="hamburg.h0ano">
  <kap><tld lit="h"/>ano</kap>
  <snc>
    <dif>
      Lo&gcirc;anto de <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_m;&c_b;&c_u;&c_r;&c_zh;&c_a;&c_c;</trd>
  <trd lng="de">Hamburger</trd>
  <trd lng="en">Hamburger</trd>
  <trdgrp lng="fr">
    <trd>hambourgeois<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="hu">hamburgi</trd>
  <trd lng="it">amburghese<klr> (sost.)</klr></trd>
  <trd lng="nl">Hamburger</trd>
  <trd lng="pl">mieszkaniec <ind>Hamburga</ind></trd>
  <trd lng="ru">&c_g;&c_a;&c_m;&c_b;&c_u;&c_r;&c_zh;&c_je;&c_c;</trd>
</drv>
</art>
