<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Hamburg</rad>/o
</kap>
<drv mrk="hamburg.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="hamburg.0o.urbo">
    <dif>
      Havena <ref tip="super" cel="urb.0o">urbo</ref> en norda <ref
      tip="malprt" cel="german1.G0ujo">Germanujo</ref> (9&ring;58'E,
      53&ring;33'N), situanta &ccirc;e la rivero <ref tip="vid"
      cel="elb.0o">Elbo</ref>, regurbo de la federacia
      lando Hamburgo.
    </dif>
    <trd lng="de">Hamburg</trd>
    <trd lng="es">Hamburgo</trd>
    <trd lng="lat">Hamburgum</trd>
  </snc>
  <snc mrk="hamburg.0o.sxtato">
    <dif>
      Unu el la federaciaj &scirc;tatoj de <ref tip="malprt"
      cel="german1.G0ujo">Germanujo</ref>.
      Regurbo kaj plej granda urbo estas Hamburgo.
    </dif>
    <trd lng="de"><klr>(Stadtstaat) </klr>Hamburg</trd>
  </snc>
  <trd lng="be">&c_G;&c_a;&c_m;&c_b;&c_u;&c_r;&c_g;</trd>
  <trdgrp lng="fr">
    <trd>Hambourg</trd>
  </trdgrp>
  <trd lng="hu">Hamburg</trd>
  <trd lng="nl">Hamburg</trd>
  <trd lng="pt">Hamburgo</trd>
  <trd lng="ru">&c_G;&c_a;&c_m;&c_b;&c_u;&c_r;&c_g;</trd>
</drv>

<drv mrk="hamburg.h0ano">
  <kap><tld lit="h"/>ano</kap>
  <snc>
    <dif>
      Civitano de Hamburgo.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_m;&c_b;&c_u;&c_r;&c_zh;&c_a;&c_c;</trd>
  <trd lng="de">Hamburger</trd>
  <trdgrp lng="fr">
    <trd>hambourgeois<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="hu">hamburgi</trd>
  <trd lng="nl">Hamburger</trd>
  <trd lng="ru">&c_g;&c_a;&c_m;&c_b;&c_u;&c_r;&c_zh;&c_je;&c_c;</trd>
</drv>
</art>
