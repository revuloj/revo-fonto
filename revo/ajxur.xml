<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ajxur.xml,v 1.21 2026/02/08 10:43:48 revo Exp $">
<kap>
  <rad>a&jcirc;ur</rad>/a
  <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="ajxur.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Kvalifikas ion, kie oni intence lasis multajn malplena&jcirc;ojn;
      <ref cel="punt.0o">punto</ref>simila:
      <ekz>
        Zonjo surmetas blankajn, <tld/>ajn &scirc;trumpetojn kaj la <esc>var&scirc;avajn</esc>, blankajn,
        atlasajn &scirc;uetojn
        <fnt>
          <bib>SrT</bib>
          <lok>Libro V.</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a silueto de la Ejfelturo;
      </ekz>
      <ekz>
        triangulaj <tld/>oj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="krad.0a">krada</ref>,
      <ref cel="bor.tra0i">traborita</ref>,
      <ref cel="tru.0a">trua</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_u;&c_n;&c_k;&c_a;&c_v;&c_y;</trd>
  <trd lng="ca">calat</trd>
  <trd lng="de">durchbrochen</trd>
  <trdgrp lng="en">
    <trd>pierced-work</trd>,
    <trd>open-work</trd>
  </trdgrp>
  <trd lng="es">calado</trd>
  <trd lng="fr">ajour&eacute;</trd>
  <trd lng="he">&shin;&qof;&vav;&fpe;</trd>
  <trdgrp lng="ja">
    <trd>&#x900F;&#x304B;&#x3057;&#x306E;&#x5165;&#x3063;&#x305F;  <pr>&#x3059;&#x304B;&#x3057;&#x306E;&#x306F;&#x3044;&#x3063;&#x305F;</pr></trd>,
    <trd>&#x3059;&#x304B;&#x3057;&#x7D30;&#x5DE5;&#x306E;  <pr>&#x3059;&#x304B;&#x3057;&#x3055;&#x3044;&#x304F;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">opengewerkt</trd>
  <trd lng="pl">a&zdot;urowy</trd>
  <trd lng="pt">rendado</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_zh;&c_u;&c_r;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_k;&c_r;&c_u;&c_zh;&c_je;&c_v;&c_n;&c_o;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">a&zcaron;&uacute;rov&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_a;&c_zh;&c_u;&c_r;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_z;&c_o;&c_r;&c_i;&c_j;</trd>,
    <trd>&c_m;&c_je;&c_r;&c_je;&c_zh;&c_i;&c_v;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="ajxur.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc>
    <dif>
      Ornamo <tld/>a:
      <ekz>
        <tld/>a&jcirc;o apartenas ne nur al gotiko, sed anka&ubreve; al aliaj stiloj (baroko, arabaj
        ar&hcirc;itekturoj ktp
        <fnt>
          <aut>Jan Werner</aut>
          <lok><url ref="http://fabien.galerio.org/kofo/cirkuleroj/kofo14.pdf"
              >Konstru-Forumo, n-ro 14, Jan 2009</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Bai Lili</nom> elektis hele purpuran mamzonon kun <tld/>a&jcirc;o
        <fnt>
          <bib>CRI</bib>
          <vrk><url ref="https://esperanto.cri.cn/581/2009/06/22/1s97536.htm"
              >&Ccirc;inaj virinoj komencis postkuri modon de subvesto</url></vrk>
          <lok>2009-06-22</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="filigr.0o.BELA">filigrano<sncref/></ref>,
      <ref cel="punt.0o">punto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Durchbrucharbeit</trd>,
    <trd>Ma&szlig;werk</trd>,
    <trd>Filigran</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>openwork</trd>,
    <trd>tracery</trd>,
    <trd>filigree</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ajour</trd>,
    <trd>tracerie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_zh;&c_u;&c_r; (&c_u;&c_z;&c_o;&c_r;)</trd>,
    <trd>&c_m;&c_a;&c_s;&c_v;&c_je;&c_r;&c_k;</trd>,
    <trd>&c_f;&c_i;&c_l;&c_i;&c_g;&c_r;&c_a;&c_n;&c_mol;</trd>
  </trdgrp>
</drv>

</art>
