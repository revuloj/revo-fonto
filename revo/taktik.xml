<kap>
  <ofc>1</ofc>
  <rad>taktik</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="taktik.0o">
  <kap><tld/>o</kap>
  <snc mrk="taktik.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Arto aran&gcirc;i kaj utiligi armeojn sur batalkampo:
      <ekz>
        la aliancoj, <tld/>oj kaj kolizioj inter la diversaj gentoj,
        iliaj estroj kaj armeoj, enpensigas paralela&jcirc;ojn
        niatere
        <fnt><aut>Familio Macgill</aut>,
        <vrk>June, nejune, pretertempe</vrk>,
        <bib>Monato</bib>,
        <lok>2001:7, p. 26a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="strate.0o">strategio</ref>.
    </refgrp>
    <trd lng="hu">harc&aacute;szat</trd>
  </snc>
  <snc mrk="taktik.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Lertaj rimedoj celantaj sukceson.
    </dif>
    <trd lng="hu">taktika</trd>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_k;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="bg">&c_t;&c_a;&c_k;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="ca">t&agrave;ctica</trd>
  <trd lng="cs">taktika</trd>
  <trd lng="de">Taktik</trd>
  <trd lng="en">tactics</trd>
  <trd lng="es">t&aacute;ctica</trd>
  <trdgrp lng="fa">
    <trd>&a_t;&a_A;&f_k;&a_t;&f_y;&f_k;</trd>,
    <trd>&a_r;&a_A;&a_h;&f_k;&a_A;&a_r;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>tactique</trd>
  </trdgrp>
  <trd lng="it">tattica</trd>
  <trdgrp lng="ja">
    <trd>&#x6226;&#x8853;  <pr>&#x305B;&#x3093;&#x3058;&#x3085;&#x3064;</pr></trd>,
    <trd>&#x7528;&#x5175;  <pr>&#x3088;&#x3046;&#x3078;&#x3044;</pr></trd>,
    <trd>&#x99C6;&#x3051;&#x5F15;&#x304D;  <pr>&#x304B;&#x3051;&#x3072;&#x304D;</pr></trd>,
    <trd>&#x7B56;&#x7565;  <pr>&#x3055;&#x304F;&#x308A;&#x3083;&#x304F;</pr></trd>,
    <trd>&#x8853;&#x7B56;  <pr>&#x3058;&#x3085;&#x3063;&#x3055;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">tactiek</trd>
  <trd lng="pl">taktyka</trd>
  <trdgrp lng="pt">
   <trd>t&aacute;tica</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_k;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">taktika</trd>
  <trd lng="sv">taktik</trd>
  <trd lng="uk">&#x442;&#x430;&#x43A;&#x442;&#x438;&#x43A;&#x430;</trd>
