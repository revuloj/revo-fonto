<vortaro>
<art mrk="$Id$">
<kap>
  <rad>vesti&gcirc;</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="vestigx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ARKE</uzo>
  <snc>
    <dif>
      Stumpa <ref tip="super" cel="rest.0ajxo">resta&jcirc;o</ref> de io
      ruini&gcirc;inta a&ubreve; ne plu funkcia:
      <ekz>
        la antikvaj <tld/>oj de la Romia Teatro
        <fnt><aut>Turisma skabenejo de la urbo Torino</aut>, <vrk>Torino,
        1984</vrk></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_sh;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_e;&c_sh;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_mol;&c_l;&c_je;&c_d;</trd>
  </trdgrp>
  <trd lng="fr">vestige</trd>
  <trd lng="pt">vest&iacute;gio</trd>
</drv>
</art>

