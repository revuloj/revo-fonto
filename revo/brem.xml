<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: brem.xml,v 1.4 2023/05/07 08:25:38 revo Exp $">
<kap>
    <rad>Brem</rad>/o <fnt><bib>Fab3</bib></fnt>
</kap>
<drv mrk="brem.0o">
  <kap><tld/>o</kap>
  <snc mrk="brem.0o.urbo">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Germana urbo, vd. <ref tip="sin" cel="bremen.0o">Bremeno</ref>:
      <ekz>
        komercistoj de <tld/>o kaj Lubeko
        <fnt><bib>Fab3</bib>, <lok>Nokta &ccirc;apo de
        fra&ubreve;lo</lok></fnt>;
      </ekz>
 <ekz>
        <tld/>a biero
        <fnt><bib>Fab3</bib>, <lok>Nokta &ccirc;apo de
        fra&ubreve;lo</lok></fnt>;
      </ekz>      
    </dif>
  </snc>
  <trd lng="be">&c_B;&c_r;&c_e;&c_m;&c_e;&c_n;</trd>
  <trd lng="de">Bremen</trd>
  <trd lng="en">Bremen</trd>
  <trd lng="fr">Br&ecirc;me</trd>
</drv>
</art>

