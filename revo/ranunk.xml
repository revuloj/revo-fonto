<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>ranunkol</rad>/o
</kap>
<drv mrk="ranunk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="ranunk.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Ranunculus</trd>), tre ofta herbo kun plej ofte brilflavaj
      kvinfoliaj floroj.
    </dif>
  </snc>
  <bld lok="../bld/ranunk.jpg">
    <ind>akra <tld/>o</ind> 
    <trd lng="la">Ranunculus acris</trd>
  </bld>
  <trd lng="de">Hahnenfu&szlig;</trd>
</drv>
<drv mrk="ranunk.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Ranunculaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Hahnenfu&szlig;gew&auml;chse</trd>
</drv>
</art>
