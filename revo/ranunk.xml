<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ranunk.xml,v 1.30 2023/09/03 07:53:09 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ranunkol</rad>/o
</kap>
<drv mrk="ranunk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio 
      <ref tip="malprt" cel="ranunk1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Ranunculus</trd>), tre ofta herbo kun plej ofte
      brilflavaj kvinfoliaj floroj:
      <ekz>
        kiel vegeta&jcirc;a tapi&scirc;o tie kreskis nur <tld/>oj
        <fnt>
          <aut>M&aacute;ire NicAoidh</aut>
          <vrk><url ref="&Monato;/2000/006878.php">&Gcirc;ardenumi</url></vrk>
          <bib>Monato</bib>
          <lok>2000/04, p. 20</lok>
          </fnt>;
      </ekz>
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> havas kvinnombrajn hipoginajn florojn kun 
        brilflava petalo kaj kun pluropaj fruktoj, kies unuopaj fruktoj estas 
        akenoj, ili kreskas ofte en malsekaj lokoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=30290">Ranunkolo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/b/b4/Ranunculus_acris_%28Mount_Tsurugi%29.JPG/320px-Ranunculus_acris_%28Mount_Tsurugi%29.JPG" prm="CC BY-SA 4.0"><ind>akra <tld/>o</ind> (Ranunculus acris subsp. nipponicus), Monto Tsurugi,
    Japanujo
         <fnt>
           <aut>Alpsdake, propra verko</aut>
           <lok><url ref="&WCW;/File:Ranunculus_acris_(Mount_Tsurugi).JPG">&WCW;/File:Ranunculus_acris_(Mount_Tsurugi).JPG</url></lok>
         </fnt>
      <trd lng="hu">r&eacute;ti <ind>bogl&aacute;rka</ind></trd>
      <trd lng="la">Ranunculus acris</trd>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_z;&c_ja;&c_l;&c_je;&c_c;</trd>,
    <trd>&c_k;&c_u;&c_r;&c_a;&c_s;&c_mol;&c_l;&c_je;&c_p;</trd>
  </trdgrp>
  <trd lng="ca">ranuncle</trd>
  <trd lng="de">Hahnenfu&szlig;</trd>
  <trd lng="es">ran&uacute;nculo</trd>
  <trd lng="fr">renoncule</trd>
  <trd lng="hu">bogl&aacute;rka</trd>
  <trdgrp lng="ja">
    <trd>&#x30AD;&#x30F3;&#x30DD;&#x30A6;&#x30B2;</trd>,
    <trd>&#x30A6;&#x30DE;&#x30CE;&#x30A2;&#x30B7;&#x30AC;&#x30BF;</trd>
  </trdgrp>
  <trd lng="nl">boterbloem</trd>
  <trd lng="pl">jaskier</trd>
  <trdgrp lng="pt">
    <trd>ran&uacute;nculo</trd>,
    <trd>rain&uacute;nculo</trd>,    
    <trd>bot&atilde;o-de-ouro</trd>    
  </trdgrp>
  <trd lng="ro">ranunculus</trd>
  <trd lng="ru">&c_l;&c_ju;&c_t;&c_i;&c_k;</trd>
  <trd lng="sk">iskiernik</trd>
  <trd lng="cs">prysky&rcaron;n&iacute;k</trd>
  <trd lng="uk">&c_zh;&#x43E;&c_v;&c_t;&c_je;&c_c;&#x44C;</trd>
</drv>
</art>
