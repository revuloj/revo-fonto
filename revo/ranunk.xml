<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>ranunkol</rad>/o
</kap>
<drv mrk="ranunk.0o">
<kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="ranunk.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Ranunculus</trd>), tre ofta herbo kun brilflavaj
      kvinpetalaj floroj.
    </dif>
  </snc>
  <trd lng="de">Hahnenfu&szlig;</trd>
</drv>
<drv mrk="ranunk.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj 
      (<trd lng="la">Ranunculaceae</trd>).
     </dif>
  </snc>
  <trd lng="de">Hahnenfu&szlig;gew&auml;chse</trd>  
</drv>
</art>
