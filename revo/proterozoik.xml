<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>proterozoik</rad>/o
  <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="proterozoik.0o">
  <kap><tld/>o</kap>
  <snc mrk="proterozoik.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <uzo tip="fak">PAL</uzo>
    <dif>
      La 2a <ref tip="super" cel="era.0o.GEOL">erao</ref>
      de <ref tip="super" cel="kambri.antaux0o.GEOL">anta&ubreve;kambrio
      </ref>.
      <ekz>
        <tld/>a erao da&ubreve;ris anta&ubreve; &ccirc;irka&ubreve; 
        inter 1500 kaj 600 milionoj da jaroj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kenozoik.0o.GEOL">kenozoiko</ref>,
      <ref cel="mezozoik.0o.GEOL">mezozoiko</ref>
      <ref cel="paleozoik.0o.GEOL">paleozoiko</ref>
      <ref cel="arkean.0o.GEOL">arkeano</ref>
    </refgrp>
    <trdgrp lng="pl">
      <trd>proterozoik</trd>,
      <trd>era <ind>proterozoiczna</ind></trd>,
      <trd>eozoik</trd>,
      <trd>era <ind>eozoiczna</ind></trd>,
      <trd>algonk</trd>,
      <trd>era <ind>algoncka</ind></trd>
    </trdgrp>
    <trd lng="pt">Proteroz&#243;ico</trd>
  </snc>
</drv>
</art>

