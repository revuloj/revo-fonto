<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>portret</rad>/o
</kap>
<drv mrk="portre.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Desegna&jcirc;o a&ubreve; pentra&jcirc;o prezentanta personon:
      <ekz>
        pa&scirc;tela <tld/>o;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li estas la <tld/>o de sia patro
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        fari en sia verko la <tld/>on
        <klr>(fizikan kaj moralan priskribon)</klr> de
        siaj plej famaj samtempuloj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_t;&c_r;&c_e;&c_t;</trd>
  <trd lng="de">Portrait</trd>
  <trd lng="en">portrait</trd>
  <trd lng="fr">portrait</trd>
  <trd lng="hu">portr&eacute;</trd>
  <trd lng="nl">portret</trd>
  <trd lng="ru">&c_p;&c_o;&c_r;&c_t;&c_r;&c_je;&c_t;</trd>
  <trd lng="sv">portr&auml;tt</trd>
</drv>
</art>
