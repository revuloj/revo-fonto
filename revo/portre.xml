<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>portret</rad>/o
</kap>
<drv mrk="portre.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Desegna&jcirc;o a&ubreve; pentra&jcirc;o prezentanta personon:
      <ekz>
        pa&scirc;tela <tld/>o
        <klr>(f)</klr>;
      </ekz>
      <ekz>
        li estas la <tld/>o de sia patro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        fari en sia verko la <tld/>on (fizikan kaj moralan priskribon) de
        siaj plej famaj samtempuloj.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Portrait</trd>
  <trd lng="en">portrait</trd>
  <trd lng="fr">portrait</trd>
  <trd lng="nl">portret</trd>
  <trd lng="ru">&c_p;&c_o;&c_r;&c_t;&c_r;&c_je;&c_t;</trd>
  <trd lng="sv">portr&auml;tt</trd>
</drv>
</art>
