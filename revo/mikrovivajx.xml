<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mikroviva&jcirc;</rad>/o
</kap>
<drv mrk="mikrovivajx.0o">
  <kap><tld/>o</kap>
  <snc mrk="mikrovivajx.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      &Ccirc;ia tre malgranda, okule ne videbla
      <ref tip="super" cel="viv.0ajxo.BIO">viva&jcirc;o</ref>:
      bakterio, viruso, gisto, iuj algoj...:
      <ekz>
        la insektoj, <tld/>oj ne dama&gcirc;as &gcirc;in
        <fnt>
          <bib>Viki</bib>,
          <lok><url
          ref="http://eo.wikipedia.org/wiki/Poliestero">poliestero</url>,
          
          2009-09-15</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="mikrorganism.0o">mikroorganismo</ref>
    </refgrp>
  </snc>
  <trd
  lng="bg">&c_m;&c_i;&c_k;&c_r;&c_o;&c_o;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_malmol;&c_m;</trd>
  <trd lng="ca">microorganisme</trd>
  <trd lng="de">Mikroorganismus</trd>
  <trd lng="fr">microorganisme</trd>
  <trd lng="id">mikroorganisme</trd>
  <trd lng="pl">mikroorganizm</trd>
</drv>
</art>

