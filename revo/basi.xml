<art mrk="$Id: basi.xml,v 1.18 2025/04/10 12:56:39 revo Exp $">
<kap>
<rad>basi</rad>/o
</kap>
<drv mrk="basi.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio de
      <ref tip="malprt" cel="sapotac.0oj">sapotacoj</ref>
      (<trd lng="la">Diploknema</trd>):
      <ekz>
        la sep specioj de <tld/>o kreskas en tropika kaj subtropika Azio.
      </ekz>
    </dif>
    <bld
    lok="&WCU;/thumb/8/85/Diploknema_butyracea_Flower.JPG/320px-Diploknema_butyracea_Flower.JPG"
    prm="CC BY-SA 3.0">florado de <tld/>o (Diploknema butyracea), Nepalo
        <fnt>
          <aut>Krish Dulal, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Diploknema_butyracea_Flower.JPG">&WCW;/File:Diploknema_butyracea_Flower.JPG</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Butterbaum</trd>
    <trd lng="en">butter <ind>tree</ind> (of Nepal)</trd>
    <trdgrp lng="hu">
      <trd>indiai vajfa</trd>,
      <trd>ken&odblac;csfa</trd>
    </trdgrp>
    <trd lng="pl">mas&lstroke;osz</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_y;&c_p;&c_l;&c_ja;&c_k;&c_n;&c_e;&c_m;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_s;&c_mol;&c_l;&c_ja;&c_n;&c_a;&c_je; <ind>&c_d;&c_r;&c_e;&c_v;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="cs">baissa</trd>
  <trdgrp lng="he">
    <trd>&#x5D1;&#x5E1;&#x5D9;&#x5D4;</trd>,
    <trd>&#x5E6;&#x5DE;&#x5D7; &#x5DE;&#x5DE;&#x5E9;&#x5E4;&#x5D7;&#x5EA;
    &#x5D4;&#x5E1;&#x5DC;&#x5E7;&#x5D9;&#x5D9;&#x5DD;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30DB;&#x30A6;&#x30AD;&#x30AE;</trd>,
    <trd>&#x30DB;&#x30A6;&#x30AD;&#x30B0;&#x30B5;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>baissa</trd>,
    <trd>pokles kurzov na burze</trd>
  </trdgrp>
  <trd lng="uk">&c_a;&c_s;&#x43B;&#x44F;&#x43D;&c_je;&#xA0;&c_d;&c_je;&c_r;&c_je;&c_v;&#x43E;</trd>
</drv>
</art>
