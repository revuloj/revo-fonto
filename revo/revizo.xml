<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: revizo.xml,v 1.16 2018/11/24 11:10:19 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>revizor</rad>/o
</kap>
<drv mrk="revizo.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">EKON</uzo>
    <dif>
      Supera <ref tip="super" cel="inspek1.0o">inspektoro</ref>:
      <ekz>
        mi invitis vin, sinjoroj, al mi, por komuniki al vi tre malagrablan nova&jcirc;on: al
        ni venas <tld/>o
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        jam de kelkaj monatoj <tld/>oj rekontrolas la librotenadon de L&amp;H, sed da&ubreve;re
        mankas konkludoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007334.html"
              >Belgio: Lernout kaj Hauspie: revo fini&gcirc;is</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_v;&c_ib;&c_z;&c_o;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;ehl&iacute;&zcaron;itel</trd>,
    <trd>revizor</trd>
  </trdgrp>
  <trd lng="de">Revisor</trd>
  <trdgrp lng="en">
    <trd>government inspector</trd>,
    <trd>inspector-general</trd>
  </trdgrp>
  <trd lng="es">supervisor</trd>
  <trd lng="fr">r&eacute;viseur</trd>
  <trdgrp lng="hu">
    <trd>revizor</trd>,
    <trd>fel&uuml;gyel&odblac;</trd>,
    <trd>ellen&odblac;r</trd>
    </trdgrp>
  <trd lng="nl">revisor</trd>
  <trd lng="pt">inspetor geral</trd>
  <trd lng="ru">&c_r;&c_je;&c_v;&c_i;&c_z;&c_o;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>kontrol&oacute;r</trd>,
    <trd>rev&iacute;zor</trd>
  </trdgrp>
</drv>
</art>
