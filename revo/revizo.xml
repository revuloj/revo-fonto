<kap>
  <ofc>3</ofc>
  <rad>revizor</rad>/o
</kap>
<drv mrk="revizo.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">EKON</uzo>
    <dif>
      Supera <ref tip="super" cel="inspek1.0o">inspektoro</ref>:
      <ekz>
        mi invitis vin, sinjoroj, al mi, por komuniki al vi tre malagrablan nova&jcirc;on: al
        ni venas <tld/>o
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        jam de kelkaj monatoj <tld/>oj rekontrolas la librotenadon de L&amp;H, sed da&ubreve;re
        mankas konkludoj
       <fnt>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2001/007334.php">Lernout kaj Hauspie: revo fini&gcirc;is</url></vrk>
          <bib>Monato</bib>
          <lok>2001/02, p. 8</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_v;&c_ib;&c_z;&c_o;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;ehl&iacute;&zcaron;itel</trd>,
    <trd>revizor</trd>
  </trdgrp>
  <trd lng="de">Revisor</trd>
  <trdgrp lng="en">
    <trd>government inspector</trd>,
    <trd>inspector-general</trd>
  </trdgrp>
  <trd lng="es">supervisor</trd>
  <trd lng="fr">r&eacute;viseur</trd>
  <trdgrp lng="hu">
    <trd>revizor</trd>,
    <trd>fel&uuml;gyel&odblac;</trd>,
    <trd>ellen&odblac;r</trd>
    </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x691C;&#x5BDF;&#x5B98;  <pr>&#x3051;&#x3093;&#x3055;&#x3064;&#x304B;&#x3093;</pr></trd>,
    <trd>&#x76E3;&#x67FB;&#x5F79;  <pr>&#x304B;&#x3093;&#x3055;&#x3084;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">revisor</trd>
  <trdgrp lng="pl">
    <trd>rewident</trd>,
    <trd>audytor</trd>
  </trdgrp>
  <trd lng="pt">inspetor geral</trd>
  <trdgrp lng="ro">
    <trd>revizor</trd>,
    <trd>auditor</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_v;&c_i;&c_z;&c_o;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>kontrol&oacute;r</trd>,
    <trd>rev&iacute;zor</trd>
  </trdgrp>
  <trd lng="uk">&c_r;&c_je;&c_v;&c_ib;&c_z;&#x43E;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#x5BA1;&#x8BA1;&#x5458; <pr>sh&#x11B;nj&#xEC;yu&#xE1;n</pr></trd>,
    <trd>&#x5BE9;&#x8A08;&#x54E1; <pr>sh&#x11B;nj&#xEC;yu&#xE1;n</pr></trd>,
    <trd>&#x590D;&#x67E5;&#x5458; <pr>f&#xF9;ch&#xE1;yu&#xE1;n</pr></trd>,
    <trd>&#x5FA9;&#x67E5;&#x54E1; <pr>f&#xF9;ch&#xE1;yu&#xE1;n</pr></trd>,
    <trd>&#x590D;&#x6838;&#x4EBA; <pr>f&#xF9;h&#xE9;r&#xE9;n</pr></trd>,
    <trd>&#x5FA9;&#x6838;&#x4EBA; <pr>f&#xF9;h&#xE9;r&#xE9;n</pr></trd>,
    <trd>&#x68C0;&#x9A8C;&#x5458; <pr>ji&#x1CE;ny&#xE0;nyu&#xE1;n</pr></trd>,
    <trd>&#x6AA2;&#x9A57;&#x54E1; <pr>ji&#x1CE;ny&#xE0;nyu&#xE1;n</pr></trd>
