<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hortikultur</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="hortikultur.0o">
  <kap><tld/>o</kap>
  <snc mrk="hortikultur.0o.HOR">
    <uzo tip="fak">HOR</uzo>
    <ref tip="dif" cel="kultur.gxarden0o.HOR">ĝardenkulturo</ref>
    <ekz>
      la sala-oj plejparte okupas sin pri terkulturado, brutobredado kaj
      <tld/>o
      <fnt>
      <aut>Ĉina Radio Internacia</aut>,
      <vrk><url
      ref="http://es.chinabroadcast.cn/1/2005/09/19/1@29351.htm">Ĉinaj
      naciminoritatoj... (4)</url></vrk>,
      <lok>2005-09-19</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="en">horticulture</trd>
  <trd lng="fr">horticulture</trd>
  <trd lng="he">גננות</trd>
  <trdgrp lng="pl">
    <trd>ogrodnictwo</trd>
    <trd>sadownictwo</trd>
  </trdgrp>
  <trd lng="pt">horticultura</trd>
</drv>

<drv mrk="hortikultur.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="hortikultur.0isto.HOR">
    <dif>
      Ĝardenkulturisto:
      <ekz>
        <tld/>istoj ĉiam preferas akiri semojn de granda grupo da plantoj
        de la sama variaĵo
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>ĉapitro 4a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">horticulteur</trd>
  <trdgrp lng="pl">
    <trd>ogrodnik</trd>
    <trd>sadownik</trd>
  </trdgrp>
</drv>
</art>

