<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hortikultur.xml,v 1.15 2025/03/30 16:20:54 revo Exp $">
<kap>
  <rad>hortikultur</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="hortikultur.0o">
  <kap><tld/>o</kap>
  <snc mrk="hortikultur.0o.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      <ref tip="dif" cel="kultur.gxarden0o.HOR">&Gcirc;ardenkulturo</ref>:
      <ekz>
        la sala-oj plejparte okupas sin pri terkulturado, brutobredado kaj
        <tld/>o
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url ref="http://es.chinabroadcast.cn/1/2005/09/19/1@29351.htm">
          &Ccirc;inaj naciminoritatoj... (4)</url></vrk>,
          <lok>2005-09-19</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estigis la tradicion helpi la Anglikanan Eklezion <klr>[&#8230;]</klr> kaj anka&ubreve;
        postulis la uzon de plej altaj normoj en la arkitekturo, ligno-skulpta&jcirc;oj,
        interna dekoracio kaj <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>David Curtis</aut>
          <vrk><url ref="&Monato;/2004/008282.html">Volontuli en Tyntesfield</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>&#285;ardenkultivado</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_d;&c_a;&c_v;&c_o;&c_d;&c_z;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_s;&c_a;&c_d;&c_o;&c_w;&c_n;&c_ib;&c_c;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hortikultura</trd>,
    <trd>zahradnictv&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hortikultur</trd>,
    <trd>Gartenbau</trd>
  </trdgrp>
  <trd lng="en">horticulture</trd>
  <trd lng="fr">horticulture</trd>
  <trd lng="he">&gimel;&nun;&nun;&vav;&tav;</trd>
  <trd lng="it">orticoltura</trd>
  <trd lng="ja">&#22290;&#33464;  <pr>&#12360;&#12435;&#12370;&#12356;</pr></trd>
  <trdgrp lng="pl">
    <trd>ogrodnictwo</trd>
    <trd>sadownictwo</trd>
  </trdgrp>
  <trd lng="pt">horticultura</trd>
  <trd lng="sk">z&aacute;hradn&iacute;ctvo</trd>
  <trd lng="uk">&c_s;&c_a;&c_d;&c_ib;&c_v;&#1085;&c_i;&c_c;&c_t;&c_v;&#1086; &c_ib; &#1082;&c_v;&c_ib;&c_t;&#1085;&c_i;&#1082;&c_a;&c_r;&c_s;&c_t;&c_v;&#1086;</trd>
  <trdgrp lng="zh">
    <trd>&#22253;&#33402; <pr>yu&#225;ny&#236;</pr></trd>,
    <trd>&#22253;&#33402;&#23398; <pr>yu&#225;ny&#236;xu&#233;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hortikultur.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="hortikultur.0isto.HOR">
    <dif>
      &Gcirc;ardenkulturisto:
      <ekz>
        <tld/>istoj &ccirc;iam preferas akiri semojn de granda grupo da
        plantoj de la sama varia&jcirc;o
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&Ccirc;apitro 4a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kamp.0arano">kamparano</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_d;&c_o;&c_w;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_s;&c_a;&c_d;&c_a;&c_v;&c_o;&c_d;</trd>
  </trdgrp>
  <trd lng="cs">zahradn&iacute;k</trd>
  <trdgrp lng="de">
    <trd>G&auml;rtner</trd>,
    <trd>Florist</trd>
  </trdgrp>
  <trd lng="fr">horticulteur</trd>
  <trd lng="it">orticoltore</trd>
  <trdgrp lng="pl">
    <trd>ogrodnik</trd>
    <trd>sadownik</trd>
  </trdgrp>
  <trd lng="sk">z&aacute;hradn&iacute;k</trd>
  <trd lng="uk">&c_s;&c_a;&c_d;&c_ib;&c_v;&#1085;&c_i;&#1082;</trd>
  <trdgrp lng="zh">
    <trd>&#22253;&#19969; <pr>yu&#225;nd&#299;ng</pr></trd>,
    <trd>&#22253;&#33402;&#23478; <pr>yu&#225;ny&#236;ji&amacron;</pr></trd>,
    <trd>&#33457;&#21280; <pr>hu&amacron;ji&#224;ng</pr></trd>,
    <trd>&#33457;&#21830; <pr>hu&amacron;sh&amacron;ng</pr></trd>,
    <trd>&#33457;&#24037; <pr>hu&amacron;g&#333;ng</pr></trd>,
    <trd>&#21334;&#33457;&#20154; <pr>m&#224;ihu&amacron;r&#233;n</pr></trd>
  </trdgrp>
</drv>
</art>

