<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hortikultur.xml,v 1.16 2025/12/07 06:41:09 revo Exp $">
<kap>
  <rad>hortikultur</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="hortikultur.0o">
  <kap><tld/>o</kap>
  <snc mrk="hortikultur.0o.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      <ref tip="dif" cel="kultur.gxarden0o.HOR">&Gcirc;ardenkulturo</ref>:
      <ekz>
        la sala-oj plejparte okupas sin pri terkulturado, brutobredado kaj
        <tld/>o
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url ref="http://es.chinabroadcast.cn/1/2005/09/19/1@29351.htm">
          &Ccirc;inaj naciminoritatoj... (4)</url></vrk>,
          <lok>2005-09-19</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estigis la tradicion helpi la Anglikanan Eklezion <klr>[&#x2026;]</klr> kaj anka&ubreve;
        postulis la uzon de plej altaj normoj en la arkitekturo, ligno-skulpta&jcirc;oj,
        interna dekoracio kaj <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>David Curtis</aut>
          <vrk><url ref="&Monato;/2004/008282.html">Volontuli en Tyntesfield</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="en">horticulture</trd>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>&gcirc;ardenkultivado</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_d;&c_a;&c_v;&c_o;&c_d;&c_z;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_s;&c_a;&c_d;&c_o;&c_w;&c_n;&c_ib;&c_c;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hortikultura</trd>,
    <trd>zahradnictv&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hortikultur</trd>,
    <trd>Gartenbau</trd>
  </trdgrp>
  <trd lng="fr">horticulture</trd>
  <trd lng="he">&gimel;&nun;&nun;&vav;&tav;</trd>
  <trd lng="it">orticoltura</trd>
  <trd lng="ja">&#x5712;&#x82B8;  <pr>&#x3048;&#x3093;&#x3052;&#x3044;</pr></trd>
  <trdgrp lng="pl">
    <trd>ogrodnictwo</trd>
    <trd>sadownictwo</trd>
  </trdgrp>
  <trd lng="pt">horticultura</trd>
  <trd lng="sk">z&aacute;hradn&iacute;ctvo</trd>
  <trd lng="uk">&c_s;&c_a;&c_d;&c_ib;&c_v;&c_n;&c_i;&c_c;&c_t;&c_v;&c_o; &c_ib; &c_k;&c_v;&c_ib;&c_t;&c_n;&c_i;&c_k;&c_a;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>
  <trdgrp lng="zh">
    <trd>&#x56ED;&#x827A; <pr>yu&aacute;ny&igrave;</pr></trd>,
    <trd>&#x56ED;&#x827A;&#x5B66; <pr>yu&aacute;ny&igrave;xu&eacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hortikultur.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="hortikultur.0isto.HOR">
    <dif>
      &Gcirc;ardenkulturisto:
      <ekz>
        <tld/>istoj &ccirc;iam preferas akiri semojn de granda grupo da
        plantoj de la sama varia&jcirc;o
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&Ccirc;apitro 4a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kamp.0arano">kamparano</ref>
  <trd lng="en">horticulturist</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_d;&c_o;&c_w;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_s;&c_a;&c_d;&c_a;&c_v;&c_o;&c_d;</trd>
  </trdgrp>
  <trd lng="cs">zahradn&iacute;k</trd>
  <trdgrp lng="de">
    <trd>G&auml;rtner</trd>,
    <trd>Florist</trd>
  </trdgrp>
  <trd lng="fr">horticulteur</trd>
  <trd lng="it">orticoltore</trd>
  <trdgrp lng="pl">
    <trd>ogrodnik</trd>
    <trd>sadownik</trd>
  </trdgrp>
  <trd lng="sk">z&aacute;hradn&iacute;k</trd>
  <trd lng="uk">&c_s;&c_a;&c_d;&c_ib;&c_v;&c_n;&c_i;&c_k;</trd>
  <trdgrp lng="zh">
    <trd>&#x56ED;&#x4E01; <pr>yu&aacute;nd&imacron;ng</pr></trd>,
    <trd>&#x56ED;&#x827A;&#x5BB6; <pr>yu&aacute;ny&igrave;ji&amacron;</pr></trd>,
    <trd>&#x82B1;&#x5320; <pr>hu&amacron;ji&agrave;ng</pr></trd>,
    <trd>&#x82B1;&#x5546; <pr>hu&amacron;sh&amacron;ng</pr></trd>,
    <trd>&#x82B1;&#x5DE5; <pr>hu&amacron;g&omacron;ng</pr></trd>,
    <trd>&#x5356;&#x82B1;&#x4EBA; <pr>m&agrave;ihu&amacron;r&eacute;n</pr></trd>
  </trdgrp>
</drv>
</art>

