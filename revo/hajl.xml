<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hajl.xml,v 1.32 2022/08/18 07:30:58 revo Exp $">
<kap><ofc>*</ofc> <rad>hajl</rad>/o</kap>

<drv mrk="hajl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      Glacii&gcirc;inta, grajnforme dense falanta <ref tip="super" cel="pluv.0o">pluvo</ref>:
      <ekz>
        la <tld/>o batdifektis <klr>[&#8230;]</klr> &ccirc;ion, kio estis sur la kampo
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 9:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o pasis <klr>[&#8230;]</klr>, sed tuj poste komencis mu&gcirc;i pluvego
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        larmoj <tld/>esimile &scirc;prucis el la okuloj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;ia roza viza&gcirc;o brilis de sciemo kaj <klr>[&#8230;]</klr>, &scirc;iaj demandoj falis kiel
        <tld/>o kaj e&ccirc; sen doni tempon por respondo
        <fnt>
          <bib>HsT</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        siblantaj sagoj <klr>[&#8230;]</klr> elver&scirc;i&gcirc;is super &scirc;i kiel &ccirc;ion &scirc;utkovranta
        <tld/>ego
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="grajl.0o">grajlo</ref>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_d;</trd>
  <trd lng="bg">&c_g;&c_r;&c_a;&c_d;</trd>
  <trd lng="bo">&t_sa;&t_drengbu;&t_tsheg;&t_ra;&t_tsheg;</trd>
  <trdgrp lng="br">
    <trd>grizilh</trd>,
    <trd>kazarc'h</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>calamarsada</trd>,
    <trd>pedregada</trd>
  </trdgrp>
  <trd lng="cs">krupobit&iacute;</trd>
  <trd lng="de">Hagel</trd>
  <trd lng="en">hail</trd>
  <trd lng="es">granizo</trd>
  <trd lng="fr">gr&ecirc;le<klr tip="ind"> (m&eacute;t&eacute;o.)</klr>
  </trd>
  <trd lng="hu">j&eacute;ges&odblac;</trd>
  <trdgrp lng="it">
   <trd>grandine</trd>, 
   <trd>grandinata</trd> 
  </trdgrp>
  <trd lng="nl">hagel</trd>
  <trd lng="pl">grad</trd>
  <trdgrp lng="pt">
   <trd>chuva de granizo</trd>, 
   <trd>saraivada</trd> 
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_d;</trd>
  <trd lng="sk">krupobitie</trd>
  <trd lng="sv">hagel</trd>
  <trd lng="uk">&c_g;&c_r;&c_a;&c_d;</trd>
</drv>

<drv mrk="hajl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <uzo tip="fak">MET</uzo>
    <dif>
      Fali <klr>(p.p. <tld/>o)</klr>:
      <ekz>
        &ccirc;u pluvos, &ccirc;u <tld/>os, &ccirc;u ne&gcirc;os, &ccirc;u uraganos, mi nepre estos tie
        <fnt>
          <bib>KKl</bib>
        </fnt>;
      </ekz>
      <ekz>
        kia fulmotondro! mi supozas, ke <tld/>os
        <fnt>
          <bib>QuV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_d;&c_a;&c_c;&c_mol; <klr tip="amb">
    (&c_p;&c_r;&c_a; &c_g;&c_r;&c_a;&c_d;)</klr></trd>
    <trdgrp lng="br">
      <trd>grizilha&ntilde;</trd>,
      <trd>kazarc'hi&ntilde;</trd>,
      <trd>ober <ind>kazarc'h</ind></trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>pedregar</trd>,
      <trd>granissar</trd>,
      <trd>calamarsejar</trd>
    </trdgrp>
    <trd lng="en">hail</trd>
    <trd lng="es">granizar</trd>
    <trd lng="fr">gr&ecirc;ler<klr tip="ind"> (impers.)</klr></trd>
    <trd lng="hu">j&eacute;ges&odblac; esik</trd>
    <trdgrp lng="pl">
      <trd>pada&cacute;<klr> (o gradzie)</klr></trd>,
      <trd>sypa&cacute;<klr> (o gradzie)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_a;&c_d;&c_a;&c_t;&c_mol;<klr tip="ind">
      (&c_b;&c_je;&c_z;&c_l;., &c_o; &c_g;&c_r;&c_a;&c_d;&c_je;)</klr>
      </trd>,
      <trd>&c_v;&c_y;&c_p;&c_a;&c_d;&c_a;&c_t;&c_mol;<klr tip="ind">
      (&c_b;&c_je;&c_z;&c_l;., &c_o; &c_g;&c_r;&c_a;&c_d;&c_je;)</klr>
      </trd>,
      <trd>&c_v;&c_y;&c_p;&c_a;&c_s;&c_t;&c_mol;<klr tip="amb">
      (&c_b;&c_je;&c_z;&c_l;., &c_o; &c_g;&c_r;&c_a;&c_d;&c_je;)</klr>
      </trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fali dense kiel <tld/>o:
      <ekz>
        malanta&ubreve; lia dorso sur la podion <tld/>as bronzaj moneroj kaj daktiloj
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        sur la plankon <tld/>is kristalaj rompa&jcirc;oj de la lustro
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>antaj malla&ubreve;doj en stranga, e&ubreve;nukeca naztono
        <fnt>
          <bib>KKr</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fal.sur0i">surfali</ref>
    </refgrp>
    <trd lng="be">
    &c_p;&c_a;&c_d;&c_a;&c_c;&c_mol; &c_ja;&c_k; &c_g;&c_r;&c_a;&c_d;
    </trd>
    <trd lng="br">kouezha&ntilde; ken stank hag ar grizilh</trd>
    <trdgrp lng="ca">
      <trd>ploure</trd>,
      <trd>caure</trd>
    </trdgrp>
    <trd lng="fr">tomber<klr tip="ind"> (dru, comme la gr&ecirc;le)
    </klr></trd>
    <trdgrp lng="pl">
      <trd>pada&cacute;<klr> niczym grad</klr></trd>,
      <trd>pada&cacute;<klr> jak grad</klr></trd>,
      <trd>pada&cacute;<klr> gradem</klr></trd>,
      <trd>sypa&cacute;<klr> niczym grad</klr></trd>,
      <trd>sypa&cacute;<klr> jak grad</klr></trd>,
      <trd>sypa&cacute;<klr> gradem</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="de">hageln</trd>
  <trd lng="hu">z&aacute;porozik<klr tip="ind"> (&aacute;tv.)</klr>
  </trd>
  <trd lng="it">grandinare</trd>
  <trd lng="nl">hagelen</trd>
  <trdgrp lng="pt">
   <trd>chover granizo</trd>, 
   <trd>saraivar</trd> 
  </trdgrp>
  <trd lng="ru">
  &c_p;&c_a;&c_d;&c_a;&c_t;&c_mol; &c_k;&c_a;&c_k; &c_g;&c_r;&c_a;&c_d;
  </trd>
  <trd lng="sv">hagla</trd>
</drv>

<drv mrk="hajl.0ero">
  <kap><tld/>ero</kap>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      <tld lit="H"/>ograjno:
      <ekz>
        hiera&ubreve; falis granda <tld/>o, &ccirc;iu <tld/>ero pezis pli ol kvindek gramojn
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        blindigite <klr>[&#8230;]</klr> de <tld/>eroj, kiuj skur&gcirc;is lian viza&gcirc;on, la grafo
        havis nur tempon malfermi la pordon
        <fnt>
          <bib>KPr</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_a;</trd>
  <trdgrp lng="br">
    <trd>grizilhenn</trd>,
    <trd>kazarc'henn</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>pedra</trd>,
    <trd>calamarsa</trd>
  </trdgrp>
  <trd lng="cs">kroupa <klr>(ledov&aacute;)</klr></trd>
  <trd lng="de">Hagelkorn</trd>
  <trd lng="en">hailstone</trd>
  <trd lng="es">granizo</trd>
  <trd lng="fr">gr&ecirc;lon</trd>
  <trdgrp lng="hu">
    <trd>j&eacute;gdarab</trd>,
    <trd>j&eacute;gszem</trd>
  </trdgrp>
  <trdgrp lng="it">
   <trd>chicco di <ind>grandine</ind></trd>, 
   <trd>grandine<klr> (chicco di)</klr></trd> 
  </trdgrp>
  <trd lng="nl">hagelkorrel</trd>
  <trdgrp lng="pl">
    <trd>gradzina</trd>,
    <trd>kulka <ind>gradu</ind></trd>,
    <trd>kulka <ind>gradowa</ind></trd>
  </trdgrp>
  <trd lng="pt">granizo<klr tip="amb"> (pedra)</klr></trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_d;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">kr&uacute;p <klr>(pri krupobit&iacute;)</klr></trd>
  <trd lng="uk">&c_g;&c_r;&c_a;&c_d;&c_i;&#x43D;&c_a;</trd>
</drv>

<drv mrk="hajl.0eto0negxo">
  <kap><tld/>eto, <var><kap><tld/>ne&gcirc;o</kap></var></kap>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      Maldikgrajna <tld/>o, malmola kaj maldiafana.
    </dif>
    <refgrp tip="vid">
      <ref cel="grajl.0o">grajlo</ref>,
      <ref cel="prujn.0o">prujno</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_r;&c_o;&c_b;&c_y; &c_g;&c_r;&c_a;&c_d;</trd>,
    <trd>&c_k;&c_r;&c_u;&c_p;&c_k;&c_a; <klr tip="amb">(&c_a;&c_p;&c_a;&c_d;&c_k;&c_ib;)</klr></trd>
  </trdgrp>
  <trd lng="br">milerc'h</trd>
  <trd lng="ca">calamars&oacute;</trd>  
  <trd lng="de">Eiskorn</trd>
  <trd lng="fr">gr&eacute;sil<klr tip="ind"> (m&eacute;t&eacute;o.)</klr></trd>
  <trd lng="nl">hagelsneeuw</trd>
  <trd lng="pt">saraivisco</trd>
  <trd lng="hu">j&eacute;gdara</trd>
  <trdgrp lng="it">
   <trd>grandine<klr> (lieve)</klr></trd>, 
   <trd>grandinata<klr> (lieve)</klr></trd>, 
   <trd><ind>pioggia</ind> ghiacciata</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_l;&c_k;&c_i;&c_j; &c_g;&c_r;&c_a;&c_d;</trd>,
    <trd>&c_k;&c_r;&c_u;&c_p;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_r;&c_ib;&c_b;&#x43D;&c_i;&c_j; &c_g;&c_r;&c_a;&c_d;</trd>,
    <trd>&#x43A;&c_r;&c_u;&#x43F;&c_a;</trd>
  </trdgrp>
</drv>
</art>
