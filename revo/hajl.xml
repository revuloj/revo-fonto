<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc> <rad>hajl</rad>/o</kap>
<drv mrk="hajl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      Glacii&gcirc;inta, grajnforme dense falanta pluvo:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>o da kugloj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Hagel</trd>
  <trd lng="es">granizo</trd>
  <trd lng="fr">gr&ecirc;le<klr tip="ind"> (m&eacute;t&eacute;o.)</klr></trd>
  <trd lng="hu">j&eacute;ges&odblac;</trd>
  <trd lng="nl">hagel</trd>
  <trd lng="pt">chuva de granizo</trd>
  <trd lng="sv">hagel</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_d;</trd>
</drv>

<drv mrk="hajl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <uzo tip="fak">MET</uzo>
    <dif>
      Fali <klr>(p.p. <tld/>o)</klr>.
    </dif>
    <trd lng="es">granizar</trd>
    <trd lng="fr">gr&ecirc;ler<klr tip="ind"> (impers.)</klr></trd>
    <trd lng="hu">j&eacute;ges&odblac; esik</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_a;&c_d;&c_a;&c_t;&c_mol;<klr tip="ind"> (&c_b;&c_je;&c_z;&c_l;., &c_o; &c_g;&c_r;&c_a;&c_d;&c_je;)</klr></trd>,
      <trd>&c_v;&c_y;&c_p;&c_a;&c_d;&c_a;&c_t;&c_mol;<klr tip="ind"> (&c_b;&c_je;&c_z;&c_l;., &c_o; &c_g;&c_r;&c_a;&c_d;&c_je;)</klr></trd>,
      <trd>&c_v;&c_y;&c_p;&c_a;&c_s;&c_t;&c_mol;<klr tip="amb"> (&c_b;&c_je;&c_z;&c_l;., &c_o; &c_g;&c_r;&c_a;&c_d;&c_je;)</klr></trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fali dense kiel <tld/>o.
    </dif>
    <trd lng="fr">tomber<klr tip="ind"> (dru, comme la gr&ecirc;le)</klr></trd>
  </snc>
  <trd lng="de">hageln</trd>
  <trd lng="hu">z&aacute;porozik<klr tip="ind"> (&aacute;tv.)</klr></trd>
  <trd lng="nl">hagelen</trd>
  <trd lng="ru">&c_p;&c_a;&c_d;&c_a;&c_t;&c_mol; &c_k;&c_a;&c_k; &c_g;&c_r;&c_a;&c_d;</trd>
  <trd lng="sv">hagla</trd>
</drv>

<drv mrk="hajl.0ero">
  <kap><tld/>ero<fnt>Z</fnt></kap>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      <tld lit="H"/>ograjno.
    </dif>
  </snc>
  <trd lng="de">Hagelkorn</trd>
  <trd lng="es">granizo</trd>
  <trd lng="fr">gr&ecirc;lon</trd>

    <trdgrp lng="hu">
      <trd>j&eacute;gdarab</trd>,
      <trd>j&eacute;gszem</trd>
    </trdgrp>
  <trd lng="nl">hagelkorrel</trd>
  <trd lng="pt">granizo</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_d;&c_i;&c_n;&c_a;</trd>
</drv>

<drv mrk="hajl.0eto0negxo">
  <kap><tld/>eto, <tld/>ne&gcirc;o</kap>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      Maldikgrajna <tld/>o, malmola kaj maldiafana.
    </dif>
    <refgrp tip="vid">
      <ref cel="prujn.0o">prujno</ref>.
    </refgrp>
  </snc>
  <trd lng="fr">gr&eacute;sil<klr tip="ind"> (m&eacute;t&eacute;o.)</klr></trd>
  <trd lng="nl">hagelsneeuw</trd>
  <trd lng="hu">j&eacute;gdara</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_l;&c_k;&c_i;&c_j; &c_g;&c_r;&c_a;&c_d;</trd>,
    <trd>&c_k;&c_r;&c_u;&c_p;&c_a;</trd>
  </trdgrp>
</drv>
</art>
