<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>hajl</rad>/o
</kap>
<drv mrk="hajl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Glacii&gcirc;inta, grajnforme dense falanta pluvo: (f)
      <ekz><tld/>o da kugloj<fnt>Z</fnt>,</ekz> 
    </dif>
  </snc>
  <trd lng="de">Hagel</trd>
  <trd lng="sv">hagel</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_d;</trd>
</drv>
<drv mrk="hajl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc num="1">
    <dif>
      Fali (p.p. <tld/>o). 
    </dif>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fali dense kiel <tld/>o. 
    </dif>
  </snc>
  <trd lng="sv">hagla</trd>
</drv>
<drv mrk="hajl.0ero">
  <kap><tld/>ero<fnt>Z</fnt></kap>
  <snc>
    <dif>
      <tld lit="H"/>ograjno. 
    </dif>
  </snc>
  <trd lng="de">Hagelkorn</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_d;&c_i;&c_n;&c_a;</trd>
</drv>
<drv mrk="hajl.0eto0negxo">
  <kap><tld/>eto, <tld/>ne&gcirc;o</kap>
  <snc>
    <dif>
      Maldikgrajna <tld/>o, malmola kaj maldiafana. 
    </dif>
    <refgrp tip="sin">
      <ref cel="prujn.0o">prujno</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_l;&c_k;&c_i;&c_j; &c_g;&c_r;&c_a;&c_d;</trd>,
    <trd>&c_k;&c_r;&c_u;&c_p;&c_a;</trd>
  </trdgrp>
</drv>
</art>
