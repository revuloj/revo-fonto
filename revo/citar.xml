<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>citar</rad>/o
</kap>
<drv mrk="citar.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Antikva greka <ref tip="super"
      cel="instru2.kord0o">kordinstrumento</ref>,
      plektre sonigata liro, kies sonkesto flanke
      havas du kornoformajn partojn kunigitajn per stango,
      kun la&ubreve;epoke 4 &gcirc;is 15 kordoj
      stre&ccirc;itaj inter la kesto kaj la transversa stango.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_f;&c_a;&c_r;&c_a;</trd>
  <trdgrp lng="de">
     <trd>Kithara</trd>,
     <trd>altgriechische <ind>Zither</ind></trd>
  </trdgrp>
  <trd lng="en">zither</trd>
  <trd lng="es">c&iacute;tara</trd>
  <trd lng="fr">cithare<klr tip="ind"> (lyre antique)</klr></trd>
  <trd lng="hu">kithara</trd>
  <trdgrp lng="it">
  <trd>cetra<klr tip="amb"> (antica)</klr></trd>, 
  <trd>citara<klr tip="amb"> (antica)</klr></trd>
  </trdgrp>
  <trd lng="nl">kithara</trd>
  <trd lng="pl">kitara</trd>
  <trd lng="pt">c&iacute;tara</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_f;&c_a;&c_r;&c_a;</trd>,
    <trd>&c_k;&c_i;&c_t;&c_a;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">citara</trd>
</drv>
</art>

