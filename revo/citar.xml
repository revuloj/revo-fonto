<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>citar</rad>/o
</kap>
<drv mrk="citar.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Antikva greka <ref tip="super"
      cel="instru2.kord0o">kordinstrumento</ref>,
      plektre sonigata liro, kies sonkesto flanke
      havas du kornoformajn partojn kunigitajn per stango,
      kun la&ubreve;epoke 4 &gcirc;is 15 kordoj
      stre&ccirc;itaj inter la kesto kaj la transversa stango.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Kithara</trd>,
     <trd>altgriechische <ind>Zither</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cithare<klr> (lyre antique)</klr></trd>
  </trdgrp>
</drv>
</art>

