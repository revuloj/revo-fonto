<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: citar.xml,v 1.21 2023/10/31 13:37:03 revo Exp $">
<kap>
  <rad>citar</rad>/o,
  <var><kap><rad>kitar</rad>/o <fnt><bib>EBV</bib></fnt></kap></var>
</kap>
<drv mrk="citar.0o">
  <kap><tld/>o,
    <var><kap>kitaro</kap></var>
  </kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Antikva greka <ref tip="lst" cel="instru2.kord0o.MUZ" lst="voko:kordinstrumentoj">kordinstrumento</ref>,
      plektre sonigata liro, kies sonkesto flanke
      havas du kornoformajn partojn kunigitajn per stango,
      kun la&ubreve;epoke 4 &gcirc;is 15 kordoj
      stre&ccirc;itaj inter la kesto kaj la transversa stango:
      <ekz>
        la na&ubreve; muzoj: <klr>[&#x2026;]</klr> Polimnio &dash; religia poezio, mimiko, odo (kun vualo
        a&ubreve; <tld/>o)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=13241">Muzoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/f/fa/2005-12-28_Berlin_Pergamon_museum_Statue_of_Apollon_with_Kithara.jpg/360px-2005-12-28_Berlin_Pergamon_museum_Statue_of_Apollon_with_Kithara.jpg" prm="CC-BY-SA-3.0">Apollo Kitharoidos, marmora statuo, ver&scirc;ajne en 2a jc farita en Romo
        <fnt>
          <aut>Magnus Manske, propra verko</aut>
          <lok><url ref="&WCW;/File:2005-12-28_Berlin_Pergamon_museum_Statue_of_Apollon_with_Kithara.jpg">&WCW;/File:2005-12-28_Berlin_Pergamon_museum_Statue_of_Apollon_with_Kithara.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_f;&c_a;&c_r;&c_a;</trd>
  <trd lng="ca">c&iacute;tara</trd>
  <trd lng="cs">citera</trd>
  <trdgrp lng="de">
     <trd>Kithara</trd>,
     <trd>altgriechische <ind>Zither</ind></trd>
  </trdgrp>
  <trd lng="en">zither</trd>
  <trd lng="es">c&iacute;tara</trd>
  <trd lng="fr">cithare<klr tip="ind"> (lyre antique)</klr></trd>
  <trd lng="hu">kithara</trd>
  <trd lng="id">siter</trd>
  <trdgrp lng="it">
  <trd>cetra<klr tip="amb"> (antica)</klr></trd>, 
  <trd>citara<klr tip="amb"> (antica)</klr></trd>
  </trdgrp>
  <trd lng="nl">kithara</trd>
  <trd lng="pl">kitara</trd>
  <trd lng="pt">c&iacute;tara</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_f;&c_a;&c_r;&c_a;</trd>,
    <trd>&c_k;&c_i;&c_t;&c_a;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">citara</trd>
  <trd lng="zh">&#x88E1;&#x62C9;&#x7434; <pr>l&#x1D0;l&#x101;q&#xED;n</pr></trd>
</drv>
</art>

