<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: anakar1.xml,v 1.10 2022/03/11 20:05:17 revo Exp $">
<kap>
  <rad>anakardiac</rad>/oj
</kap>
<drv mrk="anakar1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Anacardiaceae</trd>), tropikaj
      lignoplantoj:
      <ekz>
        la <tld/>oj estas unu a&ubreve; duseksaj arboj, arbedoj de la tropika, subtropika
        klimato kun drupa frukto kaj inter&ccirc;elaj rezinduktoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19617">Anakardiacoj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        pistako estas la nukso de la <ref tip="prt" cel="pistak.0ujo">pistakujo</ref>,
        membro de la botanika familio de <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19630">Pistako</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
      <ref cel="anakar.0o">anakardio</ref>,
      <ref cel="mang.0arbo">mangujo</ref>
    </refgrp>
  </snc>
  <trd lng="bg">&c_s;&c_m;&c_r;&c_a;&c_d;&c_l;&c_i;&c_k;&c_o;&c_v;&c_i;</trd>
  <trd lng="de">Sumachgew&auml;chse</trd>
  <trdgrp lng="en">
    <trd>anacardiaceae</trd>,
    <trd>cashew family</trd>,
    <trd>sumac family</trd>
  </trdgrp>
  <trd lng="fr">Anacardiac&eacute;es</trd>
  <trdgrp lng="hu">
    <trd>sz&ouml;m&ouml;rcef&eacute;l&eacute;k</trd>,
    <trd>kesufaf&eacute;l&eacute;k</trd>
  </trdgrp>
  <trd lng="nl">pruikenboomfamilie</trd>
  <trd lng="pl">nanerczowate</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_m;&c_a;&c_h;&c_o;&c_v;&c_y;&c_je;</trd>, 
    <trd>&c_a;&c_n;&c_a;&c_k;&c_a;&c_r;&c_d;&c_i;&c_je;&c_v;&c_y;&c_je;</trd>,
    <trd>&c_f;&c_i;&c_s;&c_t;&c_a;&c_sh;&c_k;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x430;&#x43D;&#x430;&#x43A;&#x430;&#x440;&#x434;&#x456;&#x454;&#x432;&#x456;</trd>,
    <trd>&#x441;&#x443;&#x43C;&#x430;&#x445;&#x43E;&#x432;&#x456;</trd>
  </trdgrp>
</drv>
</art>

