<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>plump</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="plump.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">POE</uzo>
  <snc>
    <dif>
      Malelegante peza&ccirc;a, malharmonia:
      <ekz>
        &scirc;i, <tld/>a kaj grasa, havis lar&gcirc;an viza&gcirc;on kaj grandajn piedojn
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 73a, volumo 2a, p.
        537a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">plump</trd>
  <trdgrp lng="fr">
    <trd>lourd<klr> (in&eacute;l&eacute;gant)</klr></trd>,
    <trd>pataud</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>plump</trd>,
    <trd>esetlen</trd>
  </trdgrp>
</drv>
</art>
