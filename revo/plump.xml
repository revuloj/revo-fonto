<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>plump</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="plump.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">POE</uzo>
  <snc>
    <dif>
      Malelegante peza&ccirc;a, malharmonia:
      <ekz>
        &scirc;i, <tld/>a kaj grasa, havis lar&gcirc;an viza&gcirc;on kaj grandajn piedojn
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 73a, volumo 2a, p.
        537a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">plump</trd>
  <trdgrp lng="fr">
    <trd>lourd<klr> (in&eacute;l&eacute;gant)</klr></trd>,
    <trd>pataud</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>plump</trd>,
    <trd>esetlen</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>deselegante</trd>,
    <trd>tosco</trd>,
    <trd>r&uacute;stico</trd>    
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_u;&c_k;&c_l;&c_ju;&c_zh;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_l;&c_o;&c_v;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_o;&c_v;&c_o;&c_r;&c_o;&c_t;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
