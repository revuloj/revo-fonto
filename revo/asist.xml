<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>asist</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="asist.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Pro ofica devo &ccirc;eesti por helpi al iu (profesoro, pastro,
      kuracisto, ju&gcirc;isto).
    </dif>
    <refgrp tip="sin">
      <ref cel="asesor.0o">asesoro</ref>,
      <ref cel="adjunk.0o">adjunkto</ref>,
      <ref cel="diakon.0o">diakono</ref>.
    </refgrp>
  </snc>
  <trd lng="fr">assister</trd>
  <trd lng="nl">assisteren</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_s;&c_s;&c_i;&c_s;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_o;&c_g;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_o;&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
