<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>asist</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="asist.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Pro ofica devo &ccirc;eesti por <ref tip="super"
cel="help.0i">helpi</ref>
      al iu (profesoro, pastro, kuracisto, ju&gcirc;isto):
      <ekz>
        ili e&ccirc; kulpigis la organiza&jcirc;on, kiu konsilas kaj
<tld/>as
        azilpetantojn<fnt><aut>Guido Van Damme</aut>: <vrk>Sep en unu
        bato</vrk>, <bib>Monato</bib><lok>jaro 1999a, numero 3a, p.
        6a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_p;&c_a;&c_m;&c_a;&c_g;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_s;&c_y;&c_s;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">assistieren</trd>
  <trd lng="en">assist</trd>
  <trd lng="fr">assister<klr> (pr&eacute;ter assistance)</klr></trd>
  <trdgrp lng="hu">
    <trd>seg&eacute;dkezik</trd>,
    <trd>assziszt&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">assisteren</trd>
  <trdgrp lng="nl">
    <trd>bijstaan<klr tip="ind"> (assisteren)</klr></trd>,
    <trd>assisteren</trd>,
    <trd>seconderen</trd>,
    <trd>ruggensteunen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>asystowa&cacute;</trd>,
    <trd>towarzyszy&cacute;</trd>,
    <trd>pe&lstroke;ni&cacute; funkcj&eogonek; asystenta</trd>
  </trdgrp>
  <trdgrp lng="ru">
<trd>
&c_a;&c_s;&c_s;&c_i;&c_s;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;
</trd>,
<trd>&c_p;&c_o;&c_m;&c_o;&c_g;&c_a;&c_t;&c_mol;</trd>,
<trd>
&c_s;&c_o;&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;
</trd>
</trdgrp>
  <trd lng="es">asistir <klr>(ayudar, servir)</klr></trd>
</drv>

<drv mrk="asist.0anto">
  <kap><tld/>anto</kap>
  <snc>
    <dif>
      Profesia helpanto al &ccirc;efa respondeculo:
      <ekz>
        aresto de <tld/>antoj de la urbestro de
        Teherano<fnt><aut>Mori</aut>: <vrk>Torturo... &ccirc;u
vere</vrk>,
        <bib>Monato</bib><lok>jaro 1998a, numero 5a, p. 9a</lok></fnt>.
      </ekz>
      <ekz>
        por via rpopono [...] pri ia <tld/>anto a&ubreve; sekretario mi
dankas
        <fnt><bib>Z</bib></fnt>;
      </ekz>
      <ekz>
        li ricevis la oficon de <tld/>anto &ccirc;e la profesoro
        <fnt><bib>Z</bib></fnt>;
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="asesor.0o">asesoro</ref>,
      <ref cel="adjunk.0o">adjunkto</ref>,
      <ref cel="diakon.0o">diakono</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_m;&c_o;&c_ch;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_a;&c_s;&c_y;&c_s;&c_t;&c_e;&c_n;&c_t;</trd>
  </trdgrp>
  <trd lng="de">Assistent</trd>
  <trd lng="en">assistant</trd>
  <trdgrp lng="fr">
    <trd>assistant</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>asszisztens</trd>,
    <trd>seg&eacute;der&odblac;</trd>,
    <trd>seg&eacute;d</trd>
  </trdgrp>
  <trd lng="nl">assistent</trd>
  <trd lng="pl">asystent</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_m;&c_o;&c_shch;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_a;&c_s;&c_s;&c_i;&c_s;&c_t;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
  <trd lng="es">asistente <klr>(profesional)</klr></trd>
</drv>

<drv mrk="asist.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Stato kaj agado de tiu, kiu <tld/>as
      <ekz>
        publika <tld/>ado al laborkripluloj
        <fnt><bib>PIV2</bib></fnt>;
      </ekz>
    </dif>
    <trd lng="de">Assistenz</trd>
    <trdgrp lng="nl">
      <trd>bijstand</trd>,
      <trd>hulp<klr tip="ind"> (assistentie)</klr></trd>,
      <trd>assistentie</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>asysta</trd>,
      <trd>asystowanie</trd>
    </trdgrp>
    <trd lng="es">asistencia</trd>
  </snc>
</drv>
</art>
