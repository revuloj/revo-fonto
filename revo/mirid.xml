<art mrk="$Id: mirid.xml,v 1.15 2023/09/02 06:04:12 revo Exp $">
<kap>
  <rad>mirid</rad>/o
</kap>
<drv mrk="mirid.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">Myrrhis</trd>), iom simila al
      <ref cel="cerefo.0o">cerefolio</ref> sed odoranta je anizo:
      <ekz>
        folilegomo estas <klr>[&#x2026;]</klr> &ccirc;ina brasiko, kardono, portulako, <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=309731">Folia legomo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/1/1f/Illustration_Myrrhis_odorata0.jpg/345px-Illustration_Myrrhis_odorata0.jpg" prm="PD">botanika tabulo de <tld/>o (Myrrhis odorata)
        <fnt>
          <aut>Prof. Dr. Otto Wilhelm Thom&eacute;: &leftquot;Flora von Deutschland, &Ouml;sterreich und der Schweiz&rightquot;,
          1885, Gera, Germanujo</aut>
          <lok><url ref="&WCW;/File:Illustration_Myrrhis_odorata0.jpg">&WCW;/File:Illustration_Myrrhis_odorata0.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="ca">mirris</trd>
  <trdgrp lng="de">
    <trd>S&uuml;&szlig;dolde</trd>,
    <trd>Myrrhenkerbel</trd>
  </trdgrp>
  <trd lng="hu">mirhaf&udblac;</trd>
  <trdgrp lng="ja">
    <trd>&#x30DF;&#x30EB;&#x30EA;&#x30B9;</trd>,
    <trd>&#x30B9;&#x30A4;&#x30FC;&#x30C8;&#x30B7;&#x30B9;&#x30EA;&#x30FC;</trd>
  </trdgrp>
  <trd lng="nl">Roomse <ind>kervel</ind></trd>
  <trd lng="pl">woskownica</trd>     
  <trd lng="ro">mirtul de cear&abreve;</trd>
  <trd lng="ru">&c_m;&c_i;&c_r;&c_r;&c_i;&c_s;</trd>
  <trd lng="uk">&#x43C;&#x456;&#x440;&#x438;&#x441;</trd>
</drv>
