<art mrk="$Id: stratagem.xml,v 1.11 2024/03/25 07:28:36 revo Exp $">
<kap>
  <rad>stratagem</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="stratagem.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="artifi.0o">Artifiko</ref> planita por trompi malamikon:
      <ekz>
        diveni a&ubreve; scii la intencojn, planojn, <tld/>ojn kaj embuskojn de
        la malamiko
        <fnt>
        <bib>DonKihxoto</bib>,
        <lok>&ccirc;apitro 37a, p. 286a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ruz.0o0ajxo">ruzo</ref>,
      <ref cel="strate.0o">strategio</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_ib;&c_t;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_mol; <klr tip="amb">(&c_v;&c_a;&c_j;&c_s;&c_k;&c_o;&c_v;&c_a;&c_ja;)</klr></trd>,
    <trd>&c_v;&c_y;&c_k;&c_r;&c_u;&c_t;</trd>
  </trdgrp>
  <trd lng="cs">strategie</trd>
      <trdgrp lng="de">
      <trd>Kriegslist</trd>,
      <trd>Kunstgriff</trd>,
      <trd>Strategem</trd>
    </trdgrp>
  <trd lng="fr">stratag&egrave;me</trd>
  <trdgrp lng="hu">
    <trd>hadicsel</trd>,
    <trd>stratag&eacute;ma</trd>
  </trdgrp>
  <trd lng="it">stratagemma</trd>
  <trd lng="pl">podst&eogonek;p</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_je;&c_n;&c_n;&c_a;&c_ja; <ind>&c_h;&c_i;&c_t;&c_r;&c_o;&c_s;&c_t;&c_mol;</ind></trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_t;&c_a;&c_g;&c_je;&c_m;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">strat&eacute;gia</trd>
  <trdgrp lng="zh">
    <trd>&#x8ADD; <pr>x&#x16B;</pr></trd>,
    <trd>&#x8C1E; <pr>x&#x16B;</pr></trd>,
    <trd>&#x8A08;&#x7B56; <pr>j&#xEC;c&#xE8;</pr></trd>,
    <trd>&#x8BA1;&#x7B56; <pr>j&#xEC;c&#xE8;</pr></trd>,
    <trd>&#x8B00; <pr>m&#xF3;u</pr></trd>,
    <trd>&#x8C0B; <pr>m&#xF3;u</pr></trd>
  </trdgrp>
</drv>
</art>

