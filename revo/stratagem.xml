<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>stratagem</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="stratagem.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Artifiko planita por trompi malamikon:
      <ekz>
        diveni a&ubreve; scii la intencojn, planojn, <tld/>ojn kaj embuskojn de
        la malamiko<klr>...</klr>
        <fnt>
        <bib>DonKihxoto</bib>,
        <lok>&ccirc;apitro 37a, p. 286a</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_ib;&c_t;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_mol; <klr tip="amb">(&c_v;&c_a;&c_j;&c_s;&c_k;&c_o;&c_v;&c_a;&c_ja;)</klr></trd>,
    <trd>&c_v;&c_y;&c_k;&c_r;&c_u;&c_t;</trd>
  </trdgrp>
  <trd lng="fr">stratag&egrave;me</trd>
  <trdgrp lng="hu">
    <trd>hadicsel</trd>,
    <trd>stratag&eacute;ma</trd>
  </trdgrp>
</drv>
</art>

