<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>aktiv</rad>/a
</kap>
<drv mrk="aktiv.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Produktanta, laboranta, aganta:
      <ekz>
        li estas tiel <tld/>a, ke li apena&ubreve; dormas;
      </ekz>
      <ekz>
        <tld/>a membro;
      </ekz>
      <ekz>
        ne<tld/>a firmano
        <klr>(ano de komerca societo, kiu pruntedonas kapitalon, ne
        partoprenante en la administrado de la afero)</klr>;
      </ekz>
      <ekz>
        vere <tld/>a kuracilo;
      </ekz>
      <ekz>
        <tld/>a propagando;
      </ekz>
      <ekz>
        <tld/>a pruvo de kompato<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>aj eksplodoj de pasio<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="fak">GRA</uzo>
    <dif>
      Esprimanta agon:
      <ekz>
        &leftquot;amata&rightquot; estas pasiva, &leftquot;amanta&rightquot; <tld/>a vorto.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">active</trd>
  <trd lng="es">activo</trd>
  <trd lng="fr">actif</trd>
  <trd lng="nl">actief</trd>
  <trd lng="pt">ativo</trd>
  <trd lng="it">attivo</trd>
  <trd lng="ru">&c_a;&c_k;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">akt&iacute;vny</trd>
</drv>

<drv mrk="aktiv.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Posedataj a&ubreve; ricevotaj sumoj:
      <ekz>
        la <tld/>o de tiu
        banko malsuperas &gcirc;ian pasivon.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Forderungen</trd>,
      <trd>Aktiva</trd>,
      <trd>Haben</trd>,
      <trd>Einlagen</trd>
    </trdgrp>
    <trd lng="es">activo</trd>
    <trd lng="fr">actif</trd>
    <trdgrp lng="nl">
      <trd>actief</trd>,
      <trd>activa</trd>
    </trdgrp>
    <trd lng="pt">ativo</trd>
    <trd lng="ru">&c_a;&c_k;&c_t;&c_i;&c_v;</trd>
    <trd lng="sk">akt&iacute;vum</trd>
  </snc>
  <snc>
    <uzo tip="fak">GRA</uzo>
    <dif>
      Aktiva konjugacio.
    </dif>
    <trdgrp lng="de">
      <trd>Aktiv</trd>,
      <trd>T&auml;tigkeitsform</trd>
    </trdgrp>
    <trd lng="en">active voice</trd>
    <trd lng="es">voz <ind>activa</ind></trd>
    <trd lng="fr">voix <ind>active</ind></trd>
    <trd lng="nl">actief</trd>
    <trd lng="pt">voz <ind>ativa</ind></trd>
    <trd lng="ru">&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_z;&c_a;&c_l;&c_o;&c_g;</trd>
  </snc>
</drv>
</art>
