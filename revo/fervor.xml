    <trd>&c_z;&c_a;&c_p;&c_o;&c_p;&c_a;&c_d;&c_l;&c_i;&c_v;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_je;&c_v;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_je;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_z;&c_ja;&c_t;&c_t;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6FC0;&#x60C5; <pr>j&#x12B;q&#xED;ng</pr></trd>,
    <trd>&#x79EF;&#x6781;&#x6027; <pr>j&#x12B;j&#xED;x&#xEC;ng</pr></trd>,
    <trd>&#x70ED;&#x98CE; <pr>r&#xE8;f&#x113;ng</pr></trd>,
    <trd>&#x70ED;&#x5FC3; <pr>r&#xE8;x&#x12B;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fervor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="fervor.0i.PSI">
    <dif>
      Esti <tld/>a:
      <ekz>
        tro <tld/>i ofte estas dan&gcirc;ere
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi &ccirc;iam estas preta <tld/>i en servado al la patrujo
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Zamenhof</nom> <tld/>is por muziko, li ludis fortepianon, kaj li &scirc;atis kanti
        <fnt>
          <bib>VivZam</bib>
          <lok>Vivo de Zamenhof</lok>
        </fnt>;
      </ekz>
      <ekz>
        nek lingvistoj nek politikistoj <tld/>is identigi la nunan Esperanton kun la
        estonta mondlingvo
        <fnt>
          <bib>EeP</bib>
          <lok>Kvina Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        Karlo <tld/>is &ccirc;irka&ubreve; li kun profesia komplezemo
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>;
      </ekz>
      <ekz>
        sen&ccirc;ese vi ion strebas, klopodas, aktivas kaj <tld/>as, dum &ccirc;ia &ccirc;i via penado
        celas nenien
        <fnt>
          <bib>SkandalJozef</bib>
          <lok>Plendoj de Jozefo arestita</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ard.0i.FIG">ardi <sncref/></ref>,
      <ref cel="bol.0i.fervori">boli <sncref/></ref>,
      <ref cel="brul.0i.FIG">bruli <sncref/></ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_r;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_r;&c_u;&c_p;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_d;&c_b;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>d&aacute;vat p&rcaron;ednost <klr>(n&ecaron;komu)</klr></trd>,
    <trd>nadr&zcaron;ovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>eifern</trd>,
    <trd><ind>eifrig</ind> sein</trd>,
    <trd>mit <ind>Begeisterung</ind> bei der Sache sein</trd>,
    <trd>mit <ind>Inbrunst</ind> tun</trd>,
    <trd><ind>leidenschaftlich</ind> dabei sein</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>be <ind>fervent</ind></trd>,
    <trd>be <ind>passionate</ind></trd>,
    <trd>be <ind>zealous</ind></trd>
  </trdgrp>
  <trd lng="fr">&ecirc;tre <ind>fervent</ind></trd>
  <trd lng="hu">buzg&oacute;lkodik</trd>
  <trd lng="ja">&#x71B1;&#x5FC3;&#x3067;&#x3042;&#x308B;  <pr>&#x306D;&#x3063;&#x3057;&#x3093;&#x3067;&#x3042;&#x308B;</pr></trd>
  <trd lng="nl">ijveren</trd>
  <trd lng="ru">&c_u;&c_s;&c_je;&c_r;&c_d;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">d&aacute;va&tcaron; prednos&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&c_p;&c_a;&c_d;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_ja;&c_v;&c_l;&c_ja;&c_t;&c_i; &c_r;&c_je;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_o;&c_p;&c_a;&c_d;&c_l;&c_i;&c_v;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_z;&c_ja;&c_t;&c_t;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_r;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5F37;&#x70C8;&#x62A8;&#x64CA; <pr>qi&#xE1;ngli&#xE8;p&#x113;ngj&#x12B;</pr></trd>,
    <trd>&#x70ED;&#x671B; <pr>r&#xE8;w&#xE0;ng</pr></trd>,
    <trd>&#x8CAC;&#x7F75; <pr>z&#xE9;m&#xE0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fervor.0a">
  <kap><tld/>a</kap>
  <snc mrk="fervor.0a.PSI">
    <dif>
      Varmege, pasie <ref cel="dilige.0a">diligenta</ref>:
      <ekz>
        la Levidoj estis pli <tld/>aj en la sinsanktigado, ol la pastroj
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 29:34</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la postuloj ili estis tre <tld/>aj kaj postuladis la plej delikatan silkon
        <fnt>
          <bib>Fab1</bib>
          <lok>La novaj vestoj de la re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        komenci&gcirc;is <tld/>a demandado
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi &scirc;ajnas senti al via parenco tre <tld/>an amemon
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        la plej <tld/>aj partianoj komencis lin mal&scirc;ati
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; la plej <tld/>aj Volapukistoj nature timis, ke morga&ubreve; aperos nova lingvo
        <fnt>
          <bib>EE</bib>
          <lok>VII</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="entuzi.0a">entuziasma</ref>,
<ref cel="ost.gxis0a">&gcirc;isosta</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
