<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fervor</rad>/o
</kap>
<drv mrk="fervor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Diligenta agemo, ardo por iu a&ubreve; io:
      <ekz>religia, politika <tld/>o;</ekz>
      <ekz><tld/>a esp-isto;</ekz>
      <ekz><tld/>e labori.</ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Eifer</trd>,
     <trd>Begeisterung</trd>,
     <trd>Inbrunst</trd>,
     <trd>Leidenschaft</trd></trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_je;&c_r;&c_d;&c_i;&c_je;</trd>,
    <trd>&c_r;&c_v;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
<drv mrk="fervor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>a: <ekz>tro <tld/>i estas dan&gcirc;ere.</ekz>
    </dif>
  </snc>
  <trd lng="ru">&c_u;&c_s;&c_je;&c_r;&c_d;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="fervor.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari iun <tld/>a. 
    </dif>
    <refgrp tip="sin">
      <ref cel="varm.0o">varmo</ref>,
      <ref cel="flam.0o">flamo</ref>,
      <ref cel="entuzi">entuziamso</ref>,
      <ref cel="vigl">vigleco</ref>,
      <ref cel="febr.0o">febro</ref>,
      <ref cel="bol.0i">boli</ref>.
    </refgrp>
  </snc>
</drv>
</art>
