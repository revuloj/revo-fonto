    <trd>&c_r;&c_u;&c_p;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_d;&c_b;&c_a;&c_j;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>horliv&yacute;</trd>,
    <trd>piln&yacute;</trd>,
    <trd>s p&iacute;l&iacute; konan&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>eifrig</trd>,
    <trd>begeistert</trd>,
    <trd>inbr&uuml;nstig</trd>,
    <trd>leidenschaftlich</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ardent</trd>,
    <trd>eager</trd>,
    <trd>fervent</trd>,
    <trd>fervid</trd>,
    <trd>impassioned</trd>,
    <trd>zealous</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ardent</trd>,
    <trd>fervent</trd>,
    <trd>passionn&eacute;</trd>
  </trdgrp>
  <trd lng="hu">buzg&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x71B1;&#x5FC3;&#x306A;  <pr>&#x306D;&#x3063;&#x3057;&#x3093;&#x306A;</pr></trd>,
    <trd>&#x71B1;&#x70C8;&#x306A;  <pr>&#x306D;&#x3064;&#x308C;&#x3064;&#x306A;</pr></trd>,
    <trd>&#x71B1;&#x60C5;&#x306B;&#x3042;&#x3075;&#x308C;&#x305F;  <pr>&#x306D;&#x3064;&#x3058;&#x3087;&#x3046;&#x306B;&#x3042;&#x3075;&#x308C;&#x305F;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
