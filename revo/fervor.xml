<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>fervor</rad>/o
</kap>
<drv mrk="fervor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Diligenta agemo, ardo por iu a&ubreve; io:
      <ekz>
        religia, politika <tld/>o;
      </ekz>
      <ekz>
        <tld/>o pri Via domo min konsumis
        <fnt><bib>MT</bib> <lok>&Psa; 69:9</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>e labori.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ard.0o">ardo</ref>,
      <ref cel="ekzalt">ekzalteco</ref>,
      <ref cel="entuzi.0o">entuziasmo</ref>,
      <ref cel="fajr.0o.fervoro">fajro <sncref/></ref>,
      <ref cel="febr.0o.fervoro">febro <sncref/></ref>,
      <ref cel="flam.0o.fervoro">flamo <sncref/></ref>,
      <ref cel="pasi.0o">pasio</ref>,
      <ref cel="varm.0ego.fervoro">varmego <sncref/></ref>,
      <ref cel="verv.0o">vervo</ref>,
      <ref cel="vigl">vigleco</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Eifer</trd>,
    <trd>Begeisterung</trd>,
    <trd>Inbrunst</trd>,
    <trd>Leidenschaft</trd>
  </trdgrp>
  <trd lng="es">fervor</trd>
  <trdgrp lng="fr">
    <trd>ferveur</trd>,
    <trd>ardeur</trd>,
    <trd>passion</trd>,
    <trd>z&egrave;le</trd>
  </trdgrp>
  <trd lng="hu">buzgalom</trd>
  <trd lng="nl">ijver</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_je;&c_r;&c_d;&c_i;&c_je;</trd>,
    <trd>&c_r;&c_v;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="fervor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>a:
      <ekz>
        tro <tld/>i estas dan&gcirc;ere.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ard.0i.FIG">ardi <sncref/></ref>,
      <ref cel="bol.0i.fervori">boli <sncref/></ref>,
      <ref cel="brul.0i.FIG">bruli <sncref/></ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>eifern</trd>,
    <trd>eifrig sein</trd>,
    <trd><klr>mit </klr>Begeisterung bei der <ind>Sache</ind> sein</trd>,
    <trd><klr>mit </klr>Inbrunst <ind>tun</ind></trd>,
    <trd>leidenschaftlich dabeisein</trd>
  </trdgrp>
  <trd lng="fr">&ecirc;tre <ind>fervent</ind></trd>
  <trd lng="hu">buzg&oacute;lkodik</trd>
  <trd lng="nl">ijveren</trd>
  <trd lng="ru">&c_u;&c_s;&c_je;&c_r;&c_d;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="fervor.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Varmege, pasie diligenta:
      <ekz>
        <tld/>a esperantisto.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>eifrig</trd>,
    <trd>begeistert</trd>,
    <trd>inbr&uuml;nstig</trd>,
    <trd>leidenschaftlich</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ardent</trd>,
    <trd>fervent</trd>,
    <trd>passionn&eacute;</trd>
  </trdgrp>
  <trd lng="hu">buzg&oacute;</trd>
  <trd lng="nl">ijverig</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_je;&c_r;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_je;&c_v;&c_n;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="fervor.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari iun <tld/>a.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>anfeuern</trd>,
    <trd>anstacheln</trd>,
    <trd>begeistern</trd>,
    <trd><klr>in </klr>Begeisterung <ind>versetzen</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>susciter la <ind>ferveur</ind> (de)</trd>,
    <trd>exalter<klr tip="ind"> (passionner)</klr></trd>,
    <trd>galvaniser<klr tip="ind"> (passionner)</klr></trd>,
    <trd>passionner<klr tip="ind"> (susciter la ferveur)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>buzd&iacute;t</trd>,
    <trd>&ouml;szt&ouml;n&ouml;z</trd>
  </trdgrp>
</drv>

<drv mrk="fervor.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Pasia kaj diligenta aganto por iu afero:
      <ekz>
        ili &ccirc;iuj estas <tld/>uloj por la le&gcirc;o
        <fnt><bib>NT</bib>, <lok>&Ago; 21:20</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">adepte fervent</trd>
</drv>
</art>
