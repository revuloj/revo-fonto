<drv mrk="fervor.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fervor.0igi.PSI">
    <dif>
      Fari iun <tld/>a:
      <ekz>
        kapabla <tld/>igi homojn al kunlaboro
        <fnt>
          <bib>Kon11</bib>
          <lok>TEJO ser&ccirc;as... novan redaktoron de TEJO tutmonde</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="entuzi.0igi">entuziasmigi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_t;&c_h;&c_n;&c_ja;&c_c;&c_mol;</trd>
  <trd lng="cs">rozohnit</trd>
  <trdgrp lng="de">
    <trd>anfeuern</trd>,
    <trd>anstacheln</trd>,
    <trd>begeistern</trd>,
    <trd><klr>in </klr>Begeisterung <ind>versetzen</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>incite the <ind>fervour</ind> (of)</trd>,
    <trd>galvanize<klr tip="ind"> (excite passion)</klr></trd>,
    <trd>inflame<klr tip="ind"> (incite the fervour of)</klr></trd>,
    <trd>make fervid</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>susciter la <ind>ferveur</ind> (de)</trd>,
    <trd>exalter<klr tip="ind"> (passionner)</klr></trd>,
    <trd>galvaniser<klr tip="ind"> (passionner)</klr></trd>,
    <trd>passionner<klr tip="ind"> (susciter la ferveur)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>buzd&iacute;t</trd>,
    <trd>&ouml;szt&ouml;n&ouml;z</trd>
  </trdgrp>
  <trd lng="ja">&#x71B1;&#x610F;&#x3092;&#x6301;&#x305F;&#x305B;&#x308B;  <pr>&#x306D;&#x3064;&#x3044;&#x3092;&#x3082;&#x305F;&#x305B;&#x308B;</pr></trd>
  <trdgrp lng="pl">
    <trd>rozentuzjamowa&cacute;</trd>,
    <trd>rozpala&cacute;<klr tip="ind"> (np. rozpala&cacute;
    uczucie)</klr></trd>,
    <trd>zagrzewa&cacute;<klr tip="ind"> (np. zagrzewa&cacute; do
    walki)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_o;&c_d;&c_u;&c_sh;&c_je;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>
  <trd lng="sk">rozhorli&tcaron;</trd>
  <trdgrp lng="zh">
    <trd>&#x52A9;&#x9635; <pr>zh&#xF9;zh&#xE8;n</pr></trd>,
    <trd>&#x52A9;&#x9577; <pr>zh&#xF9;zh&#x1CE;ng</pr></trd>,
    <trd>&#x6388;&#x610F; <pr>sh&#xF2;uy&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fervor.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="fervor.0ulo.PSI">
    <dif>
      Pasia kaj diligenta aganto por iu afero:
      <ekz>
        ili &ccirc;iuj estas <tld/>uloj por la le&gcirc;o
        <fnt><bib>NT</bib>, <lok>&Ago; 21:20</lok></fnt>;
      </ekz>
      <ekz>
        dekkelke da junaj <tld/>uloj kure eskortis lian veturilon, jen super&scirc;utante lin
        per la&ubreve;doj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
