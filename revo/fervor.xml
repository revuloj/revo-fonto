<drv mrk="fervor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fervor.0o.PSI">
    <dif>
      Diligenta agemo, <ref cel="ard.0o.pasio">ardo</ref> por iu a&ubreve; io:
      <ekz>
        <tld/>o pri Via domo min konsumis
        <fnt><bib>MT</bib> <lok>&Psa; 69:9</lok></fnt>;
      </ekz>
      <ekz>
        kun granda <tld/>o li laboris por la ideo de lingvo internacia
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista en Boulogne sur Mer en la 5a de a&ubreve;gusto 1905</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> en tro granda sed ne prudenta <tld/>o faris ian falsan pa&scirc;on
        kaj ridindigis <klr>[la&#x2026;]</klr> tutan aferon
        <fnt>
          <bib>EE</bib>
          <lok>VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        sankta <tld/>o
        <fnt>
          <aut>L. L. Zamenhof</aut>
          <vrk>Al la Fratoj</vrk>
        </fnt>;
      </ekz>
      <ekz>
        blinda <tld/>o
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista en Boulogne sur Mer en la 5a de a&ubreve;gusto 1905</vrk>
        </fnt>;
      </ekz>
      <ekz>
        &jcirc;etataj en la kirlon de senbrida <tld/>o
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel aran&gcirc;i tiamaniere, ke la estraro vidu mian <tld/>on
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto tria</lok>
        </fnt>?
      </ekz>
      <ekz>
        per pacienco kaj <tld/>o sukcesas &ccirc;iu laboro
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekzalt">ekzalteco</ref>,
      <ref cel="entuzi.0o">entuziasmo</ref>,
      <ref cel="fajr.0o.fervoro">fajro<sncref/></ref>,
      <ref cel="febr.0o.fervoro">febro<sncref/></ref>,
      <ref cel="flam.0o.fervoro">flamo<sncref/></ref>,
      <ref cel="pasi.0o">pasio</ref>,
      <ref cel="varm.0ego.fervoro">varmego<sncref/></ref>,
      <ref cel="verv.0o">vervo</ref>,
      <ref cel="vigl">vigleco</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_r;&c_a;&c_n;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_b;&c_a;&c_j;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_u;&c_p;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;</trd>
  </trdgrp>
  <trd lng="cs">horlivost</trd>
  <trdgrp lng="de">
    <trd>Eifer</trd>,
    <trd>Begeisterung</trd>,
    <trd>Inbrunst</trd>,
    <trd>Leidenschaft</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ardour</trd>,
    <trd>fervour</trd>,
    <trd>fervency</trd>,
    <trd>passion</trd>,
    <trd>zeal</trd>
  </trdgrp>
  <trd lng="es">fervor</trd>
  <trdgrp lng="fr">
    <trd>ferveur</trd>,
    <trd>ardeur</trd>,
    <trd>passion</trd>,
    <trd>z&egrave;le</trd>
  </trdgrp>
  <trd lng="hu">buzgalom</trd>
  <trdgrp lng="ja">
    <trd>&#x71B1;&#x610F;  <pr>&#x306D;&#x3064;&#x3044;</pr></trd>,
    <trd>&#x71B1;&#x5FC3;  <pr>&#x306D;&#x3063;&#x3057;&#x3093;</pr></trd>,
    <trd>&#x71B1;&#x60C5;  <pr>&#x306D;&#x3064;&#x3058;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
