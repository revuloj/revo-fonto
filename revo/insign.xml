<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>insign</rad>/o
</kap>
<drv mrk="insign.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Tuto de la emblemoj, difinitaj per heraldiko, kiuj servas kiel
      distingigaj signoj al iu nobela familio, al urbo ktp; blazono. 
    </dif>
  <trdgrp lng="nl">
    <trd>wapen</trd>,
    <trd>blazoen</trd>
  </trdgrp>
  </snc>
  <snc num="2">
    <dif>
      Distingiga signo de rango, ofico, societo ktp:
      <ekz>la esperantistoj alprenis verdan stelon kiel <tld/>on.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="atribu1.0o">atributo</ref>,
      <ref cel="emblem.0o">emblemo</ref>,
      <ref cel="simbol.0o">simbolo</ref>.
    </refgrp>
  <trd lng="nl">insigne</trd>
  </snc>
  <trd lng="de">Abzeichen</trd>
  <trd lng="en">insignia</trd>
  <trd lng="ru">&c_z;&c_n;&c_a;&c_ch;&c_o;&c_k;</trd>
</drv>
</art>
