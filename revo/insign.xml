<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>insign</rad>/o
</kap>
<drv mrk="insign.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="insign.0o.signo">
    <dif>
      Distingiga signo de rango, ofico, societo ktp:
      <ekz>
        la esperantistoj alprenis verdan stelon kiel <tld/>on.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="atribu1.0o">atributo</ref>,
      <ref cel="emblem.0o">emblemo</ref>,
      <ref cel="simbol.0o">simbolo</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_z;&c_n;&c_a;&c_k;</trd>,
      <trd>&c_z;&c_n;&c_a;&c_ch;&c_o;&c_k;</trd>,
      <trd>&c_e;&c_m;&c_b;&c_l;&c_je;&c_m;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>ins&iacute;gnia</trd>,
      <trd>ensenya</trd>,
      <trd>emblema</trd>
    </trdgrp>
    <trd lng="de">Abzeichen</trd>
    <trdgrp lng="el">
      <trd>&sigma;&eta_ton;&my;&alfa;</trd>,
      <trd>&epsilon_ton;&my;&beta;&lambda;&eta;&my;&alfa;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>embl&egrave;me</trd>,
      <trd>insigne<klr tip="ind"> (subst.)</klr></trd>,
      <trd>logo</trd>
    </trdgrp>
    <trd lng="hu">jelv&eacute;ny</trd>
    <trd lng="nl">insigne</trd>
    <trd lng="pt">ins&iacute;gnia</trd>
  </snc>
  <snc mrk="insign.0o.HER">
    <uzo tip="fak">HER</uzo>
    <ref tip="dif" cel="blazon.0o">Blazono</ref>
    <trd lng="be">&c_g;&c_je;&c_r;&c_b;</trd>
    <trd lng="hu">c&iacute;mer</trd>
    <trd lng="de">Wappen</trd>
    <trdgrp lng="nl">
      <trd>wapen</trd>,
      <trd>blazoen</trd>
    </trdgrp>
    <trdgrp lng="pt">
     <trd>bras&atilde;o</trd>, 
     <trd>emblema</trd>, 
     <trd>escudo de armas</trd> 
    </trdgrp>
  </snc>
  <trd lng="en">insignia</trd>
  <trd lng="es">insignia</trd>
  <trdgrp lng="it">
     <trd>emblema</trd>, 
     <trd>blasone</trd>,
     <trd>insegna<klr> (blasone)</klr></trd>
    </trdgrp>
  <trd lng="ru">&c_z;&c_n;&c_a;&c_ch;&c_o;&c_k;</trd>
  <trd lng="sk">odznak</trd>
</drv>
</art>
