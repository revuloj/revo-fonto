<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>insign</rad>/o
</kap>
<drv mrk="insign.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="insign.0o.signo">
    <dif>
      Distingiga signo de rango, ofico, societo ktp:
      <ekz>
        la esperantistoj alprenis verdan stelon kiel <tld/>on.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="atribu1.0o">atributo</ref>,
      <ref cel="emblem.0o">emblemo</ref>,
      <ref cel="simbol.0o">simbolo</ref>.
    </refgrp>
    <trdgrp lng="fr">
      <trd>embl&ecirc;me</trd>,
      <trd>insigne</trd>,
      <trd>logo</trd>
    </trdgrp>
    <trd lng="nl">insigne</trd>
  </snc>
  <snc mrk="insign.0o.HER">
    <uzo tip="fak">HER</uzo>
    <ref tip="dif" cel="blazon.0o">Blazono</ref>
    <trdgrp lng="nl">
      <trd>wapen</trd>,
      <trd>blazoen</trd>
    </trdgrp>
  </snc>
  <trd lng="de">Abzeichen</trd>
  <trd lng="en">insignia</trd>
  <trd lng="ru">&c_z;&c_n;&c_a;&c_ch;&c_o;&c_k;</trd>
</drv>
</art>
