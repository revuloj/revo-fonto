<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: pirog.xml,v 1.21 2023/10/20 16:38:17 revo Exp $">
    <kap>
      <rad>pirog</rad>/o
    </kap>

<drv mrk="pirog.0o">
  <kap><tld/>o</kap>
  <snc mrk="pirog.0o.boato">
    <dif>
      <ref tip="super" cel="boat.0o">Boato</ref> farita el kavigita arbotrunko kun altigitaj flankoj:
      <ekz>
        tri kondamnitoj nove el&scirc;ipigitaj, trovinte malnovan <tld/>on tute difektitan, ne
        &scirc;anceli&gcirc;is konfidi sian vivon al tiu rompebla marveturilo
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>on remas la edzino, rapidigata de la edzo
        <fnt>
          <bib>LOdE</bib>
          <aut>trad. Gbeglo Koffi</aut>
          <vrk>Kiam la bestoj estis kiel homoj</vrk>
        </fnt>;
      </ekz>
      <ekz>
        meze de la rivero, kun motorgrumblado, la <tld/>o rapidegas tra la mallumo
        <fnt>
          <bib>MD05</bib>
          <vrk>En Venez&ubreve;elo, voja&gcirc;o en indiana lando</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kanot.0o">kanoto</ref>,
      <ref cel="kanu.0o">kanuo</ref>
    </refgrp>
    <bld lok="&WCU;/thumb/7/7a/Traditional_fishing_Maquili_2.jpg/640px-Traditional_fishing_Maquili_2.jpg" prm="CC BY-SA 3.0">fi&scirc;ista <tld/>o, Maquili
        <fnt>
          <aut>David Palaz&oacute;n, Tatoli ba Kultura, Tatoli ba Kultura</aut>
          <lok><url ref="&WCW;/File:Traditional_fishing_Maquili_2.jpg">&WCW;/File:Traditional_fishing_Maquili_2.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="be">&c_p;&c_ib;&c_r;&c_o;&c_g;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Piroge</trd>,
      <trd>Einbaum</trd>
    </trdgrp>
    <trd lng="en">pirogue</trd>
    <trdgrp lng="hu">
      <trd>b&ouml;d&ouml;ncs&oacute;nak</trd>,
      <trd>piroga</trd>
    </trdgrp>
    <trd lng="nl">prauw</trd>
    <trdgrp lng="pt">
      <trd>piroga</trd>,
      <trd>canoa</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_i;&c_r;&c_o;&c_g;&c_a;</trd>
  </snc>  
  <rim>
    <ref tip="hom" cel="pirog1.0o.KUI">Pirogo</ref> estas anka&ubreve; far&ccirc;a&jcirc;o.
  </rim> 
  <trd lng="cs">piro&zcaron;ka</trd>
  <trd lng="ja">&#x4E38;&#x6728;&#x821F;  <pr>&#x307E;&#x308B;&#x304D;&#x3076;&#x306D;</pr></trd>
  <trdgrp lng="sk">
    <trd>indi&aacute;nske kanoe</trd>,
    <trd>piroga</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x456;&#x440;&#x43E;&#x491;&#x430;</trd>,
    <trd>&#x43F;&#x438;&#x440;&#x456;&#x433;</trd>
  </trdgrp>
</drv>
