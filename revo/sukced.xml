<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: sukced.xml,v 1.10 2023/11/25 01:31:55 revo Exp $">
<kap>
  <rad>sukced</rad>/i <fnt><bib>SPIV</bib></fnt>
</kap>
<drv mrk="sukced.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="sukced.0i.sekvi">
    <dif>
      <ref tip="super" cel="sekv.0i.okazi_post">Sekvi</ref> post iu,
      surprenante ties oficon, postenon, rangon,
      regadon:
      <ekz>
        la 1-an de decembro, 1997, li
        <klr>(<nom>Mohamed</nom> el <nom>Baradei</nom>)</klr>
        <tld/>is la svedon <nom>Hans Blix</nom>
        kaj enofici&gcirc;is kiel &gcirc;enerala direktoro de <nom>IAEA</nom>
        <fnt>
          <vrk>&Gcirc;enerala direktoro de IAEA Mohamed el Baradei</vrk>.
          <lok><url
          ref="http://big5.cri.cn/gate/big5/esperanto.cri.cn/1/2006/11/29/1@49608.htm">&Ccirc;inia
          Radio Internacia</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="post.0eulo">posteulo</ref>
  </snc>
  <rim>
    Ne konfuzu kun <ref cel="sukces.0i">sukcesi</ref>!
  </rim>
  <rim>
    Anstata&ubreve; riski nekomprenon, oni simple povas precizigi la kutiman verbon &leftquot;sekvi&rightquot;:
    <ekz>
      Johano Pa&ubreve;lo la 2-a <em>sekvis</em> la 1-an sur la sankta se&gcirc;o (en sia ofico; ofice a.s.);
    </ekz>
    <ekz>
      caro Petro la Granda <em>sekvis</em> post Ivano la 5-a sur la trono.
    </ekz>
    <aut>Wolfram Diestel</aut>
  </rim>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_s;&c_mol;&c_l;&c_je;&c_d;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_u;&c_p;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_n;&c_a; &c_z;&c_mol;&c_m;&c_je;&c_n;&c_u;)</klr></trd>,
    <trd>&c_ib;&c_s;&c_mol;&c_c;&c_ib; &c_n;&c_a; <ind>&c_z;&c_mol;&c_m;&c_je;&c_n;&c_u;</ind></trd>
  </trdgrp>
  <trd lng="de"><klr>(im Amt) </klr>nachfolgen</trd>
  <trd lng="en">succeed<klr> (to smb, smth)</klr></trd>
  <trd lng="fr">succ&eacute;der</trd>
  <trdgrp lng="pl">
    <trd>sukcedowa&cacute;</trd>,
    <trd>dziedziczy&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_je;&c_m;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_j;&c_t;&c_i; &c_n;&c_a; &c_s;&c_m;&c_je;&c_n;&c_u;</trd>
  </trdgrp>
  <trd lng="sk">prevzia&tcaron; n&aacute;stupn&iacute;ctvo</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;evz&iacute;t n&aacute;stupnictv&iacute;</trd>,
    <trd>p&rcaron;ijmout n&aacute;stupnictv&iacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_i;&c_h;&c_o;&c_d;&c_i;&c_t;&c_i; &c_n;&c_a; &c_z;&c_m;&c_ib;&c_n;&c_u;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_b;&c_i;&c_r;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_u;&c_s;&c_p;&c_a;&c_d;&c_k;&c_o;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i; &c_p;&c_o;&c_s;&c_a;&c_d;&c_u;</trd>,
    <trd>&c_v;&c_l;&c_a;&c_d;&c_u;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_ib;&c_shch;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="sukced.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      La ago <tld/>i:
      <ekz>
        la <ind>apostola <tld/>o</ind>
        <trd lng="en">apostolic succession</trd>
        <trd lng="ru">&c_a;&c_p;&c_o;&c_s;&c_t;&c_o;&c_l;&c_mol;&c_s;&c_k;&c_o;&c_je; &c_p;&c_r;&c_je;&c_je;&c_m;&c_s;&c_t;&c_v;&c_o;</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_s;&c_mol;&c_l;&c_je;&c_d;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_u;&c_p;&c_n;&c_ib;&c_c;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_u;&c_p;&c_s;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Nachfolge <klr>(im Amt)</klr></trd>
  <trd lng="en">succeed<klr> (to smb, smth)</klr></trd>
  <trd lng="fr">succession</trd>
  <trdgrp lng="pl">
    <trd>sukcesja</trd>,
    <trd>dziedzictwo</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_je;&c_m;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
</drv>
</art>

