<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: sukced.xml,v 1.7 2023/06/13 13:30:03 revo Exp $">
    <kap>
      <rad>sukced</rad>/i <fnt><bib>SPIV</bib></fnt>
    </kap>
    <drv mrk="sukced.0i">
      <kap><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Veni post iu, surprenante ties oficon, postenon, rangon,
          regadon:
          <ekz>
            la 1-an de decembro, 1997, li
            <klr>(Mohamed el Baradei)</klr>
            <tld/>is la svedon Hans Blix
            kaj enofici&gcirc;is kiel &gcirc;enerala direktoro de IAEA
            <fnt>
              <vrk>&Gcirc;enerala direktoro de IAEA Mohamed el Baradei</vrk>.
              <lok><url
              ref="http://big5.cri.cn/gate/big5/esperanto.cri.cn/1/2006/11/29/1@49608.htm">&Ccirc;inia
              Radio Internacia</url></lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <rim>
        Ne konfuzu kun <ref cel="sukces.0i">sukcesi</ref>!
      </rim>
      <trdgrp lng="be">
        <trd>&c_n;&c_a;&c_s;&c_mol;&c_l;&c_je;&c_d;&c_v;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_n;&c_a;&c_s;&c_t;&c_u;&c_p;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_n;&c_a; &c_z;&c_mol;&c_m;&c_je;&c_n;&c_u;)</klr></trd>,
        <trd>&c_ib;&c_s;&c_mol;&c_c;&c_ib; &c_n;&c_a; <ind>&c_z;&c_mol;&c_m;&c_je;&c_n;&c_u;</ind></trd>
      </trdgrp>
      <trd lng="en">succeed<klr> (to smb, smth)</klr></trd>
      <trd lng="fr">succ&eacute;der</trd>
      <trdgrp lng="ru">
        <trd>&c_n;&c_a;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_p;&c_r;&c_je;&c_je;&c_m;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_p;&c_r;&c_i;&c_j;&c_t;&c_i; &c_n;&c_a; &c_s;&c_m;&c_je;&c_n;&c_u;</trd>
      </trdgrp>
      <trd lng="sk">prevzia&tcaron; n&#xE1;stupn&#xED;ctvo</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;evz&#xED;t n&#xE1;stupnictv&#xED;</trd>,
    <trd>p&rcaron;ijmout n&#xE1;stupnictv&#xED;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&c_i;&c_h;&#x43E;&c_d;&c_i;&c_t;&c_i; &#x43D;&c_a; &c_z;&#x43C;&c_ib;&#x43D;&c_u;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_je;&c_b;&c_i;&c_r;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_u;&c_s;&#x43F;&c_a;&c_d;&#x43A;&#x43E;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i; &#x43F;&#x43E;&c_s;&c_a;&c_d;&c_u;</trd>,
    <trd>&c_v;&#x43B;&c_a;&c_d;&c_u;</trd>,
    <trd>&c_z;&c_a;&#x43C;&c_ib;&c_shch;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

    <drv mrk="sukced.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          La evento a&ubreve; ago <tld/>i:
          <ekz>
            la <ind>apostola <tld/>o</ind>
            <trd lng="en">apostolic succession</trd>
            <trd lng="ru">&c_a;&c_p;&c_o;&c_s;&c_t;&c_o;&c_l;&c_mol;&c_s;&c_k;&c_o;&c_je; &c_p;&c_r;&c_je;&c_je;&c_m;&c_s;&c_t;&c_v;&c_o;</trd>
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_n;&c_a;&c_s;&c_mol;&c_l;&c_je;&c_d;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
        <trd>&c_z;&c_a;&c_s;&c_t;&c_u;&c_p;&c_n;&c_ib;&c_c;&c_t;&c_v;&c_a;</trd>,
        <trd>&c_n;&c_a;&c_s;&c_t;&c_u;&c_p;&c_s;&c_t;&c_v;&c_a;</trd>
      </trdgrp>
      <trd lng="en">succeed<klr> (to smb, smth)</klr></trd>
      <trd lng="fr">succession</trd>
      <trdgrp lng="ru">
        <trd>&c_n;&c_a;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
        <trd>&c_p;&c_r;&c_je;&c_je;&c_m;&c_s;&c_t;&c_v;&c_o;</trd>
      </trdgrp>
    </drv>

  </art>

