<art mrk="$Id: scenografi.xml,v 1.4 2023/07/28 20:26:18 revo Exp $">
<kap>
  <rad>scenografi</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="scenografi.0o">
  <kap><tld/>o</kap>
  <snc mrk="scenografi.0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Arto pri aran&gcirc;o de scenejo, dekoro kaj
      <ref tip="vid" cel="scen.en0igo.TEA">enscenigo</ref>:
      <ekz>
        la&ubreve; la <tld/>o oni facile komprenas, ke la grupo de
        alilandanoj
        festas geedzi&gcirc;on
        <fnt>
         <aut>K. Kniivil&auml;</aut>,
         <vrk><url
         ref="http://www.liberafolio.org/kartonaj-figuroj-sur-scenejo">
         Kartonaj figuroj sur scenejo</url></vrk>,
         <lok>libera folio, 2015-07-29</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_c;&c_e;&c_n;&c_a;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>
  <trd lng="es">escenograf&iacute;a</trd>
  <trd lng="fr">sc&eacute;nographie</trd>
  <trd lng="pl">scenografia</trd>
</drv>
</art>

