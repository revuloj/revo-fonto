<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bas.xml,v 1.23 2022/03/16 16:54:19 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>bas</rad>/o
</kap>
<drv mrk="bas.0o">
  <kap><tld/>o<fnt><bib>VdE</bib></fnt></kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="bas.0o.registro">
    <dif>
      Plej malalta homvo&ccirc;a
      <ref tip="super" cel="regist.0o.MUZ">registro</ref>,
      kante uzata preska&ubreve; nur de viroj.
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;iam la profunda <tld/>o de la akvofalo nin akompanadis.
      </ekz>
    </dif>
    <trd lng="de">Bassstimme</trd>
    <trd lng="fr">basse<klr> (voix)</klr></trd>
    <trd lng="pl">bas</trd>
    <trd lng="ru">&c_b;&c_a;&c_s;<klr tip="amb">
    (&c_g;&c_o;&c_l;&c_o;&c_s;)</klr></trd>
  </snc>
  <snc mrk="bas.0o.ulo">
    <dif>
      Muzikilo, vo&ccirc;o a&ubreve; kantisto kun basa,
      a&ubreve; &gcirc;enerale plej malalta registro.
    </dif>
    <trd lng="pl">bas</trd>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_s;</trd>
  <trdgrp lng="cs">
    <trd>bas</trd>,
    <trd>basov&yacute; hlas</trd>
  </trdgrp>
  <trd lng="de">Bass</trd>
  <trd lng="en">bass</trd>
  <trd lng="es">bajo</trd>
  <trd lng="he">&#x5D1;&#x5E1;</trd>
  <trd lng="hu">basszus</trd>
  <trd lng="it">basso<klr> (mus.)</klr></trd>
  <trd lng="ru">&c_b;&c_a;&c_s;<klr tip="amb">
  (&c_p;&c_je;&c_v;&c_je;&c_c;)</klr></trd>
  <trd lng="sk">bas</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&c_s; <klr>(&c_g;&#x43E;&#x43B;&#x43E;&c_s;)</klr></trd>,
    <trd>&c_d;&c_u;&c_h;&#x43E;&c_v;&c_i;&c_j; <klr>(&c_ib;&#x43D;&c_s;&c_t;&c_r;&c_u;&#x43C;&c_je;&#x43D;&c_t;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="bas.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      De plej malalta registro inter vo&ccirc;oj
      a&ubreve; inter samfamiliaj instrumentoj.
    </dif>
    <trd lng="be">&c_b;&c_a;&c_s;&c_o;&c_v;&c_y;</trd>
    <trdgrp lng="de">
      <trd>tief</trd>,
      <trd>bassig</trd>,
      <trd>Bass-</trd>
    </trdgrp>
    <trd lng="en">bass</trd>
    <trd lng="es">bajo</trd>
    <trdgrp lng="fr">
      <trd>bas<klr> (registre musical)</klr></trd>,
      <trd>de <ind>basse</ind><klr> (musique)</klr></trd>
    </trdgrp>
    <trd lng="hu">basszus</trd>
    <trd lng="it">basso<klr> (agg.) (mus.)</klr></trd>
    <trd lng="pl">basowy</trd>
    <trdgrp lng="ru">
      <trd>&c_b;&c_a;&c_s;&c_o;&c_v;&c_y;&c_j;</trd>,
      <trd>&c_n;&c_i;&c_z;&c_k;&c_i;&c_j;<klr tip="amb"> (&c_o;
      &c_z;&c_v;&c_u;&c_k;&c_je;)</klr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>doln&iacute;</trd>,
    <trd>n&iacute;zk&yacute; <klr>(ze&dcaron;, hlas)</klr></trd>
  </trdgrp>
  <trd lng="he">&#x5E0;&#x5DE;&#x5D5;&#x5DA;</trd>
  <trdgrp lng="sk">
    <trd>doln&yacute;</trd>,
    <trd>n&iacute;zky</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&c_s;&#x43E;&c_v;&c_i;&c_j;</trd>,
    <trd>&#x43D;&c_i;&c_z;&#x44C;&#x43A;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="bas.0isto">
  <kap><tld/>isto</kap>
  <fnt><bib>RokPop</bib></fnt>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="instru2.0isto">Instrumentisto</ref>
      kiu ludas basan muzikilon,
      ekzemple basgitaron, kontrabason.
    </dif>
    <trd lng="be">&c_b;&c_a;&c_s;&c_ib;&c_s;&c_t;</trd>
    <trd lng="de">Bassist</trd>
    <trd lng="en">bassist</trd>
    <trd lng="es">bajista</trd>
    <trdgrp lng="fr">
      <trd>(joueur de) <ind>basse</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>basszus<klr tip="amb"> (zen&eacute;sz)</klr></trd>,
      <trd>basszista<klr tip="amb"> (zen&eacute;sz)</klr></trd>,
    </trdgrp>
    <trd lng="it">bassista<klr> (mus.)</klr></trd>
    <trd lng="pl">basista</trd>
    <trd lng="ru">&c_b;&c_a;&c_s;&c_i;&c_s;&c_t;</trd>
  </snc>
</drv>

<drv mrk="bas.0ulo">
  <kap><tld/>ulo</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Basvo&ccirc;a homo, precipe basvo&ccirc;a <ref tip="super" cel="kant.0isto.profesio">kantisto</ref>:
      <ekz>
        la kantoro, kies kantado instrumentosimile estis akompanata de
        la vo&ccirc;oj de liaj du helpantoj, la <tld/>ulo kaj la sopranulo
        <fnt><aut>H.Heine, tr. &Z;</aut>:
           <vrk>La Rabeno de Ba&hcirc;ara&hcirc;</vrk></fnt>;
      </ekz><ekz>
        la kapitano, kiu ronkadis kiel <tld/>ulo
        <fnt>
          <aut>Henri Vallienne</aut>: <vrk>Kastelo de Prelongo</vrk>.
          <lok>1907</lok>.
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_a;&c_s; <klr tip="amb">(&c_s;&c_mol;&c_p;&c_ja;&c_v;&c_a;&c_k;)</klr></trd>
    <trd lng="de">Bass<klr> [Person]</klr></trd>
    <trd lng="en">bass (singer)</trd>
    <trd lng="es">bajo</trd>
    <trd lng="fr">basse<klr> (chanteur)</klr></trd>
    <trdgrp lng="hu">
      <trd>basszus(&eacute;nekes)</trd>,
      <trd>basszista</trd>,
    </trdgrp>
    <trd lng="it">basso<klr> (mus.)</klr></trd>
    <trd lng="pl">bas<klr> (&sacute;piewak)</klr></trd>
    <trd lng="ru">&c_b;&c_a;&c_s;<klr tip="amb">
    (&c_p;&c_je;&c_v;&c_je;&c_c;)</klr></trd>
  </snc>
  <trd lng="cs">basista <klr>(zp&ecaron;v&aacute;k)</klr></trd>
  <trd lng="sk">basista</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&c_s;</trd>,
    <trd>&#x43B;&#x44E;&c_d;&c_i;&#x43D;&c_a;</trd>,
    <trd>&c_shch;&#x43E; &c_v;&#x43E;&#x43B;&#x43E;&c_d;&c_ib;&c_jeu; <ind>&c_b;&c_a;&c_s;</ind>&#x43E;&#x43C;</trd>
  </trdgrp>
</drv>
</art>
