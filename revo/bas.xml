<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>bas</rad>/o
</kap>
<drv mrk="bas.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Plej malalta vira vo&ccirc;o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;iam la profunda <tld/>o de la akvofalo nin akompanadis.
      </ekz>
    </dif>
    <trd lng="fr">basse<klr tip="ind"> (voix)</klr></trd>
    <trd lng="ru">&c_b;&c_a;&c_s; <klr tip="amb">(&c_g;&c_o;&c_l;&c_o;&c_s;)</klr></trd>
  </snc>
  <snc>
    <ref tip="dif" cel="bas.0ulo">Basulo.</ref>
  </snc>
</drv>

<drv mrk="bas.0ulo">
  <kap><tld/>ulo</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Kantisto kun tia vo&ccirc;o.
    </dif>
  </snc>
  <trd lng="fr">basse<klr tip="ind"> (chanteur)</klr></trd>
  <trd lng="ru">&c_b;&c_a;&c_s; <klr tip="amb">(&c_p;&c_je;&c_v;&c_je;&c_c;)</klr></trd>
</drv>
</art>
