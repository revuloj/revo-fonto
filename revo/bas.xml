<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>bas</rad>/o
</kap>
<drv mrk="bas.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="bas.0o.registro">
    <dif>
      Plej malalta homvo&ccirc;a
      <ref tip="super" cel="regist.0o.MUZ">registro</ref>,
      kante uzata preska&ubreve; nur de viroj.
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;iam la profunda <tld/>o de la akvofalo nin akompanadis.
      </ekz>
    </dif>
    <trd lng="de">Bassstimme</trd>
    <trd lng="fr">basse<klr> (voix)</klr></trd>
    <trd lng="ru">&c_b;&c_a;&c_s;<klr tip="amb"> (&c_g;&c_o;&c_l;&c_o;&c_s;)</klr></trd>
  </snc>
  <snc mrk="bas.0o.ulo">
    <dif>
      Muzikilo, vo&ccirc;o a&ubreve; kantisto kun basa,
      a&ubreve; &gcirc;enerale plej malalta registro.
    </dif>
  </snc>
  <trd lng="de">Bass</trd>
  <trd lng="en">bass</trd>
  <trd lng="es">bajo</trd>
  <trd lng="hu">basszus</trd>
  <trd lng="ru">&c_b;&c_a;&c_s;<klr tip="amb"> (&c_p;&c_je;&c_v;&c_je;&c_c;)</klr></trd>
</drv>

<drv mrk="bas.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      De plej malalta registro inter vo&ccirc;oj
      a&ubreve; inter samfamiliaj instrumentoj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>tief</trd>,
    <trd>bassig</trd>,
    <trd>Bass-</trd>
  </trdgrp>
  <trd lng="en">bass</trd>
  <trd lng="es">bajo</trd>
  <trdgrp lng="fr">
    <trd>bas<klr> (registre musical)</klr></trd>,
    <trd>de <ind>basse</ind><klr> (musique)</klr></trd>
  </trdgrp>
  <trd lng="hu">basszus</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_s;&c_o;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_i;&c_z;&c_k;&c_i;&c_j;<klr tip="amb"> (&c_o; &c_z;&c_v;&c_u;&c_k;&c_je;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="bas.0isto">
  <kap><tld/>isto</kap>
  <fnt><bib>RokPop</bib></fnt>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="instru2.0isto">Instrumentisto</ref>
      kiu ludas basan muzikilon,
      ekzemple basgitaron, kontrabason.
    </dif>
  </snc>
  <trd lng="de">Bassist</trd>
  <trd lng="en">bassist</trd>
  <trd lng="es">bajista</trd>
  <trdgrp lng="fr">
    <trd>(joueur de) <ind>basse</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>basszus<klr tip="amb"> (zen&eacute;sz)</klr></trd>,
    <trd>basszista<klr tip="amb"> (zen&eacute;sz)</klr></trd>,
  </trdgrp>
  <trd lng="ru">&c_b;&c_a;&c_s;&c_i;&c_s;&c_t;</trd>
</drv>

<drv mrk="bas.0ulo">
  <kap><tld/>ulo</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Basvo&ccirc;a <ref tip="super" cel="kant.0isto.profesio">kantisto</ref>.
    </dif>
  </snc>
  <trd lng="de">Bass<klr> [Person]</klr></trd>
  <trd lng="en">bass (singer)</trd>
  <trd lng="es">bajo</trd>
  <trd lng="fr">basse<klr> (chanteur)</klr></trd>
  <trdgrp lng="hu">
    <trd>basszus(&eacute;nekes)</trd>,
    <trd>basszista</trd>,
  </trdgrp>
  <trd lng="ru">&c_b;&c_a;&c_s;<klr  tip="amb"> (&c_p;&c_je;&c_v;&c_je;&c_c;)</klr></trd>
</drv>
</art>
