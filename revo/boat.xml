    <trd>&lstroke;&oacute;dka</trd>
  </trdgrp>
  <trd lng="ps">&#x6A9;&#x69A;&#x62A;&#x6CD;</trd>
  <trd lng="pt">bote</trd>
  <trd lng="ro">barc&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_l;&c_ju;&c_p;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sd">&#x67B;&#x64A;&#x699;&#x64A;&#x621;</trd>
  <trd lng="si">&#xDB6;&#xDDD;&#xDA7;&#xDCA;&#xDA7;&#xDD4;</trd>
