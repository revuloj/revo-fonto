<drv mrk="boat.0i">
  <kap>
    <tld/>i,
    <var><kap><tld/>umi</kap></var>
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="boat.0i.SHI">
    <dif>
      <ref tip="super" cel="vetur.0i.TRA">Veturi</ref> per <tld/>o:
      <ekz>
        ni vizitos la famegajn Akvofalojn de Niagaro kaj <tld/>os en la mirinda regiono
        de la Mil Insuloj
        <fnt>
          <aut>UEA</aut>
          <vrk><url ref="&UEA;kongresoj/2022/Dua_Bulteno">107a UK, Dua Bulteno</url></vrk>
          <lok>2022</lok>
        </fnt>;
      </ekz>
      <ekz>
        turistoj <tld/>umas en la lago
        <fnt>
          <aut>R. H. Zerbetto</aut>
          <vrk><url ref="http://esperanto.china.org.cn/2021-04/19/content_77420074.htm">Somera
          feriejo de &ccirc;inaj imperiestroj en Chengde</url></vrk>
          <lok>Esperanto.china.org.cn, 2021-04-19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_a;&c_r;&c_o;&c_zh;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol; &c_n;&c_a; <ind>&c_ch;&c_o;&c_w;&c_n;&c_je;</ind></trd>
  <trd lng="de">Kahn <ind>fahren</ind></trd>
  <trd lng="fr">faire du canot</trd>
  <trd lng="pl">p&lstroke;ywa&cacute;  <ind>&lstroke;&oacute;dk&aogonek;</ind></trd>
</drv>

<drv mrk="boat.sur0igi">
  <kap>
    sur<tld/>igi
  </kap>
  <snc>
    <dif>
      Meti sur <tld/>on:
      <ekz>
        ili sur<tld/>igis la infanojn, remis al domo
        <fnt>
          <aut>Mark Twain, trad. Dorothea kaj Hans-Georg Kaiser</aut>
          <vrk><url ref="https://verkoj.com/verkistoj/hans-georg-kaiser/twain_tom_sawyer.pdf"
              >La Aventuroj de Tom Sawyer</url></vrk>
          <lok>&Ccirc;apitro 32</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_mol; &c_u; <ind>&c_l;&c_o;&c_d;&c_k;&c_u;</ind></trd>,
    <trd>&c_k;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_ib; &c_w; <ind>&c_l;&c_o;&c_d;&c_k;&c_u;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><klr>(ein Boot)</klr> beladen</trd>,
    <trd>an <ind>Bord</ind> bringen</trd>
  </trdgrp>
  <trd lng="en">embark on</trd>
  <trd lng="fr">embarquer</trd>
</drv>

<drv mrk="boat.gum0o">
  <kap>gum<tld/>o</kap>
  <snc mrk="boat.gum0o.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      <ref tip="super" cel="boat.0o.SHI"><tld lit="B"/>o</ref>
      kun &scirc;elo el gumo ordinare &scirc;veligebla kaj
      mal&scirc;veligebla:
      <ekz>
        speciala formacio <klr>[&#8230;]</klr> kunportis anka&ubreve;
        blov&scirc;veligeblan gum<tld/>on &ccirc;i tien
        <fnt>
          <aut>J. Rejt&odblac;, trad. L. Bal&aacute;zs</aut>
          <vrk><url ref="http://www.freeweb.hu/verkoj/21.htm">
          Sinjoro Vanek en Parizo</url></vrk>
          <lok>2002</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_d;&c_z;&c_ib;&c_m;&c_a;&c_n;&c_y; <ind>&c_ch;&c_o;&c_v;&c_je;&c_n;</ind></trd>,
    <trd>&c_n;&c_a;&c_d;&c_z;&c_ib;&c_m;&c_a;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schlauchboot</trd>,
    <trd>Gummiboot</trd>
  </trdgrp>
  <trd lng="en">inflatable boat</trd>
  <trd lng="fr">canot pneumatique</trd>
  <trd lng="pl">ponton</trd>
</drv>

<drv mrk="boat.motor0o">
  <kap>motor<tld/>o</kap>
  <snc mrk="boat.motor0o.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      <ref tip="super" cel="boat.0o.SHI"><tld lit="B"/>o</ref> pelata
      de motoro:
      <ekz>
        la&ubreve;&scirc;ajne, vi e&ubreve;ropanoj estas sa&gcirc;aj,
        tre sa&gcirc;aj <klr>[&#8230;]</klr>: vi
        inventis &ccirc;aspafilon, motor<tld/>on, kuir- kaj
        kudroma&scirc;inojn
        <fnt><bib>&Ccirc;uk&ccirc;oj</bib> <lok>la trimova
        &scirc;akproblemo, 4, La fatala
        sa&gcirc;eco</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#26426;&#33337; <pr>j&imacron;chu&aacute;n</pr></trd>,
    <trd>&#39340;&#36948;&#33337; <pr>m&#462;d&aacute;chu&aacute;n</pr></trd>,
    <trd>&#25705;&#25176;&#33351; <pr>m&oacute;tu&omacron;t&#464;ng</pr></trd>,
    <trd>&#27773;&#33351; <pr>q&igrave;t&#464;ng</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_t;&c_o;&c_r;&c_n;&c_y; <ind>&c_ch;&c_o;&c_v;&c_je;&c_n;</ind></trd>,
    <trd>&c_m;&c_a;&c_t;&c_o;&c_r;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">motorov&yacute; &ccaron;lun</trd>
  <trd lng="de">Motorboot</trd>
  <trd lng="en">motorboat</trd>
  <trdgrp lng="fr">
    <trd>bateau &agrave; moteur</trd>,
    <trd>canot &agrave; moteur</trd>
  </trdgrp>
  <trd lng="ja">&#12514;&#12540;&#12479;&#12540;&#12508;&#12540;&#12488;
  <pr>&#12418;&#12540;&#12383;&#12540;&#12412;&#12540;&#12392;</pr></trd>
  <trdgrp lng="pl">
    <trd>motor&oacute;wka</trd>,
    <trd>&lstroke;&oacute;d&zacute; motorowa</trd>
  </trdgrp>
  <trd lng="sk">motorov&yacute; &ccaron;ln</trd>
  <trd lng="uk">&c_m;&c_o;&c_t;&c_o;&c_r;&c_n;&c_i;&c_j; &c_ch;&c_o;&c_v;&c_je;&c_n;</trd>
</drv>

<drv mrk="boat.pedal0o">
  <kap>pedal<tld/>o</kap>
  <snc mrk="boat.pedal0o.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      <ref tip="super" cel="boat.0o.SHI"><tld lit="B"/>o</ref>,
      kiun la veturantoj irigas per pedaloj:
      <ekz>
        oni kune kuiras, babilas kaj sportumas <klr>[&#8230;]</klr> per
        <tld/>oj, kanuoj, pedal<tld/>oj
        <fnt>
        <aut>-</aut>
        <vrk><url ref="http://www.eventoj.hu/arkivo/eve-123.htm">Printempa
        Renkonti&gcirc;o</url></vrk>
        <lok>Eventoj, 1997-08 (123)</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_a;&c_d;&c_z;&c_ja;&c_n;&c_y; <ind>&c_r;&c_o;&c_v;&c_a;&c_r;</ind></trd>
    <trd lng="zh">&#33050;&#36367;&#33337; <pr>ji&#462;ot&agrave;chu&aacute;n</pr></trd>
</snc>
  <trd lng="de">Tretboot</trd>
  <trd lng="en">paddleboat</trd>
  <trd lng="fr">p&eacute;dalo</trd>
  <trd lng="ja">&#12506;&#12480;&#12523;&#12508;&#12540;&#12488;</trd>
  <trd lng="pl">rower wodny</trd>
</drv>

<drv mrk="boat.rem0o">
  <kap>
    rem<tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="B"/>o pelata per remiloj:
      <ekz>
        proksime de la pram&scirc;ipo estis multaj rem<tld/>etoj sur la rivero
        <fnt>
          <aut>Mark Twain, trad. Dorothea kaj Hans-Georg Kaiser</aut>
          <vrk><url ref="https://verkoj.com/verkistoj/hans-georg-kaiser/twain_tom_sawyer.pdf"
              >La Aventuroj de Tom Sawyer</url></vrk>
          <lok>&Ccirc;apitro 14</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni trovis la rem<tld/>on renversita, sed &gcirc;i flosis surmare
        <fnt>
          <bib>VRA</bib>
          <lok>Eyvind Johnson</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_jo;&c_s;&c_l;&c_a;&c_v;&c_y; <ind>&c_ch;&c_o;&c_v;&c_je;&c_n;</ind></trd>,
    <trd>&c_v;&c_jo;&c_s;&c_l;&c_a;&c_v;&c_a;&c_ja; <ind>&c_sh;&c_l;&c_ju;&c_p;&c_k;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="de">Ruderboot</trd>
  <trd lng="en">rowboat</trd>
  <trd lng="fr">barque &agrave; rames</trd>
</drv>

<drv mrk="boat.sav0o">
  <kap>sav<tld/>o</kap>
  <snc mrk="boat.sav0o.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      <ref tip="super" cel="boat.0o.SHI"><tld lit="B"/>o</ref> por savo
      el akvodan&gcirc;eroj, precipe tia, kiun oni tenas sur &scirc;ipo
      por
      okazo de pereo:
      <ekz>
        elser&ccirc;u por vi la sav<tld/>on, per kiu vi povas fu&gcirc;i
        <fnt><bib>Mortul&Scirc;ip</bib> <lok>dua libro, &ccirc;apitro
        28a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#25937;&#29983;&#33337; <pr>ji&ugrave;sh&emacron;ngchu&aacute;n</pr></trd>,
    <trd>&#25937;&#29983;&#33351; <pr>ji&ugrave;sh&emacron;ngt&#464;ng</pr></trd>
  </trdgrp>
