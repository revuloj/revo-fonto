<drv mrk="boat.0i">
  <kap>
    <tld/>i,
    <var><kap><tld/>umi</kap></var>
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="boat.0i.SHI">
    <dif>
      <ref tip="super" cel="vetur.0i.TRA">Veturi<sncref/></ref> per <tld/>o:
      <ekz>
        ni vizitos la famegajn Akvofalojn de Niagaro kaj <tld/>os en la mirinda regiono
        de la Mil Insuloj
        <fnt>
          <aut>UEA</aut>
          <vrk><url ref="https://uea.org/kongresoj/2022/Dua_Bulteno"
              >107a UK, Dua Bulteno</url></vrk>
          <lok>2022</lok>
        </fnt>;
      </ekz>
      <ekz>
        turistoj <tld/>umas en la lago
        <fnt>
          <aut>R. H. Zerbetto</aut>
          <vrk><url ref="http://esperanto.china.org.cn/2021-04/19/content_77420074.htm"
              >Somera feriejo de &ccirc;inaj imperiestroj en Chengde</url></vrk>
          <lok>Esperanto.china.org.cn, 2021-04-19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_a;&c_r;&c_o;&c_zh;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol; &c_n;&c_a; <ind>&c_ch;&c_o;&c_w;&c_n;&c_je;</ind></trd>
  <trd lng="fr">faire du canot</trd>
</drv>

<drv mrk="boat.gum0o">
  <kap>gum<tld/>o</kap>
  <snc mrk="boat.gum0o.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      <ref tip="super" cel="boat.0o.SHI"><tld lit="B"/>o</ref>
      kun &scirc;elo el gumo ordinare &scirc;veligebla kaj
      mal&scirc;veligebla:
      <ekz>
        speciala formacio <klr>[&#8230;]</klr> kunportis anka&ubreve; 
        blov&scirc;veligeblan gum<tld/>on &ccirc;i tien
        <fnt>
          <aut>J. Rejt&odblac;, trad. L. Bal&aacute;zs</aut>
          <vrk><url ref="http://www.freeweb.hu/verkoj/21.htm">
          Sinjoro Vanek en Parizo</url></vrk>
          <lok>2002</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_d;&c_z;&c_ib;&c_m;&c_a;&c_n;&c_y; <ind>&c_ch;&c_o;&c_v;&c_je;&c_n;</ind></trd>,
    <trd>&c_n;&c_a;&c_d;&c_z;&c_ib;&c_m;&c_a;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="en">inflatable boat</trd>
  <trd lng="fr">canot pneumatique</trd>
</drv>

<drv mrk="boat.motor0o">
  <kap>motor<tld/>o</kap>
  <snc mrk="boat.motor0o.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      <ref tip="super" cel="boat.0o.SHI"><tld lit="B"/>o</ref> pelata
      de motoro:
      <ekz>
        la&ubreve;&scirc;ajne, vi e&ubreve;ropanoj estas sa&gcirc;aj,
        tre sa&gcirc;aj <klr>[&#8230;]</klr>: vi
        inventis &ccirc;aspafilon, motor<tld/>on, kuir- kaj
        kudroma&scirc;inojn
        <fnt><bib>&Ccirc;uk&ccirc;oj</bib> <lok>la trimova
        &scirc;akproblemo, 4, La fatala
        sa&gcirc;eco</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_t;&c_o;&c_r;&c_n;&c_y; <ind>&c_ch;&c_o;&c_v;&c_je;&c_n;</ind></trd>,
    <trd>&c_m;&c_a;&c_t;&c_o;&c_r;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">motorov&yacute; &ccaron;lun</trd>
  <trd lng="de">Motorboot</trd>
  <trd lng="en">motorboat</trd>
  <trdgrp lng="fr">
    <trd>bateau &agrave; moteur</trd>,
    <trd>canot &agrave; moteur</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>motor&oacute;wka</trd>,
    <trd>&lstroke;&oacute;d&zacute; motorowa</trd>
  </trdgrp>
  <trd lng="sk">motorov&yacute; &ccaron;ln</trd>
  <trd lng="uk">&c_m;&c_o;&c_t;&c_o;&c_r;&c_n;&c_i;&c_j; &c_ch;&c_o;&c_v;&c_je;&c_n;</trd>
</drv>

<drv mrk="boat.pedal0o">
  <kap>pedal<tld/>o</kap>
  <snc mrk="boat.pedal0o.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      <ref tip="super" cel="boat.0o.SHI"><tld lit="B"/>o</ref>,
      kiun la veturantoj irigas per pedaloj:
      <ekz>
        oni kune kuiras, babilas kaj sportumas <klr>[&#8230;]</klr> per
        <tld/>oj, kanuoj, pedal<tld/>oj
        <fnt>
        <aut>-</aut>
        <vrk><url ref="http://www.eventoj.hu/arkivo/eve-123.htm">Printempa
        Renkonti&gcirc;o</url></vrk>
        <lok>Eventoj, 1997-08 (123)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Tretboot</trd>
  <trd lng="en">paddleboat</trd>
  <trd lng="fr">p&eacute;dalo</trd>
  <trd lng="pl">rower wodny</trd>
</drv>

<drv mrk="boat.platfunda0o">
  <kap>platfunda <tld/>o</kap>
  <snc mrk="boat.platfunda0o.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      <tld lit="B"/>o kun fundo ne kurba,
      sed plata:
      <ekz>
        aliaj remis en sia stumpnaza platfunda <tld/>o
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut> <vrk>Sep 
        fratoj</vrk> <lok>1947</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_a;&c_s;&c_k;&c_a;&c_d;&c_o;&c_n;&c_n;&c_y; <ind>&c_ch;&c_o;&c_v;&c_je;&c_n;</ind></trd>
  <trd lng="en">flat-bottom boat</trd>
  <trd lng="fr">barque &agrave; fond plat</trd>
</drv>

<drv mrk="boat.sav0o">
  <kap>sav<tld/>o</kap>
  <snc mrk="boat.sav0o.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      <ref tip="super" cel="boat.0o.SHI"><tld lit="B"/>o</ref> por savo
      el akvodan&gcirc;eroj, precipe tia, kiun oni tenas sur &scirc;ipo
      por
      okazo de pereo:
      <ekz>
        elser&ccirc;u por vi la sav<tld/>on, per kiu vi povas fu&gcirc;i
        <fnt><bib>Mortul&Scirc;ip</bib> <lok>dua libro, &ccirc;apitro
        28a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_t;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_y; <ind>&c_ch;&c_o;&c_v;&c_je;&c_n;</ind></trd>,
    <trd>&c_r;&c_a;&c_t;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; <ind>&c_sh;&c_l;&c_ju;&c_p;&c_k;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="cs">z&aacute;chrann&yacute; &ccaron;lun</trd>
  <trd lng="de">Rettungsboot</trd>
  <trd lng="en">lifeboat</trd>
  <trd lng="fr">canot de sauvetage</trd>
  <trdgrp lng="pl">
    <trd>szalupa</trd>,
    <trd>&lstroke;&oacute;d&zacute; ratunkowa</trd>
  </trdgrp>
  <trd lng="sk">z&aacute;chrann&yacute; &ccaron;ln</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_ja;&c_t;&c_u;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j; &c_ch;&c_o;&c_v;&c_je;&c_n;</trd>,
    <trd>&c_r;&c_ja;&c_t;&c_u;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_a; &c_sh;&c_l;&c_ju;&c_p;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="boat.vel0o">
  <kap>vel<tld/>o</kap>
  <snc mrk="boat.vel0o.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      <ref tip="super" cel="boat.0o.SHI"><tld lit="B"/>o</ref> pelata 
      de <ref tip="vid" cel="vel.0o.SHI">veloj</ref>:
      <ekz>
        La veturo en balastkila vel<tld/>o estas do pli trankvila, pli
        agrabla
        <fnt>
          <aut>T. Ujlaky-Nagy</aut>
          <vrk>La sporta lingvo en Esperanto</vrk>
          <lok>1972</lok>
       </fnt>;
      </ekz>
      <ekz>
        Reala, granda vel<tld/>o, tabulkovre konstruita, kun remiloj 
        kaj fi&scirc;ujo kaj pentrita verde kaj blanke
        <fnt>
          <aut>T. Jansson, trad. S. Johansson</aut>
          <vrk>&Ccirc;apelo de Sor&ccirc;isto</vrk>
          <lok>[vidita en 2009]</lok>
       </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_h;&c_t;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>jachta</trd>,
    <trd>&ccaron;lun s plachtou</trd>
  </trdgrp>
  <trd lng="en">sailboat</trd>
  <trd lng="fr">canot &agrave; voile</trd>
  <trdgrp lng="pl">
    <trd>&zdot;agl&oacute;wka</trd>,
    <trd>&lstroke;&oacute;d&zacute; &zdot;aglowa</trd>,
    <trd>jacht</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>jachta</trd>,
    <trd>plachetnica</trd>,
    <trd>&ccaron;ln s plachtou</trd>
  </trdgrp>
</drv>
