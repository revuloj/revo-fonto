<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: salama1.xml,v 1.2 2022/03/26 20:43:10 revo Exp $">
    <kap>
      <rad>salamandred</rad>/oj
    </kap>

<drv mrk="salama1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref tip="malprt" cel="urodel.0oj">urodeloj</ref>
      (<trd lng="la">Salamandridae</trd>):
      <ekz>
        trituro estas akva amfibio de la familio de <tld lit="S"/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=441782">Trituro (amfibio)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="salama.0o.genro">salamandro<sncref/></ref>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_l;&c_a;&c_m;&c_a;&c_n;&c_d;&c_r;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de"><klr tip="ind">(Echte)</klr> Salamander</trd>
  <trd lng="hu">szalamandraf&eacute;l&eacute;k</trd>
  <trd lng="it">salamandridi</trd>
  <trd lng="pl">salamandrowate</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_a;&c_l;&c_a;&c_m;&c_a;&c_n;&c_d;&c_r;&c_y;<klr tip="ind">
    (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_i;&c_je; <ind>&c_s;&c_a;&c_l;&c_a;&c_m;&c_a;&c_n;&c_d;&c_r;&c_y;</ind> <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="uk">&c_s;&#x430;&#x43B;&#x430;&#x43C;&#x430;&#x43D;&#x434;&#x440;&#x43E;&#x432;&#x456;</trd>
</drv>

  </art>

