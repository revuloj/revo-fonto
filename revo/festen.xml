<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>festen</rad>/o
</kap>
<drv mrk="festen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">KUI</uzo>
    <dif>
      Luksa kaj abunda <ref tip="super" cel="mangx.0o">manĝo</ref>:
      <ekz>
        edziĝa <tld/>o;
      </ekz>
      <ekz>
        osetoj tradicie preĝas dum <ctl>preĝaj <tld/>oj</ctl>
        <fnt>
        <aut>Vjaĉeslav Slavik Ivanov</aut>,
        <vrk>La oseta lingvo</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2003a, numero 1a, p. 24a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="banked.0o">bankedo</ref>,
      <ref cel="regal">regalo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>банкет</trd>,
    <trd>баль</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Festessen</trd>,
    <trd>Festmahl</trd>,
    <trd>Bankett</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>banquet</trd>,
    <trd>feast</trd>
  </trdgrp>
  <trd lng="es">banquete</trd>
  <trd lng="fr">festin</trd>
  <trdgrp lng="hu">
    <trd>lakoma</trd>,
    <trd>dáridó</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>festijn</trd>,
    <trd>feestmaal</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>banquete</trd>,
    <trd>ágape</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>пир</trd>,
    <trd>пирушка</trd>,
    <trd>банкет</trd>
  </trdgrp>
</drv>

<drv mrk="festen.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Ĝui <tld/>on.
    </dif>
    <refgrp tip="vid">
      <ref cel="frand.0i">frandi</ref>
    </refgrp>
  </snc>
  <trd lng="be">баляваць</trd>
  <trdgrp lng="de">
    <trd>tafeln</trd>,
    <trd>schmausen</trd>,
    <trd>an einem Bankett <ind>teilnehmen</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>banquet</trd>,
    <trd>feast</trd>
  </trdgrp>
  <trd lng="fr">festoyer</trd>
  <trdgrp lng="hu">
    <trd>lakomázik</trd>,
    <trd>dáridózik</trd>
  </trdgrp>
  <trd lng="nl">tafelen</trd>
  <trd lng="ru">пировать</trd>
</drv>

<drv mrk="festen.0estro">
  <kap><tld/>estro</kap>
  <snc>
    <dif>
      Gvidanto de la servado en <tld/>o:
      <ekz>
        la <tld/>estro gustumis la akvon, nun fariĝintan vino
        <fnt><bib>NT</bib>, <lok>&Joh; 2:9</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>amphitryon</trd>,
    <trd>maître de banquet</trd>,
    <trd>maître d'hôtel</trd>
  </trdgrp>
</drv>
</art>
