<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: festen.xml,v 1.31 2023/11/09 19:13:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>festen</rad>/i
</kap>
<drv mrk="festen.0i">
  <kap><ofc>*</ofc><tld/>i </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      &Gcirc;ui <tld/>on:
      <ekz>
        kolektinte en la palacon la tutan nobelan junularon, li amuzi&gcirc;is kaj <tld/>is
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        estu malbenitaj, ke vi <tld/>as &ccirc;e luksaj tabloj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>!
      </ekz>
      <ekz>
        la Fili&scirc;toj kunvenis, por alporti grandan oferon al sia dio Dagon kaj por <tld/>i
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 16:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        certatempe vi devos bridi vian apetiton, &ccirc;ar via stomako kredeble dekutimi&gcirc;is
        <tld/>i
        <fnt>
          <bib>SkandalJozef</bib>
          <lok>Potifar, la bonkorulo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;oju kaj <tld/>u, sed malri&ccirc;ulojn subtenu
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiam kato promenas, la musoj <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dibocx.0i">dibo&ccirc;i</ref>,
      <ref cel="fest.0i">festi</ref>,
      <ref cel="frand.0i">frandi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">hodovat</trd>
  <trdgrp lng="de">
    <trd>tafeln</trd>,
    <trd>schmausen</trd>,
    <trd>an einem Bankett <ind>teilnehmen</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>banquet</trd>,
    <trd>feast</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>celebrar</trd>,
    <trd>festejar</trd>
  </trdgrp>
  <trd lng="fa">&a_d;&a_r; <ind>&a_D;&f_y;&a_A;&a_f;&a_t;</ind> &a_sh;&a_r;&f_k;&a_t; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">festoyer</trd>
  <trdgrp lng="hu">
    <trd>lakom&aacute;zik</trd>,
    <trd>d&aacute;rid&oacute;zik</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3054;&#x3061;&#x305D;&#x3046;&#x3092;&#x98DF;&#x3079;&#x308B;  <pr>&#x3054;&#x3061;&#x305D;&#x3046;&#x3092;&#x305F;&#x3079;&#x308B;</pr></trd>,
    <trd>&#x5BB4;&#x4F1A;&#x306B;&#x53C2;&#x52A0;&#x3059;&#x308B;  <pr>&#x3048;&#x3093;&#x304B;&#x3044;&#x306B;&#x3055;&#x3093;&#x304B;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x795D;&#x5BB4;&#x3092;&#x50AC;&#x3059;  <pr>&#x3057;&#x3085;&#x304F;&#x3048;&#x3093;&#x3092;&#x3082;&#x3088;&#x304A;&#x3059;</pr></trd>
  </trdgrp>
  <trd lng="nl">tafelen</trd>
  <trd lng="pt">banquetear-se</trd>
  <trd lng="ru">&c_p;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>hodova&tcaron;</trd>,
    <trd>hosti&tcaron; sa</trd>
  </trdgrp>
  <trd lng="uk">&c_b;&c_je;&c_n;&c_k;&c_je;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  <trd lng="zh">&#x9472;&#x6728;&#x677F; <pr>xi&#x101;ngm&#xF9;b&#x1CE;n</pr></trd>
</drv>

<drv mrk="festen.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">KUI</uzo>
    <dif>
      Luksa kaj abunda <ref tip="super" cel="mangx.0o">man&gcirc;o</ref>:
      <ekz>
        edzi&gcirc;a <tld/>o;
      </ekz>
      <ekz>
        <tld/>o postenteriga
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Paska <tld/>o
        <fnt>
          <bib>MkM</bib>
          <lok>26</lok>
        </fnt>;
      </ekz>
      <ekz>
        li faris al ili <tld/>on, kaj li bakis macojn, kaj ili man&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 19:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        sep tagojn <klr>[&#x2026;]</klr> da&ubreve;ris &ccirc;e ili la <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 14:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        osetoj tradicie pre&gcirc;as dum <ctl>pre&gcirc;aj <tld/>oj</ctl>
        <fnt>
          <aut>Vja&ccirc;eslav Slavik Ivanov</aut>,
          <vrk>La oseta lingvo</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2003a, numero 1a, p. 24a</lok>
        </fnt>;
      </ekz>
      <ekz>
        granda tipe balkana <tld/>o kun dancado-kantado-kriado
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        pagi sen partopreno por fremda <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o kaj &ccirc;aso kaj da &scirc;uldoj amaso
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="banked.0o">bankedo</ref>,
      <ref cel="regal">regalo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_n;&c_k;&c_je;&c_t;</trd>,
    <trd>&c_b;&c_a;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hody</trd>,
    <trd>hostina</trd>,
    <trd>kvas</trd>,
    <trd>slavnost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Festessen</trd>,
    <trd>Festmahl</trd>,
    <trd>Bankett</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>banquet</trd>,
    <trd>feast</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>banquete</trd>,
    <trd>fest&iacute;n</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_D;&f_y;&a_A;&a_f;&a_t;</trd>,
    <trd>&a_m;&a_h;&a_m;&a_A;&a_n;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">festin</trd>
  <trdgrp lng="hu">
    <trd>lakoma</trd>,
    <trd>d&aacute;rid&oacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3054;&#x3061;&#x305D;&#x3046;</trd>,
    <trd>&#x5BB4;&#x4F1A;  <pr>&#x3048;&#x3093;&#x304B;&#x3044;</pr></trd>,
    <trd>&#x795D;&#x5BB4;  <pr>&#x3057;&#x3085;&#x304F;&#x3048;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>festijn</trd>,
    <trd>feestmaal</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>banquete</trd>,
    <trd>&aacute;gape</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_i;&c_r;</trd>,
    <trd>&c_p;&c_i;&c_r;&c_u;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_b;&c_a;&c_n;&c_k;&c_je;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">hostina</trd>
  <trd lng="tr">&scedil;&ouml;len</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_je;&c_n;&c_k;&c_je;&c_t;</trd>,
    <trd>&c_b;&c_a;&c_n;&c_k;&c_je;&c_t;</trd>,
    <trd>&c_s;&c_v;&c_ja;&c_t;&c_o;</trd>,
    <trd>&c_t;&c_o;&c_r;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7B73;&#x5E2D; <pr>t&#xED;ngx&#xED;</pr></trd>,
    <trd>&#x9152;&#x5E2D; <pr>ji&#x1D4;x&#xED;</pr></trd>,
    <trd>&#x5BB4;&#x4F1A; <pr>y&#xE0;nhu&#xEC;</pr></trd>,
    <trd>&#x76DB;&#x5BB4; <pr>sh&#xE8;ngy&#xE0;n</pr></trd>,
    <trd>&#x7F8E;&#x9910; <pr>m&#x11B;ic&#x101;n</pr></trd>,
    <trd>&#x5BB4;&#x5E2D; <pr>y&#xE0;nx&#xED;</pr></trd>,
    <trd>&#x5BB4; <pr>y&#xE0;n</pr></trd>,
    <trd>&#x665A;&#x5BB4; <pr>w&#x1CE;ny&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="festen.0estro">
  <kap><tld/>estro</kap>
  <snc>
    <dif>
      Gvidanto de la servado en <tld/>o:
      <ekz>
        la <tld/>estro gustumis la akvon, nun fari&gcirc;intan vino
        <fnt><bib>NT</bib>, <lok>&Joh; 2:9</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_s;&c_p;&c_a;&c_d;&c_a;&c_r; <klr tip="amb">(&c_s;&c_mol;&c_v;&c_ja;&c_t;&c_a;, &c_b;&c_a;&c_l;&c_ju;)</klr></trd>
  <trdgrp lng="es">
    <trd>anfitri&oacute;n</trd>,
    <trd>camarero de mesa</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>amphitryon</trd>,
    <trd>ma&icirc;tre de banquet</trd>,
    <trd>ma&icirc;tre d'h&ocirc;tel</trd>
  </trdgrp>
</drv>
</art>
