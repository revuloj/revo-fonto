<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>festen</rad>/o
</kap>
<drv mrk="festen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Luksa kaj abunda man&gcirc;o: <ekz>edzi&gcirc;a <tld/>o.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="banked.0o">bankedo</ref>,
      <ref cel="regal">regalo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Festessen</trd>,
     <trd>Festmahl</trd>,
     <trd>Bankett</trd></trdgrp>
  <trd lng="fr">festin</trd>
  <trdgrp lng="nl">
    <trd>festijn</trd>,
    <trd>feestmaal</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_i;&c_r;</trd>,
    <trd>&c_p;&c_i;&c_r;&c_u;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_b;&c_a;&c_n;&c_k;&c_je;&c_t;</trd>
  </trdgrp>
</drv>
<drv mrk="festen.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      &Gcirc;ui <tld/>on.
    </dif>
    <refgrp tip="vid">
      <ref cel="frand.0i">frandi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>tafeln</trd>,
     <trd>schmausen</trd>,
     <trd>an einem Bankett <ind>teilnehmen</ind></trd></trdgrp>
  <trd lng="en">banquet</trd>
  <trd lng="fr">festoyer</trd>
  <trd lng="nl">tafelen</trd>
  <trd lng="ru">&c_p;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
