<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: heterodoks.xml,v 1.5 2019/10/06 20:10:30 revo Exp $">
<kap>
  <rad>heterodoks</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="heterodoks.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="heterodoks.0a.gxenerale">
    <dif>
      <ref tip="dif" cel="ortodo.mal0a.gxenerale">Malortodoksa</ref>:
      <ekz>
        en &ccirc;iu granda <tld/>a kunveno, kaj e&ccirc; en la ortodoksaj, &ccirc;iam estis doktrinaj kaj
        <esc>principiologiaj</esc> malkonsentoj, speciale kiam kunveni&gcirc;as sciencistoj kun
        religiuloj
        <fnt>
          <vrk><url ref="https://www.recantodasletras.com.br/cronicas/3850017"
              >Kronikoj Pri La Transiro</url></vrk>
          <lok>parto 12-a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Jia Pingwa</nom> &ccirc;iam estas <tld/>a en la nuna literaturo de &Ccirc;inio
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://es.chinabroadcast.cn">
          Originala Kavaliro</url></vrk>,
          <lok>&Ccirc;ina Radio Internacia, 2007-09-06</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="herez.0o.REL">herezo<sncref/></ref>
  </snc>
  <trdgrp lng="cs">
    <trd>jinov&ecaron;rn&yacute;</trd>,
    <trd>nepravov&ecaron;rn&yacute;</trd>
  </trdgrp>
  <trd lng="de">h&auml;retisch</trd>
  <trd lng="fr">h&eacute;t&eacute;rodoxe</trd>
  <trdgrp lng="pl">
    <trd>heterodoksyjny</trd>,
    <trd>nieprawowierny</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bludn&yacute;</trd>,
    <trd>blud&aacute;rsky</trd>
  </trdgrp>
</drv>
</art>

