      </ekz>
      <ekz>
	li sende<tld/>i&gcirc;e iris al la elektita celo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_o;&c_t;&c_k;&c_l;&c_o;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
