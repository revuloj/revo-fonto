    <trd>transgression</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;cart<klr tip="ind"> (par rapport &agrave; la
    r&egrave;gle)</klr></trd>,
    <trd>d&eacute;clinaison<klr tip="ind"> (&eacute;cart)</klr></trd>,
    <trd>d&eacute;viance</trd>,
    <trd>transgression</trd>,
  </trdgrp>
  <trd lng="be">&c_a;&c_d;&c_h;&c_ib;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="ja">
    <trd>&#x305D;&#x308C;&#x308B;&#x3053;&#x3068;</trd>,
    <trd>&#x9038;&#x8131;  <pr>&#x3044;&#x3064;&#x3060;&#x3064;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>deklinacja</trd>,
    <trd>odchylenie</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>declina&#x21B;ie</trd>,
    <trd>varian&#x21B;&abreve;</trd>,
    <trd>declina&#x21B;ie</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_t;&c_k;&c_l;&c_o;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="uk">&c_v;&c_ib;&c_d;&c_h;&c_i;&#x43B;&c_je;&#x43D;&#x43D;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x504F;&#x5DEE; <pr>pi&#x101;nch&#x101;</pr></trd>,
    <trd>&#x5DEE; <pr>ch&#x101;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="klin.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malproksimigi de si <tld/>ante, deturni al alia direkto:
      <ekz>
        &scirc;i for<tld/>is flanken unu el la folioj
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        for<tld/>inte la kapon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili for<tld/>as la direkton de sia vojo, iras en la dezerton, kaj malaperas
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 6:18</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="klin.for0i.FIG">
    <dif>
      Flankenigi iun de vero, justo, intenco:
      <ekz>
	for<tld/>i ies koleron, ripro&ccirc;ojn, koron;
      </ekz>
      <ekz>
        <klr>[li]</klr> for<tld/>as la rajton de fremdulo
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 27:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        observu kaj plenumu &ccirc;ion, skribitan en la libro de instruo de Moseo, ne
        for<tld/>i&gcirc;ante de &gcirc;i dekstren nek maldekstren
        <fnt>
          <bib>MT</bib>
          <lok>Josuo 23:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne forklini&gcirc;u al la vanta&jcirc;oj, kiuj ne helpas nek savas
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 12:21</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="turn.de0i">deturni</ref>,
      <ref cel="voj.de0igi">devojigi</ref>,
      <ref cel="distr.0i.deatentigi">distri<sncref/></ref>,
      <ref cel="diversi.0i">diversii</ref>,
      <ref cel="korupt.0i.morale">korupti</ref>
    </refgrp>
    <trd lng="de">ablenken</trd>
  </snc>
    <trd lng="en">turn away</trd>
  <trd lng="be">&c_a;&c_d;&c_h;&c_ib;&c_l;&c_ja;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_o;&c_t;&c_k;&c_l;&c_o;&c_n;&c_ja;&c_v;&c_a;&c_m;</trd>
  <trd lng="cs">odklonit</trd>
  <trdgrp lng="de">
    <trd>wegbiegen</trd>,
    <trd>beugen</trd>
  </trdgrp>
  <trd lng="es">desviar</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;tourner<klr tip="ind"> (de soi)</klr></trd>,
    <trd>&eacute;carter<klr tip="ind"> (de soi)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x308F;&#x304D;&#x3078;&#x5411;&#x3051;&#x308B;  <pr>&#x308F;&#x304D;&#x3078;&#x3080;&#x3051;&#x308B;</pr></trd>,
    <trd>&#x305D;&#x3089;&#x3059;</trd>,
    <trd>&#x304B;&#x308F;&#x3059;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>afweren</trd>,
    <trd>ombuigen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>odwraca&cacute;</trd>,
    <trd>odchyla&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>&icirc;ntoarce</trd>,
    <trd>inversa</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_k;&c_l;&c_o;&c_n;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_k;&c_l;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">odkloni&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_h;&c_i;&#x43B;&#x44F;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_g;&c_i;&#x43D;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_g;&#x43E;&c_r;&c_t;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="klin.ador0igxi">
  <kap>ador<tld/>i&gcirc;i</kap>
  <snc mrk="klin.ador0igxi.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      <tld lit="K"/>i&gcirc;i anta&ubreve;en, eventuale &gcirc;is tu&scirc;o de la
      grundo, elmontrante adoron:
      <ekz>
	li iros kaj servos al aliaj dioj, kaj ador<tld/>i&gcirc;os
	anta&ubreve; ili a&ubreve; anta&ubreve; la suno a&ubreve; anta&ubreve;
	la luno
	<fnt>
	  <bib>MT</bib>,
	  <lok>&Rea; 17:3</lok>
	</fnt>;
      </ekz>
      <ekz>
        David <tld/>is sian viza&gcirc;on al la tero kaj ador<tld/>i&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 24:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu dio, la&ubreve; mi, tute ne bezonas ador<tld/>i&gcirc;on de la homoj, nek de &ccirc;io ajn en
        la naturo
        <fnt>
          <bib>Monato</bib>
          <aut>Gbeglo Koffi</aut>
          <vrk><url ref="&Monato;/2012/010655.php">Dio kaj religioj</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ador.0i.religie">adori<sncref/></ref>,
      <ref cel="apoteo.0i">apoteozi</ref>,
      <ref cel="lauxd.0i">la&ubreve;di</ref>
    </refgrp>
    <trd lng="de">anbeten</trd>
  </snc>
  <trdgrp lng="en">
    <trd>bend in reverence</trd>,
    <trd>kowtow</trd>
  </trdgrp>
  <trd lng="be">&c_k;&c_l;&c_a;&c_n;&c_ja;&c_c;&c_c;&c_a; <klr>(&c_u; &c_n;&c_o;&c_g;&c_ib;, &c_d;&c_a; &c_z;&c_ja;&c_m;&c_l;&c_ib;, &c_u; &c_z;&c_n;&c_a;&c_k; &c_p;&c_a;&c_sh;&c_a;&c_n;&c_y;)</klr></trd>
  <trd lng="bg">&c_p;&c_r;&c_je;&c_k;&c_l;&c_a;&c_n;&c_ja;&c_m; &c_s;&c_je;</trd>
  <trd lng="fr">se <ind>prosterner</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x3072;&#x308C;&#x4F0F;&#x3059;  <pr>&#x3072;&#x308C;&#x3075;&#x3059;</pr></trd>,
    <trd>&#x306C;&#x304B;&#x305A;&#x304F;</trd>,
    <trd>&#x62DD;&#x793C;&#x3059;&#x308B;  <pr>&#x306F;&#x3044;&#x308C;&#x3044;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="pl">k&lstroke;ania&cacute; si&eogonek; z szacunkiem</trd>
  <trd lng="ro">arc cu respect</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43A;&#x43B;&#x43E;&#x43D;&#x44F;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&#x43F;&c_r;&c_i;&c_h;&#x43E;&c_d;&c_i;&c_t;&c_i; &c_z; &#x43F;&#x43E;&#x43A;&#x43B;&#x43E;&#x43D;&#x43E;&#x43C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x62DC; <pr>b&#xE0;i</pr></trd>,
    <trd>&#x78D5;&#x5934; <pr>k&#x113;t&#xF3;u</pr></trd>,
    <trd>&#x9813;&#x9996; <pr>d&#xF9;nsh&#x1D2;u</pr></trd>,
