<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>9</ofc><rad>aidos</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="aidos.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <uzo tip="fak">BAK</uzo>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Akirita malsufi&ccirc;o de la imunsistemo, grava <ref tip="lst"
      cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>, kiu
      malfortigas la
      korpajn defendojn kontra&ubreve; &ccirc;iaj malsanoj, plej ofte transdonata
      per
      seksaj kontaktoj a&ubreve; infektita sango:
      <!--Kolapso de la natura imuno kontra&ubreve; dan&gcirc;eraj a&ubreve;
      mortigaj malsanoj, ka&ubreve;zata de retroviruso HIV, infekta
      per sango kaj spermo, aperinta fine de la 20a jarcento:
      -->
      <ekz>
        la viruso de <tld/>o estas kapabla longe konservi&gcirc;i kaj
        reprodukti&gcirc;i en tiuj inflamaj fokusoj
        <fnt><aut>Vladimir Lemelev</aut>: 
        <vrk>Grava bazo por preventi aidoson</vrk>,
        <bib>Monato</bib>, 
        <lok>jaro 2001a, numero 5a, p.15a</lok></fnt>.
      </ekz>
    </dif>
    <rim>
      La vorto estas mallongigo de la termino
      &leftquot;Akirita ImunDeficita Sindromo&rightquot;.
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_y;&c_n;&c_d;&c_r;&c_o;&c_m;
    &c_n;&c_a;&c_b;&c_y;&c_t;&c_a;&c_g;&c_a;
    &c_ib;&c_m;&c_u;&c_n;&c_a;&c_d;&c_e;&c_f;&c_ib;&c_c;&c_y;&c_t;&c_u;
    </trd>,
    <trd>&c_S;&c_N;&c_Ib;&c_D;</trd>
  </trdgrp>
  <trd lng="bg">&c_S;&c_P;&c_I;&c_N;</trd>
  <trd lng="ca">sida</trd>
  <trd lng="en">AIDS<klr> (Acquired Immune Deficiency
  Syndrome)</klr></trd>
  <trd lng="es">sida</trd>
  <trd lng="de">AIDS</trd>
  <trdgrp lng="fr">
    <trd>sida</trd>
  </trdgrp>
  <trd lng="hu">AIDS</trd>
  <trd lng="id">AIDS</trd>
  <trd lng="nl">aids</trd>
  <trdgrp lng="pl">
    <trd>AIDS<klr> (zesp&oacute;&lstroke; nabytej niewydolno&sacute;ci
    systemu odporno&sacute;ciowego)</klr></trd>,
    <trd>adidas<klr> (pot.)</klr></trd>,
    <trd>HIV<klr> (pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>AIDS</trd>,
    <trd>SIDA</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_S;&c_P;&c_I;&c_D;</trd>,
    <trd>&c_s;&c_i;&c_n;&c_d;&c_r;&c_o;&c_m;
    &c_p;&c_r;&c_i;&c_o;&c_b;&c_r;&c_je;&c_t;&c_jo;&c_n;&c_n;&c_o;&c_g;&c_o;
    &c_i;&c_m;&c_m;&c_u;&c_n;&c_o;&c_d;&c_je;&c_f;&c_i;&c_c;&c_i;&c_t;&c_a;
    </trd>
  </trdgrp>
  <trd lng="sv">aids</trd>
</drv>

<drv mrk="aidos.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Malsanulo je <tld/>o.
    </dif>
  </snc>
  <trd lng="bg">&c_b;&c_o;&c_l;&c_je;&c_n; &c_o;&c_t;
  &c_S;&c_P;&c_I;&c_N;</trd>
  <trd lng="ca">sid&oacute;s</trd>
  <trd lng="be">&c_h;&c_v;&c_o;&c_r;&c_y; &c_n;&c_a;
  <ind>&c_S;&c_N;&c_Ib;&c_D;</ind></trd>
  <trd lng="en">AIDS patient</trd>
  <trd lng="es">sidoso</trd>
  <trdgrp lng="fr">
    <trd>sid&eacute;en<klr> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>AIDS infizierter Mensch</trd>
    <trd>AIDS positiver Mensch</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>AIDS-es</trd>,
    <trd>AIDS-beteg</trd>
  </trdgrp>
  <trd lng="id">ODHA</trd>
  <trd lng="nl">aidspati&euml;nt</trd>
  <trdgrp lng="pl">
    <trd>chory na <ind>AIDS</ind></trd>,
    <trd>chory na <ind>HIV</ind></trd>,
    <trd>nosiciel <ind>AIDS</ind></trd>,
    <trd>nosiciel <ind>HIV</ind></trd>,
    <trd>seropozytywny<klr> (pot.)</klr></trd>,
    <trd>adidas<klr> (pot.)</klr></trd>,
    <trd>HIV<klr> (pot.)</klr></trd>
  </trdgrp>
  <trd lng="pt">aid&eacute;tico</trd>
  <trd lng="ru">
  &c_b;&c_o;&c_l;&c_mol;&c_n;&c_o;&c_j;
  &c_S;&c_P;&c_I;&c_D;&c_o;&c_m;</trd>
  <trd lng="sv"><ind>aids-sjuk</ind> person</trd>
</drv>
</art>
