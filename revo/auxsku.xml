<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>a&ubreve;skult</rad>/i
</kap>
<drv mrk="auxsku.0i">
   <kap><ofc>*</ofc><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc num="1">
     <dif>
       Atenti por a&ubreve;di:
       <ekz>
         kiu bone <tld/>as bone lernas;
       </ekz>
       <ekz>
         ne <tld/>inte ne kondamnu
         <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
       </ekz>
       <ekz>
         <tld/>i sub la pordo, trans la muro<fnt>Z</fnt>;
       </ekz>
       <ekz>
         kiu <tld/>as, kie li ne devas, tiu a&ubreve;das,
         kion li ne revas
         <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>.
       </ekz>
     </dif>
   </snc>
   <snc num="2">
     <uzo tip="klr">(f)</uzo>
     <dif>
       Obei, sekvi:
       <ekz>
         li ne volis <tld/>i miajn konsilojn.
       </ekz>
     </dif>
   </snc>
   <trd lng="cs">poslouchat</trd>
   <trd lng="de">zuh&ouml;ren</trd>
   <trd lng="en">listen</trd>
   <trd lng="fr">&eacute;couter</trd>
   <trd lng="nl">luisteren</trd>
   <trd lng="ru">&#1089;&#1083;&#1091;&#1096;&#1072;&#1090;&#1100;</trd>
   <trd lng="tr">dinlemek</trd>
</drv>
<drv mrk="auxsku.brust0i">
   <kap>(brust)<tld/>i</kap>
   <snc>
     <dif>
       Kuraciste esplori la internajn bruojn de
       la pulmoj k de la koro.
     </dif>
   </snc>
   <trd lng="fr">ausculter</trd>
   <trd lng="nl">ausculteren</trd>
   <trdgrp lng="ru">
     <trd>&#1072;&#1091;&#1089;&#1082;&#1091;&#1083;&#1100;&#1090;&#1080;&#1088;&#1086;&#1074;&#1072;&#1090;&#1100;</trd>,
     <trd>&#1074;&#1099;&#1089;&#1083;&#1091;&#1096;&#1080;&#1074;&#1072;&#1090;&#1100;</trd>
   </trdgrp>
</drv>
<drv mrk="auxsku.sub0i">
   <kap>sub<tld/>i</kap>
   <snc>
     <dif>
       Ka&scirc;e <tld/>i.
     </dif>
   </snc>
   <trd lng="fr">espionner (conversation)</trd>
   <trd lng="nl">afluisteren</trd>
   <trd 
lng="ru">&#1087;&#1086;&#1076;&#1089;&#1083;&#1091;&#1096;&#1080;&#1074;&#1072;&#1090;&#1100;</trd>
</drv>
</art>
