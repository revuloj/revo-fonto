<drv mrk="auxsku.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="auxsku.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="atent.0i.sinturni">Atenti</ref> por a&ubreve;di:
      <ekz>
        <tld/>i la predikon
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la patrino metis la okulvitrojn kaj legis vo&ccirc;e el granda libro, amba&ubreve; knabinoj
        <tld/>is
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino kaj Rozulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#x2026;]</klr> povosciis rakonti tiom multe da fabeloj kaj historioj, ke
        estis plezuro lin <tld/>i
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> faris unu pa&scirc;on anta&ubreve;en, <tld/>is, faris denove unu pa&scirc;on
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne <tld/>inte ne kondamnu<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        en la palaco estis ordinara afero <tld/>i &ccirc;e la pordoj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu trans muro <tld/>as, tiun la muro insultas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu <tld/>as, kie li ne devas, tiu a&ubreve;das,
        kion li ne revas<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> ka&scirc;e a&ubreve;skultas la radion
        <fnt>
          <bib>Monato</bib>
          <aut>Nina Kor&jcirc;enevskaja-Gouriou</aut>
          <vrk><url ref="&Monato;/2006/008819.php">Kial lundo ekas sabate?</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>hallgat</trd>,
      <trd>figyel</trd>
    </trdgrp>
  </snc>
  <snc mrk="auxsku.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="atent.0i.gravigi">Atenti</ref> parolon por
      <ref tip="dif" cel="sekv.0i.FIG">sekvi</ref>:
      <ekz>
        li ne volis <tld/>i miajn konsilojn;
      </ekz>
      <ekz>
        li malmoligis sian koron kaj ne <tld/>is ilin
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 8:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi diris al vi, sed vi ne <tld/>is kaj vi malobeis la vortojn
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 1:43</lok>
        </fnt>;
      </ekz>
      <ekz>
        miaj &scirc;afoj <tld/>as mian vo&ccirc;on, kaj mi ilin konas, kaj ili min sekvas
        <fnt>
          <bib>NT</bib>
          <lok>Johano 10:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi ne <tld/>as la le&gcirc;on
        <fnt>
          <bib>NT</bib>
          <lok>Galatoj 4:21</lok>
        </fnt>?
      </ekz>
    </dif>
    <ref tip="ant" cel="ignor.0i">ignori</ref>
    <trdgrp lng="de">
      <trd>anh&ouml;ren</trd>,
      <trd>erh&ouml;ren</trd>
    </trdgrp>
    <trd lng="hu">hallgat<klr tip="ind"> (vkire)</klr></trd>
    <trdgrp lng="pl">
      <trd>s&lstroke;ucha&cacute;</trd>,
      <trd>pos&lstroke;ucha&cacute;</trd>
    </trdgrp>
  </snc>
