<drv mrk="auxsku.fiks0i">
  <kap>fiks<tld/>i</kap>
  <snc mrk="auxsku.fiks0i.KOMUNE">
    <dif>
      Atente <ref tip="super" cel="auxsku.0i.KOMUNE"><tld/>i</ref>,
      ne movi&gcirc;ante, ne bruante:
      <ekz>
        li eksilentis kaj fiks<tld/>is,
        trans la pordo la&ubreve;te ekridis la prizonulo
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/mondo.html">
          La mondo brilanta</url></vrk>,
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#8230;]</klr> en fajfo de malvarma vento fiks<tld/>as &mdash; &ccirc;u tio estas &scirc;an&gcirc;o
        de direkto en la &gcirc;enerala kurso a&ubreve; kontra&ubreve;submarina zigzago
        <fnt>
          <aut>J. German</aut>
          <vrk><url ref="https://esperanto.mv.ru/German/mi_responsas-12-1.html">Mi responsas pri
          &ccirc;io</url></vrk>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_s;&c_l;&c_u;&c_h;&c_o;&c_w;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_s;&c_l;&c_u;&c_h;&c_o;&c_w;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de">angespannt <ind>lauschen</ind></trd>
  <trd lng="fr">tendre l'oreille</trd>
  <trd lng="it">tendere le orecchie</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_s;&#x43B;&c_u;&c_h;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&c_v;&c_s;&#x43B;&c_u;&c_h;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&c_v;&c_s;&#x43B;&c_u;&c_h;&#x43E;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&#x43F;&c_r;&c_i;&c_s;&#x43B;&c_u;&c_h;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&#x43F;&c_r;&c_i;&c_s;&#x43B;&c_u;&c_h;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&#x43F;&c_r;&c_i;&c_s;&#x43B;&c_u;&c_h;&#x43E;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#22402;&#x542C; <pr>chu&#xED;t&#x12B;ng</pr></trd>,
    <trd>&#22402;&#x807D; <pr>chu&#xED;t&#x12B;ng</pr></trd>
