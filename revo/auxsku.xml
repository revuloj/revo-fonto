</snc>
  <snc mrk="auxsku.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="atent.0i.gravigi">Atenti</ref> parolon por
      <ref tip="dif" cel="sekv.0i.FIG">sekvi</ref>:
      <ekz>
