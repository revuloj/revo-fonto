<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>a&ubreve;skult</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="auxsku.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="auxsku.0i.KOMUNE">
    <dif>
      Atenti por a&ubreve;di:
      <ekz>
        kiu bone <tld/>as bone lernas;
      </ekz>
      <ekz>
        ne <tld/>inte ne kondamnu<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>i sub la pordo, trans la muro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kiu <tld/>as, kie li ne devas, tiu a&ubreve;das,
        kion li ne revas<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>hallgat</trd>,
      <trd>figyel</trd>
    </trdgrp>
  </snc>
  <snc mrk="auxsku.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Obei, sekvi:
      <ekz>
        li ne volis <tld/>i miajn konsilojn.
      </ekz>
    </dif>
    <trd lng="hu">hallgat<klr tip="ind"> (vkire)</klr></trd>
    <trdgrp lng="pl">
      <trd>s&lstroke;ucha&cacute;</trd>,
      <trd>pos&lstroke;ucha&cacute;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_s;&c_l;&c_u;&c_h;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">poslouchat</trd>
  <trd lng="de">zuh&ouml;ren</trd>
  <trd lng="en">listen</trd>
  <trd lng="es">escuchar</trd>
  <trd lng="fr">&eacute;couter</trd>
  <trd lng="nl">luisteren</trd>
  <trd lng="pl">s&lstroke;ucha&cacute;</trd>
  <trd lng="ru">&c_s;&c_l;&c_u;&c_sh;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">po&ccaron;&uacute;va&tcaron;</trd>
  <trd lng="tp">kute</trd>
  <trd lng="tr">dinlemek</trd>
</drv>

<drv mrk="auxsku.0ilo">
  <kap><tld/>ilo</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="auxsku.0ilo.ELE">
    <uzo tip="fak">ELE</uzo>
    <ref tip="dif" cel="auxd.0ilo">a&ubreve;dilo</ref>
    <ekz>
      mi ne plu ion scias, Niko, &#8212; sonis en la <tld/>ilo.
      &#8212; &Ccirc;u vi venos
      <fnt><bib>Jamburg</bib>,
      <lok>&ccirc;apitro 16a, p. 100a</lok></fnt>?
    </ekz>
  </snc>
  <trd lng="de">H&ouml;rer</trd>
  <trd lng="fr">&eacute;couteur</trd>
  <trd lng="pl">s&lstroke;uchawka</trd>
</drv>

<drv mrk="auxsku.brust0i">
  <kap>brust<tld/>i</kap>
  <snc mrk="auxsku.brust0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Kuraciste esplori la internajn bruojn de
      la pulmoj kaj de la koro.
    </dif>
  </snc>
  <trd lng="de">abh&ouml;ren <klr tip="ind"> (der Brust)</klr></trd>
  <trd lng="fr">ausculter</trd>
  <trd lng="hu">meghallgat<klr tip="ind"> (mellkast)</klr></trd>
  <trd lng="nl">ausculteren</trd>
  <trd lng="pl">os&lstroke;uchiwa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>
    &c_a;&c_u;&c_s;&c_k;&c_u;&c_l;&c_mol;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;
    &c_mol;
    </trd>,
    <trd>&c_v;&c_y;&c_s;&c_l;&c_u;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_mol;
    <klr tip="amb">(&c_m;&c_je;&c_d;.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="auxsku.kap0ilo">
  <kap>kap<tld/>ilo</kap>
  <snc mrk="auxsku.kap0ilo.ELE">
    <uzo tip="fak">ELE</uzo>
    <ref tip="dif" cel="auxd.kap0ilo.ELE">kapa&ubreve;dilo</ref>
      <ekz>
        krei sian propran sonmondon? Iuj faras tion &#8211; tute konkrete per 
        kap<tld/>iloj 
        <fnt>
          <aut>M. Sandelin</aut>,
          <vrk><url ref="http://mariasandelin.livejournal.com">[blogo]</url></vrk>,
          <lok>2005-12-28</lok>
        </fnt>.
      </ekz>
  </snc>
  <trd lng="fr">&eacute;couteurs</trd>
  <trdgrp lng="pl">
    <trd>s&lstroke;uchawki<klr> nag&lstroke;owne</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_u;&c_sh;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_g;&c_o;&c_l;&c_o;&c_v;&c_n;&c_o;&c_j; <ind>&c_t;&c_je;&c_l;&c_je;&c_f;&c_o;&c_n;</ind></trd>,
    <trd>&c_sh;&c_l;&c_je;&c_m;&c_o;&c_f;&c_o;&c_n;</trd>
  </trdgrp>
</drv>

<drv mrk="auxsku.sub0i">
  <kap>sub<tld/>i</kap>
  <snc mrk="auxsku.sub0i.KOMUNE">
    <dif>
      Ka&scirc;e <tld/>i:
      <ekz>
        Bob&ccirc;inskij enrigardas tra la pordo kaj sub<tld/>as
        <fnt><bib>Revizoro</bib>, <lok>akto 2a, sceno 8a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">
  &c_p;&c_a;&c_d;&c_s;&c_l;&c_u;&c_h;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;
  </trd>
  <trdgrp lng="de">
    <trd>lauschen</trd>,
    <trd>abh&ouml;ren</trd>,
    <trd>(zuf&auml;llig) <ind>mith&ouml;ren</ind></trd>
  </trdgrp>
  <trd lng="en">eavesdrop</trd>
  <trd lng="fr">espionner (conversation)</trd>
  <trd lng="hu">hallgat&oacute;zik</trd>
  <trd lng="nl">afluisteren</trd>
  <trd lng="pl">pods&lstroke;uchiwa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>
    &c_p;&c_o;&c_d;&c_s;&c_l;&c_u;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_mol;
    </trd>,
    <trd>&c_p;&c_o;&c_d;&c_s;&c_l;&c_u;&c_sh;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
