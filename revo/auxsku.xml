<drv mrk="auxsku.kap0ilo">
  <kap>kap<tld/>ilo</kap>
  <snc mrk="auxsku.kap0ilo.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      <ref tip="dif" cel="auxd.kap0ilo.ELE">Kapa&ubreve;dilo</ref>
      <ekz>
        krei sian propran sonmondon? iuj faras tion &ndash; tute konkrete
        per kap<tld/>iloj
        <fnt>
          <aut>M. Sandelin</aut>,
          <vrk><url ref="http://mariasandelin.livejournal.com">[blogo]</url></vrk>,
          <lok>2005-12-28</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam preleganto parolis angle, eble dekono metis la kap<tld/>ilon, por
        francparolanto duono uzis &gcirc;in
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2007/009191.php">Spertoj de amatora lobianto</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        je ioma distanco trovi&gcirc;is kap<tld/>iloj, per kiuj oni povas a&ubreve;skulti la poemojn
        deklamatajn de la verkistoj mem
        <fnt>
          <bib>Monato</bib>
          <aut>Jakvo Schram</aut>
          <vrk><url ref="&Monato;/2012/010809.php">Ekspozicio, kiu prenis je la gor&gcirc;o</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#32819;&#x673A; <pr>&#x11B;rj&#x12B;</pr></trd>,
    <trd>&#32819;&#x6A5F; <pr>&#x11B;rj&#x12B;</pr></trd>,
    <trd>&#32819;&#x7B52; <pr>&#x11B;rt&#x1D2;ng</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_v;&c_u;&c_sh;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_s;&c_l;&c_y;&c_h;&c_a;&c_v;&c_y; &c_t;&c_e;&c_l;&c_je;&c_f;&c_o;&c_n;</trd>
  </trdgrp>
  <trd lng="de">Kopfh&ouml;rer</trd>
  <trd lng="fr">casque<klr tip="ind"> (&agrave; &eacute;couteurs)</klr></trd>
  <trd lng="id"><baz>suara</baz>penyuara telinga</trd>
  <trd lng="it">cuffia<klr> (auricolari)</klr></trd>
  <trdgrp lng="pl">
    <trd>s&lstroke;uchawki<klr> nag&lstroke;owne</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_u;&c_sh;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_g;&c_o;&c_l;&c_o;&c_v;&c_n;&c_o;&c_j; <ind>&c_t;&c_je;&c_l;&c_je;&c_f;&c_o;&c_n;</ind></trd>,
    <trd>&c_sh;&c_l;&c_je;&c_m;&c_o;&c_f;&c_o;&c_n;</trd>
  </trdgrp>
</drv>

<drv mrk="auxsku.sub0i">
  <kap>sub<tld/>i</kap>
  <snc mrk="auxsku.sub0i.KOMUNE">
    <dif>
      Ka&scirc;e <tld/>i:
      <ekz>
        vi certe a&ubreve;dis, kion &scirc;i rakontis, &ccirc;ar <klr>[&#8230;]</klr> vi havas la kutimon
        sub<tld/>i
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Bob&ccirc;inskij</nom> enrigardas tra la pordo kaj sub<tld/>as
        <fnt><bib>Revizoro</bib>, <lok>akto 2a, sceno 8a</lok></fnt>.
      </ekz>
      <ekz>
        la&ubreve;dire la militistoj sub<tld/>is anka&ubreve; la &ccirc;efministron de la slovaka registaro
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2012/010570.php">Suba&ubreve;skulta skandalo</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        iu taksis min sufi&ccirc;e grava, almena&ubreve; politike interesa, por sub<tld/>i kaj
        registri miajn telefonmesa&gcirc;ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2013/011111.php">Saluton, ser&gcirc;ento!</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        iu instalis <ind>sub<tld/>ilon</ind> en la &ccirc;ambro, kie kunvenis la unua komisiono
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2005/008786.php">Ekzameno ekzamenata</url></vrk>
          <lok>2005</lok>
        </fnt>.
        <trdgrp lng="de">
          <trd>Abh&ouml;reinrichtung</trd>,
          <trd>Wanze <klr>(zum Abh&ouml;ren)</klr></trd>
        </trdgrp>
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#20599;&#x542C; <pr>t&#x14D;ut&#x12B;ng</pr></trd>,
    <trd>&#20599;&#x807D; <pr>t&#x14D;ut&#x12B;ng</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_s;&c_l;&c_u;&c_h;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>lauschen</trd>,
    <trd>abh&ouml;ren</trd>,
    <trd>(zuf&auml;llig) <ind>mith&ouml;ren</ind></trd>
  </trdgrp>
  <trd lng="en">eavesdrop</trd>
  <trd lng="fr">espionner (conversation)</trd>
  <trd lng="hu">hallgat&oacute;zik</trd>
  <trdgrp lng="id">
    <trd><baz>kuping</baz>menguping</trd>,
    <trd><baz>dengar</baz>mendengarkan<klr> (scr diam-diam)</klr></trd>
  </trdgrp>
  <trd lng="it">origliare</trd>
  <trd lng="nl">afluisteren</trd>
  <trd lng="no">avlytte</trd>
  <trd lng="pl">pods&lstroke;uchiwa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_s;&c_l;&c_u;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_s;&c_l;&c_u;&c_sh;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_ib;&c_d;&c_s;&#x43B;&c_u;&c_h;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_ib;&c_d;&c_s;&#x43B;&c_u;&c_h;&#x43E;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_ib;&c_d;&c_s;&#x43B;&c_u;&c_h;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
