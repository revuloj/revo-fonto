<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: prokurator.xml,v 1.4 2023/04/11 07:24:30 afido Exp $">
  <kap>
    <rad>prokurator</rad>/o
    <fnt><bib>PIV1</bib></fnt>
  </kap>

<drv mrk="prokurator.0o">
  <kap><tld/>o</kap>
  <snc mrk="prokurator.0o.romia">
    <uzo tip="fak">HIS</uzo>
    <dif>
      <klr>(en antikva Romio)</klr> <ref tip="super" cel="provin.0estro">Provincestro</ref> 
      a&ubreve; plej alta financa administranto de provinco:
      <ekz>
        Poncio Pilato, la <tld/>o de Judujo, <klr>[&#8230;]</klr> eliris en la portikon inter
        la du aloj de la palaco de Herodo la Granda
        <fnt>
          <bib>MkM</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gubern1.0estro">guberniestro</ref>,
      <ref cel="prokur.0isto">prokuristo</ref>,
      <ref cel="prokur1.0o">prokuroro</ref>
    </refgrp>
    <trd lng="be">&c_p;&c_r;&c_a;&c_k;&c_u;&c_r;&c_a;&c_t;&c_a;&c_r;</trd>
    <trd lng="de">Prokurator (r&ouml;mischer Statthalter)</trd>
    <trd lng="en">procurator</trd>
    <trdgrp lng="pl">
      <trd>gubernator rzymski</trd>,
      <trd>prefekt rzymski</trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>guvernator roman</trd>,
      <trd>prefect roman</trd>
    </trdgrp>
    <trd lng="uk">&#x43F;&c_r;&#x43E;&#x43A;&c_u;&c_r;&c_a;&c_t;&#x43E;&c_r;</trd>
  </snc>
</drv>

</art>

