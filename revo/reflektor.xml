<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: reflektor.xml,v 1.13 2024/04/13 20:15:52 revo Exp $">
<kap>
  <rad>reflektor</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="reflektor.0o">
  <kap><tld/>o</kap>
  <snc mrk="reflektor.0o.FOT">
  <uzo tip="fak">AUT</uzo>
  <uzo tip="fak">FOT</uzo>
    <dif>
      Lum&jcirc;etilo kun reflektiloj por alfokusigi la lumon:
      <ekz>
        dekoj da <tld/>oj tran&ccirc;as per bluaj radioj la
        densi&gcirc;intan krepuskon
        <fnt><bib>Metrop</bib></fnt>;
      </ekz>
      <ekz>
        ie-tie ekbrilis <tld/>oj de solecaj a&ubreve;toj
        <fnt>
        <aut>Julian Modest</aut>,
        <vrk>En la taksio</vrk>,
        <bib>LOdE</bib>,
        <lok>jaro 2001a, numero 11a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_f;&c_l;&c_je;&c_k;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_a;&c_d;&c_b;&c_ib;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>reflektor</trd>,
    <trd>zrcadlov&#xFD; dalekohled</trd>
  </trdgrp>
  <trd lng="de">Scheinwerfer</trd>
  <trdgrp lng="en">
    <trd>headlight</trd>,
    <trd>spotlight</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>feux<klr> (projecteurs, phares etc.)</klr></trd>,
    <trd>projecteur<klr> (lumi&egrave;re)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>f&eacute;nysz&oacute;r&oacute;</trd>,
    <trd>reflektor</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>faro<klr> (di automobile)</klr></trd>,
    <trd>proiettore<klr> (lampada)</klr></trd>,
    <trd>riflettore<klr> (lampada)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x30D8;&#x30C3;&#x30C9;&#x30E9;&#x30A4;&#x30C8;</trd>
  <trd lng="pl">reflektor</trd>
  <trdgrp lng="sk">
    <trd>reflektor</trd>,
    <trd>zrkadlov&#xFD; &#x10F;alekoh&#x13E;ad</trd>
  </trdgrp>
  <trd lng="uk">&c_r;&c_je;&c_f;&#x43B;&c_je;&#x43A;&c_t;&#x43E;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#x8FFD;&#x5149;&#x706F; <pr>zhu&#x12B;gu&#x101;ngd&#x113;ng</pr></trd>,
    <trd>&#x8FFD;&#x5149;&#x71C8; <pr>zhu&#x12B;gu&#x101;ngd&#x113;ng</pr></trd>,
    <trd>&#x524D;&#x706F; <pr>qi&#xE1;nd&#x113;ng</pr></trd>,
    <trd>&#x524D;&#x71C8; <pr>qi&#xE1;nd&#x113;ng</pr></trd>,
    <trd>&#x805A;&#x5149;&#x706F; <pr>j&#xF9;gu&#x101;ngd&#x113;ng</pr></trd>,
    <trd>&#x805A;&#x5149;&#x71C8; <pr>j&#xF9;gu&#x101;ngd&#x113;ng</pr></trd>
  </trdgrp>
</drv>
</art>

