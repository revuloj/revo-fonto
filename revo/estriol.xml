<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: estriol.xml,v 1.3 2023/11/07 20:34:12 revo Exp $">
<kap>
    <rad>estriol</rad>/o
</kap>
<drv mrk="estriol.0o">
  <kap><tld/>o</kap>
  <snc mrk="estriol.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      <ref tip="super" cel="estrogen.0o.FIZL">Estrogeno</ref>, kiu estas
      precipe produktita dum gravedeco:
      <ekz>
        naturaj estrogenoj estas la foliklohormonoj estradiolo, estrono
        kaj <tld/>o
        <fnt>
          <vrk>Medicinalaj Plantoj kaj Sufero de la Prostato</vrk>,
          <lok><url ref="https://scienca-revuo.info/article/download/115/94">Scienca
          Revuo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_s;&c_t;&c_r;&c_y;&c_jo;&c_l;</trd>
  <trd lng="ca">estriol</trd>
  <trd lng="cs">estriol</trd>
  <trd lng="de">&Ouml;striol</trd>
  <trdgrp lng="en">
    <trd>estriol</trd>,
    <trd>oestriol</trd>
  </trdgrp>
  <trd lng="es">estriol</trd>
  <trd lng="fr">&oelig;striol</trd>
  <trd lng="gl">estriol</trd>
  <trdgrp lng="nl">
    <trd>oestriol</trd>,
    <trd>estriol</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_s;&c_t;&c_r;&c_i;&c_o;&c_l;</trd>
  <trd lng="sv">&ouml;striol</trd>
  <trd lng="zh">&#x96CC;&#x4E09;&#x9187; <pr>c&#xED;s&#x101;nch&#xFA;n</pr></trd>
</drv>
</art>

