</kap>
<drv mrk="apr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="pork.0o">porko</ref>
      (<trd lng="la">Sus scrofa</trd>), sova&gcirc;a porko.
    </dif>
  </snc>
  <bld lok="../bld/apr.jpg">
    <tld/>oj kun <tld/>idoj
  </bld>
  <trdgrp lng="be">
    <trd>&c_v;&c_ja;&c_p;&c_r;&c_u;&c_k;</trd>, 
    <trd>&c_d;&c_z;&c_ib;&c_k;&c_ib; &c_k;&c_a;&c_b;&c_a;&c_n;</trd> 
  </trdgrp>  
  <trd lng="de">Wildschwein</trd>
  <trd lng="es">jabal&iacute;</trd>
  <trdgrp lng="fr">
    <trd>sanglier</trd>,
    <trd>laie</trd>,
    <trd>marcassin</trd>
  </trdgrp>
  <trd lng="hu">vaddiszn&oacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_p;&c_r;&c_mol;</trd>,
    <trd>&c_d;&c_i;&c_k;&c_i;&c_j;
<ind>&c_k;&c_a;&c_b;&c_a;&c_n;</ind></trd>
  </trdgrp>
</drv>
</art>
