<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: apr.xml,v 1.42 2023/10/28 08:33:08 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>apr</rad>/o
    </kap>

<drv mrk="apr.0o">
  <kap>
    <ofc>*</ofc>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="apr.0o.ZOO">
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_specioj" cel="speci.0o">Specio</ref>
      de <ref tip="super" cel="pork.0o">porko</ref>
      (<trd lng="la">Sus scrofa</trd>), sova&gcirc;a porko:
      <ekz>
        tro nutrata kapro fari&gcirc;as kiel <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        subfosas &gcirc;in arbara <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 80:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam la <tld/>o lin ekvidis, &gcirc;i kuris kun &scirc;a&ubreve;manta bu&scirc;ego
        montrante la akrajn dentojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>La kura&gcirc;a tajloreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li havis <klr>[&#x2026;]</klr> sur la dorso ha&ubreve;ton de <tld/>o
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/apr.jpg">
    <tld/>o kun <tld/>idoj
  </bld>
  <trdgrp lng="be">
    <trd>&c_v;&c_ja;&c_p;&c_r;&c_u;&c_k;</trd>, 
    <trd>&c_d;&c_z;&c_ib;&c_k;&c_ib; &c_k;&c_a;&c_b;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="bg">&c_d;&c_i;&c_v;&c_a; &c_s;&c_v;&c_i;&c_n;&c_ja;</trd>
  <trd lng="br">hoc'h-gouez</trd>
  <trd lng="ca">porc senglar</trd>
  <trd lng="de">Wildschwein</trd>
  <trd lng="en">Eurasian wild <ind>boar</ind></trd>
  <trd lng="es">jabal&iacute;</trd>
  <trd lng="fr">sanglier</trd>
  <trd lng="he">&het;&zayin;&yod;&resh; &bet;&resh;</trd>
  <trd lng="hu">vaddiszn&oacute;</trd>
  <trd lng="io">apro</trd>
  <trd lng="ja">&#x30A4;&#x30CE;&#x30B7;&#x30B7;  <pr>&#x3044;&#x306E;&#x3057;&#x3057;</pr></trd>
  <trdgrp lng="pl">
    <trd>dzik</trd>,
    <trd>dzika <ind>&sacute;winia</ind></trd>
  </trdgrp>
  <trd lng="pt">javali</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_p;&c_r;&c_mol;</trd>,
    <trd>&c_d;&c_i;&c_k;&c_i;&c_j;
    <ind>&c_k;&c_a;&c_b;&c_a;&c_n;</ind></trd>
  </trdgrp>
  <trd lng="sk">diviak</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_je;&#x43F;&c_r;</trd>,
    <trd>&c_d;&c_i;&#x43A;&c_i;&c_j; &#x43A;&c_a;&c_b;&c_a;&#x43D;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x91CE;&#x732A; <pr>y&#x11B;zh&#x16B;</pr></trd>,
    <trd>&#x91CE;&#x8C6C; <pr>y&#x11B;zh&#x16B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="apr.vir0o">
  <kap>
    vir<tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="apr.vir0o.ZOO">
    <dif>
      Virseksa <tld/>o.
    </dif>
    <trd lng="de">Keiler</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_s;&c_ju;&c_k; <klr>(&c_d;&c_z;&c_ib;&c_k;&c_ib;)</klr></trd>,
    <trd>&c_v;&c_ja;&c_p;&c_r;&c_u;&c_k;</trd>,
    <trd>&c_ja;&c_p;&c_r;&c_u;&c_k;</trd>
  </trdgrp>
  <trd lng="io">aprulo</trd>
  <trdgrp lng="pl">
    <trd>odyniec</trd>,
    <trd>knur</trd>
  </trdgrp>
  <trd lng="fr">sanglier</trd>
</drv>

<drv mrk="apr.0ino">
  <kap>
    <tld/>ino
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="apr.0ino.ZOO">
    <dif>
      Inseksa <tld/>o:
      <ekz>
         duonlaneca <tld/>ino <klr>[&#x2026;]</klr> estas mitologia besto, la
         simbolo de la urbo de Milano
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=270733">Duonlaneca
          porkino</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_mol;&c_v;&c_ib;&c_n;&c_ja; <klr>(&c_d;&c_z;&c_ib;&c_k;&c_a;&c_ja;)</klr></trd>
    <trdgrp lng="de">
      <trd>Bache</trd>,
      <trd>Wildsau</trd>
    </trdgrp>
    <trd lng="fr">laie</trd>
    <trd lng="io">aprino</trd>
    <trdgrp lng="pl">
      <trd>locha</trd>,
      <trd>maciora</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="apr.0ido">
  <kap>
    <tld/>ido
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="apr.0ido.ZOO">
    <dif>
      Ido de <tld/>o:
      <ekz>
        en la sama momento juna <tld/>ido kuris preter li
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_s;&c_ja; <klr>(&c_d;&c_z;&c_ib;&c_k;&c_a;&c_je;)</klr></trd>
    <trd lng="de">Frischlinge</trd>
    <trd lng="fr">marcassin</trd>
    <trdgrp lng="pl">
      <trd>warchlak</trd>
      <trd>prosi&eogonek; <ind>dzika</ind></trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="apr.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc mrk="apr.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Viando de <tld/>o:
      <ekz>
        elprovu anka&ubreve; la lokan stufitan <tld/>a&jcirc;on
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2012/010845.php">Saturnia: Vintra
          magio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_mol;&c_v;&c_ib;&c_n;&c_ib;&c_n;&c_a; <klr>(&c_z;&c_mol; &c_d;&c_z;&c_ib;&c_k;&c_a;&c_j; &c_s;&c_mol;&c_v;&c_ib;&c_n;&c_ib;)</klr></trd>
    <trd lng="ca">carn de porc senglar</trd>
    <trd lng="de">Wildschwein<klr>(braten)</klr></trd>
    <trd lng="pl">dziczyzna</trd>
    <trdgrp lng="zh">
    <trd>&#x91CE;&#x732A;&#x8089; <pr>y&#x11B;zh&#x16B;r&#xF2;u</pr></trd>,
    <trd>&#x91CE;&#x8C6C;&#x8089; <pr>y&#x11B;zh&#x16B;r&#xF2;u</pr></trd>
  </trdgrp>
</snc>
</drv>

<drv mrk="apr.veruk0o">
  <kap>veruk<tld/>o <fnt><bib>PIV2</bib>,
  <lok>apro</lok></fnt></kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Nerema&ccirc;anta <ref tip="super" cel="huf.par0uloj">parhufulo</ref>
      el la genro <trd lng="la">Phacochoerus</trd>, vivanta en Afriko
      sude de
      Saharo, parenca al <tld/>o, kun verukformaj
      &scirc;vela&jcirc;oj sur la muzelo:
      <ekz>
        notindaj bestospecioj estas: la nila krokodilo, la hipopotamo,
        la bunta tragelafo, 
        la veruk<tld/>o, la kobo, la &ccirc;evala hipotrago <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=485851">Nacia
          Parko Sumbuo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fakocx.0o">fako&ccirc;ero, fako&hcirc;ero</ref>
    </refgrp>
    <ref tip="vid" cel="babiru.0o">babiruso</ref>
    <bld lok="&WCU;/thumb/b/be/Facocero_com&uacute;n_(Phacochoerus_africanus)%2C_parque_nacional_de_Zambeze%2C_Zimbabue%2C_2018-07-28%2C_DD_01.jpg/640px-Facocero_com&uacute;n_(Phacochoerus_africanus)%2C_parque_nacional_de_Zambeze%2C_Zimbabue%2C_2018-07-28%2C_DD_01.jpg" prm="CC BY-SA 4.0">Komuna veruk<tld/>o (Phacochoerus africanus), Nacia Parko
      Zambezi, Zimbabvo
        <fnt>
          <aut>Diego Delso, propra verko</aut>
          <lok><url ref="&WCW;/File:Facocero_com%C3%BAn_(Phacochoerus_africanus),_parque_nacional_de_Zambeze,_Zimbabue,_2018-07-28,_DD_01.jpg">&WCW;/File:Facocero_com&uacute;n_(Phacochoerus_africanus),_parque_nacional_de_Zambeze,_Zimbabue,_2018-07-28,_DD_01.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
      <trdgrp lng="be">
      <trd>&c_b;&c_a;&c_r;&c_o;&c_d;&c_a;&c_v;&c_a;&c_ch;&c_n;&c_ib;&c_k;</trd>,
      <trd>&c_b;&c_a;&c_r;&c_o;&c_d;&c_a;&c_w;&c_n;&c_ib;&c_k;</trd>
    </trdgrp>
  <trd lng="bg">&c_b;&c_r;&c_a;&c_d;&c_a;&c_v;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_a; &c_s;&c_v;&c_i;&c_n;&c_ja;</trd>
  <trd lng="ca">facoquer</trd>
  <trd lng="de">Warzenschwein</trd>
  <trd lng="en">warthog</trd>
  <trd lng="fr">phacoch&egrave;re</trd>
  <trd lng="hu">varacskos <ind>diszn&oacute;</ind></trd>
  <trd lng="ja">&#x30A4;&#x30DC;&#x30A4;&#x30CE;&#x30B7;&#x30B7;</trd>
  <trd lng="nl">wrattenzwijn</trd>
  <trdgrp lng="pl">
    <trd>guziec</trd>,
    <trd>&sacute;winia afryka&nacute;ska</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_o;&c_r;&c_o;&c_d;&c_a;&c_v;&c_o;&c_ch;&c_n;&c_i;&c_k;</trd>
  <trd lng="uk">&c_b;&#x43E;&c_r;&#x43E;&c_d;&c_a;&c_v;&#x43E;&c_ch;&#x43D;&c_i;&#x43A;</trd>
</drv>

