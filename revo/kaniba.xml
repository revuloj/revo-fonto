<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kaniba.xml,v 1.27 2023/12/03 08:00:22 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>kanibal</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kaniba.0o">
  <kap><tld/>o</kap>
  <snc mrk="kaniba.0o.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      Voranto de sam-speciuloj.
    </dif>
    <trd lng="ca">can&iacute;bal</trd>
    <trd lng="de">Kannibale</trd>
    <trd lng="fr">cannibale<klr tip="ind"> (subst.)</klr></trd>
    <trd lng="hu">kannib&aacute;l</trd>
    <trd lng="pl">kanibal</trd>     
    <trd lng="ro">canibal</trd>
  </snc>
  <snc mrk="kaniba.0o.KOMUNE">
  <uzo tip="stl">KOMUNE</uzo>
    <ref tip="dif" cel="mangx.hom0ulo">homman&gcirc;ulo</ref>
    <trd lng="ca">antrop&ograve;fag</trd>
    <trdgrp lng="de">
      <trd>Menschenfresser</trd>
    </trdgrp>
    <trd lng="hu">emberev&odblac;</trd>
    <trdgrp lng="pl">
      <trd>kanibal</trd>,
      <trd>ludo&zdot;erca</trd>
    </trdgrp>   
    <trd lng="ro">canibal</trd>
    <trdgrp lng="zh">
    <trd>&#x5403;&#x4EBA;&#x8005; <pr>ch&#x12B;r&#xE9;nzh&#x11B;</pr></trd>,
    <trd>&#x98DF;&#x4EBA;&#x8005; <pr>sh&#xED;r&#xE9;nzh&#x11B;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_n;&c_ib;&c_b;&c_a;&c_l;</trd>, 
    <trd>&c_l;&c_ju;&c_d;&c_a;&c_je;&c_d;</trd>, 
    <trd>&c_l;&c_ju;&c_d;&c_a;&c_zh;&c_e;&c_r;</trd>, 
    <trd>&c_l;&c_ju;&c_d;&c_a;&c_zh;&c_e;&c_r;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">can&iacute;bal</trd>
  <trdgrp lng="cs">
    <trd>kanibal</trd>,
    <trd>lidojed</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kannibale</trd>
  </trdgrp>
  <trd lng="en">cannibal</trd>
  <trd lng="es">can&iacute;bal</trd>
  <trd lng="ja">&#x98DF;&#x4EBA;&#x7A2E;  <pr>&#x3057;&#x3087;&#x304F;&#x3058;&#x3093;&#x3057;&#x3085;</pr></trd>
  <trdgrp lng="nl">
    <trd>kannibaal</trd>,
    <trd>menseneter</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_n;&c_n;&c_i;&c_b;&c_a;&c_l;</trd>,
    <trd>&c_l;&c_ju;&c_d;&c_o;&c_je;&c_d;</trd>
  </trdgrp>
  <trd lng="sk">&lcaron;udo&zcaron;r&uacute;t</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_a;&#x43D;&c_ib;&c_b;&c_a;&#x43B;</trd>,
    <trd>&#x43B;&#x44E;&c_d;&#x43E;&c_zh;&c_je;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="kaniba.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="kaniba.0ismo.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      Man&gcirc;ado de samspeciuloj:
      <ekz>
        pro la <tld/>ismo de la kokcinelaj larvoj
        foje inter 10% kaj 12% de la ovoj estas
        neniigitaj
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Kokcinelo">
          kokcinelo</url>, 2011-11-07</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_n;&c_ib;&c_b;&c_a;&c_l;&c_ib;&c_z;&c_m;</trd>,
    <trd>&c_l;&c_ju;&c_d;&c_a;&c_zh;&c_e;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kanibalizmus</trd>,
    <trd>kanibalstv&iacute;</trd>,
    <trd>lidojedstv&iacute;</trd>
  </trdgrp>
  <trd lng="fr">cannibalisme</trd>
  <trdgrp lng="ja">
    <trd>&#x4EBA;&#x98DF;&#x3044;  <pr>&#x3072;&#x3068;&#x304F;&#x3044;</pr></trd>,
    <trd>&#x6B8B;&#x5FCD;&#x306A;&#x884C;&#x70BA;  <pr>&#x3056;&#x3093;&#x306B;&#x3093;&#x306A;&#x3053;&#x3046;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="pl">kanibalizm</trd>     
  <trd lng="ro">canibalism</trd>
  <trd lng="sk">kanibalizmus</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_a;&#x43D;&c_ib;&c_b;&c_a;&#x43B;&c_ib;&c_z;&#x43C;</trd>,
    <trd>&#x43B;&#x44E;&c_d;&#x43E;&c_zh;&c_je;&c_r;&c_s;&c_t;&c_v;&#x43E;</trd>
  </trdgrp>
  <trd lng="zh">&#x98DF;&#x4EBA;&#x4FD7; <pr>sh&#xED;r&#xE9;ns&#xFA;</pr></trd>
</drv>
</art>
