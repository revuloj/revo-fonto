<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kanibal</rad>/o
</kap>

<drv mrk="kaniba.0o">
  <kap><tld/>o</kap>
  <snc mrk="kaniba.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Voranto de sam-speciuloj.
    </dif>
    <trd lng="ca">can&iacute;bal</trd>
    <trdgrp lng="de">
      <trd>Kannibale</trd>
    </trdgrp>
    <trd lng="hu">kannib&aacute;l</trd>
  </snc>
  <snc mrk="kaniba.0o.KOMUNE">
  <uzo tip="stl">KOMUNE</uzo>
    <ref tip="dif" cel="mangx.hom0ulo">homman&gcirc;ulo</ref>
    <trd lng="ca">antrop&ograve;fag</trd>
    <trdgrp lng="de">
      <trd>Menschenfresser</trd>
    </trdgrp>
    <trd lng="hu">emberev&odblac;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_n;&c_ib;&c_b;&c_a;&c_l;</trd>, 
    <trd>&c_l;&c_ju;&c_d;&c_a;&c_je;&c_d;</trd>, 
    <trd>&c_l;&c_ju;&c_d;&c_a;&c_zh;&c_e;&c_r;</trd>, 
    <trd>&c_l;&c_ju;&c_d;&c_a;&c_zh;&c_e;&c_r;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">can&iacute;bal</trd>
  <trdgrp lng="de">
    <trd>Kannibale</trd>
  </trdgrp>
  <trd lng="en">cannibal</trd>
  <trd lng="es">can&iacute;bal</trd>
  <trdgrp lng="fr">
    <trd>cannibale<klr tip="ind"> (subst.)</klr></trd>,
    <trd>anthopophage<klr tip="ind"> (subst.)</klr></trd>,
    <trd>mangeur d'hommes</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kannibaal</trd>,
    <trd>menseneter</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_n;&c_n;&c_i;&c_b;&c_a;&c_l;</trd>,
    <trd>&c_l;&c_ju;&c_d;&c_o;&c_je;&c_d;</trd>
  </trdgrp>
</drv>
</art>
