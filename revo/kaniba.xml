<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kaniba.xml,v 1.23 2022/05/01 08:09:23 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>kanibal</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kaniba.0o">
  <kap><tld/>o</kap>
  <snc mrk="kaniba.0o.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      Voranto de sam-speciuloj.
    </dif>
    <trd lng="ca">can&iacute;bal</trd>
    <trd lng="de">Kannibale</trd>
    <trd lng="fr">cannibale<klr tip="ind"> (subst.)</klr></trd>
    <trd lng="hu">kannib&aacute;l</trd>
    <trd lng="pl">kanibal</trd>     
    <trd lng="ro">canibal</trd>
  </snc>
  <snc mrk="kaniba.0o.KOMUNE">
  <uzo tip="stl">KOMUNE</uzo>
    <ref tip="dif" cel="mangx.hom0ulo">homman&gcirc;ulo</ref>
    <trd lng="ca">antrop&ograve;fag</trd>
    <trdgrp lng="de">
      <trd>Menschenfresser</trd>
    </trdgrp>
    <trd lng="hu">emberev&odblac;</trd>
    <trdgrp lng="pl">
      <trd>kanibal</trd>,
      <trd>ludo&zdot;erca</trd>
    </trdgrp>   
    <trd lng="ro">canibal</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_n;&c_ib;&c_b;&c_a;&c_l;</trd>, 
    <trd>&c_l;&c_ju;&c_d;&c_a;&c_je;&c_d;</trd>, 
    <trd>&c_l;&c_ju;&c_d;&c_a;&c_zh;&c_e;&c_r;</trd>, 
    <trd>&c_l;&c_ju;&c_d;&c_a;&c_zh;&c_e;&c_r;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">can&iacute;bal</trd>
  <trdgrp lng="cs">
    <trd>kanibal</trd>,
    <trd>lidojed</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kannibale</trd>
  </trdgrp>
  <trd lng="en">cannibal</trd>
  <trd lng="es">can&iacute;bal</trd>
  <trdgrp lng="nl">
    <trd>kannibaal</trd>,
    <trd>menseneter</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_n;&c_n;&c_i;&c_b;&c_a;&c_l;</trd>,
    <trd>&c_l;&c_ju;&c_d;&c_o;&c_je;&c_d;</trd>
  </trdgrp>
  <trd lng="sk">&lcaron;udo&zcaron;r&uacute;t</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x430;&#x43D;&#x456;&#x431;&#x430;&#x43B;</trd>,
    <trd>&#x43B;&#x44E;&#x434;&#x43E;&#x436;&#x435;&#x440;</trd>
  </trdgrp>
</drv>

<drv mrk="kaniba.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="kaniba.0ismo.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      Man&gcirc;ado de samspeciuloj:
      <ekz>
        pro la <tld/>ismo de la kokcinelaj larvoj
        foje inter 10% kaj 12% de la ovoj estas
        neniigitaj
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Kokcinelo">
          kokcinelo</url>, 2011-11-07</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>kanibalizmus</trd>,
    <trd>kanibalstv&iacute;</trd>,
    <trd>lidojedstv&iacute;</trd>
  </trdgrp>
  <trd lng="fr">cannibalisme</trd>
  <trd lng="pl">kanibalizm</trd>     
  <trd lng="ro">canibalism</trd>
  <trd lng="sk">kanibalizmus</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x430;&#x43D;&#x456;&#x431;&#x430;&#x43B;&#x456;&#x437;&#x43C;</trd>,
    <trd>&#x43B;&#x44E;&#x434;&#x43E;&#x436;&#x435;&#x440;&#x441;&#x442;&#x432;&#x43E;</trd>
  </trdgrp>
</drv>
</art>
