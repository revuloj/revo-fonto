<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: leuxkom.xml,v 1.8 2023/11/28 01:36:37 revo Exp $">
<kap>
  <rad>le&ubreve;kom</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="leuxkom.0o">
  <kap><tld/>o</kap>
  <snc mrk="leuxkom.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Makulo sur la okula korneo, denaska a&ubreve; restanta post
      vundo a&ubreve; brulumo:
      <ekz>
        Pelton havis Vulpo rufan /
        Kaj la korpon ne tre grasan, /
        Unu okulon <tld/>an, /
        Karakteron tre malbonan
        <fnt>
          <aut>J. Brzechwa, trad. D. Kowalska</aut>,
          <vrk><url ref="http://www.brzechwaesperante.strefa.pl/vulpo.html">
          Friponecoj de Vulpo Vitaliso</url></vrk>,
          <lok>2011[?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ja;&c_l;&c_mol;&c_m;&c_o;</trd>,
    <trd>&c_l;&c_je;&c_w;&c_k;&c_o;&c_m;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Leukom</trd>
  <trdgrp lng="en">
    <trd>leukoma</trd>,
    <trd>corneal leukoma</trd>, <trd>walleye</trd>
  </trdgrp>
  <trd lng="es">leucoma</trd>
  <trdgrp lng="fr">
    <trd>leucome</trd>,
    <trd>taie<klr> (leucome)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bielmo</trd>,
    <trd>katarakta</trd>,
    <trd>leukoma</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>endosperm</trd>,
    <trd>leukom</trd>,
    <trd>cataract&abreve;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43B;&c_je;&c_j;&#x43A;&#x43E;&#x43C;&c_a;</trd>,
    <trd>&c_b;&c_ib;&#x43B;&#x44C;&#x43C;&#x43E;</trd>
  </trdgrp>
</drv>
</art>

