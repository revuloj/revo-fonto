<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: emblem.xml,v 1.20 2018/12/15 17:10:14 revo Exp $">
    <kap>
      <ofc>1</ofc>
      <rad>emblem</rad>/o
    </kap>
    <drv mrk="emblem.0o">
      <kap><tld/>o</kap>
      <snc mrk="emblem.0o.simbolo">
        <dif>
          Figuro de objekto a&ubreve; estulo konvencie reprezentanta
          abstraktan ideon:
          <ekz>
            verda stelo estas <tld/>o de Esperanto;
          </ekz><ekz>
            pesilo estas <tld/>o de la justeco.
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="simbol.0o">simbolo</ref>,
          <ref cel="insign.0o">insigno</ref>.
        </refgrp>
        <refgrp tip="vid">
          <ref cel="alegor.0o">alegorio</ref>,
          <ref cel="atribu1.0o">atributo</ref>,
          <ref cel="deviz.0o">devizo</ref>.
        </refgrp>
        <trd lng="be">&c_e;&c_m;&c_b;&c_l;&c_je;&c_m;&c_a;</trd>
        <trd lng="ca">emblema</trd>
        <trdgrp lng="cs">
          <trd>embl&eacute;m</trd>,
          <trd>logo</trd>
        </trdgrp>
        <trdgrp lng="de">
          <trd>Sinnbild</trd>,
          <trd>Emblem</trd>,
          <trd>Kennzeichen</trd>,
          <trd>Zeichen</trd>,
          <trd>Wappenbild</trd></trdgrp>
        <trdgrp lng="en">
           <trd>emblem</trd>,
           <trd>logo</trd>
        </trdgrp>
        <trd lng="es">emblema</trd>
        <trd lng="fr">embl&egrave;me</trd>
        <trdgrp lng="pl">
          <trd>emblemat</trd>,
          <trd>god&lstroke;o</trd>,
          <trd>logo</trd>
        </trdgrp><trdgrp lng="pt">
          <trd>emblema</trd>,
          <trd>ins&iacute;gnia</trd>
        </trdgrp>
        <trd lng="ru">&c_e;&c_m;&c_b;&c_l;&c_je;&c_m;&c_a;</trd>
        <trdgrp lng="sk">
          <trd>logo</trd>,
          <trd>embl&eacute;m</trd>
        </trdgrp>
        <trdgrp lng="sv">
           <trd>emblem</trd>,
           <trd>tecken</trd>
        </trdgrp>
      </snc>
      <snc mrk="emblem.0o.karta">
        <uzo tip="fak">SPO</uzo>
        <dif>
          &Ccirc;iu el la serioj po 13
          <ref tip="vid" cel="kart.lud0o">ludkartoj</ref>
          signitaj per aparta <tld/>o <sncref ref="emblem.0o.simbolo"/>:
          trefo (&#9831;),
          piko (&#9824;),
          kero (&#9825;),
          karoo (&#9826;).
        </dif>
        <ref tip="super" cel="kolor.0o.ludkarta">koloro</ref>
        <trd lng="be">&c_m;&c_a;&c_s;&c_mol;&c_c;&c_mol;
        <klr>(&c_u; &c_k;&c_a;&c_r;&c_t;&c_a;&c_h;)</klr></trd>
        <trd lng="ca">coll</trd>
        <trd lng="cs">barva<klr> (v kart&aacute;ch)</klr></trd>
        <trdgrp lng="de">
          <trd>Farbe</trd>,
          <trd>Kartenfarbe</trd></trdgrp>
        <trd lng="en">suit <klr tip="ind">(of cards)</klr></trd>
        <trd lng="es">palo<klr tip="amb"> (de la baraja)</klr></trd>
        <trd lng="fr">couleur<klr tip="ind"> (tr&egrave;fle, carreau,
        etc.)</klr></trd>
        <trd lng="pl">kolor<klr> karciany</klr></trd>
        <trd lng="pt">naipe</trd>
        <trd lng="ru">&c_m;&c_a;&c_s;&c_t;&c_mol;</trd>
        <trd lng="sk">farba<klr> (v kart&aacute;ch)</klr></trd>
      </snc>
</drv>
  </art>
