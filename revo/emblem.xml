<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>1</ofc>
      <rad>emblem</rad>/o
    </kap>
    <drv mrk="emblem.0o">
      <kap><tld/>o</kap>
      <snc mrk="emblem.0o.simbolo">
        <dif>
          Figuro de objekto a&ubreve; estulo konvencie reprezentanta
          abstraktan ideon:
          <ekz>
            verda stelo estas <tld/>o de Esperanto;
          </ekz><ekz>
            pesilo estas <tld/>o de la justeco.
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="simbol.0o">simbolo</ref>,
          <ref cel="insign.0o">insigno</ref>.
        </refgrp>
        <refgrp tip="vid">
          <ref cel="alegor.0o">alegorio</ref>,
          <ref cel="atribu1.0o">atributo</ref>,
          <ref cel="deviz.0o">devizo</ref>.
        </refgrp>
        <trdgrp lng="de">
          <trd>Sinnbild</trd>,
          <trd>Emblem</trd>,
          <trd>Kennzeichen</trd>,
          <trd>Zeichen</trd>,
          <trd>Wappenbild</trd></trdgrp>
        <trd lng="fr">embl&egrave;me</trd>
        <trd lng="ru">&c_e;&c_m;&c_b;&c_l;&c_je;&c_m;&c_a;</trd>
        <trdgrp lng="sv">
           <trd>emblem</trd>,
           <trd>tecken</trd>
        </trdgrp>
      </snc>
      <snc mrk="emblem.0o.karta">
        <uzo tip="fak">SPO</uzo>
        <dif>
          &Ccirc;iu el la serioj po 13
          <ref tip="vid" cel="kart.lud0o">ludkartoj</ref>
          signitaj per aparta <tld/>o <sncref ref="emblem.0o.simbolo"/>:
          trefo (&#x2667;),
          piko (&#x2660;),
          kero (&#x2661;),
          karoo (&#x2662;).
        </dif>
        <ref tip="super" cel="kolor.0o.ludkarta">koloro</ref>
        <trdgrp lng="de">
          <trd>Farbe</trd>,
          <trd>Kartenfarbe</trd></trdgrp>
        <trd lng="fr">couleur<klr tip="ind"> (tr&egrave;fle, carreau, etc.)</klr></trd>
        <trd lng="ru">&c_m;&c_a;&c_s;&c_t;&c_mol;</trd>
      </snc>
    </drv>
  </art>
