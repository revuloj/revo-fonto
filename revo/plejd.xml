<kap>
  <rad>plejd</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="plejd.0o">
  <kap><tld/>o</kap>
  <snc mrk="plejd.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Voja&gcirc;o<ref tip="super" cel="sxal.0o">&scirc;alo</ref>:
      <ekz>
        <nom>Bob</nom> sidis sur du se&gcirc;oj, kun la apogitaj kruroj
        kovritaj de <tld/>o
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Kamira</nom> eltiris sian maldekstran brakon de sub la <tld/>o
        <fnt>
          <aut>  Julia Sigmond, Sen Rodin</aut>
          <vrk><url ref="https://books.google.de/books?id=vTGFDXzWbzgC&amp;lpg=PA317&amp;ots=evHGjUzVca&amp;pg=PA317#v=onepage"
              > Libazar' Kaj Tero</url></vrk>
          <lok> p. 317</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="skarp.0o.virina">skarpo<sncref/></ref>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_je;&c_d;</trd>
  <trdgrp lng="cs">
    <trd>pl&eacute;d</trd>,
    <trd>&scaron;&aacute;la</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Umhangtuch</trd>,
     <trd>Reisedecke</trd>,
     <trd>Plaid</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>plaid wrap</trd>,
    <trd>travel shawl</trd>
  </trdgrp>
  <trd lng="fr">plaid</trd>
  <trd lng="hu">pl&eacute;d</trd>
  <trdgrp lng="ja">
    <trd>&#12503;&#12524;&#12540;&#12489;</trd>,
    <trd>&#26053;&#34892;&#29992;&#12398;&#32937;&#25499;&#12369;  <pr>&#12426;&#12423;&#12371;&#12358;&#12424;&#12358;&#12398;&#12363;&#12383;&#12363;&#12369;</pr></trd>
  </trdgrp>
  <trd lng="nl">plaid</trd>
  <trd lng="pl">pled</trd>
  <trdgrp lng="pt">
    <trd>xalemanta</trd>,
    <trd>xale de <ind>viagem</ind></trd>
  </trdgrp>
  <trd lng="ro">pled</trd>
  <trd lng="ru">&c_p;&c_l;&c_je;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>pl&eacute;d</trd>,
    <trd>prikr&yacute;vka</trd>
  </trdgrp>
  <trd lng="uk">&c_p;&c_l;&c_je;&c_d;</trd>
