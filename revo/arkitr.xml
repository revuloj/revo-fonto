<art mrk="$Id: arkitr.xml,v 1.22 2023/10/13 16:46:48 revo Exp $">
<kap>
  <rad>arkitrav</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="arkitr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ARKI</uzo>
  <snc>
    <dif>
      Malsupra parto de <ref tip="malprt" cel="entabl.0o">entablemento</ref>:
      <ekz>
        la ionika ordo estas unu el la kvin klasikaj kolonaj ordoj, diferencoj inter la
        atika kaj malgrandazia formoj estas malsamaj kolona bazo kaj figura friso
        super la <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=286445">Ionika ordo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_h;&c_ib;&c_t;&c_r;&c_a;&c_w;</trd>
  <trd lng="bg">&c_a;&c_r;&c_h;&c_i;&c_t;&c_r;&c_a;&c_v;</trd>
  <trd lng="ca">arquitrau</trd>
  <trd lng="cs">architr&aacute;v</trd>
  <trd lng="de">Architrav</trd>
  <trd lng="en">architrave</trd>
  <trd lng="es">arquitrabe</trd>
  <trdgrp lng="fr">
    <trd>architrave</trd>,
    <trd>&eacute;pistyle</trd>
  </trdgrp>
  <trd lng="hu">architr&aacute;v</trd>
  <trd lng="it">architrave</trd>
  <trdgrp lng="ja">
    <trd>&#x30A2;&#x30FC;&#x30AD;&#x30C8;&#x30EC;&#x30FC;&#x30D6;</trd>,
    <trd>&#x53F0;&#x8F2A;  <pr>&#x3060;&#x3044;&#x308F;</pr></trd>
  </trdgrp>
  <trd lng="nl">architraaf</trd>
  <trdgrp lng="pl">
    <trd>architraw</trd>
    <trd>epistyl</trd>
    <trd>nads&lstroke;upie</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_r;&c_h;&c_i;&c_t;&c_r;&c_a;&c_v;</trd>
  <trdgrp lng="sk">
    <trd>architr&aacute;v</trd>,
    <trd>hlavn&aacute; prie&ccaron;na hrada</trd>
  </trdgrp>
  <trd lng="uk">&#x430;&#x440;&#x445;&#x456;&#x442;&#x440;&#x430;&#x432;</trd>
</drv>
