<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: arkitr.xml,v 1.23 2025/02/22 20:25:27 revo Exp $">
<kap>
  <rad>ar&hcirc;itrav</rad>/o <fnt><bib>JED</bib></fnt>,
  <var><kap><rad var="k">arkitrav</rad>/o <fnt><bib>PVEF</bib></fnt></kap></var>
</kap>
<drv mrk="arkitr.0o">
  <kap><tld/>o, <var><kap><tld var="k"/>o</kap></var></kap>
  <uzo tip="fak">ARKI</uzo>
  <snc mrk="arkitr.0o.ARKI">
    <dif>
      Malsupra parto de <ref tip="malprt" cel="entabl.0o">entablemento</ref>:
      <ekz>
        traba ar&hcirc;itravo
        <fnt>
          <aut>Jan Werner</aut>
          <vrk><url ref="https://dvd.ikso.net/faka/scienco/Terminologiaj_konsideroj.pdf"
              >Terminologiaj konsideroj</url></vrk>
          <lok>p. 107</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ionika ordo estas unu el la kvin klasikaj kolonaj ordoj, diferencoj inter la
        atika kaj malgrandazia formoj estas malsamaj kolona bazo kaj figura friso
        super la <tld var="k"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=286445">Ionika ordo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_h;&c_ib;&c_t;&c_r;&c_a;&c_w;</trd>
  <trd lng="bg">&c_a;&c_r;&c_h;&c_i;&c_t;&c_r;&c_a;&c_v;</trd>
  <trd lng="ca">arquitrau</trd>
  <trd lng="cs">architr&aacute;v</trd>
  <trdgrp lng="de">
    <trd>Architrav</trd>,
    <trd>Epistyl</trd>
  </trdgrp>
  <trd lng="en">architrave</trd>
  <trd lng="es">arquitrabe</trd>
  <trdgrp lng="fr">
    <trd>architrave</trd>,
    <trd>&eacute;pistyle</trd>
  </trdgrp>
  <trd lng="hu">architr&aacute;v</trd>
  <trd lng="it">architrave</trd>
  <trdgrp lng="ja">
    <trd>&#12450;&#12540;&#12461;&#12488;&#12524;&#12540;&#12502;</trd>,
    <trd>&#21488;&#36650;  <pr>&#12384;&#12356;&#12431;</pr></trd>
  </trdgrp>
  <trd lng="nl">architraaf</trd>
  <trdgrp lng="pl">
    <trd>architraw</trd>
    <trd>epistyl</trd>
    <trd>nads&lstroke;upie</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_r;&c_h;&c_i;&c_t;&c_r;&c_a;&c_v;</trd>
  <trdgrp lng="sk">
    <trd>architr&aacute;v</trd>,
    <trd>hlavn&aacute; prie&ccaron;na hrada</trd>
  </trdgrp>
  <trd lng="uk">&c_a;&c_r;&c_h;&c_ib;&c_t;&c_r;&c_a;&c_v;</trd>
</drv>
</art>
