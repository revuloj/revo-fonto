<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ratifik</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ratifik.0i">
  <kap><tld/>i</kap>
  <snc mrk="ratifik.0i.JUR">
    <uzo tip="fak">JUR</uzo>
    <ref tip="dif" cel="ratif.0i.JUR">ratifi</ref>
    <ekz>
      la menciitaj konvencioj estas kaj morale kaj jure devigaj por 
      &ccirc;iuj &scirc;tatoj, kiuj subskribis kaj <tld/>is ilin
      <fnt>
        <aut>I. Lapenna</aut>,
        <vrk><url ref="http://www.ivolapenna.org/historiegravaj/manifesto_100107.htm">
        Por Paco en Libereco - Por Libereco en Paco</url></vrk>,
        <lok>Horizonto, 1977:8</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_t;&c_y;&c_f;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">ratificar</trd>
  <trd lng="de">ratifizieren</trd>
  <trd lng="en">ratify</trd>
  <trd lng="es">ratificar</trd>
  <trd lng="fr">ratifier</trd>
  <trdgrp lng="hu">
    <trd>ratifik&aacute;l</trd>,
    <trd>j&oacute;v&aacute;hagy</trd>
  </trdgrp>
  <trd lng="nl">ratificeren</trd>
  <trd lng="pt">ratificar</trd>
  <trd lng="ru">&c_r;&c_a;&c_t;&c_i;&c_f;&c_i;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="ratifik.0o">
  <kap><tld/>o</kap>
  <snc mrk="ratifik.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <ref tip="dif" cel="ratif.0o.JUR">ratifo</ref>
    <ekz>
      li rapide alproprigis al si la regon kaj balda&ubreve; ricevis la 
      <tld/>on de la konsilantaro de ulemoj <klr>...</klr> de la urbo
      <fnt>
        <aut>J. Bousac, trad. M. Leon</aut>,
        <vrk><url ref="http://eo.mondediplo.com/article841.html">
        Afganio vidate de Herato</url></vrk>,
        <lok>le Monde diplomatique, 2003-12</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="fr">ratification</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>