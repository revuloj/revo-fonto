<drv mrk="signif.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="signif.sen0a.sensenca">
    <dif>
      Ne havanta kompreneblan <tld/>on, <ref tip="vid" cel="senc.sen0a">sensenca</ref>, <ref
      tip="vid" cel="absurd.0a">absurda</ref>:
      <ekz>
        la kantoj de la amkantistoj &#8208; <klr>[nun por li]</klr> nenio krom
        sen<tld/>aj
        sonoj
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>!
      </ekz>
      <ekz>
        la vortoj estas sen<tld/>aj
        <fnt>
          <bib>Monato</bib>
          <aut>Princo Henriko Oguinye</aut>
          <vrk><url ref="&Monato;/2011/010450.php">Bomboj
          kaj mortoj: la lingvo de islama radikalismo</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        la poema karaktero de la sen<tld/>a frazo i&gcirc;as pli evidenta
        <fnt>
          <aut>K.-G. Gustafsson</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/kgesperanto/318636">
          Frazoj sen signifo!</url></vrk>,
          <lok>2011-04-02</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="kompre.ne0ebla">nekomprenebla</ref>
    <trd lng="de">unverst&auml;ndlich</trd>
    <trd lng="en">meaningless</trd>
    <trd lng="fr">insens&eacute;</trd>
    <trd lng="nl">betekenisloos</trd>
    <trdgrp lng="pl">
      <trd>bezwarto&sacute;ciowy</trd>,
      <trd>b&lstroke;ahy</trd>
    </trdgrp>
  </snc>
  <snc mrk="signif.sen0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ne havanta <tld/>on, gravon a&ubreve; valoron en la koncerna situacio,
      rigardebla kiel <ref tip="vid" cel="grav.mal0a.KOMUNE">malgrava</ref>:
      <ekz>
        estas tiom da specoj de vo&ccirc;oj en la mondo, kaj nenia estas
        sen<tld/>a
        <fnt>
          <bib>NT</bib>
          <lok>I. Korintanoj 14:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la planko sidis <klr>[&#8230;]</klr> la sen<tld/>a &leftquot;mi&rightquot; de la
        persono, admirante
        sian propran grandecon
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        titolo sen mono &#8208; sen<tld/>a sono
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        gajaj sen<tld/>uloj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi notis la faktojn &scirc;ajne plej sen<tld/>ajn, &scirc;iajn plej
        malgrandajn gestojn, &scirc;iajn plej senvalorajn parolojn
        <fnt>
          <aut>H. Vallienne</aut>,
          <vrk>&Ccirc;u li?</vrk>,
          <lok>1908</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="grav.mal0a">malgrava</ref>
    <trd lng="de">unbedeutend</trd>
    <trd lng="en">insignificant</trd>
    <trdgrp lng="fr">
      <trd>d&eacute;pourvu d'importance</trd>,
      <trd>insignifiant</trd>,
      <trd>n&eacute;gligeable</trd>
    </trdgrp>
    <trd lng="es">sin sentido, sin significado</trd>
    <trdgrp lng="nl">
      <trd>onbeduidend</trd>,
      <trd>onbelangrijk</trd>
    </trdgrp>
    <trd lng="pl">bez <ind>znaczenia</ind></trd>
    <trdgrp lng="zh">
    <trd>&#x6E3A;&#x5C0F; <pr>mi&#x1CE;oxi&#x1CE;o</pr></trd>,
