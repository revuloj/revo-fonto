<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>arteri</rad>/o
</kap>
<drv mrk="arteri.0o">
  <kap><tld/>o</kap>
  <snc mrk="arteri.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Tubeto, kiu kondukas la sangon el la koro &gcirc;is &ccirc;iuj partoj de
      la korpo.
    </dif>
    <trd lng="en">artery</trd>
    <trd lng="nl">slagader</trd>
  </snc>
  <snc mrk="arteri.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Trafikvojo.
    </dif>
    <trd lng="en">traffic artery</trd>
    <trd lng="nl">verkeersader</trd>
  </snc>
  <trd lng="fr">art&egrave;re</trd>
  <trd lng="ru">&c_a;&c_r;&c_t;&c_je;&c_r;&c_i;&c_ja;</trd>
</drv>
</art>
