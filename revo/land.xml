<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>land</rad>/o
</kap>
<drv mrk="land.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Surtera difinita regiono konsiderata kiel formanta apartan tuton
      la&ubreve; geografia vidpunkto:
      <ekz>
        <tld/>o dezerta<fnt>Z</fnt>,
      </ekz>
      <ekz>
        malproksima<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la e&ubreve;ropaj <tld/>oj;
      </ekz>
      <ekz>
        ali<tld/>ulo;
      </ekz>
      <ekz>
        duk<tld/>o, princ<tld/>o;
      </ekz>
      <ekz>
        Skot<tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Land</trd>
    </trdgrp>
    <trd lng="tr">&uuml;lke</trd>
  </snc>
  <snc>
    <dif>
      Patrujo:
      <ekz>
        li sopiris al sia <tld/>o;
      </ekz>
      <ekz>
        la <tld/>o de miaj avoj;
      </ekz>
      <ekz>
        mia vila&gcirc;o restas por mi la plej kara <tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Land</trd>,
      <trd>Vaterland</trd>,
      <trd>Heimatland</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Regiono:
      <ekz>
        la &ccirc;irka&ubreve;a <tld/>o estas bela kaj ri&ccirc;a.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Umland</trd>,
      <trd>Region</trd>,
      <trd>Gebiet</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Regno:
      <ekz>
        fremda <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        Pol<tld/>o;
      </ekz>
      <ekz>
        nia <tld/>o venkos<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ministro pri la en<tld/>aj aferoj.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Staat</trd>,
      <trd>Land</trd>,
      <trd>Reich</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_a;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_j;</trd>,
    <trd>&c_z;&c_ja;&c_m;&c_l;&c_ja;</trd>
  </trdgrp>
  <trd lng="cs">zem&ecaron;</trd>
  <trd lng="el">&chi;&omega_ton;&rho;&alfa;</trd>
  <trd lng="en">land</trd>
  <trd lng="es">pa&iacute;s</trd>
  <trd lng="nl">land</trd>
  <trdgrp lng="pt">
    <trd>pa&iacute;s</trd>,
    <trd>regi&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_a;&c_n;&c_a;</trd>
  <trd lng="vo">laned</trd>
</drv>

<drv mrk="land.en0ano">
  <kap>
    (en)<tld/>ano
  </kap>
  <snc>
    <dif>
      Lo&gcirc;anto de <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ja;&c_s;&c_c;&c_o;&c_v;&c_y; &c_zh;&c_y;&c_h;&c_a;&c_r;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_je;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Einwohner</trd>,
     <trd>Bewohner</trd>
  </trdgrp>
  <trd lng="nl">bewoner</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_s;&c_t;&c_n;&c_y;&c_j; &c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="land.sam0ano">
  <kap>
    sam<tld/>ano
  </kap>
  <snc>
    <dif>
      Lo&gcirc;anto de <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_a;&c_j;&c_ch;&c_y;&c_n;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trd lng="nl">landgenoot</trd>
  <trd lng="ru">&c_s;&c_o;&c_o;&c_t;&c_je;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_i;&c_k;</trd>
</drv>

<drv mrk="land.0ido">
  <kap>
    <tld/>ido
  </kap>
  <snc>
    <dif>
      Homo, kiu lo&gcirc;as en sama <tld/>o kiel &ccirc;iam anta&ubreve;e liaj avoj.
    </dif>
    <refgrp tip="sin">
      <ref cel="indigx.0o">indi&gcirc;eno</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Eingeborener</trd>,
     <trd>Einheimischer</trd>
  </trdgrp>
  <trd lng="nl">inboorling</trd>
</drv>

<drv mrk="land.intern0o">
  <kap>
    intern<tld/>o
  </kap>
  <snc>
    <dif>
      Interna parto de <tld/>o, plej malproksime de marbordo.
    </dif>
  </snc>
  <trd lng="de">Binnenland</trd>
  <trd lng="nl">binnenland</trd>
</drv>

<drv mrk="land.ekster0o">
  <kap>
    ekster<tld/>o
  </kap>
  <snc>
    <dif>
      &Ccirc;iuj <tld/>oj krom la propra.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_m;&c_je;&c_zh;&c_zh;&c_a;</trd>
  <trd lng="de">Ausland</trd>
  <trd lng="nl">buitenland</trd>
  <trd lng="ru">&c_z;&c_a;&c_g;&c_r;&c_a;&c_n;&c_i;&c_c;&c_a;</trd>
</drv>
</art>
