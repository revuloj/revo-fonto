<drv mrk="land.0estro">
  <kap><tld/>estro</kap>
  <snc mrk="land.0estro.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Reganto, komisiata a&ubreve; memstara, de <tld/>o:
      <ekz>
        li faris festenon <klr>[&#8230;]</klr> por la potenculoj de Persujo
        kaj Medujo, por la <tld/>estroj kaj regionestroj
        <fnt><bib>MT</bib> <lok>&Est; 1:3</lok></fnt>;
      </ekz><ekz>
        la popolo staris ekstere sur la strato kaj demandadis la 
        kortegestron, kia estas la stato de ilia <tld/>estro
        <fnt><bib>Fab2</bib> <lok>La najtingalo</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="regn.0estro">regnestro</ref>,
      <ref cel="provin.0estro">provincestro</ref>,
      <ref cel="gubern1.0estro">guberniestro</ref>
    </refgrp>
    <trd
    lng="th">&#xE1C;&#xE39;&#xE49;&#xE19;&#xE33;&#xE1B;&#xE23;&#xE30;&#xE40;&#xE17;&#xE28;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_r;&c_a;&c_w;&c_n;&c_ib;&c_k; &c_r;&c_e;&c_g;&c_ib;&c_jo;&c_n;&c_a;</trd>,
    <trd>&c_g;&c_u;&c_b;&c_e;&c_r;&c_n;&c_a;&c_t;&c_a;&c_r;</trd>
