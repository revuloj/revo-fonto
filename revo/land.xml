<drv mrk="land.0ano">
  <kap><tld/>ano</kap>
  <snc mrk="land.0ano.KOMUNE">
    <dif>
      Lo&gcirc;anto de <tld/>o:
      <ekz>
        monpotencaj landoj povas facile doni &scirc;ancojn al siaj
        <tld/>anoj kaj, sekve, la sopiratajn medalojn, dum en aliaj
        mondopartoj sportistoj ne havas monon e&ccirc; por a&ccirc;eti siajn &scirc;uojn
        <fnt>
          <bib>Monato</bib>
          <aut>Karles Berga kaj Molina</aut>
          <vrk><url ref="&Monato;/2008/009483.php">&Ccirc;u
          bojkoto? (2)</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="th">&#xE1B;&#xE23;&#xE30;&#xE0A;&#xE32;&#xE01;&#xE23;
    (&#xE02;&#xE2D;&#xE07;&#xE1B;&#xE23;&#xE30;&#xE40;&#xE17;&#xE28;)</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ja;&c_s;&c_c;&c_o;&c_v;&c_y;
    &c_zh;&c_y;&c_h;&c_a;&c_r;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_je;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
