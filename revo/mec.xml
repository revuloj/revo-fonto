<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: mec.xml,v 1.5 2024/01/25 20:10:35 revo Exp $">
    <kap>
      <rad>Mec</rad>/o
    </kap>

<drv mrk="mec.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">Urbo</ref> en nordorienta Francujo,
      <frm>49&deg;07&minute;13&second;N 06&deg;10&minute;40&second;E</frm>,
      &ccirc;efurbo de A&ubreve;strazio de 561 &gcirc;is 751:
      <ekz>
        Arnulfo de <tld/>o;
      </ekz>
      <ekz>
        ekde la 6-a jarcento la episkopujo fari&gcirc;is &ccirc;efepiskopujo, kiu estris la
        episkopujojn de <tld/>o, Tulo kaj Verduno
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=455728">Elektoepiskopujo Treviro</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la franca optika telegrafia linio de <tld/>o al Majenco estis ina&ubreve;gurita la
        29-an de majo 1813
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=579724">Optika telegrafia linio de Metz al Mainz</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_M;&c_e;&c_c;</trd>
    <trd lng="de">Metz</trd>
    <trd lng="nl">Metz</trd>
    <trd lng="pl">Metz</trd>
    <trd lng="ro">Metz</trd>
  </snc>
  <trd lng="zh">&#x6885;&#x65AF; <pr>m&#xE9;is&#x12B;</pr></trd>
</drv>

  </art>

