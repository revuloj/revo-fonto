<kap>
  <rad>stilistik</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="stilis.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">BELE</uzo>
    <dif>
      Metoda konsilaro pri <ref cel="stil.0o">stilo</ref>:
      <ekz>
        dum la <tld/>o kaj la vortotrezoro de la moderna normiga araba estas distinga
        de la klasika araba, la morfologio kaj la sintakso plurestis fundamente sen&scirc;an&gcirc;e
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=336">Araba lingvo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o analizas vortelekton, sintakson, ritmon kaj aliajn stilelementojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=49970">Stilistiko</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_y;&c_l;&c_ib;&c_s;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="ca">estil&iacute;stica</trd>
  <trd lng="cs">stylistika</trd>
  <trd lng="de">Stilistik</trd>
  <trd lng="en">stylistics</trd>
  <trd lng="fa">&a_s;&a_b;&f_k;&zwnj;&a_sh;&a_n;&a_A;&a_s;&f_y;</trd>
  <trd lng="fr">stylistique</trd>
  <trd lng="hu">stilisztika</trd>
  <trd lng="ja">&#x6587;&#x4F53;&#x8AD6;  <pr>&#x3076;&#x3093;&#x305F;&#x3044;&#x308D;&#x3093;</pr></trd>
  <trd lng="nl">stilistiek</trd>
  <trd lng="ru">&c_s;&c_t;&c_i;&c_l;&c_i;&c_s;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">&scaron;tylistika</trd>
  <trd lng="uk">&#x441;&#x442;&#x438;&#x43B;&#x456;&#x441;&#x442;&#x438;&#x43A;&#x430;</trd>
