<drv mrk="perd.dis0i">
  <kap>dis<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Poiome <tld/>i a&jcirc;ojn:
      <ekz>
        tie li dis<tld/>is sian havon per dibo&ccirc;a vivado
        <fnt><bib>NT</bib>, <lok>&Luk; 15:13</lok></fnt>;
      </ekz>
      <ekz>
        ne havas sufi&ccirc;an monon por dis<tld/>i per reklamoj
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 3 (65)</lok>
        </fnt>;
      </ekz>
      <ekz>
        la valonaj politikistoj misregis, ili dis<tld/>is la ri&ccirc;a&jcirc;ojn de &ccirc;iuj
        <fnt>
          <bib>Monato</bib>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="&Monato;/2007/009338.php"
              >Separismo ne, egalrajteco jes</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="disip.0i">disipi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_g;&c_u;&c_b;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="br">
