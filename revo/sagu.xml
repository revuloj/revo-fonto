<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sagu.xml,v 1.23 2023/10/21 06:31:28 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>sagu</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sagu.0o">
  <kap><tld/>o</kap>
  <snc mrk="sagu.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Nutra <ref tip="super" cel="gri.0o">grio</ref>
      eltirita el medolo de iuj palmarboj:
      <ekz>
        en orienta Indonezio kie la indi&gcirc;enoj estas pli influitaj de la pacifik-insulanaj
        kulturoj kiel tiuj de la insuloj Nov-Gvineo kaj Timoro, la man&gcirc;oj povas esti
        centrigitaj &ccirc;irka&ubreve; aliaj fontoj de karbohidratoj kiel <tld/>o kaj/a&ubreve; greno
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=79374">Indonezia kuirarto</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tapiok.0o.KUI">tapioko</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_g;&c_a; <klr tip="amb">(&c_k;&c_r;&c_u;&c_p;&c_y;)</klr></trd>
  <trd lng="ca">sag&uacute;</trd>
  <trd lng="cs">s&aacute;go</trd>
  <trdgrp lng="de">
    <trd>Sago</trd>,
    <trd>Palmst&auml;rke</trd>
  </trdgrp>
  <trd lng="en">sago</trd>
  <trd lng="es">sag&uacute;</trd>
  <trd lng="fr">sagou</trd>
  <trdgrp lng="hu">
    <trd>sz&aacute;g&oacute;</trd>,
    <trd>sz&aacute;g&oacute;liszt</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30B5;&#x30B4;</trd>,
    <trd>&#x6C99;&#x7A40;&#x7C73; <pr>&#12373;&#12372;&#12409;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">sago</trd>
  <trd lng="pl">sago</trd>
  <trdgrp lng="pt">
   <trd>sagu</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_a;&c_g;&c_o;</trd>
  <trd lng="sk">s&aacute;go</trd>
  <trd lng="uk">&c_s;&c_a;&c_g;&#x43E;</trd>
</drv>
</art>
