<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>moskul</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="moskul.0o">
  <kap><tld/>o</kap>
  <snc mrk="moskul.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zooligaj_genroj">Genro</ref> el familio 
      <ref tip="malprt" cel="mused.0oj">musedoj</ref>
      (<trd lng="la">Ondatra</trd>), granda ron&gcirc;ulo el Norda Ameriko kun
      longa vosto, vivanta apud mar&ccirc;oj a&ubreve; malrapidaj riveroj:
      <ekz>
        dek <tld/>aj feloj
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 105a, volumo 3a, p.
        367a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_d;&c_a;&c_t;&c_r;&c_a;</trd>
  <trd lng="de">Bisamratte</trd>
  <trd lng="fr">rat musqu&eacute;</trd>
  <trd lng="nl">muskusrat</trd>
  <trdgrp lng="pt">
   <trd>rato-almiscarado</trd>,
    <trd>Ondatra zibethicus</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_n;&c_d;&c_a;&c_t;&c_r;&c_a;</trd>
</drv>
</art>

