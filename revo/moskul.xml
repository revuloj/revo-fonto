<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>moskul</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="moskul.0o">
  <kap><tld/>o</kap>
  <snc mrk="moskul.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="mused.0oj">musedoj</ref>
      (<trd lng="la">Ondrata</trd>), granda ron&gcirc;ulo el Norda Ameriko kun
      longa vosto, vivanta apud mar&ccirc;oj a&ubreve; malrapidaj riveroj:
      <ekz>
        dek <tld/>aj feloj
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 105a, volumo 3a, p.
        367a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Bisamratte</trd>
  <trd lng="fr">rat musqu&eacute;</trd>
  <trd lng="nl">muskusrat</trd>
</drv>
</art>

