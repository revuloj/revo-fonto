<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>val</rad>/o
</kap>
<drv mrk="val.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Intermonta malalta vasta&jcirc;o:
      <ekz>
        se ne&gcirc;as sur la monto, estas malvarme en la <tld/>o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_a;&c_l;&c_ib;&c_n;&c_a;</trd>,
      <trd>&c_l;&c_o;&c_g;</trd>,
      <trd>&c_l;&c_a;&c_g;&c_ch;&c_y;&c_n;&c_a;</trd>
    </trdgrp>
    <trd lng="de">Tal</trd>
    <trd lng="es">valle</trd>
    <trd lng="hu">v&ouml;lgy</trd>    
  </snc>
  <snc>
    <dif>
      Baseno de rivero.
    </dif>
    <trd lng="de">Bett</trd>
    <trd lng="hu">foly&oacute;v&ouml;lgy</trd>    
  </snc>
  <refgrp tip="vid">
    <ref cel="basen.0o.GEOG">baseno</ref>
  </refgrp>
  <trd lng="cs">&uacute;dol&iacute;</trd>
  <trd lng="el">&kappa;&omikron;&jota;&lambda;&alfa_ton;&delta;&alfa;</trd>
  <trd lng="es">cuenca</trd>
  <trdgrp lng="fr">
    <trd>val</trd>,
    <trd>vall&eacute;e</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vallei</trd>,
    <trd>dal</trd>
  </trdgrp>
  <trd lng="pt">vale</trd>
  <trd lng="ru">&c_d;&c_o;&c_l;&c_i;&c_n;&c_a;</trd>
  <trd lng="sv">dal</trd>
</drv>
</art>
