<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>val</rad>/o
</kap>
<drv mrk="val.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Intermonta malalta vasta&jcirc;o:
      <ekz>se ne&gcirc;as sur la monto, estas malvarme en la <tld/>o
<fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
    <trd lng="de">Tal</trd>
    <trd lng="es">valle</trd>
  </snc>
  <snc>
    <dif>
      Baseno de rivero.
    </dif>
    <trd lng="de">Bett</trd>
  </snc>
  <refgrp tip="vid">
    <ref cel="basen.0o.GEOG">baseno</ref>
  </refgrp>
  <trd lng="cs">&uacute;dol&iacute;</trd>
  <trd lng="es">cuenca</trd>
  <trdgrp lng="fr">
    <trd>val</trd>,
    <trd>vall&eacute;e</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vallei</trd>,
    <trd>dal</trd>
  </trdgrp>
  <trd lng="sv">dal</trd>
</drv>
</art>
