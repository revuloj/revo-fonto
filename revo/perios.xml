<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: perios.xml,v 1.17 2022/03/25 12:53:16 revo Exp $">
<kap>
   <rad>periost</rad>/o
</kap>
<drv mrk="perios.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">ANA</uzo>
   <snc>
     <dif>
       Fibreca <ref tip="super" cel="membra.0o.ANA">membrano</ref>, kovranta la ostojn:
      <ekz>
        la diafizon &ccirc;irka&ubreve;as densa konektiva histo ri&ccirc;a je nutraj angioj, la <tld/>o,
        kiu esence rolas en la dikeca kreskado dum osti&gcirc;o, kiu komenci&gcirc;as dum la 3-a
        monato de la embria vivo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=573124">Diafizo</url></lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trdgrp lng="cs">
    <trd>okostice</trd>,
    <trd>periost</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Periost</trd>,
     <trd>Knochenhaut</trd>
   </trdgrp>
  <trd lng="en">periosteum</trd>
  <trd lng="es">periostio</trd>
  <trdgrp lng="fa">
     <trd>&a_D;&a_r;&f_y;&a_ayn;</trd>,
     <trd>&f_p;&a_w;&a_sh;&a_sh; &a_A;&a_s;&a_t;&a_kh;&a_w;&a_A;&a_n;</trd>
   </trdgrp>
  <trd lng="fr">p&eacute;rioste</trd>
  <trdgrp lng="hu">
     <trd>csonth&aacute;rtya</trd>,
     <trd>periosteum</trd>
   </trdgrp>
  <trd lng="nl">beenvlies</trd>
  <trd lng="pl">okostna</trd>
  <trd lng="pt">per&iacute;&oacute;steo</trd>
  <trdgrp lng="ru">
     <trd>&c_p;&c_je;&c_r;&c_i;&c_o;&c_s;&c_t;</trd>,
     <trd>&c_n;&c_a;&c_d;&c_k;&c_o;&c_s;&c_t;&c_n;&c_i;&c_c;&c_a;</trd>
   </trdgrp>
  <trd lng="sk">okostica</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x435;&#x440;&#x456;&#x43E;&#x441;&#x442;</trd>,
    <trd>&#x43E;&#x43A;&#x456;&#x441;&#x442;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
