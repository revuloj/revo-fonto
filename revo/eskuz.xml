<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>eskuz</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="eskuz.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="eskuz.0i.ekskuzi">
    <ref tip="dif" cel="ekskuz.0i">ekskuzi</ref>
    <ekz>
      li nur nun rimarkas sian &leftquot;eraron&rightquot;, kaj kiel
      eble plej humile sin
      <tld/>is
      <fnt>
      <aut>Milo&scirc; Luka&scirc;</aut>,
      <vrk><url
      ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr55/lerteco.html">Lerteco
      sor&ccirc;on ne bezonas</url></vrk>,
      <lok>Nica Literatura Revuo, 1960-05 &gcirc;is 06, numero 5/5, p.
      177a-179a</lok>
      </fnt>
    </ekz>
    <trdgrp lng="pl">
      <trd>t&lstroke;umaczy&cacute;<klr> (kogo&sacute; z
      czego&sacute;)</klr></trd>,
      <trd>usprawiedliwia&cacute;</trd>
    </trdgrp>
    <trd lng="pt">desculpar-se</trd>
  </snc>
</drv>

<drv mrk="eskuz.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="eskuz.0o.ekskuzo">
    <ref tip="dif" cel="ekskuz.0o">ekskuzo</ref>
    <trdgrp lng="pl">
      <trd>wyt&lstroke;umaczenie</trd>,
      <trd>usprawiedliwienie</trd>
    </trdgrp>
  </snc>
</drv>
</art>
