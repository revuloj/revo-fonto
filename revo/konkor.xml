<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>konkord</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="konkor.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Kora a&ubreve; morala plenakordo; harmonio de sentoj kaj opinioj
      inter pluraj personoj:
      <ekz>kie regas <tld/>o, regas ordo<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz><tld/>o malgranda&jcirc;on kreskigas, mal<tld/>o granda&jcirc;on
        ruinigas<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        pli valoras <tld/>a ovo, ol mal<tld/>a bono<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="konsen.0o">konsento</ref>,
      <ref cel="simpat.0o">simpatio</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="en">
    <trd>concord</trd>,
    <trd>accord</trd>,
    <trd>agreement</trd>,
    <trd>harmony</trd>
  </trdgrp>
  <trd lng="nl">eendracht</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_i;&c_je;</trd>,
    <trd>&c_l;&c_a;&c_d;</trd>,
    <trd>&c_m;&c_i;&c_r;</trd>,
    <trd>&c_v;&c_z;&c_a;&c_i;&c_m;&c_o;&c_p;&c_o;&c_n;&c_i;&c_m;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
