  <trdgrp lng="fr">
    <trd>cavalier<klr tip="ind"> (partenaire de danse)</klr></trd>,
    <trd>compagnon<klr> (dans un couple)</klr></trd>,
    <trd>partenaire</trd>
  </trdgrp>
  <trd lng="pl">partner</trd>
  <trd lng="ro">partener</trd>
  <trdgrp lng="sk">
    <trd>partner</trd>,
    <trd>partnerka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_a;&c_r;&c_a; (&c_o;&c_s;&c_o;&c_b;&c_a;</trd>,
    <trd>&c_ja;&c_k;&c_a; &c_u;&c_t;&c_v;&c_o;&c_r;&c_ju;&c_jeu; &c_p;&c_a;&c_r;&c_u; &c_z; &c_k;&c_i;&c_m;&c_o;&c_s;&c_mol;)</trd>
  </trdgrp>
</drv>

