<drv mrk="par.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="par.0eco.KOMUNE">
    <dif>
      Stato de <tld/>o:
      <ekz>
        la poeto anka&ubreve; meditas pri la propra intimeco &ndash; soleco, animaj malvarmoj
        <klr>[&#x2026;]</klr>, pri amikeco kaj <tld/>eco
        <fnt>
          <bib>Monato</bib>
          <aut>Antonio Val&eacute;n</aut>
          <vrk><url ref="&Monato;/2005/008499.php">&Ccirc;u Ertl provizore stilekzercas?</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Zweisamkeit</trd>
  </snc>
  <snc>
    <uzo tip="fak">MAT</uzo>
    <dif>
      Eco de io <ref cel="par.0a.MAT"><tld/>a</ref>:
      <ekz>
        <tld/>eco de entjero estas ekvivalenta al <tld/>eco de &gcirc;ia
        lasta dekuma cifero;
      </ekz>
      <ekz>
        la <tld/>eco de <ref cel="permut.0o.bijekcio">permuto</ref>
        <klr>(identigebla kun la valoro de
        <ref cel="sign.0umo.permuto">signumo</ref> &ccirc;e &gcirc;i)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">parder</trd>
  <trdgrp lng="cs">
    <trd>parita</trd>,
    <trd>p&aacute;rovost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Parit&auml;t</trd>,
    <trd>Signatur</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>parity</trd>,
    <trd>sign<klr tip="ind"> (of a permutation)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>parit&eacute;</trd>,
    <trd>signature<klr tip="ind"> (d'une permutation)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>p&aacute;ross&aacute;g</trd>,
    <trd>parit&aacute;s</trd>
  </trdgrp>
  <trd lng="ja">&#x30D1;&#x30EA;&#x30C6;&#x30A3;</trd>
  <trdgrp lng="pl">
    <trd>parzysto&sacute;&cacute;<klr tip="ind"> (liczby)</klr></trd>,
    <trd>znak<klr tip="ind"> (permutacji)</klr></trd>,
  </trdgrp>
  <trdgrp lng="ro">
    <trd>paritate<klr tip="ind"> (numere)</klr></trd>,
    <trd>semn<klr tip="ind"> (permutare)</klr></trd>,
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_jo;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_n;&c_a;&c_k;<klr tip="ind">
    (&c_p;&c_je;&c_r;&c_je;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_k;&c_i;)</klr></trd>
