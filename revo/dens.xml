<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>dens</rad>/a
</kap>
<drv mrk="dens.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Karakterizas ion, kies eroj, tre proksimaj unuj al aliaj, estas
      multnombre enhavataj en malgranda volumeno:
      <ekz>
        <tld/>a arbaro, la&ubreve;bo, nebulo, pluvo, mallumo, solva&jcirc;o,
        hararo, trupo, popolamaso;
      </ekz>
      <ekz>
        <tld/>foliaj arboj;
      </ekz>
      <ekz>
        tro mal<tld/>a aero sufokas la homon;
      </ekz>
      <ekz>
        mal<tld/>aj spikoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e lo&gcirc;ata lando;
      </ekz>
      <ekz>
        vesto <tld/>e brodita per oro
        <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 10</lok></fnt>;
      </ekz>
      <ekz>
        la grizeco <tld/>e ar&gcirc;entis liajn harojn.
      </ekz>
    </dif>
    <trd lng="de">dicht</trd>
    <trd lng="en">dense</trd>
    <trd lng="es">denso</trd>
    <trdgrp lng="fr">
      <trd>dense</trd>,
      <trd>dru</trd>,
      <trd>serr&eacute;</trd>,
      <trd>touffu</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_g;&c_u;&c_s;&c_t;&c_o;&c_j;</trd>,
      <trd>&c_ch;&c_a;&c_s;&c_t;&c_y;&c_j;</trd>,
      <trd>&c_p;&c_l;&c_o;&c_t;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="dens.0a.subaro">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>EKV</bib><lok>&para;73</lok></fnt>
    <dif>
      <klr>(p.p. <ref cel="ar.sub0o">subaro</ref> <em>A</em>
      de <ref cel="spac.topologia0o">topologia spaco</ref>, rilate
      al subaro <em>B</em> de la sama spaco)</klr>
      Tia, ke &ccirc;iu <ref cel="punkt.0o.spaco">punkto</ref>
      en <em>B</em> estas <ref cel="adher.0a.punkto">adhera</ref>
      al <em>A</em>:
      <ekz>
        &ccirc;iu subaro estas <tld/>a rilate al si mem;
      </ekz>
      <ekz>
        <em>A</em> estas <tld/>a rilate al <em>B</em>, se kaj nur se
        la <ref cel="adher.0ajxo.MAT">adhera&jcirc;o</ref> de
        <em>A</em> inkluzivas <em>B</em>;
      </ekz>
      <ekz>
        subaron <tld/>an rilate al la tuta spaco oni nomas
        <ind>&ccirc;ie <tld/>a</ind>.
        <trd lng="de">&uuml;berall <ind>dichte</ind><klr tip="amb"> (Teilmenge)</klr></trd>
        <trd lng="en">everywhere <ind>dense</ind><klr tip="ind"> (subset)</klr></trd>
        <trd lng="fr"><klr tip="ind">(sous-ensemble) </klr>partout <ind>dense</ind></trd>
        <trd lng="pl"><klr tip="ind">(podzbi&oacute;r) </klr>g&eogonek;sty w przestrzeni</trd>
        <trd lng="ru">&c_v;&c_s;&c_ju;&c_d;&c_u; <ind>&c_p;&c_l;&c_o;&c_t;&c_n;&c_o;&c_je;</ind><klr tip="amb"> (&c_p;&c_o;&c_d;&c_m;&c_n;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
      </ekz>
    </dif>
    <rim>
      La sama termino aperas anka&ubreve; en
      <fnt><bib>MatTerm</bib><lok>p. 18</lok></fnt>, tamen nur alude.
    </rim>
    <rim>
      Notindas, ke foje oni anstata&ubreve;igas <ctl>adhera</ctl> per
      <ctl>akumuli&gcirc;a</ctl> en la difino, el kio sekvas, ke subaro
      ne nepre estas <tld/>a rilate al si.
    </rim>
    <trd lng="de">dichte<klr tip="amb"> (Teilmenge)</klr></trd>
    <trd lng="en">dense<klr tip="ind"> (subset)</klr></trd>
    <trd lng="fr"><klr tip="ind">(sous-ensemble) </klr>dense</trd>
    <trd lng="pl"><klr tip="ind">(podzbi&oacute;r) </klr>g&eogonek;sty</trd>
    <trd lng="ru">&c_p;&c_l;&c_o;&c_t;&c_n;&c_o;&c_je;<klr tip="amb"> (&c_p;&c_o;&c_d;&c_m;&c_n;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </snc>
  <trd lng="sk">hust&yacute;</trd>
</drv>

<drv mrk="dens.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Eco de tio, kio estas <tld/>a.
    </dif>
    <trd lng="ru">&c_g;&c_u;&c_s;&c_t;&c_o;&c_t;&c_a;</trd>
  </snc>
  <snc mrk="dens.0eco.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <uzo tip="klr">(de substanco)</uzo>
    <dif>
      <ref cel="dimens.0o.FIZ">Sendimensia</ref>
      <ref tip="super" cel="grand.fizika0o">fizika grando</ref>,
      esprimanta la <ref cel="rilat.0o.kvociento">rilaton</ref> de la
      <ref cel="mas.0o">maso</ref> de
      certa <ref cel="volume.0o">volumeno</ref> de la substanco al
      la maso de sama volumeno da pura akvo (je 4 &ring;C), a&ubreve;
      da aero (je 0 &ring;C kaj 1 atm), se temas pri gaso:
      <ekz>
        la <tld/>eco de hidrargo estas 13,6;
      </ekz>
      <ekz>
        la <tld/>eco de oksigeno estas 1,105.
      </ekz>
    </dif>
    <trd lng="ru">&c_p;&c_l;&c_o;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <uzo tip="fak">FIZ</uzo>
    <uzo tip="stl">ARK</uzo>
    <ref tip="dif" cel="mas.volumena0o">volumena maso.</ref>
    <trd lng="ru">&c_p;&c_l;&c_o;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="de">Dichte</trd>
  <trd lng="es">densidad</trd>
  <trd lng="fr">densit&eacute;</trd>
</drv>

<drv mrk="dens.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Loko en arbaro, kie la arboj kaj arbetoj estas <tld/>aj.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>broussaille</trd>,
    <trd>fourr&eacute;</trd>,
    <trd>endroit <ind>touffu</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_a;&c_shch;&c_a;</trd>
  <trd lng="sk">h&uacute;&scaron;tina</trd>
</drv>

<drv mrk="dens.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi <tld/>a:
      <ekz>
        <tld/>igu la vicojn!
      </ekz>
    </dif>
  </snc>
  <trd lng="de">verdichten</trd>
  <trdgrp lng="fr">
    <trd>densifier</trd>,
    <trd>resserrer</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_p;&c_l;&c_o;&c_t;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_l;&c_o;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
