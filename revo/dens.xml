<drv mrk="dens.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="dens.0igi.kundensigi">
    <dif>
      Igi <tld/>a:
      <ekz>
        <tld/>igu la vicojn!
      </ekz>
      <ekz>
        kun<tld/>igi
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 10:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni uzis kiel tradician konstrumaterialon miksitan kaj
        <tld/>igitan argilon
        <fnt>
          <bib>Monato</bib>
          <aut>Anatoli Ionesov</aut>
          <vrk><url ref="&Monato;/2006/008920.php">Perlo &ccirc;e
          la Silka Vojo</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        por malebligi tro fruan sedimenti&gcirc;on de la sablo, oni uzas kiel
        <tld/>igilon
        (dispersilon) la tiel nomatan guar-gumon
        <fnt>
          <bib>Monato</bib>
          <aut>Werner Fuss</aut>
          <vrk><url ref="&Monato;/2014/011348.php">Novtipa
          gasminado: &ccirc;u vere dan&gcirc;era?</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_u;&c_sh;&c_ch;&c_y;&c_l;&c_mol;&c_n;&c_ib;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_g;&c_u;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">verdichten</trd>
    <trdgrp lng="fr">
      <trd>densifier</trd>,
      <trd>resserrer</trd>
    </trdgrp>
    <trd lng="hu">s&udblac;r&iacute;t</trd>
    <trd lng="id"><baz>padat</baz> memadatkan</trd>
    <trd lng="nl">verdichten</trd>
    <trdgrp lng="pl">
      <trd>zag&eogonek;szcza&cacute;</trd>,
      <trd>kondensowa&cacute;</trd>,
      <trd>&sacute;cie&sacute;nia&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_u;&c_p;&c_l;&c_o;&c_t;&c_n;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_g;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_p;&c_l;&c_o;&c_t;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="dens.0igi.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Malgrandigi datumaron per rearan&gcirc;o de &gcirc;iaj bitoj:
      <ekz>
        <uzo tip="fak">KOMP</uzo>
        post el&scirc;utado oni havas sur sia komputilo <tld/>igitan
        (<esc>ZIP-itan</esc>) dosieron
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2005/008659.php">OpenOffice:
          senpaga alternativo por Microsoft Office</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Gzip</nom> estas bazita sur la datum<tld/>iga algoritmo
        &leftquot;<nac>deflate</nac>&rightquot;
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Gzip">
          Gzip</url>, 2013-03-07</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">packen <klr>(Daten)</klr></trd>
    <trd lng="fr">compresser<klr> (des donn&eacute;es)</klr></trd>
    <trd lng="id"><baz>kompresi</baz> mengompresi</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>hustit</trd>,
    <trd>zahustit</trd>,
    <trd>zahu&scaron;&tcaron;ovat</trd>
  </trdgrp>
  <trd lng="de">komprimieren</trd>
  <trd lng="he">&#x5DC;&#x5D3;&#x5D7;&#x5D5;&#x5E1;</trd>
  <trdgrp lng="ja">
    <trd>&#x6FC3;&#x304F;&#x3059;&#x308B;  <pr>&#x3053;&#x304F;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5BC6;&#x306B;&#x3059;&#x308B;  <pr>&#x307F;&#x3064;&#x306B;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x88DC;&#x529B;&#x3059;&#x308B;  <pr>&#x307B;&#x308A;&#x304D;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="sk">zahusti&tcaron; <klr>(m&uacute;kou)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_g;&c_u;&c_shch;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_g;&c_u;&c_shch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_t;&c_i;&c_s;&#x43A;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6709;&#x538B;&#x7F29;&#x529B; <pr>y&#x1D2;uy&#x101;su&#x14D;l&#xEC;</pr></trd>,
    <trd>&#x538B;&#x7F29; <pr>y&#x101;su&#x14D;</pr></trd>
  </trdgrp>
</drv>

