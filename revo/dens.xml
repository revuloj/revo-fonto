<drv mrk="dens.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="dens.0igxi.KOMUNE">
    <dif>
      Interproksimi&gcirc;i, lasante malmultan interspacon,
      kunpremi&gcirc;i:
      <ekz>
        <tld/>i&gcirc;is abismoj en la mezo de la maro
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 15:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        flava nebulo, sen&ccirc;ese <tld/>i&gcirc;anta
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ombro <klr>[&#x2026;]</klr> <tld/>i&gcirc;is minuton post minuto
        <fnt>
          <aut>G. de Maupassant, trad. N. Lagrafo</aut>,
          <vrk>Hororega</vrk>,
          <lok><url ref="http://www.elerno.cn/penseo/PEN259.pdf">
          Penseo, 2013-06 (259)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">sich <ind>verdichten</ind></trd>
  </snc>
  <trdgrp lng="fr">
    <trd>se <ind>densifier</ind></trd>,
    <trd>se <ind>resserrer</ind></trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5E2;&#x5D1;&#x5D5;&#x5EA;</trd>
