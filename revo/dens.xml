<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>dens</rad>/a
</kap>
<drv mrk="dens.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Karakterizas ion, kies eroj, tre proksimaj unuj al aliaj, estas
      multnombre enhavataj en malgranda volumeno:
      <ekz>
        <tld/>a arbaro, la&ubreve;bo, nebulo, pluvo, mallumo, solva&jcirc;o,
        hararo,  trupo, popolamaso;
      </ekz>
      <ekz>
        <tld/>foliaj arboj;
      </ekz>
      <ekz>
        tro mal<tld/>a aero sufokas la homon;
      </ekz>
      <ekz>
        mal<tld/>aj spikoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e lo&gcirc;atat lando;
      </ekz>
      <ekz>
        vesto <tld/>e brodita per oro
        <fnt>(<aut>Kabe</aut>: <vrk>La Faraono, 2a vol.</vrk>,
        <lok>&ccirc;ap. 10</lok>)</fnt>;
      </ekz>
      <ekz>
        la grizeco <tld/>e ar&gcirc;entis liajn harojn.
      </ekz>
    </dif>
    <trd lng="de">dicht</trd>
    <trd lng="en">dense</trd>
    <trdgrp lng="fr">
      <trd>dense</trd>,
      <trd>dru</trd>,
      <trd>serr&eacute;</trd>,
      <trd>touffu</trd></trdgrp> 
  </snc>

      <snc mrk="dens.0a.subaro">
        <uzo tip="fak">MAT</uzo>
        <dif>
          <klr>(p.p. <ref cel="ar.sub0o">subaro</ref> <em>A</em>
          de <ref cel="spac.topologia0o">topologia spaco</ref>, rilate
          al subaro <em>B</em> de la sama spaco)</klr>
          Tia, ke &ccirc;iu <ref cel="punkt.0o.spaco">punkto</ref>
          en <em>B</em> estas <ref cel="adher.0a.punkto">adhera</ref>
          al <em>A</em>:
          <ekz>
            &ccirc;iu subaro estas <tld/>a rilate al si mem;
          </ekz>
          <ekz>
            <em>A</em> estas <tld/>a rilate al <em>B</em>, nur se 
            la <ref cel="adher.0ajxo.MAT">adhera&jcirc;o</ref> de 
            <em>A</em> inkluzivas <em>B</em>;
          </ekz>
          <ekz>
            subaron <tld/>an rilate al la tuta spaco oni nomas
            <ind>&ccirc;ie <tld/>a</ind>.
            <trd lng="fr">partout <ind>dense</ind> (sous-ensemble)</trd>
            <trd lng="ru">
            &c_v;&c_s;&c_ju;&c_d;&c_u; <ind>&c_p;&c_l;&c_o;&c_t;&c_n;&c_o;&c_je;</ind> (&c_p;&c_o;&c_d;&c_m;&c_n;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_o;)</trd>
          </ekz>
        </dif>
        <trd lng="fr">dense (sous-ensemble)</trd>
        <trd lng="ru">&c_p;&c_l;&c_o;&c_t;&c_n;&c_o;&c_je; (&c_p;&c_o;&c_d;&c_m;&c_n;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_o;)</trd>
      </snc>
</drv>

<drv mrk="dens.0eco">
  <kap><tld/>eco</kap>
  <snc num="1">
    <dif>
      Eco de tio, kio estas <tld/>a. 
    </dif>
  </snc>
  <snc mrk="dens.0eco.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <uzo tip="klr">(p.p. substanco)</uzo>
    <dif>
      <ref cel="dimens.0o.FIZ">Sendimensia</ref> nombro, 
      <ref cel="rilat.0o.kvociento">rilato</ref> de la 
      <ref cel="mas.0o">maso</ref> de
      certa <ref cel="volume.0o">volumeno</ref> de la substanco al
      la maso de sama volumeno da pura akvo (je 4 &#xb0;C), a&ubreve;
      da aero (je 0 &#xb0;C kaj 1 atm), se temas pri gaso:
      <ekz>
        la <tld/>eco de hidrargo estas 13,6;
      </ekz>
      <ekz>
        la <tld/>eco de oksigeno estas 1,105.
      </ekz>
    </dif>
  </snc>
  <snc num="3">
    <uzo tip="fak">FIZ</uzo>
    <uzo tip="stl">ARK</uzo>
    <ref tip="sin" cel="mas.volumena0o">volumena maso.</ref>
  </snc>
  <trd lng="de">Dichte</trd>
  <trd lng="fr">densit&eacute;</trd>
</drv>

<drv mrk="dens.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Loko en arbaro, kie la arboj k arbetoj estas <tld/>aj. 
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>broussaille</trd>,
    <trd>fourr&eacute;</trd>,
    <trd>endroit <ind>touffu</ind></trd></trdgrp> 
</drv>
<drv mrk="dens.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi <tld/>a:
      <ekz>
        <tld/>igu la vicojn!
      </ekz>
    </dif>
  </snc>
  <trd lng="de">verdichten</trd>
  <trdgrp lng="fr">
    <trd>densifier</trd>,
    <trd>resserrer</trd></trdgrp> 
</drv>
</art>
