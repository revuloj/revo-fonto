<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tifon</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tifon.0o">
  <kap><tld/>o</kap>
  <snc mrk="tifon.0o.MET">
    <ref tip="dif" cel="ciklon.0o.MET">ciklono</ref>
    <ekz>
      tio estas <tld lit="T"/>o &ndash; murmuretis la pastro &ndash; nur
      la dioj povas
      savi nin, se ili volos
      <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 19a</lok></fnt>.
    </ekz>
    <trd lng="pl">tyfon</trd>
    <trdgrp lng="pt">
      <trd>ciclone</trd>
    </trdgrp>
  </snc>
</drv>
</art>

