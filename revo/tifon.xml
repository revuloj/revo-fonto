<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tifon</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tifon.0o">
  <kap><tld/>o</kap>
  <snc mrk="tifon.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
    <ref tip="sin" cel="tife.0o.MIT">Tifeo</ref>. La&ubreve; la helena mito,
    monstra esta&jcirc;o iel rilata al ventegoj kaj vulkanoj.
    </dif>
    <trd lng="en">Typhon</trd>
    <trd lng="it">Tifone<klr> (mit.)</klr></trd>
   </snc>
  <snc mrk="tifon.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
    <ref tip="sin" cel="tajfun.0o">tajfuno</ref> 
    <ekz>
      tio estas <tld lit="T"/>o &ndash; murmuretis la pastro &ndash; nur
      la dioj povas
      savi nin, se ili volos
      <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 19a</lok></fnt>.
    </ekz>
    </dif>
    <trd lng="en">typhoon</trd>
    <trd lng="it">tifone<klr> (met.)</klr></trd>
    <trd lng="pl">tyfon</trd>
    <trdgrp lng="pt">
      <trd>ciclone</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">tajfun</trd>
  <trdgrp lng="sk">
    <trd>horúci vietor</trd>,
    <trd>tyfón</trd>
  </trdgrp>
</drv>
</art>

