<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tifon.xml,v 1.11 2023/06/21 14:29:58 revo Exp $">
<kap>
  <rad>tifon</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tifon.0o">
  <kap><tld/>o</kap>
  <snc mrk="tifon.0o.MET">
    <uzo tip="fak">MET</uzo>
    <ref tip="dif" cel="tajfun.0o">tajfuno</ref> 
    <trd lng="de">Taifun</trd>
    <trd lng="en">typhoon</trd>
    <trd lng="fr">typhon</trd>
    <trd lng="it">tifone<klr> (met.)</klr></trd>
    <trd lng="pl">tyfon</trd>
    <trdgrp lng="pt">
      <trd>ciclone</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_j;&c_f;&c_u;&c_n;</trd>
  <trd lng="cs">tajfun</trd>
  <trdgrp lng="sk">
    <trd>hor&uacute;ci vietor</trd>,
    <trd>tyf&oacute;n</trd>
  </trdgrp>
</drv>

<drv mrk="tifon.T0o">
  <kap><tld lit="T"/>o,
    <var><kap>Tifaono</kap></var>
  </kap>
  <snc mrk="tifon.T0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      <ref tip="dif" cel="tife.0o.MIT">Tifeo</ref>:
      <ekz>
        <tld lit="T"/>o ekposedis vin
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio estas <tld lit="T"/>o &ndash; murmuretis la pastro &ndash; nur
        la dioj povas savi nin, se ili volos
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 19a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_T;&c_y;&c_f;&c_o;&c_n;</trd>
    <trd lng="de">Typhon</trd>
    <trd lng="en">Typhon</trd>
    <trd lng="fr">Typhon</trd>
    <trd lng="it">Tifone<klr> (mit.)</klr></trd>
  </snc>
  <trd lng="uk">&c_t;&c_a;&c_j;&c_f;&c_u;&#x43D;</trd>
</drv>
</art>

