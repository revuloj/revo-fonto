<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>sortiment</rad>/o
</kap>
<drv mrk="sortim.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Aro da diversaj samfakaj elmontrotaj komerca&jcirc;oj; specimenaro.
    </dif>
    <trd lng="en">selection</trd>
    <trdgrp lng="pt">
      <trd>sortimento</trd>,
      <trd>estoque</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Aro da diversaj samspecaj a&ubreve; samcelaj objektoj.
    </dif>
  <trdgrp lng="pt">
    <trd>sortimento</trd>,
    <trd>estoque</trd>
  </trdgrp>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_a;&c_r;&c_t;&c_y;&c_m;&c_e;&c_n;&c_t;</trd>
  <trd lng="de">Sortiment</trd>
  <trdgrp lng="en">
    <trd>assortment</trd>,
    <trd>collection</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>v&aacute;laszt&eacute;k</trd>,
    <trd>szortiment</trd>
  </trdgrp>
  <trd lng="nl">assortiment</trd>
  <trdgrp lng="pt">
    <trd>sortimento</trd>,
    <trd>estoque</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_s;&c_s;&c_o;&c_r;&c_t;&c_i;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_n;&c_a;&c_b;&c_o;&c_r;</trd>,
    <trd>&c_k;&c_o;&c_m;&c_p;&c_l;&c_je;&c_k;&c_t;</trd>
  </trdgrp>
  <trd lng="sv">sortiment</trd>
</drv>

<drv mrk="sortim.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ta&ubreve;ge elekti kaj kunmeti komerca&jcirc;ojn inter si diferencajn, sed
      &ccirc;iujn
      de sama speco.
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>&ouml;sszev&aacute;logat</trd>,
    <trd>&ouml;ssze&aacute;ll&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">assorteren</trd>
  <trd lng="pt">fazer <ind>sortimento</ind> de</trd>
  <trd lng="en">stock</trd>
</drv>
</art>
