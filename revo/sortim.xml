<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>sortiment</rad>/o
</kap>
<drv mrk="sortim.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Aro da diversaj samfakaj elmontrotaj komerca&jcirc;oj; specimenaro.
    </dif>
  </snc>
  <snc>
    <dif>
      Aro da diversaj samspecaj a&ubreve; samcelaj objektoj.
    </dif>
  </snc>
  <trd lng="de">Sortiment</trd>
  <trd lng="nl">assortiment</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_s;&c_s;&c_o;&c_r;&c_t;&c_i;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_n;&c_a;&c_b;&c_o;&c_r;</trd>,
    <trd>&c_k;&c_o;&c_m;&c_p;&c_l;&c_je;&c_k;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="sortim.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ta&ubreve;ge elekti kaj kunmeti komerca&jcirc;ojn inter si diferencajn, sed &ccirc;iujn
      de sama speco.
    </dif>
  </snc>
  <trd lng="nl">assorteren</trd>
</drv>
</art>
