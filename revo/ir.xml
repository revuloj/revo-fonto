        sur<tld/>i tegmenton, &scirc;tuparon, arbon, monton
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="grimp.0i">grimpi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>betreten</trd>,
    <trd>dahingehen</trd>,
    <trd>beschreiten</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>monter (sur)</trd>,
    <trd>grimper (sur)</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_j;&c_t;&c_i; <klr>(&c_n;&c_a; &c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>,
    <trd>&c_v;&c_z;&c_o;&c_j;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_z;&c_o;&c_b;&c_r;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="ir.sxip0i">
  <kap>
    &scirc;ip<tld/>i
  </kap>
