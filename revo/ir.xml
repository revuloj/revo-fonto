      <ekz>
        la aeroplano de<tld/>is de Romo;
      </ekz>
      <ekz>
        la <ind>de<tld/>punkto</ind> de ekskurso, de elpensa&jcirc;o.
        <trd lng="ru">&c_i;&c_s;&c_h;&c_o;&c_d;&c_n;&c_y;&c_j; &c_p;&c_u;&c_n;&c_k;&c_t;</trd>
      </ekz>
    </dif>
