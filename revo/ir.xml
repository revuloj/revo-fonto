  <snc>
    <dif>
      <tld lit="I"/>i piede:
      <ekz>
        David venkoprenis de li mil &ccirc;arojn kaj sep mil rajdantojn kaj
        dudek mil <ind>pied<tld/>anto</ind>jn
        <trd lng="fr">pi&eacute;ton</trd>
        <trd lng="ru">&c_p;&c_je;&c_sh;&c_je;&c_h;&c_o;&c_d;</trd>
        <fnt><bib>M</bib>, <lok>&Kr1; 18:4</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>zu Fu&szlig; gehen</trd>,
    <trd>wandeln</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>marcher<klr tip="ind"> (&agrave; pied)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_d;&c_t;&c_i; &c_p;&c_je;&c_sh;&c_k;&c_o;&c_m;</trd>
</drv>

<drv mrk="ir.re0i">
  <kap>
    re<tld/>i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <tld lit="I"/>i kaj veni returne al la deira loko:
      <ekz>
        <tld/>a kaj re<tld/>a bileto.
      </ekz>
    </dif>
    <trd lng="fr">revenir<klr tip="ind"> (&agrave; son point de
    d&eacute;part)</klr></trd>
  </snc>
  <snc>
    <dif>
      Iri denove, duafoje:
      <ekz>
        mi re<tld/>os al vi la proksiman semajnon.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>zur&uuml;ckgehen</trd>,
      <trd>zur&uuml;ckkehren</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>retourner<klr tip="ind"> (aller de nouveau)</klr></trd>,
      <trd>revenir<klr tip="ind"> (aller de nouveau)</klr></trd>,
      <trd>aller de nouveau</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="ru">
