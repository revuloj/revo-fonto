<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fold.xml,v 1.16 2022/03/17 13:47:20 revo Exp $">
<kap>
  <rad>fold</rad>/o
</kap>

<drv mrk="fold.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Entajlo, entran&ccirc;o farita la&ubreve;longe en ligna&jcirc;o, metalpeco:
      <ekz>
        labirinta, pi&scirc;tringa <tld/>o;
      </ekz>
      <ekz>
        <ind><tld/>o kaj lango</ind> 
        <klr>(vd <ref tip="super" cel="junt.0o.kunigo">junto</ref>, 
        <ref cel="tenon.0o">tenono</ref> kaj <ref cel="mortez.0o">mortezo</ref>)</klr>
        estas sistemo por kunigi similajn objektojn rando al rando
        <fnt>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Foldo_kaj_lango">Foldo kaj lango - Vikipedio</url></vrk>
        </fnt>;
        <trdgrp lng="de">
          <trd>Nut und Feder</trd>,
          <trd>Spund</trd>
        </trdgrp>
        <trd lng="en">tongue and groove</trd>
        <trd lng="fr">rainure et languette</trd>
        <trd lng="ru">&c_sh;&c_p;&c_u;&c_n;&c_t;</trd>
      </ekz>
      <ekz>
        ne estas malfacile per inspektado de la <tld/>o inter viaj maldekstra
        montrofingro kaj dikfingro por esti certe ke vi ne proponas investi vian
        malgrandan kapitalon en la orominoj
        <fnt>
          <vrk><url ref="https://royallib.com/read/Doyle_Arthur/La_aventuro_de_la_dancantoj.html">La aventuro de la dancantoj - Doyle Arthur :: &c_R;&c_je;&c_zh;&c_i;&c_m; &c_ch;&c_t;&c_je;&c_n;&c_i;&c_ja;</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kanel.0o">kanelo</ref>,
      <ref cel="sulk.0o.KOMUNE">sulko</ref>
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>ranura</trd>,
    <trd>solc</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dr&aacute;&zcaron;ka</trd>,
    <trd>r&yacute;ha</trd>,
    <trd>&zcaron;l&aacute;bek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Nut</trd>,
    <trd>Falz</trd>,
    <trd>Kerbe</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>rainure</trd>,
    <trd>feuillure<klr tip="ind"> (rainure)</klr></trd>,
    <trd>gorge<klr tip="ind"> (rainure)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>horony</trd>,
    <trd>v&aacute;jat</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>entalhe</trd>,
    <trd>ranhura</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_je;&c_l;&c_o;&c_b;&c_o;&c_k;</trd>,
    <trd>&c_b;&c_o;&c_r;&c_o;&c_z;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_z;</trd>,
    <trd>&c_f;&c_a;&c_l;&c_mol;&c_c;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_r;&c_je;&c_z;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_je;&c_m;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_a;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">dr&aacute;&zcaron;ka</trd>
</drv>

<drv mrk="fold.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on:
      <ekz>
        la&ubreve; formo la stiftoj povas esti cilindraj, konusaj a&ubreve; <tld/>itaj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=133349">Stifto</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kanel.0o">kanelo</ref>,
      <ref cel="modlur.0o">modluro</ref>,
      <ref cel="rabot.0i">raboti</ref>,
      <ref cel="mortez.0o">mortezo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>fer ranures</trd>,
    <trd>solcar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dr&aacute;&zcaron;kovat</trd>,
    <trd>r&yacute;hovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>eine <ind>Nut</ind> ziehen</trd>,
    <trd>einkerben</trd>,
    <trd>Falz</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>rainer</trd>,
    <trd>rainurer</trd>
  </trdgrp>
  <trd lng="hu">hornyol</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_zh;&c_je;&c_l;&c_o;&c_b;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>p&aacute;hova&tcaron;</trd>,
    <trd>vydr&aacute;&zcaron;kova&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="fold.en0igi">
  <kap>en<tld/>igi</kap>
  <snc>
    <dif>
      Loki iun pecon en <tld/>on:
      <ekz>
        la plej antikvaj teknikoj de konstruado de &scirc;ipoj probable estis <klr>[&#x2026;]</klr>
        malplenigado de trunkoj por formi kanuon, a&ubreve; de en<tld/>igo de trunkoj
        <klr>[&#x2026;]</klr> por formi flosojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=435071">&Scirc;ipkonstruado</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        li decidis konstrui la statuon en Francio <klr>[&#x2026;]</klr> kaj transportita al
        Usono <klr>[en partoj]</klr> enfoldigi &gcirc;in surloke
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18983">Statuo de Libereco</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">encastar</trd>
  <trd lng="cs">vlo&zcaron;it kus s perem do dr&aacute;&zcaron;ky</trd>
  <trd lng="fr">encastrer<klr tip="ind"> (dans une rainure)</klr></trd>
  <trd lng="hu">beilleszt<klr tip="ind"> (horonyba)</klr></trd>
  <trd lng="ru">&c_v;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol; &c_v; &c_p;&c_a;&c_z;</trd>
  <trd lng="sk">umiestni&tcaron; v&nbsp;dr&aacute;&zcaron;ke</trd>
  <trd lng="uk">&#x432;&#x43A;&#x43B;&#x430;&#x434;&#x430;&#x442;&#x438; &#x443; &#x444;&#x430;&#x43B;&#x44C;&#x446;</trd>
</drv>

<drv mrk="fold.glit0o">
  <kap>glit<tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      <tld lit="F"/>o por ion en&scirc;ovi kaj movi en &gcirc;i:
      <ekz>
        glit<tld/>o por tabulo, tirkesto.
      </ekz>
    </dif>
  </snc>
  <adm>
    Ial PIV1 sxangxis la kapvorton al "sxovo~o". <aut>MB</aut>
  </adm>
  <trd lng="ca">col&iacute;s</trd>
  <trd lng="de">Gleitnut</trd>
  <trd lng="fr">coulisse<klr tip="ind"> (rainure)</klr></trd>
  <trdgrp lng="hu">
    <trd>cs&uacute;sz&oacute;vezet&eacute;k</trd>,
    <trd>kulissza<klr tip="ind"> (horony)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_z;<klr tip="amb"> (&c_d;&c_l;&c_ja; &c_s;&c_k;&c_o;&c_l;&c_mol;&c_zh;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>
  </trdgrp>
  <trd lng="sk">klzn&aacute; dr&aacute;&zcaron;ka</trd>
  <trd lng="cs">kluzn&aacute; dr&aacute;&zcaron;ka</trd>
</drv>
</art>
