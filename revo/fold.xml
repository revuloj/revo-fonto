<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fold.xml,v 1.14 2015/07/04 08:37:34 revo Exp $">
<kap>
  <rad>fold</rad>/o
  <fnt>Z</fnt>
</kap>

<drv mrk="fold.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Entajlo, entran&ccirc;o farita la&ubreve;longe en ligna&jcirc;o, metalpeco:
      <ekz>
        labirinta, pi&scirc;tringa <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>ranura</trd>,
    <trd>solc</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>drážka</trd>,
    <trd>rýha</trd>,
    <trd>žlábek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Nut</trd>,
    <trd>Falz</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>rainure</trd>,
    <trd>feuillure<klr tip="ind"> (rainure)</klr></trd>,
    <trd>gorge<klr tip="ind"> (rainure)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>horony</trd>,
    <trd>v&aacute;jat</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>entalhe</trd>,
    <trd>ranhura</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_je;&c_l;&c_o;&c_b;&c_o;&c_k;</trd>,
    <trd>&c_b;&c_o;&c_r;&c_o;&c_z;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_z;</trd>,
    <trd>&c_f;&c_a;&c_l;&c_mol;&c_c;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_r;&c_je;&c_z;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_je;&c_m;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_a;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">drážka</trd>
</drv>

<drv mrk="fold.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on.
    </dif>
    <refgrp tip="vid">
      <ref cel="kanel.0o">kanelo</ref>,
      <ref cel="modlur.0o">modluro</ref>,
      <ref cel="rabot.0i">raboti</ref>,
      <ref cel="mortez.0o">mortezo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>fer ranures</trd>,
    <trd>solcar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>drážkovat</trd>,
    <trd>rýhovat</trd>
  </trdgrp>
  <trd lng="de">falzen</trd>
  <trdgrp lng="fr">
    <trd>rainer</trd>,
    <trd>rainurer</trd>
  </trdgrp>
  <trd lng="hu">hornyol</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_zh;&c_je;&c_l;&c_o;&c_b;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>páhovať</trd>,
    <trd>vydrážkovať</trd>
  </trdgrp>
</drv>

<drv mrk="fold.en0igi">
  <kap>en<tld/>igi</kap>
  <snc>
    <dif>
      Loki iun pecon en <tld/>on.
    </dif>
  </snc>
  <trd lng="ca">encastar</trd>
  <trd lng="cs">vložit kus s perem do drážky</trd>
  <trd lng="fr">encastrer<klr tip="ind"> (dans une rainure)</klr></trd>
  <trd lng="hu">beilleszt<klr tip="ind"> (horonyba)</klr></trd>
  <trd lng="ru">&c_v;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol; &c_v; &c_p;&c_a;&c_z;</trd>
  <trd lng="sk">umiestniť v drážke</trd>
</drv>

<drv mrk="fold.glit0o">
  <kap>glit<tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      <tld lit="F"/>o por ion en&scirc;ovi kaj movi en &gcirc;i:
      <ekz>
        glit<tld/>o por tabulo, tirkesto.
      </ekz>
    </dif>
  </snc>
  <adm>
    Ial PIV1 sxangxis la kapvorton al "sxovo~o". <aut>MB</aut>
  </adm>
  <trd lng="ca">col&iacute;s</trd>
  <trd lng="de">Gleitnut</trd>
  <trd lng="fr">coulisse<klr tip="ind"> (rainure)</klr></trd>
  <trdgrp lng="hu">
    <trd>cs&uacute;sz&oacute;vezet&eacute;k</trd>,
    <trd>kulissza<klr tip="ind"> (horony)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_z;<klr tip="amb"> (&c_d;&c_l;&c_ja; &c_s;&c_k;&c_o;&c_l;&c_mol;&c_zh;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>
  </trdgrp>
  <trd lng="sk">klzná drážka</trd>
  <trd lng="cs">kluzná drážka</trd>
</drv>
</art>
