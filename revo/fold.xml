<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>fold</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="fold.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Entajlo, entran&ccirc;o farita la&ubreve;longe en ligna&jcirc;o, metalpeco:
      <ekz>labirinta, pi&scirc;tringa <tld/>o.</ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Nut</trd>,
     <trd>Falz</trd></trdgrp>
  <trdgrp lng="fr">
    <trd>rainure</trd>,
    <trd>feuillure<klr tip="ind"> (rainure)</klr></trd>,
    <trd>gorge<klr tip="ind"> (rainure)</klr></trd></trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_zh;&c_je;&c_l;&c_o;&c_b;&c_o;&c_k;</trd>,
    <trd>&c_b;&c_o;&c_r;&c_o;&c_z;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_z;</trd>,
    <trd>&c_f;&c_a;&c_l;&c_mol;&c_c;</trd>
  </trdgrp>
</drv>
<drv mrk="fold.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on. 
    </dif>
    <refgrp tip="vid">
      <ref cel="kanel.0o">kanelo</ref>,
      <ref cel="modlur.0o">modluro</ref>,
      <ref cel="rabot.0i">raboti</ref>,
      <ref cel="mortez.0o">mortezo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>rainer</trd>,
    <trd>rainurer</trd></trdgrp> 
</drv>
<drv mrk="fold.en0igi">
  <kap>en<tld/>igi</kap>
  <snc>
    <dif>
      Loki iun pecon en <tld/>on. 
    </dif>
  </snc>
  <trd lng="fr">encastrer<klr tip="ind"> (dans une rainure)</klr></trd>
</drv>
<drv mrk="fold.glit0o">
  <kap>glit<tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      <tld lit="F"/>o por ion en&scirc;ovi k movi en &gcirc;i:
      <ekz>glit<tld/>o por tabulo, tirkesto.</ekz>
    </dif>
  </snc>
  <adm>
  Ial PIV1 sxangxis la kapvorton al "sxovo~o". <aut>MB</aut>
  </adm>
  <trd lng="fr">coulisse<klr tip="ind"> (rainure)</klr></trd>
</drv>
</art>
