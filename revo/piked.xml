<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>piked</rad>/o
</kap>
<drv mrk="piked.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="piked.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Malgranda ta&ccirc;mento da soldatoj gardanta postenon:
      <ekz>
        kozakoj en &ccirc;irka&ubreve;urbaj <tld/>oj longsone intervoki&gcirc;is
        <fnt><bib>Mary</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_ib;&c_k;&c_je;&c_t;</trd>
    <trd lng="de">Bereitschaftseinheit</trd>
    <trdgrp lng="hu">
      <trd>&odblac;rcsapat</trd>,
      <trd>k&eacute;sz&uuml;lts&eacute;g</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_i;&c_k;&c_je;&c_t;</trd>
  </snc>
  <snc mrk="piked.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Speco de <ref tip="super" cel="kart.0ludo.SPO">kartludo</ref>.
    </dif>
    <trd lng="be">&c_p;&c_ib;&c_k;&c_je;&c_t;</trd>
    <trd lng="de">Pikett</trd>
    <trd lng="hu">pik&eacute;t<klr tip="ind"> (k&aacute;rtya)</klr></trd>
    <trd lng="ru">&c_p;&c_i;&c_k;&c_je;</trd>
  </snc>
  <trd lng="nl">piket</trd>
  <trd lng="fr">piquet</trd>
</drv>
</art>
