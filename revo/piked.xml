<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: piked.xml,v 1.21 2022/03/26 10:43:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>piked</rad>/o
</kap>
<drv mrk="piked.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="piked.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <fnt><bib>VdE</bib></fnt>
    <dif>
      Malgranda <ref tip="super" cel="tacxme.0o.MIL">ta&ccirc;mento</ref> da soldatoj, 
      garde staranta &ccirc;e preskribita loko:
      <ekz>
        kozakoj en &ccirc;irka&ubreve;urbaj <tld/>oj longsone intervoki&gcirc;is
        <fnt><bib>Mary</bib></fnt>;
      </ekz>
      <ekz>
        &ccirc;ie, &gcirc;is la vidlimo estas samformaj sablodunoj, la <tld/>o e&ccirc; en le&gcirc;era kamizolo
        duonmorte atendas, ke oni de&scirc;an&gcirc;u ilin
        <fnt>
          <aut>Jen&odblac; Rejt&odblac;, trad. L&aacute;szl&oacute; Bal&aacute;zs</aut>
          <vrk><url ref="https://bookscafe.net/read/rejt_jen-la_flava_garnizono-192424.html">La Flava Garnizono</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="posten.0o.MIL">posteno<sncref/></ref>,
      <ref cel="patrol.0o">patrolo</ref>,
      <ref cel="gard.0istejo">gardistejo</ref>,
      <ref cel="gvardi.0o">gvardio</ref>
    </refgrp>
    <trd lng="be">&c_p;&c_ib;&c_k;&c_je;&c_t;</trd>
    <trdgrp lng="de">
      <trd>Bereitschaftseinheit</trd>,
      <trd>Sto&szlig;trupp</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd><ofc>*</ofc>picket</trd>,
      <trd>sentry</trd>,
      <trd>outpost</trd>,
      <trd>guard</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&odblac;rcsapat</trd>,
      <trd>k&eacute;sz&uuml;lts&eacute;g</trd>
    </trdgrp>
    <trd lng="it">picchetto</trd>
    <trd lng="pt">piquete</trd>
    <trdgrp lng="ru">
      <trd><ofc>*</ofc>&c_p;&c_i;&c_k;&c_je;&c_t;</trd>,
      <trd>&c_z;&c_a;&c_s;&c_t;&c_a;&c_v;&c_a;</trd>,
      <trd>&c_b;&c_l;&c_o;&c_k;&c_p;&c_o;&c_s;&c_t;</trd>
    </trdgrp>
  </snc>
  <snc mrk="piked.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <fnt><bib>VdE</bib></fnt>
    <dif>
      Speco de <ref tip="super" cel="kart.0ludo.SPO">kartludo</ref>.
    </dif>
    <trd lng="be">&c_p;&c_ib;&c_k;&c_je;&c_t;</trd>
    <trd lng="de">Pikett</trd>
    <trd lng="en">piquet</trd>
    <trd lng="hu">pik&eacute;t<klr tip="ind"> (k&aacute;rtya)</klr></trd>
    <trd lng="it">piquet<klr> (gioco)</klr></trd>
    <trd lng="ru">&c_p;&c_i;&c_k;&c_je;&c_t;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>piketa</trd>,
    <trd>vojensk&aacute; hl&iacute;dka</trd>
  </trdgrp>
  <trd lng="fr"><ofc>*</ofc>piquet</trd>
  <trd lng="nl">piket</trd>
  <trd lng="pl">plac&oacute;wka</trd>
  <trd lng="pt">piqu&ecirc;</trd>
  <trd lng="ro">po&scedil;t&abreve;</trd>
  <trdgrp lng="sk">
    <trd>piketa</trd>,
    <trd>vojensk&yacute; oddiel</trd>
  </trdgrp>
    <adm>
      trovu ekz-on kaj apartigu en propra artikolo...
      <aut>WD</aut>
    </adm>
  <trd lng="uk">&#x43F;&#x456;&#x43A;&#x435;&#x442;</trd>
</drv>
</art>
