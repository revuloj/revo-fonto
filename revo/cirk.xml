<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>1</ofc>
   <rad>cirk</rad>/o
</kap>

<drv mrk="cirk.0o">
   <kap><tld/>o</kap>
   <snc mrk="cirk.0o.ARKI">
     <uzo tip="fak">ARKI</uzo>
     <dif>
       Rondforma, &ccirc;irka&ubreve;fermita loko, por rajdaj, akrobataj
       kaj
       burleskulaj spektakloj.
     </dif>
     <trdgrp lng="pl">
       <trd>cyrk</trd>,
       <trd>namiot <ind>cyrkowy</ind></trd>
     </trdgrp>
   </snc>
   <snc mrk="cirk.0o.entrepreno">
     <uzo tip="fak">TEA</uzo>
     <dif>
       Artista grupo, ordinare migra, kiu prezentas homajn a&ubreve;
       bestajn
       akrobata&jcirc;ojn kaj ceterajn distra&jcirc;ojn:
       <ekz>
         mi provis min kiel muzikisto <klr>...</klr> en turneanta
         <tld/>o
         <fnt><aut>-</aut>: <vrk>Mikaelo Lineckij</vrk>,
         <bib>Monato</bib>,
         <lok>jaro 2002a, numero 9a, p. 6a</lok></fnt>.
       </ekz>
     </dif>
     <trd lng="pl">cyrk</trd>
   </snc>
   <trd lng="be">&c_c;&c_y;&c_r;&c_k;</trd>
   <trd lng="br">sirk</trd>
   <trd lng="de">Zirkus</trd>
   <trd lng="en">circus</trd>
   <trd lng="es">circo</trd>
   <trd lng="fr">cirque</trd>
   <trd lng="hu">cirkusz</trd>
   <trd lng="it">circo</trd>
   <trd lng="nl">circus</trd>
   <trd lng="ru">&c_c;&c_i;&c_r;&c_k;</trd>
   <trd lng="sk">cirkus</trd>
</drv>
</art>
