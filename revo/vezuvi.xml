        eksplodo dui&gcirc;is
        <fnt><bib>IntArkeol</bib>, <lok>p. 25a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_V;&c_e;&c_z;&c_u;&c_v;&c_ib;&c_j;</trd>
  <trd lng="en">Vesuvius</trd>
  <trd lng="es">Vesubio</trd>
  <trd lng="fr">V&eacute;suve</trd>
  <trd lng="hu">Vez&uacute;v</trd>
  <trd lng="nl">Vesuvius</trd>
  <trd lng="ru">&c_V;&c_je;&c_z;&c_u;&c_v;&c_i;&c_j;</trd>
</drv>
</art>

