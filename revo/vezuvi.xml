<kap>
  <rad>Vezuvi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="vezuvi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Da&ubreve;re aktiva vulkano en Italio, en provinco Kampanio, apud la
      urbo
      Napolo:
      <ekz>
        dum la erupcio en la jaro 1872. la konuso de <tld/>o pro terura
        eksplodo dui&gcirc;is
        <fnt><bib>IntArkeol</bib>, <lok>p. 25a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_V;&c_e;&c_z;&c_u;&c_v;&c_ib;&c_j;</trd>
  <trd lng="ca">Vesuvi</trd>
  <trd lng="cs">Vesuv</trd>
  <trd lng="en">Vesuvius</trd>
  <trd lng="es">Vesubio</trd>
  <trdgrp lng="fa">
    <trd>&a_w;&a_z;&a_w;&a_w;</trd>,
    <trd>&a_w;&a_z;&a_w;&a_w;&f_y;&a_w;&a_s;</trd>
  </trdgrp>
  <trd lng="fr">V&eacute;suve</trd>
  <trd lng="hu">Vez&uacute;v</trd>
  <trd lng="ja">&#x30D9;&#x30B9;&#x30D3;&#x30AA;&#x30B9;&#x5C71;  <pr>&#x30D9;&#x30B9;&#x30D3;&#x30AA;&#x30B9;&#x3055;&#x3093;</pr></trd>
  <trd lng="nl">Vesuvius</trd>
  <trd lng="ru">&c_V;&c_je;&c_z;&c_u;&c_v;&c_i;&c_j;</trd>
  <trd lng="sk">Vezuv</trd>
  <trd lng="uk">&#x412;&#x435;&#x437;&#x443;&#x432;&#x456;&#x439; (&#x432;&#x443;&#x43B;&#x43A;&#x430;&#x43D;)</trd>
</drv>
