<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vagon.xml,v 1.39 2022/04/06 08:35:16 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vagon</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="vagon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vagon.0o.FER">
    <uzo tip="fak">FER</uzo>
    <dif>
      Surrela <ref tip="lst" cel="vetur.0ilo" lst="voko:veturiloj">veturilo</ref>
      por voja&gcirc;antoj a&ubreve; komerca&jcirc;oj:
      <ekz>
        li veturis en <tld/>o de la unua klaso
        <fnt>
          <aut>A. Giridhar Rao</aut>,
          <vrk>La formi&gcirc;o de Mahatmo</vrk>,
          <bib>Monato</bib>,
          <lok>1997:7, p. 26a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_g;&c_o;&c_n;</trd>
  <trd lng="bg">&c_v;&c_a;&c_g;&c_o;&c_n;</trd>
  <trd lng="br">bagon</trd>
  <trd lng="ca">vag&oacute;</trd>
  <trdgrp lng="cs">
    <trd>v&uring;z</trd>,
    <trd>vag&oacute;n</trd>,
    <trd>vag&oacute;n</trd>,
    <trd>v&uring;z <klr>(kolejov&yacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Wagen</trd>,
    <trd>Waggon</trd>
  </trdgrp>
  <trd lng="el">&beta;&alfa;&gamma;&omikron_ton;&ny;&jota;</trd>
  <trdgrp lng="en">
    <trd>car</trd>,
    <trd>coach</trd>
  </trdgrp>
  <trd lng="es">vag&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>wagon</trd>
  </trdgrp>
  <trd lng="he">&qof;&resh;&vav;&fnun;</trd>
  <trdgrp lng="hu">
    <trd>vagon</trd>,
    <trd>vas&uacute;ti kocsi</trd>
  </trdgrp>
  <trd lng="id"><ind>gerbong</ind> (kereta)</trd>
  <trd lng="nl">wagon</trd>
  <trd lng="pl">wagon</trd>
  <trd lng="pt">vag&atilde;o</trd>
  <trd lng="ru">&c_v;&c_a;&c_g;&c_o;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>vag&oacute;n</trd>,
    <trd>voze&ncaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_a;&c_g;&#x43E;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x8F66;</trd>,
    <trd><klr>tr. </klr>&#x8ECA; [<ind>ch&emacron;</ind>]</trd>
  </trdgrp>
</drv>

<drv mrk="vagon.0aro">
  <kap><ofc>*</ofc><tld/>aro</kap>
  <snc mrk="vagon.0aro.FER">
  <uzo tip="fak">FER</uzo>
    <dif>
      <ref tip="super" cel="vetur.0ilo">Veturilo</ref>
      el serio de <tld/>oj, ligitaj inter si kaj tirataj de
      lokomotivo a&ubreve; alia motorveturilo;
      <ref tip="sin" cel="trajn.0o.FER">trajno</ref>:
      <ekz>
        mankas a&ubreve;tomata trajnbremsado
        ta&ubreve;ga por la plej rapidaj <tld/>aroj
        <fnt>
          <aut>P. Gubbins</aut>, 
          <vrk>Politiko elreligita</vrk>,
          <bib>Monato</bib>,
          <lok>2001:2, p. 6a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>                         
  <trd lng="be">&c_c;&c_ja;&c_g;&c_n;&c_ib;&c_k;</trd>
  <trd lng="bg">&c_v;&c_l;&c_a;&c_k;</trd>
  <trd lng="br">tren</trd>
  <trd lng="ca">tren</trd>
  <trd lng="de">Zug</trd>
  <trd lng="el">&tau;&rho;&alfa;&jota_ton;&ny;&omikron;</trd>
  <trd lng="en">train</trd>
  <trd lng="es">tren</trd>
  <trd lng="fr">train</trd>
  <trd lng="he">&resh;&kaf;&bet;&tav;</trd>
  <trd lng="hu">vonat</trd>
  <trd lng="id"><ind>kereta</ind> (api)</trd>
  <trd lng="nl">trein</trd>
  <trd lng="pl">poci&aogonek;g</trd>
  <trdgrp lng="pt">
    <trd>trem</trd>,
    <trd>comboio</trd>
  </trdgrp>
  <trd lng="ro">tren</trd>
  <trd lng="ru">&c_p;&c_o;&c_je;&c_z;&c_d;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&c_ji;&c_z;&c_d;</trd>,
    <trd>&#x43F;&#x43E;&c_t;&#x44F;&c_g;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x706B;&#x8F66;</trd>,
    <trd><klr>tr. </klr>&#x706B;&#x8ECA; [<ind>hu&#x1D2;ch&emacron;</ind>]</trd>
  </trdgrp>
</drv>

<drv mrk="vagon.mangx0o">
  <kap>man&gcirc;<tld/>o</kap>
  <snc mrk="vagon.mangx0o.FER">
    <uzo tip="fak">FER</uzo>
    <dif>
      <tld lit="V"/>o en kiu oni provizas man&gcirc;ojn
      al la veturantoj:
      <ekz>
        la grafo malvolonte cedis; li sin klinis
        kaj forlasis la man&gcirc;-<tld/>on
        <fnt>
          <aut>A. Christie, trad. [O. Goldsmith]</aut>,
          <vrk>Murdo en la Orienta Ekspreso</vrk>,
          <lok>[1936?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_g;&c_o;&c_n;-&c_r;&c_e;&c_s;&c_t;&c_a;&c_r;&c_a;&c_n;</trd>
  <trd lng="bg">&c_v;&c_a;&c_g;&c_o;&c_n; &c_r;&c_je;&c_s;&c_t;&c_o;&c_r;&c_a;&c_n;&c_t;</trd>
  <trdgrp lng="es">
    <trd>coche restaurante</trd>,
    <trd>vag&oacute;n restaurante</trd>
  </trdgrp>
  <trd lng="fr">wagon-restaurant</trd>
  <trdgrp lng="id">
    <trd><ind>gerbong</ind> makan</trd>,
    <trd>restorasi</trd>
  </trdgrp>
  <trd lng="pl">wagon restauracyjny</trd>
</drv>
</art>
