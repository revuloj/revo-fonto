<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vagon</rad>/o
</kap>
<drv mrk="vagon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">FER</uzo>
  <snc>
    <dif>
      Surrela <ref tip="super" cel="vetur.0ilo">veturilo</ref>
      por pasa&gcirc;eroj a&ubreve; varoj.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>vag&oacute;n</trd>,
    <trd>v&ugrave;z</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Wagen</trd>,
    <trd>Waggon</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>car</trd>,
    <trd>coach</trd>
  </trdgrp>
  <trd lng="es">vag&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>wagon</trd>
  </trdgrp>
  <trd lng="nl">wagon</trd>
  <trd lng="pl">wagon</trd>
  <trd lng="ru">&c_v;&c_a;&c_g;&c_o;&c_n;</trd>
</drv>
<drv mrk="vagon.0aro">
  <kap><ofc>*</ofc><tld/>aro</kap>
  <uzo tip="fak">FER</uzo>
  <snc>
    <dif>
      Serio da <tld/>oj, ligitaj inter si kaj trenataj de
      lokomotivo a&ubreve; alia motorveturilo. 
    </dif>
    <ref tip="sin" cel="trajn.0o">trajno</ref>
    <ref tip="super" cel="vetur.0ilo">veturilo</ref>
  </snc>
  <trd lng="de">Zug</trd>
  <trd lng="en">train</trd>
  <trd lng="es">tren</trd>
  <trdgrp lng="fr">
    <trd>train</trd>
  </trdgrp>
  <trd lng="nl">trein</trd>
  <trd lng="pl">poci&aogonek;g</trd>
  <trd lng="ru">&c_p;&c_o;&c_je;&c_z;&c_d;</trd>
</drv>
</art>
