<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vagon</rad>/o
</kap>
<drv mrk="vagon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vagon.0o.FER">
    <uzo tip="fak">FER</uzo>
    <dif>
      Surrela <ref tip="super" cel="vetur.0ilo">veturilo</ref>
      por pasa&gcirc;eroj a&ubreve; varoj:
      <ekz>
        li veturis en <tld/>o de la unua klaso, kaj li ne estis
        blankha&ubreve;ta<fnt><aut>A. Giridhar Rao</aut>, <vrk>La formi&gcirc;o de
        Mahatmo</vrk>, <bib>Monato</bib>, <lok>jaro 1997a, numero 7a, p.
        26a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_g;&c_o;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>vag&oacute;n</trd>,
    <trd>v&uring;z</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Wagen</trd>,
    <trd>Waggon</trd>
  </trdgrp>
  <trd lng="el">&beta;&alfa;&gamma;&omikron_ton;&ny;&jota;</trd>
  <trdgrp lng="en">
    <trd>car</trd>,
    <trd>coach</trd>
  </trdgrp>
  <trd lng="es">vag&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>wagon</trd>
  </trdgrp>
  <trd lng="he">&qof;&resh;&vav;&fnun;</trd>
  <trdgrp lng="hu">
    <trd>vagon</trd>,
    <trd>vas&uacute;ti kocsi</trd>
  </trdgrp>
  <trd lng="nl">wagon</trd>
  <trd lng="pt">vag&atilde;o</trd>
  <trd lng="pl">wagon</trd>
  <trd lng="ru">&c_v;&c_a;&c_g;&c_o;&c_n;</trd>
  <trdgrp lng="zh">
    <trd>&#36710;</trd>,
    <trd><klr>tr. </klr>&#36554;<klr> [ch&#275;]</klr></trd>
  </trdgrp>
</drv>

<drv mrk="vagon.0aro">
  <kap><ofc>*</ofc><tld/>aro</kap>
  <snc mrk="vagon.0aro.FER">
  <uzo tip="fak">FER</uzo>
    <dif>
      <ref tip="super" cel="vetur.0ilo">Veturilo</ref>
      el serio de <tld/>oj, ligitaj inter si kaj trenataj de
      lokomotivo a&ubreve; alia motorveturilo:
      <ekz>
        mankas a&ubreve;tomata trajnbremsado
        ta&ubreve;ga por la plej rapidaj <tld/>aroj<fnt><aut>Paul
        Gubbins</aut>, <vrk>Politiko elreligita</vrk>,
        <bib>Monato</bib>, <lok>jaro 2001a, numero 2a, p. 6a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="trajn.0o">trajno</ref>
  </snc>
  <trd lng="be">&c_c;&c_ja;&c_g;&c_n;&c_ib;&c_k;</trd>
  <trd lng="de">Zug</trd>
  <trd lng="el">&tau;&rho;&alfa;&jota_ton;&ny;&omikron;</trd>
  <trd lng="en">train</trd>
  <trd lng="es">tren</trd>
  <trdgrp lng="fr">
    <trd>train</trd>
  </trdgrp>
  <trd lng="he">&resh;&kaf;&bet;&tav;</trd>
  <trd lng="hu">vonat</trd>
  <trd lng="nl">trein</trd>
  <trdgrp lng="pt">
    <trd>trem</trd>,
    <trd>comboio</trd>
  </trdgrp>
  <trd lng="pl">poci&aogonek;g</trd>
  <trd lng="ru">&c_p;&c_o;&c_je;&c_z;&c_d;</trd>
  <trdgrp lng="zh">
    <trd>&#28779;&#36710;</trd>,
    <trd><klr>tr. </klr>&#28779;&#36554;<klr> [hu&#466;ch&#275;]</klr></trd>
  </trdgrp>
</drv>
</art>
