<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fal&ccirc;</rad>/i
</kap>
<drv mrk="falcx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">AGR</uzo>
  <snc num="1">
    <dif>
      Detran&ccirc;i herbon k grenon per <tld/>ilo:
      <ekz><tld/>i grenon;</ekz>
      <ekz><tld/>a (irareira) movado;</ekz>
      <ekz>pri<tld/>i kampon.</ekz>
    </dif>
   <trdgrp lng="de">
     <trd>m&auml;hen</trd>,
     <trd>absensen</trd></trdgrp>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Bu&ccirc;i, amase kaj rapide mortigi:
      <ekz>la pafado <tld/>is multajn rekrutojn;</ekz>
      <ekz>la tempo <tld/>as la homajn vivojn.</ekz>
    </dif>
    <trdgrp lng="de">
     <trd>niederm&auml;hen</trd>,
     <trd>abschlachten</trd></trdgrp>
  </snc>
  <trd lng="en">mow</trd>
  <trdgrp lng="fr">
    <trd>faucher<klr tip="ind"> (l'herbe, des vies)</klr></trd>,
    <trd>d&eacute;cimer</trd></trdgrp> 
  <trd lng="nl">maaien</trd>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_i;&c_t;&c_mol; <klr>(&c_t;&c_r;&c_a;&c_v;&c_u;)</klr></trd>
</drv>
<drv mrk="falcx.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Longa, lar&gcirc;a, iom kurba &scirc;tala klingo, fiksita al longa
      ligna tenilo, kaj servanta por detran&ccirc;i herbon kaj grenon;
      <ekz><tld/>ilo estas atributo de Tempo.</ekz>
    </dif>
  </snc>
  <trd lng="de">Sense</trd>
  <trd lng="fr">faux<klr tip="ind"> (instrument)</klr></trd>
  <trd lng="nl">zeis</trd>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_a; <klr>(&c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;)</klr></trd>
</drv>
<drv mrk="falcx.0ileto">
  <kap><tld/>ileto</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Malgranda <tld/>ilo, duonronde kurba kun mallonga tenilo,
      por detran&ccirc;i grenon a&ubreve; herbon. 
    </dif>
  </snc>
  <trd lng="fr">faucille</trd>
  <trd lng="nl">sikkel</trd>
  <trd lng="ru">&c_s;&c_je;&c_r;&c_p;</trd>
</drv>
<drv mrk="falcx.0masxino">
  <kap><tld/>ma&scirc;ino</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Ma&scirc;ino anstata&ubreve;anta <tld/>ilon por rapide k grandakvante
      <tld/>i rikoltojn.  
    </dif>
  </snc>
  <trd lng="de">M&auml;hmaschine</trd>
  <trd lng="fr">faucheuse<klr tip="ind"> (machine)</klr></trd>
  <trd lng="nl">maaimachine</trd>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_i;&c_l;&c_k;&c_a;</trd>
</drv>
</art>

