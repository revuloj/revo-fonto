<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>fal&ccirc;</rad>/i
</kap>
<drv mrk="falcx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">AGR</uzo>
  <snc mrk="falcx.0i.detrancxi">
    <dif>
      Detran&ccirc;i herbon kaj grenon per <tld/>ilo:
      <ekz>
        <tld/>i grenon;
      </ekz>
      <ekz>
        <tld/>a (irareira) movado;
      </ekz>
      <ekz>
        pri<tld/>i kampon.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>m&auml;hen</trd>,
      <trd>absensen</trd>
    </trdgrp>
  </snc>
  <snc mrk="falcx.0i.bucxi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Bu&ccirc;i, amase kaj rapide mortigi:
      <ekz>
        la pafado <tld/>is multajn rekrutojn;
      </ekz>
      <ekz>
        la tempo <tld/>as la homajn vivojn.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>niederm&auml;hen</trd>,
      <trd>abschlachten</trd>
    </trdgrp>
    <trd lng="fr">d&eacute;cimer</trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_s;&c_ib;&c_c;&c_mol; <klr>(&c_t;&c_r;&c_a;&c_v;&c_u;)</klr></trd>
  <trd lng="de">m&auml;hen</trd>
  <trd lng="en">mow</trd>
  <trd lng="es">segar</trd>
  <trdgrp lng="fr">
    <trd>faucher<klr tip="ind"> (l'herbe, des vies)</klr></trd>
  </trdgrp>
  <trd lng="hu">kasz&aacute;l</trd>
  <trd lng="it">falciare</trd>
  <trd lng="nl">maaien</trd>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_i;&c_t;&c_mol; <klr>(&c_t;&c_r;&c_a;&c_v;&c_u;)</klr></trd>
</drv>

<drv mrk="falcx.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Longa, lar&gcirc;a, iom kurba &scirc;tala klingo, fiksita al longa
      ligna tenilo, kaj servanta por detran&ccirc;i herbon kaj grenon;
      <ekz>
        <tld/>ilo estas atributo de Tempo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_s;&c_a; <klr>(&c_p;&c_r;&c_y;&c_l;&c_a;&c_d;&c_a;)</klr></trd>
  <trd lng="ca">dalla</trd>
  <trd lng="de">Sense</trd>
  <trdgrp lng="es">
    <trd>guada&ntilde;a</trd>,
    <trd>hoz</trd>
  </trdgrp>
  <trd lng="fr">faux<klr tip="ind"> (instrument)</klr></trd>
  <trd lng="gl">gada&ntilde;a</trd>
  <trd lng="hu">kasza</trd>
  <trd lng="it">falce</trd>
  <trd lng="nl">zeis</trd>
  <trd lng="pt">foice</trd>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_a; <klr>(&c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;)</klr></trd>
</drv>

<drv mrk="falcx.0ileto">
  <kap><tld/>ileto</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Malgranda <tld/>ilo, duonronde kurba kun mane longa tenilo,
      por detran&ccirc;i grenon a&ubreve; herbon.
    </dif>
    <ref tip="sin" cel="serp.0o">serpo</ref>
  </snc>
  <trd lng="be">&c_s;&c_je;&c_r;&c_p;</trd>
  <trdgrp lng="fr">
    <trd>faucille</trd>,
    <trd>serpe</trd>
  </trdgrp>
  <trd lng="hu">sarl&oacute;</trd>
  <trd lng="nl">sikkel</trd>
  <trd lng="ru">&c_s;&c_je;&c_r;&c_p;</trd>
</drv>

<drv mrk="falcx.post0ajxo">
  <kap>post<tld/>a&jcirc;o<fnt>Z</fnt></kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Herbo, kiu kreskas post la unua <tld/>o:
      <ekz>
        Li kreis akridojn en la komenco de kreskado de la
        post<tld/>a&jcirc;o
        <fnt><bib>MT</bib><lok>&Amo; 7:1</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="fojn.post0o">postfojno</ref>
  </snc>
   <trdgrp lng="fr">
     <trd>regain<klr tip="ind"> (herbe de seconde coupe)</klr></trd>
   </trdgrp>
   <trd lng="ru">&c_o;&c_t;&c_a;&c_v;&c_a;</trd>
</drv>

<drv mrk="falcx.0masxino">
  <kap><tld/>ma&scirc;ino</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Ma&scirc;ino anstata&ubreve;anta <tld/>ilon por rapide kaj grandakvante
      <tld/>i rikoltojn.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_s;&c_ib;&c_l;&c_k;&c_a;</trd>
  <trd lng="de">M&auml;hmaschine</trd>
  <trd lng="es">segadora</trd>
  <trd lng="fr">faucheuse<klr tip="ind"> (machine)</klr></trd>
  <trd lng="hu">kasz&aacute;l&oacute;g&eacute;p</trd>
  <trd lng="nl">maaimachine</trd>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_i;&c_l;&c_k;&c_a;</trd>
</drv>

<drv mrk="falcx.krur0o">
  <kap>krur<tld/>o</kap>
  <snc>
    <dif>
      Kapto au baro de la piedo de iranto, kauzanta stumblon a&ubreve; falon:
      <ekz>
        &ccirc;u <klr>...</klr> la diablo ne faligus min per krur<tld/>o, por ke
        mi rompu al mi la kapon?
        <fnt><bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 4a, p.
        419a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">croc-en-jambe</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_n;&c_o;&c_zh;&c_k;&c_a;<klr tip="amb"> (&c_p;&c_r;&c_i;&c_jo;&c_m;)</klr></trd>
</drv>
</art>
