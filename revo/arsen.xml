<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>arsen</rad>/o
</kap>
<drv mrk="arsen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 33, atompezo 74,261, simbolo As. Solida <ref tip="super"
      cel="metalo.0o">metaloido</ref> de ferogriza koloro.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Arsen</trd>,
    <trd>Fliegenstein</trd>
  </trdgrp>
  <trd lng="fr">arsenic (corps simple)</trd>
  <trdgrp lng="nl">
    <trd>arseen</trd>,
    <trd>arsenicum</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_y;&c_sh;&c_mol;&c_ja;&c_k;</trd>
  <trd lng="sk">arz&eacute;n</trd>
  <trd lng="tr">arsenik</trd>
</drv>
</art>
