<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>arsen</rad>/o
</kap>

<drv mrk="arsen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 33, atompezo 74,92, simbolo As. Solida <ref tip="super"
      cel="metalo.0o">metaloido</ref> de ferogriza koloro.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_y;&c_sh;'&c_ja;&c_k;</trd>     
  <trd lng="bg">&c_a;&c_r;&c_s;&c_je;&c_n;</trd>
  <trd lng="br">arsenik<klr tip="ind"> (elfenn eeun</klr></trd>
  <trdgrp lng="de">
    <trd>Arsen</trd>,
    <trd>Fliegenstein</trd>
  </trdgrp>
  <trd lng="en">arsenic</trd>
  <trd lng="es">ars&eacute;nico</trd>
  <trd lng="fr">arsenic (corps simple)</trd>
  <trd lng="hu">arz&eacute;n</trd>
  <trd lng="it">arsenico</trd>
  <trd lng="la">Arsenium</trd>
  <trdgrp lng="nl">
    <trd>arseen</trd>,
    <trd>arsenicum</trd>
  </trdgrp>
  <trd lng="pl">arsen</trd>
  <trd lng="pt">ars&ecirc;nio</trd>
  <trd lng="ru">&c_m;&c_y;&c_sh;&c_mol;&c_ja;&c_k;</trd>
  <trd lng="sk">arz&eacute;n</trd>
  <trd lng="tr">arsenik</trd>
</drv>
</art>
