<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kie</rad>
</kap>
<drv mrk="kie.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Demanda adv., signifanta &leftquot;en kiu loko?&rightquot;:
      <ekz>
        <tld/> li estas?
      </ekz>
    </dif>
    <trdgrp lng="he">
      <trd>&alef;&yod;&he;</trd>,
      <trd>&alef;&yod;&pe;&he;</trd>,
      <trd>&he;&yod;&kaf;&fnun;</trd>
    </trdgrp>
    <trd lng="hu">hol</trd>
    <trd lng="vo">kiplad</trd>
  </snc>
  <snc>
    <dif>
      Relativa adv., signifanta &leftquot;en la loko, en kiu&rightquot;:
      <ekz>
        <tld/> jukas, tie ni gratas<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/> estas sufero, estas anka&ubreve; espero<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        li lo&gcirc;as tie, <ind><tld/>n</ind> e&ccirc; birdo ne flugas;
        <trd lng="be">&c_k;&c_u;&c_d;&c_y;</trd>
        <trd lng="ru">&c_k;&c_u;&c_d;&c_a;</trd><fnt><bib>PrV</bib></fnt>
      </ekz>
      <ekz>
        mi flanki&gcirc;adis de tiuj &ccirc;i reguloj nur tie, <tld/> tion &ccirc;i
        postulis apartaj cirkonstancoj<fnt>Z</fnt>
        (<refgrp tip="sin">
          <ref cel="tiam.0">tiam</ref>,
          <ref cel="kiam.0">kiam</ref>
        </refgrp>);
      </ekz>
      <ekz>
        en tiaj okazoj, <tld/> vi ne scias, &ccirc;u...<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la&ubreve; la krio de la koko, tuj spirito &ccirc;iu, <tld/> ajn
        li vagas, rapidas hejmen<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind><tld/>n kudrilo iras, tien fadenon &gcirc;i tiras</ind>.
        <trd lng="ru">&c_k;&c_u;&c_d;&c_a; &c_i;&c_g;&c_o;&c_l;&c_k;&c_a;, &c_t;&c_u;&c_d;&c_a; &c_i;
        &c_n;&c_i;&c_t;&c_k;&c_a;</trd><fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <trd lng="he">&bet;&mem;&qof;&vav;&fmem; &bet;&vav;</trd>
    <trd lng="hu">ahol</trd>
    <trd lng="vo">k&ouml;</trd>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_je;</trd>
  <trd lng="cs">kde</trd>
  <trd lng="de">wo</trd>
  <trd lng="fr">o&ugrave;</trd>
  <trd lng="nl">waar</trd>
  <trd lng="ru">&c_g;&c_d;&c_je;</trd>
</drv>
</art>
