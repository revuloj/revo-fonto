<drv mrk="kie.0n">
  <kap><ofc>*</ofc><tld/>n</kap>
  <snc mrk="kie.0n.demanda">
    <dif>
      Demanda adverbo, signifanta &leftquot;al kiu loko?&rightquot;
      (en demando pri <ref cel="direkt.0o.kien">direkto</ref>
      a&ubreve; celo de movo ktp):
      <ekz>
        <tld lit="K"/>n vi iras? &#8208; Mi iras en la &gcirc;ardenon
        <fnt><bib>F</bib>, <lok>&FE;26.</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_n;&c_a;&c_k;&c_malmol;&c_d;&c_je;</trd>
    <trdgrp lng="en">
      <trd>where <klr>(question)</klr></trd>,
      <trd>to what <ind>place</ind></trd>,
      <trd>whither</trd>
    </trdgrp>
    <trd lng="es">ad&oacute;nde</trd>
    <trd lng="hu">hov&aacute;</trd>
    <trd lng="th">&#3651;&#3609;&#3607;&#3637;&#3656;&#3652;&#3627;&#3609;</trd>
  </snc>
  <snc mrk="kie.0n.relativa">
    <dif>
      Relativa adverbo, signifanta &leftquot;al tiu loko,
      <tld/>&rightquot;:
      <ekz>
        li trafis ien, <tld/>n li ne celis;
      </ekz><ekz>
        li lo&gcirc;as tie, <tld/>n e&ccirc; birdo ne flugas
        <fnt><bib>PrV</bib></fnt>;
      </ekz><ekz>
        <ind><tld/>n kudrilo iras, tien fadenon &gcirc;i tiras</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="hu">oda megy a c&eacute;rna, <ind>ahov&aacute;</ind> a
        t&udblac;
        h&uacute;zza</trd>
        <trd lng="ru">&c_k;&c_u;&c_d;&c_a;
        &c_i;&c_g;&c_o;&c_l;&c_k;&c_a;,
          &c_t;&c_u;&c_d;&c_a; &c_i; &c_n;&c_i;&c_t;&c_k;&c_a;</trd>
      </ekz>
    </dif>
    <trd lng="bg">&c_n;&c_a;&c_k;&c_malmol;&c_d;&c_je;&c_t;&c_o;</trd>
    <trdgrp lng="en">
      <trd>where <klr>(introducing relative phrase)</klr></trd>,
      <trd>to the <ind>place</ind> that</trd>
    </trdgrp>
    <trd lng="es">adonde</trd>
    <trd lng="hu">ahov&aacute;</trd>
    <trd lng="th">&#3651;&#3609;&#3607;&#3637;&#3656;&#3595;&#3638;&#3656;&#3591;</trd>
