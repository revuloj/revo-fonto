<art mrk="$Id: hub.xml,v 1.20 2024/02/23 18:26:26 revo Exp $">
<kap>
  <rad>hub</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="hub.0o">
  <kap><tld/>o</kap>
  <snc mrk="hub.0o.MEZ">
    <uzo tip="fak">HIS</uzo>
    <dif>
      <ref tip="lst" lst="voko:mezurunuoj" cel="are.0o.mezuro">Areo</ref> de kulturebla
      tero la&ubreve; regiono inter 7 kaj 15 <ref cel="hektar.0o">hektaroj</ref>:
      <ekz>
        <ind><tld/>ulo</ind> <klr>(kiu posedas rajton pri unu <tld/>o)</klr>
        <fnt><bib>JED</bib></fnt>;
        <trd lng="de">Hufner</trd>
      </ekz>
      <ekz>
        <nom>Ferda</nom> <klr>[&#8230;]</klr> rigardis la forkurantajn
        <tld/>ojn de la kampoj
        <fnt>
          <aut>S. Bubeni&ccaron;</aut>,
          <vrk>Sub Cirkotendo</vrk>,
          <lok>1969</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Oginski</nom> arbar-<tld/>ojn cent, <klr>[&#8230;]</klr> perdis por melo
        <fnt>
          <bib>SrT</bib>
          <lok>Libro II.</lok>
        </fnt>!
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akre.0o">akro, akreo</ref>,
      <ref cel="parcel.0o">parcelo</ref>
    </refgrp>
    <bld
      lok="&WCU;/thumb/c/cd/Hufen_in_Sch&ouml;nbrunn_(Kreis_Sagan)_1815.jpg/700px-Hufen_in_Sch&ouml;nbrunn_(Kreis_Sagan)_1815.jpg"
      prm="PD"
      >numeritaj <tld/>oj de silezia vila&gcirc;o Sch&ouml;nbrunn, 1815 (nun Jab&lstroke;on&oacute;w)
        <fnt>
          <aut>August Meitzen (1822-1910), Der Boden und die landwirthschaftlischen Verh&auml;ltnisse des preu&szlig;ischen Staates,  Verlag von Paul Parey, Berlin, 1868</aut>
          <lok><url ref="&WCW;/File:Hufen_in_Sch%C3%B6nbrunn_(Kreis_Sagan)_1815.jpg"
              >https://commons.wikimedia.org/wiki/File:Hufen_in_Sch&ouml;nbrunn_(Kreis_Sagan)_1815.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="cs">l&aacute;n</trd>
  <trdgrp lng="de">
    <trd>Hufe</trd>,
    <trd>Hube</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>oxgang</trd>,
    <trd>bovate</trd>
  </trdgrp>
  <trd lng="fr">manse</trd>
  <trdgrp lng="ja">
    <trd>&#23567;&#36786;&#22320;  <pr>&#12375;&#12423;&#12358;&#12398;&#12358;&#12385;</pr></trd>,
    <trd>&#12501;&#12540;&#12501;&#12455;</trd>
  </trdgrp>
  <trd lng="pt">jeira<klr tip="amb"> (unidade de &aacute;rea)</klr></trd>
  <trd lng="ru">&c_g;&c_u;&c_f;&c_a;</trd>
  <trd lng="sk">l&aacute;n <klr>(plo&scaron;n&aacute; miera)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_je;&c_m;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_j; &c_n;&c_a;&c_d;&c_ib;&c_l;</trd>,
    <trd>&c_l;&c_a;&c_n;</trd>
  </trdgrp>
</drv>
</art>

