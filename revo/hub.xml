<vortaro>
<art mrk="$Id$">
<kap>
<rad>hub</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="hub.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Malnova komunuza <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de
      <ref cel="are.0o">areo</ref>, uzata por mezuri terenon, egala al
      &ccirc;irka&ubreve; 10-15 <ref cel="hektar.0o">hektaroj</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Hufe</trd>,
     <trd>Hube</trd>
  </trdgrp>
</drv>
</art>

