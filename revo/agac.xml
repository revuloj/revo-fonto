<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: agac.xml,v 1.35 2023/10/28 08:32:44 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>agac</rad>/o
</kap>

<drv mrk="agac.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="agac.0o.FIZL">
    <dif>
      Malagrabla, incita <ref tip="super" cel="sent.0o.impreso">sento</ref>
      &ccirc;e la <ref cel="dent.0o.busxo">dento</ref>j a&ubreve; &ccirc;e la nervaro, pro acido,
      malvarmo, akra bruo ks:
      <ekz>
        la <klr>[&#x2026;]</klr> nobeluloj sentis <tld/>on &ccirc;e la penso, ke ilia kamparano povus
        resti senokupa kvindek tagojn en la jaro
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la nordo la akvo fari&gcirc;as &scirc;tono, kaj la vaporo &#x2010; blanka salo, kiu tamen havas
        neniun guston, sed pin&ccirc;as la fingrojn kaj <tld/>igas la dentojn
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x7259;&#x75BC; <pr>y&#xE1;t&#xE9;ng</pr></trd>
</snc>
  <trd lng="be">&c_a;&c_s;&c_k;&c_o;&c_m;&c_a;</trd>
  <trdgrp lng="de">
    <trd><ofc>*</ofc><ind>Stumpf</ind>werden der Z&auml;hne</trd>,
    <trd>Zahnschmerz <klr>(wg. K&auml;lte, S&auml;ure)</klr></trd>
  </trdgrp>
  <trd lng="en"><ofc>*</ofc>setting on edge</trd>
  <trd lng="fr">agacement <klr>(des dents, des nerfs)</klr></trd>
  <trdgrp lng="he">
    <trd>&het;&resh;&yod;&qof;&tav; (&shin;&yod;&nun;&yod;&yod;&fmem;</trd>,
    <trd>&tsadi;&yod;&pe;&vav;&resh;&fnun;)</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3044;&#x3089;&#x3060;&#x3061;</trd>,
    <trd>&#x3044;&#x3089;&#x3060;&#x305F;&#x3057;&#x3055;</trd>
  </trdgrp>
  <trd lng="pl"><ofc>*</ofc>dr&eogonek;two&sacute;&cacute;</trd>
  <trd lng="ru"><ofc>*</ofc>&c_o;&c_s;&c_k;&c_o;&c_m;&c_i;&c_n;&c_a;</trd>
</drv>

<drv mrk="agac.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Sentiganta <tld/>on, <klr>(en figura senco)</klr> inciton:
      <ekz>
        &ccirc;ion mi atendis, nur tion ne <klr>[&#x2026;]</klr> mi sentis ian strangan, <tld/>an
        guston malanta&ubreve; la aferoj
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        li ja venis kadre de oficiala enketo kaj povos klarigi la situacion &ndash; sed la
        afero tamen estis <tld/>a: neniam bone impresas al la &ccirc;efoj kiam oni &scirc;ajne
        trouzas sian policanecon
        <fnt>
          <bib>ChB</bib>
        </fnt>;
      </ekz>
      <ekz>
        damne, Eternulo, vi fakte &ccirc;ion povas, se el nura ripo de nia prapatro vi
        kapablis elsor&ccirc;i tian belega&jcirc;on <tld/>e efikan
        <fnt>
          <bib>SkandalJozef</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="acid.0a">acida</ref>,
      <ref cel="adstri.0a">adstringa</ref>,
      <ref cel="dolor.0a">dolora</ref>,
      <ref cel="pik.0a.FIG">pika<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_k;&c_o;&c_m;&c_ib;&c_s;&c_t;&c_y;</trd>
  <trd lng="de">aufreizend</trd>
  <trd lng="en">aggravating</trd>
  <trdgrp lng="ja">
    <trd>&#x795E;&#x7D4C;&#x3092;&#x523A;&#x6FC0;&#x3059;&#x308B;  <pr>&#x3057;&#x3093;&#x3051;&#x3044;&#x3092;&#x3057;&#x3052;&#x304D;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x3044;&#x3089;&#x3060;&#x305F;&#x305B;&#x308B;&#x3088;&#x3046;&#x306A;</trd>
  </trdgrp>
</drv>

<drv mrk="agac.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="agac.0i.dento">
    <dif>
      Ka&ubreve;zi <tld/>on,
      <ref tip="vid" cel="irit.0i.MED">iriti</ref>,
      forte <ref tip="vid" cel="juk.0i.KOMUNE">juki</ref>:
      <ekz>
        Dio man&gcirc;on donacis, sed la dentoj <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la dentoj <tld/>i&gcirc;os &ccirc;e tiu, kiu man&gcirc;is la nematurajn vinberojn
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 31:30</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_b;&c_ib;&c_v;&c_a;&c_c;&c_mol;
      <ind>&c_a;&c_s;&c_k;&c_o;&c_m;&c_u;</ind></trd>,
      <trd>&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_n;&c_ja;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="br">tozona&ntilde;</trd>
    <trd lng="de">reizen <klr>(das Zahnfleisch)</klr></trd>
    <trdgrp lng="en">
      <trd>aggravate</trd>,
      <trd>annoy</trd>,
      <trd>irritate</trd>
    </trdgrp>
    <trd lng="es">causar dentera</trd>
    <trdgrp lng="fr">
      <trd>agacer <klr>(les dents)</klr></trd>,
      <trd>porter sur les <ind>nerfs</ind></trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>allappare</trd>,
      <trd>stridere<klr> (fig.)</klr></trd>,
      <trd>irritare</trd>,
      <trd>dare sui nervi</trd>
    </trdgrp>
    <trd lng="nl">pijn doen aan de tanden of aan de zenuwen</trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_y;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;
      <ind>&c_o;&c_s;&c_k;&c_o;&c_m;&c_i;&c_n;&c_u;</ind></trd>,
      <trd>&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="agac.0i.inciti">
    <dif>
      <ref tip="dif" cel="incit.0i.kolerigi">Inciti<sncref/></ref>:
      <ekz>
        &scirc;ia certeco, ke mi trovos solvon, ne tu&scirc;is min flate, sed nur <tld/>is miajn
        nervojn
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        min tiuj bruoj &ccirc;iufoje &ccirc;agrenas kaj <tld/>as
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iam pla&ccirc;is al &scirc;i esti la unua portanto de nova&jcirc;o, <tld/>is min &scirc;ia kontenta
        esprimo
        <fnt>
          <aut>Anna L&ouml;wenstein</aut>
          <vrk>La &scirc;tona urbo</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_n;&c_ja;&c_c;&c_mol;</trd>
    <trd lng="br">hegazi&ntilde;</trd>
    <trd lng="de">aufreizen</trd>
    <trd lng="nl">op de <ind>zenuwen</ind> werken</trd>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="cs">trnout <klr>(zuby)</klr></trd>
  <trdgrp lng="he">
    <trd>&lamed;&het;&resh;&vav;&qof; (&shin;&yod;&nun;&yod;&yod;&fmem;</trd>,
    <trd>&tsadi;&pe;&vav;&resh;&nun;&yod;&yod;&fmem;)</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x795E;&#x7D4C;&#x306B;&#x969C;&#x308B;  <pr>&#x3057;&#x3093;&#x3051;&#x3044;&#x306B;&#x3055;&#x308F;&#x308B;</pr></trd>,
    <trd>&#x3044;&#x3089;&#x3060;&#x305F;&#x305B;&#x308B;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>st&racute;pnu&tcaron;</trd>,
    <trd>t&racute;pnu&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_k;&c_l;&c_i;&c_k;&c_a;&c_t;&c_i; &c_o;&c_s;&c_k;&c_o;&c_m;&c_u;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_r;&c_a;&c_z;&c_n;&c_je;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
