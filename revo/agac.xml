<?xml version="1.0"?>

<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>agac</rad>/i
</kap>
<drv mrk="agac.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="agac.0i.dento">
    <dif>
      Ka&ubreve;zi malagrablan, incitan senton &ccirc;e la dentoj a&ubreve;
      &ccirc;e la nervaro:
      <ekz>
        frotado de fajlilo <tld/>as;
      </ekz>
      <ekz>
        la patroj man&gcirc;is nematurajn vinberojn, kaj la dentoj de la
        filoj
        <tld/>i&gcirc;is
        <fnt><bib>MT</bib>, <lok>&Jer; 31:29</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_b;&c_ib;&c_v;&c_a;&c_c;&c_mol; <ind>&c_a;&c_s;&c_k;&c_o;&c_m;&c_ib;&c_n;&c_u;</ind></trd>,
      <trd>&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_n;&c_ja;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="br">tozona&ntilde;</trd>
    <trd lng="de">reizen</trd>
    <trd lng="en">aggravate</trd>
    <trd lng="es">dentera</trd>
    <trdgrp lng="fr">
      <trd>agacer <klr>(les dents)</klr></trd>,
      <trd>porter sur les <ind>nerfs</ind></trd>
    </trdgrp>
    <trd lng="nl">pijn doen aan de tanden of aan de zenuwen</trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_y;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol; <ind>&c_o;&c_s;&c_k;&c_o;&c_m;&c_i;&c_n;&c_u;</ind></trd>,
      <trd>&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="agac.0i.inciti">
    <ref tip="dif" cel="incit.0i.kolerigi">inciti</ref>
    <trd lng="br">hegazi&ntilde;</trd>
    <trd lng="nl">op de <ind>zenuwen</ind> werken</trd>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol;</trd>
  </snc>
</drv>
</art>
