<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: agac.xml,v 1.28 2021/02/28 14:48:40 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>agac</rad>/o
</kap>

<drv mrk="agac.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="agac.0o.FIZL">
    <dif>
      Malagrabla, incita sento &ccirc;e la dentoj a&ubreve;
      &ccirc;e la nervaro.
    </dif>
  </snc>
  <trd lng="fr">agacement <klr>(des dents, des nerfs)</klr></trd>
  <trdgrp lng="he">
    <trd>&#x5D7;&#x5E8;&#x5D9;&#x5E7;&#x5EA; (&#x5E9;&#x5D9;&#x5E0;&#x5D9;&#x5D9;&#x5DD;</trd>,
    <trd>&#x5E6;&#x5D9;&#x5E4;&#x5D5;&#x5E8;&#x5DF;)</trd>
  </trdgrp>
</drv>

<drv mrk="agac.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="agac.0i.dento">
    <dif>
      Ka&ubreve;zi <tld/>on,
      <ref tip="vid" cel="irit.0i.MED">iriti</ref>,
      forte <ref tip="vid" cel="juk.0i.KOMUNE">juki</ref>:
      <ekz>
        frotado de fajlilo <tld/>as;
      </ekz>
      <ekz>
        la patroj man&gcirc;is nematurajn vinberojn, kaj la dentoj de la
        filoj <tld/>i&gcirc;is
        <fnt><bib>MT</bib>, <lok>&Jer; 31:29</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_b;&c_ib;&c_v;&c_a;&c_c;&c_mol; <ind>&c_a;&c_s;&c_k;&c_o;&c_m;&c_ib;&c_n;&c_u;</ind></trd>,
      <trd>&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_n;&c_ja;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="br">tozona&ntilde;</trd>
    <trd lng="de">reizen</trd>
    <trdgrp lng="en">
      <trd>aggravate</trd>,
      <trd>annoy</trd>,
      <trd>irritate</trd>
    </trdgrp>
    <trd lng="es">causar dentera</trd>
    <trdgrp lng="fr">
      <trd>agacer <klr>(les dents)</klr></trd>,
      <trd>porter sur les <ind>nerfs</ind></trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>allappare</trd>,
      <trd>stridere<klr> (fig.)</klr></trd>,
      <trd>irritare</trd>,
      <trd>dare sui nervi</trd>
    </trdgrp>
    <trd lng="nl">pijn doen aan de tanden of aan de zenuwen</trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_y;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol; <ind>&c_o;&c_s;&c_k;&c_o;&c_m;&c_i;&c_n;&c_u;</ind></trd>,
      <trd>&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="agac.0i.inciti">
    <dif>
      <ref tip="dif" cel="incit.0i.kolerigi">Inciti<sncref/></ref>.
    </dif>
    <trd lng="be">&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_n;&c_ja;&c_c;&c_mol;</trd>
    <trd lng="br">hegazi&ntilde;</trd>
    <trd lng="nl">op de <ind>zenuwen</ind> werken</trd>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="cs">trnout <klr>(zuby)</klr></trd>
  <trdgrp lng="he">
    <trd>&#x5DC;&#x5D7;&#x5E8;&#x5D5;&#x5E7; (&#x5E9;&#x5D9;&#x5E0;&#x5D9;&#x5D9;&#x5DD;</trd>,
    <trd>&#x5E6;&#x5E4;&#x5D5;&#x5E8;&#x5E0;&#x5D9;&#x5D9;&#x5DD;)</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>st&racute;pnu&tcaron;</trd>,
    <trd>t&racute;pnu&tcaron;</trd>
  </trdgrp>
</drv>
</art>
