<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rikecioz.xml,v 1.6 2022/11/07 23:36:17 revo Exp $">
<kap>
  <rad>rikecioz</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="rikecioz.0o">
  <kap><tld/>o</kap>
  <snc mrk="rikecioz.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Iu el la infektaj
      <ref tip="lst" cel="inflam.0o" lst="voko:malsanoj">malsanoj</ref>
      ka&ubreve;zataj de
      <ref tip="vid" cel="rikeci.0o.ZOO">rikecioj</ref>,
      interalie kaj plej dan&gcirc;ere ekzantema tifo:
      <ekz>
        <tld/>o povas mortigi, precipe infanon
        <fnt>
          <aut>J.-L. Tortel</aut>,
          <vrk><url ref="&Sezonoj;2013/10/recenzo-25/">
          Homarano: Nepre havenda libro</url></vrk>,
          <lok>La Ondo de Esperanto, 2013:11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_y;&c_k;&c_je;&c_c;&c_y;&c_jo;&c_z;</trd>
  <trd lng="en">rickettsiose</trd>
  <trd lng="fr">rickettsiose</trd>
  <trd lng="pl">riketsjoza</trd>
  <trd lng="ro">rickettioza</trd>
  <trd lng="uk">&c_r;&c_i;&#x43A;&c_je;&c_t;&c_s;&c_ib;&#x43E;&c_z;</trd>
</drv>
</art>

