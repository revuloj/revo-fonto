<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>vot</rad>/o
</kap>
<drv mrk="vot.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Promeso farita al Dio plenumi meritan agon, se li konsentas ian
      peton; sanktpromeso.
    </dif>
    <trd lng="de">Gel&ouml;bnis</trd>
  </snc>
  <snc>
    <dif>
      Religia promeso de pastro a&ubreve; mona&hcirc;o sin dedi&ccirc;i al la dia servo.
    </dif>
  </snc>
  <trd lng="de">Gel&uuml;bde</trd>
  <trd lng="es">voto</trd>
  <trd lng="nl">gelofte</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_je;&c_t;</trd>,
    <trd>&c_z;&c_a;&c_r;&c_o;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="vot.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Promesi per <tld/>o.
    </dif>
  </snc>
  <trd lng="de">geloben</trd>
  <trdgrp lng="es">
    <trd>hacer un <ind>voto</ind> o votos</trd>,
    <trd>prometer</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>een <ind>gelofte</ind> afleggen</trd>,
    <trd>plechtig <ind>beloven</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_a;&c_t;&c_mol; <ind>&c_o;&c_b;&c_je;&c_t;</ind></trd>,
    <trd>&c_z;&c_a;&c_r;&c_je;&c_ch;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
