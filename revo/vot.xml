<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>vot</rad>/o
</kap>
<drv mrk="vot.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">REL</uzo>
  <snc>
    <dif>
      Sankta <ref tip="super" cel="promes.0o">promeso</ref> plenumi ion a&ubreve;
      sin deteni de io.
    </dif>
    <trd lng="de">Gel&ouml;bnis</trd>
    <trd lng="fr">voeu<klr> (rel.)</klr></trd>
  </snc>
  <snc>
    <dif>
      Promeso de nova <ref tip="vid" cel="klerik.0o">kleriko</ref> sin dedi&ccirc;i al dia servo.
    </dif>
    <trd lng="fr">voeux<klr> (rel.)</klr></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_ja;&c_c;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_z;&c_a;&c_r;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Gel&uuml;bde</trd>
  <trd lng="el">&epsilon;&ypsilon;&chi;&eta_ton;</trd>
  <trd lng="en">vow</trd>
  <trd lng="es">voto</trd>
  <trd lng="hu">fogadalom</trd>
  <trd lng="nl">gelofte</trd>
  <trd lng="pt">voto</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_je;&c_t;</trd>,
    <trd>&c_z;&c_a;&c_r;&c_o;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="vot.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Sankte promesi iun agadon, sindedi&ccirc;on a&ubreve; sindetenon.
    </dif>
    <refgrp tip="vid">
      <ref cel="dedicx.0i.destini">dedi&ccirc;i <sncref/></ref>
      <ref cel="ofer.0i.REL">oferi <sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_c;&c_mol;
    <ind>&c_a;&c_b;&c_ja;&c_c;&c_a;&c_n;&c_mol;&c_n;&c_je;</ind></trd>,
    <trd>&c_z;&c_a;&c_r;&c_a;&c_ch;&c_y;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="de">geloben</trd>
  <trd lng="el">&epsilon;&ypsilon_ton;&chi;&omikron;&my;&alfa;&jota;</trd>
  <trd lng="en">vow</trd>
  <trdgrp lng="es">
    <trd>hacer un <ind>voto</ind> o votos</trd>,
    <trd>prometer</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire <ind>voeu</ind></trd>,
    <trd>prononcer ses <ind>voeux</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fogadkozik</trd>,
    <trd>fogadalmat tesz</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>een <ind>gelofte</ind> afleggen</trd>,
    <trd>plechtig <ind>beloven</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>votar</trd>,
    <trd>fazer votos</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_a;&c_t;&c_mol; <ind>&c_o;&c_b;&c_je;&c_t;</ind></trd>,
    <trd>&c_z;&c_a;&c_r;&c_je;&c_ch;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
