<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>salpetr</rad>/o
</kap>
<drv mrk="salpet.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">KEM</uzo>
    <dif>
      KNO<sub>3</sub>. Kalia azotato, kiu formi&gcirc;as nature sur malsekaj
      muroj kaj estas uzata por produkti azotikan acidon kaj fabriki
      pulvon.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_l;&c_je;&c_t;&c_r;&c_a;</trd>
  <trd lng="cs">ledek draselný</trd>
  <trd lng="de">Salpeter</trd>
  <trd lng="fr">salp&ecirc;tre</trd>
  <trd lng="hu">sal&eacute;trom</trd>
  <trd lng="it">salnitro</trd>
  <trd lng="nl">salpeter</trd>
  <trd lng="ru">&c_s;&c_je;&c_l;&c_i;&c_t;&c_r;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>liadok draselný</trd>,
    <trd>sanitra</trd>
  </trdgrp>
</drv>
</art>
