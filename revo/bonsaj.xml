<drv mrk="bonsaj.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HOR</uzo>
  <snc mrk="bonsaj.0o.arbo">
    <dif>
      <ref tip="super" cel="arb.0o.BOT">Arbo</ref> kulturita en ujo kiel 
      malgranda <ref tip="super" cel="model.0o.imitajxo">modelo</ref> de natura arbo:
      <ekz>
        oni dorlotas la bonsajon per &ccirc;iutaga &scirc;prucado, ta&ubreve;ga sterko, kontrolo de la
        temperaturo kaj lumo en la &ccirc;ambro
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2007/009228.php">Kultivi bonsajon</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        eblas viziti Zen-&gcirc;ardenon a&ubreve; &ccirc;eesti kurseton pri <tld/>o-arbetoj
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2010/010132.php">Monda kulturo apud la
          maro</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/22/Japanese_Black_Pine%2C_1936-2007.jpg/566px-Japanese_Black_Pine%2C_1936-2007.jpg" prm="CC BY-SA 4.0"><tld/>o de Nigra pino
        <fnt>
          <aut>Ragesoss, propra verko</aut>
          <lok><url ref="&WCW;/File:Japanese_Black_Pine,_1936-2007.jpg">&WCW;/File:Japanese_Black_Pine,_1936-2007.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="zh">
    <trd>&#x76C6;&#x666F; <pr>p&#xE9;nj&#x1D0;ng</pr></trd>,
    <trd>&#x76C6;&#x666F;&#x6811; <pr>p&#xE9;nj&#x1D0;ngsh&#xF9;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_b;&c_a;&c_n;&c_s;&c_a;&c_j;</trd>
  <trd lng="de">Bonsai</trd>
  <trd lng="en">Bonsai</trd>
  <trd lng="fr">Bonsa&iuml;</trd>
  <trd lng="uk">&#x43A;&c_a;&c_r;&#x43B;&c_i;&#x43A;&#x43E;&c_v;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&#x43E;/&c_d;&c_je;&c_r;&c_je;&c_v;&c_c;&c_je;</trd>
</drv>

