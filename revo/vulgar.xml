<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vulgar.xml,v 1.24 2022/05/06 12:17:22 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>vulgar</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vulgar.0a">
  <kap><tld/>a</kap>
  <snc mrk="vulgar.0a.popola">
    <dif>
      &Gcirc;enerale, <ref tip="sin" cel="ordina.0a">ordinar</ref>e uzata:
      <ekz>
        la nomo virgulino estas sufi&ccirc;e <tld/>a
        <fnt>
          <bib>Fab3</bib>
          <lok>Du pavimbatiloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        duonklera, <tld/>a, sed ne malbona homo
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>.
      </ekz>
    </dif>
   <trdgrp lng="be">
    <trd>&c_z;&c_v;&c_y;&c_k;&c_l;&c_y;</trd>,
      <trd>&c_b;&c_a;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
      <trd>&c_a;&c_g;&c_u;&c_l;&c_mol;&c_n;&c_a;&c_p;&c_r;&c_y;&c_n;&c_ja;&c_t;&c_y;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_w;&c_s;&c_ju;&c_d;&c_zh;&c_a;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="bg">
      <trd>&c_v;&c_u;&c_l;&c_g;&c_a;&c_r;&c_je;&c_n;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_s;&c_t;</trd>,
      <trd>&c_n;&c_a;&c_r;&c_o;&c_d;&c_je;&c_n;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>banal</trd>,
      <trd>corrent</trd>
    </trdgrp>
    <trd lng="de">gew&ouml;hnlich</trd>
    <trdgrp lng="en">
      <trd>common</trd>,
      <trd>proletarian</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>vernaculaire</trd>,
      <trd>vulgaire</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>k&ouml;z&ouml;ns&eacute;ges</trd>,
      <trd>szokv&aacute;nyos</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>gebruikelijk</trd>,
      <trd>gewoon</trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_b;&c_shch;&c_je;&c_p;&c_r;&c_i;&c_n;&c_ja;&c_t;&c_y;&c_j;</trd>
    <trdgrp lng="sv">
      <trd>vanlig</trd>,
      <trd>allm&auml;n</trd>
    </trdgrp>
  </snc>
  <snc mrk="vulgar.0a.maldelikata">
    <dif>
      <ref tip="sin" cel="trivia.0a">Triviala</ref>, 
      <ref tip="sin" cel="delika.mal0a.iu">maldelikata</ref>:
      <ekz>
        mizera vagabondo <klr>[&#x2026;]</klr> perversa, e&ccirc; <tld/>a
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
          <lok>Al Paul Verlaine</lok>
        </fnt>;
      </ekz>
      <ekz>
        vidante &ccirc;i tiujn fremdulojn, fortajn, malgraciajn, <tld/>e ridantajn, odorantajn
        sebon kaj parolantajn nekonatan kaj malmolan lingvon, <klr>[li]</klr> ekbolis
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi esperas, ke vi ne fari&gcirc;os edzino de &ccirc;i tiu <tld/>ulo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_u;&c_l;&c_mol;&c_g;&c_a;&c_r;&c_n;&c_y;</trd>,
      <trd>&c_p;&c_o;&c_sh;&c_l;&c_y;</trd>,
      <trd>&c_n;&c_je;&c_d;&c_a;&c_l;&c_ib;&c_k;&c_a;&c_t;&c_n;&c_y;</trd>,
      <trd>&c_p;&c_l;&c_ju;&c_g;&c_k;&c_ib;</trd>
    </trdgrp>
    <trd lng="bg">&c_p;&c_r;&c_o;&c_s;&c_t;&c_a;&c_sh;&c_k;&c_i;</trd>
    <trdgrp lng="ca">
      <trd>ordinari</trd>,
      <trd>groller</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>grossier</trd>,
      <trd>trivial</trd>,
      <trd>vulgaire</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>goromba</trd>,
      <trd>mosdatlan</trd>,
      <trd>vulg&aacute;ris</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_u;&c_l;&c_mol;&c_g;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_p;&c_o;&c_sh;&c_l;&c_y;&c_j;</trd>
    </trdgrp>
    <trd lng="sv">vulg&auml;r</trd>
  </snc>
  <refgrp tip="vid">
    <ref cel="banal.0a">banala</ref>,
    <ref cel="trivia.0a">triviala</ref>,
    <ref cel="primit.0a">primitiva</ref>,
    <ref cel="perver.0a">perversa</ref>,
    <ref cel="profan.0a">profana</ref>,
    <ref cel="proz.0a.FIG">proza<sncref/></ref>
  </refgrp>
  <trd lng="ca">vulgar</trd>
  <trdgrp lng="cs">
    <trd>lidov&yacute;</trd>,
    <trd>neomalen&yacute;</trd>,
    <trd>oby&ccaron;ejn&yacute;</trd>,
    <trd>sprost&yacute;</trd>,
    <trd>vulg&aacute;rn&iacute;</trd>,
    <trd>zjednodu&scaron;en&yacute;</trd>
  </trdgrp>
  <trd lng="de">vulg&auml;r</trd>
  <trd lng="en">vulgar</trd>
  <trd lng="es">vulgar</trd>
  <trdgrp lng="nl">
    <trd>vulgair</trd>,
    <trd>grof<klr tip="ind"> (vulgair)</klr></trd>
  </trdgrp>
  <trd lng="pt">vulgar</trd>
  <trdgrp lng="sk">
    <trd>neomalen&yacute;</trd>,
    <trd>oby&ccaron;ajn&yacute;</trd>,
    <trd>vulg&aacute;rny</trd>,
    <trd>v&scaron;eobecn&yacute;</trd>,
    <trd>zjednodu&scaron;en&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x443;&#x43B;&#x44C;&#x491;&#x430;&#x440;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x43F;&#x430;&#x441;&#x43A;&#x443;&#x434;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x43D;&#x435;&#x43F;&#x440;&#x438;&#x441;&#x442;&#x43E;&#x439;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x437;&#x430;&#x433;&#x430;&#x43B;&#x44C;&#x43D;&#x43E;&#x43F;&#x440;&#x438;&#x439;&#x43D;&#x44F;&#x442;&#x438;&#x439;</trd>,
    <trd>&#x443;&#x437;&#x432;&#x438;&#x447;&#x430;&#x454;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x441;&#x43F;&#x440;&#x43E;&#x449;&#x435;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="vulgar.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="vulgar.0eco.KOMUNE">
    <dif>
      Eco de tio, kio estas <tld/>a, banala, maldelikata:
      <ekz>
        la cindro, kiu falis el liaj haroj, la <tld/>eco de la &ccirc;ifona vesto, &ccirc;io &ccirc;i
        estis por li abomena
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        li volas sufoki la <tld/>econ kaj mizerecon
        de la ordinara vivo 
        <fnt>
          <aut>H. Sienkewicz, trad. L. Zamenhof</aut>, <vrk>Quo 
          vadis?</vrk>, <lok>1933</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>lidovost</trd>,
    <trd>spros&tcaron;&aacute;ctv&iacute;</trd>,
    <trd>vulgarita</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>trivialit&eacute;</trd>,
    <trd>vulgarit&eacute;</trd>
  </trdgrp>
  <trd lng="sk">vulgarita</trd>
  <trd lng="uk">&#x432;&#x443;&#x43B;&#x44C;&#x491;&#x430;&#x440;&#x43D;&#x456;&#x441;&#x442;&#x44C;</trd>
</drv>

<drv mrk="vulgar.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vulgar.0igi.konigi">
    <dif>
      Vaste konigi, uzigi;
      igi ordinara komunuza:
      <ekz>
        <tld/>igi novan eltrovon.
      </ekz>
    </dif>
    <trd lng="fr">vulgariser</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>vulgarizovat</trd>,
    <trd>zjednodu&scaron;it</trd>,
    <trd>zkreslit</trd>,
    <trd>zpopularizovat</trd>,
    <trd>zvulgarizovat</trd>
  </trdgrp>
  <trd lng="sk">zvulgarizova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x432;&#x443;&#x43B;&#x44C;&#x491;&#x430;&#x440;&#x438;&#x437;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x441;&#x43F;&#x440;&#x43E;&#x449;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="vulgar.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="vulgar.mal0a.KOMUNE">
    <dif>
      Kiu ne <tld/>as, ne banalas, ne krudas,
      sed elegante distingi&gcirc;as;
      <ref tip="vid" cel="rafin.0ita">rafinita</ref>.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>distingu&eacute;<klr> (recherch&eacute;)</klr></trd>,
    <trd>recherch&eacute;<klr> (distingu&eacute;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="vulgar.mal0eco">
  <kap>mal<tld/>eco</kap>
  <snc mrk="vulgar.mal0eco.KOMUNE">
    <dif>
      Elita, eleganta sindistingo disde ordinara&jcirc;oj:
      <ekz>
        &scirc;i estis bongustege vestita kaj faris al mi 
        impreson de senkompara mal<tld/>eco
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Olivia_Shakespear">
          Olivia Shakespear</url>, 2013-04-01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">distinction<klr> (&eacute;l&eacute;gance)</klr></trd>
</drv>
</art>
