<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>vulgar</rad>/a
</kap>
<drv mrk="vulgar.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      &Gcirc;enerale, ordinare uzata:
      <ekz>
        <tld/>a anta&ubreve;ju&gcirc;o, eraro;
      </ekz>
      <ekz>
        <tld/>a lingvo
        <klr>(bu&scirc;a, kontraste kun la skriba)</klr>;
      </ekz>
      <ekz>
        <tld/>e akceptita traduko;
      </ekz>
      <ekz>
        <tld/>igi novan eltrovon.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="banal.0a">banala</ref>.
    </refgrp>
   <trdgrp lng="be">
    <trd>&c_z;&c_v;&c_y;&c_k;&c_l;&c_y;</trd>,
      <trd>&c_b;&c_a;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
      <trd>&c_a;&c_g;&c_u;&c_l;&c_mol;&c_n;&c_a;&c_p;&c_r;&c_y;&c_n;&c_ja;&c_t;&c_y;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_w;&c_s;&c_ju;&c_d;&c_zh;&c_a;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="de">gew&ouml;hnlich</trd>
    <trdgrp lng="en">
      <trd>common</trd>,
      <trd>proletarian</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>banal</trd>,
      <trd>vulgaire</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>k&ouml;z&ouml;ns&eacute;ges</trd>,
      <trd>szokv&aacute;nyos</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>gebruikelijk</trd>,
      <trd>gewoon</trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_b;&c_shch;&c_je;&c_p;&c_r;&c_i;&c_n;&c_ja;&c_t;&c_y;&c_j;</trd>
  </snc>
  <snc>
    <dif>
      Triviala, maldelikata:
      <ekz>
        fremduloj malgraciaj, odorantaj sebon kaj <tld/>e
        ridantaj<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_u;&c_l;&c_mol;&c_g;&c_a;&c_r;&c_n;&c_y;</trd>,
      <trd>&c_p;&c_o;&c_sh;&c_l;&c_y;</trd>,
      <trd>&c_n;&c_je;&c_d;&c_a;&c_l;&c_ib;&c_k;&c_a;&c_t;&c_n;&c_y;</trd>,
      <trd>&c_p;&c_l;&c_ju;&c_g;&c_k;&c_ib;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>goromba</trd>,
      <trd>mosdatlan</trd>,
      <trd>vulg&aacute;ris</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_u;&c_l;&c_mol;&c_g;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_p;&c_o;&c_sh;&c_l;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="de">vulg&auml;r</trd>
  <trd lng="en">vulgar</trd>
  <trd lng="es">vulgar</trd>
  <trdgrp lng="fr">
    <trd>grossier</trd>,
    <trd>trivial</trd>,
    <trd>vulgaire</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vulgair</trd>,
    <trd>grof<klr tip="ind"> (vulgair)</klr></trd>
  </trdgrp>
  <trd lng="pt">vulgar</trd>
</drv>
</art>
