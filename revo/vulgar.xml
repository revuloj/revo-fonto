<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>vulgar</rad>/a
</kap>
<drv mrk="vulgar.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      &Gcirc;enerale, ordinare uzata:
      <ekz>
        <tld/>a anta&ubreve;ju&gcirc;o, eraro;
      </ekz>
      <ekz>
        <tld/>a lingvo
        <klr>(bu&scirc;a, kontraste kun la skriba)</klr>;
      </ekz>
      <ekz>
        <tld/>e akceptita traduko;
      </ekz>
      <ekz>
        <tld/>igi novan eltrovon.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="banal.0a">banala</ref>.
    </refgrp>
    <trd lng="de">gew&ouml;hnlich</trd>
    <trdgrp lng="fr">
      <trd>banal</trd>,
      <trd>vulgaire</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>gebruikelijk</trd>,
      <trd>gewoon</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Triviala, maldelikata:
      <ekz>
        fremduloj malgraciaj, odorantaj sebon kaj <tld/>e ridantaj<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">vulg&auml;r</trd>
  <trd lng="es">vulgar</trd>
  <trdgrp lng="fr">
    <trd>grossier</trd>,
    <trd>trivial</trd>,
    <trd>vulgaire</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vulgair</trd>,
    <trd>grof<klr tip="ind"> (vulgair)</klr></trd>
  </trdgrp>
</drv>
</art>
