<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vulgar.xml,v 1.21 2015/07/04 08:37:46 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>vulgar</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vulgar.0a">
  <kap><tld/>a</kap>
  <snc mrk="vulgar.0a.popola">
    <dif>
      &Gcirc;enerale, ordinare uzata:
      <ekz>
        <tld/>a anta&ubreve;ju&gcirc;o, eraro;
      </ekz>
      <ekz>
        <tld/>a lingvo
        <klr>(bu&scirc;a, kontraste kun la skriba)</klr>;
      </ekz>
      <ekz>
        <tld/>e akceptita traduko.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="banal.0a">banala</ref>.
    </refgrp>
   <trdgrp lng="be">
    <trd>&c_z;&c_v;&c_y;&c_k;&c_l;&c_y;</trd>,
      <trd>&c_b;&c_a;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
      <trd>&c_a;&c_g;&c_u;&c_l;&c_mol;&c_n;&c_a;&c_p;&c_r;&c_y;&c_n;&c_ja;&c_t;&c_y;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_w;&c_s;&c_ju;&c_d;&c_zh;&c_a;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="bg">
      <trd>&c_v;&c_u;&c_l;&c_g;&c_a;&c_r;&c_je;&c_n;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_s;&c_t;</trd>,
      <trd>&c_n;&c_a;&c_r;&c_o;&c_d;&c_je;&c_n;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>banal</trd>,
      <trd>corrent</trd>
    </trdgrp>
    <trd lng="de">gew&ouml;hnlich</trd>
    <trdgrp lng="en">
      <trd>common</trd>,
      <trd>proletarian</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>vernaculaire</trd>,
      <trd>vulgaire</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>k&ouml;z&ouml;ns&eacute;ges</trd>,
      <trd>szokv&aacute;nyos</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>gebruikelijk</trd>,
      <trd>gewoon</trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_b;&c_shch;&c_je;&c_p;&c_r;&c_i;&c_n;&c_ja;&c_t;&c_y;&c_j;</trd>
    <trdgrp lng="sv">
      <trd>vanlig</trd>,
      <trd>allm&auml;n</trd>
    </trdgrp>
  </snc>
  <snc mrk="vulgar.0a.maldelikata">
    <dif>
      Triviala, maldelikata:
      <ekz>
        fremduloj malgraciaj, odorantaj sebon kaj <tld/>e
        ridantaj<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_u;&c_l;&c_mol;&c_g;&c_a;&c_r;&c_n;&c_y;</trd>,
      <trd>&c_p;&c_o;&c_sh;&c_l;&c_y;</trd>,
      <trd>&c_n;&c_je;&c_d;&c_a;&c_l;&c_ib;&c_k;&c_a;&c_t;&c_n;&c_y;</trd>,
      <trd>&c_p;&c_l;&c_ju;&c_g;&c_k;&c_ib;</trd>
    </trdgrp>
    <trd lng="bg">&c_p;&c_r;&c_o;&c_s;&c_t;&c_a;&c_sh;&c_k;&c_i;</trd>
    <trdgrp lng="ca">
      <trd>ordinari</trd>,
      <trd>groller</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>grossier</trd>,
      <trd>trivial</trd>,
      <trd>vulgaire</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>goromba</trd>,
      <trd>mosdatlan</trd>,
      <trd>vulg&aacute;ris</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_u;&c_l;&c_mol;&c_g;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_p;&c_o;&c_sh;&c_l;&c_y;&c_j;</trd>
    </trdgrp>
    <trd lng="sv">vulg&auml;r</trd>
  </snc>
  <trd lng="ca">vulgar</trd>
  <trdgrp lng="cs">
    <trd>lidový</trd>,
    <trd>neomalený</trd>,
    <trd>obyčejný</trd>,
    <trd>sprostý</trd>,
    <trd>vulgární</trd>,
    <trd>zjednodušený</trd>
  </trdgrp>
  <trd lng="de">vulg&auml;r</trd>
  <trd lng="en">vulgar</trd>
  <trd lng="es">vulgar</trd>
  <trdgrp lng="nl">
    <trd>vulgair</trd>,
    <trd>grof<klr tip="ind"> (vulgair)</klr></trd>
  </trdgrp>
  <trd lng="pt">vulgar</trd>
  <trdgrp lng="sk">
    <trd>neomalený</trd>,
    <trd>obyčajný</trd>,
    <trd>vulgárny</trd>,
    <trd>všeobecný</trd>,
    <trd>zjednodušený</trd>
  </trdgrp>
</drv>

<drv mrk="vulgar.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="vulgar.0eco.KOMUNE">
    <dif>
      Eco de tio, kio estas <tld/>a, banala, maldelikata:
      <ekz>
        li volas sufoki la <tld/>econ kaj mizerecon
        de la ordinara vivo 
        <fnt><aut>H. Sienkewicz, trad. L. Zamenhof</aut>, <vrk>Quo 
        vadis?</vrk>, <lok>1933</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>lidovost</trd>,
    <trd>sprosťáctví</trd>,
    <trd>vulgarita</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>trivialit&eacute;</trd>,
    <trd>vulgarit&eacute;</trd>
  </trdgrp>
  <trd lng="sk">vulgarita</trd>
</drv>

<drv mrk="vulgar.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vulgar.0igi.konigi">
    <dif>
      Vaste konigi, uzigi;
      igi ordinara komunuza:
      <ekz>
        <tld/>igi novan eltrovon.
      </ekz>
    </dif>
    <trd lng="fr">vulgariser</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>vulgarizovat</trd>,
    <trd>zjednodušit</trd>,
    <trd>zkreslit</trd>,
    <trd>zpopularizovat</trd>,
    <trd>zvulgarizovat</trd>
  </trdgrp>
  <trd lng="sk">zvulgarizovať</trd>
</drv>

<drv mrk="vulgar.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="vulgar.mal0a.KOMUNE">
    <dif>
      Kiu ne <tld/>as, ne banalas, ne krudas,
      sed elegante distingi&gcirc;as;
      <ref tip="vid" cel="rafin.0ita">rafinita</ref>.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>distingu&eacute;<klr> (recherch&eacute;)</klr></trd>,
    <trd>recherch&eacute;<klr> (distingu&eacute;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="vulgar.mal0eco">
  <kap>mal<tld/>eco</kap>
  <snc mrk="vulgar.mal0eco.KOMUNE">
    <dif>
      Elita, eleganta sindistingo disde ordinara&jcirc;oj:
      <ekz>
        &scirc;i estis bongustege vestita kaj faris al mi 
        impreson de senkompara mal<tld/>eco
        <fnt>
          <bib>Viki</bib>,
          <lok><url
          ref="http://eo.wikipedia.org/wiki/Olivia_Shakespear">
          Olivia Shakespear</url>, 2013-04-01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">distinction<klr> (&eacute;l&eacute;gance)</klr></trd>
</drv>
</art>
