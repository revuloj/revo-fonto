<vortaro>
<art mrk="$Id$">
<kap>
  <rad>beletr</rad>/o
</kap>
<drv mrk="beletr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELE</uzo>
  <snc>
    <dif>
      Literaturo la&ubreve; la artisma vidpunkto:
      <ekz><tld/>isto<fnt>Z</fnt>.</ekz>
    </dif>
    <ref tip="sin" cel="litera.bela0o">bela literaturo;</ref>
    <ref tip="vid" cel="beletr1.0o">beletristiko.</ref>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_s;&c_t;&c_a;&c_c;&c_k;&c_a;&c_ja; &c_l;&c_ib;&c_t;&c_a;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
  <trdgrp lng="en">
     <trd>literature</trd>,
     <trd>letters</trd>,
     <trd>belle lettres</trd></trdgrp>
  <trd lng="es">letras</trd>
  <trdgrp lng="fr">
     <trd>art de l'&eacute;crivain</trd>,
     <trd>litt&eacute;rature <klr tip="ind">(art de l'&eacute;crivain)</klr></trd>,
     <trd>lettres <klr tip="ind">(art de l'&eacute;crivain)</klr></trd></trdgrp>
  <trd lng="hu">sz&eacute;pirodalom</trd>
  <trd lng="ru">&c_h;&c_u;&c_d;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_a;&c_ja; &c_l;&c_i;&c_t;&c_je;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
  <trd lng="sk">beletria</trd>
</drv>
</art>
