<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: beletr.xml,v 1.28 2022/05/14 06:53:30 revo Exp $">
<kap>
  <ofc>9</ofc>
  <rad>beletr</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="beletr.0o">
  <kap><ofc>9</ofc><tld/>o <fnt><bib>PV</bib></fnt></kap>
  <snc mrk="beletr.0o.BELE">
    <uzo tip="fak">BELE</uzo>
    <dif>
      Arta literaturo plezurocela a&ubreve; distrocela, precipe tia,
      kiu rakontas pri okaza&jcirc;oj kaj personoj imagitaj.
      <!-- Literaturo la&ubreve; la artisma vidpunkto.-->
      <ekz>
        &ccirc;iu numero enhavas 20 pa&gcirc;ojn de <tld/>o de Mikaelo
        <nom>Gi&scirc;pling</nom>, 
        Mikaelo <nom>Bron&scirc;tejn</nom>, <klr>[&#8230;]</klr> kaj aliaj maturaj
        a&ubreve;toroj
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk><url ref="&Steloj;monato2/006807.html">Fascina
          gazeto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Esperanto-<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Reinaldo Marcelo Ferreira</aut>
          <vrk><url ref="&Monato;/2005/008629.php">Ne nur
          intimaj temoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la Biblio, pro historiaj ka&ubreve;zoj, en la e&ubreve;ropa kulturo estas
        konstanta fonto de temoj por la arto, inkluzive <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="&Monato;/2003/007814.html">Malnova
          biblia epizodo en modernaj vestoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li studis filozofion kaj <tld/>on kaj da&ubreve;rigis sian poezian
        verkadon
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="&Monato;/2004/008467.html">Kampoj de
          Soria Antonio Machado, universala poeto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        per la rimedoj de <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="&Monato;/2004/008392.html">Reveno de
          heroo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="litera.bela0o">bela literaturo</ref>
    <ref tip="vid" cel="beletr1.0o.BELE">beletristiko <sncref/></ref>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_s;&c_t;&c_a;&c_c;&c_k;&c_a;&c_ja;
  &c_l;&c_ib;&c_t;&c_a;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
  <trd lng="de">Belletristik</trd>
  <trdgrp lng="en">
    <trd>literature</trd>,
    <trd>letters</trd>,
    <trd>belles lettres</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>caligraf&iacute;a</trd>,
    <trd>literatura</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>art de l'&eacute;crivain</trd>,
    <trd>litt&eacute;rature<klr tip="ind"> (art de
    l'&eacute;crivain)</klr></trd>,
    <trd>lettres<klr tip="ind"> (art de l'&eacute;crivain)</klr></trd>
  </trdgrp>
  <trd lng="he">&#x5E1;&#x5B4;&#x5E4;&#x5B0;&#x5E8;&#x5D5;&#x5BC;&#x5EA;</trd>
  <trdgrp lng="it">
    <trd>lettere</trd>,
    <trd>belle lettere</trd>
  </trdgrp>
  <trd lng="pl">beletrystyka</trd>
  <trd lng="hu">sz&eacute;pirodalom</trd>
  <trd lng="ru">&c_h;&c_u;&c_d;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_a;&c_ja;
  &c_l;&c_i;&c_t;&c_je;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
  <trd lng="sk">beletria</trd>
  <trdgrp lng="uk">
    <trd>&c_h;&c_u;&c_d;&#x43E;&c_zh;&#x43D;&#x44F; &#x43B;&c_ib;&c_t;&c_je;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>,
    <trd>&#x43A;&c_r;&c_a;&c_s;&#x43D;&c_je; &#x43F;&c_i;&c_s;&#x44C;&#x43C;&c_je;&#x43D;&c_s;&c_t;&c_v;&#x43E;</trd>
  </trdgrp>
</drv>

<drv mrk="beletr.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
       Rilata al <tld/>o:
      <ekz>
        la interbalkana <tld/>a komisiono
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2004/008232.html">Albano
          balkane premiita</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        vera romano &ndash; ampleksa <tld/>a verko kun pluraj
        interplekti&gcirc;antaj enhavfadenoj
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="&Monato;/2004/008392.html">Reveno de
          heroo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a panoramo
        <fnt>
          <bib>Monato</bib>
          <aut>Antonio Val&eacute;n</aut>
          <vrk><url ref="&Monato;/2005/008499.php">&Ccirc;u Ertl
          provizore stilekzercas?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld lit="B"/>a Almanako
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2009/009745.php">Bizara
          romano ... blaisecendrarseska</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_s;&c_t;&c_a;&c_c;&c_k;&c_ib; <klr>(&c_d;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y; &c_m;&c_a;&c_s;&c_t;&c_a;&c_c;&c_k;&c_a;&c_j; &c_l;&c_ib;&c_t;&c_a;&c_r;&c_a;&c_t;&c_u;&c_r;&c_y;)</klr></trd>
  <trd lng="de">belletristisch</trd>
  <trd lng="es">literario</trd>
</drv>

<drv mrk="beletr.0ajxo">
  <kap><ofc>9</ofc><tld/>a&jcirc;o <fnt><bib>PIV1</bib></fnt></kap>
  <snc>
    <dif>
      Verko <tld/>a:
      <ekz>
        <tld/>a&jcirc;oj tradukitaj el la rusa
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk><url ref="&Steloj;monato2/006807.html">Fascina
          gazeto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        balkanpopola <tld/>a&jcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2004/008232.html">Albano
          balkane premiita</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        miaj &scirc;atokupoj estas legado de <tld/>a&jcirc;oj, kinoarto, muziko,
        <klr>[&#8230;]</klr>
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Monato;/2003/008035.html">Evgeni
          Georgiev</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_t;&c_a;&c_r;&c_a;&c_t;&c_u;&c_r;&c_n;&c_y; <ind>&c_t;&c_v;&c_o;&c_r;</ind></trd>
  <trd lng="en">literary work</trd>
  <trdgrp lng="es">
    <trd>pieza literaria</trd>,
    <trd>obra literaria</trd>
  </trdgrp>
  <trd lng="it">opera letteraria</trd>
  <trd lng="pl">dzie&lstroke;o <ind>literackie</ind></trd>
</drv>

<drv mrk="beletr.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="beletr.0ismo.BELE">
    <dif>
      Vigla interesi&gcirc;o a&ubreve; profesia agado
      pri <tld/>o:
      <ekz>
        vi vidas, ke <tld/>ismo ne malutilas e&ccirc; en diplomatiaj
        cirkonstancoj
        <fnt>
          <aut>M. Proust, trad. &Jcirc; Va&scirc;e</aut>
          <vrk><url ref="http://jxvasxe.free.fr/tradukoj/en-ombro-de-florantaj-junulinoj-eo.htm">
          En Ombro de Florantaj Junulinoj</url></vrk>
          <lok>2017-</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd>caligrafismo</trd>
  </trdgrp>
</drv>

<drv mrk="beletr.0isto">
  <kap><ofc>9</ofc><tld/>isto
    <fnt><bib>LR</bib></fnt>
  </kap>
  <snc mrk="beletr.0isto.BELE">
    <dif>
      <tld lit="B"/>a verkisto a&ubreve; specialisto.
      <!-- Literaturisto, kiu verkas artisme: -->
      <ekz>
        la plej a&ubreve;toritata <tld/>isto a&ubreve; filologo kreus &gcirc;in sensence
        <fnt>
          <bib>LR</bib>
          <lok>4. Vortaroj</lok>
        </fnt>;
      </ekz>
      <ekz>klarigo de la &scirc;an&gcirc;o
        nenio en la ri&ccirc;a kultura hereda&jcirc;o de la homaro devas esti
        perdita,
        kaj tio certe ne povas ne inkludi la kontribuon de esperantaj
        <tld/>istoj
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2012/010751.php">El&ccirc;erpita
          trezoro</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;Praga Altliteratura Skolo&#x201D; <klr>[&#8230;]</klr> alprenis
        <klr>[&#8230;]</klr> iom fieran sintenon, neglektan al la alitendencaj
        <tld/>istoj
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2004/008398.html">Ankora&ubreve;foje:
          retrorigardo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_je;&c_l;&c_je;&c_t;&c_r;&c_y;&c_s;&c_t;</trd>,
    <trd>&c_p;&c_ib;&c_s;&c_mol;&c_m;&c_je;&c_n;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Belletrist</trd>,
    <trd>Schriftsteller <klr>(Belletristik)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>author</trd>,
    <trd>writer</trd>,
    <trd>man of letters</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>literato</trd>,
    <trd>escritor</trd>,
    <trd>autor</trd>
  </trdgrp>
  <trd lng="fr">&eacute;crivain</trd>
  <trd lng="he">&#x5DE;&#x5B0;&#x5E9;&#x5C1;&#x5D5;&#x5B9;&#x5E8;&#x5B5;&#x5E8;</trd>
  <trd lng="it">letterato</trd>
  <trdgrp lng="pl">
    <trd>literat</trd>,
    <trd>pisarz</trd>,
    <trd>autor</trd>,
    <trd>beletrysta<klr> (rzadkie)</klr></trd>
  </trdgrp>
</drv>
</art>
