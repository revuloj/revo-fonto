<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sambuk</rad>/o
</kap>
<drv mrk="sambuk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio
      <ref tip="super" cel="kaprif1.0oj">kaprifoliacoj</ref>
      (<trd lng="la">Sambucus</trd>).
      La floroj de la nigra <tld/>o estas uzataj por &scirc;vitiga tizano,
      el la nigraj beroj oni faras sukon a&ubreve; supon.
    </dif>
  </snc>
  <bld lok="../bld/sambuk.jpg">
    floraro kaj folioj de
    <ind>nigra <tld/>o</ind>
    <trd lng="la">Sambucus nigra</trd>
  </bld>
  <trd lng="de">Holunder</trd>
  <trd lng="fr">sureau</trd>
  <trd lng="nl">vlier</trd>
  <trd lng="ru">&c_b;&c_u;&c_z;&c_i;&c_n;&c_a; <klr tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>
</drv>
</art>
