<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>kosekant</rad>/o
</kap>
<drv mrk="koseka.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 20</lok></fnt>
  <snc mrk="koseka.0o.entriangulo">
    <dif>
      <klr>(de <ref cel="angul.0o.MAT">angulo</ref> <frm>&alfa;</frm>)</klr>
      <ref cel="sekant.0o">Sekanto</ref> de &gcirc;ia 
      <ref cel="komple1.0o.angulo">komplemento</ref>; simb. 
      <frm>cosec &alfa;</frm> a&ubreve; <frm>kosek &alfa;</frm>:
      <ekz>
        la <tld/>o de orto egalas al <frm>1</frm>.
      </ekz>
    </dif>
    <bld lok="../bld/trigo.jpg">
      <!-- la <tld/>o de angulo <frm>&alfa;</frm> egalas al <frm>OU/OM</frm> -->
      la <tld/>o de angulo >&alfa; egalas al OU/OM 
    </bld>    
  </snc>
  <snc mrk="koseka.0o.funkcio">
    <dif>
      Reela <ref tip="super" cel="funkci.0o.MAT">funkcio</ref>, kiu &jcirc;etas
      reelon <frm><k>x</k></frm> al la 
      <tld/>o <sncref ref="koseka.0o.entriangulo"/> de angulo, kies mezuro en 
      <ref cel="radian.0o">radianoj</ref> diferencas
      de <frm><k>x</k></frm> per oblo de <frm>2&pi;</frm>; 
      simb. <frm>cosec</frm> a&ubreve; <frm>kosek</frm>:
      <ekz>
        <tld/>o estas perioda funkcio, kun 
        <ref cel="period.0o.funkcio">periodo <sncref/></ref> <frm>2&pi;</frm>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_s;&c_e;&c_k;&c_a;&c_n;&c_s;</trd>
  <trd lng="de">Kosekans</trd>
  <trd lng="en">cosecant</trd>
  <trd lng="es">cosecante</trd>
  <trd lng="fr">cos&eacute;cante</trd>
  <trd lng="nl">cosecans</trd>
  <trd lng="pl">cosecans</trd>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_je;&c_k;&c_a;&c_n;&c_s;</trd>
</drv>

<drv mrk="koseka.ark0o">
  <kap>ark<tld/>o, arko<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      <ref cel="bild.inversa0igo">Inversa bildigo</ref> de
      la <ref cel="vast.mal0igajxo.bildigo">malvastiga&jcirc;o</ref> de
      <ref tip="super" cel="funkci.0o.MAT">funkcio</ref>
      <ref cel="koseka.0o.funkcio"><tld/>o</ref> al
      <frm>[-&pi;/2,0[&#x222a;]0,&pi;/2]</frm>; 
      simb. <frm>arc cosec</frm> a&ubreve; <frm>ark kosek</frm>:
      <ekz>
        ark<tld/>o de <frm>1</frm> egalas al <frm>&pi;/2</frm>.
      </ekz>
    </dif>
    <rim>
      Vd rimarkon sub <ref cel="ark.0o.angulo">arko</ref>.
    </rim>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_k;&c_k;&c_a;&c_s;&c_e;&c_k;&c_a;&c_n;&c_s;</trd>
  <trd lng="de">Arkuskosekans</trd>
  <trd lng="en">arc cosecant</trd>
  <trd lng="fr">arc cos&eacute;cante</trd>
  <trd lng="pl">arcus cosecans</trd>
  <trd lng="ru">&c_a;&c_r;&c_k;&c_k;&c_o;&c_s;&c_je;&c_k;&c_a;&c_n;&c_s;</trd>
</drv>
</art>

