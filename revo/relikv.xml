<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>relikv</rad>/o
</kap>
<drv mrk="relikv.0o">
  <kap><tld/>o</kap>
  <snc mrk="relikv.0o.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Resta&jcirc;o el korpo de mortinta sanktulo, a&ubreve; eventuale alia 
      intima postlasa&jcirc;o de li:
      <ekz>
        lia ceremonia vesto trovi&gcirc;as inter la <tld/>oj de la templo de 
        Amon
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 1a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_m;&c_o;&c_shch;&c_i;</trd>
  </snc>
  <snc mrk="relikv.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Respektinda resta&jcirc;o el io.
    </dif>
  </snc>
  <trd lng="de">Reliquie</trd>
  <trd lng="fr">relique</trd>
  <trdgrp lng="hu">
    <trd>ereklye</trd>,
    <trd>relikvia</trd></trdgrp>
  <trd lng="nl">relikwie</trd>
  <trd lng="ru">&c_r;&c_je;&c_l;&c_i;&c_k;&c_v;&c_i;&c_ja;</trd>
</drv>

<drv mrk="relikv.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="relikv.0ujo.BELA">
    <uzo tip="fak">BELA</uzo>
    <uzo tip="fak">REL</uzo>
    <dif>
      Artisme ornamita kesto por enteni <tld/>ojn.
    </dif>
  </snc>
  <trd lng="de">Reliquiar</trd>
  <trd lng="fr">reliquaire</trd>
  <trdgrp lng="hu">
    <trd>ereklyetart&oacute;</trd>,
    <trd>relikvi&aacute;rium</trd></trdgrp>
  <trd lng="nl">relikwieschrijn</trd>
</drv>
</art>
