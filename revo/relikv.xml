<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: relikv.xml,v 1.21 2024/04/13 20:15:52 revo Exp $">
<kap>
  <rad>relikv</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="relikv.0o">
  <kap><tld/>o</kap>
  <snc mrk="relikv.0o.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Resta&jcirc;o el korpo de mortinta sanktulo, a&ubreve; eventuale alia 
      intima postlasa&jcirc;o de li:
      <ekz>
        lia ceremonia vesto trovi&gcirc;as inter la <tld/>oj de la templo de 
        Amon
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 1a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_m;&c_o;&c_shch;&c_i;</trd>
  </snc>
  <snc mrk="relikv.0o.BIO">
  <uzo tip="fak">BIO</uzo>
    <dif>
      <ref tip="dif" cel="relikt.0o.BIO">Relikto</ref>:
      <ekz>
        la torfejo estas la rifu&gcirc;ejo de multaj insektoj,
        interalie de kvar specioj de libeloj,
        kiuj ja estas glaciepokaj <tld/>oj
        <fnt>
          <aut>[M. Schubnel?]</aut>,
          <vrk><url ref="http://frankenthal.pagesperso-orange.fr/es_fau&amp;flo.html">
          Melkejo Frankenthal, fa&ubreve;no kaj fla&ubreve;ro</url></vrk>,
          <lok>[vidita en 2018]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="relikv.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Respektinda resta&jcirc;o el io.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_l;&c_ib;&c_k;&c_v;&c_ib;&c_ja;</trd>,
    <trd>&c_m;&c_o;&c_sh;&c_ch;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">rel&iacute;quia</trd>
  <trdgrp lng="cs">
    <trd>cenn&eacute; pam&aacute;tky</trd>,
    <trd>ostatky</trd>,
    <trd>poz&uring;statky</trd>,
    <trd>relikvie</trd>
  </trdgrp>
  <trd lng="de">Reliquie</trd>
  <trd lng="en">relic</trd>
  <trd lng="es">reliquia</trd>
  <trd lng="fr">relique</trd>
  <trdgrp lng="hu">
    <trd>ereklye</trd>,
    <trd>relikvia</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8056;&#x907A;&#x7269;  <pr>&#x305B;&#x3044;&#x3044;&#x3076;&#x3064;</pr></trd>,
    <trd>&#x907A;&#x9AA8;  <pr>&#x3044;&#x3053;&#x3064;</pr></trd>,
    <trd>&#x907A;&#x54C1;  <pr>&#x3044;&#x3072;&#x3093;</pr></trd>,
    <trd>&#x907A;&#x5B58;&#x7A2E;  <pr>&#x3044;&#x305D;&#x3093;&#x3057;&#x3085;</pr></trd>,
    <trd>&#x6B8B;&#x5B58;&#x751F;&#x7269;  <pr>&#x3056;&#x3093;&#x305D;&#x3093;&#x305B;&#x3044;&#x3076;&#x3064;</pr></trd>
  </trdgrp>
  <trd lng="nl">relikwie</trd>
  <trd lng="pl">relikwia</trd>
  <trd lng="ro">moa&#x219;te</trd>
  <trd lng="ru">&c_r;&c_je;&c_l;&c_i;&c_k;&c_v;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>drahocenn&eacute; pamiatky</trd>,
    <trd>relikvie</trd>,
    <trd>telesn&eacute; ostatky</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_je;&#x43B;&c_ib;&#x43A;&c_v;&c_ib;&#x44F;</trd>,
    <trd>&#x43C;&#x43E;&c_shch;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5269;&#x9918;&#x7269; <pr>sh&#xE8;ngy&#xFA;w&#xF9;</pr></trd>,
    <trd>&#x5269;&#x9980;&#x7269; <pr>sh&#xE8;ngy&#xFA;w&#xF9;</pr></trd>,
    <trd>&#x5723;&#x4EBA;&#x7684;&#x9057;&#x7269; <pr>sh&#xE8;ngr&#xE9;ndey&#xED;w&#xF9;</pr></trd>,
    <trd>&#x8056;&#x4EBA;&#x7684;&#x907A;&#x7269; <pr>sh&#xE8;ngr&#xE9;ndey&#xED;w&#xF9;</pr></trd>,
    <trd>&#x5723;&#x9AA8; <pr>sh&#xE8;ngg&#x1D4;</pr></trd>,
    <trd>&#x8056;&#x9AA8; <pr>sh&#xE8;ngg&#x1D4;</pr></trd>,
    <trd>&#x9057;&#x5B9D; <pr>y&#xED;b&#x1CE;o</pr></trd>,
    <trd>&#x907A;&#x5BF6; <pr>y&#xED;b&#x1CE;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="relikv.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="relikv.0ujo.BELA">
    <uzo tip="fak">BELA</uzo>
    <uzo tip="fak">REL</uzo>
    <dif>
      Artisme ornamita kesto por enteni <tld/>ojn.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_l;&c_ib;&c_k;&c_v;&c_a;&c_r;</trd>,
    <trd>&c_r;&c_e;&c_l;&c_ib;&c_k;&c_v;&c_a;&c_r;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="ca">reliquiari</trd>
  <trd lng="cs">relikvi&aacute;&rcaron;</trd>
  <trd lng="de">Reliquiar</trd>
  <trdgrp lng="en">
    <trd>reliquary</trd>,
    <trd>casket</trd>,
    <trd>shrine</trd>
  </trdgrp>
  <trd lng="es">relicario</trd>
  <trd lng="fr">reliquaire</trd>
  <trdgrp lng="hu">
    <trd>ereklyetart&oacute;</trd>,
    <trd>relikvi&aacute;rium</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8056;&#x907A;&#x7269;&#x7BB1;  <pr>&#x3072;&#x3058;&#x308A;&#x3044;&#x3076;&#x3064;&#x306F;&#x3053;</pr></trd>,
    <trd>&#x8056;&#x9AA8;&#x7BB1;  <pr>&#x305B;&#x3044;&#x3053;&#x3064;&#x3070;&#x3053;</pr></trd>
  </trdgrp>
  <trd lng="nl">relikwieschrijn</trd>
  <trd lng="pl">relikwiarz</trd>
  <trdgrp lng="ro">
    <trd>relicvariu</trd>,
    <trd>racl&abreve;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>relikvi&aacute;r</trd>,
    <trd>schr&aacute;nka na relikvie</trd>
  </trdgrp>
</drv>
</art>
