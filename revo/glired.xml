<vortaro>
<art mrk="$Id$">
<kap>
   <rad>glired</rad>/oj
</kap>
<drv mrk="glired.0oj">
   <kap><tld/>oj</kap>
   <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
       Familio el la ordo <ref tip="super" cel="rongx.0uloj">ron&gcirc;uloj</ref>
       (<trd lng="la">Gliridae</trd>), kies anoj dormas dum malvarmaj a&ubreve;
       sekaj periodoj.
     </dif>
   </snc>
   <trdgrp lng="de">
     <trd>Bilche</trd>,
     <trd>Schl&auml;fer<klr tip="ind"> (Nagerfamilie)</klr></trd>
   </trdgrp>
   <trd lng="en">dormice</trd>
   <trd lng="fr">glirid&eacute;s</trd>
   <trd lng="hu">pelef&eacute;l&eacute;k</trd>
   <trd lng="nl">slaapmuizen<klr tip="ind"> (fam.)</klr></trd>
   <trd lng="pt">glir&#237;deos</trd>
   <trd lng="ru">&c_s;&c_o;&c_n;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o; 
&c_g;&c_r;&c_y;&c_z;&c_u;&c_n;&c_o;&c_v;)</klr></trd>
</drv>
</art>
