<art mrk="$Id: glired.xml,v 1.14 2022/03/19 19:18:23 revo Exp $">
<kap>
   <rad>glired</rad>/oj
</kap>
<drv mrk="glired.0oj">
   <kap><tld/>oj</kap>
   <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
       <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref tip="malprt" cel="rongx.0uloj">ron&gcirc;uloj</ref>
       (<trd lng="la">Gliridae</trd>), kies anoj dormas dum malvarmaj a&ubreve;
       sekaj periodoj:
      <ekz>
        <ref tip="prt" cel="glir.0o">gliro</ref>
        <klr>[&#x2026;]</klr> estas <klr>[&#x2026;]</klr> la plej granda el &ccirc;iuj <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=379749">Gliro</url></lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trd lng="ca">gl&iacute;rids</trd>
  <trd lng="cs">plchovit&iacute;</trd>
  <trdgrp lng="de">
     <trd>Bilche</trd>,
     <trd>Schl&auml;fer<klr tip="ind"> (Nagerfamilie)</klr></trd>
   </trdgrp>
  <trd lng="en">dormice</trd>
  <trd lng="fr">glirid&eacute;s</trd>
  <trd lng="hu">pelef&eacute;l&eacute;k</trd>
  <trd lng="nl">slaapmuizen<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pt">glir&iacute;deos</trd>
  <trd lng="ru">&c_s;&c_o;&c_n;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o; 
&c_g;&c_r;&c_y;&c_z;&c_u;&c_n;&c_o;&c_v;)</klr></trd>
  <trd lng="sk">plchovit&eacute;</trd>
  <trd lng="uk">&#x441;&#x43E;&#x43D;&#x435;&#x432;&#x456;</trd>
</drv>
</art>
