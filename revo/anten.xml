<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>anten</rad>/o
</kap>
<drv mrk="anten.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="anten.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Palpaj  fadenformaj organoj trovi&gcirc;antaj sur la
      kapo de multaj insektoj  kaj krustuloj.
    </dif>
    <trd lng="de">F&uuml;hler</trd>
    <trdgrp lng="en">
      <trd>antenna</trd>,
      <trd>feeler</trd>
    </trdgrp>
    <trd lng="ru">&#1072;&#1085;&#1090;&#1077;&#1085;&#1085;&#1072;</trd>
    <trd lng="sv">antenn</trd>
  </snc>
  <snc mrk="anten.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Longa kaj fleksebla stango, kiun oni fiksas al mezo a&ubreve; supro de
      masto
      por teni triangulan velon.
    </dif>
    <trd lng="de">Rahe</trd>
  </snc>
  <snc mrk="anten.0o.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Konduko, plej ofte el drato, por disradiadi la ondojn de
      radioelektra
      sendstacio a&ubreve; por kaptadi la ondojn de  aliaj radiostacioj.
    </dif>
    <trd lng="de">Antenne</trd>
    <trdgrp lng="en">
      <trd>antenna</trd>,
      <trd>aerial</trd>
    </trdgrp>
    <trd lng="ru">&#1072;&#1085;&#1090;&#1077;&#1085;&#1085;&#1072;</trd>
    <trd lng="sv">antenn</trd>
  </snc>
  <trd lng="fr">antenne</trd>
</drv>
</art>
