<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>gru</rad>/o
</kap>
<drv mrk="gru.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="gru.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Grus</trd>), granda, longakrura migranta birdo.
    </dif>
    <trd lng="cs">je&rcaron;&aacute;b popelav&yacute;</trd>
    <trd lng="de">Kranich</trd>
    <trd lng="nl">kraanvogel</trd>
    <trd lng="en">crane</trd>
    <trd lng="fr">grue</trd>
    <trd lng="hu">daru</trd>
    <trd lng="pl">&zdot;uraw pospolity</trd>
    <trd lng="ru">&c_s;&c_je;&c_r;&c_y;&c_j;
    &c_zh;&c_u;&c_r;&c_a;&c_v;&c_l;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Longstanga levma&scirc;ino:
      <ekz>
        &ccirc;irka&ubreve; tiuj insuloj krakis <tld/>oj kun
        siteloj<fnt>B</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="argan.0o">argano</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Kran</trd>
  <trd lng="fr">grue</trd>
  <trd lng="nl">kraan</trd>
</drv>
<drv mrk="gru.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="gru.0oformaj"><tld/>oformaj
      birdoj</ref> (<trd lng="la">Gruidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Kraniche <klr tip="ind">(Familie)</klr></trd>
  <trd lng="en">cranes <klr tip="ind">(family)</klr></trd>
</drv>
<drv mrk="gru.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Gruiformes</trd>).
    </dif>
  </snc>
  <trd lng="de">Kranichv&ouml;gel</trd>
</drv>
</art>
