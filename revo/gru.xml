<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>gru</rad>/o
</kap>
<drv mrk="gru.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="grued.0oj"><tld/>edoj</ref>
      (<trd lng="la">Grus</trd>), granda, longakrura migranta birdo.
    </dif>
    <trd lng="cs">je&rcaron;&aacute;b popelav&yacute;</trd>
    <trd lng="de">Kranich</trd>
    <trd lng="nl">kraanvogel</trd>
    <trd lng="en">crane</trd>
    <trd lng="fr">grue</trd>
    <trd lng="hu">daru</trd>
    <trd lng="pl">&zdot;uraw pospolity</trd>
    <trd lng="ru">&c_s;&c_je;&c_r;&c_y;&c_j; &c_zh;&c_u;&c_r;&c_a;&c_v;&c_l;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Longstanga levma&scirc;ino:
      <ekz>
        &ccirc;irka&ubreve; &ccirc;iuj &ccirc;i insuloj krakis
        <tld/>oj kun siteloj
        <fnt>(<aut>Prus, trad. Kabe</aut>: <vrk>La Faraono, 1a vol.</vrk>,
        <lok>&ccirc;ap. 14</lok>)</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="argan.0o">argano</ref>.
    </refgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_l;&c_o;&c_d;&c_je;&c_z;&c_n;&c_y;&c_j; &c_zh;&c_u;&c_r;&c_a;&c_v;&c_l;&c_mol;</trd>,
      <trd>&c_p;&c_o;&c_d;&c_malmol;&c_jo;&c_m;&c_n;&c_y;&c_j; &c_k;&c_r;&c_a;&c_n;</trd>
    </trdgrp>
  </snc>
  <trd lng="de">Kran</trd>
  <trd lng="fr">grue</trd>
  <trd lng="nl">kraan</trd>
</drv>

<drv mrk="gru.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Gruiformes</trd>).
    </dif>
  </snc>
  <trd lng="de">Kranichv&ouml;gel</trd>
</drv>
</art>
