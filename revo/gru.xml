<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>gru</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="gru.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gru.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Migra birdo kun longaj, maldikaj kruroj,
      kolo kaj beko, simbolo de longviveco en
      Orientazio;
      genro el familio gruedoj kaj el la ordo
      <tld/>formaj (<trd lng="la">Grus</trd>):
      <ekz>
        <tld/>o man&gcirc;as fi&scirc;ojn kaj vivas en mar&ccirc;o
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url
          ref="http://esperanto.cri.cn/721/2010/03/05/1s109793.htm">
          Vivo de Li Binsheng...</url></vrk>,
          <lok>2010-03-05</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_zh;&c_u;&c_r;&c_a;&c_v;&c_je;&c_l;&c_mol;</trd>,
      <trd>&c_zh;&c_o;&c_r;&c_a;&c_w;</trd>
    </trdgrp>
    <trd lng="bg">&c_zh;&c_je;&c_r;&c_a;&c_v;</trd>
    <trd lng="cs">je&rcaron;&aacute;b popelav&yacute;</trd>
    <trd lng="de">Kranich</trd>
    <trd lng="nl">kraanvogel</trd>
    <trd lng="en">crane</trd>
    <trd lng="es">grulla</trd>
    <trd lng="pl">&zdot;uraw pospolity</trd>
    <trd lng="ru">&c_s;&c_je;&c_r;&c_y;&c_j; &c_zh;&c_u;&c_r;&c_a;&c_v;&c_l;&c_mol;</trd>
    <trd lng="pt">grou</trd>
    <trd lng="sv">trana</trd>
  </snc>
  <snc mrk="gru.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Levma&scirc;ino konsistanta &gcirc;enerale el oblikva brako, kies
      supra parto portas pulion; oni levas &scirc;ar&gcirc;on per
      &scirc;nurego a&ubreve; &ccirc;eno suriranta tiun pulion kaj tirata per
      malsupra vin&ccirc;o:
      <ekz>
        &ccirc;irka&ubreve; &ccirc;iuj &ccirc;i insuloj krakis
        <tld/>oj kun siteloj
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 14</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_zh;&c_u;&c_r;&c_a;&c_v;&c_je;&c_l;&c_mol; <klr>(&c_s;&c_t;&c_u;&c_d;&c_n;&c_ib;)</klr></trd>,
      <trd>&c_p;&c_a;&c_d;'&c_jo;&c_m;&c_n;&c_y; &c_k;&c_r;&c_a;&c_n;</trd>
    </trdgrp>
    <trd lng="de">Kran</trd>
    <trd lng="en">crane</trd>
    <trd lng="es">gr&uacute;a</trd>
    <trd lng="nl">kraan</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_l;&c_o;&c_d;&c_je;&c_z;&c_n;&c_y;&c_j; &c_zh;&c_u;&c_r;&c_a;&c_v;&c_l;&c_mol;</trd>,
      <trd>&c_p;&c_o;&c_d;&c_malmol;&c_jo;&c_m;&c_n;&c_y;&c_j; &c_k;&c_r;&c_a;&c_n;</trd>
    </trdgrp>
    <trd lng="sv">lyftkran</trd>
    <trdgrp lng="pt">
      <trd>grua</trd>,
      <trd>guindaste</trd>
    </trdgrp>
  </snc>
  <trd lng="ca">grua</trd>
  <trd lng="fr">grue<klr tip="ind"> (oiseau, machine)</klr></trd>
  <trd lng="hu">daru<klr tip="ind"> (mad&aacute;r, g&eacute;p)</klr></trd>
</drv>

<drv mrk="gru.G0o">
  <kap><tld lit="G"/>o</kap>
  <mlg>Gru</mlg>
  <snc mrk="gru.G0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Sud-hemisfera <ref tip="super" cel="stel.0figuro">stelfiguro</ref>
      (<trd lng="la">Grus</trd>).
    </dif>
  </snc>
  <trd lng="ca">la <ind>Grua</ind></trd>
  <trd lng="de">Kranich</trd>
  <trd lng="en">Grus</trd>
  <trd lng="es">Grulla</trd>
</drv>

<drv mrk="gru.0formaj">
  <kap><tld/>formaj</kap>
  <snc mrk="gru.0formaj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Gruiformes</trd>).
    </dif>
  </snc>
  <trd lng="ca">gru&iuml;formes</trd>
  <trd lng="de">Kranichv&ouml;gel</trd>
  <trdgrp lng="fr">
    <trd>gruiformes</trd>,
    <trd>ralliformes</trd>
  </trdgrp>
  <trd lng="hu">daruszer&udblac; madarak</trd>
  <trd lng="nl">kraanvogels</trd>
  <trd lng="ru">&c_zh;&c_u;&c_r;&c_a;&c_v;&c_l;&c_je;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trd lng="sv">tran- och rallf&aring;glar</trd>
</drv>
</art>
