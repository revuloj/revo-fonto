<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gru.xml,v 1.48 2023/11/11 11:36:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>gru</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="gru.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gru.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Migra <ref tip="lst" lst="voko:birdoj" cel="bird.0o.ZOO">birdo</ref> kun longaj, 
      maldikaj kruroj, kolo kaj beko, simbolo de longviveco en Orientazio;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio 
      <ref tip="malprt" cel="grued.0oj">gruedoj</ref> kaj el la ordo
      <tld/>formaj (<trd lng="la">Grus</trd>):
      <ekz>
        turto, hirundo, kaj <tld/>o observas la tempon, kiam ili devas veni
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 8:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        la cignoj flugas en oblikva direkto, la <tld/>oj triangule
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        etendinte la kolon, kiel <tld/>o <klr>[li]</klr> flustris mistere
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i decidis iri labori en tre fora naturrezervejo, por prizorgi la ru&gcirc;vertajn
        <tld/>ojn
        <fnt>
          <bib>Kon11</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o man&gcirc;as fi&scirc;ojn kaj vivas en mar&ccirc;o
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url ref="&CRI;721/2010/03/05/1s109793.htm">
          Vivo de Li Binsheng...</url></vrk>,
          <lok>2010-03-05</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/5/50/Grus_carunculata_Moremi.jpg/640px-Grus_carunculata_Moremi.jpg" prm="CC BY-SA 2.0">du <tld/>oj (Grus carunculata), Nacia Parko Moremi, Botsvano
        <fnt>
          <aut>Fiver L&ouml;cker from Wellington, New Zealand, Moremi - 14</aut>
          <lok><url ref="&WCW;/File:Grus_carunculata_Moremi.jpg">&WCW;/File:Grus_carunculata_Moremi.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="be">
      <trd>&c_zh;&c_u;&c_r;&c_a;&c_v;&c_je;&c_l;&c_mol;</trd>,
      <trd>&c_zh;&c_o;&c_r;&c_a;&c_w;</trd>
    </trdgrp>
    <trd lng="bg">&c_zh;&c_je;&c_r;&c_a;&c_v;</trd>
    <trd lng="cs">je&rcaron;&aacute;b popelav&yacute;</trd>
    <trd lng="de">Kranich</trd>
    <trd lng="nl">kraanvogel</trd>
    <trd lng="en">crane</trd>
    <trd lng="es">grulla</trd>
    <trd lng="pl">&zdot;uraw <klr>pospolity</klr></trd>
    <trd lng="ro">cocor</trd>
    <trd lng="ru">&c_s;&c_je;&c_r;&c_y;&c_j; &c_zh;&c_u;&c_r;&c_a;&c_v;&c_l;&c_mol;</trd>
    <trd lng="pt">grou</trd>
    <trd lng="sv">trana</trd>
    <trd lng="zh">&#x9DB4;&#x79D1; <pr>h&#xE8;k&#x113;</pr></trd>
</snc>
  <snc mrk="gru.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Lev<ref tip="super" cel="masxin.0o.TEK">ma&scirc;ino</ref> 
      konsistanta &gcirc;enerale el oblikva brako, kies
      supra parto portas <ref tip="prt" cel="puli.0o">pulio</ref>n; 
      oni levas &scirc;ar&gcirc;on per &scirc;nurego a&ubreve; &ccirc;eno suriranta tiun pulion 
      kaj tirata per malsupra <ref tip="prt" cel="vincx.0o">vin&ccirc;o</ref>:
      <ekz>
        &ccirc;irka&ubreve; &ccirc;iuj &ccirc;i insuloj krakis <tld/>oj kun siteloj
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 14</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> &gcirc;irafoj <klr>[&#x2026;]</klr> similas arganojn, a&ubreve; longajn levilegojn
        super putoj en rusaj vila&gcirc;oj, nomatajn tie <tld/>oj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni povis vidi malnovajn marajn &scirc;ipojn sur la kanalo a&ubreve; historian havenan
        <tld/>on
        <fnt>
          <bib>Kon11</bib>
        </fnt>;
      </ekz>
      <ekz>
        kolosa svelta pikilo, el brila korod-imuna &scirc;talo kaj nomita &leftquot;<nom>The Spike</nom>&rightquot;
        <klr>[&#x2026;]</klr> per la plej alta <tld/>o en Irlando estis alfiksita
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2003/008042.html">Pinte!</url></vrk>
          <bib>Monato</bib>
          <lok>2003/12, p. 16</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="argan.0o">argano</ref>,
      <ref cel="takel.0o">takelo</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_zh;&c_u;&c_r;&c_a;&c_v;&c_je;&c_l;&c_mol; <klr>(&c_s;&c_t;&c_u;&c_d;&c_n;&c_ib;)</klr></trd>,
      <trd>&c_p;&c_a;&c_d;'&c_jo;&c_m;&c_n;&c_y; &c_k;&c_r;&c_a;&c_n;</trd>
    </trdgrp>
    <trd lng="de">Kran</trd>
    <trd lng="en">crane</trd>
    <trd lng="es">gr&uacute;a</trd>
    <trd lng="nl">kraan</trd>
    <trdgrp lng="pl">
      <trd>&zdot;uraw</trd>,
      <trd>d&zacute;wig</trd>
    </trdgrp>
    <trd lng="ro">lift</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_l;&c_o;&c_d;&c_je;&c_z;&c_n;&c_y;&c_j; &c_zh;&c_u;&c_r;&c_a;&c_v;&c_l;&c_mol;</trd>,
      <trd>&c_p;&c_o;&c_d;&c_malmol;&c_jo;&c_m;&c_n;&c_y;&c_j; &c_k;&c_r;&c_a;&c_n;</trd>
    </trdgrp>
    <trd lng="sv">lyftkran</trd>
    <trdgrp lng="pt">
      <trd>grua</trd>,
      <trd>guindaste</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x8D77;&#x91CD;&#x673A; <pr>q&#x1D0;zh&#xF2;ngj&#x12B;</pr></trd>,
    <trd>&#x9DB4; <pr>h&#xE8;</pr></trd>,
    <trd>&#x9E1B; <pr>gu&#xE0;n</pr></trd>,
    <trd>&#x540A; <pr>di&#xE0;o</pr></trd>
  </trdgrp>
</snc>
  <trd lng="ca">grua</trd>
  <trd lng="cs">je&rcaron;&aacute;b <klr>(zdvihac&iacute; stroj)</klr></trd>
  <trd lng="fr">grue<klr tip="ind"> (oiseau, machine)</klr></trd>
  <trd lng="hu">daru<klr tip="ind"> (mad&aacute;r, g&eacute;p)</klr></trd>
  <trd lng="sk">&zcaron;eriav <klr>(stroj na zdv&iacute;hanie)</klr></trd>
</drv>

<drv mrk="gru.G0o">
  <kap><tld lit="G"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
  </kap>
  <mlg>Gru</mlg>
  <snc mrk="gru.G0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Sud-hemisfera <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref>
      (<trd lng="la">Grus</trd>):
      <ekz>
        NGC 7117 estas galaksio en konstelacio <tld lit="G"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=120484">NGC 7117</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_Zh;&c_u;&c_r;&c_a;&c_v;&c_je;&c_l;&c_mol; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#x2019;&c_je;)</klr></trd>
  <trd lng="bo">&t_ba;&t_yata;&t_tsheg;&t_kha;&t_rata;&t_shabchu;&t_nga;&t_tsheg;&t_kha;&t_rata;&t_shabchu;&t_nga;&t_tsheg;</trd>
  <trd lng="ca">la <ind>Grua</ind></trd>
  <trd lng="cs">je&rcaron;&aacute;b <klr>(zdvihac&iacute; stroj)</klr></trd>
  <trd lng="de">Kranich</trd>
  <trd lng="en">Grus</trd>
  <trd lng="es">Grulla</trd>
  <trd lng="fr">Grue<klr tip="ind"> (constellation)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x30AF;&#x30EC;&#x30FC;&#x30F3;</trd>,
    <trd>&#x8D77;&#x91CD;&#x6A5F;  <pr>&#x304D;&#x3058;&#x3085;&#x3046;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="pl">&Zdot;uraw  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Cocorul  <klr>(constela&#x21B;ie)</klr></trd>
  <trd lng="sk">&zcaron;eriav <klr>(stroj na zdv&iacute;hanie)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_zh;&c_u;&c_r;&c_a;&c_v;&c_je;&#x43B;&#x44C;</trd>,
    <trd>&#x43F;&c_ib;&c_d;&c_j;&#x43E;&#x43C;&#x43D;&c_i;&c_j; &#x43A;&c_r;&c_a;&#x43D;</trd>
  </trdgrp>
</drv>

<drv mrk="gru.0formaj">
  <kap><tld/>formaj</kap>
  <snc mrk="gru.0formaj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref> el la klaso <ref tip="malprt" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Gruiformes</trd>):
      <ekz>
        la A&ubreve;stralia otido <klr>[&#x2026;]</klr> estas granda surtera birdo de <klr>[&#x2026;]</klr>
        ordo <tld/>formaj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=127862">A&ubreve;stralia otido</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_u;&c_r;&c_a;&c_w;&c_l;&c_je;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja; <klr>(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="ca">gru&iuml;formes</trd>
  <trd lng="de">Kranichv&ouml;gel</trd>
  <trdgrp lng="fr">
    <trd>gruiformes</trd>,
    <trd>ralliformes</trd>
  </trdgrp>
  <trd lng="hu">daruszer&udblac; madarak</trd>
  <trd lng="nl">kraanvogels</trd>
  <trd lng="pl">&zdot;urawiowe</trd>
  <trd lng="ro">gruiformes</trd>
  <trd lng="ru">&c_zh;&c_u;&c_r;&c_a;&c_v;&c_l;&c_je;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trd lng="sv">tran- och rallf&aring;glar</trd>
  <trd lng="uk">&c_zh;&c_u;&c_r;&c_a;&c_v;&#x43B;&c_je;&#x43F;&#x43E;&c_d;&c_ib;&c_b;&#x43D;&c_ib;</trd>
  <trd lng="zh">&#x9DB4;&#x5F62;&#x76EE; <pr>h&#xE8;x&#xED;ngm&#xF9;</pr></trd>
</drv>
</art>
