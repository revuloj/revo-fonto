<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nektarin.xml,v 1.5 2022/03/24 10:41:39 revo Exp $">
<kap>
  <rad>nektarin</rad>/o
</kap>
<drv mrk="nektarin.0o">
  <kap><tld/>o</kap>
  <snc mrk="nektarin.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <tld/>o estas man&gcirc;ebla frukto,
      glat&scirc;ela mutaciinto de <ref tip="vid" cel="persik.0o">persiko</ref>
      <ekz>
        <klr>...</klr> glataj folioj, helru&gcirc;aj floroj kaj vilaj a&ubreve;
        glataj (<tld/>o) fruktoj (drupoj), kiuj havas flavan, pli-malpli
        ru&gcirc;vangan ha&ubreve;ton kaj tre sukoplenan, bongustan karnon
        <fnt>
          <vrk>Ilustrita Esperanto vortaro</vrk>,
          <lok>
            <url ref="http://www.krokodilo.de/vortaro/p/vortlisto-p.php">persiko</url>
          </lok>
        </fnt>
      </ekz>
    </dif>
     <trd lng="de">Nektarine</trd>   
    <trd lng="en">nectarine</trd>
    <trd lng="fr">nectarine</trd>
    <trd lng="pl">nektarynka</trd>
  </snc>
  <trd lng="cs">nektarinka</trd>
  <trd lng="sk">nekt&aacute;rinka</trd>
  <trd lng="uk">&#x43D;&#x435;&#x43A;&#x442;&#x430;&#x440;&#x438;&#x43D; (&#x441;&#x43E;&#x440;&#x442; &#x43F;&#x435;&#x440;&#x441;&#x438;&#x43A;&#x456;&#x432;)</trd>
</drv>
</art>

