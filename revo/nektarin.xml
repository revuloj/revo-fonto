<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>nektarin</rad>/o
</kap>
<drv mrk="nektarin.0o">
  <kap><tld/>o</kap>
  <snc mrk="nektarin.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <tld/>o estas man&gcirc;ebla frukto,
      glat&scirc;ela mutaciinto de <ref tip="vid"
      cel="persik.0o">persiko</ref>
      <ekz>
        <klr>...</klr> glataj folioj, helru&gcirc;aj floroj kaj vilaj a&ubreve;
        glataj (<tld/>o) fruktoj (drupoj), kiuj havas flavan, pli-malpli
        ru&gcirc;vangan ha&ubreve;ton kaj tre sukoplenan, bongustan karnon
        <fnt>
          <vrk>Ilustrita Esperanto vortaro</vrk>,
          <lok>
            <url
            ref="http://www.krokodilo.de/vortaro/p/vortlisto-p.php">persiko</url>
          </lok>
        </fnt>
      </ekz>
    </dif>
    <trd lng="en">nectarine</trd>
    <trd lng="fr">nectarine</trd>
    <trd lng="pl">nektarynka</trd>
  </snc>
</drv>
</art>

<!--
$Log$
-->
</vortaro>