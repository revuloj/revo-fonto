<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>bombon</rad>/o
</kap>
<drv mrk="bombon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bombon.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Malgranda sukera&jcirc;o prenata kiel <ref tip="super"
      cel="frand.0ajxo.KUI">franda&jcirc;o</ref>:
      <ekz>
        kelkaj funtoj da <tld/>oj por miaj infanetoj
        <fnt><bib>FK</bib>, <lok>3, anekdotoj</lok></fnt>.
      </ekz>
    </dif>
    <rim>Pro nacilingva konfuzo fojfoje aperas "bonbono",
      kiu ne &scirc;ajnas ekzisti en vortaroj.</rim>
  </snc>
  <trd lng="be">&c_c;&c_u;&c_k;&c_je;&c_r;&c_k;&c_a;</trd>
  <trd lng="br">madig</trd>
  <trd lng="de">Bonbon</trd>
  <trdgrp lng="en">
    <trd>bonbon</trd>,
    <trd>candy</trd>,
    <trd>sweet</trd>
  </trdgrp>
  <trd lng="es">bonbo&oacute;n</trd>
  <trd lng="fr">bonbon</trd>
  <trdgrp lng="hu">
    <trd>bonbon</trd>,
    <trd>cukorka</trd>
  </trdgrp>
  <trd lng="nl">bonbon</trd>
  <trd lng="pl">cukierek</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_f;&c_je;&c_t;&c_a;</trd>
</drv>
</art>
