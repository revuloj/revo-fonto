<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: smegm.xml,v 1.2 2018/10/22 18:10:18 revo Exp $">
<kap>
    <rad>smegm</rad>/o
</kap>
<drv mrk="smegm.0o">
  <kap><tld/>o</kap>
  <snc mrk="smegm.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Blanketa seba <ref tip="super"
      cel="sekrec.0ajxo.FIZL">sekrecia&jcirc;o</ref>, kiu kolekti&gcirc;as inter la
      <ref tip="vid" cel="penis.0o.ANA">penisa</ref> <ref tip="vid"
      cel="glan.0o.ANA">glano</ref> kaj <ref tip="vid"
      cel="prepuc.0o.penisa">prepucio</ref> a&ubreve; en la <ref tip="vid"
      cel="vulv.0o.ANA">vulvo</ref>:
      <ekz>
        <tld/>o estas kombina&jcirc;o de delasitaj ha&ubreve;taj &ccirc;eloj, ha&ubreve;taj oleoj kaj likvoj
        <klr>[&ccirc;e]</klr> eksteraj seksorganoj de viraj kaj inaj mamuloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=591147">Smegmo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ar">&a_A;&a_l;&a_l;&a_kh;&a_n;</trd>
  <trd lng="be">&c_s;&c_m;&c_je;&c_g;&c_m;&c_a;</trd>
  <trd lng="bg">&c_s;&c_m;&c_je;&c_g;&c_m;&c_a;</trd>
  <trd lng="ca">esmegma</trd>
  <trd lng="cs">smegma</trd>
  <trdgrp lng="da">
    <trd>nakkeost</trd>,
    <trd>smegma</trd>
  </trdgrp>
  <trd lng="de">Smegma</trd>
  <trd lng="el">&sigma;&my;&eta_ton;&gamma;&my;&alfa;</trd>
  <trd lng="en">smegma</trd>
  <trd lng="es">esmegma</trd>
  <trd lng="et">smeegma</trd>
  <trd lng="eu">esmegma</trd>
  <trdgrp lng="fi">
    <trd>smegma</trd>,
    <trd>esinahkatali</trd>
  </trdgrp>
  <trd lng="fr">smegma</trd>
  <trd lng="fy">smegma</trd>
  <trd lng="gl">esmegma</trd>
  <trd lng="he">&samekh;&mem;&gimel;&mem;&he;</trd>
  <trd lng="hu">szmegma</trd>
  <trdgrp lng="is">
    <trd>reðurfarði</trd>,
    <trd>limfarði</trd>,
    <trd>forh&uacute;ðarostur</trd>,
    <trd>ostur</trd>
  </trdgrp>
  <trd lng="it">smegma</trd>
  <trd lng="ko">&#44480;&#46160;&#51648;</trd>
  <trd lng="la">smegma</trd>
  <trd lng="lt">smegma</trd>
  <trdgrp lng="mi">
    <trd>paraheka</trd>,
    <trd>ngahu</trd>
  </trdgrp>
  <trdgrp lng="mk">
    <trd>&c_s;&c_m;&c_je;&c_g;&c_m;&c_a;</trd>,
    <trd>&c_u;&c_r;&c_d;&c_a;</trd>
  </trdgrp>
  <trd lng="nl">smegma</trd>
  <trd lng="no">smegma</trd>
  <trd lng="pl">mastka</trd>
  <trd lng="pt">esmegma</trd>
  <trd lng="ru">&c_s;&c_m;&c_je;&c_g;&c_m;&c_a;</trd>
  <trd lng="sh">smegma</trd>
  <trd lng="sq">smegma</trd>
  <trdgrp lng="sv">
    <trd>smegma</trd>,
    <trd>flensost</trd>,
    <trd>kukost</trd>
  </trdgrp>
  <trd lng="th">&#3586;&#3637;&#3657;&#3648;&#3611;&#3637;&#3618;&#3585;</trd>
  <trdgrp lng="tl">
    <trd>kupal</trd>,
    <trd>hima</trd>
  </trdgrp>
  <trd lng="tr">smegma</trd>
  <trd lng="uk">&c_s;&c_m;&c_je;&c_g;&c_m;&c_a;</trd>
</drv>
</art>

