<drv mrk="kruc.turno0o">
  <kap>turno<tld/>o
    <fnt><bib>GDEF</bib></fnt>
  </kap>
  <snc mrk="kruc.turno0o.pasejo">
    <dif>
      Aran&gcirc;a&jcirc;o en formo de turnebla horizontala <tld/>o muntita
      sur fosteto, kiajn oni instalas &ccirc;e enirejo en stadionon,
      kasejon ktp, por tralasi publikon po unu homo la&ubreve;vice,
      eventuale kontrolante biletojn ktp:
      <ekz>
        en la butiko &scirc;i a&ccirc;etis sunfloran oleon, kaj jen, kontra&ubreve; la turno<tld/>o la
        litrujo rompi&gcirc;is, la tuta jupo makuli&gcirc;is
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        &jcirc;etinte <klr>kvinfrankan moneron</klr> en a&ubreve;tomaton, oni
        eniras tra sekureca turno<tld/>o
        <fnt>
          <aut>M. Ecott</aut>,
          <vrk><url ref="http://www.oocities.org/paris/metro/6148/espstudy.html#Artikolo%204">Velka
          eta floro</url></vrk>, <bib>Monato</bib>,
          <lok>Junio 1998.</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_r;&c_n;&c_ib;&c_k;&c_je;&c_t;</trd>
  <trd lng="de">Drehkreuz <klr>(T&uuml;r)</klr></trd>
  <trd lng="en">turnstile</trd>
  <trd lng="fr">tourniquet</trd>
  <trd lng="pl">bramka obrotowa</trd>
  <trd lng="ro">turnichet</trd>
  <trd lng="ru">&c_t;&c_u;&c_r;&c_n;&c_i;&c_k;&c_je;&c_t;</trd>
  <trd lng="uk">&c_t;&c_u;&c_r;&#x43D;&c_ib;&#x43A;&c_je;&c_t;</trd>
</drv>

