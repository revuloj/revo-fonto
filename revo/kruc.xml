  <trd lng="es">cruzado</trd>
  <trd lng="fr">crois&eacute; <klr tip="ind">(soldat des
  croisades)</klr></trd>
  <trd lng="hu">keresztes <klr tip="ind">(lovag)</klr></trd>
  <trd lng="nl">kruisridder</trd>
  <trd lng="pl">krzy&zdot;owiec</trd>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_s;&c_t;&c_o;&c_n;&c_o;&c_s;&c_je;&c_c;</trd>
  <trd lng="sk">kri&zcaron;iak</trd>
</drv>

<drv mrk="kruc.0umi">
  <kap><ofc>*</ofc><tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kruc.0umi.TOR">
    <dif>
      <ref tip="super" cel="mort.0igi.malvivigi">Mortigi</ref> per pendigo sur <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_k;&c_r;&c_y;&c_zh;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>uk&rcaron;i&zcaron;ovat</trd>,
    <trd>uk&rcaron;i&zcaron;ov&aacute;vat</trd>
  </trdgrp>
  <trd lng="de">kreuzigen</trd>
  <trd lng="en">crucify</trd>
  <trd lng="es">crucificar</trd>
  <trd lng="fr">crucifier</trd>
  <trd lng="hu">keresztre fesz&iacute;t</trd>
  <trd lng="nl">kruisigen</trd>
  <trd lng="pl">ukrzy&zdot;owa&cacute;</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_ja;&c_t;&c_mol;</trd>
  <trd lng="sk">ukri&zcaron;ova&tcaron;</trd>
  <trd lng="sv">korsf&auml;sta</trd>
</drv>

<drv mrk="kruc.greka0o">
  <kap>greka <tld/>o, <var><kap>regula <tld/>o</kap></var></kap>
  <snc mrk="kruc.greka0o.FORM">
    <dif>
      <ref tip="super" cel="kruc.0o.FORM"><tld lit="K"/>o</ref> kun
      &ccirc;iuj bran&ccirc;oj egale longaj, tradicia figuro en orienta
      Kristanismo:
      <ekz>
        konstrua&jcirc;o <klr>...</klr> en formo de greka <tld/>o
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;Libra turo&rightquot;,
        2006-06-22</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">croix grecque</trd>
  <trd lng="es">cruz griega</trd>
  <trd lng="it">croce greca</trd>
  <trd lng="pl">krzy&zdot; grecki</trd>
</drv>

<drv mrk="kruc.hoko0o">
  <kap>hoko<tld/>o, <var><kap>hok<tld/>o</kap></var></kap>
  <snc mrk="kruc.hoko0o.FORM">
    <dif>
      <ref tip="super" cel="kruc.0o.FORM"><tld lit="K"/>o</ref> kun
      bran&ccirc;oj orte fleksitaj, bona&ubreve;gura figuro en Hindujo kaj
      Orientazio, historia insigno de Naziismo en E&ubreve;ropo;
      <ref tip="sin" cel="svastik.0o.FORM">svastiko</ref>:
      <ekz>
        Lauburu estas ronda hok<tld/>o, uzata kiel ornama&jcirc;o en E&ubreve;skio
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;Lauburu&rightquot;,
        2006-11-09</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>h&aacute;kov&yacute; k&rcaron;&iacute;&zcaron;</trd>,
    <trd>svastika</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>esv&aacute;stica</trd>,
    <trd>cruz gamada</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>croix gamm&eacute;e</trd>,
    <trd>svastika</trd>
  </trdgrp>
  <trd lng="hu">horogkereszt</trd>
  <trdgrp lng="it">
    <trd>croce uncinata</trd>,
    <trd>svastica</trd>
  </trdgrp>
  <trd lng="pl">swastyka</trd>
  <trd lng="sk">h&aacute;kov&yacute; kr&iacute;&zcaron;</trd>
</drv>

<drv mrk="kruc.kelta0o">
  <kap>kelta <tld/>o</kap>
  <snc mrk="kruc.kelta0o.FORM">
    <dif>
      <ref tip="super" cel="kruc.0o.FORM"><tld lit="K"/>o</ref> kun
      ringo &ccirc;irka&ubreve; la centro, tradicia figuro en nordokcidenta
      keltlingva E&ubreve;ropo:
      <ekz>
        kelta <tld/>o en Monasterboice, Irlando
        <fnt>
        <aut>P. Carlos Musazzi</aut>,
        <vrk>La dua vatikana koncilio por la unueco de la
        kristanoj</vrk>,
        <lok><url ref="http://www.esperokatolika.org/ek19861990/ek1986_02.htm">
        Espero Katolika, 1986-2</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">croix celtique</trd>
  <trd lng="es">cruz celta</trd>
  <trd lng="pl">krzy&zdot; celtycki</trd>
</drv>

<drv mrk="kruc.latina0o">
  <kap>latina <tld/>o</kap>
  <snc mrk="kruc.latina0o.FORM">
    <dif>
      <ref tip="super" cel="kruc.0o.FORM"><tld lit="K"/>o</ref> kun
      pli longa malsupra bran&ccirc;o, tradicia figuro en okcidenta
      Kristanismo:
      <ekz>
        &gcirc;ia formo estas tiu de latina <tld/>o
        <fnt>
        <aut>A. &Ccaron;i&zcaron;ikait&#279;</aut>,
        <vrk><url ref="http://www.esperanto.lt/board/zboard.php?id=ikue">
        57a Kongreso de Internacia Katolika Unui&gcirc;o
        Esperantista</url></vrk>,
        <lok>2004-07</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">croix latine</trd>
  <trd lng="it">croce latina</trd>
  <trd lng="es">cruz latina</trd>
  <trd lng="pl">krzy&zdot; &lstroke;aci&nacute;ski</trd>
</drv>

<drv mrk="kruc.malta0o">
  <kap>malta <tld/>o</kap>
  <snc mrk="kruc.malta0o.FORM">
    <dif>
      <ref tip="super" cel="kruc.0o.FORM"><tld lit="K"/>o</ref> kun
      bran&ccirc;oj dispintaj, lar&gcirc;i&gcirc;antaj al ekstero, insigno de la 
      malta ordeno:
      <ekz>
        la instanco stampis ilin per ru&gcirc;a stampo en la formo de malta 
        <tld/>o
        <fnt>
          <aut>D. Fielding</aut>,
          <vrk><url ref="http://verdadrako.50g.com/asap.html">
          Esperanto - asap</url></vrk>,
          <lok>&ccirc;apitro 19a, 2007-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">croix de Malte</trd>
  <trd lng="es">cruz de Malta</trd>
  <trd lng="pl">krzy&zdot; malta&nacute;ski</trd>
</drv>

<drv mrk="kruc.voj0igxo">
  <kap>voj<tld/>i&gcirc;o, <var><kap>voj<tld/>o</kap></var></kap>
  <snc mrk="kruc.voj0igxo.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      Loko, en kiu du a&ubreve; pluraj vojoj renkonti&gcirc;as
      transverse:
      <ekz>
	&gcirc;i staras <klr>[...]</klr> apud la vojo, &ccirc;e la
	voj<tld/>i&gcirc;oj
	<fnt><bib>MT</bib>, <lok>&Sen; 8:2</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;e &ccirc;iu voj<tld/>o la trumpetistoj ludis signalon
        <fnt>
          <bib>Far1</bib>,
          <lok>&ccirc;apitro 10a</lok>
        </fnt>.
      </ekz>
    </dif><refgrp tip="vid">
      <ref cel="rond.trafik0o.TRA">trafikrondo</ref>,
      <ref cel="nod.voj0o.TRA">vojnodo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_r;&c_y;&c_zh;&c_a;&c_v;&c_a;&c_n;&c_mol; &c_n;&c_je;</trd>
  <trd lng="de">Kreuzung</trd>
  <trd lng="es">encrucijada</trd>
  <trdgrp lng="fr">
    <trd>carrefour</trd>,
    <trd>crois&eacute;e des chemins</trd>,
    <trd>croisement</trd>,
    <trd>intersection</trd>
  </trdgrp>
  <trd lng="he">&tsadi;&vav;&mem;&tav;</trd>
  <trd lng="hu">&uacute;tkeresztez&odblac;d&eacute;s</trd>
  <trdgrp lng="it">
      <trd>incrocio<klr> (di strade e sim.)</klr></trd>
    </trdgrp>
  <trd lng="nl">kruispunt</trd>
  <trdgrp lng="pl">
    <trd>skrzy&zdot;owanie</trd>,
    <trd>krzy&zdot;&oacute;wka<klr> dr&oacute;g</klr></trd>,
    <trd>przejazd<klr> (np. kolejowy)</klr></trd>
  </trdgrp>
  <trd lng="pt">encruzilhada</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_k;&c_r;&c_jo;&c_s;&c_t;&c_o;&c_k;</trd>
</drv>

<drv mrk="kruc.0olevo">
  <kap><tld lit="K"/>olevo</kap>
  <snc mrk="kruc.0olevo.KRI">
    <uzo tip="fak">KAL</uzo>
    <uzo tip="fak">KRI</uzo>
    <dif>
      Unu el la kristanaj
      <ref tip="dif" cel="fest.dekduopa0o.KRI">dekduopaj festoj</ref>,
      dedi&ccirc;ita al la rememoro pri retrovo de la <tld/>o sur kiu
      Jesuo suferis la <tld/>umon (festata la 14an de septembro).
    </dif>
  </snc>
  <trd lng="de">Kreuzerh&ouml;hung</trd>
  <trd lng="fr">Exaltation de la Croix</trd>
  <trd lng="el">&Ypsilon_densa_acute;&psi;&omega;&sigma;&jota;&sigma_fina; &tau;&omikron;&ypsilon_ton; &Tau;&jota;&my;&jota_ton;&omikron;&ypsilon; &Sigma;&tau;&alfa;&ypsilon;&rho;&omikron;&ypsilon_ton;</trd>
  <trd lng="pl">&Sacute;wi&eogonek;to Podwy&zdot;szenia Krzy&zdot;a &Sacute;wi&eogonek;tego</trd>
  <trd lng="ru">&c_K;&c_r;&c_je;&c_s;&c_t;&c_o;&c_v;&c_o;&c_z;&c_d;&c_v;&c_i;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="kruc.turno0o">
  <kap>turno<tld/>o
    <fnt><bib>GDEF</bib></fnt>
  </kap>
  <snc mrk="kruc.turno0o.pasejo">
    <dif>
      Aran&gcirc;a&jcirc;o en formo de turnebla horizontala <tld/>o muntita
      sur fosteto, kiajn oni instalas &ccirc;e enirejo en stadionon,
      kasejon ktp, por tralasi publikon po unu homo la&ubreve;vice,
      eventuale kontrolante biletojn ktp:
      <ekz>
        &jcirc;etinte <klr>kvinfrankan moneron</klr> en a&ubreve;tomaton, oni
        eniras tra sekureca turno<tld/>o
        <fnt>
          <aut>M. Ecott</aut>,
          <vrk><url ref="http://www.oocities.org/paris/metro/6148/espstudy.html#Artikolo%204">Velka
          eta floro</url></vrk>, <bib>Monato</bib>,
          <lok>Junio 1998.</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Drehkreuz</trd>
  <trd lng="fr">tourniquet</trd>
  <trd lng="ru">&c_t;&c_u;&c_r;&c_n;&c_i;&c_k;&c_je;&c_t;</trd>
</drv>

