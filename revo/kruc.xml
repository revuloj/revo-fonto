<drv mrk="kruc.greka0o">
  <kap>greka <tld/>o, <var><kap>regula <tld/>o</kap></var></kap>
  <snc mrk="kruc.greka0o.FORM">
    <dif>
      <ref tip="super" cel="kruc.0o.FORM"><tld lit="K"/>o</ref> kun
      &ccirc;iuj bran&ccirc;oj egale longaj, tradicia figuro en orienta
      Kristanismo:
      <ekz>
        konstrua&jcirc;o <klr>[&#8230;]</klr> en formo de greka <tld/>o
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;Libra turo&rightquot;,
        2006-06-22</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">croix grecque</trd>
  <trd lng="es">cruz griega</trd>
  <trd lng="it">croce greca</trd>
  <trd lng="pl">krzy&zdot; grecki</trd>
</drv>

<drv mrk="kruc.hoko0o">
  <kap>hoko<tld/>o, <var><kap>hok<tld/>o</kap></var></kap>
  <snc mrk="kruc.hoko0o.FORM">
    <dif>
      <ref tip="super" cel="kruc.0o.FORM"><tld lit="K"/>o</ref> kun
      bran&ccirc;oj orte fleksitaj, bona&ubreve;gura figuro en Hindujo kaj
      Orientazio, historia insigno de Naziismo en E&ubreve;ropo;
      <ref tip="sin" cel="svastik.0o.FORM">svastiko</ref>:
      <ekz>
        <nom>Lauburu</nom> estas ronda hok<tld/>o, uzata kiel ornama&jcirc;o en E&ubreve;skio
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;Lauburu&rightquot;,
        2006-11-09</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>h&aacute;kov&yacute; k&rcaron;&iacute;&zcaron;</trd>,
    <trd>svastika</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>esv&aacute;stica</trd>,
    <trd>cruz gamada</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>croix gamm&eacute;e</trd>,
    <trd>svastika</trd>
  </trdgrp>
  <trd lng="hu">horogkereszt</trd>
  <trdgrp lng="it">
    <trd>croce uncinata</trd>,
    <trd>svastica</trd>
  </trdgrp>
  <trd lng="pl">swastyka</trd>
  <trd lng="sk">h&aacute;kov&yacute; kr&iacute;&zcaron;</trd>
</drv>

<drv mrk="kruc.kelta0o">
  <kap>kelta <tld/>o</kap>
  <snc mrk="kruc.kelta0o.FORM">
    <dif>
      <ref tip="super" cel="kruc.0o.FORM"><tld lit="K"/>o</ref> kun
      ringo &ccirc;irka&ubreve; la centro, tradicia figuro en nordokcidenta
      keltlingva E&ubreve;ropo:
      <ekz>
        gravuritaj kolonoj en la formo de antikva kelta <tld/>o, videbla en arkeologejoj
        kaj tombejoj en la tuta insulo
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="https://www.monato.be/2009/009638.php">La tulamora tombejo</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelta <tld/>o en <nom>Monasterboice</nom>, Irlando
        <fnt>
          <aut>P. Carlos Musazzi</aut>,
          <vrk>La dua vatikana koncilio por la unueco de la
          kristanoj</vrk>,
          <lok><url ref="http://www.esperokatolika.org/ek19861990/ek1986_02.htm">
          Espero Katolika, 1986-2</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Keltisches Kreuz</trd>
  <trd lng="fr">croix celtique</trd>
  <trd lng="es">cruz celta</trd>
  <trd lng="pl">krzy&zdot; celtycki</trd>
</drv>

<drv mrk="kruc.latina0o">
  <kap>latina <tld/>o</kap>
