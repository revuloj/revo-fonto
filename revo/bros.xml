<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>bros</rad>/o
</kap>
<drv mrk="bros.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Ilo farita el haregoj a&ubreve; vergetoj plantitaj sur tabuleto, por forigi
      per froto la malpura&jcirc;on.
    </dif>
  </snc>
  <trd lng="nl">borstel</trd>
  <trd lng="cs">kart&aacute;&ccaron;</trd>
  <trd lng="en">brush</trd>
  <trd lng="fr">brosse</trd>
  <trd lng="ru">&c_shch;&c_jo;&c_t;&c_k;&c_a;</trd>
  <trd lng="sv">borste</trd>
</drv>

<drv mrk="bros.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Froti ion per <tld/>o:
      <ekz>
        <tld/>i vestojn, tapi&scirc;on, barbon, dentojn, harojn.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="bala.0i">balai</ref>,
      <ref cel="polv.sen0igi">senpolvigi</ref>,
      <ref cel="pur.0igi">purigi</ref>,
      <ref cel="skrap.0i">skrapi</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">borstelen</trd>
  <trd lng="fr">brosser</trd>
  <trd lng="ru">&c_ch;&c_i;&c_s;&c_t;&c_i;&c_t;&c_mol; &c_shch;&c_jo;&c_t;&c_k;&c_o;&c_j;</trd>
  <trd lng="sv">borsta</trd>
</drv>
</art>
