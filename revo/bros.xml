<vortaro>
<art mrk="$Id: bros.xml,v 1.42 2024/12/05 08:37:11 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bros</rad>/o
</kap>

<drv mrk="bros.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bros.0o.HEJM">
    <dif>
      Ilo farita el haregoj a&ubreve; vergetoj kaj tenilo,
      por forigi per froto malpura&jcirc;on, kombi, masa&gcirc;i:
      <ekz>
        mi prenis <tld/>on kaj purigis la veston
        <fnt>
          <bib>F</bib>,
          <vrk>Ekzercaro, 39</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la pordisto kuras post mi sur la &scirc;tuparo kun <tld/>o: permesu <klr>[&#8230;]</klr> mi
        al vi purigos la botojn
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia hararo estis lavita per rotacianta <tld/>o
        <fnt>
          <bib>VaK</bib>
          <lok>Tria &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu balaas, &ccirc;iu <tld/>lavas, &ccirc;iu uzas <tld/>ojn por cent aferoj
        <fnt>
          <bib>KrM</bib>
          <lok>3. Ne Gutas Mielo El La &Ccirc;ielo.</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia pantalono estas kovrita per polvo, &ccirc;u vi havas <tld/>on
        <fnt>
          <bib>IdO</bib>
          <lok>Kvara Parto</lok>
        </fnt>?
      </ekz>
      <ekz>
        <nom>Margarita</nom> turnis suben la <tld/>on de la balailo, tiel ke &gcirc;ia tenilo levi&gcirc;is
        malanta&ubreve; &scirc;ia dorso
        <fnt>
          <bib>MkM</bib>
          <lok>21</lok>
        </fnt>;
      </ekz>
      <ekz>
        li frotis sin en malvarma akvo, frotis sin per malmola <tld/>o
        <fnt>
          <bib>VRA</bib>
          <lok>Lars Ahlin</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x70AD;&#x7CBE;&#21047; <pr>t&#xE0;nj&#x12B;ngshu&amacron;</pr></trd>,
    <trd>&#37509;&#21047; <pr>t&#xF3;ngshu&amacron;</pr></trd>,
    <trd>&#21047; <pr>shu&amacron;</pr></trd>,
    <trd>&#21047;&#x5B50; <pr>shu&amacron;zi</pr></trd>,
    <trd>&#x6BDB;&#21047; <pr>m&#xE1;oshu&#xE0;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_sh;&c_ch;&c_o;&c_t;&c_k;&c_a;</trd>
  <trd lng="bg">&c_ch;&c_je;&c_t;&c_k;&c_a;</trd>
  <trd lng="br">broust</trd>
  <trd lng="ca">raspall</trd>
  <trd lng="cs">kart&aacute;&ccaron;</trd>
  <trd lng="de">B&uuml;rste</trd>
  <trd lng="en">brush</trd>
  <trd lng="es">cepillo</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&a_A;&a_h;&a_w;&a_t;&#8204;&f_p;&a_A;&f_k;&#8204;&f_k;&a_n;</trd>,
    <trd>&a_b;&a_r;&a_damma;&a_s;</trd>
  </trdgrp>
  <trd lng="fr">brosse</trd>
  <trd lng="he">&mem;&#x5B4;&bet;&resh;&#x5B6;&shin;&#x5C1;&#x5B6;&tav;</trd>
  <trd lng="hu">kefe</trd>
  <trd lng="ja">&#x30D6;&#x30E9;&#x30B7;</trd>
  <trd lng="nl">borstel</trd>
  <trd lng="pl">szczotka</trd>
  <trd lng="pt">escova</trd>
  <trd lng="ru">&c_shch;&c_jo;&c_t;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>kart&aacute;&ccaron;</trd>,
    <trd>kefa</trd>
  </trdgrp>
  <trd lng="sv">borste</trd>
  <trd lng="uk">&c_shch;&c_ib;&c_t;&c_k;&c_a;</trd>
</drv>

<drv mrk="bros.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="bros.0i.HEJM">
    <dif>
      Froti per <tld/>o:
      <ekz>
        <tld/>i tapi&scirc;on, barbon, dentojn;
      </ekz>
      <ekz>
        li <tld/>is al si la botojn kaj la surtuton, kombis al si la belajn blondajn
        harojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> <tld/>is tiujn longajn harojn trans la verton
        <fnt>
          <bib>TAA</bib>
          <lok>&Ccirc;apitro VIII.</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ligis al li la kravaton, <klr>[&#8230;]</klr> <tld/>is al li la &ccirc;apelon per la
        manplato
        <fnt>
          <bib>Fab4</bib>
          <lok>Kion la patro faras, estas &ccirc;iam &gcirc;usta</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> placo<klr>[n&#8230;]</klr> zume kaj senhaste <tld/>is specialaj ma&scirc;inoj
        <fnt>
          <bib>MkM</bib>
          <lok>27</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> regule <tld/>u siajn dentojn post abunda man&gcirc;ado
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2004/008297.html">Havi &ccirc;impanzon hejme</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bala.0i">balai</ref>,
      <ref cel="pur.0igi">purigi</ref>,
      <ref cel="polv.sen0igi">senpolvigi</ref>,
      <ref cel="skrap.0i">skrapi</ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#21047;&#25481; <pr>shu&amacron;di&#xE0;o</pr></trd>,
    <trd>&#21047;&#x53BB; <pr>shu&amacron;q&#xF9;</pr></trd>,
    <trd>&#21047; <pr>shu&amacron;</pr></trd>,
    <trd>&#x68B3;&#29702; <pr>sh&#x16B;l&#x1D0;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_ch;&c_y;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol; <ind>&c_sh;&c_ch;&c_o;&c_t;&c_k;&c_a;&c_j;</ind></trd>
  <trd lng="bg">&c_ch;&c_je;&c_t;&c_k;&c_a;&c_m;</trd>
  <trd lng="br">brousta&ntilde;</trd>
  <trd lng="ca">raspallar</trd>
  <trd lng="cs">kart&aacute;&ccaron;ovat</trd>
  <trd lng="de">b&uuml;rsten</trd>
  <trd lng="en">brush</trd>
  <trd lng="es">cepillar</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&a_A;&a_h;&a_w;&a_t;&#8204;&f_p;&a_A;&f_k;&#8204;&f_k;&a_n; &a_z;&a_d;&a_n;</trd>,
    <trd>&a_b;&a_r;&a_damma;&a_s; &a_z;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">brosser</trd>
  <trd lng="he">&lamed;&he;&bet;&resh;&yod;&shin;</trd>
  <trd lng="hu">kef&eacute;l</trd>
  <trd lng="ja">&#x30D6;&#x30E9;&#x30B7;&#12434;&#x304B;&#12369;&#x308B;  <pr>&#12406;&#12425;&#12375;&#12434;&#x304B;&#12369;&#x308B;</pr></trd>
  <trd lng="nl">borstelen</trd>
  <trd lng="pl">szczotkowa&cacute;</trd>
  <trd lng="ru">&c_ch;&c_i;&c_s;&c_t;&c_i;&c_t;&c_mol; &c_shch;&c_jo;&c_t;&c_k;&c_o;&c_j;</trd>
  <trd lng="sk">kefova&tcaron;</trd>
  <trd lng="sv">borsta</trd>
  <trd lng="uk">&c_ch;&c_i;&c_s;&c_t;&c_i;&c_t;&c_i; &c_shch;&c_ib;&c_t;&c_k;&c_o;&c_ju;</trd>
</drv>

<drv mrk="bros.har0o">
  <kap>har<tld/>o</kap>
  <snc mrk="bros.har0o.KOS">
    <dif>
      <tld lit="B"/>o por malimpliki la hararon:
      <ekz>
        suspirinte, &scirc;i <tld/>is siajn longajn harojn per
        har<tld/>o, dum &scirc;i nevole rememoris, kion <nom>Tingting</nom>
        diris
        <fnt>
          <aut>Qiong Yao, trad. Vejdo</aut>
          <vrk><url ref="http://www.ipernity.com/blog/35811/338001">En Profunda Korto</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="komb.0ilo">kombilo</ref>
    <trd lng="zh">&#x53D1;&#21047; <pr>f&#xE0;shu&amacron;</pr></trd>
</snc>
  <trd lng="be">&c_sh;&c_ch;&c_o;&c_t;&c_k;&c_a; &c_d;&c_l;&c_ja; &c_v;&c_a;&c_l;&c_a;&c_s;&c_o;&c_w;</trd>
  <trd lng="bg">&c_ch;&c_je;&c_t;&c_k;&c_a; &c_z;&c_a; &c_k;&c_o;&c_s;&c_a;</trd>
  <trd lng="ca">raspall de cap</trd>
  <trd lng="de">Haarb&uuml;rste</trd>
  <trd lng="en">hairbrush</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_r;&a_damma;&a_s; &a_m;&a_w;</trd>,
    <trd>&a_b;&a_r;&a_damma;&a_s; &a_s;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">brosse &agrave; cheveux</trd>
  <trd lng="ja">&#x30D8;&#x30A2;&#x30D6;&#x30E9;&#x30B7;</trd>
  <trd lng="pl">szczotka do w&lstroke;os&oacute;w</trd>
  <trd lng="sv">h&aring;rborste</trd>
</drv>

<drv mrk="bros.dent0o">
  <kap>dent<tld/>o,
  <var><kap>dento<tld/>o</kap></var></kap>
  <snc mrk="bros.dent0o.KOS">
    <dif>
      <tld lit="B"/>o por la dentoj:
      <ekz>
        Estus peke alkonsili, &#11005; Ke &scirc;i uzu dento<tld/>on
        <fnt>
          <bib>LSB</bib>
          <lok>Vila&gcirc;anino</lok>
        </fnt>;
      </ekz>
      <ekz>
        nur la sapon kaj dent<tld/>on ili forportas pro higienaj ka&ubreve;zoj
        <fnt>
          <bib>VaK</bib>
          <lok>Sesa &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        elektra dento<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="&Monato;/2011/010227.php">Magia tapi&scirc;o</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i nur prenu sian dento<tld/>on kaj pi&jcirc;amon, kaj ek
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2006/008760.php">En fremda lando</url></vrk>
          <lok>2006</lok>
        </fnt>!
      </ekz>
    </dif>
    <trd lng="zh">&#29273;&#21047; <pr>y&#xE1;shu&amacron;</pr></trd>
</snc>
  <trd lng="be">&c_z;&c_u;&c_b;&c_n;&c_a;&c_ja; &c_sh;&c_ch;&c_o;&c_t;&c_k;&c_a;</trd>
  <trd lng="bg">&c_ch;&c_je;&c_t;&c_k;&c_a; &c_z;&c_a; &c_z;&c_malmol;&c_b;&c_i;</trd>
  <trd lng="bo">&t_sa;&t_naro;&t_tsheg;&t_-a;&t_kha;&t_rata;&t_shabchu;&t_tsheg;</trd>
  <trd lng="ca">raspall de dents</trd>
  <trd lng="de">Zahnb&uuml;rste</trd>
  <trd lng="en">toothbrush</trd>
  <trd lng="fa">&a_m;&a_s;&a_w;&a_A;&f_k;</trd>
  <trd lng="fr">brosse &agrave; dents</trd>
  <trd lng="he">&mem;&#x5B4;&bet;&resh;&#x5B6;&shin;&#x5C1;&#x5B6;&tav;
  &shin;&#x5C1;&#x5B4;&nun;&#x5B7;&yod;&#x5B4;&fmem;</trd>
  <trd lng="ja">&#x6B6F;&#x30D6;&#x30E9;&#x30B7;  <pr>&#x306F;&#x30D6;&#x30E9;&#x30B7;</pr></trd>
  <trd lng="pl">szczoteczka do z&eogonek;b&oacute;w</trd>
  <trd lng="sv">tandborste</trd>
  <trd lng="uk">&c_z;&c_u;&c_b;&c_n;&c_a; &c_shch;&c_ib;&c_t;&c_k;&c_a;</trd>
</drv>

<drv mrk="bros.drat0o">
  <kap>
    drat<tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="B"/>o kun metalaj haregoj uzata por prilaboro de metalaj surfacoj:
      <ekz>
        drat<tld/>o estas <tld/>o ne por dratoj, sed el dratoj
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#8470; 8-9 (106-107)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_a;&c_c;&c_ja;&c_n;&c_a;&c_ja; <ind>&c_sh;&c_ch;&c_o;&c_t;&c_k;&c_a;</ind></trd>
  <trd lng="de">Drahtb&uuml;rste</trd>
  <trd lng="en">wire brush</trd>
</drv>

<drv mrk="bros.lav0o">
  <kap>
    lav<tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="B"/>o uzata por lavi ion (ha&ubreve;ton, man&gcirc;ilaron k.a.):
      <ekz>
        sapon oni ne provizas <klr>[&#8230;]</klr> anka&ubreve; ne lav<tld/>ojn
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 27</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_ch;&c_o;&c_t;&c_k;&c_a; &c_d;&c_l;&c_ja; &c_m;&c_y;&c_c;&c_mol;&c_c;&c_ja;</trd>
  <trd lng="de">Sp&uuml;lb&uuml;rste</trd>
  <trd lng="en">washing brush</trd>
</drv>

<drv mrk="bros.visx0o">
  <kap>
    vi&scirc;<tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="B"/>o kun longa tenilo por vi&scirc;i plankon:
      <ekz>
        ni ja havis nek vi&scirc;<tld/>on nek balailon
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 43</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_v;&c_a;&c_b;&c_r;&c_a;</trd>,
    <trd>&c_sh;&c_ch;&c_o;&c_t;&c_k;&c_a; &c_d;&c_l;&c_ja; &c_p;&c_a;&c_d;&c_l;&c_o;&c_g;&c_ib;</trd>
  </trdgrp>
  <trd lng="de">Schrubber</trd>
  <trd lng="en">scrubber</trd>
  <trd lng="uk">&c_Sh;&c_v;&c_a;&c_b;&c_r;&c_a;</trd>
</drv>

</art>
