<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>bros</rad>/o
</kap>

<drv mrk="bros.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Ilo farita el haregoj a&ubreve; vergetoj plantitaj sur tabuleto,
      por forigi
      per froto la malpura&jcirc;on.
    </dif>
  <trd lng="be">&c_sh;&c_ch;&c_o;&c_t;&c_k;&c_a;</trd>
  <trd lng="bg">&c_ch;&c_je;&c_t;&c_k;&c_a;</trd>
  <trd lng="br">broust</trd>
  <trd lng="cs">kart&aacute;&ccaron;</trd>
  <trd lng="de">B&uuml;rste</trd>
  <trd lng="en">brush</trd>
  <trd lng="es">cepillo</trd>
  <trd lng="fr">brosse</trd>
  <trd lng="hu">kefe</trd>   
  <trd lng="nl">borstel</trd>
  <trd lng="pl">szczotka</trd>
  <trd lng="ru">&c_shch;&c_jo;&c_t;&c_k;&c_a;</trd>
  <trd lng="sv">borste</trd>
  </snc>
</drv>

<drv mrk="bros.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Froti ion per <tld/>o:
      <ekz>
        <tld/>i vestojn, tapi&scirc;on, barbon, dentojn, harojn.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="bala.0i">balai</ref>,
      <ref cel="polv.sen0igi">senpolvigi</ref>,
      <ref cel="pur.0igi">purigi</ref>,
      <ref cel="skrap.0i">skrapi</ref>.
    </refgrp>
  <trd lng="be">&c_ch;&c_y;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;
  <ind>&c_sh;&c_ch;&c_o;&c_t;&c_k;&c_a;&c_j;</ind></trd>
  <trd lng="bg">&c_ch;&c_je;&c_t;&c_k;&c_a;&c_m;</trd>
  <trd lng="br">brousta&ntilde;</trd>
  <trd lng="de">b&uuml;rsten</trd>
  <trd lng="es">cepillar</trd>
  <trd lng="fr">brosser</trd>
  <trd lng="hu">kef&eacute;l</trd>     
  <trd lng="nl">borstelen</trd>
  <trd lng="pl">szczotkowa&cacute;</trd>
  <trd lng="ru">&c_ch;&c_i;&c_s;&c_t;&c_i;&c_t;&c_mol;
  &c_shch;&c_jo;&c_t;&c_k;&c_o;&c_j;</trd>
  <trd lng="sv">borsta</trd>
  </snc>
</drv>

<drv mrk="bros.har0o">
  <kap>har<tld/>o</kap>
  <snc>
    <dif>
      <tld/>o por la hararo.
    </dif>
    <trd lng="bg">&c_ch;&c_je;&c_t;&c_k;&c_a; &c_z;&c_a; &c_k;&c_o;&c_s;&c_a;</trd>
    <trd lng="de">Haarb&uuml;rste</trd>
    <trd lng="pl">szczotka do w&lstroke;os&oacute;w</trd>
  </snc>
</drv>

<drv mrk="bros.dent0o">
  <kap>dent<tld/>o</kap>
  <snc>
    <dif>
      <tld/>o por la dentoj.
      <ekz>
        Dentobroso estas ilo por brosi dentojn.
        <fnt><bib>Viki</bib>, 
        <lok>http://eo.wikipedia.org/wiki/Dentobroso</lok></fnt>
      </ekz>
    </dif>
    <trd lng="bg">&c_ch;&c_je;&c_t;&c_k;&c_a; &c_z;&c_a;
    &c_z;&c_malmol;&c_b;&c_i;</trd>
    <trd lng="de">Zahnb&uuml;rste</trd>
    <trd lng="pl">szczoteczka do z&eogonek;b&oacute;w</trd>
  </snc>
</drv>


</art>
