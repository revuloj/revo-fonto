<kap>
  <ofc>*</ofc>
  <rad>mien</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="mien.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="mien.0o.ANA">
    <dif>
      Esprima aspekto de la viza&gcirc;o:
      <ekz>
        gaja, severa, memfida, ruza <tld/>o;
      </ekz>
      <ekz>
        havi vinagron en la <tld/>o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_m;&c_ib;&c_n;&c_a;</trd>,
      <trd>&c_v;&c_y;&c_r;&c_a;&c_z; &c_t;&c_v;&c_a;&c_r;&c_u;</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_s;&f_y;&a_m;&a_A;</trd>,
      <trd>&a_q;&f_y;&a_A;&a_f;&a_h;</trd>,
      <trd>&a_H;&a_A;&a_l;&a_t; <ind>&a_S;&a_w;&a_r;&a_t;</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>arckifejez&eacute;s</trd>,
      <trd>&aacute;br&aacute;zat</trd>,
      <trd>fizimiska</trd>
    </trdgrp>
   <trdgrp lng="it">
      <trd>espressione</trd>,
      <trd>aria<klr> (espressione)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_y;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;
&c_l;&c_i;&c_c;&c_a;</trd>
  </snc>
  <snc mrk="mien.0o.FIG">
    <dif>
      Ekstera aspekto de iu a&ubreve; io:
      <ekz>
        sana <tld/>o;
      </ekz>
      <ekz>
        havi <tld/>on de fripono;
      </ekz>
      <ekz>
        vulpo <tld/>on &scirc;an&gcirc;as, sed plue kokidojn
        man&gcirc;as
        (<ref tip="vid" cel="far.0o">faro</ref>)
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ili faras la <tld/>on kvaza&ubreve; ili laboras<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tie la bordoj de la rivero perdas sian ridantan
        <tld/>on<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_y;&c_g;&c_l;&c_ja;&c_d;</trd>
    <trd lng="fa">&a_Z;&a_A;&a_h;&a_r;</trd>
  <trdgrp lng="hu">
    <trd>kin&eacute;zet</trd>,
    <trd>k&eacute;p <klr tip="ind">(&aacute;br&aacute;zat)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>aspetto</trd>,
    <trd>sembianza<klr> (aria)</klr></trd>
  </trdgrp>
    <trd lng="ru">&c_v;&c_i;&c_d;</trd>
  </snc>
  <trd lng="bg">&c_i;&c_z;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="ca">aire</trd>
  <trdgrp lng="cs">
    <trd>vzez&rcaron;en&iacute;</trd>,
    <trd>v&yacute;raz</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Miene</trd>,
    <trd>Gesichtsausdruck</trd>
  </trdgrp>
  <trd lng="en">expression</trd>
  <trd lng="es">gesto</trd>
  <trdgrp lng="fr">
    <trd>expression du visage</trd>,
    <trd>mine<klr> (expression du visage)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8868;&#x60C5;  <pr>&#x3072;&#x3087;&#x3046;&#x3058;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x9854;&#x3064;&#x304D;  <pr>&#x304B;&#x304A;&#x3064;&#x304D;</pr></trd>,
    <trd>&#x9854;&#x8272;  <pr>&#x304B;&#x304A;&#x3044;&#x308D;</pr></trd>,
    <trd>&#x5916;&#x898B;  <pr>&#x304C;&#x3044;&#x3051;&#x3093;</pr></trd>,
    <trd>&#x614B;&#x5EA6;  <pr>&#x305F;&#x3044;&#x3069;</pr></trd>,
    <trd>&#x3088;&#x3046;&#x3059;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gelaatsuitdrukking</trd>,
    <trd>uiterlijk</trd>
  </trdgrp>
  <trd lng="oc">aire</trd>
  <trd lng="pl">mina <klr>(wyraz twarzy)</klr></trd>
  <trdgrp lng="pt">
     <trd>cara</trd>,
     <trd>semblante</trd>,
     <trd>fisionomia</trd>,
     <trd>apar&ecirc;ncia</trd>,
     <trd>express&atilde;o <klr>(do rosto)</klr></trd>,
     <trd>ar <klr>(aspecto)</klr></trd>
  </trdgrp>
  <trd lng="ro">fa&#x21B;&abreve; <klr>(expresie facial&abreve;)</klr></trd>
  <trd lng="sk">v&yacute;raz <klr>(tv&aacute;re)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_r;&c_a;&c_z; &#x43E;&c_b;&#x43B;&c_i;&c_ch;&c_ch;&#x44F;</trd>,
    <trd>&#x43C;&c_ib;&#x43D;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x795E;&#x8272; <pr>sh&#xE9;ns&#xE8;</pr></trd>,
    <trd>&#x795E;&#x6001; <pr>sh&#xE9;nt&#xE0;i</pr></trd>,
    <trd>&#x795E;&#x614B; <pr>sh&#xE9;nt&#xE0;i</pr></trd>,
    <trd>&#x795E;&#x60C5; <pr>sh&#xE9;nq&#xED;ng</pr></trd>,
    <trd>&#x795E;&#x91C7; <pr>sh&#xE9;nc&#x1CE;i</pr></trd>,
    <trd>&#x8868;&#x60C5; <pr>bi&#x1CE;oq&#xED;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mien.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Havi <tld/>on de, aspekti:
      <ekz>
        <tld/>i kiel malsanulo;
      </ekz>
      <ekz>
        kokete <tld/>i;
      </ekz>
      <ekz>
        la internaciaj vortoj en Esperanto <tld/>as kaj
        elparoli&gcirc;as pli
        slave kaj germane ol latinide.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fizion.0o">fizionomio</ref>,
      <ref cel="ten.sin0o">sinteno</ref>,
      <ref cel="sxajn.0o">&scirc;ajno</ref>,
      <ref cel="ten.0igxo">teni&gcirc;o</ref>,
      <ref cel="trajt.0o">trajtoj</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_g;&c_l;&c_ja;&c_d;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">tv&aacute;&rcaron;it se <klr>(jako)</klr></trd>
  <trdgrp lng="fa">
    <trd>&a_n;&a_m;&a_w;&a_d;&a_n;</trd>,
    <trd>&a_b;&a_h; &a_n;&a_Z;&a_r; &a_A_madda;&a_m;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">avoir l'air</trd>
  <trdgrp lng="hu">
    <trd>arcot v&aacute;g</trd>,
    <trd>k&eacute;pet v&aacute;g</trd>,
    <trd>kin&eacute;z</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>sembrare<klr> (aver cera)</klr></trd>,
    <trd>aver <ind>cera</ind></trd>,
    <trd>parere<klr> (sembrare)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x9854;&#x3064;&#x304D;&#x3092;&#x3057;&#x3066;&#x3044;&#x308B;  <pr>&#x304B;&#x304A;&#x3064;&#x304D;&#x3092;&#x3057;&#x3066;&#x3044;&#x308B;</pr></trd>,
    <trd>&#x898B;&#x3048;&#x308B;  <pr>&#x307E;&#x307F;&#x3048;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">er <ind>uitzien</ind></trd>
  <trdgrp lng="pl">
    <trd>wydawa&cacute; si&eogonek;</trd>
    <trd>wygl&aogonek;da&cacute; na</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>p&abreve;rea</trd>
    <trd>arat&abreve; ca</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_y;&c_g;&c_l;&c_ja;&c_d;&c_je;&c_t;&c_mol;</trd>
  <trd lng="sk">tv&aacute;ri&tcaron; sa</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_a;&c_t;&c_i; &c_v;&c_i;&c_g;&#x43B;&#x44F;&c_d;</trd>,
    <trd>&c_v;&c_i;&c_g;&#x43B;&#x44F;&c_d;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="mien.0eto">
<kap><tld/>eto, <var><kap><tld/>simbolo</kap></var>,
<var><kap><tld/>vinjeto</kap></var></kap>
  <snc mrk="mien.0eto.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <uzo tip="fak">TIP</uzo>
    <dif>
      Viza&gcirc;a bildeto montranta la humoron de redaktanto,
      origine kunfarita el interpunkciaj signoj:
      kontenta :-) malkontenta :-( kaj tiel plu:
      <ekz>
        aldonu vian <tld/>eton en komento
        <fnt>
          <aut>S. Kisa</aut>,
          <vrk><url ref="http://sonjaaa.livejournal.com/">
          La lernema lesbanino</url></vrk>,
          <lok>2005-03-01</lok>
        </fnt>;
      </ekz>
      <ekz>
        aperis novaj tre &ccirc;armaj kaj pla&ccirc;aj <tld/>simboloj en
        <ctl>Lernu</ctl>
        <fnt>
          <aut>Terur&ccirc;jo</aut>,
          <vrk>
<url ref="http://cs.lernu.net/komunikado/forumo/temo.php?t=1460">
          Novaj miensimboloj...</url></vrk>,
          <lok>2007-11-13</lok>
        </fnt>;
      </ekz>
      <ekz>
        Katalin demandis, kiel oni povas facile aldoni <tld/>vinjetojn
        al sia komentoj
        <fnt>
          <aut>P. Peeraerts</aut>,
          <vrk><url ref="http://www.ipernity.com/group/37673/discuss/14463">
          Kiel aldoni...</url></vrk>,
          <lok>2008-02-26</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_m;&c_a;&c_j;&c_l;&c_ib;&c_k;</trd>
  <trd lng="bg">&c_je;&c_m;&c_o;&c_t;&c_i;&c_k;&c_o;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Emoticon</trd>,
    <trd>Smiley</trd>
  </trdgrp>
  <trd lng="en">smiley</trd>
  <trd lng="fa">&a_A;&a_s;&a_m;&a_A;&f_y;&a_l;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>binette<klr> (&eacute;motic&ocirc;ne)</klr></trd>,
    <trd>&eacute;motic&ocirc;ne</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>faccina<klr> (icona del computer)</klr></trd>,
    <trd>emoticon</trd>
  </trdgrp>
  <trd lng="ja">&#x9854;&#x6587;&#x5B57;  <pr>&#x304B;&#x304A;&#x3082;&#x3058;</pr></trd>
  <trdgrp lng="pl">
    <trd>emotikon</trd>
    <trd>minka</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>emoticon</trd>
    <trd>fa&#x21B;&abreve; z&acirc;mbitoare</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7B11;&#x8138; <pr>xi&#xE0;oli&#x1CE;n</pr></trd>,
    <trd>&#x7B11;&#x81C9; <pr>xi&#xE0;oli&#x1CE;n</pr></trd>,
    <trd>&#x8868;&#x60C5;&#x7B26;&#x53F7; <pr>bi&#x1CE;oq&#xED;ngf&#xFA;h&#xE0;o</pr></trd>,
    <trd>&#x8868;&#x60C5;&#x7B26;&#x865F; <pr>bi&#x1CE;oq&#xED;ngf&#xFA;h&#xE0;o</pr></trd>,
    <trd>&#x8868;&#x60C5;&#x56FE; <pr>bi&#x1CE;oq&#xED;ngt&#xFA;</pr></trd>,
    <trd>&#x8868;&#x60C5;&#x5716; <pr>bi&#x1CE;oq&#xED;ngt&#xFA;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mien.grav0eco">
  <kap>grav<tld/>eco<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Mieno, aspekto serioza, sen&scirc;erca:
      <ekz>
        grav<tld/>a homo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_r;&#x2019;&c_jo;&c_z;&c_n;&c_y; &c_v;&c_y;&c_g;&c_l;&c_ja;&c_d;</trd>,
    <trd>&c_z;&c_m;&c_r;&c_o;&c_ch;&c_n;&c_y; &c_v;&c_y;&c_g;&c_l;&c_ja;&c_d;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_je;&c_r;&c_i;&c_o;&c_z;&c_n;&c_o; &c_i;&c_z;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="fr">
    <trd>air important</trd>,
    <trd>gravit&eacute;<klr> (air important)</klr></trd>,
    <trd>s&eacute;rieux<klr> (air important)</klr></trd>
  </trdgrp>
  <trd lng="hu">fontoskod&aacute;s</trd>
  <trd lng="it">gravit&agrave;<klr> (di persona seria o
  severa)</klr></trd>
  <trd lng="pl">powaga twarzy</trd>
  <trd lng="ro">seriozitatea fe&#x21B;ei</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_a;&c_zh;&c_n;&c_y;&c_j; &c_v;&c_i;&c_d;</trd>,
    <trd>&c_s;&c_je;&c_r;&c_mol;&c_jo;&c_z;&c_n;&c_y;&c_j;
    &c_v;&c_i;&c_d;</trd>
  </trdgrp>
</drv>

<drv mrk="mien.nub0a">
  <kap>nub<tld/>a</kap>
  <snc mrk="mien.nub0a.MIEN">
    <dif>
      Kun <ref tip="vid" cel="nub.0a.maltrankviliga">nuba</ref>,
      maltrankvila <tld/>o,
      <ref tip="vid" cel="moroz.0a">moroza</ref>:
      <ekz>
        mi e&ccirc; ne rekonas vin, vi ne estas tiu nub<tld/>a sinjoro
        Georgo
        <fnt>
          <aut>B. N&ecaron;mcov&aacute;, trad. V. Tobek kaj K.
          Proch&aacute;zka</aut>,
          <vrk><url ref="https://eo.wikisource.org/wiki/Avineto/XIV.">
          Avineto</url></vrk>,
          <lok>1909-1914</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_m;&c_r;&c_o;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_h;&c_m;&c_u;&c_r;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_u;&c_r;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>maussade</trd>,
    <trd>renfrogn&eacute;</trd>
  </trdgrp>
  <trd lng="pl">z pochmurn&aogonek; <ind>min&aogonek;</ind></trd>
  <trd lng="ro">cu fa&#x21B;a tulbure</trd>
</drv>
</art>
