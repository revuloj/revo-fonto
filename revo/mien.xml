<drv mrk="mien.grav0eco">
  <kap>grav<tld/>a</kap>
  <snc>
    <dif>
      Kun <tld/>o, aspekto serioza, sen&scirc;erca:
      <ekz>
        <nom>Ivan</nom> aspektis subite tre grav<tld/>a
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun troigita grav<tld/>eco <nom>Don Isaak</nom> alproksimi&gcirc;is
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        grav<tld/>aj oficistoj rapidis de la re&gcirc;a palaco al la domoj de altranguloj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_r;&#x2019;&c_jo;&c_z;&c_n;&c_y; &c_v;&c_y;&c_g;&c_l;&c_ja;&c_d;</trd>,
    <trd>&c_z;&c_m;&c_r;&c_o;&c_ch;&c_n;&c_y; &c_v;&c_y;&c_g;&c_l;&c_ja;&c_d;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_je;&c_r;&c_i;&c_o;&c_z;&c_n;&c_o;
  &c_i;&c_z;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="de">
    <trd>mit ernster <ind>Miene</ind></trd>,
    <trd>wichtigtuerisch</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>air important</trd>,
    <trd>gravit&eacute;<klr> (air important)</klr></trd>,
    <trd>s&eacute;rieux<klr> (air important)</klr></trd>
  </trdgrp>
  <trd lng="hu">fontoskod&aacute;s</trd>
  <trdgrp lng="id">
    <trd><baz>serius</baz>keseriusan</trd>,
    <trd><baz>sungguh</baz>kesungguhan</trd>
  </trdgrp>
  <trd lng="it">gravit&agrave;<klr> (di persona seria o
  severa)</klr></trd>
  <trd lng="pl">powaga twarzy</trd>
  <trd lng="ro">seriozitatea fe&tcomma;ei</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_a;&c_zh;&c_n;&c_y;&c_j; &c_v;&c_i;&c_d;</trd>,
    <trd>&c_s;&c_je;&c_r;&c_mol;&c_jo;&c_z;&c_n;&c_y;&c_j;
    &c_v;&c_i;&c_d;</trd>
  </trdgrp>
</drv>

<drv mrk="mien.nub0a">
  <kap>nub<tld/>a</kap>
  <snc mrk="mien.nub0a.MIEN">
    <dif>
      Kun <ref tip="vid" cel="nub.0a.maltrankviliga">nuba</ref>,
      maltrankvila <tld/>o,
      <ref tip="vid" cel="moroz.0a">moroza</ref>:
      <ekz>
        mi e&ccirc; ne rekonas vin, vi ne estas tiu nub<tld/>a sinjoro
        Georgo
        <fnt>
          <aut>B. N&ecaron;mcov&aacute;, trad. V. Tobek kaj K.
          Proch&aacute;zka</aut>,
          <vrk><url ref="https://eo.wikisource.org/wiki/Avineto/XIV.">
          Avineto</url></vrk>,
          <lok>1909-1914</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_m;&c_r;&c_o;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_h;&c_m;&c_u;&c_r;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_u;&c_r;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>m&uuml;rrisch</trd>,
    <trd>verstimmt</trd>
  </trdgrp>
  <trdgrp lng="fr">
