  </trdgrp>
  <trd lng="ro">fa&tcomma;&abreve; <klr>(expresie facial&abreve;)</klr></trd>
  <trd lng="sk">v&yacute;raz <klr>(tv&aacute;re)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_r;&c_a;&c_z; &c_o;&c_b;&c_l;&c_i;&c_ch;&c_ch;&c_ja;</trd>,
    <trd>&c_m;&c_ib;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x795E;&#x8272; <pr>sh&eacute;ns&egrave;</pr></trd>,
    <trd>&#x795E;&#x6001; <pr>sh&eacute;nt&agrave;i</pr></trd>,
    <trd>&#x795E;&#x614B; <pr>sh&eacute;nt&agrave;i</pr></trd>,
    <trd>&#x795E;&#x60C5; <pr>sh&eacute;nq&iacute;ng</pr></trd>,
