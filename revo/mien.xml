<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mien</rad>/o
</kap>
<drv mrk="mien.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Esprima aspekto de la viza&gcirc;o:
      <ekz>
        gaja, severa, memfida, ruza <tld/>o;
      </ekz>
      <ekz>
        grav<tld/>a homo;
      </ekz>
      <ekz>
        grav<tld/>eco<fnt>Z</fnt>;
      </ekz>
      <ekz>havi vinagron en la <tld/>o
<fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
    <trd lng="ru">&c_v;&c_y;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je; &c_l;&c_i;&c_c;&c_a;</trd>
  </snc>
  <snc>
    <dif>
      Ekstera aspekto de iu a&ubreve; io:
      <ekz>
        sana <tld/>o;
      </ekz>
      <ekz>
        havi <tld/>on de fripono;
      </ekz>
      <ekz>vulpo <tld/>on &scirc;an&gcirc;as, sed plue kokidojn man&gcirc;as
        (<ref tip="vid" cel="far.0o">faro</ref>)
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        ili faras la <tld/>on kvaza&ubreve; ili laboras<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tie la bordoj de la rivero perdas sian ridantan
        <tld/>on<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_v;&c_i;&c_d;</trd>
  </snc>
  <trdgrp lng="de">
    <trd>Miene</trd>,
    <trd>Gesichtsausdruck</trd>
  </trdgrp>
  <trd lng="en">expression</trd>
  <trdgrp lng="nl">
    <trd>gelaatsuitdrukking</trd>,
    <trd>uiterlijk</trd>
  </trdgrp>
</drv>

<drv mrk="mien.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Havi <tld/>on de, aspekti:
      <ekz>
        <tld/>i kiel malsanulo;
      </ekz>
      <ekz>
        kokete <tld/>i;
      </ekz>
      <ekz>
        la internaciaj vortoj en Esperanto <tld/>as kaj elparoli&gcirc;as pli
        slave kaj germane ol latinide.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fizion.0o">fizionomio</ref>,
      <ref cel="trajt.0o">trajtoj</ref>,
      <ref cel="sxajn.0o">&scirc;ajno</ref>,
      <ref cel="ten.sin0o">sintenado</ref>,
      <ref cel="ten.0igxo">teni&gcirc;o</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="fier.0a">fiera</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">er <ind>uitzien</ind></trd>
  <trd lng="ru">&c_v;&c_y;&c_g;&c_l;&c_ja;&c_d;&c_je;&c_t;&c_mol;</trd>
</drv>
</art>
