</kap>
<drv mrk="mien.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="mien.0o.ANA">
    <dif>
      Esprima aspekto de la <ref tip="malprt" cel="vizagx.0o">viza&gcirc;o</ref>:
      <ekz>
        li havis <tld/>on tre &gcirc;enitan
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> knabino rigardis &scirc;in kun serioza <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i balancis la kapon kaj petis lin per la rigardo kaj la <tld/>o, ke li denove
        foriru
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili &gcirc;emis kaj faris tiajn ekstazajn <tld/>ojn
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> kvaza&ubreve; tritaga pluvo
        <fnt>
          <bib>ElektFab</bib>
          <lok>La bremenaj muzikistoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> faris <tld/>ojn kvaza&ubreve; ili laboras, sed havis nenion sur la
        teksiloj
        <fnt>
          <bib>Fab1</bib>
          <lok>La novaj vestoj de la re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        malsupre de la Rejna provinco <klr>[&#x2026;]</klr> la bordoj de la rivero perdas sian
        ridantan <tld/>on
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        fiera <tld/>o &dash; kapo malplena
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        havi vinagron en la <tld/>o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="vizagx.0o.MIEN">viza&gcirc;o<sncref/></ref>
    <refgrp tip="vid">
      <ref cel="fizion.0o">fizionomio</ref>,
      <ref cel="ten.sin0o">sinteno</ref>,
      <ref cel="sxajn.0o">&scirc;ajno</ref>,
      <ref cel="ten.0igxo">teni&gcirc;o</ref>,
      <ref cel="trajt.0o">trajtoj</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_m;&c_ib;&c_n;&c_a;</trd>,
      <trd>&c_v;&c_y;&c_r;&c_a;&c_z; &c_t;&c_v;&c_a;&c_r;&c_u;</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_s;&f_y;&a_m;&a_A;</trd>,
      <trd>&a_q;&f_y;&a_A;&a_f;&a_h;</trd>,
      <trd>&a_H;&a_A;&a_l;&a_t; <ind>&a_S;&a_w;&a_r;&a_t;</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>arckifejez&eacute;s</trd>,
      <trd>&aacute;br&aacute;zat</trd>,
      <trd>fizimiska</trd>
    </trdgrp>
   <trdgrp lng="it">
      <trd>espressione</trd>,
      <trd>aria<klr> (espressione)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_y;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;
&c_l;&c_i;&c_c;&c_a;</trd>
  </snc>
<!-- aparta senco por malofta figura uzo ne necesas:
  <snc mrk="mien.0o.FIG">
    <dif>
      Ekstera aspekto de iu a&ubreve; io:
      <ekz>
        sana <tld/>o;
      </ekz>
      <ekz>
        havi <tld/>on de fripono;
      </ekz>
      <ekz>
        vulpo <tld/>on &scirc;an&gcirc;as, sed plue kokidojn
        man&gcirc;as
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_y;&c_g;&c_l;&c_ja;&c_d;</trd>
    <trd lng="fa">&a_Z;&a_A;&a_h;&a_r;</trd>
    <trdgrp lng="hu">
      <trd>kin&eacute;zet</trd>,
      <trd>k&eacute;p <klr tip="ind">(&aacute;br&aacute;zat)</klr></trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>aspetto</trd>,
      <trd>sembianza<klr> (aria)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_i;&c_d;</trd>
  </snc>
-->
  <trd lng="bg">&c_i;&c_z;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="ca">
    <trd>aire <klr tip="amb">(aparen&ccedil;a)</klr></trd>,
    <trd>aspecte</trd>,
    <trd>cara <klr tip="amb">(aparen&ccedil;a)</klr></trd>,
    <trd>semblant</trd>,
    <trd>posat</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>vzez&rcaron;en&iacute;</trd>,
    <trd>v&yacute;raz</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Miene</trd>,
    <trd>Gesichtsausdruck</trd>
  </trdgrp>
  <trd lng="en">expression</trd>
  <trd lng="es">gesto</trd>
  <trdgrp lng="fr">
    <trd>expression du visage</trd>,
    <trd>mine<klr> (expression du visage)</klr></trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>air muka</trd>,
    <trd>ekspresi</trd>,
    <trd>paras</trd>,
    <trd>roman</trd>,
    <trd>rupa muka</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8868;&#x60C5;  <pr>&#x3072;&#x3087;&#x3046;&#x3058;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x9854;&#x3064;&#x304D;  <pr>&#x304B;&#x304A;&#x3064;&#x304D;</pr></trd>,
    <trd>&#x9854;&#x8272;  <pr>&#x304B;&#x304A;&#x3044;&#x308D;</pr></trd>,
    <trd>&#x5916;&#x898B;  <pr>&#x304C;&#x3044;&#x3051;&#x3093;</pr></trd>,
    <trd>&#x614B;&#x5EA6;  <pr>&#x305F;&#x3044;&#x3069;</pr></trd>,
    <trd>&#x3088;&#x3046;&#x3059;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gelaatsuitdrukking</trd>,
    <trd>uiterlijk</trd>
  </trdgrp>
  <trd lng="oc">aire</trd>
  <trd lng="pl">mina <klr>(wyraz twarzy)</klr></trd>
  <trdgrp lng="pt">
     <trd>cara</trd>,
     <trd>semblante</trd>,
     <trd>fisionomia</trd>,
     <trd>apar&ecirc;ncia</trd>,
     <trd>express&atilde;o <klr>(do rosto)</klr></trd>,
