  <trd lng="de">Malthusianismus</trd>
  <trd lng="en"><ind>Malthus</ind>ianism</trd>
  <trd lng="fr">malthusianisme</trd>
  <trd lng="hu">malthusianizmus</trd>
  <trd lng="nl">malthusianisme</trd>
  <trd lng="pl">maltuzjanizm</trd>
  <trd lng="pt">malthusianismo</trd>
  <trdgrp lng="ro">
    <trd>malthusianism</trd>,
    <trd>teoria dominoului</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_a;&c_l;&c_mol;&c_t;&c_u;&c_z;&c_i;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>
  <trdgrp lng="zh">
