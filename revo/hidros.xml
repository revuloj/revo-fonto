<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hidros.xml,v 1.18 2022/03/20 15:36:34 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>hidrostatik</rad>/o
</kap>

<drv mrk="hidros.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Bran&ccirc;o de <ref tip="malprt" cel="fizik.0o">fiziko</ref>,
      studanta la ekvilibron de fluida&jcirc;oj.
    </dif>
  </snc>
  <snc mrk="hidros.0o.fenomenoj">
    <dif>
      Fizikaj fenomenoj de senmovaj fluida&jcirc;oj:
      <ekz>
        <tld/>a premo estas la ortanta fortefiko de premita sed statika fluido sur la
        unuon de areo pro la gravito
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=8144">Premo de fluido</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_d;&c_r;&c_a;&c_s;&c_t;&c_a;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="bg">&c_h;&c_i;&c_d;&c_r;&c_o;&c_s;&c_t;&c_a;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="el">&ypsilon;&delta;&rho;&omikron;&sigma;&tau;&alfa;&tau;&jota;&kappa;&eta_ton;</trd>
  <trd lng="en">hydrostatics</trd>
  <trdgrp lng="de">
     <trd>Hydrostatik</trd>,
     <trd>Lehre von den ruhenden Fl&uuml;ssigkeiten</trd>
  </trdgrp>
  <trd lng="es">hidrost&aacute;tica</trd>
  <trd lng="fr">hydrostatique</trd>
  <trdgrp lng="hu">
     <trd>hidrosztatika</trd>,
     <trd>folyad&eacute;kstatika</trd>
  </trdgrp>
  <trd lng="it">idrostatica</trd>
  <trd lng="nl">hydrostatica</trd>
  <trd lng="pl">hydrostatyka</trd>
  <trd lng="pt">hidrost&aacute;tica</trd>
  <trd lng="ru">&c_g;&c_i;&c_d;&c_r;&c_o;&c_s;&c_t;&c_a;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="uk">&#x433;&#x456;&#x434;&#x440;&#x43E;&#x441;&#x442;&#x430;&#x442;&#x438;&#x43A;&#x430;</trd>
</drv>
</art>
