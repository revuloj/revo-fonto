<kap>
  <rad>municip</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="municip.0o">
  <kap><tld/>o</kap>
  <snc mrk="municip.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Loka administracio de vila&gcirc;o, urbo a&ubreve; urbega distrikto:
      <ekz>
        li sukcesis levi Macondon al la kategorio de <tld/>o
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>,
        <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>;
      </ekz>
      <ekz>
        oni diskutis pri senpaga disponigo de ejoj, kiuj apartenas al 
        la urba <tld/>o
        <fnt><aut>P. Jegorovas</aut>, <vrk>Litovia Kalejdoskopo</vrk>, 
        <bib>LOdE</bib>, <lok>2004:4 (114)</lok></fnt>.
      </ekz>
    </dif>
  <ref tip="vid" cel="komun.0umo">komunumo.</ref>
  </snc>
  <trd lng="bg">&c_o;&c_b;&c_shch;&c_i;&c_n;&c_a;</trd>
  <trd lng="cs">municipium</trd>
  <trd lng="da">kommune</trd>
  <trd lng="de">Gemeinde</trd>
  <trd lng="el">&delta;&eta_ton;&my;&omikron;&sigma_fina;</trd>
  <trd lng="en">municipality</trd>
  <trd lng="es">municipios</trd>
  <trd lng="fi">kunta</trd>
  <trdgrp lng="fr">
    <trd>commune<klr> (municipalit&eacute;)</klr></trd>,
    <trd>municipalit&eacute;</trd>
  </trdgrp>
  <trd lng="it">municipio</trd>
  <trd lng="ja">&#x81EA;&#x6CBB;&#x4F53;  <pr>&#x3058;&#x3061;&#x305F;&#x3044;</pr></trd>
  <trd lng="nl">gemeente</trd>
  <trd lng="no">kommune</trd>
  <trdgrp lng="pl">
    <trd>gmina</trd>,
    <trd>gr&oacute;d</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>munic&iacute;pio</trd>,
    <trd>concelho</trd>
  </trdgrp>
  <trd lng="ro">municipiu</trd>
  <trd lng="sk">municipium</trd>
  <trd lng="sv">kommun</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x443;&#x43D;&#x456;&#x446;&#x438;&#x43F;&#x430;&#x43B;&#x456;&#x442;&#x435;&#x442;</trd>,
    <trd>&#x43C;&#x456;&#x441;&#x44C;&#x43A;&#x435; &#x441;&#x430;&#x43C;&#x43E;&#x432;&#x440;&#x44F;&#x434;&#x443;&#x432;&#x430;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5E02; <pr>sh&#xEC;</pr></trd>,
    <trd>&#x57FA;&#x5C42;&#x653F;&#x6743; <pr>j&#x12B;c&#xE9;ngzh&#xE8;ngqu&#xE1;n</pr></trd>,
    <trd>&#x57FA;&#x5C64;&#x653F;&#x6B0A; <pr>j&#x12B;c&#xE9;ngzh&#xE8;ngqu&#xE1;n</pr></trd>,
    <trd>&#x884C;&#x653F;&#x6751; <pr>x&#xED;ngzh&#xE8;ngc&#x16B;n</pr></trd>
  </trdgrp>
</drv>
