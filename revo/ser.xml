<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ser</rad>/o
</kap>
<drv mrk="ser.0o">
  <kap><tld/>o</kap>
  <snc mrk="ser.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <uzo tip="fak">MED</uzo>
    <dif>
      Diafana likva&jcirc;o, sukceneflava, disi&gcirc;anta el la sango dum &gcirc;ia
      koaguli&gcirc;o.
    </dif>
    <refgrp tip="vid">
      <ref cel="serum.0o">serumo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_v;&c_a;&c_r;&c_a;&c_t;&c_k;&c_a;</trd>
  <trd lng="de">Serum</trd>
  <trd lng="en">serum <klr tip="amb">(blood)</klr></trd>
  <trd lng="fr">s&eacute;rum</trd>
  <trdgrp lng="hu">
    <trd>v&eacute;rsav&oacute;</trd>,
    <trd>sz&eacute;rum</trd>
  </trdgrp>
  <trd lng="it">siero<klr tip="amb"> (sangue)</klr></trd>
  <trdgrp lng="nl">
    <trd>serum</trd>,
    <trd>bloedwei</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_y;&c_v;&c_o;&c_r;&c_o;&c_t;&c_k;&c_a; <klr
tip="amb">(&c_k;&c_r;&c_o;&c_v;&c_i;)</klr></trd>
</drv>
</art>
