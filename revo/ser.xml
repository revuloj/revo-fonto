<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ser</rad>/o
</kap>
<drv mrk="ser.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Diafana likva&jcirc;o, sukceneflava, disi&gcirc;anta el la sango dum &gcirc;ia
      koaguli&gcirc;o.
    </dif>
    <refgrp tip="vid">
      <ref cel="serum.0o">serumo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Serum</trd>
  <trdgrp lng="nl">
    <trd>serum</trd>,
    <trd>bloedwei</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_y;&c_v;&c_o;&c_r;&c_o;&c_t;&c_k;&c_a; <klr tip="amb">(&c_k;&c_r;&c_o;&c_v;&c_i;)</klr></trd>
</drv>

<drv mrk="ser.0oterapio">
  <kap><tld/>oterapio</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Kuracado de malsanoj per <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Serumtherapie</trd>,
    <trd>Serumbehandlung</trd>
  </trdgrp>
  <trd lng="nl">serumtherapie</trd>
  <trd lng="ru">&c_s;&c_je;&c_r;&c_o;&c_t;&c_je;&c_r;&c_a;&c_p;&c_i;&c_ja;</trd>
</drv>
</art>
