<drv mrk="pez.0a">
  <kap><tld/>a</kap>
  <snc mrk="pez.0a.havanta_0on">
    <dif>
      Havanta ian <tld/>on:
      <ekz>
        oro estas pli peza ol fero
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kubforma satelito, malpli ol 500 gramojn <tld/>a
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2014/011232.php">Kosma kubo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="br">pouezant</trd>
    <trd lng="ru">&c_i;&c_m;&c_je;&c_ju;&c_shch;&c_i;&c_j; <ind>&c_v;&c_je;&c_s;</ind></trd>
  </snc>
  <snc mrk="pez.0a.multe0a">
    <subsnc>
      <dif>
        Havanta grandan <tld/>on:
        <ekz>
          <tld/>a kiel plumbo;
        </ekz>
        <ekz>
          <tld/>a <klr>(malfacila, mallerta)</klr> pa&scirc;ado;
        </ekz>
        <ekz>
          &gcirc;i estas tiel <tld/>a, kvaza&ubreve; en &gcirc;i trovi&gcirc;us &scirc;tonoj
          <fnt>
            <bib>Fab1</bib>
            <lok>Malgranda Niko kaj granda Niko</lok>
          </fnt>;
        </ekz>
        <ekz>
          <tld/>a atmosfero, vetero.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="sxargx">&scirc;ar&gcirc;a</ref>
      </refgrp>
      <trd lng="br">pounner</trd>
      <trd lng="de">schwer</trd>
    </subsnc>
    <subsnc>
      <uzo tip="stl">FIG</uzo>
      <dif>
        Grava, peniga, teda:
        <ekz>
          <tld/>a estas &scirc;tono, kaj sablo estas &scirc;ar&gcirc;o, sed kolero de malsa&gcirc;ulo estas pli <tld/>a ol amba&ubreve;
          <fnt>
            <bib>MT</bib>
            <lok>&Sen; 27:3</lok>
          </fnt>;
        </ekz>
        <ekz>
          vi sentigis al via popolo <tld/>an sorton
          <fnt>
            <bib>MT</bib>
            <lok>&Psa; 60:3</lok>
          </fnt>;
        </ekz>
        <ekz>
          io <tld/>e ku&scirc;as sur lia konscienco
          <fnt>
            <bib>BdV</bib>
          </fnt>;
        </ekz>
        <ekz>
          ilia pekado estas tre <tld/>a
          <fnt>
            <bib>MT</bib>
            <lok>&Gen; 18:20</lok>
          </fnt>;
        </ekz>
        <ekz>
          ne povas mi sola porti tiun tutan popolon, &ccirc;ar &gcirc;i estas tro <tld/>a por mi
          <fnt>
            <bib>MT</bib>
            <lok>&Nom; 11:14</lok>
          </fnt>;
        </ekz>
        <ekz>
          <tld/>a jugo
          <fnt>
            <bib>MT</bib>
            <lok>&Re1; 12:4</lok>
          </fnt>;
        </ekz>
        <ekz>
          <uzo tip="stl">FIG</uzo>
          du <tld/>aj larmoj defluis sur la vangoj de la serioza viro
          <fnt>
            <bib>Fab3</bib>
            <lok>Historio</lok>
          </fnt>;
        </ekz>
        <ekz>
          por amiko komplezo neniam estas tro <tld/>a
          <fnt>
            <bib>PrV</bib>
          </fnt>;
        </ekz>
        <ekz>
          <uzo tip="fak">TEK</uzo>
          <uzo tip="fak">EKON</uzo>
          <tld/>a industrio
          <klr>(<ref tip="sin" cel="metalu.0o">metalurgio</ref>)</klr>
          <fnt>
            <bib>Monato</bib>
            <aut>Garbhan MacAoidh</aut>
            <vrk><url ref="&Monato;/2006/009062.php">Enmigrado anta kaj inta</url></vrk>
          </fnt>;
          <trd lng="de">Schwerindustrie</trd>
        </ekz>
      </dif>
      <trdgrp lng="br">
        <trd>a-bouez</trd>,
        <trd>pouezus</trd>
      </trdgrp>
      <trd lng="de">schwer</trd>
    </subsnc>
    <ref tip="sin" cel="pez.multe0a">multe<tld/>a</ref>
  </snc>
  <trd lng="af">swaar</trd>
  <trd lng="am">&#x12A8;&#x1263;&#x12F5;</trd>
  <trd lng="ar">&a_th;&a_q;&a_y;&a_l;&a_t_marbuta;</trd>
