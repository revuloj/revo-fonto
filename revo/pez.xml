<drv mrk="pez.mal0igo">
  <kap>mal<tld/>igo,
  <var><kap>sen<tld/>igo</kap></var></kap>
  <snc mrk="pez.mal0igo.FIZ">
    <dif>
      Forigo de fizika <tld/>o.
    </dif>
  </snc>
  <snc mrk="pez.mal0igo.FIG">
    <dif>
      Forigo de premo, de balasto, de embaraso:
      <ekz>
        mal<tld/>igo kaj elastigo de la stilo
        <fnt>
          <aut>F. J. Dorr</aut>
          <vrk>La lingvoteorioj de Karolo Pi&ccirc;</vrk>
          <lok>edukado.net, 2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">soulagement</trd>
  </snc>
  <trd lng="fr">all&egrave;gement</trd>
</drv>

<drv mrk="pez.mal0igxi">
  <kap>mal<tld/>i&gcirc;i,
  <var><kap>sen<tld/>i&gcirc;i</kap></var></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="pez.mal0igxi.FIZ">
    <dif>
      I&gcirc;i korpe malpli <tld/>a, malpli premi&gcirc;anta 
      malsupren:
      <ekz>
        la &scirc;alupo mal<tld/>i&gcirc;os, kaj ne dronos
        <fnt>
          <aut>G. Altov, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/TRIZ/angulo.html">
          Angulo de atako</url></vrk>,
          <lok>[vidita en 2015]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="pez.mal0igxi.FIG">
    <dif>
      I&gcirc;i anime malpli <tld/>a, malpli teda a&ubreve; tedata, malpli 
      malagrabla:
      <ekz>
        iliaj koroj maljunaj mal<tld/>i&gcirc;is
        <fnt>
          <aut>H. Weinhengst</aut>,
          <vrk><url ref="http://www.fadedpage.com/books/20131216/20131216.html">
          Tur-strato</url></vrk>,
          <lok>1934</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="facil.0igxi.malpeze">facili&gcirc;i</ref>,
    <ref cel="sxargx.sen0igxi.KOMUNE">sen&scirc;ar&gcirc;i&gcirc;i</ref>
  </refgrp>
  <trd lng="de"><ind>leichter</ind> werden</trd>
  <trdgrp lng="fr">
    <trd>s'<ind>all&eacute;ger</ind></trd>,
    <trd>se <ind>d&eacute;lester</ind></trd>
  </trdgrp>
</drv>

<drv mrk="pez.mal0igxo">
  <kap>mal<tld/>i&gcirc;o,
  <var><kap>sen<tld/>i&gcirc;o</kap></var></kap>
  <snc mrk="pez.mal0igxo.FIZ">
    <dif>
     Malgrandigo de korpa <tld/>o:
      <ekz>
        virino eli&gcirc;is, kale&scirc;o sen<tld/>i&gcirc;is
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="fr">soulagement</trd>
    <trd lng="de">Erleichterung</trd>
  </snc>
  <snc mrk="pez.mal0igxo.FIG">
    <dif>
      <ref tip="vid" cel="trankv.0igxi">Trankvili&gcirc;o</ref>
      pro forigo de zorgo, minaco, malagrabla&jcirc;o:
      <ekz>
        kun sento de intensa sen<tld/>i&gcirc;o, mi vidis nun
        ke la afero estos multe malpli humiliga ol mi atendis
        <fnt><aut>A. L&ouml;wenstein</aut> <vrk>La &scirc;tona urbo</vrk> 
        <lok>1999</lok></fnt>;
      </ekz>
      <ekz>
        <nom>Pujols</nom> faris geston de profunda mal<tld/>i&gcirc;o
        kaj diris: &dash; Kia trankvilo!
        <fnt>
          <aut>guillem</aut>
          <vrk><url ref="http://www.ipernity.com/blog/niforiru/182478">
          Francesc Pujols</url></vrk>
          <lok>2009-09-09</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">all&egrave;gement</trd>
</drv>


<drv mrk="pez.multe0a">
  <kap>multe<tld/>a</kap>
  <fnt><bib>PV</bib></fnt>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="pez.0a.multe0a"><tld lit="P"/>a<sncref/></ref>:
      <ekz>
        &scirc;i kun plezuro <klr>[&#x2026;]</klr> de&jcirc;etus la multe<tld/>an kronon
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vagonaro <klr>[&#x2026;]</klr> estis longa, multe<tld/>a
        kaj konsistis preska&ubreve; nur el personvagonoj
        <fnt><bib>FK</bib> <lok>La nigra virino</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kelkaj <klr>[&#x2026;]</klr> skribas senerare, sed per mult<tld/>a, malagrabla
        stilo
        <fnt>
          <bib>LR</bib>
          <vrk><url ref="https://eo.wikisource.org/wiki/Lingvaj_Respondoj/Letero_pri_la_deveno_de_Esperanto">Letero
          pri la deveno de Esperanto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <ind>multe<tld/>eco</ind>n kaj malgraciecon de la lingvo
        mi alskribis al tio, ke mi ankora&ubreve; ne sufi&ccirc;e bone &gcirc;in
        posedas
        <fnt>
          <aut>L.L. Zamenhof</aut>
          <vrk><url ref="http://albertclaesen.be/taal/laponto_esp/laponto12.pdf">Letero
          de D-ro L.L. Zamenhof al Nikolaj A. Borovko (1895)</url></vrk>
        </fnt>.
        <trd lng="br">pounnerder</trd>
        <trdgrp lng="fr">
          <trd>lourdeur</trd>,
          <trd>pesanteur<klr tip="ind"> (lourdeur)</klr></trd>,
        </trdgrp>
        <trdgrp lng="hu">
          <trd>s&uacute;lyoss&aacute;g</trd>,
          <trd>neh&eacute;zkess&eacute;g</trd>,
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_ja;&c_zh;&c_k;&c_ib;</trd>,
    <trd>&c_c;&c_ja;&c_zh;&c_k;&c_a;&c_v;&c_a;&c_g;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_c;&c_ja;&c_zh;&c_k;&c_a;&c_v;&c_a;&c_zh;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="br">pounner</trd>
  <trdgrp lng="de">
    <trd>schwer</trd>,
    <trd>gewichtig</trd>
  </trdgrp>
  <trd lng="en">weighty</trd>
  <trdgrp lng="fr">
    <trd>lourd</trd>,
    <trd>pesant</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nagyon <ind>s&uacute;lyos</ind></trd>,
    <trd>neh&eacute;z</trd>,
    <trd>neh&eacute;zkes</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_ja;&c_zh;&c_jo;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_ja;&c_zh;&c_je;&c_l;&c_o;&c_v;&c_je;&c_s;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="pez.super0i">
  <kap>super<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Pli <tld/>i, pli gravi, venki:
      <ekz>
        la kompatinda virino pensis <klr>[...]</klr>, ke la eta nuna
        momento iros sur la pesilon kontra&ubreve; la tuta pasinteco kaj
        super<tld/>os &gcirc;in
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        &ccirc;u la bona&jcirc;oj, kiujn la&ubreve;pretende, la religio donis al la homaro, 
        super<tld/>as la premon kaj la kruela&jcirc;ojn <klr>[&#x2026;]</klr> nome de Dio
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006847.html">Opinio:
           Sobraj pensoj de sobrulo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">beza&ntilde; <ind>trec'h</ind> war</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;eva&zcaron;ovat</trd>,
    <trd>p&rcaron;ev&aacute;&zcaron;it</trd>
  </trdgrp>
  <trd lng="de">&uuml;berwiegen</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;passer<klr tip="amb"> (l'emporter sur)</klr></trd>,
    <trd>l'<ind>emporter</ind> sur</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>t&ouml;bbet nyom</trd>,
    <trd>fel&uuml;lm&uacute;l</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_je;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_je;&c_s;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">prev&aacute;&zcaron;i&tcaron;</trd>
</drv>

<drv mrk="pez.super0o">
  <kap>super<tld/>o,
  <var><kap>pli<tld/>o</kap></var></kap>
  <snc mrk="pez.super0o.kroma">
    <dif>
      Tio, kio superpasas la norman, normalan, komparatan <tld/>on:
      <ekz>
        en Gotenburgo la Universala Kongreso prezentos denove la kutiman 
        bildon de super<tld/>o de l' generacio de grizuloj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2003/008117.html">Grizi&gcirc;anta mondo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi konsideras ege &gcirc;ojiga kaj siatempe e&ccirc; sava la pli<tld/>on,
        inter la fruaj praktikantoj de Esperanto, de &gcirc;ermanlingvanoj kaj
        slavlingvanoj, kiuj estis nature la plej kapablaj ekspluatantoj de la aglutina potencego
        <fnt>
          <bib>Monato</bib>
          <aut>Wim Jansen</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006827.html">Reago
             De &Gcirc;ermano: La germana kaj Esperanto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="balast.0o">balasto</ref>
    </refgrp>
  </snc>
  <trd lng="br">soulgarg</trd>
  <trd lng="ca">sobrep&egrave;s</trd>
  <trd lng="cs">p&rcaron;evaha</trd>
  <trd lng="de">&Uuml;bergewicht</trd>
  <trdgrp lng="en">
    <trd>overload</trd>,
    <trd>excess <ind>weight</ind></trd>
  </trdgrp>
  <trd lng="es">sobrepeso</trd>
  <trdgrp lng="fr">
    <trd>surcharge</trd>,
    <trd>surpoids</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>t&ouml;bblets&uacute;ly</trd>,
    <trd>t&uacute;ls&uacute;ly</trd>,
    <trd>s&uacute;lyt&ouml;bblet</trd>
  </trdgrp>
  <trd lng="nl">overgewicht</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_v;&c_je;&c_s;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_je;&c_s;</trd>
  </trdgrp>
  <trd lng="sk">prevaha</trd>
</drv>
