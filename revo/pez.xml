<drv mrk="pez.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Igi ion <tld/>a <sncref ref="pez.0a.multe0a"/>:
      <ekz>
        la pluvo <tld/>igis la velojn;
      </ekz>
      <ekz>
        via patro tro <tld/>igis nian jugon  
        <fnt>
          <bib>MT</bib>
          <lok>&Re1; 12:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiuj adjektivoj <tld/>igas la frazon;
      </ekz>
      <ekz>
        barokaj, frazkonstruoj &ndash; piprumitaj per fajne &ccirc;izitaj observa&jcirc;oj
        kaj multaj flankokomentoj por <tld/>igi la sintaksomalnodan taskon de leganto
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan MacGill</aut>
          <vrk><url ref="https://www.monato.be/2004/008216.html">Konciza 
          hororo el la dek-na&ubreve;a jarcento</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[tiu &ccirc;i sciigo]</klr> <tld/>igos la korojn
        <fnt>
          <bib>BdV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_c;&c_ja;&c_zh;&c_a;&c_r;&c_y;&c_c;&c_mol;</trd>
  <trd lng="br">pounneraat<klr tip="ind"> (v.k.)</klr></trd>
  <trdgrp lng="cs">
    <trd>zat&iacute;&zcaron;it</trd>,
    <trd>zat&ecaron;&zcaron;kat</trd>,
    <trd>zt&ecaron;&zcaron;ovat <klr>(u&ccaron;init t&ecaron;&zcaron;k&yacute;m hmotn&ecaron;)</klr></trd>
  </trdgrp>
  <trd lng="en">weigh down</trd>
  <trdgrp lng="fr">
    <trd>alourdir</trd>,
    <trd>appesantir<klr tip="ind"> (alourdir)</klr></trd>,
    <trd>rendre <ind>pesant</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>elnehez&iacute;t</trd>,
    <trd>s&uacute;lyoss&aacute; tesz</trd>,
    <trd>nehez&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_t;&c_ja;&c_zh;&c_je;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_t;&c_ja;&c_zh;&c_je;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_ja;&c_zh;&c_je;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_ja;&c_g;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>s&tcaron;a&zcaron;ova&tcaron;</trd>,
    <trd>za&tcaron;a&zcaron;i&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="pez.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      I&gcirc;i peza, malfacila:
      <ekz>
        ekde pluraj jaroj ade mi pli<tld/>i&gcirc;is kaj nun havas 10 kilogramojn pli
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2007/009234.php">Dikeco</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en la lasta vintro la kutima laboro multoble <tld/>i&gcirc;is pro ne&gcirc;ego
        <fnt>
          <bib>Monato</bib>
          <aut>Kawai Yuka</aut>
          <vrk><url ref="&Monato;/2012/010688.php">Vintra
          varmo homa</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li trovis ilin dormantaj, &ccirc;ar iliaj okuloj <tld/>i&gcirc;is
        <fnt><bib>NT</bib>, <lok>&Mat; 26:43</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">pounneraat<klr tip="ind"> (v.g.)</klr></trd>
  <trdgrp lng="fr">
    <trd>s'<ind>alourdir</ind></trd>,
    <trd>s'<ind>appesantir</ind><klr tip="ind">
    (s'alourdir)</klr></trd>,
    <trd>devenir <ind>pesant</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>elnehez&uuml;l</trd>,
    <trd>s&uacute;lyoss&aacute; v&aacute;lik</trd>,
    <trd>megnehez&uuml;l</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_ja;&c_zh;&c_je;&c_l;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_t;&c_ja;&c_zh;&c_je;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="pez.0ilo">
  <kap><ofc>*</ofc><tld/>ilo</kap>
  <snc mrk="pez.0ilo.MEZ">
    <dif>
      Maso ordinare metala servanta kiel <tld/>unuo:
      <ekz>
        <ref cel="pes.0ilo">pesilo</ref> &gcirc;usta, <tld/>iloj &gcirc;ustaj, efo &gcirc;usta, kaj hino &gcirc;usta estu
        <fnt>
          <bib>MT</bib>
          <lok>&Lev; 19:36</lok>
        </fnt>;
      </ekz>
      <ekz>
        aprobi falsan pesilon kaj trompajn <tld/>ilojn
        <fnt>
          <bib>MT</bib>
          <lok>&Mih; 6:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        &Scirc;varcvalda horlo&gcirc;o kun plumbaj <tld/>iloj faradis tik-tak kaj &ccirc;iam iris tro frue
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova strata lanterno</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>il&scirc;tonoj
        <fnt><bib>MT</bib>, <lok>&Sen; 16:11</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Gewicht <klr>(Waage)</klr></trd>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_r;&c_a;</trd>
  <trd lng="br">pouez<klr tip="ind"> (tamm metal)</klr></trd>
  <trd lng="cs">z&aacute;va&zcaron;&iacute;</trd>
  <trd lng="en">weight</trd>
  <trd lng="es">peso <klr tip="ind">(balanza)</klr></trd>
  <trdgrp lng="fr">
    <trd>masse marqu&eacute;e</trd>,
    <trd>poids<klr tip="ind"> (masse marqu&eacute;e)</klr></trd>
  </trdgrp>
  <trd lng="hu">(m&eacute;r&odblac;)<ind>s&uacute;ly</ind></trd>
  <trd lng="nl">gewicht<klr tip="ind"> (om te wegen)</klr></trd>
  <trd lng="ru">&c_g;&c_i;&c_r;&c_ja;</trd>
  <trd lng="sk">z&aacute;va&zcaron;ie</trd>
</drv>

<drv mrk="pez.egal0o">
  <kap>egal<tld/>o</kap>
  <snc>
    <dif>
      Stabileco de io, kiun egalaj kontra&ubreve;aj <tld/>oj tiras al diversaj
      flankoj:
      <ekz>
        sub la influo de diversaj a&ubreve;ditaj opinioj, ili perdis
        la egal<tld/>on
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        iom post iom <nom>Ramzes</nom> retrovis la egal<tld/>on
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 3a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="ekvili.0o">ekvilibro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_w;&c_n;&c_a;&c_v;&c_a;&c_g;&c_a;</trd>
  <trd lng="br">kempouez<klr tip="ind"> (ak.)</klr></trd>
  <trd lng="ca">equilibri</trd>
  <trd lng="cs">rovnov&aacute;ha</trd>
  <trd lng="de">Gleichgewicht</trd>
  <trd lng="es">equilibrio</trd>
  <trdgrp lng="fr">
    <trd>&eacute;quilibre</trd>,
    <trd>aplomb<klr tip="ind"> (&eacute;quilibre)</klr></trd>,
    <trd>balance<klr tip="ind"> (&eacute;quilibre)</klr></trd>
  </trdgrp>
  <trd lng="hu">egyens&uacute;ly</trd>
  <trd lng="nl">evenwicht</trd>
