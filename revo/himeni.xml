<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: himeni.xml,v 1.16 2023/10/16 05:55:37 revo Exp $">
<kap>
  <rad>himeni</rad>/o
</kap>
<drv mrk="himeni.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="membra.0o">Membrano</ref>
      kovranta la receptaklon de la <ref tip="malprt" cel="fung.0o.fruktokorpo">fungo</ref>
      kaj kiu portas la sporojn:
      <ekz>
        la kantarelacoj havas starantan, ofte funelforman fruktkorpon <klr>[kiu]</klr>
        estas mola &gcirc;is iom ledra, la <tld/>o estas glata &gcirc;is falda
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=509738">Kantarelacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_ib;&c_m;&c_e;&c_n;&c_ib;&c_j;</trd>,
    <trd>&c_g;&c_ib;&c_m;&c_e;&c_n;&c_ib;&c_ja;&c_l;&c_mol;&c_n;&c_y; &c_s;&c_l;&c_o;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bl&aacute;na houby</trd>,
    <trd>hymenium</trd>,
    <trd>rou&scaron;ko houby</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hymenium</trd>,
    <trd>Sporenlager</trd>
  </trdgrp>
  <trd lng="fr">hym&eacute;nium</trd>
  <trdgrp lng="hu">
    <trd>term&odblac;r&eacute;teg<klr tip="ind"> (gomba)</klr></trd>,
    <trd>hymenium</trd>
  </trdgrp>
  <trd lng="ja">&#x5B50;&#x5B9F;&#x5C64; <pr>&#12375;&#12376;&#12388;&#12381;&#12358;</pr></trd>
  <trd lng="nl">hymenium</trd>
  <trdgrp lng="pl">
    <trd>hymenium</trd>, <trd>ob&lstroke;ocznia</trd>
  </trdgrp>
  <trd lng="pt">him&ecirc;nio</trd>
  <trdgrp lng="sk"> himenio
    <trd>blana huby</trd>,
    <trd>r&uacute;&scaron;ko huby</trd>
  </trdgrp>
  <trd lng="uk">&c_g;&c_ib;&#x43C;&c_je;&#x43D;&c_ib;&c_j;</trd>
</drv>
</art>
