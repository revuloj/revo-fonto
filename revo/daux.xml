<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>da&ubreve;</rad>/o
</kap>
<drv mrk="daux.0o">
  <kap><tld/>o</kap>
  <snc mrk="daux.0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
     En <tld/>ismo fundamenta principo de la naturaj ordo kaj funkciado:
      <ekz>
        la kredantoj de la poste formi&gcirc;inta <tld lit="D"/>-religio
        <fnt><bib>Lusin</bib>, <lok>p. 448a</lok></fnt>
      </ekz>
    </dif>
    <ref tip="sin" cel="ta.0o">tao</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Dao</trd>,
    <trd>Tao</trd>
  </trdgrp>
  <trd lng="es">tao</trd>
  <trd lng="fa">&a_t;&a_A;&a_y_hamza;&a_w;</trd>
  <trdgrp lng="fr">
    <trd>dao<klr> (tao)</klr></trd>,
    <trd>tao</trd>
  </trdgrp>
  <trd lng="hu">tao</trd>
  <trd lng="nl">tao</trd>
  <trd lng="pl">tao</trd>
  <trdgrp lng="pt">
    <trd>tau</trd>,
    <trd>tao</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_a;&c_o;</trd>
  <trd lng="zh">&#36947;<klr> [d&agrave;o]</klr></trd>
</drv>

<drv mrk="daux.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="daux.0ismo.REL">
    <uzo tip="fak">FIL</uzo>
    <uzo tip="fak">REL</uzo>
    <dif>
      &Ccirc;indevena <ref tip="super" cel="religi.0o.REL">religio</ref>
      kuniganta ekde la 2a jarcento popolajn kultojn kaj filozofiajn
      instruojn pri ne-agado, konformi&gcirc;o al naturo kaj al <tld/>o.
    </dif>
    <ref tip="sin" cel="taoism.0o">taoismo</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Daoismus</trd>,
    <trd>Taoismus</trd>
  </trdgrp>
  <trd lng="fa">&a_t;&a_A;&a_y_hamza;&a_w;&a_y_hamza;&f_y;&a_s;&a_m;</trd>
  <trdgrp lng="fr">
    <trd>tao&iuml;sme</trd>
  </trdgrp>
  <trd lng="hu">taoizmus</trd>
  <trd lng="nl">taoisme</trd>
  <trd lng="pl">taoizm</trd>
  <trd lng="pt">tao&iacute;smo</trd>
  <trd lng="ru">&c_d;&c_a;&c_o;&c_s;&c_i;&c_z;&c_m;</trd>
  <trd lng="zh">&#36947;&#25945;<klr> [d&agrave;oji&#257;o]</klr></trd>
</drv>

<drv mrk="daux.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="daux.0isto.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Pastro, mona&hcirc;o, ermito a&ubreve; alia sindedi&ccirc;a
      praktikanto de <tld/>ismo.
    </dif>
    <ref tip="sin" cel="taoist.0o">taoisto</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Daoist</trd>,
    <trd>Taoist</trd>
  </trdgrp>
  <trd lng="fr">tao&iuml;ste</trd>
  <trd lng="hu">taoista</trd>
  <trd lng="nl">taoist</trd>
  <trd lng="pl">taoista</trd>
  <trd lng="ru">&c_d;&c_a;&c_o;&c_s;</trd>
</drv>
</art>
