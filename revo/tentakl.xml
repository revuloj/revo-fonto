<art mrk="$Id: tentakl.xml,v 1.7 2024/05/10 08:31:30 revo Exp $">
<kap>
  <rad>tentakl</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="tentakl.0o">
  <kap><tld/>o</kap>
  <snc mrk="tentakl.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Longa, elasta palpilo de iuj bestoj:
      <ekz>
        maresta&jcirc;oj <ctl>Noctiluca</ctl> <klr>...</klr>
        posedas specialan <tld/>on, kiu sekrecias longajn transversajn
        striojn de glua&jcirc;o
        <fnt>
          <aut>R. A. Lewin</aut>,
          <vrk><url ref="https://scienca-revuo.info/article/download/1109/1088">
          La graveco de glueco</url></vrk>,
          <lok>Scienca Revuo, 2006:1, p. 43a-46a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_ch;&c_u;&c_p;&c_a;&c_l;&c_mol;&c_c;&c_a;</trd>
  <trd lng="fr">tentacule</trd>
  <trdgrp lng="ja">
    <trd>&#x89E6;&#x624B;  <pr>&#x3057;&#x3087;&#x304F;&#x3057;&#x3085;</pr></trd>,
    <trd>&#x89E6;&#x8155;  <pr>&#x3057;&#x3087;&#x304F;&#x308F;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>czu&lstroke;ek</trd>,
    <trd>macka</trd>
  </trdgrp>
  <trd lng="uk">&c_shch;&c_u;&#x43F;&c_a;&#x43B;&#x44C;&c_c;&c_je;</trd>
  <trdgrp lng="zh">
    <trd>&#x89E6;&#x624B; <pr>ch&#xF9;sh&#x1D2;u</pr></trd>,
    <trd>&#x89F8;&#x624B; <pr>ch&#xF9;sh&#x1D2;u</pr></trd>,
    <trd>&#x89E6;&#x8155; <pr>ch&#xF9;w&#xE0;n</pr></trd>,
    <trd>&#x89F8;&#x8155; <pr>ch&#xF9;w&#xE0;n</pr></trd>
  </trdgrp>
</drv>
</art>

