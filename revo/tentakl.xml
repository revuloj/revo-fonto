<vortaro>
<art mrk="$Id: tentakl.xml,v 1.8 2025/03/30 20:27:55 revo Exp $">
<kap>
  <rad>tentakl</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="tentakl.0o">
  <kap><tld/>o</kap>
  <snc mrk="tentakl.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Longa, elasta palpilo de iuj bestoj:
      <ekz>
        maresta&jcirc;oj <ctl>Noctiluca</ctl> <klr>...</klr>
        posedas specialan <tld/>on, kiu sekrecias longajn transversajn
        striojn de glua&jcirc;o
        <fnt>
          <aut>R. A. Lewin</aut>,
          <vrk><url ref="https://scienca-revuo.info/article/download/1109/1088">
          La graveco de glueco</url></vrk>,
          <lok>Scienca Revuo, 2006:1, p. 43a-46a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_ch;&c_u;&c_p;&c_a;&c_l;&c_mol;&c_c;&c_a;</trd>
  <trd lng="en">tentacle</trd>
  <trd lng="fr">tentacule</trd>
  <trdgrp lng="ja">
    <trd>&#35302;&#25163;  <pr>&#12375;&#12423;&#12367;&#12375;&#12421;</pr></trd>,
    <trd>&#35302;&#33109;  <pr>&#12375;&#12423;&#12367;&#12431;&#12435;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>czu&lstroke;ek</trd>,
    <trd>macka</trd>
  </trdgrp>
  <trd lng="uk">&c_shch;&c_u;&c_p;&c_a;&c_l;&c_mol;&c_c;&c_je;</trd>
  <trdgrp lng="zh">
    <trd>&#35302;&#25163; <pr>ch&ugrave;sh&#466;u</pr></trd>,
    <trd>&#35320;&#25163; <pr>ch&ugrave;sh&#466;u</pr></trd>,
    <trd>&#35302;&#33109; <pr>ch&ugrave;w&agrave;n</pr></trd>,
    <trd>&#35320;&#33109; <pr>ch&ugrave;w&agrave;n</pr></trd>
  </trdgrp>
</drv>
</art>

