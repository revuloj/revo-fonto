    <trd>&c_a;&c_r;&c_h;&c_a;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_u;&c_s;&c_t;&c_a;&c_r;&c_je;&c_v;&c_sh;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>zastaran&yacute;</trd>,
    <trd>archaick&yacute;</trd>
