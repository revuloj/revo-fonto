<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>salon</rad>/o
</kap>
<drv mrk="salon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Eleganta &ccirc;ambro, ordinare destinita por ricevi vizitantojn.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_l;&c_jo;&c_n;</trd>,
    <trd>&c_g;&c_a;&c_s;&c_mol;&c_c;&c_jo;&c_w;&c_n;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Salon</trd>,
    <trd>Empfangszimmer</trd>
  </trdgrp>
  <trd lng="en">sitting room</trd>
  <trdgrp lng="hu">
    <trd>szalon</trd>,
    <trd>fogad&oacute;szoba</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>salon</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_s;&c_t;&c_i;&c_n;&c_a;&c_ja;</trd>,
    <trd>&c_s;&c_a;&c_l;&c_o;&c_n;</trd>
  </trdgrp>
</drv>

<drv mrk="salon.antaux0o">
  <kap>anta&ubreve;<tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;ambro, anta&ubreve;anta <tld/>on:
      <ekz>
        la unua &ccirc;ambro estis preska&ubreve; senmebla anta&ubreve;<tld/>o
        <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 5</lok></fnt>.
      </ekz>
    </dif>
  </snc>
</drv>
</art>
