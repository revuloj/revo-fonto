<art mrk="$Id: salon.xml,v 1.36 2024/03/24 19:59:59 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>salon</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="salon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="salon.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Eleganta <ref tip="super" cel="cxambr.0o.doma">&ccirc;ambro</ref>,
      ordinare destinita por akcepti vizitantojn:
      <ekz>
	en la <tld/>o estis neniu krom li kaj lia fian&ccirc;ino
	<fnt><bib>F</bib> <lok>&FE; 26</lok></fnt>;
      </ekz>
      <ekz>
         ... kaj subite, kvaza&ubreve; el&ccirc;eni&gcirc;inte, ekdancis amba&ubreve; <tld/>oj kaj
         post ili ekdancis la verando
         <fnt>
         <aut>M. A. Bulgakov, trad. S. Pokrovskij</aut>
         <vrk>La majstro kaj Margarita</vrk>
         <lok>1991</lok>
         </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_l;&c_jo;&c_n;</trd>,
    <trd>&c_g;&c_a;&c_s;&c_mol;&c_c;&c_jo;&c_w;&c_n;&c_ja;</trd>
  </trdgrp>
  <trd lng="bo">&#xF51;&#xF74;&#xF0B;&#xF41;&#xF44;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>salon</trd>,
    <trd>s&aacute;l</trd>,
    <trd>v&yacute;stavn&iacute; m&iacute;stnost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Salon</trd>,
    <trd>Empfangszimmer</trd>
  </trdgrp>
  <trd lng="en">sitting room</trd>
  <trd lng="es">sal&oacute;n</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_A;&a_l;&a_n;</trd>,
    <trd>&a_t;&a_A;&a_l;&a_A;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">salon</trd>
  <trdgrp lng="hu">
    <trd>szalon</trd>,
    <trd>fogad&oacute;szoba</trd>
  </trdgrp>
  <trd lng="it">salone</trd>
  <trdgrp lng="ja">
    <trd>&#x30B5;&#x30ED;&#x30F3;</trd>,
    <trd>&#x5E83;&#x9593;  <pr>&#x3072;&#x308D;&#x307E;</pr></trd>,
    <trd>&#x5BA2;&#x9593;  <pr>&#x304D;&#x3083;&#x304F;&#x307E;</pr></trd>,
    <trd>&#x540D;&#x58EB;&#x306E;&#x96C6;&#x3044;  <pr>&#x3081;&#x3044;&#x3057;&#x306E;&#x3064;&#x3069;&#x3044;</pr></trd>,
    <trd>&#x793E;&#x4EA4;&#x754C;  <pr>&#x3057;&#x3083;&#x3053;&#x3046;&#x304B;&#x3044;</pr></trd>,
    <trd>&#x7F8E;&#x8853;&#x5C55;  <pr>&#x3073;&#x3058;&#x3085;&#x3064;&#x3066;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">salon</trd>
  <trdgrp lng="pl">
    <trd>komnata</trd>,
    <trd>bawialnia</trd>,
    <trd>pok&oacute;j go&sacute;cinny</trd>,
    <trd>salon</trd>
  </trdgrp>
  <trd lng="pt">sal&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_s;&c_t;&c_i;&c_n;&c_a;&c_ja;</trd>,
    <trd>&c_s;&c_a;&c_l;&c_o;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>sal&oacute;n</trd>,
    <trd>s&aacute;la</trd>,
    <trd>v&yacute;stavn&aacute; miestnos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_t;&c_a;&#x43B;&#x44C;&#x43D;&#x44F;</trd>,
    <trd>&c_s;&c_a;&#x43B;&#x43E;&#x43D;</trd>,
    <trd>&c_z;&c_a;&#x43B;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8D77;&#x5C45;&#x5BA4; <pr>q&#x1D0;j&#x16B;sh&#xEC;</pr></trd>,
    <trd>&#x8D77;&#x5C45;&#x9593; <pr>q&#x1D0;j&#x16B;ji&#x101;n</pr></trd>,
    <trd>&#x8D77;&#x5C45;&#x95F4; <pr>q&#x1D0;j&#x16B;ji&#x101;n</pr></trd>,
    <trd>&#x7F8E;&#x672F;&#x5C55;&#x89C8;&#x4F1A;&#x573A; <pr>m&#x11B;ish&#xF9;zh&#x1CE;nl&#x1CE;nhu&#xEC;ch&#x1CE;ng</pr></trd>,
    <trd>&#x7F8E;&#x8853;&#x5C55;&#x89BD;&#x6703;&#x5834; <pr>m&#x11B;ish&#xF9;zh&#x1CE;nl&#x1CE;nhu&#xEC;ch&#x1CE;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="salon.0ego">
  <kap>
    <tld/>ego
  </kap>
  <snc mrk="salon.0ego.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Vasta <tld/>o por akcepto de multaj homoj,
      <ref tip="vid" cel="hal.0o.cxambrego">halo<sncref/></ref>:
            <ekz>
        la preparoj sur la estrado nur komenci&gcirc;is, kiam la samideanoj jam estis
        enirintaj la <tld/>egon
        <fnt>
          <aut>W. Knopf</aut>
          <vrk><url ref="https://eo.wikisource.org/wiki/Universala_Kongreso_1924_GE">
          Raporto pri la XVIa Universala Kongreso de Esperanto en Wien</url></vrk>
          <lok>1924</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_l;&c_ja;</trd>
  <trd lng="fr">halle</trd>
  <trdgrp lng="ja">
    <trd>&#x5927;&#x30DB;&#x30FC;&#x30EB;  <pr>&#x3060;&#x3044;&#x30DB;&#x30FC;&#x30EB;</pr></trd>,
    <trd>&#x5927;&#x96C6;&#x4F1A;&#x5BA4;  <pr>&#x3060;&#x3044;&#x3057;&#x3085;&#x3046;&#x304B;&#x3044;&#x3057;&#x3064;</pr></trd>
  </trdgrp>
  <trd lng="pl">sala</trd>
  <trdgrp lng="zh">
    <trd>&#x5802; <pr>t&#xE1;ng</pr></trd>,
    <trd>&#x5927;&#x5385; <pr>d&#xE0;t&#x12B;ng</pr></trd>,
    <trd>&#x5927;&#x5EF3; <pr>d&#xE0;t&#x12B;ng</pr></trd>,
    <trd>&#x6BBF;&#x5802; <pr>di&#xE0;nt&#xE1;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="salon.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="salon.0eto.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Luksa <ref tip="super" cel="cxambr.0o.doma">&ccirc;ambreto</ref>
      privata por akcepto a&ubreve; ripozo:
      <ekz>
	tiu <tld/>eto, en kiu &scirc;i akceptadis la personojn, kiuj bezonis
	&scirc;ian konsilon kaj helpon
	<fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="buduar.0o">buduaro</ref>
  </snc>
<trd lng="be">&c_b;&c_u;&c_d;&c_u;&c_a;&c_r;</trd>
  <trd lng="fr">boudoir<klr tip="amb"> (pi&egrave;ce)</klr></trd>
  <trd lng="it">salottino</trd>
  <trdgrp lng="pl">
    <trd>salonik</trd>,
    <trd>buduar</trd>
  </trdgrp>
  <trd lng="pt">budoar</trd>
  <trd lng="ru">&c_b;&c_u;&c_d;&c_u;&c_a;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#x95A8; <pr>gu&#x12B;</pr></trd>,
    <trd>&#x95FA; <pr>gu&#x12B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="salon.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="salon.0ulo.MOR">
    <dif>
      <ref tip="vid" cel="mond.0umo">Mondum</ref>ano,
      homo kiun oni ofte vidas en presti&gcirc;aj <tld/>oj kaj
      lokoj:
      <ekz>
	viaj konatoj estos homoj kun la plej aristokrata konduto: grafoj
	kaj &ccirc;iuj <tld/>uloj
	<fnt><bib>Revizoro</bib> <lok>akto 5a, sceno 1a</lok></fnt>...
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>mondain <klr>(subst.)</klr></trd>,
    <trd>salonard <klr>(subst.)</klr></trd>
  </trdgrp>
  <trd lng="it">uomo di <ind>mondo</ind></trd>
  <trdgrp lng="pl">
    <trd>salonowiec</trd>,
    <trd>bywalec</trd>
  </trdgrp>
  <trd lng="pt">homem de <ind>sociedade</ind></trd>
  <trd lng="ru">&c_s;&c_v;&c_je;&c_t;&c_s;&c_k;&c_i;&c_j; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>
</drv>

<drv mrk="salon.antaux0o">
  <kap>anta&ubreve;<tld/>o</kap>
  <snc mrk="salon.antaux0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="super" cel="cxambr.0o.doma">&Ccirc;ambro</ref>
      kun funkcio de <ref tip="vid" cel="vestib.0o.ARKI">vestiblo<sncref/></ref>
      kaj atendejo, en kiu vizitantoj estas akceptataj:
      <ekz>
	la unua &ccirc;ambro estis preska&ubreve; senmebla anta&ubreve;<tld/>o
	<fnt><bib>I&scirc;tar</bib> <lok>&ccirc;apitro 5a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_d;&c_p;&c_a;&c_k;&c_o;&c_j;</trd>
  <trd lng="fr">antichambre</trd>
  <trd lng="it">anticamera</trd>
  <trd lng="pl">przedpok&oacute;j</trd>
  <trdgrp lng="pt">
    <trd>antec&acirc;mara</trd>,
    <trd>ante-sala</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_d;&c_n;&c_ja;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_h;&c_o;&c_zh;&c_a;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="salon.festo0o">
  <kap>festo<tld/>o</kap>
  <snc mrk="salon.festo0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Vasta <ref tip="super" cel="cxambr.0o.doma">&ccirc;ambro</ref> por
      solena&jcirc;oj, baloj kaj similaj,
      <ref tip="vid" cel="hal.fest0o.ARKI">festhalo</ref>:
      <ekz>
	la granda festo<tld/>o estis brilante ornamita
	<fnt><bib>Fab2</bib>, <lok>monteto de elfoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_l;&c_ja; &c_w;&c_r;&c_a;&c_ch;&c_y;&c_s;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_w;</trd>
  <trdgrp lng="cs">
    <trd>hodovn&iacute; m&iacute;stnost</trd>,
    <trd>slavnostn&iacute; s&aacute;l</trd>
  </trdgrp>
  <trd lng="fa">&a_s;&a_A;&a_l;&a_n; &a_j;&a_sh;&a_n;</trd>
  <trd lng="fr">salle des f&ecirc;tes</trd>
  <trd lng="it"><ind>salone</ind> delle feste</trd>
  <trd lng="pl">aula</trd>
  <trd lng="pt">sal&atilde;o de festas</trd>
  <trd lng="ru">&c_p;&c_r;&c_a;&c_z;&c_d;&c_n;&c_i;&c_ch;&c_n;&c_y;&c_j; &c_z;&c_a;&c_l;</trd>
  <trd lng="sk">sl&aacute;vnostn&aacute; s&aacute;la</trd>
</drv>

<drv mrk="salon.lego0o">
  <kap>lego<tld/>o</kap>
  <snc mrk="salon.lego0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      &Ccirc;ambro por legado, ekzemple en <ref tip="malprt" cel="biblio1.0o.ejo">biblioteko</ref>:
      <ekz>
	sur la galerio de la &ccirc;efa Lego<tld/>o estas videbla la
	<nom>Fajszi</nom>-Kolekta&jcirc;o
	<fnt>
	  <aut>red. E. Dudich</aut>,
	  <vrk><url ref="http://www.oik.hu/eszperanto/gyujtemeny.htm">
	  Kelkaj informoj pri la Landa Fremdlingva Biblioteko</url></vrk>,
	  <lok>2008-01</lok>
	</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kabine.0o.pensejo">kabineto<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_ch;&c_y;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; <ind>&c_z;&c_a;&c_l;&c_ja;</ind></trd>
  <trd lng="cs">&ccaron;&iacute;t&aacute;rna</trd>
  <trd lng="de">Lesesaal</trd>
  <trd lng="fa">&a_s;&a_A;&a_l;&a_n; &a_m;&a_T;&a_A;&a_l;&a_ayn;&a_h;</trd>
  <trd lng="fr">salle de lecture</trd>
  <trd lng="it"><ind>sala</ind> di lettura</trd>
  <trd lng="pl">czytelnia</trd>
  <trd lng="pt">sala de leitura</trd>
  <trd lng="sk">&ccaron;it&aacute;re&ncaron;</trd>
  <trdgrp lng="zh">
    <trd>&#x95B1;&#x89BD;&#x5BA4; <pr>yu&#xE8;l&#x1CE;nsh&#xEC;</pr></trd>,
    <trd>&#x9605;&#x89C8;&#x5BA4; <pr>yu&#xE8;l&#x1CE;nsh&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="salon.mangxo0o">
  <kap>man&gcirc;o<tld/>o</kap>
  <snc mrk="salon.mangxo0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">KUI</uzo>
    <dif>
      Granda a&ubreve; luksa man&gcirc;o<ref tip="super" cel="cxambr.0o.doma">&ccirc;ambro</ref>:
      <ekz>
	mi eniris en la man&gcirc;o<tld/>on kaj matenman&gcirc;is kaj poste promenadis
	sur la perono
	<fnt>
        <aut>trad. B. G. Jonson</aut>
        <vrk>La nigra virino</vrk>
        <lok>en. Fundamenta Krestomatio, 1904</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_n;&c_k;&c_je;&c_t;&c_n;&c_a;&c_ja; <ind>&c_z;&c_a;&c_l;&c_ja;</ind></trd>
  <trd lng="de">Speisesaal</trd>
  <trd lng="fa">&a_s;&a_A;&a_l;&a_n; &a_gh;&a_dh;&a_A;&a_kh;&a_w;&a_r;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>r&eacute;fectoir</trd>,
    <trd>salle &agrave; manger</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd><ind>salone</ind> dei ricevimenti</trd>,
    <trd>refettorio</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>salon jadalny</trd>,
    <trd>jadalnia</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>sal&atilde;o de banquete</trd>,
    <trd>sala de jantar</trd>,
    <trd>refeit&oacute;rio</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_a;&c_n;&c_k;&c_je;&c_t;&c_n;&c_y;&c_j; &c_z;&c_a;&c_l;</trd>
  <trdgrp lng="zh">
    <trd>&#x9910;&#x5385; <pr>c&#x101;nt&#x12B;ng</pr></trd>,
    <trd>&#x9910;&#x5EF3; <pr>c&#x101;nt&#x12B;ng</pr></trd>
  </trdgrp>
</drv>
</art>
