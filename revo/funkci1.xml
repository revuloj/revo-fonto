<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: funkci1.xml,v 1.8 2015/07/04 08:37:34 revo Exp $">
  <kap>
    <rad>funkcional</rad>/o
  </kap>
<drv mrk="funkci1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="bild.0igo.MAT">Bildigo</ref>, kies fonto-aro
      konsistas el <ref cel="funkci.0o.MAT">funkcioj</ref> kaj kies
      celo-aro konsistas el nombroj:
      <ekz>
        la bildigo, kiu &jcirc;etas funkcion al &gcirc;ia 
        <ref cel="integr2.0o.sumo">integralo</ref> estas <tld/>o;        
      </ekz>
      <ekz>
        <ref cel="distri2.0o">distribucioj</ref> estas <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_u;&c_n;&c_k;&c_c;&c_y;&c_ja;&c_n;&c_a;&c_l;</trd>
  <trd lng="cs">funkcionál</trd>
  <trd lng="de">Funktional</trd>
  <trd lng="en">functional</trd>
  <trd lng="fr">fonctionnelle<klr tip="ind"> (math.)</klr></trd>
  <trd lng="nl">functionaal</trd>
  <trd lng="pl">funkcjona&lstroke;</trd>
  <trdgrp lng="pt">
    <trd>fun&#231;&#227;o de fun&#231;&#227;o</trd>,
    <trd>funcional <klr tip="amb"> (subst., mat.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_u;&c_n;&c_k;&c_c;&c_i;&c_o;&c_n;&c_a;&c_l;</trd>
  <trd lng="sk">funkcionál</trd>
</drv>
</art>

