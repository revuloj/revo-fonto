<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>agaragar</rad>/o
</kap>
<drv mrk="agarag.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Substanco simila al <ref cel="gelate.0o">gelateno</ref>,
      ekstraktata 
      el kelkaj specoj de hindaj algoj, kaj uzata por apreturi &scirc;tofojn 
      a&ubreve; por prepari laksigilojn.
    </dif>
    <ref tip="sin" cel="geloz.0o">gelozo.</ref>
  </snc>
  <trd lng="be">&c_a;&c_g;&c_a;&c_r;-&c_a;&c_g;&c_a;&c_r;</trd>
  <trd lng="bg">&c_a;&c_g;&c_a;&c_r;-&c_a;&c_g;&c_a;&c_r;</trd>
  <trd lng="de">Agar-Agar</trd>
  <trdgrp lng="en">
    <trd>agar</trd>,
    <trd>agar-agar</trd>
  </trdgrp>
  <trd lng="fr">agar-agar</trd>
  <trdgrp lng="it">
    <trd>agar</trd>,
    <trd>agar-agar</trd>
  </trdgrp>
  <trd lng="nl">agar-agar</trd>
  <trd lng="pt">&aacute;gar-&aacute;gar</trd>
  <trd lng="ru">&c_a;&c_g;&c_a;&c_r;-&c_a;&c_g;&c_a;&c_r;</trd>
  <trd lng="sk">agar</trd>
</drv>
</art>
