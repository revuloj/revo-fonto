<vortaro>
<art mrk="$Id: ergogr.xml,v 1.13 2020/07/02 20:32:33 revo Exp $">
<kap>
  <rad>ergograf</rad>/o
</kap>
<drv mrk="ergogr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc mrk="ergogr.0o.aparato">
    <dif>
      <ref tip="super" cel="aparat.0o.TEK">Aparato</ref> por
      mezuri la muskolan <ref cel="labor.0o.FIZ">laboro</ref>n:
      <ekz>
        bicikla <tld/>o estas ekipita per pulsosensilo kaj a&ubreve;tomate reguligas
        &scirc;ar&gcirc;intenson de pedalado
        <fnt>
          <vrk><url ref="https://www.vastalto.com/blog/?p=903"
              >Bicikla ergografo - Akvosfero</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_r;&c_g;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
  <trd lng="cs">ergograf</trd>
  <trd lng="de">Ergograph</trd>
  <trd lng="fr">ergographe</trd>
  <trd lng="hu">ergogr&aacute;f</trd>
  <trd lng="pl">ergograf</trd>
  <trd lng="pt">erg&oacute;grafo</trd>
  <trd lng="ru">&c_e;&c_r;&c_g;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
  <trd lng="sk">ergograf</trd>
</drv>
</art>
