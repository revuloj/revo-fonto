<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>miliard</rad>/o
</kap>
<drv mrk="miliar.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Mil milionoj.
    </dif>
    <refgrp tip="sin">
      <ref cel="bilion.0o">biliono</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_l;&c_mol;&c_ja;&c_r;&c_d;</trd>
  <trd lng="bg">&c_m;&c_i;&c_l;&c_i;&c_a;&c_r;&c_d;</trd>
  <trdgrp lng="ca">
    <trd>miliard</trd>,
    <trd>mil milions</trd>
  </trdgrp>
  <trd lng="de">Milliarde</trd>
  <trd lng="en">billion <klr>(US)</klr></trd>
  <trd lng="hu">milli&aacute;rd</trd>
  <trd lng="sv">miljard</trd>
  <trd lng="nl">miljard</trd>
  <trdgrp lng="pt">
  <trd>bilh&atilde;o <klr>(1.000.000.000)</klr></trd>,
  <trd>bili&atilde;o <klr>(1.000.000.000)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_i;&c_l;&c_l;&c_i;&c_a;&c_r;&c_d;</trd>
  <trd lng="vo">baliad</trd>
</drv>
</art>
