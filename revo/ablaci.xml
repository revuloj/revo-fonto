<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ablaci.xml,v 1.14 2024/01/19 09:24:52 revo Exp $">
<kap>
  <rad>ablaci</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ablaci.0o">
  <kap><tld/>o</kap>
  <snc mrk="ablaci.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Forigo de materialo.
    </dif>
    <subsnc mrk="ablaci.0o.GEOL">
      <uzo tip="fak">GEOL</uzo>
      <dif>
        Surfaca <ref tip="vid" cel="erozi.0o">erozio</ref>:
        <ekz>
          <tld/>o estas masperdo de la gla&ccirc;ero dum la metamorfozproceso
          <fnt>
            <bib>Viki</bib>,
            <vrk><url ref="&Viki;/Gla%C4%89ero#Ablacio">Gla&ccirc;ero</url></vrk>
          </fnt>.
        </ekz>
      </dif>
      <ref tip="vid" cel="abrazi.0o">abrazio</ref>
    <trd lng="de">Abschmelzen</trd>
      <trdgrp lng="zh">
    <trd>&#x6D88;&#34645; <pr>xiao1shi2</pr></trd>,
    <trd>&#x6D88;&#34432; <pr>xiao1shi2</pr></trd>
  </trdgrp>
</subsnc>
    <subsnc mrk="ablaci.0o.MED">
      <uzo tip="fak">MED</uzo>
      <uzo tip="fak">KIR</uzo>
      <dif>
        <ref tip="vid" cel="hxirur.0a.MED">&Hcirc;irurgia</ref> forigo:
        <ekz>
          <tld/>o de tumora histo;
        </ekz>
        <ekz>
          <uzo tip="stl">FIG</uzo>
          sen tia drasta <tld/>o la morto de UEA &scirc;ajnas certa
          <fnt>
            <vrk><url ref="https://www.esperantio.net/index.php?id=3888">Du domoj en komparo:
            nederlanda kaj svisa</url></vrk>
            <lok>HeKo 717 1-A, 25a a&ubreve;g. 2019</lok>
          </fnt>.
        </ekz>
      </dif>
    <trd lng="de">Ver&ouml;dung</trd>
    </subsnc>
    <subsnc mrk="ablaci.0o.AST">
      <uzo tip="fak">AST</uzo>
      <dif>
        <ref tip="vid" cel="erozi.0o">Erozio</ref> a&ubreve; elvapori&gcirc;o
        pro frotado tra la atmosfero a&ubreve; radiado.
      </dif>
    </subsnc>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_l;&c_ja;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_z;&c_m;&c_y;&c_w;</trd>
  </trdgrp>
  <trd lng="ca">ablaci&oacute;</trd>
  <trdgrp lng="cs">
    <trd>ablace</trd>,
    <trd>kon&ccaron;etiny</trd>,
    <trd>odn&ecaron;t&iacute; tk&aacute;n&ecaron;</trd>,
    <trd>org&aacute;nu</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ablation</trd>,
    <trd>Abtrag</trd>
  </trdgrp>
  <trd lng="en">ablation</trd>
  <trd lng="es">ablaci&oacute;n</trd>
  <trd lng="fr">ablation</trd>
  <trd lng="it">ablazione</trd>
  <trd lng="pl">ablacja</trd>
  <trd lng="ro">abla&#539;iune</trd>
  <trdgrp lng="sk">
    <trd>abl&aacute;cia</trd>,
    <trd>od&ncaron;atie tkaniva</trd>,
    <trd>org&aacute;nu</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&c_b;&c_l;&c_ja;&c_c;&c_ib;&c_ja;</trd>,
    <trd>&c_v;&c_i;&c_d;&c_a;&c_l;&c_je;&c_n;&c_n;&c_ja; &c_o;&c_r;&c_g;&c_a;&c_n;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="ablaci.0agxo">
 <kap><tld/>a a&gcirc;o
   <fnt><vrk>Astronomia Terminaro</vrk></fnt>
  </kap>
  <snc mrk="ablaci.0agxo.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Tempoda&ubreve;ro ekde la fandi&gcirc;o de la vitra surfaco de
      aer&scirc;tono pro &gcirc;ia enirado tra la atmosfero.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_r;&c_m;&c_ib;&c_n; <ind>&c_a;&c_b;&c_l;&c_ja;&c_c;&c_y;&c_ib;</ind></trd>
  <trd lng="ca">temps d'ablaci&oacute;</trd>
  <trd lng="en">ablation age</trd>
  <trd lng="it">tempo di ablazione</trd>
  <trd lng="pl">wiek lodowcowy</trd>
</drv>
</art>
