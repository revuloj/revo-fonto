<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ablaci.xml,v 1.11 2022/04/24 08:53:34 revo Exp $">
<kap>
  <rad>ablaci</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ablaci.0o">
  <kap><tld/>o</kap>
  <snc mrk="ablaci.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Surfaca <ref tip="vid" cel="erozi.0o">erozio</ref>.
    </dif>
  </snc>
  <snc mrk="ablaci.0o.glacio">
    <uzo tip="fak">GEOL</uzo>
    <uzo tip="fak">EKOL</uzo>
    <dif>
      Fandi&gcirc;o kaj elvapori&gcirc;o de glaciejo:
      <ekz>
        <tld/>o estas masperdo de la gla&ccirc;ero dum la metamorfozproceso
        <fnt>
          <bib>Viki</bib>,
          <vrk><url ref="&Viki;/Gla%C4%89ero#Ablacio">Gla&ccirc;ero</url></vrk>.
        </fnt>
      </ekz>
    </dif>
    <rim>
     erozio <em>de</em> glaciejo.
    </rim>
  </snc>
  <snc mrk="ablaci.0o.rokoj">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Forporto de rokoj fare de glaciejo.
    </dif>
    <rim>
     erozio <em>fare de</em> glaciejo.
    </rim>
  </snc>
  <snc mrk="ablaci.0o.MED">
    <uzo tip="fak">MED</uzo>
    <uzo tip="fak">KIR</uzo>
    <dif>
      <ref tip="vid" cel="hxirur.0a.MED">&Hcirc;irurgia</ref> forigo: 
      <ekz>
        <tld/>o de reno;
      </ekz>
      <ekz>
        <tld/>o de dentotartro
      </ekz>.
    </dif>
  </snc>
  <snc mrk="ablaci.0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="vid" cel="erozi.0o">Erozio</ref> a&ubreve; elvapori&gcirc;o
      pro frotado tra la atmosfero.  
    </dif>
  </snc>
  <trd lng="ca">ablaci&oacute;</trd>
  <trdgrp lng="cs">
    <trd>ablace</trd>,
    <trd>kon&ccaron;etiny</trd>,
    <trd>odn&ecaron;t&iacute; tk&aacute;n&ecaron;</trd>,
    <trd>org&aacute;nu</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ablation</trd>,
    <trd>Abtrag</trd>,
    <trd>Abschmelzen</trd>
  </trdgrp>
  <trd lng="en">ablation</trd>
  <trd lng="es">ablaci&oacute;n</trd>
  <trd lng="fr">ablation</trd>
  <trd lng="it">ablazione</trd>
  <trd lng="pl">ablacja</trd>
  <trd lng="ro">abla&#x021b;iune</trd>
  <trdgrp lng="sk">
    <trd>abl&aacute;cia</trd>,
    <trd>od&ncaron;atie tkaniva</trd>,
    <trd>org&aacute;nu</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x430;&#x431;&#x43B;&#x44F;&#x446;&#x456;&#x44F;</trd>,
    <trd>&#x432;&#x438;&#x434;&#x430;&#x43B;&#x435;&#x43D;&#x43D;&#x44F; &#x43E;&#x440;&#x433;&#x430;&#x43D;&#x430;</trd>
  </trdgrp>
</drv>

<drv mrk="ablaci.0agxo">
 <kap><tld/>a a&gcirc;o
   <fnt>Astronomia Terminaro</fnt>
  </kap>
  <snc mrk="ablaci.0agxo.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Tempoda&ubreve;ro ekde la fandi&gcirc;o de la vitra surfaco de
      aer&scirc;tono pro &gcirc;ia enirado tra la atmosfero.
    </dif>
  </snc>
  <trd lng="ca">temps d'ablaci&oacute;</trd>
  <trd lng="en">ablation age</trd>
  <trd lng="it">tempo di ablazione</trd>
  <trd lng="pl">wiek lodowcowy</trd>
</drv>
</art>
