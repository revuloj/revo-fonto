<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ablaci</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ablaci.0o">
  <kap><tld/>o</kap>
  <snc mrk="ablaci.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Surfaca <ref tip="vid" cel="erozi.0o">erozio</ref>.
    </dif>
  </snc>
  <snc mrk="ablaci.0o.GEOL1">
    <uzo tip="fak">GEOL</uzo>
    <uzo tip="fak">EKOL</uzo>
    <dif>
      Fandi&gcirc;o kaj elvapori&gcirc;o de glaciejo.
    </dif>
    <rim>erozio <em>de</em> glaciejo.</rim>
  </snc>
  <snc mrk="ablaci.0o.GOEL2">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Forporto de rokoj fare de glaciejo.
    </dif>
    <rim>erozio <em>fare de</em> glaciejo.</rim>
  </snc>
  <snc mrk="ablaci.0o.MED">
    <uzo tip="fak">MED</uzo>
    <uzo tip="fak">KIR</uzo>
    <dif>
      <ref tip="vid" cel="hxirur.0a.MED">&Hcirc;irurgia</ref> forigo: 
      <ekz>
        <tld/>o de reno
      </ekz>;
      <ekz>
        <tld/>o de dentotartro
      </ekz>.
    </dif>
  </snc>
  <snc mrk="ablaci.0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="vid" cel="erozi.0o">Erozio</ref> a&ubreve; elvapori&gcirc;o
      pro frotado tra la atmosfero.  
    </dif>
  </snc>
  <trd lng="ca">ablaci&oacute;</trd>
  <trd lng="de">Ablation</trd>
  <trd lng="en">ablation</trd>
  <trd lng="es">ablaci&oacute;n</trd>
  <trd lng="fr">ablation</trd>
  <trd lng="it">ablazione</trd>
</drv>

<drv mrk="ablaci.0agxo">
 <kap><tld/>a a&gcirc;o<fnt><url ref="http://at.komputilo.org/">Astronomia
Terminaro</url></fnt></kap>
  <snc mrk="ablaci.0agxo.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Tempoda&ubreve;ro ekde la fandi&gcirc;o de la vitra surfaco de
      aer&scirc;tono pro &gcirc;ia enirado tra la atmosfero.
    </dif>
  </snc>
  <trd lng="it">tempo di ablazione</trd>
  <trd lng="en">ablation age</trd>
</drv>
</art>
