<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: garn.xml,v 1.28 2023/11/11 10:00:39 revo Exp $">
<kap>
<ofc>3</ofc>
<rad>garn</rad>/i
</kap>
<drv mrk="garn.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="garn.0i.plifortigi">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Almeti ion al objekto por &gcirc;in &scirc;irmi a&ubreve; 
      plifortigi:
      <ekz>
        <tld/>i muron per pikiloj, vitron per kadro, barelon per feraj
        ringegoj, fendon per cemento, brakse&gcirc;on per tego.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>reinforce</trd>,
      <trd>strengthen</trd>,
      <trd>fortify</trd>,
      <trd>stud</trd>
    </trdgrp>
    <trd lng="es">reforzar</trd>
    <trd lng="pt">guarnecer</trd>
    <trd lng="ru">&c_s;&c_n;&c_a;&c_b;&c_d;&c_i;&c_t;&c_mol;</trd>
    <trdgrp lng="tr">
      <trd>doldurmak</trd>,
      <trd>d&ouml;&scedil;emek</trd>
    </trdgrp>
  </snc>
  <snc mrk="garn.0i.kompletigi">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Almeti ion al objekto por &gcirc;in
      <ref tip="super" cel="komple2.0igi">kompletigi</ref>
      la&ubreve; &gcirc;ia destino:
      <ekz>
        <tld/>i bibliotekon per libroj, fenestron per
        vitra&jcirc;o, veston per butonoj;
      </ekz>
      <ekz>
        <tld/>i tablon, teleron;
      </ekz>
      <ekz>
        <uzo tip="fak">KUI</uzo>
        bifsteko <tld/>ita per legomoj;
      </ekz>
      <ekz>
        <tld/>i vazon per floroj.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>complete</trd>,
      <trd>equip</trd>,
      <trd>fit out</trd>,
      <trd>furnish</trd>
    </trdgrp>
    <trd lng="es">guarnecer</trd>
    <trd lng="pl">uzupe&lstroke;nia&cacute;</trd>
    <trd lng="pt">rechear</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_n;&c_a;&c_b;&c_d;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_p;&c_r;&c_a;&c_v;&c_i;&c_t;&c_mol; <klr>(&c_o;
      &c_b;&c_l;&c_ju;&c_d;&c_je;)</klr></trd>,
      <trd>
      &c_g;&c_a;&c_r;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="garn.0i.ornami_iu">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Almeti akcesora&jcirc;on al objekto por &gcirc;in
      <ref tip="super" cel="ornam.0i">ornami</ref>:
      <ekz>
        <tld/>i &ccirc;apelon per plumoj;
      </ekz>
      <ekz>
        li <tld/>is la domon per multekostaj &scirc;tonoj por beleco
        <fnt><bib>MT</bib>, <lok>II. kroniko 3:6</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>adorn</trd>,
      <trd>decorate</trd>,
      <trd>embellish</trd>,
      <trd>trim</trd>
    </trdgrp>
    <trd lng="es">adornar</trd>
    <trdgrp lng="pl">
      <trd>garnirowa&cacute;</trd>,
      <trd>ozdabia&cacute;</trd>
    </trdgrp>
    <trd lng="pt">revestir</trd>
    <trd lng="ru">&c_u;&c_k;&c_r;&c_a;&c_s;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="garn.0i.ornami_io">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Servi por <tld/>i <sncref ref="garn.0i.ornami_iu"/>:
      <ekz>
        kadro <tld/>anta spegulon;
      </ekz>
      <ekz>
        broda&jcirc;oj, kiuj <tld/>as robon;
      </ekz>
      <ekz>
        artefarita najtingalo <tld/>ita per diamantoj, rubenoj
        kaj safiroj
        <fnt><bib>Fab2</bib>, <lok>La najtingalo</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="es">adornar</trd>
    <trd lng="pt">revestir</trd>
    <trd lng="ru">&c_u;&c_k;&c_r;&c_a;&c_sh;&c_a;&c_t;&c_mol;
    <klr tip="amb"> (&c_s;&c_o;&c_b;&c_o;&c_j;)</klr></trd>
    <trd lng="tr">s&uuml;slemek</trd>
  </snc>
  <refgrp tip="vid">
    <ref cel="ekip.0i">ekipi</ref>,
    <ref cel="proviz.0i">provizi</ref>.
  </refgrp>
  <adm>
  Pliprofundigi la paralelon kun provizi. <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_z;&c_d;&c_a;&c_b;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_r;&c_a;&c_sh;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_je;&c_zh;&c_u;)</klr></trd>,
    <trd>&c_u;&c_z;&c_m;&c_a;&c_c;&c_n;&c_ja;&c_c;&c_mol; <klr>(&c_d;&c_a;&c_d;&c_a;&c_ju;&c_ch;&c_y; &c_sh;&c_t;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>versehen <klr tip="ind">(mit etw.)</klr></trd>,
    <trd>ausstatten</trd>,
    <trd>schm&uuml;cken</trd>,
    <trd>garnieren</trd>
  </trdgrp>
  <trd lng="en">garnish</trd>
  <trdgrp lng="fr">
    <trd>garnir</trd>,
    <trd>munir</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x88DC;&#x5F37;&#x3059;&#x308B;  <pr>&#x307B;&#x304D;&#x3087;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x53D6;&#x308A;&#x4ED8;&#x3051;&#x308B;  <pr>&#x3068;&#x308A;&#x3064;&#x3051;&#x308B;</pr></trd>,
    <trd>&#x98FE;&#x308A;&#x4ED8;&#x3051;&#x308B;  <pr>&#x304B;&#x3056;&#x308A;&#x3064;&#x3051;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>garneren</trd>,
  </trdgrp>
  <trd lng="pt">guarnecer</trd>
  <trdgrp lng="sk">
    <trd>oblo&zcaron;i&tcaron;</trd>,
    <trd>ozdobi&tcaron;</trd>,
    <trd>zaopatri&tcaron; nie&ccaron;&iacute;m</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>oblo&zcaron;it</trd>,
    <trd>pob&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x434;&#x43E;&#x43F;&#x43E;&#x432;&#x43D;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x434;&#x43E;&#x434;&#x430;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x441;&#x43F;&#x43E;&#x440;&#x44F;&#x434;&#x436;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43E;&#x431;&#x43B;&#x430;&#x434;&#x43D;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x437;&#x430;&#x431;&#x435;&#x437;&#x43F;&#x435;&#x447;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x43E;&#x441;&#x442;&#x430;&#x447;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43E;&#x437;&#x434;&#x43E;&#x431;&#x43B;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x440;&#x438;&#x43A;&#x440;&#x430;&#x448;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x98FE; <pr>sh&#xEC;</pr></trd>,
    <trd>&#x5986; <pr>zhu&#x101;ng</pr></trd>,
    <trd>&#x4FEE;&#x98FE; <pr>xi&#x16B;sh&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="garn.0ajxo0ilo">
  <kap><tld/>a&jcirc;o, <var><kap><tld/>ilo</kap></var></kap>
  <snc>
    <dif>
      Tio, kio <tld/>as:
      <ekz>
        stranga kostumo, kiu havis <tld/>a&jcirc;on el grandaj blankaj
        sambukaj flora&jcirc;oj
        <fnt><bib>Fab2</bib>, <lok>Sambuka virineto</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">KUI</uzo>
        surtabligu kun <tld/>a&jcirc;o el subrostitaj legomoj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      Ne konfuzu kun:
      <ref cel="garnit.0o">garnituro</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_z;&c_d;&c_o;&c_b;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_r;&c_a;&c_s;&c_a; <klr>(&c_d;&c_a; &c_je;&c_zh;&c_y;)</klr></trd>,
    <trd>&c_g;&c_a;&c_r;&c_n;&c_ib;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schmuck</trd>,
    <trd>Garnierung</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>equipment</trd>,
    <trd>accessories</trd>,
    <trd>fittings</trd>
  </trdgrp>
  <trd lng="es">adorno</trd>
  <trdgrp lng="fr">
    <trd>garniture<klr tip="ind"> (ce qui garnit)</klr></trd>,
    <trd>accompagnement<klr tip="ind"> (garniture)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4ED8;&#x5C5E;&#x54C1;  <pr>&#x3075;&#x305E;&#x304F;&#x3072;&#x3093;</pr></trd>,
    <trd>&#x98FE;&#x308A;  <pr>&#x304B;&#x3056;&#x308A;</pr></trd>,
    <trd>&#x4ED8;&#x3051;&#x5408;&#x305B;  <pr>&#x3064;&#x3051;&#x3042;&#x308F;&#x305B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
   <trd>garnitur<klr> (komplet)</klr></trd>, 
   <trd>przybranie</trd>, 
   <trd>ozdoba</trd> 
  </trdgrp>
  <trdgrp lng="pt">
   <trd>guarni&ccedil;&atilde;o</trd>, 
   <trd>revestimento</trd>, 
   <trd>enfeite</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_k;&c_r;&c_a;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_g;&c_a;&c_r;&c_n;&c_i;&c_r; <klr>(&c_o; &c_je;&c_d;&c_je;)
    </klr></trd>,
    <trd>&c_z;&c_a;&c_p;&c_r;&c_a;&c_v;&c_k;&c_a; 
    <klr>(&c_o; &c_je;&c_d;&c_je;)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x440;&#x438;&#x43A;&#x440;&#x430;&#x441;&#x430;</trd>,
    <trd>&#x43E;&#x437;&#x434;&#x43E;&#x431;&#x430;</trd>,
    <trd>&#x491;&#x430;&#x440;&#x43D;&#x456;&#x440;</trd>,
    <trd>&#x437;&#x430;&#x43F;&#x440;&#x430;&#x432;&#x43A;&#x430;</trd>,
    <trd>&#x456;&#x437;&#x43E;&#x43B;&#x44E;&#x44E;&#x447;&#x430; &#x43F;&#x440;&#x43E;&#x43A;&#x43B;&#x430;&#x434;&#x43A;&#x430;</trd>
  </trdgrp>
  <trd lng="zh">&#x98FE;&#x54C1; <pr>sh&#xEC;p&#x1D0;n</pr></trd>
</drv>
</art>
