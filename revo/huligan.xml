<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>huligan</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="huligan.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ano de urba junula bando plimalpli perfortema, misfara a&ubreve; provoka:
      <ekz>
        Homoj de diversaj naciecoj suferas mistraktadon fare de
        <tld/>oj<fnt><aut>Garbhan MacAoidh</aut>: <vrk>Ni hontas kaj
        postulas</vrk>, <bib>Monato</bib><lok>jaro 2000a, numero 11a, p.
        7a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Halbstarker</trd>
  <trdgrp lng="en">
    <trd>hooligan</trd>,
    <trd>punk</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>hooligan</trd>,
    <trd>loubard</trd>,
    <trd>voyou</trd>
  </trdgrp>
  <trd lng="hu">hulig&aacute;n</trd>
  <trd lng="nl">hooligan</trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_u;&c_l;&c_i;&c_g;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="sv">huligan</trd>
</drv>
</art>

