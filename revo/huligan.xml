<kap>
  <rad>huligan</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="huligan.0o">
  <kap><tld/>o</kap>
  <snc mrk="huligan.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Ano de urba junula bando plimalpli perfortema, misfara a&ubreve;
      provoka:
      <ekz>
        homoj de diversaj naciecoj suferas mistraktadon fare de
        <tld/>oj
        <fnt><aut>G. MacAoidh</aut>, <vrk>Ni hontas kaj
        postulas</vrk>, <bib>Monato</bib>, <lok>jaro 2000a, numero 11a,
        p.
        7a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Anka&ubreve; eblas diri <ke>tumultisto</ke>, <ke>sporta
    tumult(am/em/kre)ulo</ke>, <ke>frakasisto</ke>.
  </rim>

  <trd lng="be">&c_h;&c_u;&c_l;&c_ib;&c_g;&c_a;&c_n;</trd>
  <trdgrp lng="ca">
    <trd>fan&agrave;tic</trd>,
    <trd>hooligan</trd>
  </trdgrp>
  <trd lng="de">Halbstarker</trd>
  <trdgrp lng="en">
    <trd>hooligan</trd>,
    <trd>punk</trd>
  </trdgrp>
  <trd lng="es">hooligan</trd>
  <trdgrp lng="fr">
    <trd>hooligan</trd>,
    <trd>loubard</trd>,
    <trd>voyou</trd>
  </trdgrp>
  <trd lng="hu">hulig&aacute;n</trd>
  <trd lng="it">hooligan</trd>
  <trd lng="nl">hooligan</trd>
  <trd lng="pl">chuligan</trd>
  <trdgrp lng="pt">
    <trd>hooligan</trd>,
    <trd>v&acirc;ndalo</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_h;&c_u;&c_l;&c_i;&c_g;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="sv">huligan</trd>
  <trd lng="uk">&c_h;&c_u;&c_l;&c_ib;&c_g;&c_a;&c_n;</trd>
  <trdgrp lng="zh">
    <trd>&#21476;&#24785;&#20180; <pr>g&#468;hu&ograve;z&#462;i</pr></trd>,
    <trd>&#23567;&#28151;&#28151; <pr>xi&#462;oh&ugrave;nh&ugrave;n</pr></trd>,
    <trd>&#38463;&#39131; <pr>&amacron;f&emacron;i</pr></trd>,
    <trd>&#27969;&#27667; <pr>li&uacute;m&aacute;ng</pr></trd>,
    <trd>&#30174;&#23376; <pr>p&#464;zi</pr></trd>
  </trdgrp>
</drv>
</art>

