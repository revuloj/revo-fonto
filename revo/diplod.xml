<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: diplod.xml,v 1.19 2023/11/04 14:29:02 revo Exp $">
<kap>
  <rad>diplodok</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="diplod.0o">
  <kap><tld/>o</kap>
  <snc mrk="diplod.0o.ZOO">
    <uzo tip="fak">PAL</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Fosilia <ref tip="lst" lst="voko:reptilioj" cel="reptil.0o">reptilio</ref>
      apartenanta al la ordo de <ref tip="malprt" cel="dinosa.0oj">dinosa&ubreve;roj</ref>
      (<trd lng="la">Diplodocus</trd>), kiu atingis e&ccirc; la longon de dudek kvar kaj la alton de kvin
      metroj, kun V-formaj ostoj sube de la vostovertebroj,
      kun tre longa kolo kaj vipeca vosto kaj anta&ubreve;aj ekstrema&jcirc;oj pli mallongaj
      ka&ubreve;zante horizontalan sitenon:
      <ekz>
        sa&ubreve;ropodoj <klr>[&#x2026;]</klr> estas konataj pro siaj rekordaj longecoj de korpo kaj
        kolo, fama ekzemplo de sa&ubreve;ropodo estas <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=604760">Sa&ubreve;ropodoj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <uzo tip="stl">VULG</uzo>
        je ducerba <tld/>o! &gcirc;uste al kiu el la tri simioj &dash; nevida, nea&ubreve;da kaj neparola
        &dash; oni komisiis por kontroli la provpresa&jcirc;on
        <fnt>
          <aut>Pejno Simono</aut>
          <vrk><url ref="&Monato;/2001/007110.php">Atentokapta sed ru&gcirc;krajoninda</url></vrk>
          <bib>Monato</bib>
          <lok>2001/01, p. 31</lok>
          </fnt>?
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/c/cf/Diplodocus_(replica).001_-_London.JPG/640px-Diplodocus_(replica).001_-_London.JPG" prm="CC BY-SA 4.0">reprodukto de <tld/>a skeleto en la Naturhistoria Muzeo de Londono
        <fnt>
          <aut>Drow male, propra verko</aut>
          <lok><url ref="&WCW;/File:Diplodocus_(replica).001_-_London.JPG">&WCW;/File:Diplodocus_(replica).001_-_London.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_p;&c_l;&c_ja;&c_d;&c_o;&c_k;</trd>
  <trd lng="de">Diplodokus</trd>
  <trd lng="en">diplodocus</trd>
  <trd lng="fr">diplodocus</trd>
  <trd lng="hu">diplodocus</trd>
  <trd lng="ja">&#x30C7;&#x30A3;&#x30D7;&#x30ED;&#x30C9;&#x30AF;&#x30B9;</trd>
  <trd lng="nl">diplodocus</trd>
  <trd lng="pl">diplodok</trd>
  <trd lng="pt">dipl&oacute;doco</trd>
  <trd lng="ru">&c_d;&c_i;&c_p;&c_l;&c_o;&c_d;&c_o;&c_k;</trd>
  <trd lng="uk">&c_d;&c_i;&#x43F;&#x43B;&#x43E;&c_d;&#x43E;&#x43A;</trd>
  <trd lng="zh">&#x6881;&#x9F8D; <pr>li&#xE1;ngl&#xF3;ng</pr></trd>
</drv>
</art>
