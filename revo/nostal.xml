    <trd>&#26395;&#37111;  <pr>&#12412;&#12358;&#12365;&#12423;&#12358;</pr></trd>,
    <trd>&#12494;&#12473;&#12479;&#12523;&#12472;&#12450;</trd>,
    <trd>&#25040;&#26087;&#12398;&#24773;  <pr>&#12405;&#12392;&#12371;&#12429;&#12365;&#12421;&#12358;&#12398;&#12376;&#12423;&#12358;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>heimwee</trd>,
    <trd>nostalgie</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>nostalgia</trd>,
    <trd>t&eogonek;sknota</trd>
  </trdgrp>
  <trd lng="pt">nostalgia</trd>
  <trdgrp lng="ro">
