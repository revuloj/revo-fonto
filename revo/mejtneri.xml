<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mejtneri</rad>/o
</kap>
<drv mrk="mejtneri.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 109, atompezo 266, simbolo Mt. IUPAC ne jam findecidis
      pri la nomo de tiu elemento kaj provizore uzas unnilenio.
      <ref tip="super" cel="urani.trans0o">Transuranio</ref>.
      &Ccirc;iuj izotopoj de tiu elemento estas radioaktivaj.
    </dif>
  </snc>
  <trd lng="de">Meitnerium</trd>
  <trd lng="hu">meitnerium</trd>
  <trd lng="nl">meitnerium</trd>
  <trd lng="ru">&c_m;&c_je;&c_j;&c_t;&c_n;&c_je;&c_r;&c_i;&c_j;</trd>
</drv>
</art>
