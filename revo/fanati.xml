<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>fanatik</rad>/a
</kap>
<drv mrk="fanati.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Blinde fervora kaj senindulge ekscesanta en religio,
      politiko a&ubreve; doktrino:
      <ekz>
        <tld/>a mona&hcirc;o, <tld/>a senkompato;
      </ekz>
      <ekz>
        <ind><tld/>eco</ind>;
        <trd lng="fr">fanatisme</trd>
        <trd lng="nl">fanatisme</trd>
      </ekz>
      <ekz>
        <ind><tld/>ulo</ind>.
        <trd lng="fr">fanatique<klr tip="ind"> (subst.)</klr></trd>
        <trd lng="nl">fanaticus</trd>
    </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="zelot.0o">zeloto</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>fanatisch</trd>,
    <trd>besessen</trd>,
    <trd>blindgl&auml;ubig</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fanatique</trd>,
    <trd>enrag&eacute;<klr tip="ind"> (fanatique)</klr></trd></trdgrp> 
  <trdgrp lng="nl">
    <trd>fanatiek</trd>,
    <trd>bezeten<klr tip="ind"> (fanatiek)</klr></trd>,
    <trd>dweepziek</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_f;&c_a;&c_n;&c_a;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_f;&c_a;&c_n;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
