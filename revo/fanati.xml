<drv mrk="fanati.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Blinde fervora k senindulge ekscesanta en religio,
      politiko a&ubreve; doktrino:
      <ekz><tld/>a mona&hcirc;o, <tld/>a senkompato;</ekz>
      <ekz><tld/>eco, <tld/>ulo.</ekz>
   </dif>
   <refgrp tip="sin">
     <ref cel="zelot.0o">zeloto</ref>.
   </refgrp>
   </snc>
   <trdgrp lng="ru">
     <trd>&c_f;&c_a;&c_n;&c_a;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
     <trd>&c_f;&c_a;&c_n;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
   </trdgrp>
</drv>
</art>
