<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kalend2.xml,v 1.25 2022/03/21 17:51:56 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>kalendul</rad>/o
</kap>
<drv mrk="kalend2.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="astera.0oj">asteracoj</ref>
      (<trd lng="la">Calendula</trd>), kun oran&gcirc;a a&ubreve; flava floraro:
      <ekz>
        ili prenis kun si anka&ubreve; la malestimatan <tld/>on kaj la sova&gcirc;an
        trikoloreton
        <fnt>
          <bib>Fab2</bib>
          <lok>An&gcirc;elo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o<klr>[n]</klr> kovras etaj glandoharoj (kiuj eligas 
        karakte<klr>[r]</klr>izan volatilan oleon), la folioj estas iom
        karnecaj,
        longaj de 10&ndash;15, la unuopaj floroj longas 2&ndash;3 cm, havas oran&gcirc;an
        a&ubreve; citronan koloron
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=129962">Kalendulo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="&WCU;/thumb/0/09/Calendula_officinalis_3517.jpg/640px-Calendula_officinalis_3517.jpg" prm="CC-BY-SA-3.0"><ind>ordinara <tld/>o</ind> (Calendula officinalis) en &gcirc;ardeno en
     la regiono Nigra Arbaro
       <fnt>
         <aut>Flominator, propra foto</aut>
         <lok><url ref="&WCW;/File:Calendula_officinalis_3517.jpg">&WCW;/File:Calendula_officinalis_3517.jpg</url></lok>
       </fnt>
    <trd lng="ca">boixac</trd>
    <trd lng="la">Calendula officinalis</trd>
  </bld>
  <trd lng="be">&c_n;&c_a;&c_g;&c_o;&c_t;&c_k;&c_a;</trd>          
  <trd lng="br">rozinilenn</trd>
  <trdgrp lng="ca">
    <trd>cal&egrave;ndula</trd>,
    <trd>boixac</trd>
  </trdgrp>
  <trd lng="de">Ringelblume</trd>
  <trd lng="en">marigold</trd>
  <trd lng="es">cal&eacute;ndula</trd>
  <trd lng="fa">&a_h;&a_m;&f_y;&a_sh;&a_h;&zwnj;&a_b;&a_h;&a_A;&a_r;</trd>
  <trd lng="fr">souci<klr tip="ind"> (bot.)</klr></trd>
  <trd lng="hu">k&ouml;r&ouml;mvir&aacute;g</trd>
  <trd lng="nl">goudsbloem</trd>
  <trd lng="pl">nagietek</trd>
  <trd lng="pt">cal&ecirc;ndula</trd>
  <trd lng="ro">calendula</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_o;&c_g;&c_o;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_je;&c_n;&c_d;&c_u;&c_l;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">mesia&ccaron;ik</trd>
  <trd lng="cs">m&ecaron;s&iacute;&ccaron;ek <klr>(rostl.)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x43D;&#x430;&#x433;&#x456;&#x434;&#x43A;&#x438;</trd>,
    <trd>&#x43A;&#x430;&#x43B;&#x435;&#x43D;&#x434;&#x443;&#x43B;&#x430;</trd>
  </trdgrp>
</drv>
</art>
