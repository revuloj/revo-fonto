<art mrk="$Id: hindukusx.xml,v 1.16 2023/11/15 17:08:00 revo Exp $">
<kap>
  <rad>Hinduku&scirc;</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="hindukusx.H0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="mont.0aro" lst="voko:montaroj">Montaro</ref> 
      en la
      <ref tip="malprt" cel="azi.InternaA0o">Interna Azio</ref>,
      proksimume 800&#x202F;km longa, formanta akvodislimon inter la basenoj de
      <ref cel="amudarj.A0o">Amudarjo</ref> nordokcidente kaj
      Induso sudoriente:
      <ekz>
        la montaroj de la <tld/>o, &gcirc;is 7500 metrojn alta, kaj de la Sefid Kuh etendi&gcirc;as
        sur vastaj partoj de <klr>[Afganujo]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=5">Afganio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_G;&c_ib;&c_n;&c_d;&c_u;&c_k;&c_u;&c_sh;</trd>
  <trd lng="de">Hindukusch</trd>
  <trd lng="en">Hindu Kush</trd>
  <trd lng="es">Hind&uacute; Kush</trd>
  <trdgrp lng="fr">
    <trd>Hindu Kush</trd>,
    <trd>Hindou Kouch</trd>
  </trdgrp>
  <trd lng="hu">Hindukus</trd>
  <trdgrp lng="it">
    <trd>Hindu Kush</trd>,
    <trd>Hindukush</trd>
  </trdgrp>
  <trd lng="ja">&#x30D2;&#x30F3;&#x30BA;&#x30FC;&#x30AF;&#x30B7;&#x5C71;&#x8108;  <pr>&#x30D2;&#x30F3;&#x30BA;&#x30FC;&#x30AF;&#x30B7;&#x3055;&#x3093;&#x307F;&#x3083;&#x304F;</pr></trd>
  <trd lng="nl">Hindoekoesj</trd>
  <trd lng="no">Hindu Kush</trd>
  <trd lng="pt">Indocuche</trd>
  <trd lng="ru">&c_G;&c_i;&c_n;&c_d;&c_u;&c_k;&c_u;&c_sh;</trd>
  <trd lng="uk">&#x433;&#x43E;&#x440;&#x438; &#x413;&#x456;&#x43D;&#x434;&#x443;&#x43A;&#x443;&#x448;</trd>
  <trdgrp lng="zh">
    <trd>&#x5174;&#x90FD;&#x5E93;&#x4EC0; <pr>x&#x12B;ngd&#x16B;k&#xF9;sh&#xED;</pr></trd>,
    <trd>&#x5174;&#x90FD;&#x5E93;&#x4EC0;&#x5C71;&#x8109; <pr>x&#x12B;ngd&#x16B;k&#xF9;sh&#xED;sh&#x101;nm&#xE0;i</pr></trd>
  </trdgrp>
</drv>
</art>
