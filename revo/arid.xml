  <art mrk="$Id: arid.xml,v 1.11 2023/05/26 14:27:16 revo Exp $">
    <kap>
      <rad>arid</rad>/a <fnt><bib>PIV1</bib></fnt>
    </kap>

    <drv mrk="arid.0a">
      <kap><tld/>a</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
    	<dif>
    	  Vetere konstante seka kaj sekve ne fekunda:
    	  <ekz>
    	    antikva civilizacio, kiu [...] disvolvis sukcesan agrikulturon en
    	    tiu <tld/>a regiono
            <fnt>
        <aut>Garbhan MacAoidh</aut>: 
        <vrk><url ref="&Monato;/2002/pdf/monato200202.pdf#page=26">La ajmara</url></vrk>,
        <bib>Monato</bib>
        <lok>2002/02, p. 26-28</lok>
        </fnt>.
    	  </ekz>
    	</dif>
      </snc>
      <ref tip="super" cel="klimat.0o">klimato</ref>
      <trdgrp lng="be">
        <trd>&c_z;&c_a;&c_s;&c_u;&c_sh;&c_l;&c_ib;&c_v;&c_y;</trd>,
        <trd>&c_a;&c_r;&c_y;&c_d;&c_n;&c_y;</trd>
      </trdgrp>
      <trd lng="ca">&agrave;rid</trd>
      <trd lng="en">arid</trd>
      <trdgrp lng="de">
        <trd>arid</trd>,
        <trd>trocken</trd>
      </trdgrp>
      <trd lng="es">&aacute;rido</trd>
      <trdgrp lng="fr">
    	<trd>aride</trd>
      </trdgrp>
      <trd lng="he">&#x5E6;&#x5B8;&#x5D7;&#x5B4;&#x5D9;&#x5D7;&#x5B7;</trd>
  <trdgrp lng="hu">
        <trd>kop&aacute;r</trd>,
        <trd>szikkadt</trd>
      </trdgrp>
      <trd lng="nl">droog</trd>
      <trdgrp lng="ru">
    	<trd>&c_z;&c_a;&c_s;&c_u;&c_sh;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>,
    	<trd>&c_a;&c_r;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>
      </trdgrp>
      <trd lng="sk">vyprahnut&#xFD;</trd>
  <trdgrp lng="cs">
    <trd>aridn&#xED;</trd>,
    <trd>pou&scaron;tn&#xED;</trd>,
    <trd>pustinn&#xFD;</trd>,
    <trd>vyprahl&#xFD;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_u;&c_h;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_u;&c_sh;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_v;&#x43E;&c_d;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
