<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>gigant</rad>/o
</kap>
<drv mrk="gigant.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Homo eksterordinare, nenormale granda:
      <ekz>la mitologiaj <tld/>oj.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="grand.0egulo">grandegulo</ref>;
    </refgrp>
    <refgrp tip="vid">
      <ref cel="kolos.0o">koloso</ref>,
      <ref cel="titan.0o">titano</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_ib;&c_g;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_v;&c_o;&c_l;&c_a;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Gigant</trd>,
     <trd>Riese</trd></trdgrp>
  <trd lng="es">gigante</trd>
  <trd lng="fr">g&eacute;ant<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="hu">&oacute;ri&aacute;s</trd>
  <trd lng="nl">reus</trd>
  <trd lng="pt">gigante<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_i;&c_g;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_i;&c_s;&c_p;&c_o;&c_l;&c_i;&c_n;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_a;&c_n;</trd>
  </trdgrp>
</drv>
<drv mrk="gigant.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Grandega:
      <ekz><tld/>a homo, monto, laboro;</ekz>
      <ekz>mi vidis <tld/>an virinan figuron<fnt>Z</fnt>;</ekz>
      <ekz>iliaj abelaj klopodoj k iliaj <tld/>aj projektoj<fnt>Z</fnt>.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kolos.0a">kolosa</ref>,
      <ref cel="mamut">mamuta</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_ib;&c_g;&c_a;&c_n;&c_c;&c_k;&c_ib;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_ib;&c_z;&c_a;&c_r;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>gigantisch</trd>,
     <trd>riesig</trd></trdgrp>
  <trd lng="es">gigante</trd>
  <trdgrp lng="fr">
    <trd>g&eacute;ant<klr tip="ind"> (adj.)</klr></trd>,
    <trd>gigantesque</trd></trdgrp> 
  <trdgrp lng="hu">
    <trd>&oacute;ri&aacute;s</trd>,
    <trd>gigantikus</trd></trdgrp>
  <trd lng="nl">reusachtig</trd>
  <trdgrp lng="pt">
   <trd>gigante<klr tip="ind"> (adj.)</klr></trd>, 
   <trd>gigantesco</trd> 
  </trdgrp>
  <trd lng="ru">&c_g;&c_i;&c_g;&c_a;&c_n;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
