<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kliv</rad>/i
</kap>
<drv mrk="kliv.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MIN</uzo>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Fendi mineralon la&ubreve; natura direkto de &gcirc;iaj tavoloj. 
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>cleave</trd>,
    <trd>split</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_k;&c_a;&c_l;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_shch;&c_je;&c_p;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_s;&c_l;&c_a;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="kliv.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i a&ubreve; la rezulto de tiu ago.  
    </dif>
  </snc>
</drv>
</art>
