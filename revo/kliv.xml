<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kliv.xml,v 1.23 2023/12/03 09:58:50 revo Exp $">
<kap>
  <rad>kliv</rad>/i
</kap>
<drv mrk="kliv.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="fak">MIN</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="fend.0i.KOMUNE">Fendi</ref>
      mineralon la&ubreve; natura direkto de &gcirc;iaj tavoloj:
      <ekz>
        <tld/>ebleco priskribas &ccirc;e mineraloj kaj kristaloj la eblecon
        por <tld/>i, fendi
        je certaj paralelaj surfacoj en kristala krado
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=36791">Klivebleco</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_ch;&c_a;&c_p;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_l;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>roz&scaron;t&ecaron;pit <klr>(nerost)</klr></trd>,
    <trd>&scaron;t&ecaron;pit</trd>
  </trdgrp>
  <trd lng="de">spalten <klr>(Mineral)</klr></trd>
  <trdgrp lng="en">
    <trd>cleave</trd>,
    <trd>split</trd>
  </trdgrp>
  <trd lng="fr">cliver<klr tip="ind"> (un min&eacute;ral)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x77F3;&#x76EE;&#x306B;&#x305D;&#x3063;&#x3066;&#x5272;&#x308B; 
    <pr>&#x3044;&#x3057;&#x3081;&#x306B;&#x305D;&#x3063;&#x3066;&#x308F;&#x308B;</pr></trd>,
    <trd>&#x5288;&#x958B;&#x3055;&#x305B;&#x308B; <pr>&#x3078;&#x304D;&#x304B;&#x3044;&#x3055;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>klieven</trd>,
    <trd>kloven</trd>,
    <trd>splijten</trd>
  </trdgrp>
  <trd lng="pl">&lstroke;upa&cacute;</trd>
  <trdgrp lng="ro">
    <trd>cliva</trd>,
    <trd>despica</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_k;&c_a;&c_l;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_l;&c_o;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_shch;&c_je;&c_p;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_shch;&c_je;&c_p;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_s;&c_l;&c_a;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_s;&c_l;&c_o;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">&scaron;tiepi&tcaron; <klr>(nerast pod&lcaron;a vrstvy)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&c_u;&#x43F;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&#x43E;&c_z;&#x43A;&#x43E;&#x43B;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&#x43E;&c_z;&c_shch;&c_je;&#x43F;&#x43B;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&#x43E;&c_z;&c_sh;&c_a;&c_r;&#x43E;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i; (&#x43C;&c_ib;&#x43D;&c_je;&c_r;&c_a;&#x43B;&c_i;)</trd>
  </trdgrp>
</drv>

<drv mrk="kliv.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i a&ubreve; la rezulto de tiu ago:
      <ekz>
        glimo estas grupo de silikatmineraloj, kiu inkluzivas plurajn
        materialojn,
        havantan tre perfektan <tld/>on
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=26793">Glimo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_l;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_ch;&c_a;&c_p;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="de">Spaltung <klr>(Mineral)</klr></trd>
  <trd lng="fr">clivage<klr tip="ind"> (min&eacute;r.)</klr></trd>
  <trd lng="ja">&#x5288;&#x958B;  <pr>&#x3078;&#x304D;&#x304B;&#x3044;</pr></trd>
  <trdgrp lng="nl">
    <trd>kloof</trd>,
    <trd>gekloof</trd>
  </trdgrp>
  <trd lng="pl">&lstroke;upanie</trd>
  <trd lng="ro">laminare</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_shch;&c_je;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_k;&c_o;&c_l;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_l;<klr tip="amb"> (&c_o; &c_m;&c_i;&c_n;&c_je;&c_r;&c_a;&c_l;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="zh">&#x4E91;&#x6BCD;&#x8584;&#x7247; <pr>y&#xFA;nm&#x1D4;b&#xF3;pi&#xE0;n</pr></trd>
</drv>
</art>
