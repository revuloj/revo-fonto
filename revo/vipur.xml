<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>vipur</rad>/o
</kap>
<drv mrk="vipur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="vipur.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Vipera</trd>), malgranda venena serpento.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Viper</trd>,
    <trd>Otter <klr tip="ind">(Schlange)</klr></trd>
  </trdgrp>
  <trd lng="en">viper</trd>
  <trd lng="fr">vip&egrave;re</trd>
</drv>
<drv mrk="vipur.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="skvam.0uloj">skvamuloj</ref>
      (<trd lng="la">Viperidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Vipern <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>
