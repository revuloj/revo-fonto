<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>asket</rad>/o
      <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
    </kap>

    <drv mrk="asket.0o">
      <kap><tld/>o</kap>
      <snc mrk="asket.0o.REL">
    	<dif>
    	  Praktikanto de <tld/>ismo.
    	</dif>
      </snc>
  <trd lng="grc">&alfa_psili;&sigma;&kappa;&eta;&tau;&eta_acute;&sigma_fina;</trd>
  <trd lng="be">&c_a;&c_s;&c_k;&c_je;&c_t;</trd>
  <trd lng="bg">&c_a;&c_s;&c_k;&c_je;&c_t;</trd>
  <trd lng="ca">asceta</trd>
  <trd lng="cs">asketa</trd>
  <trd lng="de">Asket</trd>
  <trd lng="en">ascetic</trd>
  <trd lng="es">asceta</trd>
  <trdgrp lng="fa">
         <trd>&a_r;&f_y;&a_A;&a_D;&a_t;&zwnj;&f_k;&a_sh;</trd>
         <trd>&a_m;&a_r;&a_t;&a_A;&a_D;</trd>
      </trdgrp>
  <trd lng="fr">asc&egrave;te</trd>
  <trd lng="hu">aszk&eacute;ta</trd>
  <trd lng="it">asceta</trd>
  <trd lng="nl">asceet</trd>
  <trdgrp lng="ru">
         <trd>&c_a;&c_s;&c_k;&c_je;&c_t;</trd>,
         <trd>&c_p;&c_o;&c_s;&c_t;&c_n;&c_i;&c_k;</trd>
      </trdgrp>
  <trdgrp lng="sk">
    <trd>ask&eacute;t</trd>,
    <trd>ask√©ta</trd>
  </trdgrp>
</drv>

    <drv mrk="asket.0ismo">
      <kap><tld/>ismo</kap>
      <snc mrk="asket.0ismo.REL">
    	<dif>
    	  Sistema limigado a&ubreve; subpremado de la korpaj deziroj,
    	  libervola submeti&gcirc;o al fizika doloro, soleco ktp, propra al
    	  la rekomendoj de iuj skoloj filozofiaj, ekzemple la
          <ref cel="cinik.0isto">cinikistoj</ref>, kaj precipe al
    	  diversaj religioj, pro motivoj diversaj &dash;
    	  memperfekti&gcirc;o, higieno, protesto kontra&ubreve; socia malegaleco ktp.
    	</dif>
      </snc>
      <ref tip="vid" cel="fast.0o">fasto</ref>
      <trd lng="be">&c_a;&c_s;&c_k;&c_je;&c_t;&c_y;&c_z;&c_m;</trd>
      <trd lng="bg">&c_a;&c_s;&c_k;&c_je;&c_t;&c_i;&c_z;&c_malmol;&c_m;</trd>
      <trd lng="ca">ascetisme</trd>
      <trd lng="de">Askese</trd>
      <trd lng="en">asceticism</trd>
      <trd lng="es">ascetismo</trd>
      <trdgrp lng="fa">
         <trd>&a_r;&f_y;&a_A;&a_D;&a_t;&zwnj;&f_k;&a_sh;&f_y;</trd>
         <trd>&a_r;&f_y;&a_A;&a_D;&a_t;</trd>
      </trdgrp>
      <trd lng="fr">asc&eacute;tisme</trd>
      <trd lng="hu">aszketizmus</trd>
      <trd lng="it">ascetismo</trd>
      <trdgrp lng="ru">
         <trd>&c_a;&c_s;&c_k;&c_je;&c_z;&c_a;</trd>,
         <trd>&c_a;&c_s;&c_k;&c_je;&c_t;&c_i;&c_z;&c_m;</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>ascetisme</trd>,
        <trd>ascese</trd>
      </trdgrp>
      <trd lng="sk">asketizmus</trd>
      <trd lng="cs">asketizmus</trd>
</drv>
