<drv mrk="flav.0eta">
  <kap><tld/>eta</kap>
  <snc mrk="flav.0eta.KOL">
    <dif>
      Iom, proksimume <tld/>a:
      <ekz>
        haroj <tld/>etaj
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 13:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        nia tero ku&scirc;is envolvita en <tld/>eta nebulo
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia <tld/>eta ha&ubreve;to estis makulita de pustuloj
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_w;&c_t;&c_a;&c_v;&c_a;&c_t;&c_y;</trd>
  <trd lng="cs">na&zcaron;loutl&yacute;</trd>
  <trd lng="de">gelblich</trd>
  <trd lng="en">yellowish</trd>
  <trd lng="fr">jaun&acirc;tre</trd>
  <trd lng="hu">s&aacute;rg&aacute;s</trd>
  <trdgrp lng="id">
    <trd><baz>kuning</baz>kekuningan</trd>,
    <trd><baz>kuning</baz>kekuning-kuningan</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#40644;&#33394;&#12387;&#12413;&#12356;  <pr>&#12365;&#12356;&#12429;&#12387;&#12413;&#12356;</pr></trd>,
    <trd>&#40644;&#12400;&#12435;&#12384;  <pr>&#12365;&#12400;&#12435;&#12384;</pr></trd>
  </trdgrp>
  <trd lng="nl">geelachtig</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_t;&c_o;&c_v;&c_a;&c_t;&c_y;&c_j;</trd>
  <trd lng="sk">na&zcaron;ltnut&yacute;</trd>
  <trd lng="uk">&c_zh;&c_o;&c_v;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
