<drv mrk="flav.0o">
  <kap><tld/>o</kap>
  <snc mrk="flav.0o.KOL">
    <dif>
      <tld lit="F"/>a <ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">koloro</ref>:
      <ekz>
        la arbaro ricevis la plej belajn kolorojn de ru&gcirc;o,
        <tld/>o kaj verdo
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vangoj de Janjo kavi&gcirc;is kaj kovri&gcirc;is per malsaneca <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        sulfura <tld/>o
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>En la Lando de la Noktuloj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_w;&c_c;&c_ib;&c_z;&c_n;&c_a;</trd>
  <trd lng="bg">&c_zh;&c_malmol;&c_l;&c_t;&c_o;</trd>
  <trd lng="cs">&zcaron;lu&tcaron;</trd>
  <trd lng="de">Gelb</trd>
  <trd lng="en">yellow</trd>
  <trd lng="es">amarillo</trd>
  <trd lng="fr">jaune<klr> (couleur)</klr></trd>
  <trd lng="hu">s&aacute;rga<klr tip="ind"> (sz&iacute;n)</klr></trd>
  <trd lng="ja">&#x9EC4;&#x8272;  <pr>&#x304D;&#x3044;&#x308D;</pr></trd>
  <trd lng="ku">zer</trd>
  <trd lng="nl">geel<klr tip="ind"> (z.nw.)</klr></trd>
  <trd lng="pt">amarelo</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_t;&c_i;&c_z;&c_n;&c_a;</trd>
  <trd lng="sk">&zcaron;l&tcaron;</trd>
</drv>

<drv mrk="flav.0eta">
  <kap><tld/>eta</kap>
  <snc mrk="flav.0eta.KOL">
    <dif>
      Iom, proksimume <tld/>a:
      <ekz>
        haroj <tld/>etaj
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 13:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        nia tero ku&scirc;is envolvita en <tld/>eta nebulo
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia <tld/>eta ha&ubreve;to estis makulita de pustuloj
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_w;&c_t;&c_a;&c_v;&c_a;&c_t;&c_y;</trd>
  <trd lng="cs">na&zcaron;loutl&yacute;</trd>
  <trd lng="de">gelblich</trd>
  <trd lng="en">yellowish</trd>
  <trd lng="fr">jaun&acirc;tre</trd>
  <trd lng="hu">s&aacute;rg&aacute;s</trd>
  <trdgrp lng="id">
    <trd>ke<ind>kuning</ind>an</trd>,
    <trd>ke<ind>kuning</ind>-kuningan</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x9EC4;&#x8272;&#x3063;&#x307D;&#x3044;  <pr>&#x304D;&#x3044;&#x308D;&#x3063;&#x307D;&#x3044;</pr></trd>,
    <trd>&#x9EC4;&#x3070;&#x3093;&#x3060;  <pr>&#x304D;&#x3070;&#x3093;&#x3060;</pr></trd>
  </trdgrp>
  <trd lng="nl">geelachtig</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_t;&c_o;&c_v;&c_a;&c_t;&c_y;&c_j;</trd>
  <trd lng="sk">na&zcaron;ltnut&yacute;</trd>
  <trd lng="uk">&c_zh;&c_o;&c_v;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
