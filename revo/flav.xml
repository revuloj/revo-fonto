<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>flav</rad>/a
</kap>
<drv mrk="flav.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Samkolora kiel interna bulo de kokinovo, citrono, oro, matura
      tritiko:
      <ekz>
        la neimageblaj ebloj de la <tld/>a florpolvo<fnt><aut>Robert
        Roze</aut>: <vrk>Vivisekcio de polenero</vrk>,
        <bib>Monato</bib><lok>jaro 1997a, numero 11a, p. 12a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_o;&c_w;&c_t;&c_y;</trd>
  <trd lng="cs">&zcaron;lut&yacute;</trd>
  <trd lng="de">gelb</trd>
  <trd lng="en">yellow</trd>
  <trd lng="es">amarillo</trd>
  <trd lng="fr">jaune<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="hu">s&aacute;rga</trd>
  <trd lng="nl">geel</trd>
  <trd lng="pl">&zdot;&oacute;&lstroke;ty</trd>
  <trd lng="ru">&c_zh;&c_jo;&c_l;&c_t;&c_y;&c_j;</trd>
  <trd lng="sv">gul</trd>
  <trd lng="tr">sar&inodot;</trd>
  <trd lng="vo">yelibik</trd>
</drv>

<drv mrk="flav.0eta">
  <kap><tld/>eta</kap>
  <snc>
    <dif>
      Iom, proksimume <tld/>a.
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_w;&c_t;&c_a;&c_v;&c_a;&c_t;&c_y;</trd>
  <trd lng="de">gelblich</trd>
  <trd lng="en">yellowish</trd>
  <trd lng="fr">jaun&acirc;tre</trd>
  <trd lng="hu">s&aacute;rg&aacute;s</trd>
  <trd lng="nl">geelachtig</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_t;&c_o;&c_v;&c_a;&c_t;&c_y;&c_j;</trd>
</drv>

<drv mrk="flav.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="F"/>a <ref tip="super" cel="kolor.0o.propre">koloro</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_w;&c_c;&c_ib;&c_z;&c_n;&c_a;</trd>
  <trd lng="es">amarillo</trd>
  <trd lng="en">yellow</trd>
  <trd lng="fr">jaune<klr> (couleur)</klr></trd>
  <trd lng="hu">s&aacute;rga<klr tip="ind"> (sz&iacute;n)</klr></trd>
  <trd lng="nl">geel<klr tip="ind"> (z.nw.)</klr></trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_t;&c_i;&c_z;&c_n;&c_a;</trd>
</drv>

<drv mrk="flav.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="flav.0ulo.flavrasulo">
    <dif>
      Homo el <tld/>a raso.
    </dif>
    <trd lng="be">&c_zh;&c_a;&c_w;&c_t;&c_a;&c_s;&c_k;&c_u;&c_r;&c_y;</trd>
    <trd lng="fr">jaune<klr tip="ind"> (asiatique)</klr></trd>
    <trd lng="en">yellow man</trd>
    <trd lng="hu">s&aacute;rgab&odblac;r&udblac;<klr tip="ind"> (ember)</klr></trd>
    <trd lng="nl">geelhuid</trd>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k; <ind>&c_zh;&c_jo;&c_l;&c_t;&c_o;&c_j;</ind> &c_r;&c_a;&c_s;&c_y;</trd>,
      <trd>&c_a;&c_z;&c_i;&c_a;&c_t;</trd>
    </trdgrp>
  </snc>
  <snc mrk="flav.0ulo.laboristo">
    <dif>
      Laboristo kontra&ubreve;a al la klasbatala politiko.
    </dif>
    <trd lng="fr">jaune<klr tip="ind"> (briseur de gr&egrave;ve)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_sh;&c_t;&c_r;&c_je;&c_j;&c_k;&c_b;&c_r;&c_je;&c_h;&c_je;&c_r;</trd>,
    </trdgrp>
  </snc>
</drv>

<drv mrk="flav.ov0o">
  <kap>ovo<tld/>(a&jcirc;)o</kap>
  <snc>
    <dif>
      <tld lit="F"/>a nutra parto de ovo.
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_w;&c_t;&c_o;&c_k;</trd>
  <trd lng="es">yema</trd>
  <trd lng="en">yolk</trd>
  <trd lng="fr">jaune d'&oelig;uf</trd>
  <trd lng="de">Eigelb</trd>
  <trd lng="hu">toj&aacute;ss&aacute;rg&aacute;ja</trd>
  <trd lng="nl">eigeel</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_t;&c_o;&c_k;</trd>
  <trd lng="sv">&auml;ggula</trd>
  <trd lng="tr">yumurta sar&inodot;s&inodot;</trd>
</drv>

<drv mrk="flav.F0aRivero">
  <kap><tld lit="F"/>a Rivero</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Granda rivero (4845 km) de la
      <ref tip="malprt" cel="azi.OrientaA0o.GEOG">Orienta Azio</ref>,
      en la Norda &Ccirc;inujo, fluas de Tibeto en
      la <ref cel="flav.F0aMaro">Flavan Maron</ref>:
      <ekz>
        La <tld lit="F"/>a Rivero estas la dua grand rivero de
        &Ccirc;inio<fnt><bib>EncCxi</bib> <lok>&ccirc;ap. &leftquot;La Flava Rivero&rightquot;, p.
        65a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Huang He</trd>,
    <trd>Gelber Fluss</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Huang He</trd>,
    <trd>Yellow River</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Huang He</trd>,
    <trd>Fleuve jaune</trd>
  </trdgrp>
  <trd lng="hu">S&aacute;rga-foly&oacute;</trd>
  <trdgrp lng="nl">
    <trd>Huang He</trd>,
    <trd>Gele Rivier</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_H;&c_u;&c_a;&c_n;&c_h;&c_je;</trd>,
    <trd>&c_Zh;&c_jo;&c_l;&c_t;&c_a;&c_ja; &c_r;&c_je;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="flav.F0aMaro">
  <kap><tld lit="F"/>a Maro</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="malprt" cel="pacifik.0o">Pacifika</ref> maro
      inter la kontinenta &Ccirc;inujo kaj la Korea duoninsulo:
      <ekz>
        Oriente de Bohai-markolo estas la <tld lit="F"/>a
        Maro
        <fnt><bib>EncCxi</bib> <lok>&ccirc;ap. &leftquot;Maroj kaj Marbordo&rightquot;, p.
        34a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Gelbes Meer</trd>
  <trd lng="en">Yellow Sea</trd>
  <trdgrp lng="fr">
    <trd>Mer jaune</trd>
  </trdgrp>
  <trd lng="hu">S&aacute;rga-tenger</trd>
  <trd lng="nl">Gele Zee</trd>
  <trdgrp lng="ru">
    <trd>&c_Zh;&c_jo;&c_l;&c_t;&c_o;&c_je; &c_m;&c_o;&c_r;&c_je;</trd>,
    <trd>&c_H;&c_u;&c_a;&c_n;&c_h;&c_a;&c_j;</trd>
  </trdgrp>
</drv>
</art>
