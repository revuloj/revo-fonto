<drv mrk="flav.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="flav.0ulo.flavrasulo">
    <dif>
      Homo el <tld/>a raso.
    </dif>
    <trd lng="be">&c_zh;&c_a;&c_w;&c_t;&c_a;&c_s;&c_k;&c_u;&c_r;&c_y;</trd>
    <trd lng="fr">jaune<klr tip="ind"> (asiatique)</klr></trd>
    <trd lng="en">yellow man</trd>
    <trd lng="hu">s&aacute;rgab&odblac;r&udblac;<klr tip="ind">
    (ember)</klr></trd>
    <trd lng="nl">geelhuid</trd>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;
      <ind>&c_zh;&c_jo;&c_l;&c_t;&c_o;&c_j;</ind> &c_r;&c_a;&c_s;&c_y;</trd>,
      <trd>&c_a;&c_z;&c_i;&c_a;&c_t;</trd>
    </trdgrp>
  </snc>
  <snc mrk="flav.0ulo.laboristo">
    <dif>
      Laboristo kontra&ubreve;a al la klasbatala politiko.
    </dif>
    <trd lng="fr">jaune<klr tip="ind"> (briseur de gr&egrave;ve)</klr></trd>
    <trd lng="ru">&c_sh;&c_t;&c_r;&c_je;&c_j;&c_k;&c_b;&c_r;&c_je;&c_h;&c_je;&c_r;</trd>
  </snc>
  <trd lng="cs">&ccaron;lov&ecaron;k &zcaron;lut&eacute; pleti</trd>
  <trd lng="sk">&ccaron;lovek &zcaron;ltej pleti</trd>
</drv>

<drv mrk="flav.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc mrk="flav.0igxo.KOMUNE">
    <dif>
      La procezo i&gcirc;i <tld/>a a&ubreve; ties rezulto:
      <ekz>
        la Eternulo frapos vin per ftizo kaj per febro kaj per
        malsaneca varmego kaj per brulumo kaj per sekeco kaj per
        bruliga aero kaj per <tld/>i&gcirc;o de la kreska&jcirc;oj
        <fnt><bib>MT</bib>, <lok>&Rea; 28:22</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_p;&c_o;&c_zh;&c_je;&c_l;&c_t;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="flav.0igxo.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Simptomo de pluraj malsanoj (kaj speciale de la
      <ref cel="san.flavmal0o"><tld/>malsano</ref>),
      konsistanta en tio ke la ha&ubreve;to la okula
      konjunktivo i&gcirc;as <tld/>aj.
    </dif>
    <trd lng="en">jaundice</trd>
    <trdgrp lng="fr">
      <trd>ict&egrave;re</trd>, <trd>jaunisse</trd>
    </trdgrp>
    <trd lng="id"><ind>sakit</ind> kuning</trd>
    <trd lng="ru">&c_zh;&c_je;&c_l;&c_t;&c_u;&c_h;&c_a;<klr>
    (&c_s;&c_i;&c_m;&c_p;&c_t;&c_o;&c_m;)</klr></trd>
  </snc>
</drv>


<drv mrk="flav.ov0o">
  <kap>ovo<tld/>o, <var><kap>ovo<tld/>a&jcirc;o</kap></var></kap>
  <snc mrk="flav.ov0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="F"/>a ena parto de <ref tip="malprt" cel="ov.0o.KUI">ovo</ref>, 
      el kiu &ccirc;e fekundigita ovo fari&gcirc;as la birdido:
      <ekz>
        pip, pip! oni a&ubreve;dis, &ccirc;iuj ovo<tld/>oj vivi&gcirc;is kaj el&scirc;ovis la
        kapon
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        zabajono estas franda&jcirc;o el sukero, ovo<tld/>o, vanilo kaj
        likvoro, kuirbanita
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=49816">Zabajono</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        bearnezo estas sa&ubreve;co farita el batmiksita butero kaj
        ovo<tld/>o kun aldono de vino, vinagro, drakunkolo kaj
        askalonoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=80660">Bearnezo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="blank.ov0o">ovoblanko</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_w;&c_t;&c_o;&c_k;</trd>
  <trdgrp lng="de">
    <trd>Eigelb</trd>,
    <trd>Dotter</trd>
  </trdgrp>
  <trd lng="es">yema</trd>
  <trd lng="en">yolk</trd>
  <trd lng="fr">jaune d'&oelig;uf</trd>
  <trd lng="hu">toj&aacute;ss&aacute;rg&aacute;ja</trd>
  <trdgrp lng="id">
    <trd><ind>kuning</ind> telur</trd>,
    <trd>yolk</trd>
  </trdgrp>
  <trd lng="nl">eigeel</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_t;&c_o;&c_k;</trd>
  <trd lng="sv">&auml;ggula</trd>
  <trd lng="tr">yumurta sar&inodot;s&inodot;</trd>
</drv>

