  <snc mrk="flav.0o.KOL">
    <dif>
      <tld lit="F"/>a <ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">koloro</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_w;&c_c;&c_ib;&c_z;&c_n;&c_a;</trd>
  <trd lng="bg">&c_zh;&c_malmol;&c_l;&c_t;&c_o;</trd>
