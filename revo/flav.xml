<drv mrk="flav.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="flav.0a.KOL">
    <dif>
      Samkolora kiel interna bulo de kokinovo, citrono, oro, matura
      tritiko:
      <ekz>
        <tld/>aj akvolilioj
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a punkto en la lekanto brilis kiel
        oro kaj la malgrandaj folioj &ccirc;irka&ubreve;e
        kiel ar&gcirc;ento
        <fnt>
          <bib>Fab1</bib>
          <lok>Lekanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        podio el verda, blanka, <tld/>a, kaj nigra marmoro.
        <fnt>
          <bib>MT</bib>
          <lok>Ester 1:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen venis a&ubreve;tuno, la folioj en la arbaro
        fari&gcirc;is <tld/>aj, kaj brunaj
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ligas &ccirc;irka&ubreve; la kolon la safrane <tld/>an tukon
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia viza&gcirc;o fari&gcirc;is <tld/>a kaj liaj genuoj tremis
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la re&gcirc;ino ektimis kaj fari&gcirc;is <tld/>a kaj verda pro envio
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la neimageblaj ebloj de la <tld/>a florpolvo
        <fnt><aut>R. Roze</aut>: <vrk>Vivisekcio de polenero</vrk>,
        <bib>Monato</bib>
        <lok>jaro 1997a, numero 11a, p. 12a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
