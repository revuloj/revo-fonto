<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: latent.xml,v 1.22 2022/04/08 08:37:01 revo Exp $">
<kap>
  <rad>latent</rad>/a
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="latent.0a">
  <kap><tld/>a</kap>
  <snc mrk="latent.0a.FIL">
    <dif>
      Ne<ref tip="ant" cel="vid.0ebla">videbla</ref>, 
      sed ekzistanta kaj aperigebla:
      <ekz>
        milito <tld/>e ekzistas &ccirc;iam inter ekspluatantoj kaj ekspluatatoj
        <fnt>
          <bib>Lanti</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la Esperanto-Movado ekzistas multaj <tld/>aj fortoj, kiuj certe manifesti&gcirc;os
        kiam UEA komencos efektive gvidi la Movadon
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        li multe legis kaj studis tiutempe pri la maniero influi la homojn, a&ubreve; ekspluati
        iliajn <tld/>a&jcirc;ojn
        <fnt>
          <bib>ChC</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni taksas <klr>[ilin]</klr> <tld/>aj perfiduloj
        <fnt>
          <bib>CKv</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj ricevis longan religian predikon pri tio, ke <klr>[ili]</klr> estas
        <tld/>aj krimuloj, kaj devas pre&gcirc;i Dion por superi tiun mismoralan inklinon
        <fnt>
          <bib>ChR</bib>
        </fnt>;
      </ekz>
      <ekz>
        homo eble pro sia anta&ubreve;a sorto estas <tld/>a esperantisto 
        &gcirc;is la momento, kiam la lingvo internacia kaptas lian atenton
        <fnt>
          <aut>Stefan Maul</aut>,
          <vrk>El verva vivo &jcirc;urnalista</vrk>,
          <lok>p. 7</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>latent</trd>,
      <trd>verborgen</trd>,
      <trd>potentiell</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>latent</trd>,
      <trd>hidden</trd>,
      <trd>dormant</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>lappang&oacute;</trd>,
      <trd>rejtett</trd>,
      <trd>latens</trd>
    </trdgrp>
  </snc>
  <snc mrk="latent.0a.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Nevidebla, sensimptoma:
      <ekz>
        <tld/>aj tra&ubreve;moj
        <fnt>
          <bib>SatirRak</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>ruhend</trd>
    </trdgrp>
  </snc>
  <refgrp tip="vid">
    <ref cel="dorm.0i.FIG">dormi<sncref/></ref>,
    <ref cel="kasx.0i">ka&scirc;i</ref>,
    <ref cel="kov.0i.FIG_konservi">kovi<sncref/></ref>,
    <ref cel="kovr.0i.kasxi">kovri<sncref/></ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_l;&c_ja;&c_t;&c_e;&c_n;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_h;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_b;&c_a;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>latentn&iacute;</trd>,
    <trd>skryt&yacute;</trd>
  </trdgrp>
  <trd lng="el">&lambda;&alfa;&ny;&theta;&alfa_ton;&ny;&omega;&ny;</trd>
  <trd lng="es">latente</trd>
  <trd lng="fr">latent</trd>
  <trd lng="nl">latent</trd>
  <trdgrp lng="pl">
    <trd>ukryty</trd>,
    <trd>utajony</trd>
  </trdgrp> 
  <trd lng="pt">latente</trd>
  <trdgrp lng="ro">
    <trd>ascuns</trd>,
    <trd>latent</trd>
  </trdgrp>  
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_t;&c_je;&c_n;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_y;&c_t;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>latentn&yacute;</trd>,
    <trd>skryt&yacute;</trd>,
    <trd>utajen&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&c_i;&c_h;&#x43E;&c_v;&c_a;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43B;&c_a;&c_t;&c_je;&#x43D;&c_t;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<!-- mankas cita&jcirc;oj, &ccirc;u fakte uzata?
<drv mrk="latent.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>a.
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>lappang</trd>,
    <trd>rejtve marad</trd>,
    <trd>l&aacute;thatatlan</trd>
  </trdgrp>
  <trd lng="nl"><ind>latent</ind> aanwezig zijn</trd>
  <trdgrp lng="pl">
    <trd>by&cacute; utajonym</trd>,
    <trd>nie dawa&cacute; objaw&oacute;w</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>a fi secret</trd>,
    <trd>nu da simptome</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>by&tcaron; v&nbsp;inkuba&ccaron;nom &scaron;t&aacute;diu</trd>,
    <trd>skr&yacute;va&tcaron; sa</trd>,
    <trd>taji&tcaron; sa</trd>
  </trdgrp>
</drv>
-->

<drv mrk="latent.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Nevidebla <ref tip="super" cel="kapabl.0o">kapablo</ref> a&ubreve; eco, kiu povos aperi:
      <ekz>
        lia lingvo plene eluzas la vortludajn <tld/>ojn de Esperanto, konsistantajn el
        dusencaj esprima&jcirc;oj, komikaj pse&ubreve;do-sufiksado kaj sinonimado
        <fnt>
          <bib>EeP</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="talent.0o.kapablo">talento<sncref/></ref>,
      <ref cel="pov.0o.kapablo">povo<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_t;&c_e;&c_n;&c_t;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="cs">latentnost</trd>
  <trdgrp lng="de">
    <trd>Potential</trd>,
    <trd>versteckte <ind>M&ouml;glichkeit</ind></trd>
  </trdgrp>
  <trd lng="fr">latence</trd>
  <trd lng="nl"><ind>latent</ind>e eigenschap</trd>
  <trdgrp lng="pl">
    <trd>tajno&sacute;&cacute; <klr>(med)</klr></trd>,
    <trd>ukryto&sacute;&cacute; <klr>(med)</klr></trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>secretul <klr>(med)</klr></trd>,
    <trd>covertness <klr>(med)</klr></trd>
  </trdgrp>  
  <trd lng="sk">latentnos&tcaron;</trd>
</drv>
</art>
