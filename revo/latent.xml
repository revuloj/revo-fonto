<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>latent</rad>/a
</kap>
<drv mrk="latent.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Nevidebla, sed ekzistanta kaj aperigebla:
      <ekz>
        <tld/>a ideo, kontrasto, inklino;
      </ekz>
      <ekz>
        <tld/>a militostato;
      </ekz>
      <ekz>
        <tld/>a forto de elektra motoro.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>latent</trd>,
      <trd>hidden</trd>,
      <trd>dormant</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">MED</uzo>
    <dif>
      Nevidebla, sensimptoma:
      <ekz>
        <tld/>a malsano.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">latente</trd>
  <trd lng="nl">latent</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_t;&c_je;&c_n;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_y;&c_t;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="latent.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>a.
    </dif>
  </snc>
</drv>
</art>
