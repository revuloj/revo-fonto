<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: migr.xml,v 1.20 2016/09/10 07:10:13 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>migr</rad>/i
</kap>
<drv mrk="migr.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Transloki&gcirc;i vole en alian restadejon:
      <ekz>
        <tld/>anta popolo, birdo<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>anta bando da haringoj;
      </ekz>
      <ekz>
        la printempa <ind><tld/>ado</ind> de la brutaroj al Alpaj
        pa&scirc;tejoj;
        <trd lng="ca">migraci&oacute;</trd>
      </ekz>
      <ekz>
        la internacia oficejo studas la <ind><tld/>adan problemo</ind>n.
        <trd lng="hu">migr&aacute;ci&oacute;s probl&eacute;ma</trd>       
      </ekz>
    </dif>
    <trd lng="pl">migrowa&cacute;</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_je;&c_r;&c_je;&c_s;&c_je;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_m;&c_i;&c_g;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Transloki&gcirc;i de loko al loko nehavante fiksan sidejon:
      <ekz>
        <tld/>anta aktoro, komercisto, laboristo, predikanto, kavaliro;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>anto <ind>(senhejmulo)</ind> mi estas sur la
        tero<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="vag.0i">vagi</ref>.
    </refgrp>
    <trd lng="pl">wa&lstroke;&eogonek;sa&cacute; si&eogonek;</trd>
    <trd
    lng="ru">&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Iri de iu loko al alia malproksima:
      <ekz>
        longe <tld/>i tra la mondo;
      </ekz>
      <ekz>
        <tld/>i pilgrimante al Romo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        rigardi la stelojn <tld/>antajn sur la &ccirc;ielo<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la fenika alfabeto <tld/>is en Grekujon kaj poste en Italujon
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="vojagx.0i">voja&gcirc;i</ref>.
    </refgrp>
    <rim>
      Oni ne uzu <em>migri</em> samsence kiel <em>promeni</em>.
    </rim>
    <trd lng="pl">w&eogonek;drowa&cacute;</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_m;&c_i;&c_g;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_n;&c_d;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_ib;&c_g;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_s;&c_ja;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>divroa&ntilde;<klr> [homoj]</klr></trd>,
    <trd>tremen-distremen<klr> [birdoj]</klr></trd>
  </trdgrp>
  <trd lng="ca">migrar</trd>
  <trdgrp lng="cs">
    <trd>migrovat</trd>,
    <trd>putovat</trd>,
    <trd>st&ecaron;hovat se <klr>(o pt&aacute;c&iacute;ch, n&aacute;rodech)</klr></trd>
  </trdgrp>
  <trd lng="de">wandern</trd>
  <trd lng="en">migrate</trd>
  <trd lng="es">migrar</trd>
  <trd lng="fr">migrer</trd>
  <trdgrp lng="hu">
    <trd>v&aacute;ndorol</trd>,
    <trd>k&ouml;lt&ouml;zik</trd>,
    <trd>migr&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>rondtrekken</trd>,
    <trd>zwerven</trd>
  </trdgrp>
  <trd lng="pt">migrar</trd>
  <trdgrp lng="sk">
    <trd>putova&tcaron;</trd>,
    <trd>t&uacute;la&tcaron; sa</trd>
  </trdgrp>
</drv>

<drv mrk="migr.en0i">
  <kap>en<tld/>i</kap>
  <snc>
    <dif>
      Translo&gcirc;i&gcirc;i en fremdan landon.
    </dif>
  </snc>
  <trd
  lng="be">&c_ib;&c_m;&c_ib;&c_g;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="br">enbroa&ntilde;</trd>
  <trd lng="ca">immigrar</trd>
  <trd lng="cs">imigrovat</trd>
  <trd lng="de">einwandern</trd>
  <trd lng="es">inmigrar</trd>
  <trd lng="fr">immigrer</trd>
  <trd lng="hu">bev&aacute;ndorol</trd>
  <trd lng="nl">immigreren</trd>
  <trd lng="pl">imigrowa&cacute;</trd>
  <trd
  lng="ru">&c_i;&c_m;&c_m;&c_i;&c_g;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">pris&tcaron;ahova&tcaron; sa</trd>
</drv>

<drv mrk="migr.el0i">
  <kap>el<tld/>i</kap>
  <snc>
    <dif>
      Forlasi patrujon por translo&gcirc;i&gcirc;i en fremdan landon.
    </dif>
  </snc>
  <trd
  lng="be">&c_e;&c_m;&c_ib;&c_g;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="br">divroa&ntilde;</trd>
  <trd lng="ca">emigrar</trd>
  <trdgrp lng="cs">
    <trd>emigrovat</trd>,
    <trd>vyst&ecaron;hovat se</trd>
  </trdgrp>
  <trd lng="de">auswandern</trd>
  <trd lng="es">emigrar</trd>
  <trd lng="fr">&eacute;migrer</trd>
  <trdgrp lng="hu">
    <trd>kiv&aacute;ndorol</trd>,
    <trd>emigr&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">emigreren</trd>
  <trdgrp lng="pl">
    <trd>emigrowa&cacute;</trd>,
    <trd>wyemigrowa&cacute;</trd>
  </trdgrp>
  <trd
  lng="ru">&c_e;&c_m;&c_i;&c_g;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">emigrova&tcaron;</trd>
</drv>

<drv mrk="migr.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Ne havanta konstantan lo&gcirc;lokon.
    </dif>
    <ref tip="sin" cel="nomad.0a">nomada</ref>
    <trdgrp lng="ca">
      <trd>migratori</trd>,
      <trd>errant</trd>,
      <trd>ambulant</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>n&oacute;mada</trd>,
      <trd>migratorio</trd>
    </trdgrp>
    <trd lng="fr">migratoire</trd> 
    <trdgrp lng="pl">
      <trd>migracyjny</trd>,
      <trd>migruj&aogonek;cy</trd>,
      <trd>w&eogonek;drowny</trd>,
      <trd>koczowniczy</trd>
    </trdgrp>
  </snc>
</drv>

</art>
