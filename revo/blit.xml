<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: blit.xml,v 1.9 2019/03/08 15:10:15 revo Exp $">
<kap>
<rad>blit</rad>/o
</kap>
<drv mrk="blit.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specioj de <ref tip="super" cel="kenopodi.0o">kenopodio</ref>
      (<trd lng="la">Chenopodium foliosum</trd>,
       <trd lng="la">Chenopodium capitatum</trd>), 
      kies floroj formas ru&gcirc;ajn fragsimilajn bulojn
      (fragspinaco):
      <ekz>
        la Maldika <tld/>o estas nutra&jcirc;planto de ra&ubreve;poj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=556787"
              >Maldika blito</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/c/cd/Chenopodium_foliosum.JPG/360px-Chenopodium_foliosum.JPG"
      prm="PD"
      >Maldika <tld/>o (Chenopodium foliosum)
        <fnt>
          <aut>WildBoar, propra verko</aut>
          <lok><url ref="&WCW;/File:Chenopodium_foliosum.JPG"
              >https://commons.wikimedia.org/wiki/File:Chenopodium_foliosum.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="de">Erdbeerspinat</trd>
  <trd lng="es">acelga</trd>
  <trdgrp lng="hu">
     <trd>libatop</trd>,
     <trd>libapar&eacute;j</trd>
   </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_m;&c_i;&c_n;&c_d;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_mol;
&c_g;&c_o;&c_l;&c_o;&c_v;&c_ch;&c_a;&c_t;&c_a;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sv">
     <trd>b&auml;rm&aring;lla</trd>,
     <trd>smultronm&aring;lla</trd>
   </trdgrp>
</drv>
</art>
