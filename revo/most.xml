<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: most.xml,v 1.25 2025/12/04 16:44:11 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>most</rad>/o
<fnt><bib>UV</bib></fnt></kap>

<drv mrk="most.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc>
   <uzo tip="fak">KUI</uzo>
     <dif>
       <ref tip="super" cel="suk.0o.KUI">Suko</ref> elpremita
       el <ref cel="ber.vin0o">vinbero</ref>j (a&ubreve; pomoj k.a.), ne
       a&ubreve; nur mallonge fermentinta:
       <ekz>
        viaj vinpremejoj superbordigos <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 3:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        de <tld/>o ili ebrii&gcirc;os
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 49:26</lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_m;&c_u;&c_s;&c_t;</trd>
  <trdgrp lng="bg">
     <trd>&c_m;&c_malmol;&c_s;&c_t;</trd>,
     <trd>&c_sh;&c_i;&c_r;&c_a;</trd>
   </trdgrp>
  <trd lng="ca">most</trd>
  <trd lng="cs">mo&scaron;t</trd>
  <trd lng="de">Most</trd>
  <trdgrp lng="en">
     <trd>must</trd>,
     <trd>new <ind>wine</ind></trd>,
     <trd>unfermented <ind>grape juice</ind></trd>
   </trdgrp>
  <trd lng="es">mosto</trd>
  <trd lng="fa">&a_sh;&a_r;&a_A;&a_b; &a_n;&a_A;&a_r;&a_s;</trd>
  <trd lng="fr">mo&ucirc;t</trd>
  <trd lng="hu">must</trd>
  <trd lng="ja">&#x30D6;&#x30C9;&#x30A6;&#x6DB2;  <pr>&#x3076;&#x3069;&#x3046;&#x3048;&#x304D;</pr></trd>
  <trd lng="nl">most</trd>
  <trd lng="pl">moszcz</trd>
  <trd lng="pt">mosto (&ocirc;)</trd>
  <trdgrp lng="ru">
     <trd>&c_m;&c_u;&c_s;&c_t;</trd>,
     <trd>&c_v;&c_i;&c_n;&c_o;&c_g;&c_r;&c_a;&c_d;&c_n;&c_y;&c_j;
&c_s;&c_o;&c_k;</trd>
   </trdgrp>
  <trd lng="sk">mu&scaron;t</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_u;&c_s;&c_t;</trd>,
    <trd>&c_v;&c_i;&c_n;&c_o;&c_g;&c_r;&c_a;&c_d;&c_n;&c_i;&c_j; &c_s;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x603B;&#x5F97; <pr>z&#x1D2;ngd&ecaron;i</pr></trd>,
    <trd>&#x7E3D;&#x5F97; <pr>z&#x1D2;ngd&ecaron;i</pr></trd>
  </trdgrp>
</drv>
</art>
