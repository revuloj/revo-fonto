<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>most</rad>/o
</kap>

<drv mrk="most.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc>
     <dif>
       Suko elpremita el <ref cel="ber.vin0o">vinbero</ref>j, ne 
ankora&ubreve;
       fermentinta.
     </dif>
   </snc>
   <trd lng="ca">most</trd>
   <trd lng="de">Most</trd>
   <trdgrp lng="en">
     <trd>must</trd>,
     <trd>new <ind>wine</ind></trd>,
     <trd>unfermented <ind>grape juice</ind></trd>
   </trdgrp>
   <trd lng="es">mosto</trd>
   <trd lng="hu">must</trd>
   <trd lng="fa">&a_sh;&a_r;&a_A;&a_b; &a_n;&a_A;&a_r;&a_s;</trd>
   <trd lng="fr">mo&ucirc;t</trd>
   <trd lng="nl">most</trd>
   <trd lng="pt">mosto (&ocirc;)</trd>
   <trdgrp lng="ru">
     <trd>&c_m;&c_u;&c_s;&c_t;</trd>,
     <trd>&c_v;&c_i;&c_n;&c_o;&c_g;&c_r;&c_a;&c_d;&c_n;&c_y;&c_j; 
&c_s;&c_o;&c_k;</trd>
   </trdgrp>
   <trd lng="sk">mu&scaron;t</trd>
</drv>
</art>
