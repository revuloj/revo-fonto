     <trd>must</trd>,
     <trd>new <ind>wine</ind></trd>,
     <trd>unfermented <ind>grape juice</ind></trd>
   </trdgrp>
   <trd lng="es">mosto</trd>
   <trd lng="hu">must</trd>
   <trd lng="fr">mo&ucirc;t</trd>
   <trd lng="nl">most</trd>
   <trdgrp lng="ru">
     <trd>&c_m;&c_u;&c_s;&c_t;</trd>,
     <trd>&c_v;&c_i;&c_n;&c_o;&c_g;&c_r;&c_a;&c_d;&c_n;&c_y;&c_j; 
&c_s;&c_o;&c_k;</trd>
   </trdgrp>
   <trd lng="sk">mu&scaron;t</trd>
</drv>
</art>
