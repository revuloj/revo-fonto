   <kap><ofc>*</ofc><tld/>o</kap>
   <snc>
     <dif>
       Suko elpremita el <ref cel="ber.vin0o">vinbero</ref>j, ne 
ankora&ubreve;
       fermentinta.
     </dif>
   </snc>
   <trd lng="ca">most</trd>
