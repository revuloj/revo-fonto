<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>most</rad>/o
</kap>
<drv mrk="most.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Suko elpremita el <ref cel="ber.vin0o">vinbero</ref>j, ne ankora&ubreve;
      fermentinta.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>must</trd>,
    <trd>new <ind>wine</ind></trd>,
    <trd>unfermented <ind>grape juice</ind></trd>
  </trdgrp>
  <trd lng="nl">most</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_u;&c_s;&c_t;</trd>,
    <trd>&c_v;&c_i;&c_n;&c_o;&c_g;&c_r;&c_a;&c_d;&c_n;&c_y;&c_j; &c_s;&c_o;&c_k;</trd>
  </trdgrp>
</drv>
</art>
