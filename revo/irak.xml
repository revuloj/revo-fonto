<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: irak.xml,v 1.24 2023/11/19 10:22:14 revo Exp $">
<kap>
  <ofc>8</ofc><rad>Irak</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="irak.I0o">
  <kap><ofc>8</ofc><tld/>o
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url ref="&AdE;/aktoj/aktoj2/8oa.html"> 8a
Oficiala Aldono</url></vrk>, <lok>II.C.1</lok></fnt>
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url ref="&AdE;/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>
  </kap>
  <snc mrk="irak.I0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Araba nomo de la malsupra
      <ref cel="mezopotami.M0o">Mezopotamio</ref>:
      <ekz>
        <tld/>o formis la kernon de la abasida &hcirc;alifujo;
      </ekz>
      <ekz>
        la a&ubreve;toroj kristanaj ofte nomas la mezepokan <tld/>on
        &leftquot;<ref tip="sin" cel="babiloni.B0o">Babilonio</ref>&rightquot;;
      </ekz>
      <ekz>
        en la 19a jc &leftquot;<tld/>o&rightquot; estis nomo de du
        provincoj en la
        regnoj Turka kaj Persa.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>Iraq</trd>,
      <trd>Baixa Mesopot&agrave;mia</trd>
    </trdgrp>
    <trd lng="en">Al-Iraq</trd>
    <trdgrp lng="id">
      <trd>Babilonia</trd>,
      <trd>Mesopotamia</trd>
    </trdgrp>
    <trd lng="pl">Babilonia</trd>
  </snc>
  <snc mrk="irak.I0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      &Scirc;tato en la
      <ref tip="malprt" cel="azi.OkcidentaA0o">Okcidenta Azio</ref>,
      kies &ccirc;efa parto estas <ref cel="mezopotami.M0o">Mezopotamio</ref>,
      kaj la &ccirc;efurbo, <ref tip="prt" cel="bagdad.B0o">Bagdado</ref>:
      <ekz>
        nur la brita &ccirc;efministro deklaras sin preta ekmiliti kun
        Bush kontra&ubreve; <tld/>o kaj Sadam Husajn
        <fnt>
        <aut>P. Gubbins</aut>:
        <vrk>Ekfluas la ru&gcirc;a akvo</vrk>, <lok>Monato</lok>.
        </fnt>
      </ekz>
    </dif>
    <trd lng="en">Iraq</trd>
    <trd lng="pl">Irak</trd>
    <trd lng="pt">Iraque</trd>
  </snc>
  <trd lng="be">&c_Ib;&c_r;&c_a;&c_k;</trd>
  <trdgrp lng="ca">
    <trd>l'Iraq</trd>,
    <trd>Rep&uacute;blica de l'Iraq</trd>
  </trdgrp>
  <trd lng="cs">Ir&aacute;k</trd>
  <trd lng="de">Irak</trd>
  <trd lng="es">Iraq</trd>
  <trd lng="fa">&a_ayn;&a_r;&a_A;&a_q;</trd>
  <trdgrp lng="fr">
    <trd>Irak</trd>,
    <trd>Iraq</trd>
  </trdgrp>
  <trd lng="hu">Irak</trd>
  <trd lng="id">Irak</trd>
  <trd lng="ja">&#x30A4;&#x30E9;&#x30AF;  <pr>&#x3044;&#x3089;&#x304F;</pr></trd>
  <trd lng="ru">&c_I;&c_r;&c_a;&c_k;</trd>
  <trd lng="sk">Irak</trd>
  <trd lng="uk">&c_Ib;&c_r;&c_a;&#x43A;</trd>
  <trdgrp lng="zh">
    <trd>&#x4F0A;&#x62C9;&#x514B; <pr>Y&#x12B;l&#x101;k&#xE8;</pr></trd>,
    <trd>&#x4F0A;&#x62C9;&#x514B; <pr>y&#x12B;l&#x101;k&#xE8;</pr></trd>,
    <trd>&#x4F0A; <pr>y&#x12B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="irak.0ano">
  <kap><tld lit="i"/>ano</kap>
  <snc>
    <dif>
    &Scirc;tatano de Irako
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_r;&c_a;&c_k;&c_je;&c_c;</trd>
  <trdgrp lng="fr"> 
    <trd>Irakien<klr> (subst.)</klr></trd>
    <trd>Irakienne<klr> (subst.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="irak.0a">
  <kap><tld lit="i"/>a</kap>
  <snc>
    <dif>
    Rilata al la &scirc;tato Irako.
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_r;&c_a;&c_c;&c_k;&c_ib;</trd>
  <trdgrp lng="fr"> 
    <trd>irakien<klr> (adj.)</klr></trd>
    <trd>irakienne<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>

</art>
