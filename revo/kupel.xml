<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kupel.xml,v 1.7 2023/12/04 19:21:04 revo Exp $">
<kap>
  <rad>kupel</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kupel.0i">
  <kap><tld/>i</kap>
  <snc mrk="kupel.0i.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Apartigi noblan metalon (oron, ar&gcirc;enton) disde aliaj
      kunmiksitaj metaloj per varmigo, kiu oksidigas tiujn &ccirc;i
      lastajn:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;i estis el <tld/>ita oro
        <fnt>
          <aut>V. Catal&agrave;, trad. J. Ventura i Freixas</aut>,
          <vrk>Soleco</vrk>,
          <lok>1967</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frakci.0igi">frakciigi</ref>,
      <ref cel="rafin.0i">rafini</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_p;&c_e;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">kupelovat</trd>
  <trdgrp lng="de">
    <trd>kupellieren</trd>,
    <trd>abtreiben <klr>(Edelmetall)</klr></trd>,
    <trd>l&auml;utern <klr>(Edelmetall)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>coupeller</trd>,
    <trd>pratiquer la <ind>coupellation</ind></trd>
  </trdgrp>
  <trd lng="pl">kupelowa&cacute;</trd>
  <trd lng="sk">kupelova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_u;&c_p;&c_je;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_k;&c_a;&c_p;&c_je;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_ch;&c_i;&c_shch;&c_a;&c_t;&c_i; (&c_m;&c_je;&c_t;&c_a;&c_l;&c_i;)</trd>
  </trdgrp>
</drv>
</art>

