<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>anestez</rad>/i
</kap>
<drv mrk="aneste.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Senigi iun je la sentokapablo helpe de specialaj substancoj.
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="narkot.0i">narkoti</ref>
  </refgrp>
  <trd lng="fr">anesth&eacute;sier</trd>
  <trd lng="nl">verdoven</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_je;&c_z;&c_b;&c_o;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_a;&c_n;&c_je;&c_s;&c_t;&c_je;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="aneste.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Pli a&ubreve; malpli kompleta senigo je la sentokapablo, ka&ubreve;zita
      de specialaj substancoj a&ubreve; de malsano.
    </dif>
  </snc>
  <trd lng="fr">anesth&eacute;sie</trd>
  <trd lng="nl">verdoving</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_je;&c_s;&c_t;&c_je;&c_z;&c_i;&c_ja;</trd>,
    <trd>&c_o;&c_b;&c_je;&c_z;&c_b;&c_o;&c_l;&c_i;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_n;&c_a;&c_r;&c_k;&c_o;&c_z;</trd>
  </trdgrp>
</drv>

<drv mrk="aneste.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Substanco por anestezi:
      <ekz>
        kloroformo.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">anesth&eacute;sique</trd>
  <trd lng="nl">verdovingsmiddel</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_je;&c_s;&c_t;&c_je;&c_z;&c_i;&c_r;&c_u;&c_ju;&c_shch;&c_je;&c_je; &c_s;&c_r;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_o;&c_b;&c_je;&c_z;&c_b;&c_o;&c_l;&c_i;&c_v;&c_a;&c_ju;&c_shch;&c_je;&c_je; &c_s;&c_r;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_n;&c_a;&c_r;&c_k;&c_o;&c_z;</trd>
  </trdgrp>
</drv>
</art>
