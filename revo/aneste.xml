<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aneste.xml,v 1.25 2022/03/26 07:27:44 revo Exp $">
<kap>
  <rad>anestez</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="aneste.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="aneste.0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Senigi iun je la sentokapablo helpe de specialaj substancoj:
      <ekz>
        la doloroj obtuzi&gcirc;is, kaj la
        <tld/>ita karno jam ne konstatis e&ccirc; la
        premon de la subtenaj rokoj
        <fnt>
          <aut>J. Francis</aut>,
          <vrk><url ref="http://autodidactproject.org/other/kultent_francis.html">
          La Tento de Kul</url></vrk>,
          <lok>Belarto, 1958-04 (1), p. 11a-15a</lok>
        </fnt>.
      </ekz>      
    </dif>
  </snc>
  <refgrp tip="vid">

    <ref cel="narkot.0i">narkoti</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_a;&c_n;&c_e;&c_s;&c_t;&c_e;&c_z;&c_ib;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_ja;&c_z;&c_b;&c_o;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>bet&auml;uben</trd>,
    <trd>an&auml;sthesieren</trd>
  </trdgrp>
  <trd lng="en">anesthetize</trd>
  <trd lng="es">anestesiar</trd>
  <trd lng="fr">anesth&eacute;sier</trd>
  <trd lng="he">&lamed;&he;&resh;&dalet;&yod;&fmem;</trd>
  <trd lng="hu">&eacute;rz&eacute;stelen&iacute;t</trd>  
  <trd lng="nl">verdoven</trd>
  <trdgrp lng="pl">
    <trd>znieczula&cacute;</trd>,
    <trd>robi&cacute; <ind>narkoz&eogonek;</ind></trd>
  </trdgrp>
  <trd lng="pt">anestesiar</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_je;&c_z;&c_b;&c_o;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_a;&c_n;&c_je;&c_s;&c_t;&c_je;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">znecitlivi&tcaron;</trd>
  <trd lng="cs">znecitlivit</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_n;&c_je;&c_b;&c_o;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_a;&c_n;&c_je;&c_s;&c_t;&c_je;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_b;&c_i;&c_t;&c_i; &c_n;&c_je;&c_ch;&c_u;&c_t;&c_t;&c_jeu;&c_v;&c_i;&c_m;</trd>
  </trdgrp>
</drv>

<drv mrk="aneste.0o">
  <kap><tld/>o</kap>
  <snc mrk="aneste.0o.MED">
    <dif>
      Pli a&ubreve; malpli kompleta senigo je la sentokapablo,
      ka&ubreve;zita de specialaj substancoj a&ubreve; de malsano:
      <ekz>
        ne timu, post <tld/>o mi facile aku&scirc;igos vin
        <fnt>
          <bib>KrB</bib>
          <lok>E&ccirc; inter piuloj ne mankas pekuloj</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="analgezi.0o">analgezio</ref>,
      <ref cel="dolor.sen0eco.MED">sendoloreco</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_n;&c_e;&c_s;&c_t;&c_e;&c_z;&c_ib;&c_ja;</trd>,
    <trd>&c_a;&c_b;&c_ja;&c_z;&c_b;&c_o;&c_l;&c_mol;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_b;&c_o;&c_l;&c_je;&c_p;&c_a;&c_t;&c_a;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_n;&c_a;&c_r;&c_k;&c_o;&c_z;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>anest&eacute;zie</trd>,
    <trd>znecitliv&ecaron;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bet&auml;ubung</trd>,
    <trd>An&auml;sthesie</trd>
  </trdgrp>
  <trd lng="en">anesthesia</trd>
  <trd lng="fr">anesth&eacute;sie</trd>
  <trd lng="he">&he;&#1463;&resh;&dalet;&#1464;&mem;&#1464;&he;</trd>
  <trdgrp lng="hu">
    <trd>&eacute;rz&eacute;stelen&iacute;t&eacute;s</trd>,
    <trd>aneszt&eacute;zia</trd>
  </trdgrp>
  <trd lng="nl">verdoving</trd>
  <trdgrp lng="pl">
    <trd>znieczulenie</trd>,
    <trd>narkoza</trd>
  </trdgrp>
  <trd lng="pt">anestesia</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_je;&c_s;&c_t;&c_je;&c_z;&c_i;&c_ja;</trd>,
    <trd>&c_o;&c_b;&c_je;&c_z;&c_b;&c_o;&c_l;&c_i;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_n;&c_a;&c_r;&c_k;&c_o;&c_z;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>anest&eacute;za</trd>,
    <trd>anest&eacute;zia</trd>,
    <trd>znecitlivenie</trd>
  </trdgrp>
</drv>

<drv mrk="aneste.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="aneste.0ilo.MED">
    <dif>
      Substanco por anestezi, ekzemple <ref cel="klorof1.0o">kloroformo</ref>.
    </dif>
    <ref tip="super" cel="dolor.kontraux0ilo">kontra&ubreve;dolorilo, 
    sendolorigilo</ref>
    <refgrp tip="vid">
      <ref cel="analgezik.0o">analgeziko</ref>,
      <ref cel="narkot.0ajxo">narkotilo</ref>,
      <ref cel="sedativ.0o">sedativo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_o;&c_l;&c_je;&c_s;&c_u;&c_c;&c_ib;&c_sh;&c_a;&c_l;&c_mol;&c_n;&c_y;
    &c_s;&c_r;&c_o;&c_d;&c_a;&c_k;</trd>,
    <trd>&c_b;&c_o;&c_l;&c_je;&c_p;&c_a;&c_t;&c_o;&c_l;&c_mol;&c_n;&c_y;
    &c_s;&c_r;&c_o;&c_d;&c_a;&c_k;</trd>,
    <trd>&c_n;&c_a;&c_r;&c_k;&c_o;&c_z;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>anestezin</trd>,
    <trd>benzokain</trd>,
    <trd>paraaminobenzoan etyln&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bet&auml;ubungsmittel</trd>,
    <trd>An&auml;sthetikum</trd>
  </trdgrp>
  <trd lng="en">anesthetic</trd>
  <trd lng="es">anestesia</trd>
  <trd lng="fr">anesth&eacute;sique<klr> (subst.)</klr></trd>
  <trd lng="he">&het;&vav;&mem;&resh; &he;&resh;&dalet;&mem;&he;</trd>
  <trd lng="hu">&eacute;rz&eacute;stelen&iacute;t&odblac;</trd>
  <trd lng="nl">verdovingsmiddel</trd>
  <trdgrp lng="pl">
    <trd>&sacute;rodek <ind>znieczulaj&aogonek;cy</ind></trd>
  </trdgrp>
  <trd lng="pt">anest&eacute;sico</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_je;&c_s;&c_t;&c_je;&c_z;&c_i;&c_r;&c_u;&c_ju;&c_shch;&c_je;&c_je;
    &c_s;&c_r;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_o;&c_b;&c_je;&c_z;&c_b;&c_o;&c_l;&c_i;&c_v;&c_a;&c_ju;&c_shch;&c_je;&c_je;
    &c_s;&c_r;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_n;&c_a;&c_r;&c_k;&c_o;&c_z;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>anestetikum</trd>,
    <trd>liek na znecitlivenie</trd>
  </trdgrp>
</drv>
</art>
