<vortaro>
<art mrk="$Id$">
<kap><rad>anestez</rad>/i</kap>
<drv mrk="aneste.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Senigi iun je la sentokapablo helpe de specialaj substancoj.
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="narkot.0i">narkoti</ref>
  </refgrp>
  <trd lng="fr">anesth&eacute;sier</trd>
  <trd lng="nl">verdoven</trd>
  <trdgrp lng="ru">
    <trd>&#1086;&#1073;&#1077;&#1079;&#1073;&#1086;&#1083;&#1080;&#1074;&#1072;&#1090;&#1100;</trd>,
    <trd>&#1072;&#1085;&#1077;&#1089;&#1090;&#1077;&#1079;&#1080;&#1088;&#1086;&#1074;&#1072;&#1090;&#1100;</trd>
  </trdgrp>
</drv>

<drv mrk="aneste.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Pli a&ubreve; malpli kompleta senigo je la sentokapablo, ka&ubreve;zita
      de specialaj substancoj a&ubreve; de malsano.
    </dif>
  </snc>
  <trd lng="fr">anesth&eacute;sie</trd>
  <trd lng="nl">verdoving</trd>
  <trdgrp lng="ru">
    <trd>&#1072;&#1085;&#1077;&#1089;&#1090;&#1077;&#1079;&#1080;&#1103;</trd>,
    <trd>&#1086;&#1073;&#1077;&#1079;&#1073;&#1086;&#1083;&#1080;&#1074;&#1072;&#1085;&#1080;&#1077;</trd>,
    <trd>&#1085;&#1072;&#1088;&#1082;&#1086;&#1079;</trd>
  </trdgrp>
</drv>

<drv mrk="aneste.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Substanco por anestezi:
      <ekz>
        kloroformo.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">anesth&eacute;sique</trd>
  <trd lng="nl">verdovingsmiddel</trd>
  <trdgrp lng="ru">
    <trd>&#1072;&#1085;&#1077;&#1089;&#1090;&#1077;&#1079;&#1080;&#1088;&#1091;&#1102;&#1097;&#1077;&#1077;
    &#1089;&#1088;&#1077;&#1076;&#1089;&#1090;&#1074;&#1086;</trd>,
    <trd>&#1086;&#1073;&#1077;&#1079;&#1073;&#1086;&#1083;&#1080;&#1074;&#1072;&#1102;&#1097;&#1077;&#1077;
    &#1089;&#1088;&#1077;&#1076;&#1089;&#1090;&#1074;&#1086;</trd>,
    <trd>&#1085;&#1072;&#1088;&#1082;&#1086;&#1079;</trd>
  </trdgrp>
</drv>
</art>
