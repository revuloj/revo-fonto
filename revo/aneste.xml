<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>anestez</rad>/i
</kap>
<drv mrk="aneste.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Senigi iun je la sentokapablo helpe dredakto: +trd be
      e specialaj substancoj.
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="narkot.0i">narkoti</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_a;&c_n;&c_e;&c_s;&c_t;&c_e;&c_z;&c_ib;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_ja;&c_z;&c_b;&c_o;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>bet&auml;uben</trd>,
      <trd>an&auml;sthesieren</trd>
  </trdgrp>
  <trd lng="en">anesthetize</trd>
  <trd lng="es">anestesiar</trd>
  <trd lng="fr">anesth&eacute;sier</trd>
  <trd lng="nl">verdoven</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_je;&c_z;&c_b;&c_o;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_a;&c_n;&c_je;&c_s;&c_t;&c_je;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">znecitlivi&tcaron;</trd>
</drv>

<drv mrk="aneste.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Pli a&ubreve; malpli kompleta senigo je la sentokapablo, ka&ubreve;zita
      de specialaj substancoj a&ubreve; de malsano.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_n;&c_e;&c_s;&c_t;&c_e;&c_z;&c_ib;&c_ja;</trd>,
    <trd>&c_a;&c_b;&c_ja;&c_z;&c_b;&c_o;&c_l;&c_mol;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_b;&c_o;&c_l;&c_je;&c_p;&c_a;&c_t;&c_a;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_n;&c_a;&c_r;&c_k;&c_o;&c_z;</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Bet&auml;ubung</trd>,
      <trd>An&auml;sthesie</trd>
  </trdgrp>
  <trd lng="en">anesthesia</trd>
  <trd lng="fr">anesth&eacute;sie</trd>
  <trd lng="nl">verdoving</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_je;&c_s;&c_t;&c_je;&c_z;&c_i;&c_ja;</trd>,
    <trd>&c_o;&c_b;&c_je;&c_z;&c_b;&c_o;&c_l;&c_i;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_n;&c_a;&c_r;&c_k;&c_o;&c_z;</trd>
  </trdgrp>
  <trd lng="sk">anest&eacute;za</trd>
</drv>

<drv mrk="aneste.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Substanco por anestezi:
      <ekz>
        kloroformo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_o;&c_l;&c_je;&c_s;&c_u;&c_c;&c_ib;&c_sh;&c_a;&c_l;&c_mol;&c_n;&c_y; &c_s;&c_r;&c_o;&c_d;&c_a;&c_k;</trd>,
    <trd>&c_b;&c_o;&c_l;&c_je;&c_p;&c_a;&c_t;&c_o;&c_l;&c_mol;&c_n;&c_y; &c_s;&c_r;&c_o;&c_d;&c_a;&c_k;</trd>,
    <trd>&c_n;&c_a;&c_r;&c_k;&c_o;&c_z;</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Bet&auml;ubungsmittel</trd>,
      <trd>An&auml;sthetikum</trd>
  </trdgrp>
  <trd lng="en">anesthetic</trd>
  <trd lng="es">anestesia</trd>
  <trd lng="fr">anesth&eacute;sique</trd>
  <trd lng="nl">verdovingsmiddel</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_je;&c_s;&c_t;&c_je;&c_z;&c_i;&c_r;&c_u;&c_ju;&c_shch;&c_je;&c_je; &c_s;&c_r;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_o;&c_b;&c_je;&c_z;&c_b;&c_o;&c_l;&c_i;&c_v;&c_a;&c_ju;&c_shch;&c_je;&c_je; &c_s;&c_r;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_n;&c_a;&c_r;&c_k;&c_o;&c_z;</trd>
  </trdgrp>
  <trd lng="sk">anestetikum</trd>
</drv>
</art>
