<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap><ofc>*</ofc><rad>akn</rad>/o</kap>
<drv mrk="akn.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
    Malsaneca ru&gcirc;a &scirc;vela&jcirc;eto sur la ha&ubreve;to.
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_y;&c_sh;&c_ch;</trd>, 
      <trd>&c_v;&c_u;&c_g;&c_o;&c_r;</trd> 
    </trdgrp>
    <trd lng="de">Pickel</trd>
  <trdgrp lng="en">
     <trd>pimple</trd>,
     <trd>zit</trd>,
     <trd>blemish</trd>,
     <trd>spot</trd>
  </trdgrp>
    <trd lng="fr">bouton<klr> (sur la peau)</klr></trd>
    <trd lng="nl">acne</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_y;&c_shch;</trd>,
      <trd>&c_p;&c_r;&c_y;&c_shch;&c_i;&c_k;</trd>,
      <trd>&c_u;&c_g;&c_o;&c_r;&c_mol;</trd>
    </trdgrp>
    <trd lng="sv">finne</trd>
  </snc>
  <snc num="2">
    <dif>
      Ha&ubreve;tmalsano  karakterizata per ru&gcirc;aj &scirc;vela&jcirc;etoj.
    </dif>
    <trd lng="de">Akne</trd>
    <trd lng="en">acne</trd>
    <trd lng="fr">acn&eacute;</trd>
  </snc>
  <trd lng="es">acn&eacute;</trd>
  <trdgrp lng="pt">
     <trd>acne</trd>,
     <trd>espinha</trd>
  </trdgrp>
  <trd lng="sk">akn&eacute;</trd>
</drv>

<drv mrk="akn.0ero">
  <kap><tld/>ero</kap>
  <snc>
    <dif>
      = <tld/>o 1.
    </dif>
  </snc>
  <trd lng="de">Pickel</trd>
  <trdgrp lng="en">
     <trd>pimple</trd>,
     <trd>zit</trd>,
     <trd>blemish</trd>,
     <trd>spot</trd>
  </trdgrp>
</drv>
</art>
