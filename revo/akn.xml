<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc><rad>akn</rad>/o</kap>
<drv mrk="akn.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Malsaneca ru&gcirc;a &scirc;vela&jcirc;eto sur la ha&ubreve;to.
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_y;&c_sh;&c_ch;</trd>,
      <trd>&c_v;&c_u;&c_g;&c_o;&c_r;</trd>
    </trdgrp>
    <trd lng="br">burbu(enn)</trd>
    <trd lng="de">Pickel</trd>
    <trdgrp lng="en">
      <trd>pimple</trd>,
      <trd>zit</trd>,
      <trd>blemish</trd>,
      <trd>spot</trd>
    </trdgrp>
    <trd lng="fr">bouton<klr> (sur la peau)</klr></trd>
    <trdgrp lng="hu">
      <trd>pattan&aacute;s</trd>,
      <trd>mitesszer</trd>,
      <trd>akn&eacute;</trd>
    </trdgrp>
    <trd lng="nl">acne-blaasje</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_y;&c_shch;</trd>,
      <trd>&c_p;&c_r;&c_y;&c_shch;&c_i;&c_k;</trd>,
      <trd>&c_u;&c_g;&c_o;&c_r;&c_mol;</trd>
    </trdgrp>
    <trd lng="sv">finne</trd>
  </snc>
  <snc>
    <dif>
      Ha&ubreve;tmalsano karakterizata per ru&gcirc;aj &scirc;vela&jcirc;etoj.
    </dif>
    <trd lng="br">burbu</trd>
    <trd lng="de">Akne</trd>
    <trd lng="en">acne</trd>
    <trd lng="fr">acn&eacute;</trd>
    <trd lng="hu">akn&eacute;</trd>    
    <trd lng="nl">acne</trd>
    <trd lng="ru">&c_u;&c_g;&c_r;&c_je;&c_v;&c_a;&c_ja; &c_s;&c_y;&c_p;&c_mol;
</trd>
  </snc>
  <trd lng="es">acn&eacute;</trd>
  <trdgrp lng="pt">
    <trd>acne</trd>,
    <trd>espinha</trd>
  </trdgrp>
  <trd lng="sk">akn&eacute;</trd>
</drv>

<drv mrk="akn.0ero">
  <kap><tld/>ero</kap>
  <snc>
    <dif>
      = <tld/>o 1.
    </dif>
  </snc>
  <trdgrp lng="br">
    <trd>burbuenn</trd>,
    <trd>porbolenn</trd>
  </trdgrp>
  <trd lng="de">Pickel</trd>
  <trdgrp lng="en">
    <trd>pimple</trd>,
    <trd>zit</trd>,
    <trd>blemish</trd>,
    <trd>spot</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>pattan&aacute;s</trd>,
    <trd>mitesszer</trd>,
    <trd>akn&eacute;</trd>
  </trdgrp>  
  <trd lng="nl">acne-blaasje</trd>
  <trd lng="ru">&c_p;&c_r;&c_y;&c_shch;</trd>
</drv>
</art>
