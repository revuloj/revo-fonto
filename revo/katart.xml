<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: katart.xml,v 1.13 2022/03/21 17:51:57 revo Exp $">
<kap>
<rad>katarted</rad>/oj
</kap>
<drv mrk="katart.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_familioj" cel="famili.0o.BIO">Familio</ref>
      el la ordo <ref tip="super" cel="bird.rabo0oj">rabobirdoj</ref>
      (<trd lng="la">Cathartidae</trd>):
      <ekz>
        la katartedoj, novmondaj vulturoj a&ubreve; amerikaj vulturoj enhavas sep speciojn kiuj
        trovi&gcirc;as en Norda kaj Suda Amerikoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16413">Katartedoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_e;&c_r;&c_y;&c_k;&c_a;&c_n;&c_s;&c_k;&c_ib;&c_ja; <ind>&c_g;&c_r;&c_y;&c_f;&c_y;</ind> <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Neuweltgeier</trd>
  <trd lng="en">New World vultures</trd>
  <trd lng="fr">vautours du nouveau monde</trd>
  <trd lng="hu">&uacute;jvil&aacute;gi <ind>kesely&udblac;f&eacute;l&eacute;k</ind></trd>
  <trd lng="nl">gieren van de Nieuwe Wereld</trd>
  <trd lng="pl">kondorowate</trd>
  <trd lng="ru">&c_a;&c_m;&c_je;&c_r;&c_i;&c_k;&c_a;&c_n;&c_s;&c_k;&c_i;&c_je; <ind>&c_g;&c_r;&c_i;&c_f;&c_y;</ind> <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x430;&#x43C;&#x435;&#x440;&#x438;&#x43A;&#x430;&#x43D;&#x441;&#x44C;&#x43A;&#x456; &#x491;&#x440;&#x438;&#x444;&#x438;</trd>,
    <trd>&#x43A;&#x43E;&#x43D;&#x434;&#x43E;&#x440;&#x43E;&#x432;&#x456;</trd>
  </trdgrp>
</drv>
</art>
