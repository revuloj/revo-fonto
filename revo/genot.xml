<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: genot.xml,v 1.16 2019/02/21 20:10:29 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>genot</rad>/o
</kap>
<drv mrk="genot.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Malgranda <ref tip="lst" cel="best.rabo0o"
      lst="voko:rabobestoj">rabobesto</ref> iom katsimila
      kun pinta muzelo kaj grandaj rondaj oreloj;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="vivere.0oj">viveredoj</ref>
      (<trd lng="la">Genetta</trd>):
      <ekz>
        la <tld/>o <klr>[&#8230;]</klr> lo&gcirc;as en Iberio, la Balearoj kaj Francio kaj &scirc;ajne
        etendi&gcirc;as aktuale al nordo kaj oriento de la kontinento
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=285580"
              >Genoto</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/8/82/Genetta_genetta_felina_(Wroclaw_zoo)_(cropped).JPG/354px-Genetta_genetta_felina_(Wroclaw_zoo)_(cropped).JPG"
      prm="CC BY-SA 3.0"
      ><tld/>o en la besto&gcirc;ardeno de Wroc&lstroke;aw
        <fnt>
          <aut>Gu&eacute;rin Nicolas, propra verko</aut>
          <lok><url ref="&WCW;/File:Genetta_genetta_felina_(Wroclaw_zoo)_(cropped).JPG"
              >https://commons.wikimedia.org/wiki/File:Genetta_genetta_felina_(Wroclaw_zoo)_(cropped).JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_n;&c_e;&c_t;&c_a;</trd>
  <trd lng="ca">geneta</trd>
  <trd lng="de">Ginsterkatze</trd>
  <trd lng="en">genet</trd>
  <trd lng="fr">genette</trd>
  <trd lng="hu">petymeg</trd>
  <trdgrp lng="nl">
    <trd>genetkat</trd>,
    <trd>genetta</trd>
  </trdgrp>
  <trd lng="pt">gineta</trd>
  <trd lng="ru">&c_g;&c_je;&c_n;&c_je;&c_t;&c_t;&c_a;</trd>
</drv>
</art>

