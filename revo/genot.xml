<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>genot</rad>/o
</kap>
<drv mrk="genot.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="vivere.0oj">viveredoj</ref>
      (<trd lng="la">Genetta</trd>), malgranda rabobesto iom katsimila.
    </dif>
  </snc>
  <trd lng="de">Ginsterkatze</trd>
  <trd lng="en">genet</trd>
  <trd lng="fr">genette</trd>
  <trdgrp lng="nl">
    <trd>genetkat</trd>,
    <trd>genetta</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_je;&c_n;&c_je;&c_t;&c_t;&c_a;</trd>
</drv>
</art>

