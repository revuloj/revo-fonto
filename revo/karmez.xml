<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>karmezin</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="karmez.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Malhela ru&gcirc;a <ref tip="super" cel="kolor.0o.propre">koloro</ref> kun
      viola nuanco.
    </dif>
  </snc>
  <trd lng="de">Karmesinrot</trd>
  <trdgrp lng="en">
    <trd>crimson</trd>,
    <trd>deep <ind>red</ind><klr> inclining to purple</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>carmin<klr> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>karmozijn</trd>,
    <trd>donkerrood</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_jo;&c_m;&c_n;&c_o;-&c_k;&c_r;&c_a;&c_s;&c_n;&c_y;&c_j; &c_c;&c_v;&c_je;&c_t;</trd>,
    <trd>&c_m;&c_a;&c_l;&c_i;&c_n;&c_o;&c_v;&c_y;&c_j; &c_c;&c_v;&c_je;&c_t;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_m;&c_a;&c_z;&c_i;&c_n;&c_n;&c_y;&c_j; &c_c;&c_v;&c_je;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="karmez.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Malhele ru&gcirc;a kun viola nuanco:
      <ekz>
        Jaspa tuj surmetis botetojn el ru&gcirc;a &scirc;afledo <klr>...</klr> kaj
        <tld/>an silkan mantelon
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 49a, p. 121a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>carmin<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>
</art>
