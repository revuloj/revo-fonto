<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bulgar.xml,v 1.29 2021/03/05 18:27:14 revo Exp $">
<kap>
   <ofc>8</ofc>
   <rad>bulgar</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="bulgar.0o">
   <kap><ofc>8</ofc><tld/>o
   <fnt><aut>Akademio de Esperanto</aut>, <vrk><url ref="http://www.akademio-de-esperanto.org/aktoj/aktoj2/8oa.html"> 8a
   Oficiala Aldono</url></vrk>, <lok>II.C.1</lok></fnt></kap>
   <snc mrk="bulgar.0o.KOMUNE">
      <uzo tip="stl">KOMUNE</uzo>
      <dif>
         Ano de la &ccirc;efa etno de <tld lit="B"/>ujo:
         <ekz>
           la <tld/>oj estas sudslava etno.
         </ekz>
      </dif>
      <trd lng="be">&c_b;&c_a;&c_w;&c_g;&c_a;&c_r;&c_y;&c_n;</trd>
      <trd lng="bg">&c_b;&c_malmol;&c_l;&c_g;&c_a;&c_r;&c_i;&c_n;</trd>
      <trd lng="de">Bulgare</trd>
      <trd lng="es">b&uacute;lgaro</trd>
      <trd lng="fr">Bulgare<klr> (subst.)</klr></trd>
      <trdgrp lng="hu">
         <trd>bolg&aacute;r</trd>,
         <trd>bulg&aacute;r</trd>
      </trdgrp>
      <trd lng="nl">Bulgaar</trd>
      <trd lng="pl">Bu&lstroke;gar</trd>
      <trd lng="pt">b&uacute;lgaro</trd>
      <trd lng="ru">&c_b;&c_o;&c_l;&c_g;&c_a;&c_r;&c_i;&c_n;</trd>
      <trd lng="tp">jan pi ma Pokasi</trd>
      <trd lng="vo">Bulgar&auml;nan</trd>
   </snc>
   <snc mrk="bulgar.0o.HIS">
      <uzo tip="fak">HIS</uzo>
      <ref tip="dif" cel="bulgar.pra0o">pra<tld/>o</ref>
      <rim>
         &Gcirc;enerale rekomendindas uzi la pli precizan vorton, kiel
         &leftquot;pra<tld/>o&rightquot; a&ubreve; &leftquot;tjurko<tld/>o&rightquot;; sed tio ne &ccirc;iam eblas, ekz-e
         kiam oni priskribas la iompostioman asimiladon de la
         tjurko<tld/>oj kaj slavoj en la Danuba <tld lit="B"/>ujo.
      </rim>
      <trd lng="ru">&c_b;&c_u;&c_l;&c_g;&c_a;&c_r;</trd>
   </snc>
  <trd lng="cs">Bulhar</trd>
  <trd lng="de">Bulgare</trd>
  <trd lng="en">Bulgarian</trd>
  <trd lng="he">&#x5D1;&#x5D5;&#x5DC;&#x5D2;&#x5E8;&#x5D9;</trd>
  <trd lng="sk">Bulhar</trd>
</drv>

<drv mrk="bulgar.0a">
   <kap><tld/>a</kap>
   <snc mrk="bulgar.0a.KOMUNE">
      <uzo tip="stl">KOMUNE</uzo>
      <dif>
         Rilatanta al <tld/>oj<sncref ref="bulgar.0o.KOMUNE"/>, a&ubreve; ilia
         etno kaj lando:
         <ekz>
           la malnova <tld/>a lingvo estis unu el la &ccirc;efaj fontoj de
           Slavono.
         </ekz>
      </dif>
      <trd lng="bg">&c_b;&c_malmol;&c_l;&c_g;&c_a;&c_r;&c_s;&c_k;&c_i;</trd>
      <trd lng="de">bulgarisch</trd>
      <trd lng="es">b&uacute;lgaro</trd>
      <trd lng="fr">bulgare<klr> (adj.)</klr></trd>
      <trd lng="hu">bolg&aacute;r</trd>
      <trd lng="nl">Bulgaars</trd>
      <trd lng="pl">bu&lstroke;garski</trd>
      <trd lng="pt">b&uacute;lgaro</trd>
      <trd lng="ru">&c_b;&c_o;&c_l;&c_g;&c_a;&c_r;&c_s;&c_k;&c_i;&c_j;</trd>
      <trd lng="tp">pi ma Pokasi</trd>
      <trd lng="vo">Bulgar&auml;nik</trd>
   </snc>
   <snc mrk="bulgar.0a.HIS">
      <uzo tip="fak">HIS</uzo>
      <dif>
         Rilatanta al la pra<tld/>oj a&ubreve; ilia etno kaj lando:
         <ekz>
         <uzo tip="fak">LIN</uzo>
         en la familio de la
         <ref tip="super" cel="tjurk.0alingvo">tjurkaj lingvoj</ref>
         al la <tld/>a grupo oni alkalkulas la pra<tld/>an kaj la
         &ccirc;uva&scirc;an lingvojn.
         </ekz>
      </dif>
      <trd lng="ru">&c_b;&c_u;&c_l;&c_g;&c_a;&c_r;&c_s;&c_k;&c_i;&c_j;</trd>
   </snc>
   <trd lng="be">&c_b;&c_a;&c_w;&c_g;&c_a;&c_r;&c_s;&c_k;&c_ib;</trd>
   <trd lng="de">bulgarisch</trd>
   <trd lng="en">Bulgarian</trd>
   <trd lng="he">&#x5D1;&#x5D5;&#x5DC;&#x5D2;&#x5E8;&#x5D9;&#x5EA;</trd>
  <trd lng="sk">bulharsk&yacute;</trd>
</drv>

<drv mrk="bulgar.B0ujo">
   <kap><tld lit="B"/>ujo
     <fnt><bib>ZR</bib></fnt>
   </kap>
   <mlg kod="ISO-3166">BG</mlg>
   <snc mrk="bulgar.B0ujo.KOMUNE">
   <uzo tip="fak">GEOG</uzo>
   <uzo tip="fak">POL</uzo>
      <dif>
         &Scirc;tato en sudorienta <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref> (Balkano), kies &ccirc;efurbo estas
         Sofio.
      </dif>
   <ref tip="sin" cel="bulgari.B0o">Bulgario</ref>
   </snc>
   <trd lng="be">&c_B;&c_a;&c_w;&c_g;&c_a;&c_r;&c_y;&c_ja;</trd>
   <trd lng="bg">&c_B;&c_malmol;&c_l;&c_g;&c_a;&c_r;&c_i;&c_ja;</trd>
   <trd lng="de">Bulgarien</trd>
   <trd lng="en">Bulgaria</trd>
   <trd lng="es">Bulgaria</trd>
   <trdgrp lng="fr">
     <trd>Bulgarie</trd>,
     <trd>R&eacute;publique de Bulgarie</trd>
   </trdgrp>
   <trd lng="fy">Bulgarije</trd>
   <trd lng="hu">Bulg&aacute;ria</trd>
   <trd lng="id">Bulgaria</trd>
   <trd lng="nl">Bulgarije</trd>
   <trd lng="pl">Bu&lstroke;garia</trd>
   <trdgrp lng="pt">
     <trd>Bulg&aacute;ria</trd>,
     <trd>Rep&uacute;blica da Bulg&aacute;ria</trd>
   </trdgrp>
   <trd lng="ru">&c_B;&c_o;&c_l;&c_g;&c_a;&c_r;&c_i;&c_ja;</trd>
   <trd lng="sk">Bulharsko</trd>
   <trd lng="tp">ma Pokasi</trd>
   <trd lng="vo">Bulgar&auml;n</trd>
</drv>

<drv mrk="bulgar.pra0o">
   <kap>pra<tld/>o, <var><kap>tjurko<tld/>o</kap></var></kap>
   <snc>
   <uzo tip="fak">HIS</uzo>
      <dif>
         <ref tip="super" cel="tjurk.0o">Tjurko</ref>,
         ano de historia popolo, kies unu bran&ccirc;o konkeris
         la Balkanan <tld lit="B"/>ujon, alia i&gcirc;is &ccirc;efa etno
         de la Volga-Kama <tld lit="B"/>ujo, kaj la tria i&gcirc;is
         la Ka&ubreve;kazaj balkaroj.
      </dif>
   </snc>
  <trd lng="bg">&c_p;&c_r;&c_a;&c_b;&c_malmol;&c_l;&c_g;&c_a;&c_r;&c_i;&c_n;</trd>
  <trd lng="cs">prabulhar</trd>
  <trdgrp lng="de">
    <trd>Protobulgare</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Proto-Bulgarian</trd>,
    <trd>Hunno-Bulgar</trd></trdgrp>
  <trdgrp lng="hu">
    <trd>&oacute;bolg&aacute;r</trd>,
    <trd>bolg&aacute;r-t&ouml;r&ouml;k</trd></trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_u;&c_l;&c_g;&c_a;&c_r;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_b;&c_o;&c_l;&c_g;&c_a;&c_r;&c_i;&c_n;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_t;&c_o;&c_b;&c_o;&c_l;&c_g;&c_a;&c_r;&c_i;&c_n;</trd>
  </trdgrp>
  <trd lng="sk">prabulhar</trd>
</drv>

<drv mrk="bulgar.GrandaB0ujo">
   <kap>Granda <tld lit="B"/>ujo</kap>
   <snc>
   <uzo tip="fak">HIS</uzo>
      <dif>
         Tjurka &scirc;tato kiu ekzistis en la 7a jc &ccirc;irka&ubreve; la <ref cel="mar.AzovaM0o">Azova Maro</ref>, &gcirc;is la disiro de la
         pra<tld/>oj Okcidenten kaj Nord-Orienten.
      </dif>
   </snc>
   <trd lng="de">Gro&szlig;bulgarisches Reich</trd>
   <trd lng="en">Great Bulgaria</trd>
   <trd lng="hu">Nagy Bolg&aacute;ria</trd>
   <trd lng="ru">&c_V;&c_je;&c_l;&c_i;&c_k;&c_a;&c_ja; &c_B;&c_o;&c_l;&c_g;&c_a;&c_r;&c_i;&c_ja;</trd>
</drv>

<drv mrk="bulgar.VolgaKamaB0ujo">
   <kap>Volga-Kama <tld lit="B"/>ujo</kap>
   <snc>
   <uzo tip="fak">HIS</uzo>
      <dif>
         Tjurka &scirc;tato en la baseno de <ref cel="kam1.K0o">Kamo</ref> kaj
         &ccirc;e la meza fluo de <ref cel="volg.V0o">Volgo</ref>, kiun la
         mongoloj konkeris en la 13a jc kaj faris centro de la Ora
         Hordo.
      </dif>
   </snc>
   <trdgrp lng="bg">
     <trd>&c_V;&c_o;&c_l;&c_zh;&c_k;&c_a; &c_B;&c_malmol;&c_l;&c_g;&c_a;&c_r;&c_i;&c_ja;</trd>,
     <trd>&c_V;&c_o;&c_l;&c_zh;&c_k;&c_o;-&c_K;&c_a;&c_m;&c_s;&c_k;&c_a; &c_B;&c_malmol;&c_l;&c_g;&c_a;&c_r;&c_i;&c_ja;</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>Reich der Wolgabulgaren</trd>,
     <trd>Bulgarenreich an der Wolga</trd>
   </trdgrp>
   <trd lng="en">Volga Bulgaria</trd>
   <trd lng="ru">&c_V;&c_o;&c_l;&c_zh;&c_s;&c_k;&c_o;-&c_K;&c_a;&c_m;&c_s;&c_k;&c_a;&c_ja; &c_B;&c_o;&c_l;&c_g;&c_a;&c_r;&c_i;&c_ja;</trd>
</drv>

</art>
