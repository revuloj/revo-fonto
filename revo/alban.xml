<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: alban.xml,v 1.33 2023/10/12 18:05:09 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>alban</rad>/o <fnt><bib>Fab1</bib></fnt>
</kap>

<drv mrk="alban.0o">
  <kap><ofc>8</ofc><tld/>o</kap>
  <snc mrk="alban.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="alban.A0ujo"><tld lit="A"/>ujo</ref>.
    </dif>
  </snc>
  <snc mrk="alban.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco:
      <ekz>
        estis <tld lit="A"/>oj, kura&gcirc;aj homoj, kiel diris mia patrino
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iujare ekde 1988 estas minimume 100 <tld/>oj en Kosovo mortigitaj de jugoslava polico kaj
        armeo
        <fnt>
	<aut>Zlatko Ti&scaron;ljar</aut>
	<vrk><url ref="&Monato;/2000/006811.php">Nekredeblaj malveroj</url></vrk>
	<bib>Monato</bib>
	<lok>2000/03, p. 12-13</lok>
        </fnt>;
      </ekz>
      <ekz>
        la urbo <nom>Kosovska Mitrovica</nom>
        <klr>[&#x2026;]</klr> estas plu dividita je du partoj &dash; serboj, lo&gcirc;antaj norde, kaj <tld/>oj sude
        <fnt>
	<aut>Evgeni Georgiev</aut>
	<vrk><url ref="&Monato;/2000/007007.php">Atenco ne preteratentita</url></vrk>
	<bib>Monato</bib>
	<lok>2000/06, p. 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Mimoza</nom> estas <tld/>ino lo&gcirc;anta en <nom>&Ouml;rebro</nom>, Svedio
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2008/009414.php">Duonislamano duonferio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
      <tld lit="A"/>o estas anka&ubreve; nomo de sanktulo:
      <ekz>
        la pre&gcirc;ejo de la sankta <tld lit="A"/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Sonorila profundo</lok>
        </fnt>.
      </ekz>
  </rim>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_b;&c_a;&c_n;&c_je;&c_c;</trd>
  <trd lng="bg">&c_a;&c_l;&c_b;&c_a;&c_n;&c_je;&c_c;</trd>
  <trd lng="ca">alban&eacute;s <klr>(subst.)</klr></trd>
  <trd lng="cs">Alb&aacute;nec</trd>
  <trd lng="de">Albaner</trd>
  <trd lng="en">Albanian</trd>
  <trd lng="fr">Albanais<klr> (subst.)</klr></trd>
  <trd lng="he">&#x5D0;&#x5DC;&#x5D1;&#x5E0;&#x5D5;</trd>
  <trd lng="ja">&#x30A2;&#x30EB;&#x30D0;&#x30CB;&#x30A2;&#x4EBA;  <pr>&#x30A2;&#x30EB;&#x30D0;&#x30CB;&#x30A2;&#x3058;&#x3093;</pr></trd>
  <trd lng="nl">Albanees</trd>
  <trd lng="pl">Alba&nacute;czyk</trd>
  <trd lng="sk">Alb&aacute;nec</trd>
  <trd lng="uk">&c_a;&#x43B;&c_b;&c_a;&#x43D;&c_je;&c_c;&#x44C;</trd>
</drv>

<drv mrk="alban.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato:
      <ekz>
        la <tld/>a parlamento
        <fnt>
	<aut>Bardhyl Selimi Mezini</aut>
	<vrk><url ref="&Monato;/2000/006858.php">Diktaturo de impostoj</url></vrk>
	<bib>Monato</bib>
	<lok>2000/05, p. 6</lok>
        </fnt>;
      </ekz>
      <ekz>
        pli ol 150 lingvoj, inter kiuj la afrikansa kaj la <tld/>a, la &ccirc;ina, korea <klr>[&#x2026;]</klr>
        <fnt>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="&Monato;/1995/004514.php">Por lingvemaj
          komputilemuloj ...</url></vrk>
          <bib>Monato</bib>
          <lok>1995/06, p. 22</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>aj kosovanoj
        <fnt>
        <aut>Michel Duc Goninaz</aut>
        <vrk><url ref="&Monato;/2000/006947.html">Eks-Jugoslavio: 
          kiu kiun mistifikas?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kontra&ubreve;<tld/>aj terora&jcirc;oj de februaro 1945
        <fnt>
        <aut>Michel Duc Goninaz</aut>
        <vrk><url ref="&Monato;/2000/006947.html">Eks-Jugoslavio: 
          kiu kiun mistifikas?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        la  <tld/>a etno en Makedonio povus profiti de la nova prezidento
        <fnt>
	<aut>Evgeni Georgiev</aut>
	<vrk><url ref="&Monato;/2000/006819.php">Nova &scirc;anco por &ccirc;iuj etnoj</url></vrk>
	<bib>Monato</bib>
	<lok>2000/03, p. 10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_b;&c_a;&c_n;&c_s;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_a;&c_l;&c_b;&c_a;&c_n;&c_s;&c_k;&c_i;</trd>
  <trd lng="ca">alban&eacute;s <klr>(adj.)</klr></trd>
  <trd lng="cs">alb&aacute;nsk&yacute;</trd>
  <trd lng="de">albanisch</trd>
  <trd lng="en">Albanian</trd>
  <trd lng="fr">albanais<klr> (adj.)</klr></trd>
  <trd lng="he">&#x5D0;&#x5DC;&#x5D1;&#x5E0;&#x5D9;</trd>
  <trd lng="ja">&#x30A2;&#x30EB;&#x30D0;&#x30CB;&#x30A2;&#x306E;  <pr>&#x3042;&#x308B;&#x3070;&#x306B;&#x3042;&#x306E;</pr></trd>
  <trd lng="nl">Albanees</trd>
  <trd lng="pl">alba&nacute;ski</trd>
  <trdgrp lng="ro">
    <trd>albanez</trd>,
    <trd>albanez&abreve;</trd>
  </trdgrp>
  <trd lng="sk">alb&aacute;nsky</trd>
</drv>

<drv mrk="alban.A0ujo">
  <kap><tld lit="A"/>ujo</kap>
  <mlg kod="ISO-3166">AL</mlg>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" cel="sxtat.0o.POL" lst="voko:&scirc;tatoj">&Scirc;tato</ref> en
      la sudorienta parto de <ref tip="malprt" cel="euxrop1.E0o.geog">E&ubreve;ropo</ref>
      sur la Balkana duoninsulo:
      <ekz>
        en la nuna <tld lit="A"/>ujo <klr>[&#x2026;]</klr> atingis lin printempe de la jaro -44 la nova&jcirc;o
        pri la mortigo de Cezaro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=283">A&ubreve;gusto Cezaro</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Ferenc Nopcsa</nom>: <klr>[&#x2026;]</klr> liaj &ccirc;efaj esplorkampoj estis la dinosa&ubreve;roj kaj la
        geologio de  <tld lit="A"/>ujo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=246580">Ferenc Nopcsa</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="albani.A0o">Albanio</ref>
  </snc>
  <trd lng="be">&c_A;&c_l;&c_mol;&c_b;&c_a;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_A;&c_l;&c_b;&c_a;&c_n;&c_i;&c_ja;</trd>
  <trdgrp lng="ca">
    <trd>Alb&agrave;nia</trd>,
    <trd>Rep&uacute;blica d'Alb&agrave;nia</trd>
  </trdgrp>
  <trd lng="de">Albanien</trd>
  <trd lng="en">Albania</trd>
  <trd lng="es">Albania</trd>
  <trd lng="fr">Albanie</trd>
  <trd lng="id">Albania</trd>
  <trd lng="nl">Albani&euml;</trd>
  <trd lng="pl">Albania</trd>
  <trd lng="ro">Albania</trd>
</drv>

</art>

