<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>kate&hcirc;iz</rad>/i, katekizi
</kap>
<drv mrk="katehx2.0i">
  <kap><ofc>3</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="instru.0i">Instrui</ref> la religiajn
      elementajn principojn.
    </dif>
    <ref tip="vid" cel="predik.0i">prediki</ref>
  </snc>
  <trd lng="en">catechize</trd>
  <trdgrp lng="fr">
    <trd>cat&eacute;chiser</trd>,
    <trd>faire le <ind>cat&eacute;chisme</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hittant tan&iacute;t</trd>,
    <trd>katekiz&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">catechiseren</trd>
</drv>

<drv mrk="katehx2.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">REL</uzo>
  <snc>
    <dif>
      Instruado pri la bazoj de religio.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>cat&eacute;chisation</trd>,
    <trd>cat&eacute;chisme<klr> (enseignement)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hittantan&iacute;t&aacute;s</trd>,
    <trd>katekiz&aacute;l&aacute;s</trd>
  </trdgrp>
</drv>
</art>
