      <trd>gemeente</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>gmina</trd>,
      <trd>komuna</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>komunita</trd>,
    <trd>obec</trd>,
    <trd>osada</trd>
  </trdgrp>
  <trd lng="en">commonness</trd>
  <trdgrp lng="sk">
    <trd>komunita</trd>,
    <trd>obec</trd>,
    <trd>osada</trd>
  </trdgrp>
