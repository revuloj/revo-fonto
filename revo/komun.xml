<drv mrk="komun.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="komun.0o.0eco">
    <dif>
     <ref tip="dif" cel="komun.0eco"><tld lit="K"/>eco</ref>:
      <ekz>
        ni kunvenas por horo de silento en <tld/>o anta&ubreve; Dio
        <fnt><lok><url ref="http://www.kveker.org/kredo_kaj_vivo.htm">http://www.kveker.org/kredo_kaj_vivo.htm</url></lok></fnt>.
      </ekz>
    </dif>
  <trdgrp lng="be">
    <trd>&c_a;&c_g;&c_u;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_u;&c_p;&c_o;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_je;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
    <trd lng="nl">gemeenschap</trd>
  </snc>
  <snc mrk="komun.0o.0umo">
    <dif>
      <ref tip="dif" cel="komun.0umo.societo"><tld lit="K"/>umo
      <sncref/></ref>:
      <ekz>
        &ccirc;u mi devas anka&ubreve; la domon fordoni al la <tld/>o?
        <fnt><bib>Metrop</bib> <lok>&ccirc;ap. 5</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_a;&c_m;&c_u;&c_n;&c_a;</trd>,
      <trd>&c_g;&c_r;&c_a;&c_m;&c_a;&c_d;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>gemeenschap</trd>,
      <trd>gemeente</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>gmina</trd>,
      <trd>komuna</trd>
    </trdgrp>
  </snc>
