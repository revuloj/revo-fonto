    <trd>obec</trd>,
    <trd>osada</trd>
  </trdgrp>
</drv>

<drv mrk="komun.0e">
  <kap><tld/>e</kap>
  <dif>
    En maniero <tld/>a:
  </dif>
  <snc ref="komun.0a.rilata_al_pluraj">
    <dif>
      <ekz>
        <ind>amo kaj puno lo&gcirc;as <tld/>e</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="fr">qui aime bien <ind>ch&acirc;tie</ind> bien</trd>
      </ekz>
      <ekz>
        se krom la naski&gcirc;urbo mi ion ajn <tld/>e kun vi
        posedas, certe, ne estas la talent'
        <fnt>
        <aut>Raymond Schwartz</aut>
        <vrk>La Stranga Butiko</vrk>
        <lok>Al Paul Verlaine</lok>
        </fnt>;
      </ekz>
      <ekz>
        nenia ofero estus tro granda, se ni povus per &gcirc;i akiri
        al ni lingvon <ind><tld/>ehoma</ind>n
        <klr>(<tld/>a al la tuta homaro)</klr>
        <fnt><bib>FK</bib> <lok>El la Unua Libro de la lingvo
        Esperanto</lok></fnt>.
        <trdgrp lng="fr">
          <trd>de l'<ind>humanit&eacute;</ind></trd>,
          <trd>commun &agrave; l'<ind>humanit&eacute;</ind></trd>
        </trdgrp>
      </ekz>
    </dif>
    <ref tip="vid" cel="kun.0e">kune</ref>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_z;&c_a;&c_m;</trd>,
      <trd>&c_s;&c_u;&c_m;&c_je;&c_s;&c_n;&c_a;</trd>,
      <trd>&c_s;&c_u;&c_p;&c_o;&c_l;&c_mol;&c_n;&c_a;</trd>
    </trdgrp>
    <trd lng="de">zusammen</trd>
    <trd lng="es">com&uacute;nmente</trd>
    <trd lng="it">comunemente</trd>
    <trdgrp lng="fr">
      <trd>en <ind>commun</ind></trd>,
      <trd>ensemble</trd>
      <trd>collectivement</trd>
    </trdgrp>
    <trd lng="lat">communiter</trd>
    <trd lng="nl">samen</trd>
    <trdgrp lng="pl">
    <trd>wsp&oacute;lnie</trd>,
    <trd>razem</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_m;&c_je;&c_s;&c_t;&c_je;</trd>,
      <trd>&c_s;&c_o;&c_v;&c_m;&c_je;&c_s;&c_t;&c_n;&c_o;</trd>,
      <trd>&c_s;&c_o;&c_o;&c_b;&c_shch;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc ref="komun.0a.rilata_al_plejmulto">
    <dif>
      <ekz>
        la vortaro de lingvo internacia <klr>[...]</klr> devas konsisti
        nepre el vortoj romana-germanaj en ilia plej <tld/>e uzata formo
        <fnt><bib>EE</bib></fnt>;
      </ekz>
      <ekz>
        la arto de verkado <klr>[postulas]</klr> sufi&ccirc;e ampleksan
        sciadon <tld/>e sciencan kaj speciale teknikan
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gxener.0e">&gcirc;enerale</ref>,
      <ref cel="ordina.0e">ordinare</ref>,
      <ref cel="oft.0e">ofte</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_z;&c_v;&c_y;&c_ch;&c_a;&c_j;&c_n;&c_a;</trd>,
      <trd>&c_a;&c_g;&c_u;&c_l;&c_mol;&c_n;&c_a;</trd>,
      <trd>&c_a;&c_g;&c_u;&c_l;&c_a;&c_m;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>commun&eacute;ment</trd>,
      <trd>couramment</trd>,
      <trd>fr&eacute;quemment</trd>,
      <trd>habituellement</trd>,
      <trd>usuellement</trd>
    </trdgrp>
    <trd lng="es">popularmente</trd>
    <trd lng="pl">og&oacute;lnie</trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_y;&c_ch;&c_n;&c_o;</trd>,
      <trd>&c_v;&c_o;&c_o;&c_b;&c_shch;&c_je;</trd>
    </trdgrp>
  </snc>
