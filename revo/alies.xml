    <trd>jin&yacute;ch <klr>(p&rcaron;ivl. z&aacute;jmeno)</klr></trd>
  </trdgrp>
  <trd lng="nl">andermans</trd>
  <trdgrp lng="sk">
    <trd>in&eacute;ho</trd>,
    <trd>in&yacute;ch</trd>
  </trdgrp>
-->
</drv>
</art>

