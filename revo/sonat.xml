<kap>
  <ofc>1</ofc>
  <rad>sonat</rad>/o
</kap>
<drv mrk="sonat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="kompon.0ajxo">Kompona&jcirc;o</ref>
      por unu muzikilo a&ubreve; eta bando
      konsistanta el tri a&ubreve; kvar partoj kun
      kontrastaj rapidoj:
      <ekz>
        solo-<tld/>o por violon&ccirc;elo
        <fnt>
          <bib>Monato</bib>
          <aut>Sen Rodin (Filippo Franceschi)</aut>
          <vrk><url ref="&Monato;/2007/009131.php">Tri diskoj kun ses paroladoj de Ivo
          Lapenna</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        dum pa&ubreve;zeto inter la partoj de <tld/>o <klr>[&#x2026;]</klr> posedanto de <esc>po-to</esc> povas
        interrompi la silenton per la alta &leftquot;<esc>didelit</esc>&rightquot; a&ubreve; per aliaj belsonaj melodioj
        <fnt>
          <aut>Astrid Hanke</aut>
            <vrk><url ref="&Monato;/2000/007153.php">La benoj de po&scirc;telefono</url></vrk>
            <bib>Monato</bib>
            <lok>2000/11, p. 30</lok>
          </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="simfon.0o">simfonio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_n;&c_a;&c_t;&c_a;</trd>
  <trd lng="bg">&c_s;&c_o;&c_n;&c_a;&c_t;&c_a;</trd>
  <trd lng="ca">sonata</trd>
  <trd lng="cs">son&aacute;ta</trd>
  <trd lng="de">Sonate</trd>
  <trd lng="en">sonata</trd>
  <trdgrp lng="fr">
    <trd>sonate</trd>
  </trdgrp>
  <trd lng="hu">szon&aacute;ta</trd>
  <trd lng="it">sonata</trd>
  <trdgrp lng="ja">
    <trd>&#x30BD;&#x30CA;&#x30BF;</trd>,
    <trd>&#x594F;&#x9CF4;&#x66F2;  <pr>&#x305D;&#x3046;&#x3081;&#x3044;&#x304D;&#x3087;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">sonate</trd>
  <trd lng="pl">sonata</trd>
  <trd lng="ru">&c_s;&c_o;&c_n;&c_a;&c_t;&c_a;</trd>
  <trd lng="sk">son&aacute;ta</trd>
  <trd lng="uk">&c_s;&#x43E;&#x43D;&c_a;&c_t;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x594F;&#x9CF4;&#x66F2; <pr>z&#xF2;um&#xED;ngq&#x1D4;</pr></trd>,
    <trd>&#x594F;&#x9E23;&#x66F2; <pr>z&#xF2;um&#xED;ngq&#x1D4;</pr></trd>
  </trdgrp>
</drv>
</art>
