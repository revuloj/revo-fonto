<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: monume.xml,v 1.26 2026/02/12 20:53:30 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>monument</rad>/o <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="monume.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="monume.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="super" cel="konstr.0ajxo.domo">Konstrua&jcirc;o</ref> grava,
      impona, ofte memoriga pri iu a&ubreve; io:
      <ekz>
        <nom>Jakob</nom> starigis <tld/>on super &scirc;ia tombo
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 35:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj de la roma pasinteco kaj de la roma gloro
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XLVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tombo de <nom>Michel-Angelo</nom>, sur la <tld/>o trovi&gcirc;as lia busto kaj anka&ubreve; tri
        figuroj: la Skulpturo, la Pentrarto kaj la Ar&hcirc;itekturo
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        Pu&scirc;kin-<tld/>o staras en la ombro
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="memor.0signo">memorsigno</ref>,
      <ref cel="statu.0o">statuo</ref>
    </refgrp>
  </snc>
  <snc mrk="monume.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Da&ubreve;re grava <ref tip="super" cel="verk.0o.propra">verko</ref>:
      <ekz>
        vi jam konstruis al vi per la versa&jcirc;oj <tld/>on
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXX</lok>
        </fnt>;
      </ekz>
      <ekz>
        per sia lingvo-verko <nom>Zamenhof</nom> konstruis al si mem kaj al sia vivlaboro
        neforigeblan <tld/>on en la historio de la homara evoluo
        <fnt>
          <bib>Zam</bib>
          <lok>Rolo de Zamenhof en la internacilingva movado</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_n;&c_u;&c_m;&c_e;&c_n;&c_t;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_m;&c_o;&c_n;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_je;&c_t;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="bo">&t_da;&t_rata;&t_na;&t_tsheg;&t_ga;&t_sa;&t_naro;&t_-a;&t_gigu;&t_tsheg;&t_ra;&t_tata;&t_drengbu;&t_na;&t_tsheg;</trd>
  <trd lng="ca">monument</trd>
  <trdgrp lng="cs">
    <trd>monument</trd>,
    <trd>pam&aacute;tnost</trd>,
    <trd>pomn&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Monument</trd>,
    <trd>Denkmal</trd>
  </trdgrp>
  <trd lng="en">monument</trd>
  <trd lng="es">monumento</trd>
  <trd lng="fr">monument</trd>
  <trd lng="id">monumen</trd>
  <trdgrp lng="ja">
    <trd>&#x8A18;&#x5FF5;&#x7891;  <pr>&#x304D;&#x306D;&#x3093;&#x3072;</pr></trd>,
    <trd>&#x8A18;&#x5FF5;&#x5EFA;&#x9020;&#x7269;  <pr>&#x304D;&#x306D;&#x3093;&#x3051;&#x3093;&#x305E;&#x3046;&#x3076;&#x3064;</pr></trd>,
    <trd>&#x91CD;&#x8981;&#x306A;&#x907A;&#x7269;  <pr>&#x3058;&#x3085;&#x3046;&#x3088;&#x3046;&#x306A;&#x3044;&#x3076;&#x3064;</pr></trd>,
    <trd>&#x8A18;&#x5FF5;&#x7269;  <pr>&#x304D;&#x306D;&#x3093;&#x3076;&#x3064;</pr></trd>,
    <trd>&#x4E0D;&#x673D;&#x306E;&#x696D;&#x7E3E;  <pr>&#x3075;&#x304D;&#x3085;&#x3046;&#x306E;&#x304E;&#x3087;&#x3046;&#x305B;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="nl">monument</trd>
  <trd lng="pl">pomnik</trd>
  <trd lng="pt">monumento</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_m;&c_ja;&c_t;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_m;&c_o;&c_n;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pam&auml;tn&iacute;k</trd>,
    <trd>pomn&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_a;&c_m;&#x2019;&c_ja;&c_t;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_m;&c_o;&c_n;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7D00;&#x5FF5;&#x7891; <pr>j&igrave;ni&agrave;nb&emacron;i</pr></trd>,
    <trd>&#x7EAA;&#x5FF5;&#x7891; <pr>j&igrave;ni&agrave;nb&emacron;i</pr></trd>,
    <trd>&#x7891; <pr>b&emacron;i</pr></trd>,
    <trd>&#x4E30;&#x7891; <pr>f&emacron;ngb&emacron;i</pr></trd>,
    <trd>&#x8C50;&#x7891; <pr>f&emacron;ngb&emacron;i</pr></trd>,
    <trd>&#x7D00;&#x5FF5;&#x50CF; <pr>j&igrave;ni&agrave;nxi&agrave;ng</pr></trd>,
    <trd>&#x7EAA;&#x5FF5;&#x50CF; <pr>j&igrave;ni&agrave;nxi&agrave;ng</pr></trd>,
    <trd>&#x7D00;&#x5FF5;&#x96D5;&#x50CF; <pr>j&igrave;ni&agrave;ndi&amacron;oxi&agrave;ng</pr></trd>,
    <trd>&#x7EAA;&#x5FF5;&#x96D5;&#x50CF; <pr>j&igrave;ni&agrave;ndi&amacron;oxi&agrave;ng</pr></trd>,
    <trd>&#x8FFD;&#x601D; <pr>zhu&imacron;s&imacron;</pr></trd>,
    <trd>&#x8FFD;&#x60BC; <pr>zhu&imacron;d&agrave;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="monume.tombo0o">
  <kap>tombo<tld/>o, <var><kap>tomba <tld/>o</kap></var></kap>
  <snc mrk="monume.tombo0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Luksa <ref tip="super" cel="konstr.0ajxo.domo">konstrua&jcirc;o</ref>
      &ccirc;irka&ubreve; la <ref tip="dif" cel="tomb.0o">tombo</ref> de
      mortinto:
      <ekz>
        tio estas la tomba <tld/>o de <nom>Ra&hcirc;el</nom> &gcirc;is la nuna tago
        <fnt><bib>MT</bib>, <lok>&Gen; 35:20</lok></fnt>;
      </ekz>
      <ekz>
        la filmo montras la tombo<tld/>on de <nom>Mwl Nyerere</nom>
        <fnt>
          <aut>N. Vessella</aut>,
          <vrk><url ref="http://www.vessella.it/video.htm">
          Changamano</url></vrk>,
          <lok>1996-1999</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="mauxzo.0o.ARKI">ma&ubreve;zoleo</ref>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_w;&c_z;&c_a;&c_l;&c_je;&c_j;</trd>
  <trd lng="bg">&c_m;&c_a;&c_v;&c_z;&c_o;&c_l;&c_je;&c_j;</trd>
  <trd lng="br">mozoleon</trd>
  <trd lng="ca">mausoleu</trd>
  <trd lng="de">Mausoleum</trd>
  <trd lng="en">mausoleum</trd>
  <trd lng="es">mausoleo</trd>
  <trd lng="fr">mausol&eacute;e</trd>
  <trd lng="hu">mauz&oacute;leum</trd>
  <trd lng="id">mausoleum</trd>
  <trd lng="nl">mausoleum</trd>
  <trdgrp lng="pl">
    <trd>pomnik <klr>(pot.)</klr></trd>,
    <trd>nagrobek</trd>
  </trdgrp>
  <trd lng="pt">mausol&eacute;u</trd>
  <trd lng="ru">&c_m;&c_a;&c_v;&c_z;&c_o;&c_l;&c_je;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x9675;&#x5893; <pr>l&iacute;ngm&ugrave;</pr></trd>,
    <trd>&#x9675; <pr>l&iacute;ng</pr></trd>,
    <trd>&#x5893; <pr>m&ugrave;</pr></trd>,
    <trd>&#x7D00;&#x5FF5;&#x5802; <pr>j&igrave;ni&agrave;nt&aacute;ng</pr></trd>,
    <trd>&#x7EAA;&#x5FF5;&#x5802; <pr>j&igrave;ni&agrave;nt&aacute;ng</pr></trd>
  </trdgrp>
</drv>
</art>
