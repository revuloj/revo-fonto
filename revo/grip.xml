  <trd lng="cs">ch&rcaron;ipka</trd>
  <trdgrp lng="de">
     <trd>Grippe</trd>,
     <trd>Kopfgrippe</trd>,
     <trd>Virusgrippe</trd>
  </trdgrp>
  <trd lng="en">the <ind>flu</ind></trd>
  <trd lng="es">gripe</trd>
  <trd lng="fr">grippe</trd>
  <trd lng="nl">griep</trd>
  <trd lng="ru">&c_g;&c_r;&c_i;&c_p;&c_p;</trd>
</drv>
</art>
