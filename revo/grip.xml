<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>grip</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="grip.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Febra epidemia <ref tip="super" cel="san.mal0o">malsano</ref>, karakterizata per kapdoloro,
      lacegeco en &ccirc;iuj membroj kaj nazkataro.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_y;&c_p;</trd>
  <trd lng="cs">ch&rcaron;ipka</trd>
  <trdgrp lng="de">
     <trd>Grippe</trd>,
     <trd>Kopfgrippe</trd>,
     <trd>Virusgrippe</trd>
  </trdgrp>
  <trd lng="en">the <ind>flu</ind></trd>
  <trd lng="es">gripe</trd>
  <trd lng="fr">grippe</trd>
  <trd lng="hu">influenza</trd>
  <trd lng="nl">griep</trd>
  <trd lng="pt">gripe</trd>
  <trd lng="ru">&c_g;&c_r;&c_i;&c_p;&c_p;</trd>
</drv>
</art>
