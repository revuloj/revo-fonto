<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: katar.xml,v 1.36 2025/10/20 03:25:18 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>katar</rad>/o <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="katar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="katar.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Muka fluo pro <ref tip="lst" cel="inflam.0o" lst="voko:malsanoj">inflamo</ref> de muka
      membrano:
      <ekz>
        kronika <tld/>o de la okuloj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        li havis <tld/>on, teruran <tld/>on, tiel ke li prefere povus uzi litotukon
        anstata&ubreve; po&scirc;tukon
        <fnt>
          <bib>Fab4</bib>
          <lok>Dekdu per la po&scirc;to</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="varm.mal0umi">malvarmumi</ref>
    </refgrp>
  </snc>
  <refgrp tip="hom">
    <ref cel="katar1.0o">Kataro (Araba &scirc;tato)</ref>,
    <ref cel="katar2.0o">kataro (albigenso)</ref>
  </refgrp>
  <trd lng="be">&c_k;&c_a;&c_t;&c_a;&c_r; <klr tip="amb">(&c_h;&c_v;&c_a;&c_r;&c_o;&c_b;&c_a;)</klr></trd>
  <trdgrp lng="bg">
    <trd>&c_k;&c_a;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_k;&c_a;&c_t;&c_a;&c_r;&c_a;&c_l;&c_n;&c_o;
    &c_v;&c_malmol;&c_z;&c_p;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>katar</trd>,
    <trd>z&aacute;n&ecaron;t sliznice</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Katarrh</trd>,
    <trd>Schleimhautentz&uuml;ndung</trd>
  </trdgrp>
  <trd lng="en">catarrh</trd>
  <trd lng="es">catarro</trd>
  <trd lng="fa">&a_A_madda;&a_b;&a_r;&f_y;&a_z;&a_sh;</trd>
  <trd lng="fr">catarrhe<klr tip="ind"> (de muqueuse)</klr></trd>
  <trd lng="hu">hurut</trd>
  <trd lng="id">radang selaput lendir</trd>
  <trd lng="ja">&#12459;&#12479;&#12523;</trd>
  <trdgrp lng="nl">
    <trd>catarre</trd>,
    <trd>slijmvliesontsteking</trd>
  </trdgrp>
  <trd lng="pl">katar</trd>
  <trd lng="ro">guturai</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_v;&c_o;&c_s;&c_p;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je;
    &c_s;&c_l;&c_i;&c_z;&c_i;&c_s;&c_t;&c_o;&c_j;
    &c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>katar</trd>,
    <trd>z&aacute;pal sliznice</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;&c_je;&c_n;&c_n;&c_ja; (&c_s;&c_l;&c_i;&c_z;&c_o;&c_v;&c_o;&c_ji;
    &c_o;&c_b;&c_o;&c_l;&c_o;&c_n;&c_k;&c_i;)</trd>
  </trdgrp>
  <trd lng="zh">&#31896;&#33180;&#28814; <pr>ni&aacute;nm&oacute;y&aacute;n</pr></trd>
</drv>

<drv mrk="katar.naz0o">
  <kap>naz<tld/>o</kap>
  <snc mrk="katar.naz0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <tld lit="K"/>o de la nazo:
      <ekz>
        aperas febro, konjunktivito, dolora gor&gcirc;o, naz<tld/>o, tusado kaj na&ubreve;zo
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2012/010629.php">2020: la mondo sen morbilo?</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ne tusas, &scirc;i ne havas naz<tld/>on, tio estas tute ne malsano en medicina senco
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Destino de Atrejo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_m;&c_a;&c_r;&c_k;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_m;&c_a;&c_r;&c_g;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_h;&c_r;&c_je;&c_m;&c_a;</trd>
  <trd lng="br">friad<klr tip="ind"> (kle&ntilde;ved)</klr></trd>
  <trd lng="ca">rinitis</trd>
  <trdgrp lng="cs">
    <trd>katar nosn&iacute; sliznice</trd>,
    <trd>r&yacute;ma</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schnupfen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>catarrh <klr>(of the nose)</klr></trd>,
    <trd>coryza</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_z;&f_k;&a_A;&a_m;</trd>,
    <trd>&a_A_madda;&a_b;&a_r;&f_y;&a_z;&a_sh;
    &a_b;&f_y;&a_n;&f_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>coryza</trd>,
    <trd>rhinite</trd>,
    <trd>rhume de cerveau</trd>
  </trdgrp>
  <trd lng="hu">n&aacute;tha</trd>
  <trdgrp lng="id">
    <trd>radang selaput lendir</trd>,
    <trd>rinitis</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#40763;&#12459;&#12479;&#12523;  <pr>&#12403;&#12459;&#12479;&#12523;</pr></trd>,
    <trd>&#40763;&#28814;  <pr>&#12403;&#12360;&#12435;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>neuscatarre</trd>,
    <trd>loopneus</trd>,
    <trd>druipneus</trd>,
    <trd>rinitis</trd>,
    <trd>rhinitis</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>katar</trd>,
    <trd>nie&zdot;yt nosa</trd>
  </trdgrp>
  <trd lng="ro">rinit&abreve;</trd>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_m;&c_o;&c_r;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>katar nosnej sliznice</trd>,
    <trd>n&aacute;dcha</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#40763;&#28814; <pr>b&iacute;y&aacute;n</pr></trd>,
    <trd>&#20260;&#39118; <pr>sh&amacron;ngf&emacron;ng</pr></trd>
  </trdgrp>
</drv>
</art>
