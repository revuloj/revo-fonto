<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>katar</rad>/o
</kap>
<drv mrk="katar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Muka fluo pro inflamo de muka membrano:
      <ekz>
        <tld/>o de l' nazo, de l' intesto, ktp.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="varm.mal0umi">malvarmumi</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_a;&c_r; <klr tip="amb">(&c_h;&c_v;&c_a;&c_r;&c_o;&c_b;&c_a;)</klr></trd>
  <trdgrp lng="de">
    <trd>Katarrh</trd>,
    <trd>Schleimhautentz&uuml;ndung</trd>
  </trdgrp>
  <trd lng="en">cattarh</trd>
  <trd lng="es">catarro</trd>
  <trd lng="fr">catarrhe<klr tip="ind"> (de muqueuse)</klr></trd>
  <trd lng="hu">hurut</trd>
  <trd lng="nl">verkoudheid</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_v;&c_o;&c_s;&c_p;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je; &c_s;&c_l;&c_i;&c_z;&c_i;&c_s;&c_t;&c_o;&c_j; &c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="katar.naz0o">
  <kap>naz<tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <tld lit="K"/>o de la nazo.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_m;&c_a;&c_r;&c_k;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_m;&c_a;&c_r;&c_g;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schnupfen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>catarrhe<klr tip="ind"> (du nez)</klr></trd>,
    <trd>rhume</trd>,
    <trd>coryza</trd>
  </trdgrp>
  <trd lng="hu">n&aacute;tha</trd>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_m;&c_o;&c_r;&c_k;</trd>
</drv>
</art>
