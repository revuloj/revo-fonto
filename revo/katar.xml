<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>katar</rad>/o
</kap>
<drv mrk="katar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Muka fluo pro inflamo de muka membrano:
      <ekz><tld/>o de l' nazo, de l' intesto, ktp.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="varm">malvarmumo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Katarrh</trd>,
     <trd>Schleimhautentz&uuml;ndung</trd>
  </trdgrp>
  <trd lng="en">cattarh</trd>
  <trd lng="nl">verkoudheid</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_v;&c_o;&c_s;&c_p;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je; &c_s;&c_l;&c_i;&c_z;&c_i;&c_s;&c_t;&c_o;&c_j; &c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_i;</trd>
  </trdgrp>
</drv>
<drv mrk="katar.naz0o">
  <kap>naz<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="K"/>o de la nazo.
    </dif>
  </snc>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_m;&c_o;&c_r;&c_k;</trd>
</drv>
</art>
