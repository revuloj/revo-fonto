<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: miaux.xml,v 1.22 2024/08/06 20:11:23 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>mia&ubreve;</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="miaux.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="miaux.0.SON">
    <dif>
      <ref tip="lst" cel="blek.0o" lst="voko:blekoj">Bleko</ref>
      de <ref tip="vid" cel="kat.0o.ZOO">kato</ref>:
      <ekz>
        <tld/>! <tld/>! Kiu <tld/>as post la pordo?
        <fnt>
          <aut>L. Jevsejeva, [trad. A. Fighiera-Sikorska]</aut>,
          <vrk><url ref="http://www.literaturo.dk/anguleto.htm">
          Malvarme</url></vrk>,
          <lok>Heroldo de Esperanto, 1970-01-16 (1482)</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">meu</trd>
  </snc>
  <trd lng="be">&c_m;&c_ja;&c_w;!</trd>
  <trd lng="fr">miaou !</trd>
  <trd lng="id"><ind>meong</ind>!</trd>
  <trd lng="zh">&#21941;&#21941; <pr>mi&amacron;omi&amacron;o</pr></trd>
</drv>

<drv mrk="miaux.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="miaux.0i.SON">
    <dif>
      <ref tip="super" cel="blek.0i.beste">Bleki</ref> kiel
      <ref tip="vid" cel="kat.0o">kato</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ja;&c_w;&c_k;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">miolar</trd>
  <trd lng="de">miauen</trd>
  <trdgrp lng="en">
    <trd>mew</trd>,
    <trd>meow</trd>
  </trdgrp>
  <trd lng="es">maullar</trd>
  <trd lng="fr">miauler</trd>
  <trd lng="he">&lamed;<ind>&yod;&lamed;&lamed;</ind> &kaf;&het;&tav;&vav;&lamed;</trd>
  <trd lng="id"><baz>meong</baz>mengeong</trd>
  <trd lng="nl">miauwen</trd>
  <trdgrp lng="pt">
   <trd>miar</trd>,
   <trd>dar <ind>miados</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_ja;&c_u;&c_k;&c_a;&c_t;&c_mol;</trd>
  <trd lng="uk">&c_n;&c_ja;&c_v;&c_k;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#21941; <pr>mi&amacron;o</pr></trd>,
    <trd>&#29483;&#21483;&#22768; <pr>m&amacron;oji&agrave;osh&emacron;ng</pr></trd>,
    <trd>&#35987;&#21483;&#32882; <pr>m&amacron;oji&agrave;osh&emacron;ng</pr></trd>,
    <trd>&#21674;&#21674; <pr>m&#299;m&#299;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="miaux.0o">
  <kap><tld/>o</kap>
  <snc mrk="miaux.0o.SON">
    <dif>
      Kata bleko a&ubreve; simila <ref tip="lst" cel="son.0o" lst="voko:sonoj">sono</ref>:
      <ekz>
        tiel grandan bruon faris <klr>...</klr> la <tld/>oj de la katoj
        <fnt><bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 46a, p.
        649a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ja;&c_w;&c_k;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="ca">
    <trd>miol</trd>,
    <trd>meu</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>mew</trd>,
    <trd>meow</trd>
  </trdgrp>
  <trd lng="es">maullido</trd>
  <trd lng="fr">miaulement</trd>
  <trd lng="id"><ind>meong</ind>an</trd>
  <trdgrp lng="uk">
    <trd>&c_n;&c_ja;&c_v;&c_k;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_n;&c_ja;&c_v;&c_ch;&c_a;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
  <trd lng="zh">&#21941; <pr>mi&amacron;o</pr></trd>
</drv>
</art>
