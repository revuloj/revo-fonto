<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: omen.xml,v 1.17 2023/04/18 09:26:22 revo Exp $">
<kap>
  <rad>omen</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="omen.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Spontanee aperanta <ref tip="super" cel="auxgur.0o">a&ubreve;guro</ref>,
      pliofte malfavora:
      <ekz>
        &ccirc;i <tld/>oj, <klr>[&#8230;]</klr> la&ubreve; mia modesta opinio, ne havas
        pli da rilato kun nia sorto, ol la pasint-jaraj nuboj
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 73a, p.
        803a</lok></fnt>;
      </ekz>
      <ekz>
        la nomo de erao estis &scirc;an&gcirc;ita tute kaprice,
        kiam kataklismo plagis la popolon, <klr>[&#8230;]</klr> por ekzorci la landon
        pro malbona <tld/>o
        <fnt>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url ref="&Monato;/2000/006852.php">&Ccirc;u vere kristana erao?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/04, p. 17</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_n;&c_a;&c_k;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_d;&c_v;&c_je;&c_s;&c_mol;&c_c;&c_je; <klr>(&c_z;&c_v;&c_y;&c_ch;&c_a;&c_j;&c_n;&c_a;
    &c_n;&c_ja;&c_d;&c_o;&c_b;&c_r;&c_a;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="bg">&c_z;&c_n;&c_a;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;edzv&ecaron;st</trd>,
    <trd>v&ecaron;&scaron;tba</trd>,
    <trd>znamen&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Omen</trd>,
    <trd>Vorzeichen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>pr&eacute;sage<klr> (d&eacute;favorable)</klr></trd>,
    <trd>signe pr&eacute;monitoire</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>int&odblac; jel</trd>,
    <trd>rosz el&odblac;jel</trd>,
    <trd>&oacute;men</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>omen</trd>,
    <trd>voorteken</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>omen</trd>,
    <trd>zapowied&zacute;</trd>,
    <trd>z&lstroke;y znak</trd>
  </trdgrp>
  <trd lng="pt">mau <ind>agouro</ind></trd>
  <trdgrp lng="ro">
    <trd>augur</trd>,
    <trd>anun&#539;</trd>,
    <trd>semn r&abreve;u</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_z;&c_n;&c_a;&c_m;&c_je;&c_n;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;<klr> (&c_o;&c_b;&c_y;&c_ch;&c_n;&c_o;
  &c_n;&c_je;&c_d;&c_o;&c_b;&c_r;&c_o;&c_je;)</klr></trd>
  <trdgrp lng="sk">
    <trd>predzves&tcaron;</trd>,
    <trd>ve&scaron;tba</trd>,
    <trd>znamenie</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>omen</trd>,
    <trd>j&auml;rtecken</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_je;&c_r;&c_je;&c_d;&c_v;&c_ib;&c_s;&c_t;&#x44F;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_je;&c_d;&c_v;&c_ib;&c_s;&c_t;&#x43A;&c_a;</trd>,
    <trd>&#x43F;&c_r;&c_i;&c_z;&c_v;&c_ib;&c_s;&c_t;&#x43A;&c_a;</trd>,
    <trd>&#x43E;&c_z;&#x43D;&c_a;&#x43A;&c_a;</trd>,
    <trd>&#x43F;&c_r;&c_i;&#x43A;&#x43C;&c_je;&c_t;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="omen.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      A&ubreve;gura, <ref cel="signif.0a.FIG">signifoplena</ref>:
      <ekz>
        vi flegas kulturon per via aktivado por renaski&gcirc;o de e&ubreve;ropa kulturo <klr>[&#8230;]</klr>
        via apero en &ccirc;i e&ubreve;ropa kaj epoka trapasejo en E&ubreve;ropa Lingvojaro estas <tld/>a
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#8470; 2 (100)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sinistr.0a">sinistra</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_n;&c_ja;&c_d;&c_o;&c_b;&c_r;&c_y;</trd>,
      <trd>&c_n;&c_je;&c_s;&c_p;&c_r;&c_y;&c_ja;&c_l;&c_mol;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="pt">agourento</trd>
    <trdgrp lng="fr">
      <trd>de mauvais <ind>augure</ind></trd>,
      <trd>d&eacute;favorable<klr> (signe)</klr></trd>,
      <trd>n&eacute;faste<klr> (signe)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>balj&oacute;s</trd>,
      <trd>rosszat sejtet&odblac;</trd>
    </trdgrp>
    <trd lng="nl">nefast</trd>
    <trdgrp lng="pl">
      <trd>z&lstroke;owieszczy</trd>,
      <trd>z&lstroke;owr&oacute;&zdot;bny</trd>
    </trdgrp>
    <trd lng="ro">de r&abreve;u augur</trd>
    <trdgrp lng="ru">
      <trd>&c_n;&c_je;&c_b;&c_l;&c_a;&c_g;&c_o;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_d;&c_o;&c_b;&c_r;&c_y;&c_j;</trd>,
      <trd>&c_p;&c_l;&c_o;&c_h;&c_o;&c_j;<klr tip="amb"> (&c_o;
      &c_p;&c_r;&c_je;&c_d;&c_z;&c_n;&c_a;&c_m;&c_je;&c_n;&c_o;&c_v;&c_a;&c_n;&c_i;&c_i;)</klr></trd>
    </trdgrp>
  </snc>

<trdgrp lng="de">
  <trd>omin&ouml;s</trd>,
  <trd>bedeutsam</trd>,
  <trd>verhei&szlig;ungsvoll</trd>,
  <trd>unheilvoll</trd>
</trdgrp></drv>
</art>

