<drv mrk="efekti.0e">
  <kap><tld/>e</kap>
  <snc mrk="efekti.0e.KOMUNE">
    <dif>
      Ver<ref tip="sin" cel="fakt.0e">fakte</ref>, reale:
      <ekz>
        mi dubas, &ccirc;u li estas <tld/>e rajtigita;
      </ekz>
      <ekz>
        &ccirc;u vi estos <tld/>e sincera, fra&ubreve;lino
        <fnt>
          <bib>FK</bib>
        </fnt>?
      </ekz>
      <ekz>
        <tld/>e &scirc;i estas mia fratino, filino de mia patro, nur ne filino de mia patrino
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 20:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi <tld/>e plenumas viajn promesojn
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 44:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio <tld/>e estas dekretita, kiel neforigebla le&gcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 6:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj opiniis, ke Johano <tld/>e estis profeto
        <fnt>
          <bib>NT</bib>
          <lok>Marko 11:32</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u <tld/>e Dio lo&gcirc;us kun homoj sur la tero
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 6:18</lok>
        </fnt>?
      </ekz>
      <ekz>
        punkto, &scirc;ajne fantazia, sed <tld/>e tre grava
        <fnt>
          <bib>DL</bib>
        </fnt>.
      </ekz>
    </dif>    
    <trd lng="ca">en efecte</trd>
    <trdgrp lng="pl">
      <trd>prawdziwie</trd>,
      <trd>faktycznie</trd>,
      <trd>rzeczywi&sacute;cie</trd>,
      <trd>w&lstroke;a&sacute;ciwie</trd>
    </trdgrp>
  </snc>
  <snc mrk="efekti.0e.ARG">
    <dif>
      Kiel montras la fakto ke...,
      kion pruvas tio ke...:
      <!--
      anta&ubreve;a difino:
      Adverbo anoncanta, ke la sekvanta frazo konfirmas
      a&ubreve; motivas la anta&ubreve;an &jcirc;usan aserton
      -->
      <ekz>
        <nom>Volap&uuml;k</nom> ne povis sukcesi,
        <tld/>e &gcirc;i postulis tro grandan penadon por la memoro
        <fnt><bib>PV</bib></fnt>
      </ekz>
      <ekz>
        la okuloj de l' instruitulo tute senvidi&gcirc;is <klr>[kio]</klr> ne estas miro, &ccirc;ar
        li ilin <tld/>e tro forte malfermis
        <fnt>
          <bib>DL</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxar.0">&ccirc;ar</ref>,
      <ref cel="pruv.0e">pruve</ref>,
      <ref cel="dir.ver0e">verdire</ref>
    </refgrp>
    <trd lng="ca">de fet</trd>
    <trdgrp lng="pl">
      <trd><klr>na </klr>serio</trd>,
      <trd>w rzeczywisto&sacute;ci</trd>,
      <trd>na <ind>dobre</ind></trd>,
      <trd>zaiste</trd>
    </trdgrp>
  </snc>
  <ref tip="sin" cel="ja.0">ja</ref>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_p;&c_r;&c_a;&c_w;&c_d;&c_y;</trd>,
    <trd>&c_n;&c_a; &c_s;&c_a;&c_m;&c_o;&c_j;
    &c_s;&c_p;&c_r;&c_a;&c_v;&c_je;</trd>
  </trdgrp>
  <trd lng="ca">efectivament</trd>
  <trdgrp lng="cs">
    <trd>doopravdy</trd>,
    <trd>efektivn&ecaron;</trd>,
    <trd>opravdu</trd>,
    <trd>skute&ccaron;n&ecaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wirklich</trd>,
    <trd>tats&auml;chlich</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>actually</trd>,
    <trd>in fact</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>effectivement</trd>,
    <trd>en <ind>effet</ind></trd>,
    <trd>r&eacute;ellement</trd>,
    <trd>vraiment</trd></trdgrp>
  <trd lng="he">&#x5DC;&#x5DE;&#x5E2;&#x5E9;&#x5D4;</trd>
  <trdgrp lng="hu">
    <trd>t&eacute;nylegesen</trd>,
    <trd>val&oacute;j&aacute;ban</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5B9F;&#x969B;&#x306B;  <pr>&#x3058;&#x3063;&#x3055;&#x3044;&#x306B;</pr></trd>,
    <trd>&#x73FE;&#x5B9F;&#x306B;  <pr>&#x3052;&#x3093;&#x3058;&#x3064;&#x306B;</pr></trd>
  </trdgrp>
  <trd lng="nl">inderdaad</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;</trd>,
    <trd>&c_v; &c_s;&c_a;&c_m;&c_o;&c_m; &c_d;&c_je;&c_l;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>efekt&iacute;vne</trd>,
    <trd>skuto&ccaron;ne</trd>
  </trdgrp>
  <trd lng="sv">verkligen</trd>
  <trd lng="sw">kwa kusema kweli</trd>
  <trdgrp lng="uk">
    <trd>&c_je;&c_f;&c_je;&#x43A;&c_t;&c_i;&c_v;&#x43D;&#x43E;</trd>,
    <trd>&c_d;&c_ib;&c_j;&c_s;&#x43D;&#x43E;</trd>,
    <trd>&c_s;&#x43F;&c_r;&c_a;&c_v;&c_d;&c_ib;</trd>
  </trdgrp>
  <trd lng="zh">&#x5012;&#x662F; <pr>d&#xE0;osh&#xEC;</pr></trd>
</drv>

<drv mrk="efekti.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="efekti.0ajxo.KOMUNE">
    <dif>
      Ia <ref tip="sin" cel="real.0ajxo">reala&jcirc;o</ref>:
      <ekz>
        anstata&ubreve; miaj belaj son&gcirc;oj mi ekvidis teruran <tld/>a&jcirc;on: en la stratoj de mia
        malfeli&ccirc;a urbo de naski&gcirc;o sova&gcirc;aj homoj kun hakiloj kaj feraj stangoj sin
        &jcirc;etis kiel plej kruelaj bestoj kontra&ubreve; trankvilaj lo&gcirc;antoj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Dua Kongreso Esperantista en Gen&egrave;ve en la 28a de a&ubreve;gusto 1906</vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ne sciis, &ccirc;u &scirc;i nur son&gcirc;is a&ubreve; &ccirc;u tio estis <tld/>a&jcirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        decidoj antikvaj fari&gcirc;is <tld/>a&jcirc;oj kaj vera&jcirc;oj
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 25:1</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="fakt.0o">fakto</ref>
    <ref tip="ant" cel="imag.0ajxo.nereala">imaga&jcirc;o<sncref/></ref>
    <refgrp tip="vid">
      <ref cel="real.0a">reala</ref>,
      <ref cel="ver.0a">vera</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_ch;&c_a;&c_ib;&c_s;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>fet</trd>,
    <trd>realitat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Tatsache</trd>,
    <trd>Wirklichkeit</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>reality</trd>,
    <trd>fact</trd>
  </trdgrp>
  <trd lng="fr">r&eacute;alit&eacute;</trd>
  <trd lng="hu">val&oacute;s&aacute;g</trd>
  <trd lng="ja">&#x73FE;&#x5B9F;  <pr>&#x3052;&#x3093;&#x3058;&#x3064;</pr></trd>
  <trdgrp lng="pl">
    <trd>fakt</trd>,
    <trd>rzeczywisto&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_je;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5B9E;&#x60C5; <pr>sh&#xED;q&#xED;ng</pr></trd>,
    <trd>&#x5B9E;&#x9645;&#x60C5;&#x51B5; <pr>sh&#xED;j&#xEC;q&#xED;ngku&#xE0;ng</pr></trd>
