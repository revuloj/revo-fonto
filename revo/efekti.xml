<drv mrk="efekti.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="efekti.0igi.KOMUNE">
    <dif>
      <ref tip="dif" cel="real.0igi">Realigi</ref>, <ref cel="plen.0umi">plenumi</ref>:
      <ekz>
        <tld/>igi intencon, ideon, planon la pacon;
      </ekz>
      <ekz>
        la le&gcirc;o, kiu venis kvarcent tridek jarojn poste, ne <klr>[&#x2026;]</klr> mal<tld/>igus
        la promeson
        <fnt>
          <bib>NT</bib>
          <lok>Galatoj 3:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vivo tro frue komencis kaj rapide <tld/>igis sur &scirc;i la teruran procedon de
        detruado
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i estus originala faro, se oni povus tion &ccirc;i <tld/>igi
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la donita afero estas tre utila kaj samtempe <tld/>igebla
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        ni batalados pro nia ideo tiel longe, &gcirc;is &gcirc;i pli a&ubreve; malpli frue estos plene
        <tld/>igita
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tio estas]</klr> <ind>ne<tld/>igebla</ind> utopio
        <fnt>
          <bib>FK</bib>
          <vrk><url ref="https://eo.m.wikisource.org/wiki/Fundamenta_Krestomatio/El_la_unua_libro_de_la_lingvo_Esperanto">El
          la unua libro de la lingvo ...</url></vrk>
        </fnt>.
        <trd lng="ca">irrealitzable</trd>
        <trdgrp lng="de">
          <trd>nicht <ind>machbar</ind></trd>,
          <trd>unmachbar</trd>,
          <trd>nicht <ind>realisierbar</ind></trd>,
          <trd>unrealisierbar</trd>
        </trdgrp>
        <trd lng="fr">irr&eacute;alisable</trd>
        <trd lng="hu">megval&oacute;s&iacute;thatatlan</trd>
        <trdgrp lng="pl">
          <trd>nieziszczalny</trd>,
          <trd>nieosi&aogonek;galny</trd>
        </trdgrp>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ating.0i">atingi</ref>,
      <ref cel="akir.0i">akiri</ref>,
      <ref cel="far.el0i">elfari</ref>,
      <ref cel="valid.0a">valida</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_zh;&c_y;&c_c;&c_mol;&c_c;&c_ja;&c_v;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_d;&c_z;&c_je;&c_j;&c_s;&c_mol;&c_n;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_r;&c_a;&c_w;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_k;&c_a;&c_n;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_e;&c_a;&c_l;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>dur a terme</trd>,
    <trd>acomplir</trd>,
    <trd>realitzar</trd>,
    <trd>efectuar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>podniknout</trd>,
    <trd>prov&eacute;st</trd>,
    <trd>splnit</trd>,
    <trd>uskute&ccaron;nit</trd>,
    <trd>vykonat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verwirklichen</trd>,
    <trd>realisieren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>realize</trd>,
    <trd>accomplish</trd>,
    <trd>bring about</trd>,
    <trd>commit</trd>
  </trdgrp>
  <trd lng="es">tener efectividad</trd>
  <trdgrp lng="fr">
    <trd>effectuer</trd>,
    <trd>mat&eacute;rialiser</trd>,
    <trd>ex&eacute;cuter</trd>,
    <trd>r&eacute;aliser<klr tip="ind"> (effectuer)</klr></trd></trdgrp>
  <trd lng="he">&#x5DC;&#x5D1;&#x5E6;&#x5E2;</trd>
  <trdgrp lng="hu">
    <trd>megval&oacute;s&iacute;t</trd>,
    <trd>realiz&aacute;l</trd>
  </trdgrp>
  <trd lng="ja">&#x5B9F;&#x73FE;&#x3055;&#x305B;&#x308B;  <pr>&#x3058;&#x3064;&#x3052;&#x3093;&#x3055;&#x305B;&#x308B;</pr></trd>
  <trd lng="nl">realiseren</trd>
  <trdgrp lng="pl">
    <trd>dokonywa&cacute;</trd>,
    <trd>realizowa&cacute;</trd>,
    <trd>zrealizowa&cacute;</trd>,
    <trd>osi&aogonek;ga&cacute;</trd>,
    <trd>sprawia&cacute;</trd>,
    <trd>urzeczywistnia&cacute;</trd>,
    <trd>uskutecznia&cacute;</trd>,
    <trd>zdzia&lstroke;a&cacute;</trd>,
    <trd>ziszcza&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_i;&c_t;&c_mol;</trd>,
