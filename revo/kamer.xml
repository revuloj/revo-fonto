<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kamer</rad>/o
</kap>
<drv mrk="kamer.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Senfenestra &ccirc;ambro:
      <ekz>la porkisto ricevas mizeran <tld/>on apud la porkejo.</ekz>
    </dif>
    <trd lng="nl">kamer <klr tip="ind">(zonder venster)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_a;&c_m;&c_je;&c_r;&c_a;</trd>,
      <trd>&c_ch;&c_u;&c_l;&c_a;&c_n;</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <dif>
      Senluma kesteto: <ekz><tld/>o de fotografaparato.</ekz>
    </dif>
    <trd lng="nl">kamer <klr tip="ind">(van fototoestel)</klr></trd>
    <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_r;&c_a;</trd>
  </snc>
  <snc num="3">
    <dif>
      Organika kavo:
      <ekz>
        <tld/>etoj de la koro
        <klr>(a&ubreve;rikloj kaj ventrikloj)</klr>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="cxel.0o">&ccirc;elo</ref>.
    </refgrp>
    <trd lng="nl">kamer <klr tip="ind">(van hart)</klr></trd>
    <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_r;&c_a;</trd>
  </snc>
</drv>
</art>
