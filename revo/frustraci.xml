<vortaro>
<art mrk="$Id$">
<kap>
  <rad>frustraci</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="frustraci.0i">
  <kap><tld/>i</kap>
  <ref tip="dif" cel="frustr.0i">frustri</ref>
  <trd lng="pt">frustrar</trd>
</drv>

<drv mrk="frustraci.0o">
  <kap><tld/>o</kap>
  <ref tip="dif" cel="frustr.0o">frustro</ref>
  <ekz>
    post longa tempo &scirc;i ellasis malesperan krieton kaj <tld/>e reiris
    en sian &ccirc;ambron
    <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 100a, volumo 3a, p.
    295a</lok></fnt>.
  </ekz>
  <trd lng="pt">frustra&#231;&#227;o</trd>
</drv>
</art>

