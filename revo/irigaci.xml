<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: irigaci.xml,v 1.14 2023/11/19 10:22:14 revo Exp $">
<kap>
  <rad>irigaci</rad>/o
</kap>
<drv mrk="irigaci.0o">
  <kap><tld/>o</kap>
  <snc mrk="irigaci.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Alkonduko de akvo per tuboj, kanaloj ktp, sur tro sekan grundon:
      <ekz>
        in&gcirc;enieroj alportis projektojn de novaj metodoj de l&#x2019; <tld/>o
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 18a</lok></fnt>;
      </ekz>
      <ekz>
        en regiono kun montoj unu apud alia kaj tero sterila malfacilas
        <tld/>o kaj trafiko
        <fnt>
        <aut>&Ccirc;ina Radio Internacia</aut>,
        <vrk><url ref="http://es.chinabroadcast.cn">&Ccirc;inaj
        naciminoritatoj... (4)</url></vrk>,
        <lok>2005-09-19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>  
  <trdgrp lng="be">
    <trd>&c_ib;&c_r;&c_y;&c_g;&c_a;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_a;&c_b;&c_v;&c_a;&c_d;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_a;&c_r;&c_a;&c_sh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="bg">&c_n;&c_a;&c_p;&c_o;&c_ja;&c_v;&c_a;&c_n;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>n&aacute;lev</trd>,
    <trd>oplach</trd>,
    <trd>v&yacute;plach</trd>
  </trdgrp>
  <trd lng="de">Bew&auml;sserung</trd>
  <trd lng="en">irrigation</trd>
  <trd lng="es">irrigaci&oacute;n</trd>
  <trd lng="fr">irrigation</trd>
  <trd lng="hu">&ouml;nt&ouml;z&eacute;s</trd>
  <trdgrp lng="id">
    <trd>irigasi</trd>,
    <trd>peng<ind>air</ind>an</trd>
  </trdgrp>
  <trd lng="ja">&#x704C;&#x6F11;  <pr>&#x304B;&#x3093;&#x304C;&#x3044;</pr></trd>
  <trdgrp lng="nl">
    <trd>irrigatie</trd>,
    <trd>bevloeiing</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>irygacja</trd>,
    <trd>nawodnienie</trd>
  </trdgrp>
  <trd lng="pt">irriga&ccedil;&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_r;&c_r;&c_i;&c_g;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_o;&c_b;&c_v;&c_o;&c_d;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_o;&c_r;&c_o;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>n&aacute;lev</trd>,
    <trd>v&yacute;plach</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ib;&c_r;&c_i;&c_gu;&c_a;&c_c;&c_ib;&#x44F;</trd>,
    <trd>&#x43E;&c_b;&c_v;&#x43E;&c_d;&#x43D;&c_je;&#x43D;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43E;&c_b;&c_v;&#x43E;&c_d;&#x43D;&#x44E;&c_v;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_z;&c_r;&#x43E;&c_sh;&c_u;&c_v;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_z;&c_r;&#x43E;&c_sh;&c_je;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x704C;&#x6E89; <pr>gu&#xE0;ng&#xE0;i</pr></trd>,
    <trd>&#x6C34;&#x5229; <pr>shu&#x1D0;l&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="irigaci.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="irigaci.0i.AGR">
    <dif>
      Alkonduki akvon sur kulturatan terspacon:
      <ekz>
        la aztekaj kanaloj <klr>...</klr> iam <tld/>is la kultivejojn de
        Tenochtitlan (la malnova nomo de Meksikurbo)
        <fnt>
        <aut>A. Orellana Rojas</aut>,
        <vrk>La Kvina Tutamerika Kongreso</vrk>,
        <lok><url ref="">5a TAKE, 2001</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_v;&c_a;&c_d;&c_n;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_r;&c_a;&c_sh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_o;&c_l;&c_i;&c_v;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>prov&eacute;st v&yacute;plach</trd>,
    <trd>vypl&aacute;chnout</trd>
  </trdgrp>
  <trd lng="de">bew&auml;ssern</trd>
  <trd lng="en">to irrigate</trd>
  <trd lng="es">irrigar</trd>
  <trd lng="fr">irriguer</trd>
  <trd lng="hu">&ouml;nt&ouml;z</trd>
  <trdgrp lng="id">
    <trd>meng<ind>air</ind>i</trd>,
    <trd>mem<ind>bandar</ind>kan (air)</trd>
  </trdgrp>
  <trd lng="ja">&#x704C;&#x6F11;&#x3059;&#x308B;  <pr>&#x304B;&#x3093;&#x304C;&#x3044;&#x3059;&#x308B;</pr></trd>
  <trdgrp lng="nl">
    <trd>bevloeien</trd>,
    <trd>irrigeren</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>irygowa&cacute;</trd>,
    <trd>nawadnia&cacute;</trd>
  </trdgrp>
  <trd lng="pt">irrigar</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_v;&c_o;&c_d;&c_n;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_r;&c_o;&c_sh;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zavla&zcaron;ova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x43E;&c_b;&c_v;&#x43E;&c_d;&#x43D;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_r;&#x43E;&c_sh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x704C;&#x6E89; <pr>gu&#xE0;ng&#xE0;i</pr></trd>,
    <trd>&#x6E89; <pr>g&#xE0;i</pr></trd>,
    <trd>&#x6D47;&#x704C; <pr>ji&#x101;ogu&#xE0;n</pr></trd>
  </trdgrp>
</drv>
</art>
