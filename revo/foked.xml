      (<trd lng="la">Phocidae</trd>), vivantaj en maro.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_p;&c_r;&c_a;&c_w;&c_d;&c_n;&c_y;&c_ja; <ind>&c_c;&c_ju;&c_l;&c_je;&c_n;&c_ib;</ind> <klr
