<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap><rad>Fanar</rad>/o
      <fnt>
	<aut>A.D. Atanasov k.a.</aut>:
	<vrk>Bulgara-Esperanta vortaro</vrk>,
	<lok>Sofio, 1961</lok>
      </fnt>
    </kap>

    <drv mrk="fanar.F0o">
      <kap><tld/>o</kap>
      <snc mrk="fanar.F0o..GEOG">
	<uzo tip="fak">GEOG</uzo>
	<dif>
	  La &leftquot;Lumtura kvartalo&rightquot; en
	  <ref tip="malprt" cel="istanbul.0o">Istanbulo</ref>,
	  kie situas la rezidejo de la konstantinopola patriarko kaj
	  kie, en la epoko de la Turka Imperio, lo&gcirc;is la
	  reprezentantoj de la greka aristokrataro kaj eklezia
	  estraro.
	</dif>
      </snc>
      <snc mrk="fanar.F0o.POL">
	<uzo tip="fak">HIS</uzo>
	<uzo tip="fak">POL</uzo>
	<dif>
	  Figursence, la konstantinopola patriarkejo kaj la istanbula
	  aristokrataro grekdevena.
        </dif>
	<ref tip="vid" cel="vatikan.V0o.KAT">Vatikano<sncref/></ref>
      </snc>
      <trd lng="el">&Phi;&alfa;&ny;&alfa_ton;&rho;</trd>
      <trd lng="hu">Fanar</trd>
      <trd lng="ru">&c_F;&c_a;&c_n;&c_a;&c_r;</trd>
    </drv>

    <drv mrk="fanar.0ano">
      <kap><tld lit="f"/>ano</kap>
      <snc>
	<dif>
	  Enlo&gcirc;anto de <tld/><sncref ref="fanar.F0o.GEOG"/>o.
	</dif>
      </snc>
      <trd lng="hu">fanariai</trd>
      <trd lng="ru">&c_f;&c_a;&c_n;&c_a;&c_r;&c_i;&c_o;&c_t;</trd>
    </drv>

    <drv mrk="fanar.0ulo">
      <kap><tld lit="f"/>ulo</kap>
      <uzo tip="fak">HIS</uzo>
      <uzo tip="fak">POL</uzo>
      <snc>
	<dif>
	  Adepto a&ubreve; reprezentanto de la politiko a&ubreve; de la doktrino de
	  <tld/>o<sncref ref="fanar.F0o.POL"/>.
	</dif>
      </snc>
      <trd lng="hu">fanari&oacute;ta</trd>
      <trdgrp lng="ru">
	<trd>&c_f;&c_a;&c_n;&c_a;&c_r;&c_i;&c_o;&c_t;</trd>
	<trd>&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k; <ind>&c_F;&c_a;&c_n;&c_a;&c_r;&c_a;</ind></trd>
      </trdgrp>
    </drv>
  </art>

