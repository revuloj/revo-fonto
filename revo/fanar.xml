<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fanar.xml,v 1.15 2023/07/26 15:26:44 revo Exp $">
<kap><rad>Fanar</rad>/o
  <fnt>
	<aut>A.D. Atanasov k.a.</aut>:
	<vrk>Bulgara-Esperanta vortaro</vrk>,
	<lok>Sofio, 1961</lok>
  </fnt>
</kap>

<drv mrk="fanar.F0o">
  <kap><tld/>o</kap>
  <snc mrk="fanar.F0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      La &leftquot;Lumtura kvartalo&rightquot; en
      <ref tip="malprt" cel="istanbul.0o">Istanbulo</ref>,
      kie situas la rezidejo de la konstantinopola patriarko kaj
      kie, en la epoko de la Turka Imperio, lo&gcirc;is la
      reprezentantoj de la greka aristokrataro kaj eklezia
      estraro:
      <ekz>
        la pozicio de Atenagoro estas politike en dan&gcirc;ero: la laikisma turka &scirc;tato, kies
        lo&gcirc;antaro estas preska&ubreve; pure mahometana, ne alte &scirc;atas la <tld/>on, la
        rezidejon de la ekumena patriarko
        <fnt>
          <vrk><url ref="http://www.esperokatolika.org/ek19661970/ek1967_09.htm"
              >Al Plia Unueco: Pa&ubreve;lo VI vizitis Turkujon</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="fanar.F0o.POL">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Figursence, la konstantinopola patriarkejo kaj la istanbula
      aristokrataro grekdevena.
    </dif>
    <ref tip="vid" cel="vatikan.V0o.KAT">Vatikano<sncref/></ref>
  </snc>
  <rim>
    La vorto &leftquot;<ref tip="hom" cel="fan.0aro">fan&middot;ar&middot;o</ref>&rightquot; uzi&gcirc;as anka&ubreve;
    en la senco &leftquot;amikaro&rightquot;, &leftquot;anaro&rightquot;, ekz. pri piedpilko-kluboj.
  </rim>
  <trd lng="be">&c_F;&c_a;&c_n;&c_a;&c_r;</trd>
  <trd lng="ca">Fanar</trd>
  <trdgrp lng="de">
    <trd>Fanar</trd>,
    <trd>Phanar</trd>
  </trdgrp>
  <trd lng="el">&Phi;&alfa;&ny;&alfa_ton;&rho;</trd>
  <trd lng="fa">&a_f;&a_n;&a_A;&a_r;</trd>
  <trd lng="fr">Phanar</trd>
  <trd lng="hu">Fanar</trd>
  <trd lng="nl">Fanar</trd>
  <trd lng="pt">Fanar</trd>
  <trd lng="ru">&c_F;&c_a;&c_n;&c_a;&c_r;</trd>
</drv>

<drv mrk="fanar.0ano">
  <kap><tld lit="f"/>ano</kap>
  <snc>
    <dif>
      Lo&gcirc;anto de <tld/>o<sncref ref="fanar.F0o.GEOG"/>.
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_n;&c_a;&c_r;&c_y;&c_jo;&c_t;</trd>
  <trd lng="ca">fanariota</trd>
  <trdgrp lng="de">
    <trd>Phanariot</trd>
  </trdgrp>
  <trd lng="fr">Phanariote</trd>
  <trd lng="hu">fanariai</trd>
  <trd lng="nl">Fanarioot</trd>
  <trd lng="pt">fanariota</trd>
  <trd lng="ru">&c_f;&c_a;&c_n;&c_a;&c_r;&c_i;&c_o;&c_t;</trd>
</drv>

<drv mrk="fanar.0ulo">
  <kap><tld lit="f"/>ulo</kap>
  <snc>
  <uzo tip="fak">HIS</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      Adepto a&ubreve; reprezentanto de la politiko a&ubreve; de la doktrino de
      <tld/>o<sncref ref="fanar.F0o.POL"/>.
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_n;&c_a;&c_r;&c_y;&c_jo;&c_t;</trd>
  <trd lng="ca">fanariota</trd>
  <trdgrp lng="de">
    <trd>Phanariot</trd>
  </trdgrp>
  <trd lng="fr">Phanariote</trd>
  <trd lng="hu">fanari&oacute;ta</trd>
  <trd lng="nl">Fanarioot</trd>
  <trd lng="pt">fanariota</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_a;&c_n;&c_a;&c_r;&c_i;&c_o;&c_t;</trd>
    <trd>&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k; <ind>&c_F;&c_a;&c_n;&c_a;&c_r;&c_a;</ind></trd>
  </trdgrp>
</drv>

</art>

