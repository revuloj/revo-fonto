<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>civil</rad>/a
</kap>

<drv mrk="civil.0a">
  <kap><tld/>a</kap>
  <snc mrk="civil.0a.nemilitista">
    <dif>
      Nemilitista:
      <ekz>
        <tld/>aj vestoj, <tld/>a kura&gcirc;o.
      </ekz>
    </dif>
    <trd lng="be">&c_c;&c_y;&c_v;&c_ib;&c_l;&c_mol;&c_n;&c_y;</trd>
    <trdgrp lng="ru">
      <trd>&c_sh;&c_t;&c_a;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_g;&c_r;&c_a;&c_zh;&c_d;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_c;&c_i;&c_v;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>zivil</trd>,
      <trd>nichtmilit&auml;risch</trd>
    </trdgrp>
    <trd lng="sv">civil</trd>
  </snc>
  <snc mrk="civil.0a.neeklezia">
    <dif>
      Neeklezia:
      <ekz>
        <tld/>a edzi&gcirc;o.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="laik.0a">laika</ref>,
      <ref cel="profan.0a.civila">profana</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_c;&c_y;&c_v;&c_ib;&c_l;&c_mol;&c_n;&c_y;</trd>,
      <trd>&c_g;&c_r;&c_a;&c_m;&c_a;&c_d;&c_z;&c_ja;&c_n;&c_s;&c_k;&c_ib;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>laienhaft</trd>,
      <trd>nichtkirchlich</trd>
    </trdgrp>
    <trd lng="ru">&c_g;&c_r;&c_a;&c_zh;&c_d;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
  <snc mrk="civil.0a.privata">
    <dif>
      Ne politika, sed
      <ref tip="sin" cel="privat.0a.neoficiala">privata</ref>:
      <ekz>
        <tld/>aj rajtoj;
      </ekz><ekz>
        <tld/>a juro <klr>(nekriminala)</klr>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_y;&c_v;&c_a;&c_t;&c_n;&c_y;</trd>,
      <trd>&c_g;&c_r;&c_a;&c_m;&c_a;&c_d;&c_z;&c_ja;&c_n;&c_s;&c_k;&c_ib;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>privat</trd>,
      <trd>Privat-</trd>,
      <trd>Zivil-</trd>
    </trdgrp>
    <trd lng="ru">&c_g;&c_r;&c_a;&c_zh;&c_d;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
  <trd lng="es">civil</trd>
  <trd lng="en">civil</trd>
  <trd lng="fr">civil <klr tip="ind">(adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>civil</trd>,
    <trd>polg&aacute;ri</trd></trdgrp>
  <trd lng="pt">civil</trd>
  <trd lng="sk">civiln&yacute;</trd>
</drv>

<drv mrk="civil.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Homo kiu estas nek
      <ref tip="ant" cel="milit.0isto">militisto</ref> nek
      <ref tip="ant" cel="klerik.0o">kleriko</ref>.
    </dif>
  </snc>
  <ref tip="vid" cel="laik.0o.pasxtato">laiko</ref>
  <trd lng="be">&c_c;&c_y;&c_v;&c_ib;&c_l;&c_mol;&c_n;&c_y; <klr>(&c_n;&c_a;&c_z;.)</klr></trd>
  <trdgrp lng="de">
    <trd>Zivilist</trd>,
    <trd>Zivilperson</trd>
  </trdgrp>
  <trd lng="es">civil</trd>
  <trd lng="en">civilian</trd>
  <trd lng="fr">civil <klr tip="ind">(adj.)</klr></trd>
  <trd lng="hu">civil</trd>
  <trd lng="pt">civil</trd>
  <trd lng="ru">&c_sh;&c_t;&c_a;&c_t;&c_s;&c_k;&c_i;&c_j; <klr>(&c_s;&c_u;&c_shch;.)</klr></trd>
  <trd lng="sk">civil</trd>
</drv>
</art>
