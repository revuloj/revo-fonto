<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: civil.xml,v 1.29 2021/03/06 06:20:01 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>civil</rad>/a <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="civil.0a">
  <kap><tld/>a</kap>
  <snc mrk="civil.0a.nemilitista">
    <dif>
      Nemilitista:
      <ekz>
        <ind><tld/>aj vestoj</ind>, <tld/>a kura&gcirc;o.
        <trd lng="nl">burgerkleding</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_c;&c_y;&c_v;&c_ib;&c_l;&c_mol;&c_n;&c_y;</trd>
    <trdgrp lng="ru">
      <trd>&c_sh;&c_t;&c_a;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_g;&c_r;&c_a;&c_zh;&c_d;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_c;&c_i;&c_v;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>zivil</trd>,
      <trd>nichtmilit&auml;risch</trd>
    </trdgrp>
    <trd lng="fa">&a_gh;&f_y;&a_r;&a_n;&a_Z;&a_A;&a_m;&f_y;</trd>
    <trd lng="pl">cywilny</trd>
    <trd lng="sv">civil</trd>
  </snc>
  <snc mrk="civil.0a.neeklezia">
    <dif>
      Neeklezia:
      <ekz>
        <tld/>a edzi&gcirc;o.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="laik.0a">laika</ref>,
      <ref cel="profan.0a.civila">profana</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_c;&c_y;&c_v;&c_ib;&c_l;&c_mol;&c_n;&c_y;</trd>,
      <trd>&c_g;&c_r;&c_a;&c_m;&c_a;&c_d;&c_z;&c_ja;&c_n;&c_s;&c_k;&c_ib;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>laienhaft</trd>,
      <trd>nichtkirchlich</trd>
    </trdgrp>
    <trd lng="en">secular</trd>
    <trd lng="fa">&a_gh;&f_y;&a_r;&a_m;&a_dh;&a_h;&a_b;&f_y;</trd>
    <trd lng="pl">&sacute;wiecki</trd>
    <trd lng="ru">&c_g;&c_r;&c_a;&c_zh;&c_d;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
  <snc mrk="civil.0a.privata">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Ne politika, sed
      <ref tip="sin" cel="privat.0a.neoficiala">privata</ref>:
      <ekz>
        <tld/>aj rajtoj;
      </ekz>
      <ekz>
        <ind><tld/>a juro</ind> <klr>(nekriminala)</klr>;
        <trd lng="nl">burgerlijk <ind>recht</ind></trd>
      </ekz>
      <ekz>
        sin deklari <ind><tld/>a partio</ind>.
        <trd lng="de">Zivilpartei</trd>
        <trd lng="fr">partie civile</trd>
        <trd lng="it">parte civile</trd>
        <trd lng="nl">burgerlijke <ind>partij</ind></trd>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_y;&c_v;&c_a;&c_t;&c_n;&c_y;</trd>,
      <trd>&c_g;&c_r;&c_a;&c_m;&c_a;&c_d;&c_z;&c_ja;&c_n;&c_s;&c_k;&c_ib;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>privat</trd>,
      <trd>Privat-</trd>,
      <trd>Zivil-</trd>
    </trdgrp>
    <trd lng="fa">&a_m;&a_d;&a_n;&f_y;</trd>
    <trdgrp lng="nl">
      <trd>burgerlijk</trd>,
      <trd>civiel</trd>,
      <trd>burger-</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>cywilny</trd>,
      <trd>osobisty</trd>
    </trdgrp>
    <trd lng="ru">&c_g;&c_r;&c_a;&c_zh;&c_d;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>civiln&iacute;</trd>,
    <trd>ob&ccaron;ansk&yacute;</trd>
  </trdgrp>
  <trd lng="en">civil</trd>
  <trd lng="es">civil</trd>
  <trd lng="fr">civil <klr tip="ind">(adj.)</klr></trd>
  <trd lng="he">&#x5D0;&#x5B6;&#x5D6;&#x5E8;&#x5B8;&#x5D7;&#x5B4;&#x5D9;</trd>
  <trdgrp lng="hu">
    <trd>civil</trd>,
    <trd>polg&aacute;ri</trd>
  </trdgrp>
  <trd lng="it">civile<klr tip="ind"> (agg.)</klr></trd>
  <trd lng="pt">civil</trd>
  <trdgrp lng="sk">
    <trd>civiln&yacute;</trd>,
    <trd>ob&ccaron;iansky</trd>
  </trdgrp>
</drv>

<drv mrk="civil.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="civil.0ulo.KOMUNE">
    <dif>
      Homo kiu estas nek
      <ref tip="ant" cel="milit.0isto">militisto</ref> nek
      <ref tip="ant" cel="klerik.0o">kleriko</ref>:
      <ekz>
        la sentoj de la batalantaj nacioj fari&gcirc;is pli amaraj, kaj tiu 
        amareco estis same, e&ccirc; pli, evidenta inter <tld/>uloj ol inter 
        la soldatoj
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="laik.0o.pasxtato">laiko</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_v;&c_ib;&c_l;&c_mol;&c_n;&c_y;
  <klr>(&c_n;&c_a;&c_z;.)</klr></trd>
  <trdgrp lng="de">
    <trd>Zivilist</trd>,
    <trd>Zivilperson</trd>
  </trdgrp>
  <trd lng="es">civil</trd>
  <trd lng="en">civilian</trd>
  <trd lng="fr">civil <klr tip="ind">(subst.)</klr></trd>
  <trd lng="he">&#x5D0;&#x5D6;&#x5E8;&#x5D7;</trd>
  <trd lng="hu">civil</trd>
  <trd lng="it">civile<klr tip="ind"> (sost.)</klr></trd>
  <trd lng="nl">burger</trd>
  <trd lng="pl">cywil</trd>
  <trd lng="pt">civil</trd>
  <trd lng="ru">&c_sh;&c_t;&c_a;&c_t;&c_s;&c_k;&c_i;&c_j;
  <klr>(&c_s;&c_u;&c_shch;.)</klr></trd>
  <trd lng="sk">civil</trd>
  <trd lng="sk">civil, nevojak</trd>
  <trd lng="sk">civil, nevojak</trd>
  <trdgrp lng="cs">
    <trd>civilista</trd>,
    <trd>civiln&iacute; osoba</trd>,
    <trd>nevoj&aacute;k</trd>
  </trdgrp>
</drv>
</art>
