<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>civil</rad>/a
</kap>

<drv mrk="civil.0a">
  <kap><tld/>a</kap>
  <snc mrk="civil.0a.nemilitista">
    <dif>
      Nemilitista: 
      <ekz>
        <tld/>aj vestoj, <tld/>a kura&gcirc;o.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_sh;&c_t;&c_a;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_g;&c_r;&c_a;&c_zh;&c_d;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_c;&c_i;&c_v;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
    <trd lng="sv">civil</trd>
  </snc>
  <snc mrk="civil.0a.neeklezia">
    <dif>
      Neeklezia: 
      <ekz>
        <tld/>a edzi&gcirc;o.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="laik.0a.civila">laika</ref>,
      <ref cel="profan.0a.civila">profana</ref>
    </refgrp>
    <trd lng="ru">&c_g;&c_r;&c_a;&c_zh;&c_d;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
  <snc mrk="civil.0a.privata">
    <dif>
      Ne politika, sed
      <ref tip="sin" cel="privat.0a.neoficiala">privata</ref>: 
      <ekz>
        <tld/>aj rajtoj;
      </ekz><ekz>
        <tld/>a juro <klr>(nekriminala)</klr>.
      </ekz>
    </dif>
    <trd lng="ru">&c_g;&c_r;&c_a;&c_zh;&c_d;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
  <trd lng="fr">civil <klr tip="ind">(adj.)</klr></trd>
</drv>

<drv mrk="civil.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Homo kiu estas nek
      <ref tip="ant" cel="milit.0isto">militisto</ref> nek
      <ref tip="ant" cel="klerik.0o">kleriko</ref>.  
    </dif>
  </snc>
  <ref tip="vid" cel="laik.0o.pasxtato">laiko</ref>
  <trd lng="fr">civil <klr tip="ind">(adj.)</klr></trd>
  <trd lng="ru">&c_sh;&c_t;&c_a;&c_t;&c_s;&c_k;&c_i;&c_j; <klr>(&c_s;&c_u;&c_shch;.)</klr></trd>
</drv>
</art>
