<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: civil.xml,v 1.34 2023/10/31 13:37:03 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>civil</rad>/a <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="civil.0a">
  <kap><tld/>a</kap>
  <snc mrk="civil.0a.privata">
    <dif>
      Rilata al ordinara statuso de civitano, ne<ref tip="ant" cel="oficia.0a">oficiala</ref>, 
      <ref tip="sin" cel="privat.0a">privata</ref>.
    </dif>  
    <subsnc mrk="civil.0a.nemilitista">
      <dif>
        Nemilitista:
        <ekz>
          <ind><tld/>aj vestoj</ind>
          <fnt>
            <bib>Revizoro</bib>
            <lok>Akto unua</lok>
          </fnt>;
          <trd lng="de">Zivil <klr>(Kleidung)</klr></trd>
          <trd lng="nl">burgerkleding</trd>
        </ekz>
        <ekz>
          &ccirc;iuj pastroj, generaloj kaj <tld/>aj altranguloj jam kolekti&gcirc;is en la granda
          salono
          <fnt>
            <bib>Far3</bib>
            <lok>&Ccirc;apitro I</lok>
          </fnt>;
        </ekz>
        <ekz>
          devigita forlasi la revojn pri milita kariero por sia filo, la patro decidis
          destini lin por la <tld/>a servado
          <fnt>
            <bib>PatrojFiloj</bib>
            <lok>I</lok>
          </fnt>;
        </ekz>
        <ekz>
          <tld/>a milito
          <klr>(prefere: <ref tip="ant" cel="milit.intercivitana_0o">(inter)civitana 
          milito</ref>)</klr>
          <fnt>
            <bib>Metrop</bib>
          </fnt>;
        </ekz>
        <ekz>
          <tld/>e vestita sinjoro sin turnis tre &gcirc;entile kaj amike al Vilfrido
          <fnt>
            <bib>MrA</bib>
            <lok>&Ccirc;apitro XVI</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_c;&c_y;&c_v;&c_ib;&c_l;&c_mol;&c_n;&c_y;</trd>
      <trdgrp lng="ru">
        <trd>&c_sh;&c_t;&c_a;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>,
        <trd>&c_g;&c_r;&c_a;&c_zh;&c_d;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>,
        <trd>&c_c;&c_i;&c_v;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>zivil</trd>,
        <trd>nichtmilit&auml;risch</trd>
      </trdgrp>
      <trd lng="fa">&a_gh;&f_y;&a_r;&a_n;&a_Z;&a_A;&a_m;&f_y;</trd>
      <trd lng="pl">cywilny</trd>
      <trd lng="sv">civil</trd>
    </subsnc>
    <subsnc mrk="civil.0a.neeklezia">
      <dif>
        Ne<ref tip="ant" cel="eklezi.0a">eklezia</ref>:
        <ekz>
          mi ne komprenas ke viro tiel inteligenta kiel vi donas la plej etan gravecon al
          tiu vana formulo, kiun oni nomas la <tld/>a edzigo
          <fnt>
            <bib>KPr</bib>
            <lok>&Ccirc;apitro Dekunua</lok>
          </fnt>;
        </ekz>
        <ekz>
          ju&gcirc;isto rilate al <tld/>aj kaj religiaj aferoj ordonis, ke mi ricevu cent
          bastonfrapojn
          <fnt>
            <bib>Kandid</bib>
            <lok>&Ccirc;apitro XXVIII</lok>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="sub">
        <ref cel="laik.0a">laika</ref>,
        <ref cel="profan.0a.civila">profana</ref>
      </refgrp>
      <trdgrp lng="be">
        <trd>&c_c;&c_y;&c_v;&c_ib;&c_l;&c_mol;&c_n;&c_y;</trd>,
        <trd>&c_g;&c_r;&c_a;&c_m;&c_a;&c_d;&c_z;&c_ja;&c_n;&c_s;&c_k;&c_ib;</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>laienhaft</trd>,
        <trd>nichtkirchlich</trd>
      </trdgrp>
      <trd lng="en">secular</trd>
      <trd lng="fa">&a_gh;&f_y;&a_r;&a_m;&a_dh;&a_h;&a_b;&f_y;</trd>
      <trd lng="pl">&sacute;wiecki</trd>
      <trd lng="ru">&c_g;&c_r;&c_a;&c_zh;&c_d;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
    </subsnc>
    <subsnc mrk="civil.0a.nepolitika">  
      <dif>
        Ne politika.
      </dif>
    </subsnc>
  </snc>
  <snc mrk="civil.0a.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Koncernante la interpersonajn rilatojn; ne publika, ne 
      <ref tip="ant" cel="krimin.0a">kriminala</ref>:
      <ekz>
        <tld/>aj rajtoj;
      </ekz>
      <ekz>
        <ind><tld/>a juro</ind> <klr>(nekriminala)</klr>;
        <trd lng="nl">burgerlijk <ind>recht</ind></trd>
      </ekz>
      <ekz>
        homoj <klr>[&#x2026;]</klr> &scirc;an&gcirc;inte sian <tld/>an etaton, 
        rekomencis ekzistadon sub alia nomo
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        via edzini&gcirc;o kun mi, reguligante la <tld/>an staton de tiu infano, redonante al
        li la familion, kiun li perdis, faros lian feli&ccirc;on kaj samtempe la vian
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dektria</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni eligis el la libroj de la <tld/>a registro tiun fatalan akton
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekoka</lok>
        </fnt>;
      </ekz>
      <ekz>
        sin deklari <ind><tld/>a partio</ind>.
        <trd lng="de">Zivilpartei</trd>
        <trd lng="fr">partie civile</trd>
        <trd lng="it">parte civile</trd>
        <trd lng="nl">burgerlijke <ind>partij</ind></trd>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_y;&c_v;&c_a;&c_t;&c_n;&c_y;</trd>,
      <trd>&c_g;&c_r;&c_a;&c_m;&c_a;&c_d;&c_z;&c_ja;&c_n;&c_s;&c_k;&c_ib;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>privat</trd>,
      <trd>Privat-</trd>,
      <trd>Zivil-</trd>
    </trdgrp>
    <trd lng="fa">&a_m;&a_d;&a_n;&f_y;</trd>
    <trdgrp lng="nl">
      <trd>burgerlijk</trd>,
      <trd>civiel</trd>,
      <trd>burger-</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>cywilny</trd>,
      <trd>osobisty</trd>
    </trdgrp>
    <trd lng="ru">&c_g;&c_r;&c_a;&c_zh;&c_d;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
    <trd lng="zh">&#x79C1;&#x5BB6; <pr>s&#x12B;ji&#x101;</pr></trd>
</snc>
  <trdgrp lng="cs">
    <trd>civiln&iacute;</trd>,
    <trd>ob&ccaron;ansk&yacute;</trd>
  </trdgrp>
  <trd lng="en">civil</trd>
  <trd lng="es">civil</trd>
  <trd lng="fr">civil <klr tip="ind">(adj.)</klr></trd>
  <trd lng="he">&alef;&#x5B6;&zayin;&resh;&#x5B8;&het;&#x5B4;&yod;</trd>
  <trdgrp lng="hu">
    <trd>civil</trd>,
    <trd>polg&aacute;ri</trd>
  </trdgrp>
  <trd lng="it">civile<klr tip="ind"> (agg.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x5E02;&#x6C11;&#x306E;  <pr>&#x3057;&#x307F;&#x3093;&#x306E;</pr></trd>,
    <trd>&#x6587;&#x6C11;&#x306E;  <pr>&#x3076;&#x3093;&#x307F;&#x3093;&#x306E;</pr></trd>,
    <trd>&#x6C11;&#x4E8B;&#x306E;  <pr>&#x307F;&#x3093;&#x3058;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="pt">civil</trd>
  <trdgrp lng="sk">
    <trd>civiln&yacute;</trd>,
    <trd>ob&ccaron;iansky</trd>
  </trdgrp>
  <trd lng="uk">&c_c;&c_i;&c_v;&c_ib;&c_l;&c_mol;&c_n;&c_i;&c_j;</trd>
</drv>

<drv mrk="civil.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="civil.0ulo.KOMUNE">
    <dif>
      Homo kiu estas nek
      <ref tip="ant" cel="milit.0isto">militisto</ref> nek
      <ref tip="ant" cel="klerik.0o">kleriko</ref>:
      <ekz>
        la sentoj de la batalantaj nacioj fari&gcirc;is pli amaraj, kaj tiu 
        amareco estis same, e&ccirc; pli, evidenta inter <tld/>uloj ol inter 
        la soldatoj
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>ulo estis sur la strato haltigita de nekonitaj junuloj
        <fnt>
          <bib>ChL</bib>
          <lok>Noto De L&#x2019; A&ubreve;toro</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="burgx.0o.urbano">bur&gcirc;o<sncref/></ref>,
      <ref cel="civit.0ano">civitano</ref>,
      <ref cel="laik.0o.pasxtato">laiko</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_v;&c_ib;&c_l;&c_mol;&c_n;&c_y;
  <klr>(&c_n;&c_a;&c_z;.)</klr></trd>
  <trdgrp lng="de">
    <trd>Zivilist</trd>,
    <trd>Zivilperson</trd>
  </trdgrp>
  <trd lng="es">civil</trd>
  <trd lng="en">civilian</trd>
  <trd lng="fr">civil <klr tip="ind">(subst.)</klr></trd>
  <trd lng="he">&alef;&zayin;&resh;&het;</trd>
  <trd lng="hu">civil</trd>
  <trd lng="it">civile<klr tip="ind"> (sost.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x4E00;&#x822C;&#x5E02;&#x6C11;  <pr>&#x3044;&#x3063;&#x3071;&#x3093;&#x3057;&#x307F;&#x3093;</pr></trd>,
    <trd>&#x6C11;&#x9593;&#x4EBA;  <pr>&#x307F;&#x3093;&#x304B;&#x3093;&#x3058;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">burger</trd>
  <trd lng="pl">cywil</trd>
  <trd lng="pt">civil</trd>
  <trd lng="ru">&c_sh;&c_t;&c_a;&c_t;&c_s;&c_k;&c_i;&c_j;
  <klr>(&c_s;&c_u;&c_shch;.)</klr></trd>
  <trd lng="sk">civil</trd>
  <trd lng="sk">civil, nevojak</trd>
  <trd lng="sk">civil, nevojak</trd>
  <trdgrp lng="cs">
    <trd>civilista</trd>,
    <trd>civiln&iacute; osoba</trd>,
    <trd>nevoj&aacute;k</trd>
  </trdgrp>
  <trd lng="uk">&c_c;&c_i;&c_v;&c_ib;&c_l;&c_mol;&c_n;&c_i;&c_j; (&c_ib;&c_m;.)</trd>
  <trdgrp lng="zh">
    <trd>&#x6587;&#x804C; <pr>w&#xE9;nzh&#xED;</pr></trd>,
    <trd>&#x6587;&#x804C;&#x4EBA;&#x5458; <pr>w&#xE9;nzh&#xED;r&#xE9;nyu&#xE1;n</pr></trd>,
    <trd>&#x6587;&#x804C;&#x5DE5;&#x4F5C; <pr>w&#xE9;nzh&#xED;g&#x14D;ngzu&#xF2;</pr></trd>
  </trdgrp>
</drv>
</art>
