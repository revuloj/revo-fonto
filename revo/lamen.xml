<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>lamen</rad>/o
</kap>
<drv mrk="lamen.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Mallarĝa, tre maldika, plejofte metala plato:
      <ekz>
        ŝtala, ligna <tld/>o;
      </ekz>
      <ekz>
        balen<tld/>o
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="balen.0ajxo">balenaĵo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>пласьціна</trd>,
    <trd>пліта <klr tip="amb">(тонкая)</klr></trd>,
    <trd>ліст <klr tip="amb">(мэталёвы і да г.п.)</klr></trd>,
    <trd>аркуш <klr tip="amb">(мэталёвы і да г.п.)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Plättchen</trd>,
     <trd>Schüppchen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>lamina</trd>,
    <trd>plate</trd>,
    <trd>layer</trd>
  </trdgrp>
  <trd lng="hu">lemez</trd>
  <trdgrp lng="pt">
    <trd>lâmina</trd>,
    <trd>chapa</trd>,
    <trd>placa</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>пластина</trd>,
    <trd>лист <klr>(металлический, фанерный и т.п.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="lamen.0igi">
  <kap>
    <tld/>igi
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Platigi en <tld/>ojn aŭ foliojn.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd><klr>in </klr>dünne Plättchen <ind>aufspalten</ind></trd>,
     <trd><klr>in </klr>dünne Schichten <ind>aufspalten</ind></trd>,
     <trd>Folien auswalzen</trd>,
     <trd>Folien schlagen</trd>
  </trdgrp>
  <trd lng="en">laminate</trd>
  <trdgrp lng="hu">
     <trd>lapít</trd>,
     <trd>lemezzé alakít</trd>
  </trdgrp>
  <trd lng="nl">pletten</trd>
  <trd lng="pt">laminar</trd>
  <trd lng="ru">раскатывать <klr>(в тонкий лист)</klr></trd>
</drv>
</art>
