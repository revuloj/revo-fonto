<kap>
  <ofc>*</ofc>
  <rad>stertor</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sterto.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sterto.0o.SON">
    <dif>
      Ra&ubreve;ka <ref tip="lst" cel="son.0o" lst="voko:sonoj">sono</ref>,
      kiun a&ubreve;digas mortonto pro malfacila spirado:
      <ekz>
        tie kaj tie estis a&ubreve;deblaj la <tld/>oj de agonio
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="singul1.0i">singulto</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_h;&c_r;&c_y;&c_p;</trd>
  <trd lng="cs">chropt&#x11B;n&#xED; um&#xED;raj&#xED;c&#xED;ho</trd>
  <trdgrp lng="de">
    <trd>Stertor</trd>,
    <trd>R&ouml;cheln</trd>
  </trdgrp>
  <trd lng="en">rattle in the throat</trd>
  <trd lng="fr">r&acirc;le</trd>
  <trd lng="hu">h&ouml;rg&eacute;s</trd>
  <trdgrp lng="ja">
    <trd>&#x30BC;&#x30A4;&#x30BC;&#x30A4;&#x97F3;  [<ind>&#x30BC;&#x30A4;&#x30BC;&#x30A4;&#x304A;&#x3068;</ind>]</trd>,
    <trd>&#x3042;&#x3048;&#x304E;&#x58F0;  [<ind>&#x3042;&#x3048;&#x304E;&#x3054;&#x3048;</ind>]</trd>,
    <trd>&#x5598;&#x9CF4;  [<ind>&#x305C;&#x3093;&#x3081;&#x3044;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gerochel</trd>,
    <trd>gereutel</trd>
  </trdgrp>
  <trd lng="ru">&c_h;&c_r;&c_i;&c_p;</trd>
  <trdgrp lng="sk">
    <trd>chr&#x10D;anie <klr>(ranen&#xFD;ch, umieraj&#xFA;cich)</klr></trd>,
    <trd>chr&#x10D;anie <klr>(ranen&#xFD;ch| umieraj&#xFA;cich)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x445;&#x440;&#x438;&#x43F;</trd>,
    <trd>&#x445;&#x440;&#x438;&#x43F;&#x438;</trd>,
    <trd>&#x445;&#x440;&#x438;&#x43F;&#x456;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x445;&#x430;&#x440;&#x447;&#x430;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
