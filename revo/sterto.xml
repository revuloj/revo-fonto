<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>stertor</rad>/o
</kap>
<drv mrk="sterto.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Ra&ubreve;ka sono, kiun a&ubreve;digas la agoniantaj personoj pro malfacila
      spirado.
    </dif>
    <refgrp tip="sin">
      <ref cel="singul">singulto</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Stertor</trd>,
    <trd>R&ouml;cheln</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gerochel</trd>,
    <trd>gereutel</trd>
  </trdgrp>
  <trd lng="ru">&c_h;&c_r;&c_i;&c_p;</trd>
</drv>
</art>
