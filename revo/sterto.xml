<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>stertor</rad>/o
</kap>
<drv mrk="sterto.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Ra&ubreve;ka <ref tip="lst" cel="son.0o" lst="voko:sonoj">sono</ref>,
      kiun a&ubreve;digas la agoniantaj personoj pro malfacila
      spirado.
    </dif>
    <refgrp tip="sin">
      <ref cel="singul1.0o">singulto</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_h;&c_r;&c_y;&c_p;</trd>
  <trdgrp lng="de">
    <trd>Stertor</trd>,
    <trd>R&ouml;cheln</trd>
  </trdgrp>
  <trd lng="en">rattle in the throat</trd>
  <trd lng="hu">h&ouml;rg&eacute;s</trd>
  <trdgrp lng="nl">
    <trd>gerochel</trd>,
    <trd>gereutel</trd>
  </trdgrp>
  <trd lng="ru">&c_h;&c_r;&c_i;&c_p;</trd>
</drv>
</art>
