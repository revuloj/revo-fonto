<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc><ofc>1</ofc><ofc>7</ofc>
      <rad>absolut</rad>/a
    </kap>
    <drv mrk="absolu.0a">
      <kap><tld/>a</kap>
      <snc mrk="absolu.0a.JUR.POL">
        <uzo tip="fak">JUR</uzo>
        <uzo tip="fak">POL</uzo>
        <dif>
          Tute sendependa de &ccirc;iu ajn povo a&ubreve; kontrolo; kies
          potenco estas senlima kaj &ccirc;iopova:
          <ekz>
            <tld/>a re&gcirc;o, potenco.
          </ekz>
        </dif>
        <trd lng="de">absolut</trd>
        <trdgrp lng="ru">
          <trd>&c_a;&c_b;&c_s;&c_o;&c_l;&c_ju;&c_t;&c_n;&c_y;&c_j;</trd>,
          <trd>&c_s;&c_a;&c_m;&c_o;&c_d;&c_je;&c_r;&c_zh;&c_a;&c_v;&c_n;&c_y;&c_j;</trd></trdgrp>
      </snc>
      <snc>
        <dif>
          Senkondi&ccirc;a, malrelativa, senrezerva, senescepta,
          senmanka, nepra:
          <ekz>
            <tld/>a principo, regulo, logiko<fnt>Z</fnt>;
          </ekz><ekz>
            mi faros la landon <tld/>a dezerto
            <fnt>
              <aut>&Z;</aut>: <vrk>&X;</vrk>,
              <lok>&Jeh; 33:28</lok></fnt>;
          </ekz><ekz>
            <tld/>a manko de singardo<fnt>Z</fnt>;
          </ekz><ekz>
            mi estis <tld/>a amiko de via entrepreno<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>e nenion kompreni;
          </ekz><ekz>
            restis nenio <tld/>e, nek por mordi nek por gluti
            <fnt><aut>&Z;</aut>: <vrk>&PrV;</vrk>.</fnt>.
          </ekz>
        </dif>
        <trd lng="de">absolut</trd>
        <trd lng="ru">&c_a;&c_b;&c_s;&c_o;&c_l;&c_ju;&c_t;&c_n;&c_y;&c_j;</trd>
      </snc>
      <snc mrk="absolu.0a.SCI">
        <uzo tip="fak">SCI</uzo>
        <dif>
          Kiu ne dependas de arbitra mezursistemo:
          <ekz>
            <tld/>a nulo <klr>(pri temperaturo)</klr>.
          </ekz>
        </dif>
        <trd lng="ru">&c_a;&c_b;&c_s;&c_o;&c_l;&c_ju;&c_t;&c_n;&c_y;&c_j;</trd>
      </snc>
      <snc mrk="absolu.0a.LIN">
        <uzo tip="fak">LIN</uzo>
        <dif>
          Gramatike sendependa de la entenanta frazo
          a&ubreve; malhavas kutiman komplementon:
          <ekz>
            <tld/>a participo, ablativo;
          </ekz><ekz>
            <tld/>e <klr>(senkomplemente a&ubreve; sensubjekte)</klr>
            uzita verbo.
          </ekz>
          <trd lng="ru">&c_a;&c_b;&c_s;&c_o;&c_l;&c_ju;&c_t;&c_n;&c_y;&c_j;</trd>
        </dif>
      </snc>
      <snc>
        <dif>
          Senmiksa, &hcirc;emie pura:
          <ekz>
            <tld/>a alkoholo.
          </ekz>
        </dif>
        <trd lng="de">rein</trd>
        <trd lng="ru">&c_ch;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>
      </snc>
      <trd lng="en">absolute</trd>
      <trd lng="fr">absolu</trd>
      <trd lng="it">assoluto</trd>
    </drv>
    <drv mrk="absolu.0ismo">
      <kap><tld/>ismo</kap>
      <snc mrk="absolu.0ismo.POL">
        <uzo tip="fak">JUR</uzo>
        <uzo tip="fak">POL</uzo>
        <dif>
          Sistemo de regado per <tld/>a potenco.
        </dif>
      </snc>
      <trd lng="de">Absolutismus</trd>
      <trd lng="fr">absolutisme</trd>
      <trdgrp lng="ru">
        <trd>&c_a;&c_b;&c_s;&c_o;&c_l;&c_ju;&c_t;&c_i;&c_z;&c_m;</trd>,
        <trd>&c_s;&c_a;&c_m;&c_o;&c_d;&c_je;&c_r;&c_zh;&c_a;&c_v;&c_i;&c_je;</trd></trdgrp>
    </drv>
    <adm>
      Klarigi la oficalecon de la radiko. (MB)
    </adm>
  </art>
