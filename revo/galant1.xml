<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>galant</rad>/a <fnt>Z</fnt>
</kap>
<drv mrk="galant1.0">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Seks&gcirc;entila, flirtema, amindumema;  uzanta kontra&ubreve; la
      virinoj rafinitan &gcirc;entilecon, kun la deziro pla&ccirc;i al ili k
      akiri ilian favoron:
      <ekz>kun danda aplombo k dol&ccirc;e <tld/>a tono<fnt>Z</fnt>.</ekz>  
    </dif>
  </snc>
  <trdgrp lng="de">
   <trd>galant</trd>,
   <trd>zuvorkommend</trd></trdgrp>
   <trd lng="hu">g&aacute;l&aacute;ns</trd>
  <trdgrp lng="fr">
    <trd>galant</trd>,
    <trd>pr&eacute;venant<klr tip="ind"> (aupr&egrave;s des femmes)</klr></trd>,
    <trd>empress&eacute;<klr tip="ind"> (aupr&egrave;s des femmes)</klr></trd></trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_l;&c_a;&c_n;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_b;&c_h;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
