  <art mrk="$Id: sergi.xml,v 1.12 2022/03/27 13:09:34 revo Exp $">
    <kap>
      <rad>Sergi</rad>/o
    </kap>

    <drv mrk="sergi.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">NOM</uzo>
      <snc>
        <dif>
          Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref>:
          <ekz>
            la prokonsulo <tld/>o Pa&ubreve;lo, prudenta viro
            <fnt><bib>NT</bib>, <lok>&Ago; 13:7.</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_S;&c_ja;&c_r;&c_g;&c_je;&c_j;</trd>,
        <trd>&c_S;&c_ja;&c_r;&c_zh;&c_u;&c_k;</trd>
      </trdgrp>
      <trd lng="el">&Sigma;&epsilon_ton;&rho;&gamma;&jota;&omikron;&sigma_fina;</trd>
      <trd lng="de">Sergius<klr> (wenn in lateinischem
      Kontext)</klr></trd>
      <trd lng="en">Sergius</trd>
      <trd lng="es">Sergio</trd>
      <trd lng="fr">Serge</trd>
      <trd lng="hu">Szergej</trd>
      <trd lng="it">Sergio</trd>
      <trd lng="la">Sergius</trd>
      <trd lng="pt">S&eacute;rgio</trd>
      <trdgrp lng="ru">
        <trd>&c_S;&c_je;&c_r;&c_g;&c_je;&c_j;</trd>,
        <trd>&c_S;&c_je;&c_r;&c_g;&c_i;&c_j;<klr> (&c_c;&c_je;&c_r;&c_k;.)</klr></trd>,
        <trd>&c_S;&c_je;&c_r;&c_zh;<klr> (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>
      </trdgrp>
      <trd lng="pl">Sergiusz</trd>
      <trd lng="sk">Sergej</trd>
      <trd lng="uk">&#x421;&#x435;&#x440;&#x433;&#x456;&#x439;</trd>
</drv>
