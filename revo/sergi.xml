  <trd lng="en">Sergius</trd>
  <trd lng="fr">Serge</trd>
  <trd lng="hu">Szergej</trd>
  <trd lng="it">Sergio</trd>
  <trd lng="la">Sergius</trd>
  <trd lng="ru">&c_S;&c_je;&c_r;&c_g;&c_je;&c_j;</trd>
  <trd lng="pl">Sergiusz</trd>
  <trd lng="sk">Sergej</trd>
</drv>
