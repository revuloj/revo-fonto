<art mrk="$Id: ksifi.xml,v 1.17 2023/12/03 09:59:02 revo Exp $">
<kap>
  <rad>ksifi</rad>/o
</kap>
<drv mrk="ksifi.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">Genro</ref>
      de perkomorfaj <ref tip="super" cel="fisx.osto0oj">ostofi&scirc;oj</ref>
      (<trd lng="la">Xiphias</trd>), 
      kies supra makzelo plilongi&gcirc;as en formo de <ref cel="spad.0o">spado</ref>;
      glavofi&scirc;o:
      <ekz>
        spadofi&scirc;o a&ubreve; <tld/>o estas granda migranta karnovora fi&scirc;o 
        karakterizita per longa plata beko, kiu mezuras trionon de 
        &gcirc;ia korpo, &gcirc;i povas atingi longon de 4,3&nbsp;m kaj pezon de 540&nbsp;kg
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=197130">Spadofi&scirc;o</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/6/6c/Xiphias_gladius_stuffed.jpg/640px-Xiphias_gladius_stuffed.jpg" prm="CC BY-SA 3.0"><tld/>o (Xiphias gladius)
  
         <fnt>
           <aut>&#xA9; Citron, propra verko</aut>
           <lok><url ref="&WCW;/File:Xiphias_gladius_stuffed.jpg">&WCW;/File:Xiphias_gladius_stuffed.jpg</url></lok>
         </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_ch;-&c_r;&c_y;&c_b;&c_a;</trd>
  <trd lng="de">Schwertfisch</trd>
  <trd lng="en">swordfish</trd>
  <trd lng="fr">espadon</trd>
  <trd lng="hu">kardhal</trd>
  <trd lng="ja">&#x30E1;&#x30AB;&#x30B8;&#x30AD;</trd>
  <trd lng="la">Xiphias gladius</trd>
  <trd lng="nl">zwaardvis</trd>
  <trdgrp lng="pl">
    <trd>ryba-miecz</trd>,
    <trd>w&lstroke;&oacute;cznik</trd>
  </trdgrp>   
  <trd lng="pt">espadarte</trd>
  <trd lng="ro">pe&#x219;te spad&abreve;</trd>
  <trd lng="ru">&c_m;&c_je;&c_ch;-&c_r;&c_y;&c_b;&c_a;</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x435;&#x447;&#x43E;&#x43D;&#x456;&#x441;</trd>,
    <trd>&#x43C;&#x435;&#x447;-&#x440;&#x438;&#x431;&#x430;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5251;&#x9C7C; <pr>ji&#xE0;ny&#xFA;</pr></trd>,
    <trd>&#x528D;&#x9B5A; <pr>ji&#xE0;ny&#xFA;</pr></trd>,
    <trd>&#x67AA;&#x9C7C; <pr>qi&#x101;ngy&#xFA;</pr></trd>,
    <trd>&#x69CD;&#x9B5A; <pr>qi&#x101;ngy&#xFA;</pr></trd>,
    <trd>&#x7BAD;&#x9B5A; <pr>ji&#xE0;ny&#xFA;</pr></trd>,
    <trd>&#x7BAD;&#x9C7C; <pr>ji&#xE0;ny&#xFA;</pr></trd>
  </trdgrp>
</drv>
</art>
