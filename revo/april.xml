<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>april</rad>/o
</kap>
<drv mrk="april.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">KAL</uzo>
    <dif>
      Kvara <ref tip="lst" cel="monat.0o.KAL" lst="voko:monatoj"
      val="4">monato</ref> de la <ref tip="malprt"
      cel="jar.0o.KAL">jaro</ref> la&ubreve; la Gregoria kalendaro, inter marto
      kaj majo.
    </dif>
  </snc>
  <ref tip="vid" cel="januar.0o.RIM">januaro, Rim.</ref>
  <trd lng="af">April</trd>
  <trd lng="be">&c_k;&c_r;&c_a;&c_s;&c_a;&c_v;&c_ib;&c_k;</trd>
  <trd lng="bg">&c_a;&c_p;&c_r;&c_i;&c_l;</trd>
  <trd lng="br">Ebrel</trd>
  <trd lng="ca">abril</trd>
  <trd lng="cs">duben</trd>
  <trd lng="de">April</trd>
  <trd lng="en">April</trd>
  <trd lng="es">abril</trd>
  <trd lng="fr">avril</trd>
  <trd lng="fy">april</trd>
  <trd lng="he">&alef;&pe;&resh;&yod;&lamed;</trd>
  <trd lng="hu">&aacute;prilis</trd>
  <trd lng="id">April</trd>
  <trd lng="io">aprilo</trd>
  <trd lng="it">aprile</trd>
  <trd lng="ja">&#22235;&#26376; [<ind>&#12375;&#12364;&#12388;</ind>]</trd>
  <trdgrp lng="jbo">
    <trd>vonma'i</trd>,
    <trd>lanma'i</trd>
  </trdgrp>
  <trd lng="nl">april</trd>
  <trd lng="no">april</trd>
  <trd lng="oc">abril</trd>
  <trd lng="pl">kwiecie&nacute;</trd>
  <trd lng="pt">abril</trd>
  <trd lng="ro">aprilie</trd>
  <trd lng="ru">&c_a;&c_p;&c_r;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">apr&iacute;l</trd>
  <trd lng="sv">april</trd>
  <trd lng="tp">tenpo mun nanpa tu tu</trd>
  <trd lng="tr">nisan</trd>
  <trd lng="vo">prilul</trd>
</drv>

<drv mrk="april.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Okazanta en <tld/>o, rilata al <tld/>o.
    </dif>
  </snc>
  <trd lng="io">aprilala</trd>
  <trd lng="vo">prilulik</trd>
</drv>
</art>
