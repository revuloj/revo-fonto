<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: talus.xml,v 1.3 2024/09/06 06:37:03 afido Exp $">
    <kap>
      <rad>talus</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="talus.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOL</uzo><uzo tip="fak">KON</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="dekliv.0o">Dekliva&jcirc;o</ref>
      el rokpecoj a&ubreve; tero (pro erozio, por subporti vojon ks):
      <ekz>
        revivi&gcirc;is planoj por krei <tld/>ojn kaj V-formajn kurejojn en la maro apud Nago
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2015/011439.php"
              >Sendependi&gcirc;o - &ccirc;u senbaza?</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o fervoja tre tre proksimas la riveron
        <fnt>
          <vrk><url
          ref="https://www.radio-frei.de/index.php?iid=5.2&amp;pPAGE=7&amp;ksubmit_show=Artikel&amp;kartikel_id=7773"
              >Die Th&uuml;ringer Eisenbahn und das Reiseziel Sulza</url></vrk>
          <lok>Turingio Internacia, 96a elsendo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bar.0ajxo">bara&jcirc;o</ref>,
      <ref cel="dig.0o">digo</ref>,
      <ref cel="gxet.0o">&gcirc;eto</ref>,
      <ref cel="sxotr.0o">&scirc;otra&jcirc;o</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Wall</trd>,
      <trd>Damm</trd>,
      <trd>Talus</trd>,
      <trd>B&ouml;schung</trd>,
      <trd>Halde</trd>,
      <trd>Ger&ouml;llhalde</trd>,
      <trd>Schutthalde</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>talus</trd>,
      <trd>scree</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>talus</trd>,
      <trd>&eacute;boulis</trd>
    </trdgrp>
  </snc>
<trd lng="be">&c_a;&c_s;&c_y;&c_p;&c_a;&c_k;</trd>
</drv>

  </art>

