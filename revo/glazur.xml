<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>glazur</rad>/o
</kap>
<drv mrk="glazur.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Vitrigebla &scirc;mira&jcirc;o, per kiu oni kovras la potojn a&ubreve; 
      porcelana&jcirc;ojn por fari ilin nepenetreblaj k glatbrilantaj. 
    </dif>
    <refgrp tip="vid">
      <ref cel="emajl.0o">emajlo</ref>.
    </refgrp>
    <trd lng="de">Glasur</trd>
    <trd lng="fr">gla&ccedil;ure</trd>
  </snc>
  <snc num="2">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Fajna pulvoreca sukero, per kiu oni pretigas fluecajn ornama&jcirc;ojn.
    </dif>
    <adm>
    Fluecajn?? <aut>MB</aut>
    </adm>
    <trd lng="de">Zuckerglasur</trd>
    <trd lng="fr">sucre glace</trd>
  </snc>
  <trd lng="ru">&c_g;&c_l;&c_a;&c_z;&c_u;&c_r;&c_mol;</trd>
  <trd lng="sv">glasyr</trd>
</drv>
<drv mrk="glazur.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;miri per <tld/>o:
      <ekz><tld/>ita terpoto;</ekz>
      <ekz>
        <uzo tip="fak">KUI</uzo>
        <tld/>ita kuko, ka&scirc;tano.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="emajl.0i">emajli</ref>,
      <ref cel="glace.0igi">glaceigi</ref>.
    </refgrp>
  </snc>
  <trd lng="de">glasieren</trd>
  <trdgrp lng="fr">
    <trd>glacer<klr tip="ind"> (une p&acirc;tisserie)</klr></trd>,
    <trd>enduire de gla&ccedil;ure</trd></trdgrp> 
  <trd lng="ru">&c_g;&c_l;&c_a;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
<adm>
Precizigi la nuancojn disde: emajlo, glaceigi...
<aut>MB</aut>
</adm>
</art>
