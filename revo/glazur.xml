<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: glazur.xml,v 1.26 2023/08/31 14:29:53 revo Exp $">
    <kap>
      <ofc>2</ofc>
      <rad>glazur</rad>/o
    </kap>

    <drv mrk="glazur.0o">
      <kap><tld/>o</kap>
      <snc mrk="glazur.0o.TEK">
    	<uzo tip="fak">TEK</uzo>
    	<dif>
    	  Vitrigebla protekta &scirc;mirtavolo sur
          <ref cel="cerami.0ajxo">ceramika&jcirc;o</ref>
          (poto, porcelana&jcirc;oj, kahelo ktp),150&ndash;300&my;m
          dika,
          fiksita per bakado por igi la ceramika&jcirc;on imuna 
          kontra&ubreve; 
          akvo, acidoj kaj alkaloj a&ubreve; por ornamo (fari &gcirc;in
          glatbrilanta, <ref cel="glace.0a">glacea</ref>).  
          <tld lit="G"/>o povas esti kolora a&ubreve; senkolora, 
          travidebla a&ubreve; maldiafana: <ekz> [...]
          helan delikatecon de glazuro / en eterna lazuro /
          mar-&ccirc;iela.
          <fnt><aut>Clelia Conterno Guglielminetti</aut>:<vrk>Vivo
          Retrovita
          </vrk>, <lok>La nica literatura revuo, 3/2</lok></fnt>
    	  </ekz>
    	</dif>
    	<refgrp tip="vid">
    	  <ref cel="emajl.0o.TEK">emajlo</ref>.
    	</refgrp>
    	<trd lng="de">Glasur</trd>
        <trd lng="en">glaze</trd>
    	<trdgrp lng="es">
    	  <trd>barniz vitrificable</trd>,
    	  <trd>barniz v&iacute;treo</trd>
    	</trdgrp>
    	<trd lng="fr">gla&ccedil;ure</trd>
    	<trd lng="hu">f&eacute;nym&aacute;z</trd>
      <trdgrp lng="pl">
        <trd>glazura</trd>,
        <trd>szkliwo</trd>
      </trdgrp>
      <trd lng="pt">esmalte</trd>
      </snc>
      <snc mrk="glazur.0o.KUI">
    	<uzo tip="fak">KUI</uzo>
    	<dif>
    	  Sukersiropo por ornami dol&ccirc;a&jcirc;ojn, kaj precipe, 
          tavolo da tia siropo solidi&gcirc;inta sur baka&jcirc;o,
          fruktoj ktp.
    	</dif>
    <trdgrp lng="en"><trd>glaze</trd>, <trd>icing</trd>,
    <trd>frosting</trd></trdgrp>
    	<trd lng="de">Zuckerglasur</trd>
    	<trd lng="es">alm&iacute;bar</trd>
    	<trd lng="fr">sucre glace</trd>
    	<trd lng="hu">cukorm&aacute;z</trd>
      <trdgrp lng="pl">
        <trd>glazura</trd>,
        <trd>polewa</trd>
      </trdgrp>
      </snc>
  <trdgrp lng="be">
        <trd>&c_g;&c_l;&c_a;&c_z;&c_u;&c_r;&c_a;</trd>,
        <trd>&c_p;&c_a;&c_l;&c_ib;&c_v;&c_a;</trd>
      </trdgrp>
  <trdgrp lng="cs">
    <trd>glazura</trd>,
    <trd>poleva</trd>,
    <trd>smalt</trd>
  </trdgrp>
  <trd lng="it">glassa</trd>
  <trdgrp lng="ja">
    <trd>&#x3046;&#x308F;&#x3050;&#x3059;&#x308A;</trd>,
    <trd>&#x91C9;&#x85AC;  [<ind>&#x3046;&#x308F;&#x3050;&#x3059;&#x308A;</ind>]</trd>,
    <trd>&#x30B0;&#x30EC;&#x30FC;&#x30BA;</trd>
  </trdgrp>
  <trd lng="nl">glazuur</trd>
  <trd lng="ru">&c_g;&c_l;&c_a;&c_z;&c_u;&c_r;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>glieda</trd>,
    <trd>poleva</trd>
  </trdgrp>
  <trd lng="sv">glasyr</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43B;&#x438;&#x432;&#x430;</trd>,
    <trd>&#x491;&#x43B;&#x430;&#x437;&#x443;&#x440;</trd>
  </trdgrp>
</drv>

    <drv mrk="glazur.0i">
      <kap><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
    	<dif>
    	  &Scirc;miri per <tld/>o:
    	  <ekz>
            <tld/>ita terpoto;
    	  </ekz><ekz>
    	    <uzo tip="fak">KUI</uzo>
    	    <tld/>ita kuko, ka&scirc;tano.
            <trd lng="it">glassare</trd>
    	  </ekz>
    	</dif>
    	<refgrp tip="vid">
    	  <ref cel="emajl.0i">emajli</ref>,
    	  <ref cel="glace.0igi">glaceigi</ref>.
    	</refgrp>
    <trdgrp lng="en"><trd>frost</trd>, <trd>glaze</trd>,
    <trd>ice</trd></trdgrp>
      <trd lng="it">smaltare</trd>
      </snc>
  <trdgrp lng="be">
        <trd>&c_g;&c_l;&c_a;&c_z;&c_u;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;
        </trd>,
        <trd>&c_p;&c_a;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol; 
        <klr tip="amb">(&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; 
        &c_p;&c_a;&c_l;&c_ib;&c_v;&c_u;)</klr></trd>
      </trdgrp>
  <trdgrp lng="cs">
    <trd>glazovat</trd>,
    <trd>glazurovat</trd>,
    <trd>pol&eacute;vat glazurou</trd>
  </trdgrp>
  <trd lng="de">glasieren</trd>
  <trdgrp lng="es">
        <trd>vidriar</trd>,
        <trd>glasear</trd>,
        <trd>almibarar</trd>
      </trdgrp>
      <trdgrp lng="fr">
    	<trd>glacer<klr tip="ind"> (une p&acirc;tisserie)</klr></trd>,
    	<trd>enduire de gla&ccedil;ure</trd></trdgrp>
  <trdgrp lng="hu">
    	  <trd>f&eacute;nym&aacute;zzal bevon</trd>,
    	  <trd>cukorm&aacute;zzal bevon</trd>
    	</trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3046;&#x308F;&#x85AC;&#x3092;&#x304B;&#x3051;&#x308B;  [<ind>&#x3046;&#x308F;&#x304F;&#x3059;&#x308A;&#x3092;&#x304B;&#x3051;&#x308B;</ind>]</trd>,
    <trd>&#x30B0;&#x30EC;&#x30FC;&#x30BA;&#x3092;&#x304B;&#x3051;&#x308B;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    	<trd>glazuren</trd>,
    	<trd>verglazen</trd></trdgrp>
  <trdgrp lng="pl">
        <trd>glazurowa&cacute;</trd>,
        <trd>szkliwi&cacute;</trd>,
        <trd>polewa&cacute;<klr> (naczynia)</klr></trd>
      </trdgrp>
  <trd lng="ru">
      &c_g;&c_l;&c_a;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">glaz&uacute;rova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43B;&#x438;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x491;&#x43B;&#x430;&#x437;&#x443;&#x440;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x43E;&#x43A;&#x440;&#x438;&#x432;&#x430;&#x442;&#x438; &#x491;&#x43B;&#x430;&#x437;&#x443;&#x440;&#x2019;&#x44E;</trd>
  </trdgrp>
</drv>
    <adm>
       Precizigi la nuancojn disde: emajlo, glaceigi...
       <aut>MB</aut>
    </adm>
    <adm>
       Espereble mi sufi&ccirc;e precizigis, &ccirc;u?
       (GLAZURO: ceramika-bakita; EMAJLO: sur metalo, eventuale
       per elektra-&hcirc;emia procedo, eble plurtavola).
       <aut>Sergio</aut>
    </adm>
  </art>
