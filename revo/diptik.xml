<art mrk="$Id: diptik.xml,v 1.21 2023/10/14 11:26:54 revo Exp $">
<kap>
  <rad>diptik</rad>/o
</kap>
<drv mrk="diptik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc mrk="diptik.0o.BELA">
    <dif>
      <ref tip="super" cel="pentr.0ajxo">Pentra&jcirc;o</ref>
      sur du kunfaldeblaj tabuloj:
      <ekz>
        la&ubreve; enskriba&jcirc;o en la kadro oni ekscias, ke <nom>Joye</nom> tiam a&gcirc;is 47 jarojn, origine &gcirc;i
        estis la dekstra tabulo de <tld/>o, kies maldekstra, malaperinta flanko
        montris la Di-patrinon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=222164">Gilles Joye</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tripti.0o.BELA">triptiko</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_p;&c_t;&c_y;&c_h;</trd>
  <trd lng="ca">d&iacute;ptic</trd>
  <trdgrp lng="cs">
    <trd>diptych</trd>,
    <trd>diptychon</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Diptychon</trd>,
    <trd>zweifl&uuml;geliges Altarbild</trd>
  </trdgrp>
  <trd lng="en">diptych</trd>
  <trd lng="es">d&iacute;ptico</trd>
  <trd lng="fr">diptyque</trd>
  <trdgrp lng="hu">
    <trd>diptichon</trd>,
    <trd>k&eacute;tsz&aacute;rny&uacute;
    <ind>olt&aacute;rk&eacute;p</ind></trd>
  </trdgrp>
  <trd lng="ja">&#x4E8C;&#x3064;&#x6298;&#x308A;&#x306E;&#x7D75;&#x5C4F;&#x98A8;  <pr>&#x3075;&#x305F;&#x3064;&#x304A;&#x308A;&#x306E;&#x3048;&#x3073;&#x3087;&#x3046;&#x3076;</pr></trd>
  <trd lng="pl">dyptyk</trd>
  <trd lng="pt">d&iacute;ptico</trd>
  <trd lng="ru">&c_d;&c_i;&c_p;&c_t;&c_i;&c_h;</trd>
  <trd lng="sk">diptych</trd>
  <trd lng="uk">&#x434;&#x438;&#x43F;&#x442;&#x438;&#x445;</trd>
</drv>
</art>
