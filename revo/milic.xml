<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: milic.xml,v 1.30 2022/07/10 09:29:06 revo Exp $">
<kap>
  <rad>milic</rad>/o <fnt><bib>PV</bib></fnt>,
  <var><kap><rad var="i">milici</rad>/o</kap></var>
</kap>
<drv mrk="milic.0o">
  <kap><tld/>o,
    <var><kap><tld var="i"/>o</kap></var>
  </kap>
  <snc mrk="milic.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Nekonstanta kaj ofte volontula <ref tip="super" cel="milit.0istaro">militistaro</ref>
      destinita nur por defendo de nacia teritorio, kontraste al la konstanta
      <ref tip="vid" cel="arme.0o">armeo</ref>:
      <ekz>
        ili estigis urban <tld/>on, kiu estis &ccirc;iam
        batalpreta
        <fnt><aut>R. Roze</aut>: <vrk>&leftquot;Bravado&rightquot; en
        Saint-Tropez</vrk>, <bib>Monato</bib>,
        <lok>1997:2, p. 28a</lok></fnt>;
      </ekz>
      <ekz>
        eksterle&gcirc;aj privataj <tld var="i"/>oj, dotitaj per la plej modernaj aparataroj kaj
        armiloj, kiuj &leftquot;gardas&rightquot; la landlimon kun Meksikio
        <fnt>
          <bib>Monato</bib>
          <aut>Ionel Onet</aut>
          <vrk><url ref="&Steloj;monato2/007036.html">Ekstremismo: Pri kaldronoj
          kaj potoj: &ccirc;u amba&ubreve; kotaj?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        personaj poseda&jcirc;oj estas rabataj de <tld var="i"/>oj, kiuj semas panikon tra la lando
        <fnt>
          <bib>Monato</bib>
          <aut>Useni Kalumbi</aut>
          <vrk><url ref="&Monato;/2004/008275.html">Paco revenu ...</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ib;&c_l;&c_ib;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_a;&c_p;&c_a;&c_l;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_m;&c_i;&c_l;&c_i;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_o;&c_p;&c_malmol;&c_l;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="ca">mil&iacute;cia</trd>
  <trd lng="cs">milice</trd>
  <trdgrp lng="de">
    <trd>Miliz</trd>,
    <trd>Paramilit&auml;r</trd>,
    <trd>Volksmiliz</trd>
  </trdgrp>
  <trd lng="en">militia</trd>
  <trd lng="es">milicia</trd>
  <trd lng="fr">milice</trd>
  <trdgrp lng="hu">
    <trd>nemzet&odblac;rs&eacute;g</trd>,
    <trd>mil&iacute;cia</trd>
  </trdgrp>
  <trd lng="nl">militie</trd>
  <trd lng="pl">milicja</trd>
  <trd lng="pt">mil&iacute;cia</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_p;&c_o;&c_l;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_m;&c_i;&c_l;&c_i;&c_c;&c_i;&c_ja; <klr tip="amb">(&c_u;&c_s;&c_t;&c_a;&c_r;.)</klr></trd>
  </trdgrp>
  <trd lng="sk">mil&iacute;cia</trd>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x43F;&#x43E;&#x43B;&c_ch;&c_je;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_d;&#x43E;&c_b;&c_r;&#x43E;&c_v;&c_ib;&#x43B;&#x44C;&#x43D;&c_i;&c_j; &c_z;&c_a;&c_g;&c_ib;&#x43D;</trd>
  </trdgrp>
</drv>

<drv mrk="milic.0ano">
  <kap><tld/>ano,
    <var><kap><tld var="i"/>ano</kap></var>
  </kap>
  <snc mrk="milic.0ano.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="super" cel="volont.0ulo">Volontulo</ref>
      en <tld/>o:
      <ekz>
        la <tld/>anoj helpis min
        <fnt>
          <aut>trad. senlime</aut>,
          <vrk><url ref="http://www.senlime.be/Kino/Diktator.htm">
          La Granda Diktatoro</url></vrk>,
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        anoncis irakaj <tld var="i"/>anoj, ke pro spionoj en la britaj kazernoj ili perfekte
        scios, kie kaj kiam la princo eliros el la relativa sekureco de sia bazo
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2007/009240.php">Sango kaj rango</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld var="i"/>anoj, kiuj ka&scirc;as sin en la arbaroj, bu&ccirc;as bestojn en la naciaj parkoj
        de <nom>Virunga</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>Useni Kalumbi</aut>
          <vrk><url ref="&Monato;/2004/008275.html">Paco revenu ...</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Milizion&auml;r</trd>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_l;&c_ib;&c_c;&c_y;&c_ja;&c_n;&c_t;</trd>
  <trd lng="cs">milicion&aacute;&rcaron;</trd>
  <trdgrp lng="en">
    <trd>militia member</trd>,
    <trd>militiaman</trd>
  </trdgrp>
  <trd lng="fr">milicien</trd>
  <trd lng="pl">milicjant</trd>
  <trd lng="sk">milicion&aacute;r</trd>
  <trd lng="uk">&#x43C;&c_ib;&#x43B;&c_ib;&c_c;&c_ib;&#x43E;&#x43D;&c_je;&c_r;</trd>
</drv>
</art>
