<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>milic</rad>/o<fnt>B</fnt>
</kap>
<drv mrk="milic.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Nekonstanta kaj ofte volontula militistaro destinita
      nur por defendo de nacia teritorio, kontraste al la konstanta
      <ref tip="vid" cel="arme.0o">armeo</ref>:
      <ekz>
        ili estigis urban <tld/>on, kiu estis &ccirc;iam
        batalpreta<fnt><aut>Robert Roze</aut>: <vrk>&leftquot;Bravado&rightquot; en
        Saint-Tropez</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero 2a,
        p. 28a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="milici.0o">milicio</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Miliz</trd>,
    <trd>Volksmiliz</trd>
  </trdgrp>
  <trd lng="en">militia</trd>
  <trdgrp lng="fr">
    <trd>milice</trd>
  </trdgrp>
  <trd lng="cs">milice</trd>
  <trd lng="nl">militie</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_p;&c_o;&c_l;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_m;&c_i;&c_l;&c_i;&c_c;&c_i;&c_ja; <klr tip="amb">(&c_u;&c_s;&c_t;&c_a;&c_r;.)</klr></trd>
  </trdgrp>
</drv>
</art>
