<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>milici</rad>/o<fnt>Z</fnt>,
  <rad>milic</rad>/o<fnt>B</fnt>
</kap>
<drv mrk="milic.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Nekonstanta kaj ofte volontula militistaro destinita
      nur por defendo de nacia teritorio (kontraste al la regula
      <ref tip="vid" cel="arme.0o">armeo</ref>).
    </dif>
  </snc>
  <trd lng="en">militia</trd>
  <trd lng="cs">milice</trd>
  <trd lng="nl">militie</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_p;&c_o;&c_l;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_m;&c_i;&c_l;&c_i;&c_c;&c_i;&c_ja; <klr tip="amb">(&c_u;&c_s;&c_t;&c_a;&c_r;.)</klr></trd>
  </trdgrp>
</drv>
</art>
