<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>milic</rad>/o <fnt>B</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="milic.0o">
  <kap><tld/>o</kap>
  <snc mrk="milic.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Nekonstanta kaj ofte volontula militistaro destinita
      nur por defendo de nacia teritorio, kontraste al la konstanta
      <ref tip="vid" cel="arme.0o">armeo</ref>:
      <ekz>
        ili estigis urban <tld/>on, kiu estis &ccirc;iam
        batalpreta
        <fnt><aut>R. Roze</aut>: <vrk>&leftquot;Bravado&rightquot; en
        Saint-Tropez</vrk>, <bib>Monato</bib>,
        <lok>1997:2, p. 28a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="milici.0o">milicio</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ib;&c_l;&c_ib;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_a;&c_p;&c_a;&c_l;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_m;&c_i;&c_l;&c_i;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_o;&c_p;&c_malmol;&c_l;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="ca">mil&iacute;cia</trd>
  <trdgrp lng="de">
    <trd>Miliz</trd>,
    <trd>Volksmiliz</trd>
  </trdgrp>
  <trd lng="en">militia</trd>
  <trd lng="es">milicia</trd>
  <trd lng="fr">milice</trd>
  <trd lng="cs">milice</trd>
  <trdgrp lng="hu">
    <trd>nemzet&odblac;rs&eacute;g</trd>,
    <trd>mil&iacute;cia</trd>
  </trdgrp>
  <trd lng="nl">militie</trd>
  <trd lng="pt">mil&iacute;cia</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_p;&c_o;&c_l;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_m;&c_i;&c_l;&c_i;&c_c;&c_i;&c_ja; <klr tip="amb">(&c_u;&c_s;&c_t;&c_a;&c_r;.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="milic.0ano">
  <kap><tld/>ano</kap>
  <snc mrk="milic.0ano.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Volontulo en <tld/>o:
      <ekz>
        la <tld/>anoj helpis min
        <fnt>
          <aut>trad. senlime</aut>,
          <vrk><url ref="http://www.senlime.be/Kino/Diktator.htm">
          La Granda Diktatoro</url></vrk>,
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">milicien</trd>
</drv>
</art>
