<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&scirc;akr</rad>/i
</kap>
<drv mrk="sxakr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">KOME</uzo>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Komerci pro gajnavido, kontra&ubreve;more komerci.
    </dif>
  </snc>
  <trd lng="de">schachern</trd>
  <trdgrp lng="fr">
    <trd>arnaquer<klr> (intr.)</klr></trd>,
    <trd>escroquer<klr> (intr.)</klr></trd>
  </trdgrp>
  <trd lng="nl">sjacheren</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_r;&c_y;&c_sh;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_je;&c_k;&c_u;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="sxakr.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Homo, kiu sin vivtenas per &scirc;akrado.
    </dif>
  </snc>
  <trd lng="de">Schacherer</trd>
  <trdgrp lng="fr">
    <trd>arnaqueur</trd>,
    <trd>commer&ccedil;ant malhonn&ecirc;te</trd>
  </trdgrp>
</drv>
</art>
