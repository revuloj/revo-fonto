<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxakr.xml,v 1.21 2022/03/29 14:20:29 revo Exp $">
<kap>
  <rad>&scirc;akr</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="sxakr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sxakr.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="komerc.0i">Komerci</ref> ka&scirc;e kaj
      kontra&ubreve;le&gcirc;e:
      <ekz>
        mi rigardas mia devo informi vin, ke nia prezidanto 
        <klr>(...)</klr> <tld/>as per valuto
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 9a</lok></fnt>;
      </ekz>
      <ekz>
        jen la granda socia cunamo, la disfalo de la ordo
        kun senfina banditismo, <tld/>ado, narkota&jcirc;-komerco ...
        <fnt>
          <aut>G. Ledon</aut>,
          <vrk>Cunamoj tutmonde</vrk>,
          <bib>Monato</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_e;&c_k;&c_u;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_p;&c_a;&c_z;&c_a;&c_r;&c_ja; &c_s;&c_je;</trd>
  <trdgrp lng="ca">
    <trd>estafar</trd>,
    <trd>traficar</trd>,
    <trd>mercadejar <klr>(il&middot;legalment)</klr></trd>
  </trdgrp>
  <trd lng="de">schachern</trd>
  <trd lng="en">traffic<klr> (in contraband)</klr></trd>
  <trd lng="es">traficar<klr tip="amb"> (de forma
  ilegal)</klr></trd>
  <trd lng="fr">trafiquer<klr> (ill&eacute;galement)</klr></trd>
  <trdgrp lng="hu">
    <trd>kuf&aacute;rkodik</trd>,
    <trd>&uuml;z&eacute;rkedik</trd>
  </trdgrp>
 <trd lng="it">trafficare<klr> (comm. illegal.)</klr></trd>
  <trd lng="nl">sjacheren</trd>
  <trdgrp lng="pt">
    <trd>chatinar</trd>,
    <trd>mercadejar</trd>,
    <trd>traficar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_r;&c_y;&c_sh;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_f;&c_a;&c_r;&c_c;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_t;&c_o;&c_r;&c_g;&c_o;&c_v;&c_a;&c_t;&c_mol; &c_i;&c_z;-&c_p;&c_o;&c_d;
    &c_p;&c_o;&c_l;&c_y;</trd>,
    <trd>&c_s;&c_p;&c_je;&c_k;&c_u;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x431;&#x430;&#x440;&#x438;&#x448;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x433;&#x435;&#x43D;&#x434;&#x43B;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x437;&#x430;&#x439;&#x43C;&#x430;&#x442;&#x438;&#x441;&#x44F; &#x434;&#x440;&#x456;&#x431;&#x43D;&#x43E;&#x44E; &#x442;&#x43E;&#x440;&#x433;&#x456;&#x432;&#x43B;&#x435;&#x44E;</trd>,
    <trd>&#x444;&#x430;&#x440;&#x446;&#x43E;&#x432;&#x43A;&#x43E;&#x44E;</trd>
  </trdgrp>
</drv>

<drv mrk="sxakr.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="sxakr.0isto.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="komerc.0isto">Komercisto</ref>,
      kiu <tld/>as.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_e;&c_k;&c_u;&c_l;&c_ja;&c_n;&c_t;</trd>
  <trdgrp lng="ca">
    <trd>estafador</trd>,
    <trd>embrollador</trd>,
    <trd>trampista</trd>
  </trdgrp>
  <trd lng="de">Schacherer</trd>
  <trd lng="en">trafficker</trd>
  <trd lng="es">traficante</trd>
  <trdgrp lng="fr">
    <trd>margoulin<klr> (malhonn&ecirc;te)</klr></trd>,
    <trd>trafiquant</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kuf&aacute;r</trd>,
    <trd>&uuml;z&eacute;r</trd>
  </trdgrp>
 <trd lng="it">trafficante</trd>
 <trd lng="nl">sjacheraar</trd>
 <trd lng="ru">&c_f;&c_a;&c_r;&c_c;&c_o;&c_v;&c_shch;&c_i;&c_k;</trd>
  <trdgrp lng="uk">
    <trd>&#x431;&#x430;&#x440;&#x438;&#x448;&#x43D;&#x438;&#x43A;</trd>,
    <trd>&#x433;&#x435;&#x43D;&#x434;&#x43B;&#x44F;&#x440;</trd>,
    <trd>&#x434;&#x440;&#x456;&#x431;&#x43D;&#x438;&#x439; &#x442;&#x43E;&#x440;&#x433;&#x43E;&#x432;&#x435;&#x446;&#x44C;</trd>,
    <trd>&#x444;&#x430;&#x440;&#x446;&#x43E;&#x432;&#x449;&#x438;&#x43A;</trd>
  </trdgrp>
</drv>
</art>
