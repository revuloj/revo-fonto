<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>&scirc;akr</rad>/i
    </kap>

    <drv mrk="sxakr.0i">
      <kap><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <uzo tip="fak">EKON</uzo>
      <snc>
        <dif>
          <ref tip="super" cel="komerc.0i">Komerci</ref> ka&scirc;e kaj
          kontra&ubreve;le&gcirc;e:
          <ekz>
            mi rigardas mia devo informi vin, ke nia prezidanto 
            <klr>(...)</klr> <tld/>as per valuto
            <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 9a</lok></fnt>;
          </ekz><ekz>
            jen la granda socia cunamo, la disfalo de la ordo
            kun senfina banditismo, <tld/>ado, narkota&jcirc;-komerco ...
            <fnt><aut>Gilbert LEDON</aut>: <vrk>Cunamoj tutmonde</vrk>.
              <bib>Monato</bib>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_s;&c_p;&c_e;&c_k;&c_u;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
      <trd lng="de">schachern</trd>
      <trdgrp lng="fr">
        <trd>arnaquer<klr> (intr.)</klr></trd>,
        <trd>escroquer<klr> (intr.)</klr></trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>kuf&aacute;rkodik</trd>,
        <trd>&uuml;z&eacute;rkedik</trd>
      </trdgrp>
      <trd lng="nl">sjacheren</trd>
      <trdgrp lng="pt">
        <trd>chatinar</trd>,
        <trd>mercadejar</trd>,
        <trd>traficar</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_b;&c_a;&c_r;&c_y;&c_sh;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_f;&c_a;&c_r;&c_c;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_t;&c_o;&c_r;&c_g;&c_o;&c_v;&c_a;&c_t;&c_mol; &c_i;&c_z;-&c_p;&c_o;&c_d;
&c_p;&c_o;&c_l;&c_y;</trd>,
        <trd>&c_s;&c_p;&c_je;&c_k;&c_u;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
    </drv>

    <drv mrk="sxakr.0isto">
      <kap><tld/>isto</kap>
      <uzo tip="fak">EKON</uzo>
      <snc>
        <dif>
          <ref tip="super" cel="komerc.0isto">Komercisto</ref>, kiu <tld/>as.
        </dif>
      </snc>
      <trd lng="be">&c_s;&c_p;&c_e;&c_k;&c_u;&c_l;&c_ja;&c_n;&c_t;</trd>
      <trd lng="de">Schacherer</trd>
      <trdgrp lng="fr">
        <trd>arnaqueur</trd>,
        <trd>commer&ccedil;ant malhonn&ecirc;te</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>kuf&aacute;r</trd>,
        <trd>&uuml;z&eacute;r</trd>
      </trdgrp>
      <trd lng="ru">&c_f;&c_a;&c_r;&c_c;&c_o;&c_v;&c_shch;&c_i;&c_k;</trd>
    </drv>
  </art>
