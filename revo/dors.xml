<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>dors</rad>/o
</kap>
<drv mrk="dors.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="dors.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Malanta&ubreve;a parto de la homa korpo, de &scirc;ultroj &gcirc;is
      pelvo:
      <ekz>
        mortigi iun de post la <tld/>o;
      </ekz>
      <ekz>
        viajn malamikojn mi turnos al vi <tld/>e
        <fnt><bib>MT</bib>, <lok>&Eli; 23:27</lok></fnt>;
      </ekz>
      <ekz>
        fali <tld/>en<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        montri, turni la <tld/>on al iu
        <klr>(montri sian mal&scirc;aton)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        post la <tld/>o de iu
        <klr>(perfide, ka&scirc;e)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="drasx.0i">dra&scirc;i</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_s;&c_mol;&c_p;&c_ib;&c_n;&c_a;</trd>,
      <trd>&c_p;&c_l;&c_je;&c_ch;&c_y;</trd>
    </trdgrp>
    <trd lng="es">espalda</trd>
    <trd lng="ru">&c_s;&c_p;&c_i;&c_n;&c_a;</trd>
  </snc>
  <snc mrk="dors.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Supera parto de la korpo de bestoj, de kapo &gcirc;is vosto:
      <ekz>
        grimpi sur la <tld/>on de &ccirc;evalo;
      </ekz>
      <ekz>
        la kato &gcirc;ibigis la <tld/>on
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_mol;&c_p;&c_ib;&c_n;&c_a;</trd>
    <trd lng="ru">&c_s;&c_p;&c_i;&c_n;&c_a;</trd>
  </snc>
  <snc mrk="dors.0o.malantauxa">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malanta&ubreve;a parto de objekto:
      <ekz>
        <tld/>o de klingo, de libro, de mano, de se&gcirc;o, de folio.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_t;&c_y;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_s;&c_t;&c_o;&c_r;&c_o;&c_n;&c_a;</trd>,
      <trd>&c_o;&c_b;&c_o;&c_r;&c_o;&c_t;&c_n;&c_a;&c_ja; &c_s;&c_t;&c_o;&c_r;&c_o;&c_n;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="dors.0o.kurba">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Supera kurba parto de objekto:
      <ekz>
        la nuda <tld/>o de l' monto;
      </ekz>
      <ekz>
        la &scirc;a&ubreve;maj <tld/>oj de la ondoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">R&uuml;cken</trd>
  <trd lng="en">back</trd>
  <trd lng="es">dorso</trd>
  <trd lng="fr">dos</trd>
  <trd lng="he">&gimel;&bet;</trd>
  <trd lng="pl">plecy</trd>
</drv>

<drv mrk="dors.0direkte0en">
  <kap><tld/>direkte, <tld/>en</kap>
  <snc>
    <dif>
      Returnen:
      <ekz>
        la anasoj iris <tld/>en
        <fnt><bib>Fab2</bib>, <lok>Dio de dormo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_z;&c_a;&c_d;</trd>
  <trd lng="de">zur&uuml;ck</trd>
  <trd lng="fr">en <ind>arri&egrave;re</ind></trd>
  <trd lng="he">&alef;&het;&vav;&resh;&he;</trd>
</drv>

<drv mrk="dors.0e.de">
  <kap><tld/>e de</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="antaux.mal0">Malanta&ubreve;</ref>, en la dorsa
      direkto de:
      <ekz>
        <tld/>e de ili ondi&gcirc;is servista rumoro
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 88a, volumo 3a, p.
        122a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>dans le dos de</trd>,
    <trd>derri&egrave;re</trd>
  </trdgrp>
</drv>
</art>
