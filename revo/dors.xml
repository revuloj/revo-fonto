<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dors.xml,v 1.42 2023/11/04 18:50:51 revo Exp $">
<kap>
  <ofc>*</ofc> <rad>dors</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="dors.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="dors.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Malanta&ubreve;a parto de la homa korpo, de &scirc;ultroj
      &gcirc;is pelvo:
      <ekz>
        mortigi iun de post la <tld/>o;
      </ekz>
      <ekz>
        viajn malamikojn mi turnos al vi <tld/>e
        <fnt><bib>MT</bib>, <lok>&Eli; 23:27</lok></fnt>;
      </ekz>
      <ekz>
        li demetis de la <tld/>o <klr>(post porti &gcirc;in sur<tld/>e)</klr>
        la tablon, starigis &gcirc;in anta&ubreve; sin kaj diris: Kovru vin!
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tablo kovru vin, la orazeno kaj bastono el sako</lok>
        </fnt>;
      </ekz>
      <ekz>
        bastonoj, kiuj falis sur mian <tld/>on
        <klr>(batante)</klr>
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        malsati tiel, ke lia ventro enfalis &gcirc;is la <tld/>o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        montri, <ind>turni la <tld/>on</ind> al iu
        <klr>(montri sian mal&scirc;aton)</klr>;
        <trd lng="it">voltare le spalle</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>post la <tld/>o</ind> de iu
        <klr>(perfide, ka&scirc;e)</klr>.
        <trd lng="it">alle spalle</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="drasx.0i">dra&scirc;i</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_s;&c_mol;&c_p;&c_ib;&c_n;&c_a;</trd>,
      <trd>&c_p;&c_l;&c_je;&c_ch;&c_y;</trd>
    </trdgrp>
    <trd lng="es">espalda</trd>
    <trdgrp lng="it">
      <trd>dorso</trd>,
      <trd>schiena</trd>
    </trdgrp>
    <trd lng="pl">plecy</trd>
    <trd lng="ru">&c_s;&c_p;&c_i;&c_n;&c_a;</trd>
  </snc>
  <snc mrk="dors.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Supera parto de la korpo de bestoj, de kapo &gcirc;is posta&jcirc;o:
      <ekz>
        grimpi sur la <tld/>on de &ccirc;evalo;
      </ekz>
      <ekz>
        la kato &gcirc;ibigis la <tld/>on;
      </ekz>
      <ekz>
        la skarabo <klr>[...]</klr> falis sur la <tld/>on kaj baraktis
        per la piedetoj
        <fnt>
          <bib>Fab4</bib>
          <lok>Sterkskarabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bastono, levita super la <tld/>o de l' bruto a&ubreve; sklavo
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i &jcirc;us rajdis trans senarbarejon, kiam la terurulo [...] levis
        &scirc;in de la <tld/>o de la virin&ccirc;evalo kaj tiris &scirc;in kun si
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Drako por Heroo Hinreko</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_mol;&c_p;&c_ib;&c_n;&c_a;</trd>
    <trdgrp lng="it">
      <trd>dorso</trd>,
      <trd>schienale<klr> (di bestia macellata)</klr></trd>
    </trdgrp>
    <trd lng="pl">grzbiet</trd>
    <trd lng="ro">spate</trd>
    <trd lng="ru">&c_s;&c_p;&c_i;&c_n;&c_a;</trd>
  </snc>
  <snc mrk="dors.0o.malantauxa">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malanta&ubreve;a parto de objekto, kontraste al
      <ref tip="ant" cel="front.0o.antauxo">fronto<sncref/></ref>:
      <ekz>
        <tld/>o de klingo, de libro, de mano, de folio;
      </ekz>
      <ekz>
        mi <klr>[&#x2026;]</klr> ekfumis, rekti&gcirc;ante kaj min apogante al
        la
        se&gcirc;<tld/>o
        <fnt><aut>I. G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>,
        <lok>[1898?]</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="revers.0o.KOMUNE">reverso</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_t;&c_y;&c_l;</trd>,
      <trd>&c_t;&c_y;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_ch;&c_a;&c_s;&c_t;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="fr">face<klr> (c&ocirc;t&eacute; face)</klr></trd>
    <trdgrp lng="it">
      <trd>dorso</trd>,
      <trd>retro</trd>,
      <trd>costa<klr> (il retro)</klr></trd>
    </trdgrp>
    <trd lng="pl">plecy</trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_y;&c_l;&c_mol;&c_n;&c_a;&c_ja;
      &c_s;&c_t;&c_o;&c_r;&c_o;&c_n;&c_a;</trd>,
      <trd>&c_o;&c_b;&c_o;&c_r;&c_o;&c_t;&c_n;&c_a;&c_ja;
      &c_s;&c_t;&c_o;&c_r;&c_o;&c_n;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="dors.0o.kurba">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Supera kurba parto de ku&scirc;anta objekto:
      <ekz>
        la nuda <tld/>o de l' monto;
      </ekz>
      <ekz>
        la &scirc;a&ubreve;maj <tld/>oj de la ondoj..
      </ekz>
    </dif>
    <trd lng="be">&c_h;&c_r;&c_y;&c_b;&c_je;&c_t; <klr>(&c_n;&c_a;&c_p;&c_r;. &c_g;&c_o;&c_r;&c_n;&c_y;)</klr></trd>
    <trdgrp lng="it">
      <trd>dorso</trd>,
      <trd>cresta<klr> (di monte od onda)</klr></trd>
    </trdgrp>
    <trd lng="pl">grzbiet</trd>
  </snc>
  <trd lng="bg">&c_g;&c_r;&c_malmol;&c_b;</trd>
  <trd lng="bo">&#xF66;&#xF92;&#xF63;&#xF0B;&#xF54;&#xF0B;</trd>
  <trd lng="cs">rub <klr>(mince| medaile)</klr></trd>
  <trd lng="de">R&uuml;cken</trd>
  <trd lng="en">back</trd>
  <trd lng="es">dorso</trd>
  <trd lng="fr">dos</trd>
  <trd lng="he">&gimel;&bet;</trd>
  <trd lng="hu">h&aacute;t</trd>
  <trd lng="id">punggung</trd>
  <trdgrp lng="ja">
    <trd>&#x80CC;  <pr>&#x305D;&#x3073;&#x3089;</pr></trd>,
    <trd>&#x80CC;&#x4E2D;  <pr>&#x305B;&#x306A;&#x304B;</pr></trd>,
    <trd>&#x80CC;&#x90E8;  <pr>&#x306F;&#x3044;&#x3076;</pr></trd>,
    <trd>&#x88CF;  <pr>&#x308A;</pr></trd>,
    <trd>&#x5F8C;&#x308D;  <pr>&#x3046;&#x3057;&#x308D;</pr></trd>
  </trdgrp>
  <trd lng="nl">rug</trd>
  <trdgrp lng="pt">
    <trd>costas</trd>,
    <trd>dorso</trd>
  </trdgrp>
  <trd lng="sk">chrb&aacute;t</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43F;&c_i;&#x43D;&c_a;</trd>,
    <trd>&c_h;&c_r;&c_je;&c_b;&c_je;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x810A;&#x80CC; <pr>j&#x1D0;b&#xE8;i</pr></trd>,
    <trd>&#x8170;&#x677F; <pr>y&#x101;ob&#x1CE;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="dors.0al0e">
  <kap><tld/>al<tld/>e</kap>
  <snc mrk="dors.0al0e.KOMUNE">
    <dif>
      Duope, kun dorso turnita al la alia kaj
      rigardante en malaj direktoj:
      <ekz>
        &scirc;ia patro diris nenion, kaj ili sidis
        <tld/>-al-<tld/>e sur la malseka &scirc;tono
        <fnt>
          <aut>H. Laxness, trad. B. Ragnarsson</aut>,
          <vrk>Sendependaj homoj</vrk>,
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="front.0al0e.KOMUNE">frontalfronte</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_p;&c_ib;&c_n;&c_a; &c_d;&c_a; &c_s;&c_mol;&c_p;&c_ib;&c_n;&c_y;</trd>
  <trd lng="cs">vzadu</trd>
  <trd lng="fr">dos &agrave; dos<klr> (adv.)</klr></trd>
  <trd lng="sk">vzadu</trd>
</drv>

<drv mrk="dors.0en">
  <kap><tld/>en, <var><kap><tld/>direkte</kap></var></kap>
  <snc mrk="dors.0en.KOMUNE">
    <dif>
      Returnen:
      <ekz>
        la anasoj iris <tld/>en kaj pu&scirc;is unu la alian
        <fnt><bib>Fab2</bib>, <lok>Dio de dormo</lok></fnt>;
      </ekz>
      <ekz>
        la ministo apogis sin <tld/>en
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Minejo de l' Bildoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        li &scirc;anceli&gcirc;is <tld/>en kaj falis
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Urbo de Imperiestroj</lok>
        </fnt>;
      </ekz>
      <ekz>
        Bastjano ku&scirc;igis sin <tld/>en kaj fermis la okuloj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Batalo je la Ebura Turo</lok>
        </fnt>;
      </ekz>
      <ekz>
        rigardi kun <tld/>en klinitaj kapoj supren en la nokto&ccirc;ielon
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Stelmona&hcirc;ejo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_z;&c_a;&c_d;</trd>
  <trd lng="cs">dozadu</trd>
  <trdgrp lng="de">
    <trd>r&uuml;ckw&auml;rts</trd>,
    <trd>nach hinten</trd>;
    <trd>r&uuml;cklings</trd>
  </trdgrp>
  <trd lng="fr">en <ind>arri&egrave;re</ind></trd>
  <trd lng="he">&alef;&het;&vav;&resh;&he;</trd>
  <trd lng="hu">h&aacute;trafel&eacute;</trd>
  <trd lng="it">indietro</trd>
  <trd lng="ja">&#x5F8C;&#x308D;&#x3078;  <pr>&#x3046;&#x3057;&#x308D;&#x3078;</pr></trd>
  <trd lng="nl">achteruit</trd>
  <trdgrp lng="pl">
    <trd>wstecz</trd>,
    <trd>do <ind>ty&lstroke;u</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_z;&c_a;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>dozadu</trd>,
    <trd>nasp&auml;&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd><klr>(&#x43F;&#x43E;&c_v;&c_je;&c_r;&#x43D;&c_u;&c_v;&c_sh;&c_i;&c_s;&#x44C;)</klr>
    &c_s;&#x43F;&c_i;&#x43D;&#x43E;&#x44E;</trd>,
    <trd>&c_z;&c_a;&c_d;&#x43A;&c_u;&#x44E;&c_ch;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5411;&#x540E;&#x65B9; <pr>xi&#xE0;ngh&#xF2;uf&#x101;ng</pr></trd>,
    <trd>&#x671D;&#x540E; <pr>ch&#xE1;oh&#xF2;u</pr></trd>,
    <trd>&#x5411;&#x540E;&#x9762; <pr>xi&#xE0;ngh&#xF2;umi&#xE0;n</pr></trd>,
    <trd>&#x5411;&#x540E; <pr>xi&#xE0;ngh&#xF2;u</pr></trd>,
    <trd>&#x9006;&#x5411; <pr>n&#xEC;xi&#xE0;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="dors.0e_al">
  <kap>
    <tld/>e al
  </kap>
  <snc>
    <dif>
      <tld lit="D"/>en:
      <ekz>
        li turni&gcirc;is <tld/>e al la kasejoj kaj al la proksimi&gcirc;anta
        virino, kaj ekmar&scirc;is
        <fnt>
          <aut>Konstanten Krysakov</aut>
          <vrk><url ref="&Monato;/2000/006805.php">KOTA AKVO
           </url></vrk>
           <bib>Monato</bib>
           <lok>2000/03, p. 28</lok>
        </fnt>.
              </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_p;&c_ib;&c_n;&c_o;&c_j; &c_d;&c_a;</trd>
</drv>

<drv mrk="dors.0e_de">
  <kap><tld/>e de</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="dors.0e_de.KOMUNE">
    <dif>
      <ref tip="super" cel="antaux.mal0">Malanta&ubreve;</ref>, en la
      dorsa
      direkto de:
      <ekz>
        <tld/>e de ili ondi&gcirc;is servista rumoro
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 88a, volumo 3a, p.
        122a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a; <ind>&c_p;&c_l;&c_ja;&c_ch;&c_y;&c_m;&c_a;</ind></trd>,
    <trd>&c_z;&c_a;</trd>,
    <trd>&c_z;&c_z;&c_a;&c_d;&c_u;</trd>,
    <trd>&c_z; &c_t;&c_y;&c_l;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>dans le <ind>dos</ind> de</trd>,
    <trd>derri&egrave;re</trd>
  </trdgrp>
  <trd lng="hu">h&aacute;ta m&ouml;g&ouml;tt</trd>
  <trd lng="it">dietro</trd>
  <trd lng="nl">aan de <ind>achterzijde</ind> van</trd>
  <trdgrp lng="pl">
    <trd>za <ind>plecami</ind></trd>,
    <trd>z <ind>ty&lstroke;u</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_z;&c_a;&c_d;&c_i;</trd>,
    <trd>&c_p;&c_o;&c_z;&c_a;&c_d;&c_i;</trd>,
    <trd>&c_z;&c_a; &c_s;&c_p;&c_i;&c_n;&c_o;&c_j;</trd>
  </trdgrp>
</drv>
</art>
