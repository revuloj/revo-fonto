<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc> <rad>dors</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="dors.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="dors.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Malanta&ubreve;a parto de la homa korpo, de &scirc;ultroj
      &gcirc;is
      pelvo:
      <ekz>
        mortigi iun de post la <tld/>o;
      </ekz>
      <ekz>
        viajn malamikojn mi turnos al vi <tld/>e
        <fnt><bib>MT</bib>, <lok>&Eli; 23:27</lok></fnt>;
      </ekz>
      <ekz>
        <ind>fali <tld/>en</ind><fnt>Z</fnt>;
        <trd lng="hu">hanyattesik</trd>
        <trd lng="it">cadere supini</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        montri, <ind>turni la <tld/>on</ind> al iu
        <klr>(montri sian mal&scirc;aton)</klr>;
        <trd lng="it">voltare le spalle</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>post la <tld/>o</ind> de iu
        <klr>(perfide, ka&scirc;e)</klr>.
        <trd lng="it">alle spalle</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="drasx.0i">dra&scirc;i</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_s;&c_mol;&c_p;&c_ib;&c_n;&c_a;</trd>,
      <trd>&c_p;&c_l;&c_je;&c_ch;&c_y;</trd>
    </trdgrp>
    <trd lng="es">espalda</trd>
    <trdgrp lng="it">
      <trd>dorso</trd>,
      <trd>schiena</trd>
    </trdgrp>
    <trd lng="pl">plecy</trd>
    <trd lng="ru">&c_s;&c_p;&c_i;&c_n;&c_a;</trd>
  </snc>
  <snc mrk="dors.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Supera parto de la korpo de bestoj, de kapo &gcirc;is vosto:
      <ekz>
        grimpi sur la <tld/>on de &ccirc;evalo;
      </ekz>
      <ekz>
        la kato &gcirc;ibigis la <tld/>on
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_mol;&c_p;&c_ib;&c_n;&c_a;</trd>
    <trdgrp lng="it">
      <trd>dorso</trd>,
      <trd>schienale<klr> (di bestia macellata)</klr></trd>
    </trdgrp>
    <trd lng="pl">grzbiet</trd>
    <trd lng="ru">&c_s;&c_p;&c_i;&c_n;&c_a;</trd>
  </snc>
  <snc mrk="dors.0o.malantauxa">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malanta&ubreve;a parto de staranta objekto:
      <ekz>
        <tld/>o de klingo, de libro, de mano, de folio;
      </ekz>
      <ekz>
        mi <klr>...</klr> ekfumis, rekti&gcirc;ante kaj min apogante al
        la 
        se&gcirc;<tld/>o
        <fnt><aut>I. G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>, 
        <lok>[1898?]</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="it">
      <trd>dorso</trd>,
      <trd>retro</trd>,
      <trd>costa<klr> (il retro)</klr></trd>
    </trdgrp>
    <trd lng="pl">plecy</trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_y;&c_l;&c_mol;&c_n;&c_a;&c_ja;
      &c_s;&c_t;&c_o;&c_r;&c_o;&c_n;&c_a;</trd>,
      <trd>&c_o;&c_b;&c_o;&c_r;&c_o;&c_t;&c_n;&c_a;&c_ja;
      &c_s;&c_t;&c_o;&c_r;&c_o;&c_n;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="dors.0o.kurba">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Supera kurba parto de ku&scirc;anta objekto:
      <ekz>
        la nuda <tld/>o de l' monto;
      </ekz>
      <ekz>
        la &scirc;a&ubreve;maj <tld/>oj de la ondoj..
      </ekz>
    </dif>
    <trdgrp lng="it">
      <trd>dorso</trd>,
      <trd>cresta<klr> (di monte od onda)</klr></trd>
    </trdgrp>
    <trd lng="pl">grzbiet</trd>
  </snc>
  <trd lng="bg">&c_g;&c_r;&c_malmol;&c_b;</trd>
  <trd lng="de">R&uuml;cken</trd>
  <trd lng="en">back</trd>
  <trd lng="es">dorso</trd>
  <trd lng="fr">dos</trd>
  <trd lng="he">&gimel;&bet;</trd>
  <trd lng="hu">h&aacute;t</trd>
  <trd lng="id">punggung</trd>
  <trd lng="nl">rug</trd>
  <trdgrp lng="pt">
    <trd>costas</trd>,
    <trd>dorso</trd>
  </trdgrp>
</drv>

<drv mrk="dors.0al0e">
  <kap><tld/>al<tld/>e</kap>
  <snc mrk="dors.0al0e.KOMUNE">
    <dif>
      Duope, kun dorso turnita al la alia kaj
      rigardante en malaj direktoj:
      <ekz>
        &scirc;ia patro diris nenion, kaj ili sidis 
        <tld/>-al-<tld/>e sur la malseka &scirc;tono
        <fnt>
          <aut>H. Laxness, trad. B. Ragnarsson</aut>,
          <vrk>Sendependaj homoj</vrk>,
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="front.0al0e.KOMUNE">frontalfronte</ref>
    </refgrp>
  </snc>
  <trd lng="fr">dos &agrave; dos<klr> (adv.)</klr></trd>
</drv>

<drv mrk="dors.0en">
  <kap><tld/>en, <var><kap><tld/>direkte</kap></var></kap>
  <snc mrk="dors.0en.KOMUNE">
    <dif>
      Returnen:
      <ekz>
        la anasoj iris <tld/>en
        <fnt><bib>Fab2</bib>, <lok>Dio de dormo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_z;&c_a;&c_d;</trd>
  <trdgrp lng="de">
    <trd>r&uuml;ckw&auml;rts</trd>,
    <trd>nach hinten</trd>;
    <trd>r&uuml;cklings</trd>
  </trdgrp>
  <trd lng="fr">en <ind>arri&egrave;re</ind></trd>
  <trd lng="he">&alef;&het;&vav;&resh;&he;</trd>
  <trd lng="hu">h&aacute;trafel&eacute;</trd>
  <trd lng="it">indietro</trd>
  <trd lng="nl">achteruit</trd>
  <trdgrp lng="pl">
    <trd>wstecz</trd>,
    <trd>do <ind>ty&lstroke;u</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_z;&c_a;&c_d;</trd>
</drv>

<drv mrk="dors.0e.de">
  <kap><tld/>e de</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="dors.0e.de.KOMUNE">
    <dif>
      <ref tip="super" cel="antaux.mal0">Malanta&ubreve;</ref>, en la
      dorsa
      direkto de:
      <ekz>
        <tld/>e de ili ondi&gcirc;is servista rumoro
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 88a, volumo 3a, p.
        122a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>dans le <ind>dos</ind> de</trd>,
    <trd>derri&egrave;re</trd>
  </trdgrp>
  <trd lng="hu">h&aacute;ta m&ouml;g&ouml;tt</trd>
  <trd lng="it">dietro</trd>
  <trd lng="nl">aan de <ind>achterzijde</ind> van</trd>
  <trdgrp lng="pl">
    <trd>za <ind>plecami</ind></trd>,
    <trd>z <ind>ty&lstroke;u</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_z;&c_a;&c_d;&c_i;</trd>,
    <trd>&c_p;&c_o;&c_z;&c_a;&c_d;&c_i;</trd>,
    <trd>&c_z;&c_a; &c_s;&c_p;&c_i;&c_n;&c_o;&c_j;</trd>
  </trdgrp>
</drv>
</art>
