<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Azi</rad>/o
</kap>
<drv mrk="azi.A0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      La <ref tip="super" cel="mond.0oparto">mondoparto</ref>
      inter E&ubreve;ropo kaj Nordameriko:
      <ekz>
        &leftquot;nun vi povas jam vidi la Himalajon, &gcirc;i estas la
        plej alta montaro en <tld/>o, <klr>[...]</klr>&rightquot;,
        diris la orienta vento
        <fnt><bib>Fab1</bib>, <lok>&Gcirc;ardeno de la paradizo</lok></fnt>;
      </ekz>
      <ekz>
        <fnt><bib>Fab1</bib>, <lok></lok></fnt>;
        kaj la rozo ardis pli bele ol anta&ubreve;e, &gcirc;i estis ja en
        sia varma Azio
        <fnt><bib>Fab2</bib>, <lok>Rozo de la tombo de Homero</lok></fnt>;
      </ekz>
      <ekz>
        kelkdeko da parfumoj, koloraj vitroj, mebloj, vazoj,
        teksa&jcirc;oj, veturiloj, &ccirc;io &ccirc;i venas al ni
        el <tld/>o
        <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 3</lok></fnt>;
      </ekz>
      <ekz>
        A&ubreve;skultu do, <tld/>o, mamnutristino nia, <klr>/</klr>
        Afriko sfinkse muta, tribar' Oceania <klr>...</klr>
        <fnt>
        <aut>N. Hohlov</aut>: <vrk>E&ubreve;ropo</vrk>.</fnt>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="euxraz.0o">E&ubreve;razio</ref>
  </snc>
  <trd lng="de">Asien</trd>
  <trd lng="el">&Alfa;&sigma;&jota_ton;&alfa;</trd>
  <trdgrp lng="fr">
    <trd>Asie</trd>
  </trdgrp>
  <trd lng="hu">&Aacute;zsia</trd>
  <trd lng="nl">Azi&euml;</trd>
  <trd lng="ru">&c_A;&c_z;&c_i;&c_ja;</trd>
</drv>

<drv mrk="azi.0ano">
  <kap>
    <tld lit="a"/>ano
  </kap>
  <snc>
    <dif>
      Lo&gcirc;anto de <ref tip="malprt" cel="azi.A0o"><tld/>o</ref>:
      <ekz>
        en la vicoj oni povis facile distingi la brunajn Egiptanojn,
        nigrajn Negrojn, flavajn <tld/>anojn kaj blankajn
        lo&gcirc;antojn de Libio kaj de la insuloj de Mediteraneo
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 24</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;irka&ubreve; la palaco staris garde <tld/>anoj
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 5</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Asiate</trd>
  <trdgrp lng="fr">
    <trd>asiatique<klr> (nom)</klr></trd>
  </trdgrp>
  <trd lng="hu">&aacute;zsiai</trd>
  <trd lng="nl">Aziaat</trd>
  <trd lng="ru">&c_a;&c_z;&c_i;&c_a;&c_t;</trd>
</drv>

<drv mrk="azi.A0o.orienta">
  <kap>
    Orienta <tld/>o
  </kap>
  <snc mrk="azi.A0o.orienta.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      La tuta orienta parto de <ref tip="malprt" cel="azi.A0o"><tld/>o</ref>
      en la akvokolekta baseno de <ref cel="pacifik.0o">Pacifiko</ref>,
      de la <ref cel="bering.B0aMarkolo">Beringa Markolo</ref> norde
      &gcirc;is la Malaka Markolo sude, kun la apudaj insuloj.
    </dif>
    <trd lng="ru">&c_D;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j; &c_V;&c_o;&c_s;&c_t;&c_o;&c_k;</trd>
  </snc>
  <snc mrk="azi.A0o.orienta.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      La suda parto de la Orienta
      <tld/>o,<sncref ref="azi.A0o.orienta.GEOG"/>
      kiel fermita regiono de la &ccirc;ina, japana, korea kaj vjetnama
      civilzo kaj kulturo.
    </dif>
    <trd lng="ru">&c_Ju;&c_g;&c_o;-&c_V;&c_o;&c_s;&c_t;&c_o;&c_ch;&c_n;&c_a;&c_ja; &c_A;&c_z;&c_i;&c_ja;</trd>
  </snc>
  <trd lng="de">Ostasien</trd>
  <trdgrp lng="en">
    <trd>East Asia</trd>,
    <trd>Far East</trd>
  </trdgrp>
  <trd lng="fr">Extr&ecirc;me-Orient</trd>
</drv>

<drv mrk="azi.A0o.okcidenta">
  <kap>
    Okcidenta <tld/>o
  </kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      La okcidenta parto de <ref tip="malprt" cel="azi.A0o"><tld/>o</ref>
      (<ref tip="prt" cel="anatoli.0o">Anatolio</ref>,
      Ka&ubreve;kazo,
      <ref tip="prt" cel="mezopotami.0o">Mezopotamio</ref>,
      Irano, Arabujo).
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>Moyen-Orient</trd>
  </trdgrp>
  <trd lng="ru">&c_P;&c_je;&c_r;&c_je;&c_d;&c_n;&c_ja;&c_ja; &c_A;&c_z;&c_i;&c_ja;</trd>
</drv>

<drv mrk="azi.Malgrand0o">
  <kap>
    Malgrand-<tld/>o
  </kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="anatoli.0o">Anatolio</ref>.
    </dif>
  </snc>
  <trd lng="el">&My;&jota;&kappa;&rho;&alfa_ton; &Alfa;&sigma;&jota_ton;&alfa;</trd>
  <trd lng="ru">&c_M;&c_a;&c_l;&c_a;&c_ja; &c_A;&c_z;&c_i;&c_ja;</trd>
  <trdgrp lng="tr">
    <trd>Anadolu</trd>,
    <trd>K&uuml;&ccedil;&uuml;k Asya</trd>
  </trdgrp>
</drv>
</art>
