<drv mrk="azi.A0o">
  <kap><tld/>o</kap>
  <snc mrk="azi.A0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      La <ref tip="lst" cel="mond.0oparto" lst="voko:mondopartoj">mondoparto</ref>
      inter E&ubreve;ropo kaj Nord-Ameriko:
      <ekz>
        &leftquot;nun vi povas jam vidi la Himalajon, &gcirc;i estas la
        plej alta montaro en <tld/>o, <klr>[...]</klr>&rightquot;,
        diris la orienta vento
        <fnt><bib>Fab1</bib> <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <fnt><bib>Fab1</bib>, <lok/></fnt>;
        kaj la rozo ardis pli bele ol anta&ubreve;e, &gcirc;i estis ja
        en sia varma Azio
        <fnt><bib>Fab2</bib> <lok>Rozo de la tombo de Homero</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkdeko da parfumoj, koloraj vitroj, mebloj, vazoj,
        teksa&jcirc;oj, veturiloj, &ccirc;io &ccirc;i venas al ni
        el <tld/>o
        <fnt><bib>Far2</bib> <lok>&ccirc;ap. 3</lok></fnt>;
      </ekz>
      <ekz>
        A&ubreve;skultu do, <tld/>o, mamnutristino nia, <klr>/</klr>
        Afriko sfinkse muta, tribar' Oceania <klr>[&#x2026;]</klr>
        <fnt>
        <aut>N. Hohlov</aut> <vrk>E&ubreve;ropo</vrk>.</fnt>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="euxraz.E0o">E&ubreve;razio</ref>
    <trdgrp lng="zh">
    <trd>&#x4E9A;&#x7EC6;&#x4E9A;&#x6D32; <pr>y&#xE0;x&#xEC;y&#xE0;zh&#x14D;u</pr></trd>,
    <trd>&#x4E9E;&#x7D30;&#x4E9E;&#x6D32; <pr>y&#xE0;x&#xEC;y&#xE0;zh&#x14D;u</pr></trd>,
    <trd>&#x4E9A;&#x6D32; <pr>y&#xE0;zh&#x14D;u</pr></trd>,
    <trd>&#x4E9E;&#x6D32; <pr>y&#xE0;zh&#x14D;u</pr></trd>,
    <trd>&#x6CF0;&#x4E1C; <pr>t&#xE0;id&#x14D;ng</pr></trd>,
    <trd>&#x6CF0;&#x6771; <pr>t&#xE0;id&#x14D;ng</pr></trd>
  </trdgrp>
</snc>
  <trd lng="ja">&#x30A2;&#x30B8;&#x30A2;  <pr>&#x3042;&#x3058;&#x3042;</pr></trd>
  <trd lng="lat">Asia</trd>
  <trd lng="be">&c_A;&c_z;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_A;&c_z;&c_i;&c_ja;</trd>
  <trd lng="ca">&Agrave;sia</trd>
  <trd lng="cs">Asie</trd>
  <trd lng="de">Asien</trd>
  <trd lng="el">&Alfa;&sigma;&jota_ton;&alfa;</trd>
  <trd lng="en">Asia</trd>
  <trd lng="es">Asia</trd>
  <trd lng="fr">Asie</trd>
  <trd lng="fy">Aazje</trd>
  <trd lng="hu">&Aacute;zsia</trd>
  <trd lng="id">Asia</trd>
  <trd lng="it">Asia</trd>
  <trd lng="nl">Azi&euml;</trd>
  <trd lng="pl">Azja</trd>
  <trd lng="pt">&Aacute;sia</trd>
  <trd lng="ru">&c_A;&c_z;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>&Aacute;zia</trd>,
    <trd>&Aacute;zia</trd>
  </trdgrp>
  <trd lng="sv">Asien</trd>
  <trd lng="tok">ma Asija</trd>
  <trd lng="uk">&c_A;&c_z;&c_ib;&c_ja;</trd>
  <trd lng="vo">Siyop</trd>
</drv>

<drv mrk="azi.0ano">
  <kap><tld lit="a"/>ano</kap>
  <snc mrk="azi.0ano.GEOG">
    <dif>
      Lo&gcirc;anto de <ref tip="malprt" cel="azi.A0o"><tld/>o</ref>:
      <ekz>
        en la vicoj oni povis facile distingi la brunajn Egiptanojn,
        nigrajn Negrojn, flavajn <tld/>anojn kaj blankajn
        lo&gcirc;antojn de Libio kaj de la insuloj de Mediteraneo
        <fnt><bib>Far1</bib> <lok>&ccirc;ap. 24a</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;irka&ubreve; la palaco staris garde <tld/>anoj
        <fnt><bib>Far1</bib> <lok>&ccirc;ap. 5a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x4E9A;&#x6D32;&#x4EBA; <pr>y&#xE0;zh&#x14D;ur&#xE9;n</pr></trd>,
    <trd>&#x4E9E;&#x6D32;&#x4EBA; <pr>y&#xE0;zh&#x14D;ur&#xE9;n</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_z;&c_ib;&c_ja;&c_t;</trd>
  <trd lng="bg">&c_a;&c_z;&c_i;&c_a;&c_t;&c_je;&c_c;</trd>
  <trd lng="ca">asi&agrave;tic</trd>
  <trd lng="de">Asiate</trd>
  <trd lng="en">Asian</trd>
  <trd lng="es">asi&aacute;tico</trd>
  <trd lng="fr">asiatique<klr> (subst.)</klr></trd>
  <trd lng="hu">&aacute;zsiai</trd>
  <trd lng="id"><ind>orang</ind> Asia</trd>
  <trd lng="it">asiatico<klr> (sost.)</klr></trd>
  <trd lng="nl">Aziaat</trd>
  <trd lng="pl">azjata</trd>
  <trd lng="ru">&c_a;&c_z;&c_i;&c_a;&c_t;</trd>
  <trd lng="sk">&Aacute;zij&ccaron;an</trd>
  <trd lng="sv">asiat</trd>
  <trd lng="tok">jan Asija</trd>
  <trd lng="vo">Siyopan</trd>
</drv>

<drv mrk="azi.InternaA0o">
  <kap>Interna <tld/>o,
  <var><kap>Centra <tld/>o</kap></var></kap>
  <snc mrk="azi.InternaA0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      La meza parto de <ref tip="malprt" cel="azi.A0o"><tld/>o</ref>
      suden de Siberio, la baseno senelfluaj riveroj: la
      Arala baseno, Tibeto:
      <ekz>
        &leftquot;ili estis transportitaj en Centran <tld/>on dum la dua mondmilito, &ccirc;u ne?&rightquot;
        diris <nom>Tuurken</nom>
        <fnt>
          <aut>J. Valano</aut>
          <vrk>&Ccirc;u ni kunvenis vane?</vrk>
          <lok>1982</lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      &leftquot;La Centra <tld/>o&rightquot;,
      por iuj sinonimas kun la tuta Interna <tld/>o, por aliaj,
      kun ties Tibeta parto (dum la Aralan kaj Bal&hcirc;a&scirc;an
      basenojn oni iam nomas "la Meza <tld/>o&rightquot; a&ubreve;
      Turkestano).
    </rim>
  </snc>
  <trd lng="be">&c_S;&c_ja;&c_r;&c_e;&c_d;&c_n;&c_ja;&c_ja; &c_A;&c_z;&c_ib;&c_ja;</trd>
  <trd lng="cs">St&rcaron;edn&iacute; Asie</trd>
  <trd lng="de">Innerasien</trd>
  <trdgrp lng="en">
    <trd>Central <ind>Asia</ind></trd>
  </trdgrp>
  <trd lng="fr">Asie centrale</trd>
  <trd lng="hu">Bels&odblac;-&Aacute;zsia</trd>
  <trd lng="id"><ind>Asia</ind> Tengah</trd>
  <trd lng="it"><ind>Asia</ind> Centrale</trd>
  <trd lng="ja">&#x4E2D;&#x592E;&#x30A2;&#x30B8;&#x30A2; 
  <pr>&#x3061;&#x3085;&#x3046;&#x304A;&#x3046;&#x30A2;&#x30B8;&#x30A2;</pr></trd>
  <trd lng="nl">Midden-<ind>Azi&euml;</ind></trd>
  <trdgrp lng="pl">
    <trd>Azja &sacute;rodkowa</trd>,
    <trd>Azja wewn&eogonek;trzna</trd>
  </trdgrp>
  <trd lng="ru">&c_V;&c_n;&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_ja;&c_ja;
  &c_A;&c_z;&c_i;&c_ja;</trd>
  <trd lng="sk">Stredn&aacute; &Aacute;zia</trd>
  <trdgrp lng="zh">
    <trd>&#x4E2D;&#x4E9A; <pr>zh&#x14D;ngy&#xE0;</pr></trd>,
    <trd>&#x4E2D;&#x4E9E; <pr>zh&#x14D;ngy&#xE0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="azi.MalgrandA0o">
  <kap>Malgrand-<tld/>o,
  <var><kap>Malgranda <tld/>o</kap></var></kap>
  <snc mrk="azi.MalgrandA0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="dif" cel="anatoli.0o">Anatolio</ref>:
      <ekz>
        lia vo&ccirc;o atingus trans Konstantinopolo &gcirc;is Malgrand-<tld/>o
        <fnt>
          <aut>-</aut>
          <vrk>El la mondo</vrk>
          <lok><url ref="http://anno.onb.ac.at/cgi-content/anno-plus?aid=e0a&amp;datum=1892&amp;page=23">
          La esperantisto, 1892:2, p. 23a</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        grekoj el la insuloj kaj el Malgranda <tld/>o
        <fnt>
          <aut>H, Sienkiewicz, trad. Lidja Zamenhof</aut>
          <vrk>Quo Vadis</vrk>
          <lok>1933</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x5B89;&#x90A3;&#x6258;&#x5229;&#x4E9A; <pr>&#x101;nn&#xE0;tu&#x14D;l&#xEC;y&#xE0;</pr></trd>,
    <trd>&#x5B89;&#x90A3;&#x8A17;&#x5229;&#x4E9E; <pr>&#x101;nn&#xE0;tu&#x14D;l&#xEC;y&#xE0;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_M;&c_a;&c_l;&c_a;&c_ja; &c_A;&c_z;&c_ib;&c_ja;</trd>
  <trdgrp lng="ca">
    <trd>&Agrave;sia Menor</trd>,
    <trd>Anat&ograve;lia</trd>
  </trdgrp>
  <trd lng="de">Kleinasien</trd>
  <trd lng="el">&My;&jota;&kappa;&rho;&alfa_ton; &Alfa;&sigma;&jota_ton;&alfa;</trd>
  <trd lng="en">Asia Minor</trd>
  <trd lng="fr">Asie mineure</trd>
  <trd lng="hu">Kis-&Aacute;zsia</trd>
  <trd lng="id"><ind>Asia</ind> Kecil</trd>
  <trd lng="it"><ind>Asia</ind> Minore</trd>
  <trd lng="lat">Asia minor</trd>
  <trd lng="nl">Klein-<ind>Azi&euml;</ind></trd>
  <trd lng="pl">Azja Mniejsza</trd>
  <trd lng="ru">&c_M;&c_a;&c_l;&c_a;&c_ja; &c_A;&c_z;&c_i;&c_ja;</trd>
  <trdgrp lng="tr">
    <trd>Anadolu</trd>,
    <trd>K&uuml;&ccedil;&uuml;k Asya</trd>
  </trdgrp>
</drv>

<drv mrk="azi.OkcidentaA0o">
  <kap>Okcidenta <tld/>o</kap>
  <snc mrk="azi.OkcidentaA0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      La okcidenta parto de <ref tip="malprt" cel="azi.A0o">
      <tld/>o</ref>:
      <ref tip="prt" cel="anatoli.0o">Anatolio</ref>,
      Ka&ubreve;kazo,
      <ref tip="prt" cel="mezopotami.M0o">Mezopotamio</ref>,
      Irano, Arabujo:
      <ekz>
        fine de la Unua Mondmilito, la &ccirc;efa historia fakto en Okcidenta <tld/>o estas la
        disspliti&gcirc;o de la Otomana imperio
        <fnt>
          <aut>L. Bresler, D. Kessous, G. Mickle</aut>
          <vrk><url ref="https://satesperanto.org/spip.php?article1304">La palestinan problemon
          povos solvi nur socia movado reale laika</url></vrk>
          <lok>SAT Kulturo, [vidita en 2022]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_Z;&c_a;&c_h;&c_o;&c_d;&c_n;&c_ja;&c_ja; &c_A;&c_z;&c_ib;&c_ja;</trd>
  <trd lng="ca">Orient Mitj&agrave;</trd>
  <trd lng="cs">Z&aacute;padn&iacute; Asie</trd>
  <trd lng="de">Westasien</trd>
  <trdgrp lng="en">
    <trd>Middle <ind>East</ind></trd>,
    <trd>Near <ind>East</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Moyen-Orient</trd>
  </trdgrp>
  <trd lng="hu">Nyugat-&Aacute;zsia</trd>
  <trdgrp lng="id">
    <trd><ind>Asia</ind> Barat</trd>,
    <trd><ind>Timur</ind> Tengah</trd>
  </trdgrp>
  <trd lng="it">Medio <ind>Oriente</ind></trd>
  <trdgrp lng="nl">
    <trd>West-<ind>Azi&euml;</ind></trd>,
    <trd>Nabije <ind>Oosten</ind></trd>
  </trdgrp>
  <trd lng="pl">Bliski Wsch&oacute;d</trd>
  <trd lng="ru">&c_P;&c_je;&c_r;&c_je;&c_d;&c_n;&c_ja;&c_ja;
  &c_A;&c_z;&c_i;&c_ja;</trd>
  <trd lng="sk">Z&aacute;padn&aacute; &Aacute;zia</trd>
</drv>

<drv mrk="azi.OrientaA0o">
  <kap>Orienta <tld/>o</kap>
  <snc mrk="azi.OrientaA0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      La tuta orienta parto de <ref tip="malprt" cel="azi.A0o"><tld/>o</ref>
      en la akvokolekta baseno de <ref cel="pacifik.P0o">Pacifiko</ref>,
      de la <ref cel="kol.BeringaMar0o">Beringa Markolo</ref> norde
      &gcirc;is la Malaka Markolo sude, kun la apudaj insuloj.
    </dif>
    <trdgrp lng="be">
      <trd>&c_D;&c_a;&c_l;&c_jo;&c_k;&c_ib; &c_W;&c_s;&c_h;&c_o;&c_d;</trd>,
      <trd>&c_U;&c_s;&c_h;&c_o;&c_d;&c_n;&c_ja;&c_ja; &c_A;&c_z;&c_ib;&c_ja;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>Extrem <ind>Orient</ind></trd>,
      <trd>Orient Lluny&agrave;</trd>
    </trdgrp>
    <trd lng="de">Ostasien</trd>
    <trdgrp lng="en">
      <trd>East <ind>Asia</ind></trd>,
      <trd>Far <ind>East</ind></trd>
    </trdgrp>
    <trd lng="it">Estremo <ind>Oriente</ind></trd>
    <trdgrp lng="nl">
      <trd>Oost-<ind>Azi&euml;</ind></trd>,
      <trd>Verre <ind>Oosten</ind></trd>
    </trdgrp>
    <trd lng="pl">Daleki Wsch&oacute;d</trd>
    <trd lng="ru">&c_D;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j;
    &c_V;&c_o;&c_s;&c_t;&c_o;&c_k;</trd>
    <trdgrp lng="zh">
    <trd>&#x4E1C;&#x4E9A; <pr>d&#x14D;ngy&#xE0;</pr></trd>,
    <trd>&#x6771;&#x4E9E; <pr>d&#x14D;ngy&#xE0;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="azi.OrientaA0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      La suda parto de la Orienta
      <tld/>o<sncref ref="azi.OrientaA0o.GEOG"/>,
      kiel fermita regiono de la &ccirc;ina, japana, korea kaj vjetnama
      civilizo kaj kulturo.
    </dif>
    <trd lng="be">&c_P;&c_a;&c_w;&c_d;&c_n;&c_jo;&c_v;&c_a;-&c_W;&c_s;&c_h;&c_o;&c_d;&c_n;&c_ja;&c_ja; &c_A;&c_z;&c_ib;&c_ja;</trd>
    <trd lng="ru">
    &c_Ju;&c_g;&c_o;-&c_V;&c_o;&c_s;&c_t;&c_o;&c_ch;&c_n;&c_a;&c_ja;
    &c_A;&c_z;&c_i;&c_ja;</trd>
    <trd lng="de">S&uuml;dostasien</trd>
    <trdgrp lng="en">
      <trd>Southeast <ind>Asia</ind></trd>
    </trdgrp>
    <trd lng="it"><ind>Sudest</ind> Asiatico</trd>
    <trdgrp lng="nl">
      <trd>Oost-<ind>Azi&euml;</ind></trd>,
      <trd>Verre <ind>Oosten</ind></trd>
    </trdgrp>
    <trd lng="pl">Azja po&lstroke;udniowo-wschodnia</trd>
    <trdgrp lng="zh">
    <trd>&#x4E1C;&#x5357;&#x4E9A; <pr>d&#x14D;ngn&#xE1;ny&#xE0;</pr></trd>,
    <trd>&#x6771;&#x5357;&#x4E9E; <pr>d&#x14D;ngn&#xE1;ny&#xE0;</pr></trd>
  </trdgrp>
</snc>
  <ref tip="malprt" cel="azi.A0o"><tld/>o</ref>
  <trd lng="de">Ostasien</trd>
  <trd lng="fr">Extr&ecirc;me-Orient</trd>
  <trdgrp lng="hu">
    <trd>Kelet-&Aacute;zsia</trd>,
    <trd>T&aacute;vol-Kelet</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>Asia</ind> Timur</trd>,
    <trd><ind>Timur</ind> Jauh</trd>
  </trdgrp>
  <trd lng="ja">&#x6771;&#x30A2;&#x30B8;&#x30A2;  <pr>&#x3072;&#x304C;&#x3057;&#x30A2;&#x30B8;&#x30A2;</pr></trd>
  <trd lng="sk">&Dcaron;alek&yacute; v&yacute;chod</trd>
  <trdgrp lng="cs">
    <trd>D&aacute;ln&yacute; V&yacute;chod</trd>,
    <trd>Jihov&yacute;chodn&iacute; Asie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_D;&c_a;&c_l;&c_je;&c_k;&c_i;&c_j; &c_S;&c_h;&c_ib;&c_d;</trd>,
    <trd>&c_P;&c_ib;&c_v;&c_d;&c_je;&c_n;&c_n;&c_o;-&c_S;&c_h;&c_ib;&c_d;&c_n;&c_a;&nbsp;&c_A;&c_z;&c_ib;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="azi.SudaA0o">
  <kap>Suda <tld/>o</kap>
  <snc mrk="azi.SudaA0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      La suda parto de <ref tip="malprt" cel="azi.A0o"><tld/>o</ref>,
      precipe la parto de la baseno de la Hinda Oceano
      &ccirc;irka&ubreve;ita
      de la orienta, interna kaj okcidenta partoj de <tld/>o.
    </dif>
    <trdgrp lng="zh">
    <trd>&#x5357;&#x4E9A; <pr>n&#xE1;ny&#xE0;</pr></trd>,
    <trd>&#x5357;&#x4E9E; <pr>n&#xE1;ny&#xE0;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_P;&c_a;&c_w;&c_d;&c_n;&c_jo;&c_v;&c_a;&c_ja; &c_A;&c_z;&c_ib;&c_ja;</trd>
  <trd lng="de">S&uuml;dasien</trd>
  <trdgrp lng="en">
    <trd>South Asia</trd>,
    <trd>Southern Asia</trd>,
  </trdgrp>
  <trd lng="fr">Asie du sud</trd>
  <trd lng="hu">D&eacute;l-&Aacute;zsia</trd>
  <trd lng="id"><ind>Asia</ind> Selatan</trd>
  <trd lng="nl">Zuid-<ind>Azi&euml;</ind></trd>
  <trd lng="pl">Azja po&lstroke;udniowa</trd>
  <trd lng="ru">&c_Ju;&c_zh;&c_n;&c_a;&c_ja; &c_A;&c_z;&c_i;&c_ja;</trd>
</drv>
</art>
