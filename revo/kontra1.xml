<vortaro>
<art mrk="$Id: kontra1.xml,v 1.24 2023/12/03 09:58:57 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>kontrabas</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kontra1.0o">
  <kap><tld/>o</kap>
  <snc mrk="kontra1.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Plej granda ar&ccirc;a <ref tip="lst" cel="instru2.kord0o.MUZ" lst="voko:kordinstrumentoj">kordinstrumento</ref> havanta plej 
      malakutan tonon:
      <ekz>
        la varieco de uzitaj muzikinstrumentoj (violono, akordiono, pian-klavaro, drumo,
        <tld/>o, bas-gitaro, mandolino, buzuko, citro) tute ne ka&scirc;as la agrablan,
        amikecan basan vo&ccirc;on de <nom>Yvart</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>Alexander Shlafer</aut>
          <vrk><url ref="&Monato;/2010/009749.php">Plia sukceso de Jacques Yvart</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_t;&c_r;&c_a;&c_b;&c_a;&c_s;</trd>
  <trd lng="bg">&c_k;&c_o;&c_n;&c_t;&c_r;&c_a;&c_b;&c_a;&c_s;</trd>
  <trd lng="ca">contrabaix</trd>
  <trdgrp lng="cs">
    <trd>basa</trd>,
    <trd>kontrabas</trd>
  </trdgrp>
  <trd lng="de">Kontrabass</trd>
  <trdgrp lng="en">
    <trd>bass</trd>,
    <trd>bass viol</trd>,
    <trd>double bass</trd>
  </trdgrp>
  <trd lng="es">contrabajo</trd>
  <trd lng="fr">contrebasse</trd>
  <trdgrp lng="hu">
    <trd>nagyb&odblac;g&odblac;</trd>,
    <trd>kontrabasszus</trd>
  </trdgrp>
  <trd lng="ja">&#x30B3;&#x30F3;&#x30C8;&#x30E9;&#x30D0;&#x30B9;</trd>
  <trd lng="nl">contrabas</trd>
  <trd lng="pl">kontrabas</trd>
  <trdgrp lng="pt">
    <trd>contrabaixo</trd>,
    <trd>rabec&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_t;&c_r;&c_a;&c_b;&c_a;&c_s;</trd>
  <trd lng="sk">kontrabas</trd>
  <trd lng="zh">&#x4F4E;&#x97F3;&#x63D0;&#x7434; <pr>d&#x12B;y&#x12B;nt&#xED;q&#xED;n</pr></trd>
</drv>
</art>
