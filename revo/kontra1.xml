<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kontrabas</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kontra1.0o">
  <kap><tld/>o</kap>
  <snc mrk="kontra1.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Plej granda ar&ccirc;a <ref tip="super"
      cel="instru2.kord0o.MUZ">kordinstrumento</ref> havanta plej 
      malakutan tonon, speco de trikorda violonego.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_t;&c_r;&c_a;&c_b;&c_a;&c_s;</trd>
  <trd lng="bg">&c_k;&c_o;&c_n;&c_t;&c_r;&c_a;&c_b;&c_a;&c_s;</trd>
  <trd lng="ca">contrabaix</trd>
  <trd lng="de">Kontrabass</trd>
  <trdgrp lng="en">
    <trd>bass</trd>,
    <trd>bass viol</trd>,
    <trd>double bass</trd>
  </trdgrp>
  <trd lng="es">contrabajo</trd>
  <trd lng="fr">contrebasse</trd>
  <trdgrp lng="hu">
    <trd>nagyb&odblac;g&odblac;</trd>,
    <trd>kontrabasszus</trd>
  </trdgrp>
  <trd lng="nl">contrabas</trd>
  <trd lng="pl">kontrabas</trd>
  <trdgrp lng="pt">
    <trd>contrabaixo</trd>,
    <trd>rabec&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_t;&c_r;&c_a;&c_b;&c_a;&c_s;</trd>
</drv>
</art>
