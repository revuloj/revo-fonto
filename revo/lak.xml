<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lak</rad>/o
</kap>
<drv mrk="lak.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lak.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Speco de rezina solva&jcirc;o uzata por surkovri kaj glazurbriligi
      objektojn.
    </dif>
    <refgrp tip="vid">
      <ref cel="emajl.0o.TEK">emajlo <sncref/></ref>,
      <ref cel="farb.0o">farbo</ref>,
      <ref cel="glace.0a">glacea</ref>,
      <ref cel="glazur.0o.TEK">glazuro <sncref/></ref>
    </refgrp>
    <trd lng="de">Lackfirnis</trd>
  </snc>
  <!--
  <snc>
    <dif>
      Natura gumo, kiu eli&gcirc;as el diversaj arboj de Hindujo.
    </dif>
    <trd lng="de">Lack</trd>
  </snc>
  -->
  <trd lng="be">&c_l;&c_a;&c_k;</trd>
  <trd lng="br">lakenn</trd>
  <trd lng="el">&lambda;&alfa_ton;&kappa;&alfa;</trd>
  <trd lng="en">laquer</trd>
  <trdgrp lng="fr">
    <trd>laque</trd>,
    <trd>vernis</trd>
  </trdgrp>
  <trd lng="hu">lakk</trd>
  <trd lng="nl">lak</trd>
  <trdgrp lng="pt">
    <trd>verniz</trd>,
    <trd>(goma-)laca</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_a;&c_k;</trd>
</drv>

<drv mrk="lak.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="lak.0i.KEM">
    <dif>
      &Scirc;miri per <tld/>o:
      <ekz>
        dek du se&gcirc;oj gravuritaj kaj <tld/>itaj
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 53a, volumo 2a, p.
        192a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_k;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="br">lakenna&ntilde;</trd>
  <trd lng="de">lackieren</trd>
  <trd lng="en">laquer</trd>
  <trdgrp lng="fr">
    <trd>laquer</trd>,
    <trd>vernir</trd>
  </trdgrp>
  <trd lng="hu">lakkoz</trd>
  <trd lng="nl">lakken</trd>
  <trd lng="ru">&c_l;&c_a;&c_k;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
