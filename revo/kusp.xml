<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kusp.xml,v 1.23 2025/05/02 20:30:40 revo Exp $">
<kap>
  <rad>kusp</rad>/i <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="kusp.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kusp.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="klin.0i.MOV">Klini</ref> a&ubreve; <ref tip="super" cel="fald.0i">faldi</ref>
      ion kontra&ubreve; la natura direkto:
      <ekz>
        <tld/>i la harojn de kato, la randojn de &ccirc;apelo, sian
        pantalonon;
      </ekz>
      <ekz>
        mi vagis tien kaj reen sur la ferdeko, kun <tld/>ita kolumo kaj la manoj enpo&scirc;e
        <fnt>
          <bib>Kiso</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Kasem</nom> <tld/>is la manikojn, malfermis tenajlon
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#8470; 3 (101)</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ofte surhavis jakon strikte zonitan,
        garnitan per <tld/>itaj manikoj
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 63a, p. 372a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="tauxz.0i.malordige">ta&ubreve;zi<sncref/></ref>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Anka&ubreve; eblas diri <ke>kontra&ubreve;faldi</ke>, <ke>kontra&ubreve;karesi</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_z;&c_g;&c_ib;&c_n;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_u;
    &c_a;&c_d;&c_v;&c_a;&c_r;&c_o;&c_t;&c_n;&c_y;&c_m;
    &c_n;&c_a;&c_p;&c_r;&c_a;&c_m;&c_k;&c_u;)</klr></trd>,
    <trd>&c_z;&c_a;&c_k;&c_a;&c_s;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ohnout do protism&ecaron;ru</trd>,
    <trd>p&rcaron;eh&yacute;bat</trd>,
    <trd>&ccaron;esat <klr>(hladit)</klr> proti srsti</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>knicken</trd>,
    <trd>aufkrempeln</trd>,
    <trd>zur&uuml;ckschlagen</trd>,
    <trd>aufschlagen</trd>,
    <trd>umschlagen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd><ind>curl</ind> back</trd>,
    <trd><ind>turn</ind> up</trd>,
    <trd>to <ind>brush</ind> against the grain</trd>,
    <trd><ind>roll</ind> up</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>aller &agrave; <ind>rebrousse-poil</ind></trd>,
    <trd>retrousser</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>felt&udblac;r</trd>,
    <trd>visszahajt</trd>,
    <trd>felborzol</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#36870;&#31435;&#12390;&#12427;  <pr>&#12373;&#12363;&#12384;&#12390;&#12427;</pr></trd>,
    <trd>&#25240;&#12426;&#36820;&#12377;  <pr>&#12362;&#12426;&#12363;&#12360;&#12377;</pr></trd>,
    <trd>&#12414;&#12367;&#12426;&#19978;&#12370;&#12427;
    <pr>&#12414;&#12367;&#12426;&#12354;&#12370;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>tegen de haren in <ind>strijken</ind></trd>,
    <trd>tegen de draad in <ind>strijken</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>odwija&cacute;</trd>,
    <trd>zakasa&cacute;</trd>,
    <trd>zawija&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>de&scomma;ira</trd>,
    <trd>&icirc;nf&abreve;&scomma;ura</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_g;&c_i;&c_b;&c_a;&c_t;&c_mol; &c_v; &c_o;&c_b;&c_r;&c_a;&c_t;&c_n;&c_o;&c_m;
  &c_n;&c_a;&c_p;&c_r;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_i;</trd>
  <trdgrp lng="sk">
    <trd>1.hladi&tcaron;</trd>,
    <trd>kefova&tcaron; protismerne; 2.vyhrn&uacute;&tcaron; <klr>(nohavice,
    golier)</klr></trd>,
    <trd>&ccaron;esa&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_s;&c_u;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>(&c_p;&c_o;)&c_z;&c_a;&c_k;&c_a;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_o;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_k;&c_a;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i; (&c_r;&c_u;&c_k;&c_a;&c_v;</trd>,
    <trd>&c_sh;&c_t;&c_a;&c_n;&c_i;&c_n;&c_u;)</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_g;&c_o;&c_r;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_b;&c_i;&c_r;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_v;&c_je;&c_r;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_s;&c_m;&c_i;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i; (&c_p;&c_o;&c_d;&c_ib;&c_l;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_u; &c_ib; &c_t;.&c_p;.)</trd>,
    <trd>&c_g;&c_l;&c_a;&c_d;&c_i;&c_t;&c_i; &c_p;&c_r;&c_o;&c_t;&c_i; &c_sh;&c_je;&c_r;&c_s;&c_t;&c_ib;</trd>,
    <trd>&c_k;&c_u;&c_j;&c_o;&c_v;&c_d;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_k;&c_o;&c_sh;&c_l;&c_a;&c_t;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_ch;&c_ib;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i; &c_d;&c_o;&c_g;&c_o;&c_r;&c_i;</trd>,
    <trd>&c_ch;&c_i;&c_s;&c_t;&c_i;&c_t;&c_i; (&c_shch;&c_ib;&c_t;&c_k;&c_o;&c_ju;) &c_p;&c_r;&c_o;&c_t;&c_i; &c_sh;&c_je;&c_r;&c_s;&c_t;&c_ib;</trd>,
    <trd>&c_v;&c_o;&c_r;&c_s;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20498;&#25918; <pr>d&agrave;of&agrave;ng</pr></trd>,
    <trd>&#21367;&#36215;&#26469; <pr>ju&#462;nq&#464;lai</pr></trd>,
    <trd>&#25458;&#36215;&#20358; <pr>ju&#462;nq&#464;lai</pr></trd>,
    <trd>&#31215;&#32047;&#25104; <pr>j&imacron;l&ecaron;ich&eacute;ng</pr></trd>,
    <trd>&#31309;&#32047;&#25104; <pr>j&imacron;l&ecaron;ich&eacute;ng</pr></trd>,
    <trd>&#34949;&#34949;&#19978;&#21319; <pr>ni&#462;oni&#462;osh&agrave;ngsh&emacron;ng</pr></trd>,
    <trd>&#35018;&#35018;&#19978;&#21319; <pr>ni&#462;oni&#462;osh&agrave;ngsh&emacron;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kusp.mal0i">
  <kap>mal<tld/>i</kap>
  <snc mrk="kusp.mal0i.KOMUNE">
    <dif>
      Remeti ion <tld/>itan en naturan, normalan, senfaldan staton:
      <ekz>
        <klr>&scirc;i</klr> saltis sur la plankon, mal<tld/>is sian jupon,
        frapete reordigis siajn nigrajn buklojn kaj eliris el la
        oficejo
        <fnt>
          <aut>L. Beaucaire</aut>,
          <vrk>El la vivo de bervala senta&ubreve;gulo</vrk>,
          <lok>1974</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_g;&c_l;&c_a;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_g;&c_l;&c_a;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">gl&auml;tten</trd>
  <trdgrp lng="en">
    <trd>un<ind>ruffle</ind></trd>,
    <trd>smooth <klr>(something pliable from a ruffled state)</klr></trd>
  </trdgrp>
  <trd lng="fr">d&eacute;froisser</trd>
  <trd lng="pl">wyg&lstroke;adza&cacute;</trd>
  <trd lng="ro">neted </trd>
</drv>
</art>
