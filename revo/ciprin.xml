<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>ciprin</rad>/o</kap>
<drv mrk="ciprin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
    Genro el familio <ref tip="super" cel="ciprin.0edoj"><tld/>edoj</ref>
    (<trd lng="la">Cyprinus</trd>), bongusta riverfi&scirc;o, kies tipo estas
    la <ref tip="sub" cel="karp.0o">karpo</ref>.
    </dif>
  </snc>
  <trd lng="de">Karpfen</trd>
</drv>
<drv mrk="ciprin.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
    Familio el la ordo de <tld/>oformaj fi&scirc;oj
    (<trd lng="la">Cyprinidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Karpfenfische</trd>
</drv>
<drv mrk="ciprin.0formaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
    Ordo el la klaso <ref tip="super" cel="fisx.osto0oj">ostofi&scirc;oj</ref> 
    (<trd lng="la">Cypriniformes</trd>).
    </dif>
  </snc>
  <trd lng="de">Karpfenverwandte</trd>
</drv>
</art>
