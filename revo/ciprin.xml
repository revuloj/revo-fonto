<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ciprin.xml,v 1.32 2023/10/31 13:37:03 revo Exp $">
<kap>
  <rad>ciprin</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ciprin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="ciprin.0o.genro">
    <dif>
      Bongusta <ref tip="lst" cel="fisx.0o" lst="voko:fi&scirc;oj">fi&scirc;o</ref>,
      vivanta en nesala akvo,
      kies tipo estas la <ref tip="sub" cel="karp.0o">karpo</ref>;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio <ref tip="malprt" cel="ciprin1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Cyprinus</trd>).
    </dif>
  </snc>
  <rim>
    En <fnt><bib>PVEF</bib></fnt> (1909) kun indiko <nom>Verax</nom> aperas ankora&ubreve; la varia&jcirc;o
    &leftquot;Cipren&middot;o&rightquot;.
  </rim>
  <rim>
    &leftquot;<tld lit="C"/>o&rightquot; aperas anka&ubreve; kiel alnomo de <ref cel="afrodit.0o">Afrodito</ref>
    <klr>(greke: <nom>&Kappa;&ypsilon_ton;&pi;&rho;&jota;&sigma_fina;</nom>, t.e. el Kipro, do pli &gcirc;uste estus Cipranino)</klr>
    en unu poemo:
    <ekz>
      &gcirc;iaj mastoj tu&scirc;is la lazuron, sur nekonataj maroj &#x2AFD; La ama <tld lit="C"/>o, kun
      senordaj haroj, nudaj ha&ubreve;toj, &#x2AFD; Sterni&gcirc;is &ccirc;e ties pruo, sub ek<klr>[s]</klr>cesa suno
      <fnt>
        <aut>&Eacute;mile Nelligan</aut>
        <vrk><url ref="https://books.google.de/books?id=AXN7EAAAQBAJ&amp;lpg=PA334&amp;ots=jvCjNib2JN&amp;pg=PA334#v=onepage">La Ora &Scirc;ipo</url></vrk>
        <lok>La Kanada Verkaro</lok>
      </fnt>.
    </ekz>
  </rim>
  <trd lng="af">karp</trd>
  <trd lng="be">&c_s;&c_a;&c_z;&c_a;&c_n;</trd>
  <trd lng="bg">&c_sh;&c_a;&c_r;&c_a;&c_n;</trd>
  <trd lng="bs">&scaron;aran</trd>
  <trd lng="ca">carpa comuna</trd>
  <trd lng="da">karpe</trd>
  <trd lng="de">Karpfen</trd>
  <trd lng="el">&kappa;&ypsilon;&pi;&rho;&jota_ton;&ny;&omikron;&sigma_fina;</trd>
  <trd lng="en">carp</trd>
  <trd lng="es">carpa</trd>
  <trd lng="fa">&a_k;&f_p;&a_w;&a_r;</trd>
  <trd lng="fi">karppi</trd>
  <trd lng="fr">carpe</trd>
  <trd lng="he">&qof;&resh;&pe;&yod;&vav;&fnun;</trd>
  <trd lng="hr">&scaron;aran</trd>
  <trd lng="hu">ponty</trd>
  <trd lng="hy">&#x573;&#x561;&#x573;&#x561;&#x576;</trd>
  <trdgrp lng="id">
    <trd><ind>ikan</ind> karper</trd>,
    <trd><ind>ikan</ind> mas</trd>
  </trdgrp>
  <trd lng="is">vatnakarfi</trd>
  <trd lng="it">ciprino</trd>
  <trd lng="ja">&#x9BC9; <pr>&#x3053;&#x3044;</pr></trd>
  <trd lng="ko">&#xC789;&#xC5B4;</trd>
  <trd lng="nl">karper</trd>
  <trd lng="no">karpe</trd>
  <trdgrp lng="pl">
    <trd>sazan</trd>,
    <trd>karp</trd>
  </trdgrp>
  <trd lng="pt">carpa</trd>
  <trd lng="ru">&c_s;&c_a;&c_z;&c_a;&c_n;</trd>
  <trd lng="sk">kapor</trd>
  <trd lng="sl">krap</trd>
  <trd lng="sq">krap</trd>
  <trd lng="sr">&c_sh;&c_a;&c_r;&c_a;&c_n;</trd>
  <trd lng="sv">karp</trd>
  <trd lng="tr">sazan</trd>
  <trd lng="uk">&c_k;&c_o;&c_r;&c_o;&c_p;</trd>
  <trd lng="yi">&qof;&alef;&resh;&pe;</trd>
  <adm>
    Mi dubas, kiel enkonduki tien la francan vorton "cyprin", kiu ja
    &scirc;ajnas pli konveni ol "carpe" (karpo). <aut>MB</aut>
  </adm>
  <trd lng="zh">&#x9BC9; <pr>l&#x1D0;</pr></trd>
</drv>

<drv mrk="ciprin.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="ciprin.0oformaj.ordo">
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref> de <ref tip="malprt" cel="fisx.osto0oj">ostofi&scirc;oj</ref>
      (<trd lng="la">Cypriniformes</trd>):
      <ekz>
        la akvofluoj enhavas anka&ubreve; plurajn endemiajn speciojn <klr>[el]</klr> la <tld/>oformaj
        fi&scirc;oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=426203">Iberaj sklerofilaj kaj
          kvaza&ubreve;deciduaj arbaroj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_p;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja;
  <klr tip="amb">(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="ca">cipriniformes</trd>
  <trd lng="de">Karpfenverwandte</trd>
  <trd lng="hu">pontyalak&uacute;ak</trd>
  <trd lng="it">cipriniformi</trd>
  <trdgrp lng="pl">
    <trd>karpiokszta&lstroke;tne</trd>,
    <trd>karpiowate</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_p;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;
  <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_o;&c_r;&c_o;&c_p;&c_o;&c_p;&c_o;&c_d;&c_ib;&c_b;&c_n;&c_ib;</trd>,
    <trd>&c_k;&c_ib;&c_s;&c_t;&c_k;&c_o;&c_v;&c_o;&c_m;&c_ib;&c_h;&c_u;&c_r;&c_o;&c_v;&c_ib;</trd>
  </trdgrp>
  <trd lng="zh">&#x9BC9;&#x5F62;&#x76EE; <pr>l&#x1D0;x&#xED;ngm&#xF9;</pr></trd>
</drv>
</art>
