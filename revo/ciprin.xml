<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>ciprin</rad>/o</kap>
<drv mrk="ciprin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="ciprin.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Cyprinus</trd>), bongusta riverfi&scirc;o, kies tipo estas
      la <ref tip="sub" cel="karp.0o">karpo</ref>.
    </dif>
  </snc>
  <trd lng="de">Karpfen</trd>
  <trd lng="en">carp</trd>
  <trd lng="es">carpa</trd>
  <trd lng="fr">carpe</trd>
  <trd lng="it">carpione</trd>
  <trd lng="nl">karper</trd>
  <trd lng="pt">carpa</trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_p;&c_o;&c_v;&c_y;&c_je;</trd>
  <adm>
    Mi dubas, kiel enkonduki tien la francan vorton "cyprin", kiu ja
    &scirc;ajnas pli konveni ol "carpe" (karpo). <aut>MB</aut>
  </adm>
</drv>

<drv mrk="ciprin.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super"
      cel="fisx.osto0oj">ostofi&scirc;oj</ref>
      (<trd lng="la">Cypriniformes</trd>).
    </dif>
  </snc>
  <trd lng="de">Karpfenverwandte</trd>
</drv>
</art>
