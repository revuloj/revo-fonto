<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>gelaten</rad>/o
</kap>
<drv mrk="gelate.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Azota substanco malflueca sed mola, kiun oni ekstraktas el la ostoj
      k kartilagoj de bestoj. 
    </dif>
  </snc>
  <trd lng="de">Gelatine</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_a;&c_t;&c_i;&c_n;</trd>
</drv>
<drv mrk="gelate.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc num="1">
    <dif>
      Suko el besta substanco, kiu enhavas <tld/>on k malvarmi&gcirc;inte fari&gcirc;as
      malflueca sed mola:
      <ekz><tld/>a&jcirc;o de bovido, de fi&scirc;o;</ekz>
      <ekz>&scirc;inko garnita per <tld/>a&jcirc;o.</ekz>
    </dif>
   <trd lng="de">Gallerte</trd>
  </snc>
  <snc num="2">
    <dif>
      Suko elpremita el fruktoj, kiu, sukerite k malvarmi&gcirc;inte, fari&gcirc;as
      malflueca sed mola kiel <tld/>o:
      <ekz><tld/>a&jcirc;o el pomo, riboj.</ekz>
    </dif>
   <trd lng="de">Gelee</trd>
  </snc>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_je;</trd>
</drv>
<drv mrk="gelate.0eca">
  <kap><tld/>eca</kap>
  <snc>
    <dif>
      Havanta la aspekton de <tld/>o.  
    </dif>
  </snc>
  <trd lng="de">geleeartig</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_je;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
