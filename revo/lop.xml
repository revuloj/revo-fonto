<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lop.xml,v 1.9 2024/07/05 08:31:05 revo Exp $">
<kap>
  <rad>lop</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="lop.0i">
  <kap><tld/>i</kap>
  <snc mrk="lop.0i.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      Vertikale cirkli, unue alsuprante &gcirc;is returni&gcirc;o,
      poste alsubante por restari&gcirc;i en normala sinteno.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_k;&c_o;&c_n;&c_v;&c_a;&c_c;&c_mol; &c_m;&c_jo;&c_r;&c_t;&c_v;&c_u;&c_ju; <ind>&c_p;&c_ja;&c_t;&c_l;&c_ju;</ind></trd>
  <trd lng="cs">u&ccaron;init p&rcaron;emet</trd>
  <trdgrp lng="en">
    <trd>to loop <klr>(aerobatic)</klr></trd>,
    <trd>loop-the-loop</trd>
  </trdgrp>
  <trd lng="fr">faire un <ind>looping</ind></trd>
  <trd lng="ja">&#x5B99;&#x8FD4;&#x308A;&#12377;&#x308B;  <pr>&#12385;&#12421;&#12358;&#x304C;&#12360;&#x308A;&#12377;&#x308B;</pr></trd>
  <trd lng="pl">wykonywa&cacute; <ind>p&eogonek;tl&eogonek;</ind></trd>
  <trd lng="sk">robi&tcaron; v&yacute;vrtky</trd>
  <trd lng="uk">&c_r;&#x43E;&c_b;&c_i;&c_t;&c_i; &#x43F;&c_je;&c_t;&#x43B;&#x44E;</trd>
</drv>

<drv mrk="lop.0o">
  <kap><tld/>o</kap>
  <snc mrk="lop.0o.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      Vertikala cirklado:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la batalantoj <klr>...</klr> enrigardadis la tatuon de la
        veninto,
        senerare legante &ccirc;iujn &gcirc;ian spiralojn kaj <tld/>ojn
        <fnt>
        <aut>L. Lukina, E. Lukin, trad. J. Finkel</aut>,
        <vrk><url ref="http://www.esperanto.mv.ru/Lukin/mis1.html">
        La misiistoj</url></vrk>,
        <lok>2000-2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_jo;&c_r;&c_t;&c_v;&c_a;&c_ja; <ind>&c_p;&c_ja;&c_t;&c_l;&c_ja;</ind></trd>
  <trdgrp lng="cs">
    <trd>lopink</trd>,
    <trd>p&rcaron;emet letadla</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>loop</trd>,
    <trd>loop-the-loop</trd>
  </trdgrp>
  <trd lng="fr">looping</trd>
  <trd lng="ja">&#x5B99;&#x8FD4;&#x308A;  <pr>&#12385;&#12421;&#12358;&#x304C;&#12360;&#x308A;</pr></trd>
  <trd lng="pl">p&eogonek;tla</trd>
  <trdgrp lng="sk">
    <trd>looping</trd>,
    <trd>v&yacute;vrtka</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&c_je;&c_t;&#x43B;&#x44F; (&c_f;&c_ib;&c_g;&c_u;&c_r;&c_a; &c_v;&c_i;&c_shch;&#x43E;&c_g;&#x43E; &#x43F;&c_ib;&#x43B;&#x43E;&c_t;&c_a;&c_zh;&c_u;)</trd>
</drv>
</art>

