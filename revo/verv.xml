<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>verv</rad>/o
</kap>
<drv mrk="verv.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Fervore vigla kaj flama inspiro &ccirc;e poezia, literatura a&ubreve; belarta
      verkado:
      <ekz>
        sen<tld/>a libro;
      </ekz>
      <ekz>
        <tld/>ege polemiki.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Spirita fervoro, fantazia vigleco, spirita svingi&gcirc;a ekflugo &ccirc;e ia
      ajn parolado a&ubreve; agado:
      <ekz>
        &scirc;erci, gesti kun <tld/>o;
      </ekz>
      <ekz>
        plena je energio kaj <tld/>o li triumfis super &ccirc;iuj malhelpoj;
      </ekz>
      <ekz>
        la vivo alprenis novan kaj pli <tld/>an perspektivon;
      </ekz>
      <ekz>
        <tld/>e mar&scirc;i.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_t;&c_h;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_n;&c_a;&c_t;&c_h;&c_n;&c_jo;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schwung</trd>,
    <trd>Begeisterung</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>h&eacute;v</trd>,
    <trd>lend&uuml;let</trd>,
    <trd>t&udblac;z<klr tip="ind"> (&aacute;tv.)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verve</trd>,
    <trd>gloed</trd>,
    <trd>vuur<klr tip="ind"> (fig.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_o;&c_d;&c_u;&c_sh;&c_je;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_y;&c_l;</trd>
  </trdgrp>
</drv>
</art>
