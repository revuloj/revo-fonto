<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: verv.xml,v 1.22 2023/10/21 16:50:36 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>verv</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="verv.0a">
  <kap><tld/>a</kap>
  <snc mrk="verv.0a.KOMUNE">
    <dif>
      <ref cel="vigl.0a.energia">Vigle</ref>, <ref tip="super" cel="fervor.0a">fervore</ref> 
      parolanta a&ubreve; aganta:
      <ekz>
        vigla, <tld/>a, skizema a&ubreve;toro
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        per mallongaj, vivecaj frazoj, karakterizaj je &scirc;ia <tld/>a, observema menso,
        <nom>Ajla</nom> priskribis la okazinta&jcirc;ojn, kiuj tiom maltrankviligis &scirc;in
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fidela azeno <klr>[&#x2026;]</klr> estis rimarkinde fortmuskola kaj <tld/>a besto
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        la traduko de <nom>Newell</nom> estas pli lingve <tld/>a, pli fidela al la angla teksto,
        ofte pli poezia
        <fnt>
          <bib>Plumamikoj</bib>
          <lok>Tradukoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        amba&ubreve; geedzoj lin tre &scirc;atis, &ccirc;ar li estis <tld/>a spritulo, kies humurplena
        konversacio neniam riskis tedi
        <fnt>
          <bib>ChC</bib>
          <lok>7</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dinami.0a.FIG">dinamika<sncref/></ref>,
      <ref cel="elokve.0a">elokventa</ref>,
      <ref cel="energi.0a">energia</ref>,
      <ref cel="entuzi.0a">entuziasma</ref>,
      <ref cel="sagac.0a">sagaca</ref>,
      <ref cel="sprit.0a">sprita</ref>,
      <ref cel="viv.0a.vigla">viva<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_t;&c_h;&c_n;&c_jo;&c_n;&c_y;</trd>,
    <trd>&c_zh;&c_a;&c_r;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_zh;&c_a;&c_r;&c_s;&c_mol;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>vervn&iacute;</trd>,
    <trd>vzletn&yacute;</trd>,
    <trd>zan&iacute;cen&yacute;</trd>,
    <trd>zap&aacute;len&yacute;</trd>,
    <trd>&rcaron;&iacute;zn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schwungvoll</trd>,
    <trd>spritzig</trd>,
    <trd>kraftvoll</trd>,
    <trd>feurig</trd>,
    <trd>dynamisch</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>vehemente</trd>,
    <trd>fogoso</trd>,
    <trd>en&eacute;rgico</trd>,    
    <trd>impetuoso</trd>
  </trdgrp>
  <trd lng="fa">&a_b;&a_A; <ind>&a_sh;&a_w;&a_r; &a_w; &a_sh;&a_w;&a_q;</ind></trd>
  <trdgrp lng="fr">
    <trd>disert</trd>,
    <trd>verveux</trd>,
    <trd>volubile</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x71B1;&#x306E;&#x3053;&#x3082;&#x3063;&#x305F;  <pr>&#x306D;&#x3064;&#x306E;&#x3053;&#x3082;&#x3063;&#x305F;</pr></trd>,
    <trd>&#x6C17;&#x5408;&#x306E;&#x5165;&#x3063;&#x305F;  <pr>&#x304D;&#x3042;&#x3044;&#x306E;&#x306F;&#x3044;&#x3063;&#x305F;</pr></trd>,
    <trd>&#x6D3B;&#x6C17;&#x3042;&#x3075;&#x308C;&#x305F;  <pr>&#x304B;&#x3063;&#x304D;&#x3042;&#x3075;&#x308C;&#x305F;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vzletn&yacute;</trd>,
    <trd>zanieten&yacute;</trd>,
    <trd>zap&aacute;len&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_ib;&c_d;&c_n;&c_je;&c_s;&c_je;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="verv.0o">
  <kap><ofc>3</ofc> <tld/>o</kap>
  <snc mrk="verv.0o.BELE">
    <uzo tip="fak">BELE</uzo>
    <dif>
      Fervore vigla kaj flama <ref cel="inspir.0o">inspiro</ref> &ccirc;e poezia, 
      literatura a&ubreve; belarta verkado:
      <ekz>
        via stilo <klr>[&#x2026;]</klr> estas kerneca, vigla, en multaj lokoj plena de <tld/>o
        kaj de fajro
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la versoj &ccirc;iam fluas glate, tre varie, ofte kun virtuozaj fluecoj kaj fajra
        <tld/>o
        <fnt>
          <bib>Plumamikoj</bib>
          <lok>Dramoj en esperanto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="elokve.0o.KOMUNE">elokvento</ref>
    </refgrp>
    <trdgrp lng="fa">
      <trd>&a_s;&a_kh;&a_n;&a_w;&a_r;&f_y;</trd>,
      <trd>&a_b;&a_l;&a_A;&a_gh;&a_t;</trd>
    </trdgrp>
  </snc>
  <snc mrk="verv.0o.KOMUNE">
    <dif>
      Spirita <ref tip="super" cel="fervor.0o">fervoro</ref>, 
      fantazia <ref cel="vigl.0a.energia">vigleco</ref>, spirita svingi&gcirc;a ekflugo &ccirc;e ia
      ajn parolado a&ubreve; agado:
      <ekz>
	&scirc;erci, gesti kun <tld/>o;
      </ekz>
      <ekz>
        juneca <tld/>o
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun ne kontra&ubreve;starebla <tld/>o li fermis la knabinon inter siajn brakojn
        <fnt>
          <bib>LGA</bib>
          <lok>V.</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj glutoj el la glasoj, kelkaj tiroj de la cigaroj, kaj la diskuto denove
        ekhavis <tld/>on
        <fnt>
          <bib>Kiso</bib>
          <lok>La Sakramento De La Komunio</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam mia <tld/>o ne eltenos, mia energio komencos manki kaj la neregula vivado
        komencos tedi
        <fnt>
          <bib>KrM</bib>
          <lok>23. Kasteloj En Aero.</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="energi.0o.forto">energio<sncref/></ref>,
      <ref cel="entuzi.0o">entuziasmo</ref>,
      <ref cel="viv.0eco">viveco</ref>
    </refgrp>
    <trd lng="de">Tatkraft</trd>
    <trdgrp lng="fa">
      <trd>&a_sh;&a_w;&a_r;<klr> (&a_sh;&a_w;&a_q;)</klr></trd>
      <trd>&a_sh;&a_w;&a_q;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_t;&c_h;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_n;&c_a;&c_t;&c_h;&c_n;&c_jo;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>verva</trd>,
    <trd>vzn&ecaron;t</trd>,
    <trd>z&aacute;pal</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schwung</trd>,
    <trd>Begeisterung</trd>,
    <trd>Verve</trd>,
    <trd>Herzblut</trd>,
    <trd>Elan</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>verve</trd>,
    <trd>zest</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>vehemencia</trd>,
    <trd>&iacute;mpetu</trd>,
    <trd>fogosidad</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>brio</trd>,
    <trd>entrain</trd>,
    <trd>fougue</trd>,
    <trd>panache</trd>,
    <trd>verve</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>h&eacute;v</trd>,
    <trd>lend&uuml;let</trd>,
    <trd>t&udblac;z<klr tip="ind"> (&aacute;tv.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x60C5;&#x71B1;  <pr>&#x3058;&#x3087;&#x3046;&#x306D;&#x3064;</pr></trd>,
    <trd>&#x71B1;&#x6C17;  <pr>&#x306D;&#x3064;&#x3051;</pr></trd>,
    <trd>&#x71B1;  <pr>&#x306D;&#x3061;</pr></trd>,
    <trd>&#x610F;&#x6C17;&#x8FBC;&#x307F;  <pr>&#x3044;&#x304D;&#x3054;&#x307F;</pr></trd>,
    <trd>&#x6C17;&#x5408;  <pr>&#x304D;&#x3042;&#x3044;</pr></trd>,
    <trd>&#x6D3B;&#x6C17;  <pr>&#x304B;&#x3063;&#x304D;</pr></trd>,
    <trd>&#x7CBE;&#x5F69;  <pr>&#x305B;&#x3044;&#x3055;&#x3044;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verve</trd>,
    <trd>gloed</trd>,
    <trd>vuur<klr tip="ind"> (fig.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_o;&c_d;&c_u;&c_sh;&c_je;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_y;&c_l;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odu&scaron;evnenie</trd>,
    <trd>vzlet</trd>,
    <trd>z&aacute;pal</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_n;&c_je;&c_s;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_z;&c_ja;&c_t;&c_t;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
