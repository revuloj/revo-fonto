<vortaro>
<art mrk="$Id: jxartel.xml,v 1.18 2023/12/03 07:09:19 revo Exp $">
<kap>
  <rad>&jcirc;artel</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="jxartel.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Elasta <ref tip="super" cel="sxelk.0o">&scirc;elk</ref>eto
      tenanta &scirc;trumpon stre&ccirc;ita per interagrafo kun supra vesto:
      <ekz>
        fra&ubreve;lino, mi vidas viajn nigrajn <tld/>ojn
        <fnt>
          <bib>KrB</bib>
          <lok>Jam teni min ne povas mi facile</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i portas &scirc;trumpojn el ru&gcirc;a silko kun <tld/>oj el
        blanka tafto
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 49a, p.
        669a</lok></fnt>.
      </ekz>
    </dif>    
    <refgrp tip="vid">
      <ref cel="gxarter.0o">&gcirc;artero</ref>,
      <ref cel="sxtrum.0oligilo0osxelko">&scirc;trumpoligilo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_v;&c_ja;&c_z;&c_k;&c_a;<klr tip="amb"> (&c_d;&c_l;&c_ja;
  &c_p;&c_a;&c_n;&c_ch;&c_o;&c_h;&c_a;&c_w;)</klr></trd>
  <trd lng="bg">&c_zh;&c_a;&c_r;&c_t;&c_i;&c_je;&c_r;</trd>
  <trd lng="de">Straps</trd>
  <trd lng="en">garter belt</trd>
  <trd lng="fr">jarretelle</trd>
  <trd lng="hu">harisnyak&ouml;t&odblac;</trd>
  <trdgrp lng="ja">
    <trd>&#x9774;&#x4E0B;&#x7559;&#x3081;  <pr>&#x304F;&#x3064;&#x3057;&#x305F;&#x3069;&#x3081;</pr></trd>,
    <trd>&#x30AC;&#x30FC;&#x30BF;&#x30FC;</trd>
  </trdgrp>
  <trd lng="nl">jarretel</trd>
  <trd lng="pl">podwi&aogonek;zka</trd>
  <trdgrp lng="pt">
     <trd>liga (para meias)</trd>
     <trd>jarreteira</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_v;&c_ja;&c_z;&c_k;&c_a;<klr tip="amb"> (&c_d;&c_l;&c_ja;
  &c_ch;&c_u;&c_l;&c_o;&c_k;)</klr></trd>
  <trd lng="it">giarrettiera</trd>
  <trd lng="uk">&c_p;&c_ib;&c_d;&c_v;&#x2019;&c_ja;&c_z;&c_k;&c_a; (&c_d;&c_l;&c_ja; &c_p;&c_a;&c_n;&c_ch;&c_ib;&c_h;)</trd>
  <trd lng="zh">&#x889C;&#x5E26; <pr>w&#xE0;d&#xE0;i</pr></trd>
</drv>
</art>

