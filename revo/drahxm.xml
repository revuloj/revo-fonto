<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>dra&hcirc;m</rad>/o
</kap>
<drv mrk="drahxm.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Antikva greka <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de
      maso kaj <ref tip="super" cel="unu.mon0o">monunuo</ref>;
      monunuo de Grekujo:
      <ekz>
        &gcirc;oju kun mi, &ccirc;ar mi trovis la <tld/>on, kiun mi perdis
        <fnt><bib>X</bib>, <lok>&Luk; 15:9</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="lept.0o">lepto.</ref>
  </snc>
  <trd lng="de">Drachme</trd>
  <trd lng="fr">drachme</trd>
  <trd lng="ru">&c_d;&c_r;&c_a;&c_h;&c_m;&c_a;</trd>
</drv>
</art>

