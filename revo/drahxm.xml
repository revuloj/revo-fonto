<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>dra&hcirc;m</rad>/o
  <fnt>Z</fnt>
  <fnt><bib>ZR</bib></fnt>
</kap>
<drv mrk="drahxm.0o">
  <kap><tld/>o</kap>
  <snc mrk="drahxm.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Antikva greka <ref tip="lst" cel="unu.mezur0o"
      lst="voko:kvantoj">mezurunuo</ref>
      de
      maso kaj iama <ref tip="lst" cel="unu.mon0o"
      lst="voko:monunuoj">monunuo</ref>
      de Grekujo:
      <ekz>
        &gcirc;oju kun mi, &ccirc;ar mi trovis la <tld/>on, kiun mi perdis
        <fnt><bib>NT</bib>, <lok>&Luk; 15:9</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_a;&c_h;&c_m;&c_a;</trd>
  <trd lng="bg">&c_d;&c_r;&c_a;&c_h;&c_m;&c_a;</trd>
  <trd lng="ca">dracma</trd>
  <trd lng="de">Drachme</trd>
  <trd lng="en">drachma</trd>
  <trd lng="fr">drachme</trd>
  <trd lng="nl">drachme</trd>
  <trd lng="pl">drachma</trd>
  <trd lng="pt">dracma</trd>
  <trd lng="ru">&c_d;&c_r;&c_a;&c_h;&c_m;&c_a;</trd>
</drv>
</art>
