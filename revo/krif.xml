<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: krif.xml,v 1.14 2023/12/04 19:21:04 revo Exp $">
<kap>
  <rad>krif</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="krif.0o">
  <kap><tld/>o</kap>
  <snc mrk="krif.0o.POE">
    <uzo tip="fak">POE</uzo>
    <dif>
      <ref tip="dif" cel="ung.0ego">Ungego</ref> de sova&gcirc;aj bestoj kaj rabobirdoj:
      <ekz>
        la kato saltis <klr>[&#8230;]</klr>, kaptis per la <tld/>oj ka&ubreve;&ccirc;ukan tubon fiksitan &ccirc;e
        la malanta&ubreve;a vando kaj forveturis
        <fnt>
          <bib>MkM</bib>
          <lok>4</lok>
        </fnt>.
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        falinte en iliajn <tld/>ojn <klr>(sub tirenecon)</klr>
        mi unue perdis liberon kaj propran volon
        <fnt><aut>Horst Vogt</aut><vrk>Paperfolioj</vrk><lok>La Gazeto,
           jaro 2005a, numero 117, p. 27a</lok>
        </fnt>.
        <trd lng="pl">szpony</trd>
      </ekz>
    </dif>
    <trdgrp lng="pl">
    <trd>szpon</trd>,
    <trd>pazur</trd>
    </trdgrp>
    <trd lng="pt">garra</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_p;&c_c;&c_ju;&c_r;</trd>,
    <trd>&c_k;&c_ib;&c_p;&c_c;&c_ju;&c_r;</trd>,
    <trd>&c_k;&c_ib;&c_p;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_n;&c_o;&c_k;&c_malmol;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>pa&rcaron;&aacute;t</trd>,
    <trd>sp&aacute;r</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kralle</trd>,
    <trd>Klaue</trd>
  </trdgrp>
  <trd lng="fr">griffe</trd>
  <trd lng="hu">karom</trd>
  <trd lng="nl">klauw</trd>
  <trd lng="ru">&c_k;&c_o;&c_g;&c_o;&c_t;&c_mol;</trd>
  <trd lng="sk">drap&aacute;k</trd>
  <trd lng="uk">&c_p;&c_a;&c_z;&c_u;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#29226; <pr>zhu&#462;</pr></trd>,
    <trd>&#29226;&#23376; <pr>zhu&#462;zi</pr></trd>
  </trdgrp>
</drv>
</art>

