<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>karavan</rad>/o
</kap>
<drv mrk="karava.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      En Oriento a&ubreve; Afriko, aro da pilgrimantoj a&ubreve; komercistoj, kun iliaj
      &scirc;ar&gcirc;bestoj, voja&gcirc;antaj kune pro sendan&gcirc;ereco a&ubreve; oportuneco.
    </dif>
    <trd lng="de">Karawane</trd>
    <trd lng="en">caravan</trd>
    <trd lng="fr">caravane<klr tip="ind"> (en Orient, en Afrique)</klr></trd>
    <trd lng="nl">karavaan</trd>
    <trd lng="ru">&c_k;&c_a;&c_r;&c_a;&c_v;&c_a;&c_n;</trd>
    <trd lng="sv">karavan</trd>
  </snc>
  <snc>
    <dif>
      Grupo da kunvoja&gcirc;antoj.
    </dif>
    <trd lng="de">Reisegruppe</trd>
    <trd lng="en">caravan</trd>
    <trdgrp lng="fr">
      <trd>caravane<klr tip="ind"> (voyage organis&eacute;)</klr></trd>,
      <trd>voyage organis&eacute;</trd>
    </trdgrp>
    <trd lng="nl">reisgezelschap</trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_a;&c_v;&c_a;&c_n;</trd>
  <trd lng="hu">karav&aacute;n</trd>
</drv>

<drv mrk="karava.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Haltejo por <tld/>oj.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_a;&c_v;&c_a;&c_n;-&c_s;&c_a;&c_r;&c_a;&c_j;</trd>
  <trd lng="de">Karawanserei</trd>
  <trd lng="en">caravanasary</trd>
  <trd lng="fr">caravans&eacute;rail</trd>
  <trd lng="hu">karav&aacute;nszer&aacute;j</trd>
  <trd lng="nl">karavaanserai</trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_a;&c_v;&c_a;&c_n;-&c_s;&c_a;&c_r;&c_a;&c_j;</trd>
</drv>
</art>
