<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>ret</rad>/o
    </kap>
    <drv mrk="ret.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc num="1">
        <dif>
          Plekta&jcirc;o kun grandaj interspacoj  inter la fadenoj a&ubreve; &scirc;nuroj. 
        </dif>
        <refgrp tip="sin">
          <ref cel="teks.0ajxo">teksa&jcirc;o</ref>,
          <ref cel="masx.0aro">ma&scirc;aro</ref>,
          <ref cel="arangx">aranea&jcirc;o</ref>,
          <ref cel="filigr.0o">filigrano</ref>.
        </refgrp>
      </snc>
      <snc num="2">
        <dif>
          Tiuspeca plekta&jcirc;o uzata por kapti fi&scirc;ojn. 
        </dif>
      </snc>
      <snc num="3">
        <uzo tip="klr">(f)</uzo><dif>
          Io similanta tian plekta&jcirc;on:
          <ekz>la <tld/>o de la vejnoj;</ekz>
          <ekz>la tuta lando estas kovrita per <tld/>o da fervojoj, da filioj;</ekz>
          <ekz>&scirc;tala <tld/>o da pafa&jcirc;oj;</ekz>
          <ekz><uzo tip="fak">KOMP</uzo> komputila <tld/>o.</ekz>
        </dif>
      </snc>
      <snc num="4">
        <uzo tip="klr">(f)</uzo><dif>
          Kaptilo: <ekz>lasi sin entiri en <tld/>on<fnt>Z</fnt>;
          </ekz> <ekz>enfali en <tld/>on.
          </ekz>
        </dif>
      </snc>
      <snc num="5">
        <uzo tip="klr">(f)</uzo><dif>
          Komplika&jcirc;o, malsimpla a&ubreve; konfuza implika&jcirc;o:
          <ekz><tld/>o da intrigoj<fnt>B</fnt>;
          </ekz> <ekz>la nacioj estas preme interligataj per la
          <tld/>o de la interplektitaj interesoj; </ekz> <ekz>tiaj
          maljunuloj tute konfuzi&gcirc;as en la malsimpla <tld/>aro de niaj
          modernaj vivkondi&ccirc;oj; </ekz> <ekz><tld/>o da supersti&ccirc;oj k
          rutinoj.
          </ekz>
        </dif>
      </snc>
      <trd lng="en">net</trd>
      <trd lng="ru">&#x441;&#x435;&#x442;&#x44c;</trd>
    </drv>
    <drv mrk="ret.0ano">
      <kap><tld/>ano</kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Uzanto de la Inter<tld/>o, <ctl><tld/>landano</ctl>.
        </dif>
        <trdgrp lng="en"><trd>netter</trd>, <trd>netizen</trd></trdgrp>
        <trd lng="fr">internaute</trd>
      </snc>
    </drv>
    <drv mrk="ret.0umi">
      <kap><tld/>umi</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Iel ajn pasigi tempon en la Inter<tld/>o: por laboro a&ubreve;
          plezuro; <tld/>po&scirc;te, navigante en TTT ktp.          
          <ekz>hiera&ubreve; li <tld/>umis 3 horojn.</ekz>
        </dif>
      </snc>
    </drv>
    <drv mrk="ret.inter0o">
      <kap>inter<tld/>o</kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Aro da interkonektitaj komputilaj <tld/>oj.
        </dif>
        <trd lng="en">internet</trd>
        <trd lng="ru">&#x43c;&#x435;&#x436;&#x441;&#x435;&#x442;&#x435;&#x432;&#x43e;&#x435; &#x432;&#x437;&#x430;&#x438;&#x43c;&#x43e;&#x434;&#x435;&#x439;&#x441;&#x442;&#x432;&#x438;&#x435;</trd>
      </snc>
    </drv>
    <drv mrk="ret.inter0o.la">
      <kap>Inter<tld/>o</kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <uzo tip="fak">KOMUNE</uzo>
        <dif>
          Esperantigita nomo de <ctl>Internet</ctl>, la plej granda el
          la mallokaj <tld/>oj.
        </dif>
        <rim num="1">
          Por la fakuloj <ctl>la Interreto</ctl> estas unu speciala
          el multaj <ref tip="super"
          cel="ret.inter0o">interretoj</ref>, tial oni uzas la
          artikolon (samkiel, cetere, en la angla).  Tamen la publiko
          plej ofte tion ne konscias kaj uzas la vorton senartikole.
        </rim>
        <rim num="2">
          Oni ofte mallongigas tiun nomon &gcirc;is <ctl>la Reto</ctl>
          (kaj en tiu formo la artikolo ne estas forigata).
        </rim>
        <trd lng="en">the <ind>Internet</ind></trd>
        <trd lng="ru">&#x418;&#x43d;&#x442;&#x435;&#x440;&#x43d;&#x435;&#x442;</trd>
      </snc>
    </drv>
    <drv mrk="ret.0umilo">
      <kap><tld/>umilo</kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Programa rimedo por <tld/>umi; TTT-legilo, po&scirc;tilo ktp.
        </dif>
      </snc>
    </drv>
    <drv mrk="ret.0komerco">
      <kap><tld/>komerco</kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <uzo tip="fak">KOMUNE</uzo>
        <dif>
          Komercado per la Inter<tld/>o.
        </dif>
      </snc>
      <trd lng="en">e-commerce</trd>
      <trd lng="ru">&#x44d;&#x43b;&#x435;&#x43a;&#x442;&#x440;&#x43e;&#x43d;&#x43d;&#x430;&#x44f; &#x442;&#x43e;&#x440;&#x433;&#x43e;&#x432;&#x43b;&#x44f;</trd>
    </drv>
    <drv mrk="ret.0letero">
      <kap><tld/>letero</kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Persone adresita mesa&gcirc;o sendita per komputila <tld/>o.
          Kp <ref cel="afisx.0o.komp">afi&scirc;o</ref>.
        </dif>
      </snc>
      <trd lng="en"><klr>(piece of)</klr> <ind>e-mail</ind></trd>
    </drv>
    <drv mrk="ret.0posxto">
      <kap><tld/>po&scirc;to</kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Servo ebliganta al la rajtigitaj uzantoj inter&scirc;an&gcirc;i
          mesa&gcirc;ojn per komputila <tld/>o.
        </dif>
      </snc>
      <trd lng="en">e-mail</trd>
      <trd lng="ru">&#x44d;&#x43b;&#x435;&#x43a;&#x442;&#x440;&#x43e;&#x43d;&#x43d;&#x430;&#x44f; &#x43f;&#x43e;&#x447;&#x442;&#x430;</trd>
    </drv>
  </art>

