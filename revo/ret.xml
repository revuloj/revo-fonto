    <trd lng="es">c&aacute;mara web</trd>
    <trd lng="fr">webcam</trd>
    <trd lng="nl">webcam</trd>
    <trd lng="ru">&c_v;&c_je;&c_b;-&c_k;&c_a;&c_m;&c_je;&c_r;&c_a;</trd>
  </snc>
</drv>

