    <drv mrk="ret.0ejo">
      <kap><tld/>ejo</kap>
      <snc mrk="ret.0ejo.KOMP">
        <uzo tip="fak">KOMP</uzo>
        <ref tip="dif" cel="ttt.0ejo">TTT-ejo</ref>
        <ekz>
          <tld/>ejo <klr>...</klr> kun informo pri internaciaj voja&gcirc;oj
          <fnt>
            <aut>G. van Wilgen</aut>,
            <vrk>Usona programara entrepreno...</vrk>,
            <lok><url
 ref="http://groups.google.be/group/soc.culture.esperanto?hl=eo">soc.culture.esperanto</url>,
              1997-10-09</lok>
          </fnt>.
        </ekz>
      </snc>
      <trdgrp lng="be">
        <trd>&c_s;&c_a;&c_j;&c_t;</trd>,
        <trd>&c_v;&c_e;&c_b;-&c_s;&c_a;&c_j;&c_t;</trd>
      </trdgrp>
      <trd lng="de">Internetseite</trd>
      <trd lng="en">web site</trd>
      <trd lng="es">sitio web</trd>
      <trd lng="fr">site internet</trd>
      <trdgrp lng="hu">
        <trd>h&aacute;l&oacute;hely</trd>,
        <trd>web-hely</trd>,
        <trd>web-site</trd>
      </trdgrp>
      <trd lng="id">laman</trd>
      <trdgrp lng="nl">
        <trd>webstek</trd>,
        <trd>web site</trd>
      </trdgrp>
      <trd lng="pl">strona <ind>internetowa</ind></trd>
      <trdgrp lng="ru">
        <trd>&c_s;&c_a;&c_j;&c_t;</trd>,
        <trd>&c_v;&c_je;&c_b;-&c_s;&c_a;&c_j;&c_t;</trd>
      </trdgrp>
    </drv>

    <drv mrk="ret.0umi">
      <kap><tld/>umi</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc mrk="ret.0umi.KOMP">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Iel ajn pasigi tempon en la Inter<tld/>o: por laboro a&ubreve;
          plezuro; <tld/>po&scirc;te, navigante en TTT ktp.
          <ekz>
            hiera&ubreve; li <tld/>umis 3 horojn.
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_y;&c_s;&c_t;&c_a;&c_c;&c_c;&c_a; &c_Ib;&c_n;&c_t;&c_e;&c_r;&c_n;&c_e;&c_t;&c_a;&c_m;</trd>
  <trd lng="de">surfen (im Internet)</trd>
  <trd lng="es">navegar<klr> (por internet)</klr></trd>
  <trd lng="fr">surfer<klr tip="amb"> (sur internet)</klr></trd>
  <trdgrp lng="hu">
        <trd>internetezik</trd>,
        <trd>b&ouml;ng&eacute;szik<klr tip="amb"> (a vil&aacute;gh&aacute;l&oacute;n)</klr></trd>,
        <trd>sz&ouml;rf&ouml;z<klr tip="amb"> (a vil&aacute;gh&aacute;l&oacute;n)</klr></trd>
      </trdgrp>
  <trdgrp lng="id">
        <trd>[<ind>jelajah</ind>] menjelajah</trd>,
        <trd>[<ind>ramban</ind>] meramban</trd>        
      </trdgrp>
  <trdgrp lng="nl">
        <trd>surfen<klr tip="ind"> (Internet)</klr></trd>,
        <trd>internetten</trd>
      </trdgrp>
  <trd lng="pl">u&zdot;ywa&cacute; sieci <klr>(komputerowej)</klr></trd>
  <trd lng="ru">&c_b;&c_r;&c_o;&c_d;&c_i;&c_t;&c_mol; &c_p;&c_o; &c_I;&c_n;&c_t;&c_je;&c_r;&c_n;&c_je;&c_t;&c_u;</trd>
  <trd lng="sk">surfovať po internete</trd>
</drv>

    <drv mrk="ret.0umilo">
      <kap><tld/>umilo</kap>
      <snc mrk="ret.0umilo.KOMP">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Programa rimedo por <tld/>umi; TTT-legilo, po&scirc;tilo ktp.
        </dif>
      </snc>
      <trd lng="be">&c_b;&c_r;&c_a;&c_w;&c_z;&c_e;&c_r;</trd>
      <trd lng="de">Browser</trd>
      <trd lng="es">navegador web</trd>
      <trdgrp lng="fr">
        <trd>navigateur web</trd>,
        <trd>fureteur</trd>
      </trdgrp>
      <trd lng="hu">b&ouml;ng&eacute;sz&odblac;</trd>
      <trdgrp lng="id">
        <trd>[<ind>jelajah</ind>] penjelajah</trd>,
        <trd>[<ind>ramban</ind>] peramban</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>bladeraar</trd>,
        <trd>browser</trd>
      </trdgrp>
      <trd lng="pl">przegl&aogonek;darka</trd>
      <trd lng="ru">&c_b;&c_r;&c_a;&c_u;&c_z;&c_je;&c_r;</trd>
    </drv>

    <drv mrk="ret.en0igi">
      <kap>en<tld/>igi</kap>
      <snc mrk="ret.en0igi.KOMP">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Enmeti en atingeblan lokon de komputila <tld/>o,
          <ref tip="vid" cel="sxut.al0i.KOMP">al&scirc;uti</ref>:
          <ekz>
            mi registras la muzika&jcirc;on komputere kaj poste en<tld/>igas
            &gcirc;in
            <fnt>
              <aut>&Ccirc;ina Radio Internacia</aut>,
              <vrk><url
                  ref="http://esperanto.cri.cn/1/2006/01/06/1@33101.htm"
                  >Retaj amaskomunikiloj...</url></vrk>,
              <lok>2006-01-06</lok>
            </fnt>.
          </ekz>
        </dif>
        <trd lng="es">entregar<klr> (por una red
        inform&aacute;tica)</klr></trd>
        <trd lng="fr">livrer<klr> (sur un r&eacute;seau
        informatique)</klr></trd>
        <trd lng="id">[<ind>unggah</ind>] mengunggah</trd>
      </snc>
      <trd lng="sk">chytiť do siete</trd>
</drv>

    <drv mrk="ret.fisx0o">
      <kap>fi&scirc;<tld/>o</kap>
      <snc mrk="ret.fisx0o.CXAS">
        <dif>
          <tld lit="R"/>o uzata por kapti fi&scirc;ojn:
          <ekz>
            ili tiras per fi&scirc;hoko, kaptas per sia <tld/>o, amasigas per
            sia
            fi&scirc;<tld/>o
            <fnt><bib>MT</bib>, <lok>&Hab; 1:15</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_m;&c_ja;&c_r;&c_e;&c_zh;&c_a;</trd>
      <trd lng="de">Fischernetz</trd>
      <trdgrp lng="es">
        <trd>red de pesca</trd>,
        <trd>esparavel</trd>,
        <trd>buitr&oacute;n</trd>
      </trdgrp>
      <trd lng="fr">filet &agrave; poissons</trd>
      <trdgrp lng="id">
        <trd>jala</trd>,
        <trd>jaring</trd>
      </trdgrp>
      <trd lng="nl">visnet</trd>
      <trd lng="pl">sie&cacute; rybacka</trd>
      <trd lng="ru">&c_r;&c_y;&c_b;&c_o;&c_l;&c_o;&c_v;&c_n;&c_a;&c_ja; &c_s;&c_je;&c_t;&c_mol;</trd>
    </drv>

    <drv mrk="ret.inter0o">
      <kap>inter<tld/>o</kap>
      <snc mrk="ret.inter0o.KOMP">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Aro da interkonektitaj komputilaj <tld/>oj.
        </dif>
        </snc>
  <trd lng="de">Internet</trd>
  <trd lng="en">internet</trd>
  <trd lng="es">internet</trd>
  <trd lng="fr">internet</trd>
  <trdgrp lng="hu">
          <trd>vil&aacute;gh&aacute;l&oacute;</trd>,
          <trd>internet</trd>
        </trdgrp>
  <trd lng="id">internet</trd>
  <trd lng="nl">internet</trd>
  <trd lng="pl">sie&cacute; po&lstroke;&aogonek;czonych komputer&oacute;w</trd>
  <trd lng="ru">&c_m;&c_je;&c_zh;&c_s;&c_je;&c_t;&c_je;&c_v;&c_o;&c_je; &c_v;&c_z;&c_a;&c_i;&c_m;&c_o;&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  <trd lng="sk">internet</trd>
</drv>

    <drv mrk="ret.inter0o.la">
      <kap>Inter<tld/>o</kap>
      <snc mrk="ret.inter0o.la.KOMP">
        <uzo tip="fak">KOMP</uzo>
        <uzo tip="stl">KOMUNE</uzo>
        <dif>
          Esperantigita nomo de <ctl>Internet</ctl>, la plej granda el
          la mallokaj <tld/>oj.
        </dif>
        <rim>
          Por la fakuloj <ctl>la Interreto</ctl> estas unu speciala
          el multaj <ref tip="super"
            cel="ret.inter0o">interretoj</ref>, tial oni uzas la
          artikolon (samkiel, cetere, en la angla). Tamen la publiko
          plej ofte tion ne konscias kaj uzas la vorton senartikole.
        </rim>
        <rim>
          Oni ofte mallongigas tiun nomon &gcirc;is <ctl>la Reto</ctl>
          (kaj en tiu formo la artikolo ne estas forigata).
        </rim>
        <trd lng="be">&c_Ib;&c_n;&c_t;&c_e;&c_r;&c_n;&c_e;&c_t;</trd>
        <trd lng="de">Internet</trd>
        <trd lng="en">the <ind>Internet</ind></trd>
        <trd lng="es">Internet</trd>
        <trd lng="fr">Internet</trd>
        <trd lng="hu">Internet</trd>
        <trd lng="id">Internet</trd>
        <trd lng="nl">Internet</trd>
        <trd lng="pl">Internet</trd>
        <trd lng="ru">&c_I;&c_n;&c_t;&c_je;&c_r;&c_n;&c_je;&c_t;</trd>
        <trd lng="sv">internet</trd>
      </snc>
      <trd lng="sk">internet</trd>
</drv>

    <drv mrk="ret.voj0o">
      <kap>voj<tld/>o</kap>
      <snc mrk="ret.voj0o.TRA">
        <uzo tip="fak">TRA</uzo>
        <ref tip="dif" cel="voj.0aro.TRA">vojaro</ref>
        <ekz>
          longdistancaj biciklaj voj<tld/>oj
          <fnt>
            <aut>Zhong Q.</aut>,
            <vrk>A&ubreve;tofrakasistoj</vrk>,
            <lok>soc.culture.esperanto, 2000-06-21</lok></fnt>.
        </ekz>
      </snc>
      <trd lng="be">&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_a;&c_r;&c_t;&c_n;&c_a;&c_ja; &c_s;&c_je;&c_t;&c_k;&c_a;</trd>
      <trd lng="de">Wegenetz</trd>
      <trd lng="es">red de carreteras</trd>
      <trdgrp lng="fr">
        <trd>r&eacute;seau routier</trd>
      </trdgrp>
      <trd lng="hu">&uacute;th&aacute;l&oacute;zat</trd>
      <trd lng="nl">wegennet</trd>
      <trd lng="pl">sie&cacute; dr&oacute;g</trd>
      <trd lng="ru">&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_o;&c_r;&c_t;&c_n;&c_a;&c_ja; &c_s;&c_je;&c_t;&c_mol;</trd>
    </drv>

    <drv mrk="ret.komputila0o">
      <kap>komputila <tld/>o</kap>
      <snc mrk="ret.komputila0o.KOMP">
        <tezrad fak="KOMP"/>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Aran&gcirc;o de la komunika kaj komuta aparataro, protokoloj kaj
          programaro, kiuj arigas plurajn (a&ubreve; e&ccirc; multajn) komputilojn
          en tutecan komputan sistemon.
        </dif>
        <refgrp tip="sub">
          <ref cel="ret.0o.loka.KOMP">loka reto</ref>,
          <ref cel="ret.0o.malloka.KOMP">malloka reto</ref>,
          <ref cel="transi.0a.reto">transita reto</ref>
        </refgrp>
      </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_p;&c_u;&c_t;&c_a;&c_r;&c_n;&c_a;&c_ja; &c_s;&c_je;&c_t;&c_k;&c_a;</trd>
  <trd lng="de">Computernetzwerk</trd>
  <trd lng="en">computer network</trd>
  <trd lng="es">red de ordenadores</trd>
  <trd lng="fr">r&eacute;seau informatique</trd>
  <trd lng="hu">sz&aacute;m&iacute;t&oacute;g&eacute;p-h&aacute;l&oacute;zat</trd>
  <trd lng="id"><ind>jaring</ind>an komputer</trd>
  <trd lng="nl">computernetwerk</trd>
  <trd lng="pl">sie&cacute; komputerowa</trd>
  <trdgrp lng="ru">
        <trd>&c_s;&c_je;&c_t;&c_mol; &c_E;&c_V;&c_M;</trd>,
        <trd>&c_v;&c_y;&c_ch;&c_i;&c_s;&c_l;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_s;&c_je;&c_t;&c_mol;</trd>,
        <trd>&c_k;&c_o;&c_m;&c_p;&c_mol;&c_ju;&c_t;&c_je;&c_r;&c_n;&c_a;&c_ja; &c_s;&c_je;&c_t;&c_mol;</trd>
      </trdgrp>
  <trd lng="sk">počítačová sieť</trd>
  <trd lng="sv">datorn&auml;tverk</trd>
</drv>

    <drv mrk="ret.0o.loka">
      <kap>loka <tld/>o</kap>
      <snc mrk="ret.0o.loka.KOMP">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Komputila <tld/>o tute situanta en unu &ccirc;ambro a&ubreve; en unu
          konstrua&jcirc;o. Tia
          reto povas arigi &gcirc;is proksimume 500 dispona&jcirc;ojn.
        </dif>
      </snc>
      <trd lng="be">&c_l;&c_ja;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_k;&c_a;&c_m;&c_p;&c_u;&c_t;&c_a;&c_r;&c_n;&c_a;&c_ja;
        &c_s;&c_je;&c_t;&c_k;&c_a;</trd>
      <trd lng="de">lokales Computernetzwerk</trd>
      <trdgrp lng="en">
        <trd>local area computer network</trd>,
        <trd>LAN</trd>
