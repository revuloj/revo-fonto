<drv mrk="ret.ekster0e">
  <kap>ekster<tld/>e</kap>
  <snc mrk="ret.ekster0e.KOMP">
    <uzo tip="fak">RET</uzo>
    <dif>
      Maniero labori kun esence <tld/>aj dokumentoj
      <ref tip="super" cel="konekt.sen0e.KOMP">senkonekte</ref>
      (<ref cel="foli.0umi.KOMP">foliumi<sncref/></ref> lokan
      kopion de TTT-pa&gcirc;aro, prepari <tld/>po&scirc;tajn
      mesa&gcirc;ojn sendotajn &ccirc;e onta <tld/>konekto ktp):
      <ekz>
        eldono de DVD kun ekster<tld/>a versio de la tuta
        Vikipedio en Esperanto
        <fnt>
          <aut>Peter Bal&aacute;&zcaron;</aut>:
          <vrk>Jaro 2012: Niaj atingoj kaj malsukcesoj</vrk>.
          <lok><url ref="http://esperanto.china.org.cn/2013-02/01/content_27862683.htm">La
          &Ccirc;inia Radio Internacia</url></lok>.
        </fnt>;
      </ekz>
      <ekz>
        la mapo &scirc;ar&gcirc;os ekster<tld/>e kaj facile trovi&gcirc;as via loko
        <fnt>
          <vrk><url ref="https://gobackpackgo.com/eo/offline-maps-traveling/">Senkonektaj
          mapoj por voja&gcirc;ado - Iru tornistron GO</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la 28&#x202F;000 studentoj kaj 5&#x202F;500 dungitoj de la <nom>Justus
        Liebig</nom>-Universitato estis
        eksterrete dum pli ol unu semajno
        <fnt>
          <vrk><url ref="https://steemit.com/esperanto/@johano/no-internet-universitato-sen-interreto">No
          Internet -Universitato sen Interreto &mdash; Steemit</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">offline</trd>
    <trd lng="en">offline<klr> (browsing etc)</klr></trd>
    <trdgrp lng="es">
      <trd>sin conexi&oacute;n<klr> (sin internet)</klr></trd>,
      <trd>sin l&iacute;nea<klr> (sin internet)</klr></trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd><ind>luar</ind> jaringan</trd>,
      <trd>luring</trd>
    </trdgrp>
  </snc>
</drv>

