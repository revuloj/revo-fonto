<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>ret</rad>/o <fnt><bib>PV</bib></fnt>
    </kap>

    <drv mrk="ret.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="ret.0o.TEKS">
        <dif>
          Plekta&jcirc;o kun grandaj interspacoj inter la fadenoj a&ubreve;
          &scirc;nuroj:
          <ekz>
            nova aparato, kiu <klr>...</klr> konsistas el <tld/>o,
            plektita
            el
            facila sed fortika materialo
            <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj,
                bagateloj</lok></fnt>.
          </ekz>
        </dif>
        <subsnc mrk="ret.0o.kaptilo">
          <dif>
            Uzata kiel kaptilo, ekzemple de sova&gcirc;aj bestoj:
            <ekz>
              <uzo tip="stl">FIG</uzo>
              lasi sin entiri en <tld/>on<fnt>Z</fnt>.
            </ekz>
            <trd lng="es">trampa con red</trd>
          </dif>
        </subsnc>
        <subsnc mrk="ret.0o.fisxreto">
          <ref tip="dif" cel="ret.fisx0o.CXAS">fi&scirc;<tld/>o</ref>
          <ekz>
            Petro iris sur la &scirc;ipon, kaj tiris la <tld/>on al la tero,
            plenan
            de grandaj fi&scirc;oj
            <fnt><bib>NT</bib>, <lok>&Joh; 21:11</lok></fnt>.
          </ekz>
          <trd lng="be">&c_m;&c_ja;&c_r;&c_e;&c_zh;&c_a;</trd>
          <trd lng="de">Fischernetz</trd>
          <trdgrp lng="es">
            <trd>red de pesca</trd>,
            <trd>esparavel</trd>,
            <trd>buitr&oacute;n</trd>
          </trdgrp>
        </subsnc>
        <refgrp tip="vid">
          <ref cel="arane.0ajxo">aranea&jcirc;o</ref>,
          <ref cel="filigr.0o">filigrano</ref>,
          <ref cel="masx.0aro">ma&scirc;aro</ref>,
          <ref cel="teks.0ajxo.TEKS">teksa&jcirc;o <sncref/></ref>
        </refgrp>
      </snc>
      <snc mrk="ret.0o.interligajxo">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Io similanta tian plekta&jcirc;on:
          <ekz>
            la <tld/>o de la vejnoj;
          </ekz><ekz>
            la tuta lando estas kovrita per <tld/>o da fervojoj, da
            filioj;
          </ekz><ekz>
            &scirc;tala <tld/>o da pafa&jcirc;oj;
          </ekz><ekz>
            <uzo tip="fak">KOMP</uzo> komputila <tld/>o.
          </ekz>
          <trd lng="es">red<klr> (de comunicaciones)</klr></trd>
        </dif>
      </snc>
      <snc mrk="ret.0o.impliko">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Komplika&jcirc;o, malsimpla a&ubreve; konfuza implika&jcirc;o:
          <ekz>
            vi estas nesperta, &ccirc;irka&ubreve;ita per <tld/>o de intrigoj
            <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 5a</lok></fnt>;
          </ekz><ekz>
            la nacioj estas preme interligataj per la
            <tld/>o de la interplektitaj interesoj;
          </ekz><ekz>
            tiaj maljunuloj tute konfuzi&gcirc;as en la malsimpla <tld/>aro de
            niaj modernaj vivkondi&ccirc;oj;
          </ekz><ekz>
            <tld/>o da supersti&ccirc;oj kaj rutinoj.
          </ekz>
          <trd lng="es">red<klr> (de relaciones)</klr></trd>
        </dif>
      </snc>
  <trd lng="be">&c_s;&c_je;&c_t;&c_k;&c_a;</trd>
  <trd lng="br">rouedad</trd>
  <trd lng="de">Netz</trd>
  <trd lng="en">net</trd>
  <trd lng="es">red</trd>
  <trdgrp lng="fr">
        <trd>filet</trd>,
        <trd>r&eacute;seau</trd>
      </trdgrp>
  <trdgrp lng="hu">
        <trd>h&aacute;l&oacute;</trd>,
        <trd>h&aacute;l&oacute;zat</trd>
      </trdgrp>
  <trdgrp lng="id">
        <trd>jaring</trd>,
        <trd><ind>jaring</ind>an</trd>
      </trdgrp>
  <trdgrp lng="nl">
        <trd>net</trd>,
        <trd>netwerk</trd>
      </trdgrp>
  <trd lng="pl">sie&cacute;</trd>
  <trd lng="pt">rede</trd>
  <trd lng="ru">&c_s;&c_je;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>rozvrhnutie</trd>,
    <trd>sieť</trd>,
    <trd>sieťka</trd>
  </trdgrp>
  <trd lng="sv">n&auml;t</trd>
</drv>

    <drv mrk="ret.0ano">
      <kap><tld/>ano</kap>
      <snc mrk="ret.0ano.KOMP">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Uzanto de la Inter<tld/>o, <ctl><tld/>landano</ctl>.
        </dif>
        <trd lng="be">&c_k;&c_a;&c_r;&c_y;&c_s;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k; &c_Ib;&c_n;&c_t;&c_e;&c_r;&c_n;&c_e;&c_t;&c_a;</trd>
        <trd lng="de">Internetnutzer</trd>
        <trdgrp lng="en">
          <trd>netter</trd>, <trd>netizen</trd>
        </trdgrp>
        <trd lng="es">internauta</trd>
        <trd lng="fr">internaute</trd>
        <trdgrp lng="hu">
          <trd>internetez&odblac;</trd>,
          <trd>h&aacute;l&oacute;polg&aacute;r</trd>
        </trdgrp>
        <trd lng="nl">Internetgebruiker</trd>
        <trd lng="pl">u&zdot;ytkownik sieci <klr>(komputerowej)</klr></trd>
        <trdgrp lng="ru">
          <trd>&c_p;&c_o;&c_l;&c_mol;&c_z;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol; &c_I;&c_n;&c_t;&c_je;&c_r;&c_n;&c_je;&c_t;&c_a;</trd>,
          <trd>&c_s;&c_je;&c_t;&c_ja;&c_n;&c_i;&c_n;</trd>
