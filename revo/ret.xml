<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>ret</rad>/o</kap>
<drv mrk="ret.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Plekta&jcirc;o kun grandaj interspacoj inter la fadenoj a&ubreve;
      &scirc;nuroj.
    </dif>
    <refgrp tip="vid">
      <ref cel="teks.0ajxo">teksa&jcirc;o</ref>,
      <ref cel="masx.0aro">ma&scirc;aro</ref>,
      <ref cel="arangx">aranea&jcirc;o</ref>,
      <ref cel="filigr.0o">filigrano</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Tiuspeca plekta&jcirc;o uzata por kapti fi&scirc;ojn.
    </dif>
    <trd lng="be">&c_m;&c_ja;&c_r;&c_e;&c_zh;&c_a;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Io similanta tian plekta&jcirc;on:
      <ekz>
        la <tld/>o de la vejnoj;
      </ekz>
      <ekz>
        la tuta lando estas kovrita per <tld/>o da fervojoj, da filioj;
      </ekz>
      <ekz>
        &scirc;tala <tld/>o da pafa&jcirc;oj;
      </ekz>
      <ekz>
        <uzo tip="fak">KOMP</uzo> komputila <tld/>o.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kaptilo:
      <ekz>
        lasi sin entiri en <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        enfali en <tld/>on.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Komplika&jcirc;o, malsimpla a&ubreve; konfuza implika&jcirc;o:
      <ekz>
        <tld/>o da intrigoj<fnt>B</fnt>;
      </ekz>
      <ekz>
        la nacioj estas preme interligataj per la
        <tld/>o de la interplektitaj interesoj;
      </ekz>
      <ekz>
        tiaj maljunuloj tute konfuzi&gcirc;as en la malsimpla <tld/>aro de niaj
        modernaj vivkondi&ccirc;oj;
      </ekz>
      <ekz>
        <tld/>o da supersti&ccirc;oj kaj rutinoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_je;&c_t;&c_k;&c_a;</trd>
  <trd lng="de">Netz</trd>
  <trd lng="en">net</trd>
  <trdgrp lng="hu">
    <trd>h&aacute;l&oacute;</trd>,
    <trd>h&aacute;l&oacute;zat</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_je;&c_t;&c_mol;</trd>
  <trdgrp lng="nl">
    <trd>net</trd>,
    <trd>netwerk</trd>
  </trdgrp>
  <trd lng="sv">n&auml;t</trd>
</drv>

<drv mrk="ret.0ano">
  <kap><tld/>ano</kap>
  <snc>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Uzanto de la Inter<tld/>o, <ctl><tld/>landano</ctl>.
    </dif>
    <trd lng="be">&c_k;&c_a;&c_r;&c_y;&c_s;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k; &c_Ib;&c_n;&c_t;&c_e;&c_r;&c_n;&c_e;&c_t;&c_a;</trd>
    <trd lng="de">Internetnutzer</trd>
    <trdgrp lng="en">
      <trd>netter</trd>, <trd>netizen</trd>
    </trdgrp>
    <trd lng="fr">internaute</trd>
    <trdgrp lng="hu">
      <trd>internetez&odblac;</trd>,
      <trd>h&aacute;l&oacute;polg&aacute;r</trd>
    </trdgrp>
    <trd lng="nl">Internetgebruiker</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_l;&c_mol;&c_z;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol; &c_I;&c_n;&c_t;&c_je;&c_r;&c_n;&c_je;&c_t;&c_a;</trd>,
      <trd>&c_s;&c_je;&c_t;&c_ja;&c_n;&c_i;&c_n;</trd>
    </trdgrp>
    <trd lng="sv">internetanv&auml;ndare</trd>
  </snc>
</drv>

<drv mrk="ret.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Iel ajn pasigi tempon en la Inter<tld/>o: por laboro a&ubreve;
      plezuro; <tld/>po&scirc;te, navigante en TTT ktp.
      <ekz>
        hiera&ubreve; li <tld/>umis 3 horojn.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_y;&c_s;&c_t;&c_a;&c_c;&c_c;&c_a; &c_Ib;&c_n;&c_t;&c_e;&c_r;&c_n;&c_e;&c_t;&c_a;&c_m;</trd>
  <trd lng="de">surfen (im Internet)</trd>
  <trdgrp lng="hu">
    <trd>internetezik</trd>,
    <trd>b&ouml;ng&eacute;szik<klr tip="amb"> (a vil&aacute;gh&aacute;l&oacute;n)</klr></trd>,
    <trd>sz&ouml;rf&ouml;z<klr tip="amb"> (a vil&aacute;gh&aacute;l&oacute;n)</klr></trd>
  </trdgrp>
  <trd lng="nl">Internetten</trd>
  <trd lng="ru">&c_b;&c_r;&c_o;&c_d;&c_i;&c_t;&c_mol; &c_p;&c_o; &c_I;&c_n;&c_t;&c_je;&c_r;&c_n;&c_je;&c_t;&c_u;</trd>
</drv>

<drv mrk="ret.inter0o">
  <kap>inter<tld/>o</kap>
  <snc>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Aro da interkonektitaj komputilaj <tld/>oj.
    </dif>
    <trd lng="en">internet</trd>
    <trdgrp lng="hu">
      <trd>vil&aacute;gh&aacute;l&oacute;</trd>,
      <trd>internet</trd>
    </trdgrp>
    <trd lng="nl">internet</trd>
    <trd lng="ru">&c_m;&c_je;&c_zh;&c_s;&c_je;&c_t;&c_je;&c_v;&c_o;&c_je; &c_v;&c_z;&c_a;&c_i;&c_m;&c_o;&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  </snc>
</drv>

<drv mrk="ret.inter0o.la">
  <kap>Inter<tld/>o</kap>
  <snc>
    <uzo tip="fak">KOMP</uzo>
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Esperantigita nomo de <ctl>Internet</ctl>, la plej granda el
      la mallokaj <tld/>oj.
    </dif>
    <rim>
      Por la fakuloj <ctl>la Interreto</ctl> estas unu speciala
      el multaj <ref tip="super"
      cel="ret.inter0o">interretoj</ref>, tial oni uzas la
      artikolon (samkiel, cetere, en la angla). Tamen la publiko
      plej ofte tion ne konscias kaj uzas la vorton senartikole.
    </rim>
    <rim>
      Oni ofte mallongigas tiun nomon &gcirc;is <ctl>la Reto</ctl>
      (kaj en tiu formo la artikolo ne estas forigata).
    </rim>
    <trd lng="be">&c_Ib;&c_n;&c_t;&c_e;&c_r;&c_n;&c_e;&c_t;</trd>
    <trd lng="de">Internet</trd>
    <trd lng="en">the <ind>Internet</ind></trd>
    <trd lng="hu">Internet</trd>
    <trd lng="ru">&c_I;&c_n;&c_t;&c_je;&c_r;&c_n;&c_je;&c_t;</trd>
    <trd lng="nl">Internet</trd>
    <trd lng="sv">internet</trd>
  </snc>
</drv>

<drv mrk="ret.0umilo">
  <kap><tld/>umilo</kap>
  <snc>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Programa rimedo por <tld/>umi; TTT-legilo, po&scirc;tilo ktp.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_a;&c_w;&c_z;&c_e;&c_r;</trd>
  <trd lng="de">Browser</trd>
  <trd lng="hu">b&ouml;ng&eacute;sz&odblac;</trd>
  <trdgrp lng="nl">
    <trd>balderaar</trd>,
    <trd>browser</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_r;&c_a;&c_u;&c_z;&c_je;&c_r;</trd>
</drv>

<drv mrk="ret.0komerco">
  <kap><tld/>komerco</kap>
  <snc>
    <uzo tip="fak">KOMP</uzo>
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Komercado per la Inter<tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_n;&c_n;&c_y; &c_g;&c_a;&c_n;&c_d;&c_a;&c_l;&c_mol;</trd>
  <trd lng="da">Internethandel</trd>
  <trdgrp lng="nl">
    <trd>Internethandel</trd>,
    <trd>E-Commerce</trd>
  </trdgrp>
  <trd lng="en">e-commerce</trd>
  <trd lng="hu">e-kereskedelem</trd>
  <trd lng="nl">Internethandel</trd>
  <trd lng="ru">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_n;&c_n;&c_a;&c_ja; &c_t;&c_o;&c_r;&c_g;&c_o;&c_v;&c_l;&c_ja;</trd>
</drv>

<drv mrk="ret.0letero">
  <kap><tld/>letero</kap>
  <snc>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Persone adresita mesa&gcirc;o sendita per komputila <tld/>o.
      Kp <ref cel="afisx.0o.komp">afi&scirc;o</ref>.
    </dif>
    <ref tip="malprt" cel="ret.0posxto">retpo&scirc;to</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_v;&c_je;&c_d;&c_a;&c_m;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je; &c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_n;&c_n;&c_a;&c_j; &c_p;&c_o;&c_sh;&c_t;&c_y;</trd>
  <trd lng="de">E-Mail</trd>
  <trd lng="en"><klr>(piece of)</klr> <ind>e-mail</ind></trd>
  <trdgrp lng="hu">
    <trd>elektronikus lev&eacute;l</trd>,
    <trd>e-lev&eacute;l</trd>,
    <trd>dr&oacute;tposta</trd>,
    <trd>e-mail</trd>
  </trdgrp>
  <trd lng="nl">e-mail<klr tip="ind"> (bericht)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_n;&c_n;&c_o;&c_je; &c_p;&c_i;&c_s;&c_mol;&c_m;&c_o;</trd>,
    <trd>&c_s;&c_o;&c_o;&c_b;&c_shch;&c_je;&c_n;&c_i;&c_je; &c_p;&c_o; &c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_n;&c_n;&c_o;&c_j; &c_p;&c_o;&c_ch;&c_t;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>e-brev</trd>,
    <trd>e-mail</trd>
  </trdgrp>
</drv>

<drv mrk="ret.0posxto">
  <kap><tld/>po&scirc;to</kap>
  <snc>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Servo ebliganta al la rajtigitaj uzantoj inter&scirc;an&gcirc;i
      mesa&gcirc;ojn per
      <ref tip="malprt" cel="ret.komputila0o">komputila <tld/>o</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_n;&c_n;&c_a;&c_ja; &c_p;&c_o;&c_sh;&c_t;&c_a;</trd>
  <trd lng="de">E-Mail</trd>
  <trd lng="en">e-mail</trd>
  <trdgrp lng="hu">
    <trd>elektronikus <ind>posta</ind></trd>,
    <trd>dr&oacute;tposta</trd>
  </trdgrp>
  <trd lng="nl">e-mail<klr tip="ind"> (de dienst)</klr></trd>
  <trd lng="pl">poczta internetowa</trd>
  <trd lng="ru">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_n;&c_n;&c_a;&c_ja; &c_p;&c_o;&c_ch;&c_t;&c_a;</trd>
  <trdgrp lng="sv">
    <trd>e-post</trd>,
    <trd>e-mail</trd>
  </trdgrp>
</drv>

<drv mrk="ret.komputila0o">
  <kap>komputila <tld/>o</kap>
  <snc>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Aran&gcirc;o de la komunika kaj komuta aparataro, protokoloj kaj
      programaro, kiuj arigas plurajn (a&ubreve; e&ccirc; multajn) komputilojn
      en tutecan komputan sistemon.
    </dif>
    <refgrp tip="sub">
      <ref cel="ret.0o.loka">loka reto</ref>,
      <ref cel="ret.0o.malloka">malloka reto</ref>,
      <ref cel="transi.0a.reto">transita reto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_p;&c_u;&c_t;&c_a;&c_r;&c_n;&c_a;&c_ja; &c_s;&c_je;&c_t;&c_k;&c_a;</trd>
  <trd lng="de">Computernetzwerk</trd>
  <trd lng="en">computer network</trd>
  <trd lng="hu">sz&aacute;m&iacute;t&oacute;g&eacute;p-h&aacute;l&oacute;zat</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_t;&c_mol; &c_E;&c_V;&c_M;</trd>,
    <trd>&c_v;&c_y;&c_ch;&c_i;&c_s;&c_l;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_s;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_o;&c_m;&c_p;&c_mol;&c_ju;&c_t;&c_je;&c_r;&c_n;&c_a;&c_ja; &c_s;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="nl">computernetwerk</trd>
  <trd lng="sv">datorn&auml;tverk</trd>
</drv>

<drv mrk="ret.0o.loka">
  <kap>loka <tld/>o</kap>
  <snc>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Komputila reto tute situanta en unu &ccirc;ambro a&ubreve; en unu
      konstrua&jcirc;o. Tia
      reto povas arigi &gcirc;is proksimume 500 dispona&jcirc;ojn.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_k;&c_a;&c_m;&c_p;&c_u;&c_t;&c_a;&c_r;&c_n;&c_a;&c_ja; &c_s;&c_je;&c_t;&c_k;&c_a;</trd>
  <trd lng="de">lokales Computernetzwerk</trd>
  <trdgrp lng="en">
    <trd>local area computer network</trd>,
    <trd>LAN</trd>
  </trdgrp>
  <trd lng="hu">helyi <ind>h&aacute;l&oacute;zat</ind></trd>
  <trdgrp lng="nl">
    <trd>lokaal <ind>netwerk</ind></trd>,
    <trd>LAN</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_o;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_v;&c_y;&c_ch;&c_i;&c_s;&c_l;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_s;&c_je;&c_t;&c_mol;</trd>
  <trd lng="sv">lokalt <ind>datorn&auml;tverk</ind></trd>
</drv>

<drv mrk="ret.0o.malloka">
  <kap>malloka <tld/>o</kap>
  <snc>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Komputila reto konektanta komputilojn geografie malproksimajn.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_l;&c_ja;&c_b;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_k;&c_a;&c_m;&c_p;&c_u;&c_t;&c_a;&c_r;&c_n;&c_a;&c_ja; &c_s;&c_je;&c_t;&c_k;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Fernverkehrsnetz</trd>, <trd>Weitverkehrsnetz</trd>,
    <trd>Fernnetz</trd>,
    <trd>Langstreckennetz</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>wide area computer network</trd>,
    <trd>WAN</trd>
  </trdgrp>
  <trd lng="hu">nagy kiterjed&eacute;s&udblac; <ind>h&aacute;l&oacute;zat</ind></trd>
  <trdgrp lng="nl">
    <trd>wijd <ind>netwerk</ind></trd>,
    <trd>WAN</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_l;&c_o;&c_b;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_v;&c_y;&c_ch;&c_i;&c_s;&c_l;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_s;&c_je;&c_t;&c_mol;</trd>
  <trd lng="sv">globalt <ind>datorn&auml;tverk</ind></trd>
</drv>
</art>
