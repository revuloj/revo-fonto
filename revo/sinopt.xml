<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>sinoptik</rad>/a
</kap>
<drv mrk="sinopt.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Ebliganta ampleksi per unusola rigardo la diversajn partojn de iu
      tuto:
      <ekz>
        <tld/>a tabelo;
      </ekz>
      <ekz>
        <tld/>aj evangelioj
        <klr>(de Mateo, Marko kaj Luko)</klr>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="skem.0a">skema</ref>.
    </refgrp>
  </snc>
  <trd lng="de">synoptisch</trd>
  <trdgrp lng="nl">
    <trd>synoptisch</trd>,
    <trd>beknopt</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_v;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_b;&c_z;&c_o;&c_r;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_r;&c_a;&c_v;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_i;&c_n;&c_o;&c_p;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; <klr tip="amb">(&c_o;&c_t;&c_n;&c_o;&c_s;&c_ja;&c_shch;&c_i;&c_j;&c_s;&c_ja; &c_k; &c_s;&c_i;&c_n;&c_o;&c_p;&c_s;&c_i;&c_s;&c_u;)</klr></trd>
  </trdgrp>
</drv>
</art>
