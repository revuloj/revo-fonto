<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sinopt.xml,v 1.23 2024/03/24 20:21:57 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>sinoptik</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sinopt.0o">
  <kap><tld/>o</kap>
  <snc mrk="sinopt.0o.KLA">
    <dif>
      Unurigarda prezento de plurparta a&ubreve; pluraspekta afero:
      <ekz>
        mi elpo&scirc;igis la filmo-<tld/>on kaj &gcirc;in dis&scirc;iris
        <fnt>
          <aut>Ba Jin, trad. Li Shijun</aut>,
          <vrk>A&ubreve;tuno en la Printempo</vrk>,
          <lok>1980</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o pri la UEA-afero de <klr>[&#x2026;]</klr> pasinta direktoro
        <fnt>
          <aut>C. C. Goldsmith</aut>
          <vrk><url ref="https://bulteno.esperanto-usa.org/a/1936/20/00-pdf/bulteno.pdf">Amerika Esperantisto</url></vrk>
          <lok>vol. 50, n-ro 1, Nov. 1936</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kompar.0o.ago">komparo</ref>,
      <ref cel="kompen.0o">kompendio</ref>,
      <ref cel="resum.0o">resumo</ref>,
      <ref cel="skript.0o">skripto</ref>,
      <ref cel="rigard.super0o.KOMUNE">superrigardo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_v;&c_o;&c_d;&c_k;&c_a; <klr tip="amb">(&c_z;&c_mol;&c_v;&c_je;&c_s;&c_t;&c_a;&c_k;)</klr></trd>,
    <trd>&c_a;&c_g;&c_l;&c_ja;&c_d;</trd>
  </trdgrp>
  <trd lng="de">Synopse</trd>
  <trd lng="en">synopsis</trd>
  <trd lng="cs">kniha uspo&rcaron;&aacute;dan&aacute; synopticky</trd>
  <trdgrp lng="de">
    <trd>Synopse</trd>,
    <trd>Gegen&uuml;berstellung</trd>,
    <trd>Gesamtschau</trd>
  </trdgrp>
  <trd lng="fr">synoptique<klr> (subst.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x8981;&#x89A7;  <pr>&#x3088;&#x3046;&#x3089;&#x3093;</pr></trd>,
    <trd>&#x6982;&#x8981;  <pr>&#x304C;&#x3044;&#x3088;&#x3046;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>synopsis</trd>,
    <trd>synopsa</trd>
  </trdgrp>
  <trd lng="sk">kniha usporiadan&aacute; synopticky</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&c_n;&c_o;&c_p;&c_s;&c_i;&c_s;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j; &c_o;&c_g;&c_l;&c_ja;&c_d;</trd>,
    <trd>&c_z;&c_v;&c_je;&c_d;&c_je;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6982;&#x8981; <pr>g&#xE0;iy&#xE0;o</pr></trd>,
    <trd>&#x5BF9;&#x7167; <pr>du&#xEC;zh&#xE0;o</pr></trd>,
    <trd>&#x5C0D;&#x7167; <pr>du&#xEC;zh&#xE0;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sinopt.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Ebliganta ampleksi per unusola rigardo la diversajn partojn de iu
      tuto:
      <ekz>
        <tld/>a tabelo;
      </ekz>
      <ekz>
        <ind><tld/>aj evangelioj</ind>
        <klr>(de Mateo, Marko kaj Luko)</klr>.
        <trd lng="de">synoptische Evangelien</trd>
        <trd lng="hu">szinoptikus evang&eacute;liumok</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flank.0al0e">flankalflanka</ref>,
      <ref cel="integr.0a.kohera">integra</ref>,
      <ref cel="kompar.0i">kompara</ref>,
      <ref cel="est.kun0i">kunesta</ref>,
      <ref cel="skem.0a">skema</ref>
    </refgrp>
  </snc>
  <trd lng="en">synoptic</trd>
  <trdgrp lng="be">
    <trd>&c_z;&c_v;&c_o;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_g;&c_l;&c_ja;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_a;&c_w;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_ib;&c_n;&c_a;&c_p;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">sin&ograve;ptic</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;ehledn&yacute;</trd>,
    <trd>synoptick&yacute;</trd>
  </trdgrp>
  <trd lng="de">synoptisch</trd>
  <trd lng="fr">synoptique<klr> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>szinoptikus</trd>,
    <trd>egyszerre &aacute;ttekinthet&odblac;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4E00;&#x89A7;&#x7528;&#x306E;  <pr>&#x3044;&#x3061;&#x3089;&#x3093;&#x3088;&#x3046;&#x306E;</pr></trd>,
    <trd>&#x6982;&#x89B3;&#x7684;&#x306A;  <pr>&#x304C;&#x3044;&#x304B;&#x3093;&#x3066;&#x304D;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>synoptisch</trd>,
    <trd>beknopt</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>synoptyczny</trd>,
    <trd>pogl&aogonek;dowy</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_v;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_b;&c_z;&c_o;&c_r;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_r;&c_a;&c_v;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_i;&c_n;&c_o;&c_p;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; <klr tip="amb">(&c_o;&c_t;&c_n;&c_o;&c_s;&c_ja;&c_shch;&c_i;&c_j;&c_s;&c_ja; &c_k;
    &c_s;&c_i;&c_n;&c_o;&c_p;&c_s;&c_i;&c_s;&c_u;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>preh&lcaron;adn&yacute;</trd>,
    <trd>synoptick&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&c_n;&c_o;&c_p;&c_t;&c_i;&c_ch;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_o;&c_g;&c_l;&c_ja;&c_d;&c_o;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7D9C;&#x89C0; <pr>z&#xF2;nggu&#x101;n</pr></trd>,
    <trd>&#x7EFC;&#x89C2; <pr>z&#xF2;nggu&#x101;n</pr></trd>
  </trdgrp>
</drv>
</art>
