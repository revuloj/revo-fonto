<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>aristoloki</rad>o</kap>
<drv mrk="aristo2.0">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="aristo2.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Aristolochia</trd>), kun grimpantaj tigoj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Osterluzei</trd>,
    <trd>Pfeiffenblume</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&#1082;&#1080;&#1088;&#1082;&#1072;&#1079;&#1086;&#1085;</trd>,
    <trd>&#1082;&#1086;&#1082;&#1086;&#1088;&#1085;&#1080;&#1082;</trd>
  </trdgrp>
</drv>
<drv mrk="aristo2.acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotelidonaj vegeta&jcirc;oj
      (<trd lng="la">Aristolochiaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Osterluzeigew&auml;chse</trd>
</drv>
</art>
