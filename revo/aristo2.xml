<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>aristoloki</rad>/o
</kap>
<drv mrk="aristo2.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="aristo3.0oj"><tld/>acoj</ref>
      (<trd lng="la">Aristolochia</trd>), kun grimpantaj tigoj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Osterluzei</trd>,
    <trd>Pfeiffenblume</trd>
  </trdgrp>
  <trd lng="fr">aristoloche</trd>
  <trd lng="hu">farkasalma</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_r;&c_k;&c_a;&c_z;&c_o;&c_n;</trd>,
    <trd>&c_k;&c_o;&c_k;&c_o;&c_r;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sv">pipranka</trd>
</drv>
</art>
