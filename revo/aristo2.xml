<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aristo2.xml,v 1.24 2025/02/22 20:25:27 revo Exp $">
<kap>
  <rad>aristolo&hcirc;i</rad>/o <fnt><bib>JED</bib></fnt>,
  <var><kap><rad var="k">aristoloki</rad>/o <fnt><bib>PVEF</bib></fnt></kap></var>
</kap>
<drv mrk="aristo2.0o">
  <kap>
    <tld/>o, <var><kap><tld var="k"/>o</kap></var>
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="aristo3.0oj"><tld/>acoj</ref>
      (<trd lng="la">Aristolochia</trd>), kun grimpantaj tigoj:
      <ekz>
        la Kutima <tld var="k"/>o <klr>[&#8230;]</klr> fari&gcirc;as 20&#8239;-&#8239;90&#8239;cm alta kaj formas
        subterajn stolonojn, la senharaj folioj estas rondformaj kun korforma piedo kaj
        elrandita pinto, en la foliakseloj de la supraj folioj estas po du &gcirc;is ok
        floroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=462908">Kutima aristolokio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/d/d1/Aristolochia_clematitis_002.JPG/640px-Aristolochia_clematitis_002.JPG" prm="CC BY-SA 3.0"
      >floroj de Kutima <tld var="k"/>o (Aristolochia clematitis)
        <fnt>
          <aut>H. Zell, propra verko</aut>
          <lok><url ref="&WCW;/File:Aristolochia_clematitis_002.JPG">&WCW;/File:Aristolochia_clematitis_002.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_r;&c_k;&c_a;&c_z;&c_o;&c_n;</trd>,
    <trd>&c_k;&c_a;&c_k;&c_o;&c_r;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Osterluzei</trd>,
    <trd>Pfeiffenblume</trd>
  </trdgrp>
  <trd lng="en">clematis</trd>
  <trd lng="fr">aristoloche</trd>
  <trd lng="hu">farkasalma</trd>
  <trd lng="ja">&#12454;&#12510;&#12494;&#12473;&#12474;&#12463;&#12469;</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_r;&c_k;&c_a;&c_z;&c_o;&c_n;</trd>,
    <trd>&c_k;&c_o;&c_k;&c_o;&c_r;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sv">pipranka</trd>
  <trdgrp lng="uk">
    <trd>&c_h;&c_v;&c_i;&c_l;&c_ib;&c_v;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_k;&c_ib;&c_r;&c_k;&c_a;&c_z;&c_o;&c_n;</trd>
  </trdgrp>
</drv>
</art>
