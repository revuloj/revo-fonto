<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>konfit</rad>/i
</kap>
<drv mrk="konfit.0i">
  <kap>
    <ofc>*</ofc><tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Prepari fruktojn, restadigante ilin en densa siropo, kiu ilin
      penetras kaj konservas.
    </dif>
  </snc>
  <trd lng="de">einkochen</trd>
  <trd lng="en">preserve</trd>
  <trd lng="nl">confijten</trd>
  <trd lng="ru">&c_v;&c_a;&c_r;&c_i;&c_t;&c_mol; &c_v; &c_s;&c_a;&c_h;&c_a;&c_r;&c_je;</trd>
</drv>
<drv mrk="konfit.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc>
    <dif>
      <tld lit="K"/>itaj fruktoj.
    </dif>
  </snc>
  <trd lng="de">Marmelade</trd>
  <trd lng="nl">geconfijte <ind>vruchten</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_a;&c_r;&c_je;&c_n;&c_mol;&c_je;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_f;&c_i;&c_t;&c_ju;&c_r;</trd>
  </trdgrp>
</drv>
</art>
