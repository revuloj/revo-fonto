<kap>
  <rad>Bonaventur</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="bonaventur.0o">
  <kap><tld/>o</kap>
  <snc mrk="bonaventur.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        episkopo <tld/>o Jegli&ccirc; &ccirc;iujn ekzemplerojn de tiu
        &ccirc;i tiam vere
        provoka libro private a&ccirc;etis
        <fnt>
        <aut>Ivan Cankar, trad. Vinko O&scaron;lak</aut>,
        <vrk><url ref="http://www.luin.se/inko/126-7.pdf">La domo de
        Maria
        helpantino</url></vrk>,
        <lok>Krakovo: Jadwiga, 1999</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ctl>La teologio de la historio de sankta <tld/>o</ctl>
        <fnt>
        <vrk><url ref="http://www.ikue.org/papoj/benediktoxvi/biografio.htm">Papo
        Benedikto la
        16a</url></vrk>,
        <lok>[vidita je 2006-02-09]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_B;&c_a;&c_n;&c_a;&c_v;&c_e;&c_n;&c_t;&c_u;&c_r;&c_a;</trd>
