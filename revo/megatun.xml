<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: megatun.xml,v 1.11 2023/09/02 06:04:10 revo Exp $">
<kap>
<rad>megatun</rad>/o
</kap>
<drv mrk="megatun.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MIL</uzo>
    <uzo tip="fak">GEOL</uzo>
    <uzo tip="fak">FIZ</uzo>
    <uzo tip="fak">AST</uzo>
    <mlg>MT</mlg>
    <dif>
      Praktika 
      <ref tip="lst" cel="unu.mezur0o" lst="voko:mezurunuoj">mezurunuo</ref>,
      kiu respondas al la <ref cel="energi.0o.FIZ">energio</ref>
      ellasata de 1&#x202F;000&#x202F;000 tunoj da TNT dum eksplodo:
      <ekz>
        unu <tld/>o egali&gcirc;as al la pereo de Pari<klr>[z]</klr>o
        <fnt>
          <vrk><url ref="http://jackalgirl.blogspot.com/2006/07/matematiko.html">Matematiko</url></vrk>
        </fnt>.
      </ekz>
   </dif>
 </snc>
 <trd lng="be">&c_m;&c_e;&c_g;&c_a;&c_t;&c_o;&c_n;&c_a;</trd>
  <trd lng="ca">megatona</trd>
  <trd lng="de">Megatonne</trd>
  <trd lng="en">megaton</trd>
  <trd lng="fr">m&eacute;gatonne</trd>
  <trd lng="id">megaton</trd>
  <trd lng="it">megatone</trd>
  <trd lng="ja">&#x30E1;&#x30AC;&#x30C8;&#x30F3;  [<ind>&#x3081;&#x304C;&#x3068;&#x3093;</ind>]</trd>
  <trd lng="pl">megatona</trd>
  <trd lng="uk">&#x43C;&#x435;&#x433;&#x430;&#x442;&#x43E;&#x43D;&#x43D;&#x430;</trd>
</drv>
</art>

