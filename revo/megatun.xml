<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>megatun</rad>/o
</kap>
<drv mrk="megatun.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MIL</uzo>
    <uzo tip="fak">GEOL</uzo>
    <uzo tip="fak">FIZ</uzo>
    <uzo tip="fak">AST</uzo>
    <mlg>MT</mlg>
    <dif>
      Praktika 
      <ref tip="lst" cel="unu.mezur0o"
      lst="voko:mezurunuoj">mezurunuo</ref>,
      kiu respondas al la <ref cel="energi.0o.FIZ">energio</ref>
      ellasata 
      de 1 000 000 tunoj da TNT dum eksplodo.
   </dif>
 </snc>
  <trd lng="ca">megatona</trd>
  <trd lng="de">Megatonne</trd>
  <trd lng="en">megaton</trd>
  <trd lng="id">megaton</trd>
  <trd lng="it">megatone</trd>
</drv>
</art>

