  <trd lng="it">megatone</trd>
  <trd lng="ja">&#x30E1;&#x30AC;&#x30C8;&#x30F3;  <pr>&#x3081;&#x304C;&#x3068;&#x3093;</pr></trd>
  <trd lng="pl">megatona</trd>
  <trd lng="uk">&#x43C;&#x435;&#x433;&#x430;&#x442;&#x43E;&#x43D;&#x43D;&#x430;</trd>
  <trdgrp lng="zh">
    <trd>&#x767E;&#x4E07;&#x5428; <pr>b&#x1CE;iw&#xE0;nd&#x16B;n</pr></trd>,
    <trd>&#x767E;&#x842C;&#x5678; <pr>b&#x1CE;iw&#xE0;nd&#x16B;n</pr></trd>,
    <trd>&#x767E;&#x4E07;&#x5428;&#x7EA7; <pr>b&#x1CE;iw&#xE0;nd&#x16B;nj&#xED;</pr></trd>,
    <trd>&#x767E;&#x842C;&#x5678;&#x7D1A; <pr>b&#x1CE;iw&#xE0;nd&#x16B;nj&#xED;</pr></trd>
  </trdgrp>
</drv>
