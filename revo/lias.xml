<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lias.xml,v 1.17 2023/09/01 19:10:59 revo Exp $">
<kap>
  <rad>lias</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="lias.0o">
  <kap><tld/>o</kap>
  <snc mrk="lias.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Geologia formacio de la sekundara epoko, de &ccirc;. 201 &gcirc;is 182
      milionoj da jaroj;
      frua <ref tip="malprt" cel="jxuras.0o">&jcirc;uraso</ref>:
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> estis tiu de la amonitoj, maraj
        kapopieduloj kies &scirc;elo
        evoluis tiel rapide, ke oni uzas ilin por dati la rokajn
        tavolojn en kiu ili
        fosilii&gcirc;is
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=288373">Liaso</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_je;&c_j;&c_a;&c_s;</trd>,
    <trd>&c_ju;&c_r;&c_a; &c_ch;&c_o;&c_r;&c_n;&c_a;&c_ja;</trd>
  </trdgrp>
  <trd lng="cs">lias</trd>
  <trd lng="de">Lias</trd>
  <trdgrp lng="en">
    <trd>lias</trd>,
    <trd>blue <ind>limestone</ind></trd>
  </trdgrp>
  <trd lng="fr">Lias</trd>
  <trd lng="hu">li&aacute;sz</trd>
  <trd lng="ja">&#x30E9;&#x30A4;&#x30A2;&#x30B9;&#x7D71;</trd>
  <trd lng="nl">lias</trd>
  <trd lng="pl">jura</trd>
  <trdgrp lng="pt">
    <trd>li&aacute;ssico</trd>,
    <trd>li&aacute;sico</trd>
  </trdgrp>
  <trd lng="ro">jurasic timpuriu</trd>
  <trd lng="ru">&c_l;&c_je;&c_j;&c_a;&c_s;</trd>
  <trd lng="sk">lias</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&c_je;&c_j;&c_a;&c_s;</trd>,
    <trd>&#x43D;&c_i;&c_zh;&#x43D;&#x44F; &#x44E;&c_r;&c_a;</trd>
  </trdgrp>
</drv>
</art>
