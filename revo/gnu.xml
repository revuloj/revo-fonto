<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gnu</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="gnu.0o">
  <kap><tld/>o</kap>
  <snc mrk="gnu.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="super" cel="antilo.0o.subfamiliano">Antilopo</ref> el 
      afrika savano, 
      nigra, kun lar&gcirc;a kapo kaj kolhararo, vivanta en grandaj aroj,
      genro el familio <ref tip="super" cel="boved.0oj.ZOO">bovedoj</ref> 
      (<trd lng="la">Connochaetes</trd>):
      <ekz>
        kuiri&gcirc;is <klr>...</klr> fuma&jcirc;itaj pecoj de lumba&jcirc;o el la <tld/>o
        <fnt><aut>H. Sienkiewicz, trad. M. Sygnarski</aut>, <vrk><url 
        ref="http://esperanto.pl/page.php?tid=200000">Tra dezerto kaj 
        praarbaro</url></vrk>, <lok>Varsovio, 1978</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;e ni estas kvar striaj <tld/>oj
        <fnt>
          <aut>Best&gcirc;ardeno Veszpr&eacute;m n.o.</aut>,
          <vrk><url ref="http://www.allatkertveszprem.hu/E.Szuletett.htm">
          naski&gcirc;is</url></vrk>,
          <lok>2005-2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">gnou</trd>
  <trd lng="it">gnu<klr> (antilope)</klr></trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>