<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gnu.xml,v 1.16 2025/08/24 16:34:51 revo Exp $">
<kap>
  <rad>gnu</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>

<drv mrk="gnu.0o">
  <kap><tld/>o</kap>
  <snc mrk="gnu.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="super" cel="antilo.0o.subfamiliano">Antilopo</ref> el
      afrika savano,
      nigra, kun lar&gcirc;a kapo kaj kolhararo, vivanta en grandaj aroj,
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio
      <ref tip="malprt" cel="boved.0oj.ZOO">bovedoj</ref>
      (<trd lng="la">Connochaetes</trd>):
      <ekz>
        kuiri&gcirc;is <klr>...</klr> fuma&jcirc;itaj pecoj de lumba&jcirc;o el la <tld/>o
        <fnt><aut>H. Sienkiewicz, trad. M. Sygnarski</aut>, <vrk><url
        ref="http://esperanto.pl/page.php?tid=200000">Tra dezerto kaj
        praarbaro</url></vrk>, <lok>Varsovio, 1978</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;e ni estas kvar striaj <tld/>oj
        <fnt>
          <aut>Best&gcirc;ardeno Veszpr&eacute;m n.o.</aut>,
          <vrk><url ref="http://www.allatkertveszprem.hu/E.Szuletett.htm">
          naski&gcirc;is</url></vrk>,
          <lok>2005-2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_n;&c_u; <klr tip="amb">(&c_a;&c_n;&c_t;&c_y;&c_l;&c_jo;&c_p;&c_a;)</klr></trd>
  <trd lng="bg">&c_g;&c_n;&c_u;<klr tip="amb"> (&c_a;&c_n;&c_t;&c_i;&c_l;&c_o;&c_p;&c_a;)</klr></trd>
  <trd lng="ca">nyu</trd>
  <trd lng="cs">pak&uring;&ncaron;</trd>
  <trd lng="de">Gnu<klr> (Antilope)</klr></trd>
  <trd lng="en">gnu</trd>
  <trd lng="fr">gnou</trd>
  <trd lng="it">gnu<klr> (antilope)</klr></trd>
  <trd lng="ja">&#12492;&#12540;</trd>
  <trd lng="pl">gnu</trd>
  <trd lng="pt">gnu</trd>
  <trd lng="sk">pak&ocirc;&ncaron;</trd>
  <trd lng="uk">&c_a;&c_n;&c_t;&c_i;&c_l;&c_o;&c_p;&c_a; &c_g;&c_n;&c_u;</trd>
  <trdgrp lng="zh">
    <trd>&#29275;&#32666; <pr>ni&uacute;l&iacute;ng</pr></trd>,
    <trd>&#35282;&#39340; <pr>ju&eacute;m&#462;</pr></trd>
  </trdgrp>
</drv>
</art>

