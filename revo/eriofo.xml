<art mrk="$Id: eriofo.xml,v 1.19 2022/03/17 13:47:19 revo Exp $">
<kap>
<rad>eriofor</rad>/o
</kap>
<drv mrk="eriofo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="ciper.0oj">ciperacoj</ref>,
      (<trd lng="la">Eriophorum</trd>):
      <ekz>
        mallar&gcirc;folia <tld/>o (Eriophorum angustifolium) <klr>[&#x2026;]</klr>
        kreskas sur humida, acida grundo, kiel erikejo kaj torfejo, &gcirc;i 
        formas tie radika&jcirc;ojn per helpo de rizomoj, okulfrapa estas
        la longa, blanka fruktofloko iom simila al kotono
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=449924">Mallar&gcirc;folia erioforo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="&WCU;/thumb/a/a9/Common_cottongrass_in_a_cliff_crevice_2.jpg/411px-Common_cottongrass_in_a_cliff_crevice_2.jpg" prm="CC0">Mallar&gcirc;folia <tld/>o (Eriophorum angustifolium), &Aring;levik, Lysekil, Svedujo
       <fnt>
         <aut>W. Carter, propra verko</aut>
         <lok><url ref="&WCW;/File:Common_cottongrass_in_a_cliff_crevice_2.jpg">&WCW;/File:Common_cottongrass_in_a_cliff_crevice_2.jpg</url></lok>
       </fnt>
  </bld>
  <trd lng="be">&c_p;&c_a;&c_d;&c_v;&c_je;&c_j;</trd>
  <trd lng="bg">&c_p;&c_u;&c_sh;&c_i;&c_c;&c_a;</trd>
  <trd lng="de">Wollgras</trd>
  <trd lng="en">cotton grass</trd>
  <trdgrp lng="fr">
    <trd>linaigrette</trd>,
    <trd>jonc &agrave; coton</trd></trdgrp> 
  <trd lng="hu">gyapj&uacute;s&aacute;s</trd>
  <trd lng="nl">wollegras</trd>
  <trd lng="pl">we&lstroke;nianka</trd>
  <trd lng="pt">er&iacute;ngio</trd>
  <trd lng="ru">&c_p;&c_u;&c_sh;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">p&aacute;pern&iacute;k</trd>
  <trd lng="cs">suchop&yacute;r</trd>
  <trd lng="uk">&#x43F;&#x443;&#x445;&#x456;&#x432;&#x43A;&#x430;</trd>
</drv>
</art>
