<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>por</rad>/o
</kap>
<drv mrk="por1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;iu el la preska&ubreve; nepercepteblaj truetaj de la ha&ubreve;to, tra kiuj
      eliras la &scirc;vito.
    </dif>
  </snc>
  <snc>
    <dif>
      &Ccirc;iu el la malgrandaj malplena&jcirc;oj, kiuj apartigas la molekulojn de
      korpo, kaj igas &gcirc;in pli malpli penetrebla por akvo, gasoj, kc.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_o;&c_r;&c_a;</trd>
  <trd lng="de">Pore</trd>
  <trd lng="hu">p&oacute;rus</trd>
  <trd lng="nl">porie</trd>
  <trd lng="pt">poro</trd>
  <trd lng="ru">&c_p;&c_o;&c_r;&c_a;</trd>
  <trd lng="sv">por</trd>
</drv>
</art>
