<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>por</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="por1.0o">
  <kap><tld/>o</kap>
  <snc mrk="por1.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      &Ccirc;iu el la apena&ubreve; videblaj truetoj de la ha&ubreve;to, tra kiuj
      eliras &scirc;vito.
    </dif>
  </snc>
  <snc mrk="por1.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      &Ccirc;iu el la malgrandaj malplena&jcirc;oj, kiuj apartigas la molekulojn
      de korpo, kaj igas &gcirc;in pli malpli enlasema por akvo, gasoj, kaj
      ceteraj.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_o;&c_r;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>průduch</trd>,
    <trd>pór</trd>
  </trdgrp>
  <trd lng="de">Pore</trd>
  <trd lng="en">pore</trd>
  <trd lng="fr">pore</trd>
  <trd lng="hu">p&oacute;rus</trd>
  <trd lng="nl">porie</trd>
  <trd lng="pl">por</trd>
  <trd lng="pt">poro</trd>
  <trd lng="ru">&c_p;&c_o;&c_r;&c_a;</trd>
  <trd lng="sk">pór</trd>
  <trd lng="sv">por</trd>
</drv>
</art>
