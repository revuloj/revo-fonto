<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: blag.xml,v 1.24 2024/01/30 09:21:47 revo Exp $">
<kap>
  <rad>blag</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="blag.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="blag.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="sxerc.0i.KOMUNE">&Scirc;erce</ref>,
      mistifike mensogi:
      <ekz>
        &scirc;i son&gcirc;is pri la floroj kaj pri la studento, kiun la kancelariano ripro&ccirc;is,
        dirante, ke li nur <tld/>as al &scirc;i
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne <tld/>u! a&ubreve; &ccirc;u vi prenas min por <ref cel="dup.0o">dupo</ref>
        a&ubreve; <ref cel="sxlemil.0o">&scirc;lemilo</ref>?
        <fnt>
          <bib>MMa</bib>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;<nom>Mat</nom>, mi havas la penson, ke vi <tld/>as min&rightquot;
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxarla.0i">&ccirc;arlatani</ref>,
      <ref cel="fanfar1.0i.sxajnigi">fanfaroni<sncref/></ref>,
      <ref cel="fripon.0i.trompi">friponi<sncref/></ref>,
      <ref cel="mensog.0i">mensogi</ref>,
      <ref cel="mistifik.0i">mistifiki</ref>,
      <ref cel="mok.0i">moki</ref>,
      <ref cel="preten.0i.parole">pretendi<sncref/></ref>,
      <ref cel="tromp.0i">trompi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>nalhat</trd>,
    <trd>napov&iacute;dat</trd>,
    <trd>pr&aacute;&scaron;it</trd>,
    <trd>&zcaron;ertem lh&aacute;t</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>aufschneiden</trd>,
    <trd>flunkern</trd>,
    <trd>prahlen</trd>,
    <trd>veralbern</trd>,
    <trd>verarschen</trd>,
    <trd>auf den <ind>Arm</ind> nehmen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>pull someone's leg</trd>,
    <trd>talk tongue in cheek</trd>,
  </trdgrp>
  <trdgrp lng="es">
    <trd>bromear</trd>,
    <trd>gastar bromas</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>blaguer</trd>,
    <trd>raconter des <ind>bobards</ind></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&lamed;&mem;&tav;&vav;&het;</trd>,
    <trd>&lamed;&shin;&tet;&vav;&tav; &bet;</trd>
  </trdgrp>
  <trd lng="hu">l&oacute;d&iacute;t</trd>
  <trdgrp lng="ja">
    <trd>&#12391;&#12383;&#12425;&#12417;&#12434;&#12356;&#12358;</trd>,
    <trd>&#12411;&#12425;&#12434;&#21561;&#12367;  <pr>&#12507;&#12521;&#12434;&#12405;&#12367;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>blagowa&cacute;</trd>,
    <trd>bajerowa&cacute;</trd>,
    <trd>wkr&eogonek;ca&cacute;<klr> kogo&sacute;</klr></trd>,
    <trd>wpuszcza&cacute;<klr> kogo&sacute;</klr> w
    kana&lstroke;</trd>,
    <trd>wpuszcza&cacute;<klr> kogo&sacute;</klr> w maliny</trd>,
    <trd>wciska&cacute;<klr> komu&sacute;</klr> kit</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_u;&c_r;&c_a;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_b;&c_l;&c_a;&c_zh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_sh;&c_u;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">klama&tcaron; zo &zcaron;artu</trd>
  <trdgrp lng="uk">
    <trd>&c_h;&c_v;&c_a;&c_s;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_h;&c_v;&c_a;&c_l;&c_ja;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_b;&c_r;&c_ib;&c_h;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#25769; <pr>li&aacute;o</pr></trd>,
    <trd>&#23346; <pr>ni&#462;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="blag.0o">
  <kap><tld/>o</kap>
  <snc mrk="blag.0o.KOMUNE">
    <dif>
      &Scirc;erca <ref tip="super" cel="mensog.0o">mensogo</ref>:
      <ekz>
        oni eksopiros al kvieta soleco, odoranta idilion &dash; la <tld/>on de la poetoj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;he, for la <tld/>o!&rightquot; mi diris, &leftquot;<klr>[&#8230;]</klr> mi ne kredas je fantomoj&rightquot;
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        &#8218;kampara printempo&#8216; estas la plej kruda <tld/>o ekzistanta, &ccirc;ie malnova kaduka
        ne&gcirc;o, kaj la vojo estis kotejo
        <fnt>
          <bib>Kiso</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="iluzi.0o">iluzio</ref>,
      <ref cel="sxerc.0o">&scirc;erco</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>humbuk</trd>,
    <trd>tlach</trd>,
    <trd>&zcaron;v&aacute;st</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Flunkerei</trd>,
    <trd>Humbug</trd>,
    <trd>Prahlerei</trd>,
    <trd>Unsinn</trd>,
    <trd>Verarsche</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>blarney</trd>,
    <trd>fib</trd>,
    <trd>humbug</trd>,
    <trd>nonsense</trd>,
    <trd>hoax</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>broma</trd>,
    <trd>chanza</trd>,
    <trd>guasa</trd>,
    <trd>enga&ntilde;o</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>blague<klr tip="ind"> (mystification)</klr></trd>,
    <trd>bobard</trd>
  </trdgrp>
  <trd lng="he">&mem;&tav;&yod;&het;&he;</trd>
  <trd lng="hu">l&oacute;d&iacute;t&aacute;s</trd>
  <trdgrp lng="ja">
    <trd>&#12391;&#12383;&#12425;&#12417;</trd>,
    <trd>&#20887;&#35527;  <pr>&#12376;&#12423;&#12358;&#12384;&#12435;</pr></trd>,
    <trd>&#12411;&#12425;&#35441;  <pr>&#12411;&#12425;&#12400;&#12394;&#12375;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>blaga</trd>,
    <trd>bajer<klr> (pot.)</klr></trd>,
    <trd>wkr&eogonek;t<klr> (pot.)</klr></trd>,
    <trd>kit<klr> (pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_o;&c_z;&c_y;&c_g;&c_r;&c_y;&c_sh;</trd>,
    <trd>&c_sh;&c_u;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">lo&zcaron; zo &zcaron;artu</trd>
  <trdgrp lng="uk">
    <trd>&c_h;&c_v;&c_a;&c_s;&c_t;&c_o;&c_shch;&c_ib;</trd>,
    <trd>&c_b;&c_r;&c_je;&c_h;&c_n;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_b;&c_r;&c_je;&c_h;&c_je;&c_n;&c_mol;&c_k;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="blag.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="blag.0ulo.KOMUNE">
    <dif>
      Diranto de <tld/>oj:
      <ekz>
        nu, tion certe al vi <nom>Darja</nom> rakontis, <klr>[&#8230;]</klr> &scirc;i estas terura <tld/>ulino
        <fnt>
          <bib>MkM</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="cxarla.0o">&ccirc;arlatano</ref>,
       <ref cel="fripon.0o">fripono</ref>
    </refgrp>
    <trd lng="zh">&#25749;&#35010;&#30340;&#20154; <pr>s&imacron;li&egrave;der&eacute;n</pr></trd>
</snc>
  <trdgrp lng="de">
    <trd>Aufschneider</trd>,
    <trd>Prahlhans</trd>,
  </trdgrp>
  <trd lng="es">bromista</trd>
  <trd lng="fr">blagueur</trd>
  <trd lng="he">&shin;&qof;&resh;&fnun;</trd>
  <trdgrp lng="uk">
    <trd>&c_h;&c_v;&c_a;&c_l;&c_mol;&c_k;&c_o;</trd>,
    <trd>&c_p;&c_u;&c_s;&c_t;&c_o;&c_m;&c_je;&c_l;&c_ja;</trd>,
    <trd>&c_b;&c_r;&c_je;&c_h;&c_u;&c_n;</trd>
  </trdgrp>
</drv>
</art>
