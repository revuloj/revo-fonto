<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: blag.xml,v 1.17 2021/03/05 15:48:37 revo Exp $">
<kap>
  <rad>blag</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="blag.0o">
  <kap><tld/>o</kap>
  <snc mrk="blag.0o.KOMUNE">
    <dif>
      &Scirc;erca mensogo. 
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>humbuk</trd>,
    <trd>tlach</trd>,
    <trd>&zcaron;v&aacute;st</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Flunkerei</trd>,
    <trd>Humbug</trd>,
    <trd>Prahlerei</trd>,
    <trd>Unsinn</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>blarney</trd>,
    <trd>fib</trd>,
    <trd>humbug</trd>,
    <trd>nonsense</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>broma</trd>,
    <trd>chanza</trd>,
    <trd>guasa</trd>,
    <trd>enga&ntilde;o</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>blague<klr tip="ind">(mystification)</klr></trd>,
    <trd>bobard</trd>
  </trdgrp>
  <trd lng="he">&#x5DE;&#x5EA;&#x5D9;&#x5D7;&#x5D4;</trd>
  <trd lng="hu">l&oacute;d&iacute;t&aacute;s</trd>
  <trdgrp lng="pl">
    <trd>blaga</trd>,
    <trd>bajer<klr> (pot.)</klr></trd>,
    <trd>wkr&eogonek;t<klr> (pot.)</klr></trd>,
    <trd>kit<klr> (pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_o;&c_z;&c_y;&c_g;&c_r;&c_y;&c_sh;</trd>,
    <trd>&c_sh;&c_u;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">lo&zcaron; zo &zcaron;artu</trd>
</drv>
  
<drv mrk="blag.0i">
  <kap><tld/>i<fnt>Z</fnt></kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="blag.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="sxerc.0i.KOMUNE">&Scirc;erce</ref>,
      mistifike mensogi:
      <ekz>
        &leftquot;Mat, mi havas la penson, ke vi <tld/>as min&rightquot;
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>nalhat</trd>,
    <trd>napov&iacute;dat</trd>,
    <trd>pr&aacute;&scaron;it</trd>,
    <trd>&zcaron;ertem lh&aacute;t</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>aufschneiden</trd>,
    <trd>flunkern</trd>,
    <trd>prahlen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>pull someone's leg</trd>,
    <trd>talk tongue in cheek</trd>,
  </trdgrp>
  <trdgrp lng="es">
    <trd>bromear</trd>,
    <trd>gastar bromas</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>blaguer</trd>,
    <trd>raconter des <ind>bobards</ind></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&#x5DC;&#x5DE;&#x5EA;&#x5D5;&#x5D7;</trd>,
    <trd>&#x5DC;&#x5E9;&#x5D8;&#x5D5;&#x5EA; &#x5D1;</trd>
  </trdgrp>
  <trd lng="hu">l&oacute;d&iacute;t</trd>
  <trdgrp lng="pl">
    <trd>blagowa&cacute;</trd>,
    <trd>bajerowa&cacute;</trd>,
    <trd>wkr&eogonek;ca&cacute;<klr> kogo&sacute;</klr></trd>,
    <trd>wpuszcza&cacute;<klr> kogo&sacute;</klr> w
    kana&lstroke;</trd>,
    <trd>wpuszcza&cacute;<klr> kogo&sacute;</klr> w maliny</trd>,
    <trd>wciska&cacute;<klr> komu&sacute;</klr> kit</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_u;&c_r;&c_a;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_b;&c_l;&c_a;&c_zh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_sh;&c_u;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">klama&tcaron; zo &zcaron;artu</trd>
</drv>

<drv mrk="blag.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="blag.0ulo.KOMUNE">
    <dif>
      Diranto de <tld/>oj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Aufschneider</trd>,
    <trd>Prahlhans</trd>,
  </trdgrp>
  <trd lng="es">bromista</trd>
  <trd lng="fr">blagueur</trd>
  <trd lng="he">&#x5E9;&#x5E7;&#x5E8;&#x5DF;</trd>
</drv>
</art>
