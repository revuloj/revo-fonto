<drv mrk="plan.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="plan.0i.KOMUNE">
    <dif>
      Fari <ref cel="plan.0o.realigota"><tld/>on<sncref/></ref>:
      <ekz>
        li <tld/>is voja&gcirc;i al Oriento, tamen la unua mondmilito
        <klr>[&#8230;]</klr> devigis lin veturi al Finnlando
        <fnt>
        <aut>N. Arolovi&ccirc;</aut>,
        <vrk>La homo de la kosma konscio</vrk>.
        <lok>Monato</lok>
        </fnt>;
      </ekz>
      <ekz>
        en nokto tiel serena <klr>[&#8230;]</klr> ni <tld/>is nian estontan vivon
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XL</lok>
        </fnt>;
      </ekz>
      <ekz>
        reve li <tld/>is tiun mondon, kiu iam estos lia kaj tute la&ubreve; lia deziro
        <fnt>
          <bib>HsT</bib>
          <lok>Disaj vojoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        se estis <tld/>ita murdo, kiu <tld/>is &gcirc;in
        <fnt>
        <aut>R. Rotsaert</aut>,
        <vrk>Kiu kulpas pri la morto de Lumumba?</vrk>.
        <lok>Monato</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ja;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">planen</trd>
  <trd lng="es">planear</trd>
  <trdgrp lng="fr">
    <trd> planifier </trd>,
    <trd>avoir l'<ind>intention</ind> de</trd>
  </trdgrp>
  <trd lng="hu">tervez</trd>
  <trdgrp lng="id">
    <trd>be<ind>rencana</ind></trd>,
    <trd>me<ind>rencana</ind>kan</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#35336;&#30011;&#12377;&#12427;  <pr>&#12369;&#12356;&#12363;&#12367;&#12377;&#12427;</pr></trd>,
    <trd>&#35373;&#35336;&#12377;&#12427;  <pr>&#12379;&#12387;&#12369;&#12356;&#12377;&#12427;</pr></trd>,
    <trd>&#31435;&#26696;&#12377;&#12427;  <pr>&#12426;&#12388;&#12354;&#12435;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">ontwerpen</trd>
  <trdgrp lng="pl">
    <trd>planowa&cacute;</trd>,
    <trd>zamy&sacute;la&cacute;</trd>
