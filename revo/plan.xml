<drv mrk="plan.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="plan.0i.KOMUNE">
    <dif>
      Fari <ref cel="plan.0o.realigota"><tld/>on<sncref/></ref>:
      <ekz>
        li <tld/>is voja&gcirc;i al Oriento, tamen la unua mondmilito
        <klr>...</klr> devigis lin veturi al Finnlando
        <fnt>
        <aut>N. Arolovi&ccirc;</aut>,
        <vrk>La homo de la kosma konscio</vrk>.
        <lok>Monato</lok>
        </fnt>;
      </ekz>
      <ekz>
        se estis <tld/>ita murdo, kiu <tld/>is &gcirc;in?
        <fnt>
        <aut>R. Rotsaert</aut>,
        <vrk>Kiu kulpas pri la morto de Lumumba?</vrk>.
        <lok>Monato</lok>
        </fnt>;
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ja;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">planen</trd>
  <trd lng="es">planear</trd>
  <trdgrp lng="fr">
    <trd> planifier </trd>,
    <trd>avoir l'<ind>intention</ind> de</trd>
  </trdgrp>
  <trd lng="hu">tervez</trd>
  <trdgrp lng="id">
    <trd>be<ind>rencana</ind></trd>,
    <trd>me<ind>rencana</ind>kan</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8A08;&#x753B;&#x3059;&#x308B;  <pr>&#x3051;&#x3044;&#x304B;&#x304F;&#x3059;&#x308B;</pr></trd>,
