<drv mrk="plan.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="plan.0ilo.KOMUNE">
    <dif>
      Rimedo por fari
      <ref cel="plan.0o.realigota"><tld/>ojn<sncref/></ref>:
    </dif>
    <subsnc mrk="plan.0ilo.arangxoj">
      <uzo tip="fak">KOMP</uzo>
      <dif>
        Aplika programo en persona komputilo a&ubreve; po&scirc;komputilo
        ebliganta manipuli negrandan datumbazon por registri kaj <tld/>i
        aran&gcirc;ojn, taskojn, renkonti&gcirc;ojn ktp:
        <ekz>
        kompreneble disponeblas la funkcioj por kiuj la normaluloj kutime a&ccirc;etas
        po&scirc;komputilon: <tld/>ilo, vekilo, telefona kaj geografia datumbazo, adreslibro
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470; 10</lok>
        </fnt>.
      </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>Organizer</trd>,
        <trd>Terminplaner</trd>
      </trdgrp>
      <trd lng="en">organizer</trd>
      <trd lng="fr">organiseur</trd>
      <trd lng="hu">organizer</trd>
      <trd lng="nl">organizer</trd>
    </subsnc>
  <trd lng="de">Planer</trd>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ja;&c_n;&c_ib;&c_r;&c_o;&c_w;&c_sh;&c_ch;&c_y;&c_k;</trd>
  <trd lng="pl">organizator  <klr>(inf.)</klr></trd>
  <trd lng="ro">organizator</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_n;&c_i;&c_r;&c_o;&c_v;&c_shch;&c_i;&c_k;</trd>
</drv>

<drv mrk="plan.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="plan.sen0a.KOMUNE">
    <dif>
      Ne <tld/>ita, senprepara,
      <ref tip="vid" cel="improv.0i">improvizita</ref>:
      <ekz>
        estas tiel agrable vivi sen<tld/>e
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#8470; 2 (100)</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>lingvo Esperanto vere ne multe pli bone ta&ubreve;gus por aviado ol iu etna
        sen<tld/>a lingvo
        <fnt>
          <bib>Monato</bib>
          <vrk>&Ccirc;u ni mortu miskomprene? (2)</vrk>
        </fnt>;
      </ekz>
      <ekz>
        pli ol sen<tld/>e kreskigi la stabon de organizoj, oni devas pensi pri tio kiel
        &ccirc;iu homo en nia movado povas kontribui
        <fnt>
          <bib>Kon11</bib>
          <lok>Kion nia movado povas lerni de Boeing kaj Facebook?</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj arbaraj birdoj devis forlasi sian hejmon, flugadis sen<tld/>e kaj kriadis
        de timo
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        Li kondukas propramane, &#11005; Zigzagante kaj sen<tld/>e &#11005; Grimpas sur la trotuaron
        <fnt>
          <bib>LSB</bib>
          <lok>En Kinematografejo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <ind>sen<tld/>eco</ind> de la kunveno amuzis ilin
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>.
        <trd lng="de">Planlosigkeit</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_z;&c_a;&c_p;&c_l;&c_ja;&c_n;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
  <trd lng="cs">nepl&aacute;novan&yacute;</trd>
  <trd lng="de">planlos</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;cousu</trd>,
    <trd>sans queue ni t&ecirc;te</trd>
  </trdgrp>
  <trd lng="id">impromptu</trd>
  <trdgrp lng="ja">
    <trd>&#28961;&#35336;&#30011;&#12394;  <pr>&#12416;&#12369;&#12356;&#12363;&#12367;&#12394;</pr></trd>,
    <trd>&#35336;&#30011;&#24615;&#12398;&#12394;&#12356;
