<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <rad>epitro&hcirc;oid</rad>/o
  </kap>
<drv mrk="epitro.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <uzo tip="stl">ARK</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <refgrp tip="sub">
      <ref cel="epicik.longigita0o">longigita epicikloido</ref>,
      <ref cel="epicik.mallongigita0o">mallongigita epicikloido</ref>;
    </refgrp>
    <refgrp tip="vid">
      <ref cel="trohxo.0o">tro&hcirc;oido</ref>,
      <ref cel="hipotr.0o">hipotro&hcirc;oido</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_p;&c_ib;&c_t;&c_r;&c_a;&c_h;&c_o;&c_ib;&c_d;&c_a;</trd>
  <trd lng="de">Epitrochoide</trd>
  <trd lng="en">epitrochoid</trd>
  <trd lng="fr">&eacute;pitrocho&iuml;de</trd>
  <trd lng="hu">epitrochoid</trd>
  <trd lng="pl">epitrochoida</trd>
  <trd lng="ru">&c_e;&c_p;&c_i;&c_t;&c_r;&c_o;&c_h;&c_o;&c_i;&c_d;&c_a;</trd>
</drv>
</art>

