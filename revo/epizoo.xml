<kap>
  <rad>epizooti</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="epizoo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc mrk="epizoo.0o.epidemio">
    <dif>
      Epidemia <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref> &ccirc;e bestoj:
      <ekz>
        &ccirc;iu animala speco havas siajn <tld/>ojn;
      </ekz>
      <ekz>
        sova&gcirc;aj birdoj da&ubreve;re migras de sud-orienta Azio, kie komenci&gcirc;is la birdogripo,
        al A&ubreve;stralio, kie birdogripo ankora&ubreve; ne aperis, la <tld/>o disvasti&gcirc;is &ccirc;efe
        de &Ccirc;inio al E&ubreve;ropo
        <fnt>
          <bib>Monato</bib>
          <aut>Thierry Tailhades</aut>
          <vrk><url ref="&Monato;/2006/008985.php"
              >Gurdado de onidiroj</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_p;&c_ib;&c_z;&c_a;&c_o;&c_t;&c_y;&c_ja;</trd>
  <trdgrp lng="ca">
  <trd>epizo&ograve;tia</trd>,
  <trd>epizoonosi</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>epizootie</trd>,
    <trd>n&aacute;kaza zv&iacute;&rcaron;at</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Tierseuche</trd>,
     <trd>Viehseuche</trd>,
     <trd>Viehsterben</trd>,
     <trd>Epizootie</trd>
  </trdgrp>
  <trd lng="en">epizootic</trd>
  <trd lng="fr">&eacute;pizootie</trd>
  <trdgrp lng="hu">
  <trd>&aacute;llatj&aacute;rv&aacute;ny</trd>,
  <trd>&aacute;llatv&eacute;sz</trd>
  </trdgrp>
  <trd lng="id">epizotik</trd>
  <trdgrp lng="ja">
    <trd>&#23478;&#30044;&#27969;&#34892;&#30149; <pr>&#12363;&#12385;&#12367;&#12426;&#12421;&#12358;&#12371;&#12358;&#12403;&#12423;&#12358;</pr></trd>,
    <trd>&#29539;&#30123; <pr>&#12376;&#12421;&#12358;&#12360;&#12365;</pr></trd>
  </trdgrp>
  <trd lng="pt">epizootio</trd>
    <trdgrp lng="ru">
    <trd>&c_e;&c_p;&c_i;&c_z;&c_o;&c_o;&c_t;&c_i;&c_ja;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_jo;&c_zh; &c_s;&c_k;&c_o;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">hromadn&eacute; ochorenie zvierat <klr>(mor)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_je;&c_p;&c_ib;&c_z;&c_o;&c_o;&c_t;&c_ib;&c_ja;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_ib;&c_zh; &c_h;&c_u;&c_d;&c_o;&c_b;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#21160;&#29289;&#38450;&#30123; <pr>d&ograve;ngw&ugrave;f&aacute;ngy&igrave;</pr></trd>
</drv>
