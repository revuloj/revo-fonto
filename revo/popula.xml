<drv mrk="popula.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Facile komprenebla por la popolo:
      <ekz>
        <tld/>a elokventeco, bro&scirc;uro.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="elemen.0a">elementa</ref>,
      <ref cel="trivia.0a">triviala</ref>
    </refgrp>
  </snc>
  <snc>
    <dif>
      Konata kaj amata de la popolo:
      <ekz>
        <tld/>a verko<fnt>Z</fnt>,
      </ekz><ekz>
        a&ubreve;toro, oratoro;
      </ekz><ekz>
        tiu en la publiko tre <tld/>a opinio estas erara<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vast">disvastigita</ref>,
      <ref cel="gxener.0a">&gcirc;enerala</ref>,
      <ref cel="kutim.0a">kutima</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_p;&c_u;&c_l;&c_ja;&c_r;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>obl&iacute;ben&yacute;</trd>,
    <trd>popul&aacute;rn&iacute;</trd>
  </trdgrp>
  <trd lng="de">popul&auml;r</trd>
  <trd lng="en">popular</trd>
  <trd lng="es">popular</trd>
  <trd lng="fr">populaire</trd>
  <trd lng="hu">n&eacute;pszer&udblac;</trd>
  <trdgrp lng="ja">
    <trd>&#x4EBA;&#x6C17;&#x306E;&#12354;&#x308B;  <pr>&#x306B;&#12435;&#x304D;&#x306E;&#12354;&#x308B;</pr></trd>,
    <trd>&#x30DD;&#x30D4;&#x30E5;&#x30E9;&#x30FC;&#x306A; 
    <pr>&#x307D;&#12404;&#12421;&#12425;&#x30FC;&#x306A;</pr></trd>,
    <trd>&#22823;&#34886;&#30340;&#x306A;  <pr>&#x305F;&#12356;&#12375;&#12421;&#12358;&#12390;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x901A;&#x4FD7;&#30340;&#x306A;  <pr>&#12388;&#12358;&#x305E;&#x304F;&#12390;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x5E73;&#26131;&#x306A;  <pr>&#12408;&#12356;&#12356;&#x306A;</pr></trd>
  </trdgrp>
  <trd lng="nl">populair</trd>
  <trdgrp lng="pl">
    <trd>popularny</trd>,
    <trd>g&lstroke;o&sacute;ny</trd>,
    <trd>s&lstroke;ynny</trd>
  </trdgrp>
  <trd lng="pt">popular</trd>
    <trdgrp lng="ro">
    <trd>agreat</trd>,
    <trd>popular</trd>,
    <trd>celebru</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_p;&c_u;&c_l;&c_ja;&c_r;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>ob&lcaron;&uacute;ben&yacute;</trd>,
    <trd>popul&aacute;rny</trd>
  </trdgrp>
  <trd lng="sv">popul&auml;r</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_p;&c_u;&c_l;&c_ja;&c_r;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_d;&c_o;&c_s;&c_t;&c_u;&c_p;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6C11;&#38291; <pr>m&#xED;nji&amacron;n</pr></trd>,
    <trd>&#x6C11;&#x95F4; <pr>m&#xED;nji&amacron;n</pr></trd>,
    <trd>&#x98A8;&#x884C; <pr>f&emacron;ngx&#xED;ng</pr></trd>,
    <trd>&#x98CE;&#x884C; <pr>f&emacron;ngx&#xED;ng</pr></trd>,
    <trd>&#x53D7;&#x6B22;&#x8FCE; <pr>sh&#xF2;uhu&amacron;ny&#xED;ng</pr></trd>,
