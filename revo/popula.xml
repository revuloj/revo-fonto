</drv>

<drv mrk="popula.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="popula.0igi.scion">
    <dif>
      Diskonigi al publiko fakajn sciojn en facila, &gcirc;enerale komprenebla formo:
      <ekz>
        <tld/>igi la sciencon
        <fnt><bib>PV</bib></fnt>;
      </ekz><ekz>
        En 1971 aperis la valora <tld/>scienca originala&jcirc;o de
        Alberto Fernandez &leftquot;Sen&gcirc;enaj Dialogoj&rightquot;, kiu celas <tld/>igi la
        fundamentajn demandojn de fiziko kaj astronomio
        <fnt><vrk>Esperanto en perspektivo</vrk>. <lok>1974. P.&nbsp;266</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_p;&c_u;&c_l;&c_ja;&c_r;&c_y;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>vulgaritzar</trd>,
    <trd>divulgar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>popularizovat</trd>,
    <trd>zpopul&aacute;rnit</trd>
  </trdgrp>
  <trd lng="en">popularize</trd>
  <trdgrp lng="es">
    <trd>popularizar</trd>,
    <trd>vulgarizar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>vulgariser</trd>,
    <trd>populariser</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#22823;&#34886;&#21270;&#12377;&#12427;  <pr>&#12383;&#12356;&#12375;&#12421;&#12358;&#12363;&#12377;&#12427;</pr></trd>,
    <trd>&#36890;&#20439;&#21270;&#12377;&#12427;  <pr>&#12388;&#12358;&#12382;&#12367;&#12363;&#12377;&#12427;</pr></trd>,
    <trd>&#22823;&#34886;&#21521;&#12365;&#12395;&#12377;&#12427;  <pr>&#12383;&#12356;&#12375;&#12421;&#12358;&#12416;&#12365;&#12395;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>popularyzowa&cacute;</trd>,
    <trd>upowszechnia&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>populariza</trd>,
    <trd>s&abreve; r&abreve;sp&acirc;ndeasc&abreve;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_p;&c_u;&c_l;&c_ja;&c_r;&c_i;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_p;&c_u;&c_l;&c_ja;&c_r;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">popularizova&tcaron;</trd>
  <trd lng="uk">&c_p;&c_o;&c_p;&c_u;&c_l;&c_ja;&c_r;&c_i;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="popula.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="popula.0igxi.KOMUNE">
    <dif>
      I&gcirc;i <tld/>a, konata a&ubreve; &scirc;atata de homoj:
      <ekz>
        en la amfiteatroj <tld/>i&gcirc;is vesperaj spektakloj
        <fnt><aut>H. Sienkewicz, trad. L. Zamenhof</aut>, <vrk>Quo
        vadis?</vrk>, <lok>1933</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_p;&c_u;&c_l;&c_ja;&c_r;&c_y;&c_z;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>popularitzar-se</trd>,
    <trd>esdevenir <ind>popular</ind></trd>
  </trdgrp>
  <trd lng="es">hacerse popular</trd>
  <trd lng="fr">se <ind>populariser</ind></trd>
  <trdgrp lng="ja">
    <trd>&#20154;&#27671;&#12364;&#20986;&#12427; 
    <pr>&#12395;&#12435;&#12365;&#12364;&#12391;&#12427;</pr></trd>,
    <trd>&#22823;&#34886;&#21270;&#12377;&#12427;  <pr>&#12383;&#12356;&#12375;&#12421;&#12358;&#12363;&#12377;&#12427;</pr></trd>,
    <trd>&#36890;&#20439;&#21270;&#12377;&#12427;  <pr>&#12388;&#12358;&#12382;&#12367;&#12363;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>popularyzowa&cacute; si&eogonek;</trd>,
    <trd>spopularyzowa&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>devine <ind>popular</ind></trd>,
    <trd>s&abreve; devin&abreve; <ind>populare</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_t;&c_mol; &c_p;&c_o;&c_p;&c_u;&c_l;&c_ja;&#769;&c_r;&c_n;&c_y;&c_m;</trd>,
    <trd>&c_o;&c_b;&c_r;&c_je;&c_s;&c_t;&c_i;&#769;
    &c_p;&c_o;&c_p;&c_u;&c_l;&c_ja;&#769;&c_r;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="popula.0scienca">
  <kap><tld/>scienca</kap>
  <snc>
    <dif>
      (pri verko, prezenta&jcirc;o) Prezentanta sciencajn teoriojn kaj
      konceptojn en formo interesa kaj komprenebla al la ordinaruloj:
      <ekz>
        Elstare belstila, kaj por &ccirc;iuj legebla, <tld/>scienca verko
        estas &leftquot;Vivo de la plantoj&rightquot; de <nom>Paul Neergaard</nom>
        <fnt>
          <aut>Marjorie Boulton</aut>:
          <vrk>Ne nur leteroj de plumamikoj</vrk>. <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd
  lng="be">&c_n;&c_a;&c_v;&c_u;&c_k;&c_o;&c_v;&c_a;-&c_p;&c_a;&c_p;&c_u;&c_l;&c_ja;&c_r;&c_n;&c_y;</trd>
  <trd lng="ca">de <ind>divulgaci&oacute;</ind> cient&iacute;fica</trd>
  <trd lng="de">popul&auml;rwissenschaftlich</trd>
  <trd
  lng="ru">&c_n;&c_a;&c_u;&c_ch;&c_n;&c_o;-&c_p;&c_o;&c_p;&c_u;&c_l;&c_ja;&c_r;&c_n;&c_y;&c_j;</trd>
  <trd lng="zh">&#31185;&#26222; <pr>k&emacron;p&#468;</pr></trd>
</drv>

