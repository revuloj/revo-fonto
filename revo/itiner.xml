          <aut>T. Auderskaja</aut>,
          <vrk>Marino Curnis en Ukrainio</vrk>,
          <lok><url ref="http://www.esperanto.it/html/nsir/nsir488.htm">Nova
          Sento in Rete, 2006-07-21, numero 488a</url></lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">itin&eacute;raire<klr> (subst.)</klr></trd>
  <trd lng="it">itinerario<klr> (sost.)</klr></trd>
  <trd lng="pt">itiner&#225;rio</trd>
