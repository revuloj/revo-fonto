<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: skval.xml,v 1.4 2023/09/03 18:39:55 afido Exp $">
    <kap>
      <rad>skval</rad>/o
    </kap>

<drv mrk="skval.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">Genro</ref> 
      de <ref tip="super" cel="sxark.0o">&scirc;arko</ref>j 
      (<trd lng="la">Squalus</trd>), 
      plenkreskaj inter 50&#x202F;cm kaj 120&#x202F;cm longaj, 
      kun vive naskataj idoj; 
      sen anusa na&gcirc;ilo, pikilo anta&ubreve; amba&ubreve; dorsna&gcirc;iloj 
      kaj flankaj etenda&jcirc;oj &ccirc;e la vosto:
      <ekz>
        englutita de <tld/>o
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj sangame-malsataj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        nutri la <tld/>ojn per kadavroj
        <fnt>
          <bib>KPr</bib>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/9/97/Cuban_dogfish_nmfs.jpg/640px-Cuban_dogfish_nmfs.jpg" prm="PD">Kuba <tld/>o (Squalus cubensis)
        <fnt>
          <aut>SEFSC Pascagoula Laboratory; Collection of Brandi Noble, NOAA/NMFS/SEFSC, http://www.photolib.noaa.gov/htmls/fish4473.htm</aut>
          <lok><url ref="&WCW;/File:Cuban_dogfish_nmfs.jpg">&WCW;/File:Cuban_dogfish_nmfs.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Dornhai</trd>
    <trdgrp lng="en">
      <trd>dogfish shark</trd>,
      <trd>spurdog</trd>
    </trdgrp>
    <trd lng="fr">aiguillat</trd>
  </snc>
  <trd lng="ja">&#x30C4;&#x30CE;&#x30B6;&#x30E1;</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x430;&#x442;&#x440;&#x430;&#x43D;</trd>,
    <trd>&#x43A;&#x43E;&#x43B;&#x44E;&#x447;&#x430; &#x430;&#x43A;&#x443;&#x43B;&#x430;</trd>
  </trdgrp>
</drv>

  </art>


