<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ulcer.xml,v 1.30 2024/05/10 13:18:58 revo Exp $">
   <kap>
     <ofc>*</ofc>
     <rad>ulcer</rad>/o <fnt><bib>UV</bib></fnt>
   </kap>
   <drv mrk="ulcer.0o">
   <kap>
     <ofc>*</ofc>
     <tld/>o
   </kap>
  <snc mrk="ulcer.0o.MED">
  <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" cel="vund.0o" lst="voko:malsanoj">Vundo</ref> de
      histo, kiu ne povas cikatri&gcirc;i:
      <ekz>
        <tld/>o de la stomako;
      </ekz>
      <ekz>
        <tld/>o estas malfermita vundo, kiu pusas kaj ne povas rapide sani&gcirc;i
        <fnt>
          <bib>LR</bib>
          <lok>Unua Parto &ndash; Leksikologio</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Eternulo frapos vin per abscesoj <klr>[&#x2026;]</klr> kaj per sangaj <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 28:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        fine la kataro ofte ka&ubreve;zas <tld/>etojn de la korneo
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la bu&scirc;o de multaj HIV-infektitoj aperas multenombraj dolorigaj blankaj
        makuloj, inklinaj al <tld/>i&gcirc;o (kandidoza stomatito)
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2007/008991.php">La &ccirc;efaj murdistoj de HIV-infektitoj</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Winston</nom> <klr>[&#x2026;]</klr> havis varikan <tld/>on super sia
        dekstra maleolo
        <fnt>
          <aut>George Orwell, trad. Donald Broadribb</aut>,
          <vrk><url ref="http://i-espero.info/files/elibroj/eo%20-%20orwell,%20george%20-%201984.pdf">Mil
          Na&ubreve;cent Okdek Kvar</url></vrk>,
          <lok>p. 1</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
   <trd lng="be">&c_ja;&c_z;&c_v;&c_a;</trd>
   <trd lng="bg">&c_ja;&c_z;&c_v;&c_a;</trd>
   <trd lng="br">gouli<klr tip="ind"> (gwiadenno&ugrave; ar
   c'horf)</klr></trd>
   <trd lng="ca">&uacute;lcera</trd>
   <trdgrp lng="de">
     <trd><ofc>*</ofc>Geschw&uuml;r</trd>,
     <trd>Ulkus</trd>
   </trdgrp>
   <trd lng="el">&epsilon_ton;&lambda;&kappa;&omikron;&sigma_fina;</trd>
   <trd lng="en"><ofc>*</ofc>ulcer</trd>
   <trd lng="es">&uacute;lcera</trd>
   <trd lng="fi">haavauma</trd>
   <trd lng="fr"><ofc>*</ofc>ulc&egrave;re</trd>
   <trdgrp lng="he">
     <trd>&kaf;&yod;&bet;</trd>,
     <trd>&alef;&vav;&lamed;&qof;&vav;&samekh;</trd>
   </trdgrp>
   <trd lng="hu">fek&eacute;ly</trd>
   <trd lng="id">borok</trd>
   <trd lng="it">ulcera</trd>
   <trd lng="ja">&#x6F70;&#x760D;  <pr>&#x304B;&#x3044;&#x3088;&#x3046;</pr></trd>
   <trd lng="nl">zweer</trd>
   <trd lng="no">s&aring;r</trd>
   <trdgrp lng="pl">
     <trd><ofc>*</ofc>wrz&oacute;d</trd>,
     <trd><ofc>*</ofc>owrzodzenie</trd>
   </trdgrp>
   <trd lng="pt">&uacute;lcera</trd>
   <trd lng="ru"><ofc>*</ofc>&c_ja;&c_z;&c_v;&c_a;</trd>
   <trd lng="tr">&uuml;lser</trd>
  <trd lng="uk">&c_v;&c_i;&c_r;&c_a;&c_z;&c_k;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x6E83;&#x75A1; <pr>ku&#xEC;y&#xE1;ng</pr></trd>,
    <trd>&#x6F70;&#x760D; <pr>ku&#xEC;y&#xE1;ng</pr></trd>,
    <trd>&#x75A0; <pr>l&#xEC;</pr></trd>,
    <trd>&#x7658; <pr>l&#xEC;</pr></trd>,
    <trd>&#x75A1; <pr>y&#xE1;ng</pr></trd>,
    <trd>&#x760D; <pr>y&#xE1;ng</pr></trd>,
    <trd>&#x75AE; <pr>chu&#x101;ng</pr></trd>,
    <trd>&#x7621; <pr>chu&#x101;ng</pr></trd>
  </trdgrp>
</drv>
</art>
