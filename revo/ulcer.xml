<vortaro>
<art mrk="$Id$">
  <kap>
    <ofc>*</ofc>
    <rad>ulcer</rad>/o
  </kap>
<drv mrk="ulcer.0o">
  <kap>
    <ofc>*</ofc>
    <tld/>o
  </kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Vundo de histo, kiu ne povas cikatri&gcirc;i: 
      <ekz><tld/>o de la stomako.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_z;&c_v;&c_a;</trd>
  <trd lng="br">gouli<klr tip="ind"> (gwiadenno&ugrave; ar c'horf)</klr></trd>
  <trdgrp lng="de">
    <trd>Geschw&uuml;r</trd>,
    <trd>Ulkus [science]</trd>
  </trdgrp>
  <trd lng="el">&epsilon_ton;&lambda;&kappa;&omikron;&sigma_fina;</trd>
  <trd lng="es">&uacute;lcera</trd>
  <trd lng="en">ulcer</trd>
  <trd lng="fr">ulc&egrave;re</trd>
  <trdgrp lng="he">
    <trd>&kaf;&yod;&bet;</trd>,
    <trd>&alef;&vav;&lamed;&qof;&vav;&samekh;</trd>
  </trdgrp>
  <trd lng="hu">fek&eacute;ly</trd>
  <trd lng="nl">zweer</trd>
  <trd lng="pt">&uacute;lcera</trd>
  <trd lng="ru">&c_ja;&c_z;&c_v;&c_a;</trd>
</drv>
</art>
