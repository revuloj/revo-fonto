<art mrk="$Id: vesped.xml,v 1.14 2022/04/18 19:25:30 revo Exp $">
<kap>
  <rad>vesped</rad>/oj
</kap>
<drv mrk="vesped.0oj">
  <kap><tld/>oj</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="himeno.0oj">himenopteroj</ref>
      (<trd lng="la">Vespidae</trd>), kiuj faldas la flugilojn dum
      ripozo:
      <ekz>
        la <ref cel="krabr.0o">krabro</ref>
        (<nom>Vespa crabro</nom>) estas specio de <klr>[&#8230;]</klr> la familio de <tld lit="V"/>oj
        aspektanta kiel komuna <ref tip="prt" cel="vesp.0o.genro">vespo</ref>
        sed dufoje pli granda 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=535589">Krabro</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_o;&c_s;&c_y; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  <trd lng="ca">v&egrave;spids</trd>
  <trd lng="de">Faltenwespen</trd>
  <trd lng="fr">Vespid&eacute;s</trd>
  <trd lng="hu">t&aacute;rsasdar&aacute;zsf&eacute;l&eacute;k</trd>
  <trd lng="nl">plooivleugelwespen<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pt">vesp&iacute;deos</trd>
  <trdgrp lng="ru">
    <trd><ind>&c_o;&c_s;&c_y;</ind> &c_n;&c_a;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_i;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_ch;&c_a;&c_t;&c_o;-&c_k;&c_r;&c_y;&c_l;&c_y;&c_je; <ind>&c_o;&c_s;&c_y;</ind> <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43F;&c_r;&c_a;&c_v;&c_zh;&#x43D;&c_ib; &#x43E;&c_s;&c_i;</trd>,
    <trd>&c_s;&#x43A;&#x43B;&c_a;&c_d;&c_ch;&c_a;&c_s;&c_t;&#x43E;&#x43A;&c_r;&c_i;&#x43B;&c_ib;</trd>
  </trdgrp>
</drv>
</art>
