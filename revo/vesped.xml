<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>vesped</rad>/oj
</kap>
<drv mrk="vesped.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo
      <ref tip="super" cel="himeno.0oj">himenopteroj</ref>
      (<trd lng="la">Vespidae</trd>), kiuj faldas la flugilojn dum ripozo.
    </dif>
  </snc>
  <trd lng="de">Faltenwespen</trd>
  <trd lng="nl">plooivleugelwespen<klr tip="ind"> (fam.)</klr></trd>
  <trdgrp lng="ru">
    <trd><ind>&c_o;&c_s;&c_y;</ind> &c_n;&c_a;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_i;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_ch;&c_a;&c_t;&c_o;-&c_k;&c_r;&c_y;&c_l;&c_y;&c_je; <ind>&c_o;&c_s;&c_y;</ind> <klr
    tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
</drv>
</art>
