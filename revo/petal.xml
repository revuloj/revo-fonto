<art mrk="$Id: petal.xml,v 1.22 2024/02/17 06:43:07 revo Exp $">
<kap>
  <ofc>9</ofc><rad>petal</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="petal.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="petal.0o.korola">
    <dif>
      <ref tip="dif" cel="korol.0afolio">Korola folio</ref>:
      <ekz>
        defalis flor<tld/>'
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
          <lok>Optimismo</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili eniras la lernejan pordon sub ne&gcirc;blove falantaj <tld/>oj de
        sakura-floroj<fnt><aut>-</aut>: <vrk>Sakura-floroj</vrk>,
        <bib>Monato</bib><lok>jaro 2000a, numero 3a, p. 26a</lok></fnt>;
      </ekz>
      <ekz>
        lia plej &scirc;atata trinka&jcirc;o estis teo el pomarbaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2005/008463.php">Strangulo en la naturo</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;&scirc;i min amas&#x201D; kaj &leftquot;&scirc;i ne amas min&#x201D;, de&scirc;irante unu <tld/>on dum &ccirc;iu diro,
        la rozo havis multajn <tld/>ojn, kaj li sciis, ke la lasta estas tre grava, &ccirc;ar
        tiu determinos lian sorton
        <fnt>
          <bib>Monato</bib>
          <aut>Bunmi Osho-Davies</aut>
          <vrk><url ref="&Monato;/2006/008795.php">La velka rozo</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ja;&c_l;&c_jo;&c_s;&c_t;&c_a;&c_k;</trd>
  <trd lng="bo">&#xF63;&#xF7C;&#xF0B;&#xF60;&#xF51;&#xF56;&#xF0B;</trd>
  <trd lng="en">petal</trd>
  <trdgrp lng="fr">
    <trd>p&eacute;tale</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bl&uuml;tenblatt</trd>,
    <trd>Kronblatt</trd>
  </trdgrp>
  <trd lng="hu">szirom</trd>
  <trdgrp lng="ja">
    <trd>&#x82B1;&#x5F01;  <pr>&#x306F;&#x306A;&#x3073;&#x3089;</pr></trd>,
    <trd>&#x82B1;&#x3073;&#x3089;  <pr>&#x306F;&#x306A;&#x3073;&#x3089;</pr></trd>
  </trdgrp>
  <trd lng="nl">bloemblad</trd>
  <trd lng="pl">p&lstroke;atek korony</trd>
  <trd lng="pt">p&eacute;tala</trd>
  <trd lng="ro">coroana petal&abreve;</trd>
  <trd lng="ru">&c_l;&c_je;&c_p;&c_je;&c_s;&c_t;&c_o;&c_k;</trd>
  
  <trdgrp lng="uk">
    <trd>&#x43F;&#x435;&#x43B;&#x44E;&#x441;&#x442;&#x43A;&#x430;</trd>,
    <trd>&#x43F;&#x435;&#x43B;&#x44E;&#x441;&#x442;&#x43E;&#x43A;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x82B1;&#x74E3; <pr>hu&#x101;b&#xE0;n</pr></trd>,
    <trd>&#x74E3; <pr>b&#xE0;n</pr></trd>
  </trdgrp>
</drv>
</art>
