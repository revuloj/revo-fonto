<art mrk="$Id: lesot.xml,v 1.15 2024/01/15 16:14:09 revo Exp $">
<kap>
  <rad>Lesot</rad>/o
  <fnt><bib>RLN</bib></fnt>
</kap>

<drv mrk="lesot.L0o">
  <kap>
    <tld lit="L"/>o
  </kap>
  <snc mrk="lesot.L0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <mlg kod="ISO-3166">LS</mlg>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o.POL">&Scirc;tato</ref>
      en la suda <ref tip="malprt" cel="afrik.A0o">Afriko</ref>
      kun &ccirc;efurbo Maseruo; teritorio de Re&gcirc;lando <tld/>o,
      <ref tip="vid" cel="enklav.0o.POL">enklavo</ref> en
      <ref tip="vid" cel="afrik.SudA0o.GEOG">Sud-Afriko</ref>:
      <ekz>
        inter plena&gcirc;uloj estas HIV-infektitoj: 36&#x202F;% en Bocvano, 25&#x202F;% en Svazilando kaj
        Zimbabvo, 24&#x202F;% en Lesoto, 20&#x202F;% en Namibio kaj Zambio
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2004/008160.html">Afriko kaj aidoso</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    </snc>
    <trd lng="be">&c_L;&c_je;&c_s;&c_o;&c_t;&c_a;</trd>
    <trd lng="bg">&c_L;&c_je;&c_s;&c_o;&c_t;&c_o;</trd>
    <trd lng="de">Lesotho</trd>
    <trd lng="en">Lesotho</trd>
    <trdgrp lng="fr">
      <trd>Lesotho</trd>,
      <trd>Royaume du Lesotho</trd>
    </trdgrp>
    <trd lng="id">Lesotho</trd>
    <trd lng="ja">&#x30EC;&#x30BD;&#x30C8;</trd>
  <trd lng="nl">Lesotho</trd>
    <trdgrp lng="pl">
      <trd>Lesotho</trd>,
      <trd>Kr&oacute;lestwo <ind>Lesotho</ind></trd>
    </trdgrp>
    <trd lng="pt">Lesoto</trd>
  <trd lng="uk">&#x41B;&c_je;&c_s;&#x43E;&c_t;&#x43E;</trd>
  <trdgrp lng="zh">
    <trd>&#x83B1;&#x7D22;&#x6258; <pr>L&#xE1;isu&#x1D2;tu&#x14D;</pr></trd>,
    <trd>&#x840A;&#x7D22;&#x6258; <pr>L&#xE1;isu&#x1D2;tu&#x14D;</pr></trd>,
    <trd>&#x8CF4;&#x7D22;&#x6258; <pr>L&#xE0;isu&#x1D2;tu&#x14D;</pr></trd>,
    <trd>&#x8D56;&#x7D22;&#x6258; <pr>L&#xE0;isu&#x1D2;tu&#x14D;</pr></trd>,
    <trd>&#x83B1;&#x7D22;&#x6258; <pr>l&#xE1;isu&#x1D2;tu&#x14D;</pr></trd>,
    <trd>&#x840A;&#x7D22;&#x6258; <pr>l&#xE1;isu&#x1D2;tu&#x14D;</pr></trd>
  </trdgrp>
</drv>

</art>

