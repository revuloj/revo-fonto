<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>lesot</rad>/o
</kap>

<drv mrk="lesot.0o">
  <kap>
    <tld lit="L"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="lesot.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la suda <ref tip="malprt" cel="afrik.A0o">Afriko</ref>
      kun &ccirc;efurbo Maseruo.
      Teritorio de Re&gcirc;lando Lesotho.
      ISO-3166 kodo: LS.
      &Gcirc;i estas <ref tip="vid" cel="enklav.0o.POL">enklavo</ref> en
      <ref tip="vid" cel="afrik.sud0o.GEOG">Sud-Afriko</ref>.
    </dif>
    <trdgrp lng="pl">
      <trd>Lesotho</trd>,
      <trd>Kr&oacute;lestwo <ind>Lesotho</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

