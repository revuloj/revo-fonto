<kap>
<rad>geum</rad>/o
</kap>
<drv mrk="geum.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="rozac.0oj">rozacoj</ref>
      (<trd lng="la">Geum</trd>):
      <ekz>
        geumo<klr>[j]</klr> <klr>[&#x2026;]</klr> estas malaltaj sta&ubreve;doj kun
        rizomoj en la tersurfaco, kun ofte brilkoloraj, ru&gcirc;aj a&ubreve; flavaj
        floroj, kiuj havas kvinnombran korolon, la fruktoj estas plurakenoj,
        fruktoj unuope fini&gcirc;as per aristo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=38925">Geumo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="&WCU;/thumb/f/f2/Geum_japonicum_1.JPG/576px-Geum_japonicum_1.JPG" prm="CC BY-SA 3.0">japana <tld/>o (Geum japonicum), Fukushima, Japanujo
       <fnt>
         <aut>Qwert1234</aut>
         <lok><url ref="&WCW;/File:Geum_japonicum_1.JPG">&WCW;/File:Geum_japonicum_1.JPG</url></lok>
       </fnt>
  </bld>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_n;&c_ib;&c_k;&c_n;&c_ib;&c_c;&c_a;</trd>,
    <trd>&c_zh;&c_a;&c_b;&c_ib;&c_n;&c_y;&c_ja; &c_k;&c_u;&c_b;&c_a;&c_ch;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Nelkenwurz</trd>,
     <trd>Erdrose</trd>
  </trdgrp>
  <trd lng="fa">&a_ayn;&a_l;&a_f; &a_m;&a_b;&a_A;&a_r;&f_k;</trd>   
  <trd lng="hu">gy&ouml;mb&eacute;rgy&ouml;k&eacute;r</trd>
  <trd lng="ja">&#x30C0;&#x30A4;&#x30B3;&#x30F3;&#x30BD;&#x30A6;</trd>
  <trd lng="nl">nagelkruid</trd>
  <trd lng="pl">kuklik</trd>
  <trd lng="pt">erva-benta</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_v;&c_i;&c_l;&c_a;&c_t;</trd>
  <trd lng="sk">kukl&iacute;k</trd>
  <trd lng="cs">kukl&iacute;k</trd>
  <trd lng="uk">&#x491;&#x440;&#x430;&#x432;&#x456;&#x43B;&#x430;&#x442;</trd>
</drv>
