<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: elektrokut.xml,v 1.3 2025/07/14 20:51:00 afido Exp $">
<kap>
  <rad>elektrokut</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="elektrokut.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="elektrokut.0i.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Vunde a&ubreve; mortige trafi per elektromal&scirc;argo:
      <ekz>
        la esta&jcirc;o haltis kvaza&ubreve; &gcirc;i estus <tld/>ita
        <fnt>
          <aut>A. E. van Vogt, trad. Geo</aut>
          <vrk><url ref="https://eduinf.waw.pl/esp/lern/librejo/9995/0013.php">
          La Interplaneda Misio</url></vrk>
          <lok>2024</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="mort.elektro0igi.KOMUNE">elektromortigi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_r;&c_a;&c_c;&c_mol; <klr>(&c_s;&c_mol;&c_m;&c_je;&c_r;&c_c;&c_ju;)</klr> &c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_y;&c_m; &c_t;&c_o;&c_k;&c_a;&c_m;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_a;&c_zh;&c_a;&c_c;&c_mol; &c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_y;&c_m; &c_t;&c_o;&c_k;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>durch Stromschlag <ind>t&ouml;ten</ind></trd>,
    <trd><klr>(jmd.)</klr> eine <ind>Stromschlag</ind> verpassen</trd>
  </trdgrp>
  <trd lng="en">electrocute</trd>
  <trd lng="fr">&eacute;lectrocuter</trd>
</drv>
</art>

