<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>rib</rad>/o
</kap>
<drv mrk="rib.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio
      <ref tip="super" cel="saksifrag.0acoj">saksifragacoj</ref>
      (<trd lng="la">Ribes</trd>), arbedo kun malgrandaj bongustaj acidetaj
      beroj.
    </dif>
    <ref tip="sub" cel="gros.0o">grosujo</ref>
  </snc>
  <bld lok="../bld/rib.jpg">
    beroj de ru&gcirc;a <tld/>o (<trd lng="la">Ribes rubrum</trd>)
  </bld>
  <trdgrp lng="de">
    <trd>Johannisbeerstrauch <klr>(ru&gcirc;a, nigra ribo)</klr></trd>,
    <trd>Stachelbeerstrauch <klr>(grosujo)</klr></trd>
  </trdgrp>
  <trd lng="fr">cassis</trd>
</drv>
</art>
