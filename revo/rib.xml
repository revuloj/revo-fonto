<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rib</rad>/o
</kap>
<drv mrk="rib.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio
      <ref tip="super" cel="saksifrag.0oj">saksifragacoj</ref>
      (<trd lng="la">Ribes</trd>), arbedo kun malgrandaj bongustaj
      acidetaj
      beroj.
    </dif>
    <ref tip="sub" cel="gros.0o">grosujo</ref>
  </snc>
  <bld lok="../bld/rib.jpg">
    beroj de <ind>ru&gcirc;a <tld/>o</ind>
    <trd lng="be">&c_ch;&c_y;&c_r;&c_v;&c_o;&c_n;&c_y;&c_ja;
    <ind>&c_p;&c_a;&c_r;&c_e;&c_ch;&c_k;&c_ib;</ind></trd>
    <trd lng="hu">piros <ind>ribiszke</ind></trd>
    <trd lng="la">Ribes rubrum</trd>
    <trd lng="nl">aalbes</trd>
    <trd lng="pl">czerwona <ind>porzeczka</ind></trd>
  </bld>
  <trd lng="be">&c_p;&c_a;&c_r;&c_e;&c_ch;&c_k;&c_ib;</trd>
  <trdgrp lng="de">
    <trd>Johannisbeerstrauch <klr>(ru&gcirc;a, nigra ribo)</klr></trd>,
    <trd>Stachelbeerstrauch <klr>(grosujo)</klr></trd>
  </trdgrp>
  <trd lng="en">currant</trd>
  <trd lng="fr">cassis</trd>
  <trdgrp lng="hu">
    <trd>ribiszke</trd>,
    <trd>ribizli</trd>
  </trdgrp>
  <trd lng="nl">bes</trd>
  <trd lng="pl">porzeczka</trd>
  <trd lng="pt">groselha</trd>
  <trd lng="ru">&c_s;&c_m;&c_o;&c_r;&c_o;&c_d;&c_i;&c_n;&c_a;</trd>
  <trd lng="sv">vinb&auml;r</trd>
</drv>
</art>
