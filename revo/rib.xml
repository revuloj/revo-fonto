<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rib</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="rib.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="rib.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Frukto de <tld/>ujo.
    </dif>
  </snc>
  <snc mrk="rib.0o.BOT">
    <ref tip="dif" cel="rib.0ujo.BOT">ribujo</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_e;&c_ch;&c_k;&c_ib;</trd>
  <trd lng="bg">k&c_a;&c_s;&c_i;&c_s;</trd>
  <trdgrp lng="ca">
    <trd>riber</trd>,
    <trd>cirerola</trd>
  </trdgrp>
  <trd lng="de">Johannisbeerstrauch <klr>(ru&gcirc;a, nigra ribo)</klr></trd>
  <trd lng="en">currant</trd>
  <trdgrp lng="hu">
    <trd>ribiszke</trd>,
    <trd>ribizli</trd>
  </trdgrp>
  <trd lng="nl">bes</trd>
  <trd lng="pl">porzeczka</trd>
  <trd lng="pt">groselha</trd>
  <trd lng="ru">&c_s;&c_m;&c_o;&c_r;&c_o;&c_d;&c_i;&c_n;&c_a;</trd>
  <trd lng="sv">vinb&auml;r</trd>
</drv>

<drv mrk="rib.nigra0o">
  <kap>nigra <tld/>o</kap>
  <snc mrk="rib.nigra0o.KUI">
    <dif>
      Nigra riba bero.
    </dif>
  </snc>
  <snc mrk="rib.nigra0o.BOT">
    <dif>
      Riba speco donanta nigrajn berojn
      (el <trd lng="la">Ribes nigrum</trd>).
    </dif>
  </snc>
  <trd lng="fr">cassis</trd>
</drv>

<drv mrk="rib.rugxa0o">
  <kap>ru&gcirc;a <tld/>o</kap>
  <snc mrk="rib.rugxa0o.KUI">
    <dif>
      Ru&gcirc;a riba bero.
    </dif>
  </snc>
  <snc mrk="rib.rugxa0o.BOT">
    <dif>
      Riba speco donanta ru&gcirc;ajn berojn
      (el <trd lng="la">Ribes rubrum</trd>).
    </dif>
  </snc>
  <bld lok="../bld/rib.jpg">
    beroj de ru&gcirc;a <tld/>o
  </bld>
  <trd lng="be">&c_ch;&c_y;&c_r;&c_v;&c_o;&c_n;&c_y;&c_ja; <ind>&c_p;&c_a;&c_r;&c_e;&c_ch;&c_k;&c_ib;</ind></trd>
  <trd lng="ca"><ind>riber</ind> vermell</trd>
  <trd lng="fr">groseille<klr> (rouge)</klr></trd>
  <trd lng="hu">piros <ind>ribiszke</ind></trd>
  <trd lng="nl">aalbes</trd>
  <trd lng="pl">czerwona <ind>porzeczka</ind></trd>
</drv>

<drv mrk="rib.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="rib.0ujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbedo kun malgrandaj bongustaj acidetaj beroj;
      genro el familio
      <ref tip="super" cel="saksifrag.0oj">saksifragacoj</ref>
      (<trd lng="la">Ribes</trd>):
      <ekz>
         <tld/>ujo grefteblas, kvankam malfacile, sur la grosujo
         <fnt>
           <bib>OriginoDeSpecioj</bib>,
           <lok>&ccirc;apitro 8a</lok>
         </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="gros.0ujo.BOT">grosujo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>cassissier</trd>,
    <trd>groseiller</trd>
  </trdgrp>
</drv>
</art>
