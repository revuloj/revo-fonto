    <trd>alopecia</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>calvicie</trd>,
    <trd>alopecia</trd>
  </trdgrp>
  <trd lng="fr">alop&eacute;cie</trd>
  <trdgrp lng="nl">
    <trd>kaalheid</trd>,
    <trd>kaalhoofdigheid</trd>
  </trdgrp>
  <trd lng="pt">alopecia</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_l;&c_y;&c_s;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_l;&c_je;&c_sh;&c_i;&c_v;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_a;&c_l;&c_o;&c_p;&c_je;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">ple&scaron;ivos&tcaron;</trd>
</drv>
