<art mrk="$Id: alopec.xml,v 1.18 2021/02/28 15:34:27 revo Exp $">
<kap>
  <rad>alopeci</rad>/o
</kap>
<drv mrk="alopec.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Tuta a&ubreve; parta senhareco:
      <ekz>
        plej grava por iuj popoloj estis la velur-prozopa uzado 
        kontra&ubreve; <tld/>o, tiu flegado, nur &ccirc;e viroj, konsistis el
        apliko al kapo de la nigra suko kiu fontas el la vundoj de la arbo 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=346784">Velura prozopo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kalv.0a">kalva</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_l;&c_a;&c_p;&c_e;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_a;&c_b;&c_l;&c_y;&c_s;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_l;&c_ja;&c_sh;&c_y;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>calv&iacute;cie</trd>,
    <trd>calbesa</trd>,
    <trd>alop&egrave;cia</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>alopecie</trd>,
    <trd>holohlavost</trd>,
    <trd>ple&scaron;atost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kahlheit</trd>,
    <trd>Kahlk&ouml;pfigkeit</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>baldness</trd>,
    <trd>alopecia</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>calvicie</trd>,
    <trd>alopecia</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_l;&a_w;&f_p;&a_s;&f_y;</trd>,
    <trd>&a_T;&a_A;&a_s;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">alop&eacute;cie</trd>
  <trd lng="he">&#x5D4;&#x5EA;&#x5E7;&#x5E8;&#x5D7;&#x5D5;&#x5EA;</trd>
  <trdgrp lng="nl">
    <trd>kaalheid</trd>,
    <trd>kaalhoofdigheid</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>alopecja</trd>,
    <trd>&lstroke;ysienie</trd>
  </trdgrp>
  <trd lng="pt">alopecia</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_l;&c_y;&c_s;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_l;&c_je;&c_sh;&c_i;&c_v;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_a;&c_l;&c_o;&c_p;&c_je;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ple&scaron;ivos&tcaron;</trd>,
    <trd>vypad&aacute;vanie vlasov</trd>
  </trdgrp>
</drv>
</art>
