<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>dekant</rad>/i
</kap>
<drv mrk="dekant.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">KEM</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      For<ref tip="sin" cel="versx.0i">ver&scirc;i</ref>
      likva&jcirc;on el ujo, lasante en &gcirc;i la funde
      falintajn nesolva&jcirc;ojn:
      <ekz>
        <tld/>ita kreto servas por purigi dentojn kaj ar&gcirc;enton
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=205307"
              >Kreto (&scirc;tono)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        ekskrementa cisterno <klr>[&#8230;]</klr> celas <tld/>i la solidajn materiojn kaj
        likvigi ilin per fermentado danke al la ago de malaerobiaj bakterioj nature
        vivantaj en la elflua&jcirc;oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=297786"
              >Ekskrementa cisterno</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="pur.ripoz0igi">ripozpurigi</ref>
  </snc>
  <trd
  lng="be">&c_d;&c_e;&c_k;&c_a;&c_n;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">decantar</trd>
  <trd lng="cs">sl&iacute;t <klr>(tekutinu a ponechat usazeninu)</klr></trd>
  <trdgrp lng="de">
     <trd>dekantieren</trd>,
     <trd>abgie&szlig;en</trd>
  </trdgrp>
  <trd lng="es">decantar</trd>
  <trd lng="fr">d&eacute;canter</trd>
  <trdgrp lng="hu">
     <trd>lesz&udblac;r</trd>,
     <trd>le&ouml;nt<klr tip="ind"> (folyad&eacute;kot)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zlewa&cacute;<klr> (p&lstroke;yn)</klr></trd>,
    <trd>odlewa&cacute;<klr> (p&lstroke;yn)</klr></trd>,
    <trd>dekantowa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">decantar</trd>
  <trd
  lng="ru">&c_d;&c_je;&c_k;&c_a;&c_n;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">zlia&tcaron; tekutinu a necha&tcaron; usadeninu</trd>
</drv>
</art>
