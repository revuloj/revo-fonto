<vortaro>
<art mrk="$Id$">
<kap>
<rad>dekant</rad>/i
</kap>
<drv mrk="dekant.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">KEM</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Forver&scirc;i likva&jcirc;on el ujo, lasante en &gcirc;i la funde falintajn nesolva&jcirc;ojn.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_k;&c_a;&c_n;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
     <trd>dekantieren</trd>,
     <trd>abgie&szlig;en</trd>
  </trdgrp>
  <trd lng="es">decantar</trd>
  <trd lng="fr">d&eacute;canter</trd>
  <trdgrp lng="hu">
     <trd>lesz&udblac;r</trd>,
     <trd>le&ouml;nt<klr tip="ind"> (folyad&eacute;kot)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_je;&c_k;&c_a;&c_n;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
