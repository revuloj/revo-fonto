<drv mrk="fend.0igxi">
  <kap>
    <tld/>i&gcirc;i
  </kap>
  <snc>
    <dif>
      Ekhavi <tld/>ojn:
      <ekz>
        la lipoj al mi sange <tld/>i&gcirc;is
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun la tempo <klr>[la]</klr> konstrua&jcirc;o komencis <tld/>i&gcirc;i
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la domo <tld/>i&gcirc;is el supro malsupren
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Deka</lok>
        </fnt>;
      </ekz>
      <ekz>
        estis varmege <klr>[&#x2026;]</klr>, la ha&ubreve;to de la manoj kaj viza&gcirc;o <tld/>i&gcirc;is
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        lumo en mil splitetojn <tld/>i&gcirc;as
        <fnt>
          <bib>SrT</bib>
          <lok>Libro VIII.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[mi]</klr> ploregis, ke mia koro preska&ubreve; <tld/>i&gcirc;is
        <fnt>
          <bib>VaK</bib>
          <lok>Sesa &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="krev.0i.fendigxi">krevi<sncref/></ref>
  </snc>
  <trd lng="de">aufrei&szlig;en</trd>
  <trdgrp lng="ja">
    <trd>&#x5272;&#x308C;&#x308B;  <pr>&#x308F;&#x308C;&#x308B;</pr></trd>,
    <trd>&#x88C2;&#x3051;&#x308B;  <pr>&#x3055;&#x3051;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="zh">&#x5206;&#x88C2; <pr>f&#x113;nli&#xE8;</pr></trd>
</drv>









<drv mrk="fend.0ilo">
