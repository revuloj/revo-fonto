  </trdgrp>
</drv>

<drv mrk="fend.0o">
  <kap><tld/>o</kap>
  <snc mrk="fend.0o.KOMUNE">
    <dif>
      Mallar&gcirc;a la&ubreve;longa truo:
      <ekz>
        li trafis ne tute fermitan pordon, kie lumo penetris tra <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        lerte kuradis kelke da lacertoj en la <tld/>oj de maljuna arbo
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        horizontalaj <tld/>oj en la pordoj ebligas en&jcirc;eti po&scirc;ta&jcirc;ojn
        <fnt>
          <bib>KrB</bib>
          <lok>Ankora&ubreve; neniu pla&ccirc;is al &ccirc;iu</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malbenita kojno elglitis kaj la <tld/>o tiel rapide fermi&gcirc;is, ke mi ne povis
        plu eltiri mian <klr>[&#x2026;]</klr> barbon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino kaj Rozulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        rompi&gcirc;is tabulo en la &scirc;ipo, la akvo penetris internen, oni penis &scirc;topi la
        <tld/>on, &ccirc;iuj pumpiloj komencis labori
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="brecx.0o">bre&ccirc;o</ref>,
      <ref cel="krev.0o">krevo</ref>
    </refgrp>
    <trdgrp lng="en">
      <trd>slot</trd>,
      <trd>cleft</trd>,
      <trd>crack</trd>,
      <trd>crevice</trd>,
      <trd>fissure</trd>,
      <trd>gap</trd>,
      <trd>split</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>szpara</trd>,
      <trd>szczelina</trd>,
      <trd>rysa</trd>
    </trdgrp>
  </snc>
  <snc mrk="fend.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Aparteco, malsameco:
      <ekz>
	estas profunda <tld/>o inter amba&ubreve;
	sistemoj;
      </ekz>
      <ekz>
        <klr>[mi]</klr> havas pli ol supozeton pri la <tld/>o ekzistanta en la pastraro,
        pri la reformemaj tendencoj de unu partio, kaj la reakciaj inklinoj de la alia
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        arto provas nun fliki la kreskantan <tld/>on inter la bezono kaj oferto per
        abrupta malalti&gcirc;o de la kvalita nivelo de la verkoj
        <fnt>
          <bib>KFl</bib>
          <lok>I. Enkonduko</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni perceptas la sintenon de <nom>Lapenna</nom> kontra&ubreve; la tiama UEA: estis <tld/>o,
        estigita de la estraraj kaj komitataj elektoj
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2007/009192.php">Konata vo&ccirc;o ree sonas</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="abism.0o">abismo</ref>,
      <ref cel="difere.0o">diferenco</ref>,
      <ref cel="skism.0o">skismo</ref>
    </refgrp>
    <trd lng="de">Spaltung</trd>
    <trdgrp lng="en">
      <trd>gap</trd>,
      <trd>split</trd>
    </trdgrp>
    <trd lng="pl">rysa</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_ch;&c_y;&c_l;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_l;&c_ib;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&t_sa;&t_drengbu;&t_ra;&t_tsheg;&t_kha;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>prasklina</trd>,
    <trd>roz&scaron;t&ecaron;pen&iacute;</trd>,
    <trd>sp&aacute;ra</trd>,
    <trd>&scaron;kv&iacute;ra</trd>,
    <trd>&scaron;t&ecaron;rbina</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Spalt</trd>,
    <trd>Riss</trd>
  </trdgrp>
  <trd lng="en">split</trd>
  <trdgrp lng="es">
