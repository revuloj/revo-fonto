<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fend.xml,v 1.34 2023/02/17 21:23:43 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fend</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="fend.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fend.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="divid.0i.partigi">Dividi</ref> malmolan materion 
      la&ubreve; &gcirc;ia longeco:
      <ekz>
        li <tld/>is lignon por brulofero
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 22:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni <tld/>is la &scirc;tonegojn per kojnoj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi volis <tld/>i la arbon por havi fragmentojn por la kuirejo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino kaj Rozulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi iam <tld/>is la kapon al tiu bohema rajdisto, en la momento, kiam li levis
        sian sabron
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        sago <tld/>as al li la hepaton
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 7:23</lok>
        </fnt>;
      </ekz>
      <ekz>
	la &scirc;tata konstrua&jcirc;o komencis <tld/>i&gcirc;i
	<fnt><bib>Far1</bib>, <lok>Anta&ubreve;parolo</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        via indigno diktus al vi tro akrajn vortojn, kiuj <tld/>us 
        <klr>(dolorigus)</klr> al li la koron
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la nuna tempo estas oportuna por kojne <tld/>i la rilatojn inter <klr>[&#8230;]</klr>
        la heredaj pastroj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu etulo ru&gcirc;a, malpura, kun pirforma kapo, kun premegita nazo, kun bu&scirc;o
        <tld/>ita &gcirc;is la oreloj, &scirc;ajnis al &scirc;i monstro
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Deka</lok>
        </fnt>;
      </ekz>
      <ekz>
        porkon, &ccirc;ar &gcirc;i havas dis<tld/>itajn hufojn kaj distran&ccirc;an sulkon sur la hufoj,
        sed &gcirc;i ne rema&ccirc;as ma&ccirc;ita&jcirc;on, malpura &gcirc;i estas por vi
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 11:7</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frakas.0i">frakasi</ref>,
      <ref cel="hak.0i">haki</ref>,
      <ref cel="krev.0igi">krevigi</ref>,
      <ref cel="romp.0i">rompi</ref>,
      <ref cel="seg.0i">segi</ref>,
      <ref cel="split">spliti</ref>,
      <ref cel="sxir.0i">&scirc;iri</ref>,
      <ref cel="trancx.0i">tran&ccirc;i</ref>
    </refgrp>
    <trd lng="de">spalten</trd>
    <trdgrp lng="pl">
      <trd>&lstroke;upa&cacute;</trd>,
      <trd>roz&lstroke;upywa&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="fend.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Dividi la partojn de amaso, &gcirc;in rapide trairante:
      <ekz>
        la &scirc;ipo <tld/>is la ondojn per sia &scirc;ipnazo
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekdua</lok>
        </fnt>;
      </ekz>
      <ekz>
        nigran nubon memfide <tld/>as hirundeto
        <fnt>
          <bib>SrT</bib>
          <lok>Libro X.</lok>
        </fnt>;
      </ekz>
      <ekz>
        tira, angora &gcirc;emo <tld/>as la nokton
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>;
      </ekz>
      <ekz>
        la jubilo kaj delicaj krioj de la Kooperativestro <tld/>is la transfenestran
        vespersilenton de la &gcirc;ardeno
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 4</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dis.0igi">disigi</ref>,
      <ref cel="kur.tra0i">trakuri</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>zerschneiden</trd>,
      <trd>teilen</trd>
    </trdgrp>
    <trd lng="pl">rozcina&cacute;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_ch;&c_a;&c_p;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_l;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>poltit</trd>,
    <trd>rozr&aacute;&zcaron;et</trd>,
    <trd>&scaron;t&iacute;pat <klr>(d&rcaron;evo)</klr></trd>,
    <trd>&scaron;t&ecaron;pit</trd>
  </trdgrp>
  <trd lng="en">split</trd>
  <trdgrp lng="es">
    <trd>hendir</trd>,
    <trd>cortar</trd>,
    <trd>rajar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fendre</trd>,
    <trd>cliver</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>has&iacute;t</trd>,
    <trd>repeszt</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fender</trd>,
    <trd>rachar</trd>,
    <trd>partir</trd>,
    <trd>lascar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_shch;&c_je;&c_p;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_l;&c_o;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>roz&scaron;tiepi&tcaron;</trd>,
    <trd>&scaron;tiepa&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_z;&c_k;&c_o;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_shch;&c_je;&c_p;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="fend.0o">
  <kap><tld/>o</kap>
  <snc mrk="fend.0o.KOMUNE">
    <dif>
      Mallar&gcirc;a la&ubreve;longa truo:
      <ekz>
	<tld/>o en muro, en vazo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="brecx.0o">bre&ccirc;o</ref>
    </refgrp>
    <trdgrp lng="en">
      <trd>slot</trd>,
      <trd>cleft</trd>,
      <trd>crack</trd>,
      <trd>crevice</trd>,
      <trd>fissure</trd>,
      <trd>gap</trd>,
      <trd>split</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>szpara</trd>,
      <trd>szczelina</trd>,
      <trd>rysa</trd>
    </trdgrp>
  </snc>
  <snc mrk="fend.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Aparteco, malsameco:
      <ekz>
	estas profunda <tld/>o inter amba&ubreve;
	sistemoj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="abism.0o">abismo</ref>,
      <ref cel="difere.0o">diferenco</ref>,
      <ref cel="skism.0o">skismo</ref>
    </refgrp>
    <trdgrp lng="en">
      <trd>gap</trd>,
      <trd>split</trd>
    </trdgrp>
    <trd lng="pl">rysa</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_ch;&c_y;&c_l;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_l;&c_ib;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&t_sa;&t_drengbu;&t_ra;&t_tsheg;&t_kha;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>prasklina</trd>,
    <trd>roz&scaron;t&ecaron;pen&iacute;</trd>,
    <trd>sp&aacute;ra</trd>,
    <trd>&scaron;kv&iacute;ra</trd>,
    <trd>&scaron;t&ecaron;rbina</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Spalt</trd>,
    <trd>Riss</trd>
  </trdgrp>
  <trd lng="en">split</trd>
  <trdgrp lng="es">
    <trd>hendidura</trd>,
    <trd>hendedura</trd>,  
    <trd>cortadura</trd>,
    <trd>rajadura</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fente</trd>,
    <trd>clivage<klr tip="ind"> (s&eacute;paration)</klr></trd>,
    <trd>fracture<klr tip="ind"> (s&eacute;paration)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>r&eacute;s</trd>,
    <trd>hasad&eacute;k</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_shch;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_shch;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>diera</trd>,
    <trd>puklina</trd>,
    <trd>&scaron;trbina</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_shch;&c_ib;&c_l;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_sh;&c_p;&c_a;&c_r;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="fend.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="fend.0ego.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Granda, eventuale dan&gcirc;era <tld/>o,
      ekzemple en roko;
      <ref tip="vid" cel="fauxk.0o.granda">fa&ubreve;ko<sncref/></ref>.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_k;&c_o;&c_l;&c_ib;&c_n;&c_a;</trd>
  <trd lng="cs">rozsedlina</trd>
  <trdgrp lng="de">
    <trd>Spalte</trd>,
    <trd>Schlucht</trd>,
    <trd>Kluft</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>chasm</trd>,
    <trd>divide</trd>,
    <trd>gulf</trd>,
    <trd>ravine</trd>
  </trdgrp>
  <trd lng="fr">faille</trd>
  <trdgrp lng="hu">
    <trd>szakad&eacute;k</trd>,
    <trd>sziklahasad&eacute;k</trd>,
    <trd>szurdok</trd>
  </trdgrp>
  <trd lng="pl">rozpadlina</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_s;&c_je;&c_l;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">prasklina</trd>
  <trd lng="uk">&c_u;&c_shch;&c_je;&c_l;&c_i;&c_n;&c_a;</trd>
</drv>

<drv mrk="fend.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="fend.0eto.KOMUNE">
    <dif>
      Ne profunda <tld/>o, ne trapenetranta la objekton:
      <ekz>
	<tld/>eto en dento<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_ch;&c_y;&c_l;&c_ib;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_l;&c_ib;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>prasklina</trd>,
    <trd>skulina</trd>,
    <trd>&scaron;t&ecaron;rbina</trd>
  </trdgrp>
  <trd lng="es">fisura</trd>
  <trd lng="fr">fissure</trd>
  <trd lng="hu">reped&eacute;s</trd>
  <trd lng="pl">rysa</trd>
  <trd lng="sk">&scaron;trbinka</trd>
  <trdgrp lng="uk">
    <trd>&c_shch;&c_ib;&c_l;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_sh;&c_p;&c_a;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_ib;&c_k;&c_o;&c_n;&c_je;&c_ch;&c_k;&c_o;</trd>,
    <trd>&c_v;&c_ib;&c_ch;&c_k;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="fend.har0i">
  <kap>har<tld/>i</kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc mrk="fend.har0i.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      <ref tip="vid" cel="pedant.0i.KOMUNE">Pedanti</ref>, tro analizi,
      paroli a&ubreve; disputi pri bagatelaj detaloj:
      <ekz>
	&ccirc;ar ni nun <klr>...</klr> komencis har<tld/>i pri gramatiko, mi
	faros unu demandon
	<fnt>
	<aut>A. Iwi</aut>,
	<vrk>Re: Riisma finvenko!</vrk>,
	<lok>soc.culture.esperanto, 1997-06-19</lok>
	</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="har.0o">haro</ref>
    </refgrp>
  </snc>
  <trd lng="de">Haarspalterei betreiben</trd>
  <trd lng="en">split hairs</trd>
  <trdgrp lng="es">
    <trd>pedantear</trd>,
    <trd>cominear</trd>,
    <trd>hablar babosadas</trd>
  </trdgrp>
  <trd lng="fr">couper les cheveux en quatre</trd>
  <trd lng="pl">dzieli&cacute; <ind>w&lstroke;os</ind> <klr>na czworo</klr></trd>
</drv>


<drv mrk="fend.mont0o">
  <kap>mont<tld/>o</kap>
  <snc mrk="fend.mont0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="sin" cel="krut.inter0ejo">Interkrutejo</ref>:
      <ekz>
         la levi&gcirc;anta suno trovis nin torde rampantaj la&ubreve;
         interkruta&jcirc;oj kaj mont<tld/>oj &ccirc;iam pli kaj pli supren
        <fnt>
          <aut>S. Bergman, trad. B. Gerdman</aut>
          <vrk>Tra sova&gcirc;a Kam&ccirc;atko</vrk>
          <lok>1932</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu mont<tld/>o havis sian tonon, kaj &ccirc;iu monta deklivo
        propran sonon
        <fnt>
          <aut>Selma Lagerl&ouml;f</aut>
          <vrk>Uzino 2</vrk>
          <lok>La senpaculoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_ja;&c_s;&c_mol;&c_n;&c_ib;&c_n;&c_a;</trd>
  <trd lng="cs">horsk&aacute; str&zcaron;</trd>
  <trd lng="fr">crevasse</trd>
  <trd lng="nl">bergkloof</trd>
  <trd lng="ru">&c_u;&c_shch;&c_je;&c_l;&c_mol;&c_je;</trd>
  <trd lng="sk">horsk&aacute; str&zcaron;</trd>
</drv>


<drv mrk="fend.pantalon0o">
  <kap>pantalon<tld/>o</kap>
  <snc mrk="fend.pantalon0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Fermebla kaj malfermebla <tld/>o
      &ccirc;e la anta&ubreve;a supra parto de
      <ref tip="malprt" cel="pantal.0o.TEKS">pantalono</ref>,
      kiu faciligas &gcirc;ian surmeton kaj demeton:
      <ekz>
        klopodante por malfermi la pantalon<tld/>on
        <fnt>
          <aut>J. Teles</aut>,
          <vrk><url ref="http://www.jorgeteles.com.br/site/index.php">
          la tago sen nomo, 17</url></vrk>,
          <lok>2013-06-09</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_a;&c_r;&c_e;&c_h;</trd>,
      <trd>&c_k;&c_r;&c_e;&c_s;&c_l;&c_a; <klr>(&c_u; &c_n;&c_a;&c_g;&c_a;&c_v;&c_ib;&c_c;&c_a;&c_h;)</klr></trd>
    </trdgrp>
  <trdgrp lng="cs">
    <trd>poklopec</trd>,
    <trd>rozparek</trd>
  </trdgrp>
  <trd lng="en">fly</trd>
  <trd lng="fr">braguette</trd>
  <trd lng="pl">rozporek</trd>
  <trdgrp lng="sk">
    <trd>jatka</trd>,
    <trd>r&aacute;zporok</trd>
  </trdgrp>
  </snc>
  <trd lng="uk">&c_sh;&c_i;&c_r;&c_ib;&c_n;&c_mol;&c_k;&c_a;</trd>
</drv>
</art>
