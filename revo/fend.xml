    <trd>hasad&eacute;k</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5272;&#x308C;&#x76EE;  <pr>&#x308F;&#x308C;&#x3081;</pr></trd>,
    <trd>&#x88C2;&#x3051;&#x76EE;  <pr>&#x3055;&#x3051;&#x3081;</pr></trd>,
    <trd>&#x307F;&#x305E;</trd>,
    <trd>&#x3072;&#x3073;</trd>,
    <trd>&#x30B9;&#x30EA;&#x30C3;&#x30C8;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_shch;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_shch;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>diera</trd>,
    <trd>puklina</trd>,
    <trd>&scaron;trbina</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_shch;&c_ib;&c_l;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_sh;&c_p;&c_a;&c_r;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x586B;&#x6D1E; <pr>ti&#xE1;nd&#xF2;ng</pr></trd>,
    <trd>&#x586B;&#x8865;&#x88C2;&#x7EB9; <pr>ti&#xE1;nb&#x1D4;li&#xE8;w&#xE9;n</pr></trd>,
    <trd>&#x53BB;&#x6389;&#x7C97;&#x7CD9; <pr>q&#xF9;di&#xE0;oc&#x16B;c&#x101;o</pr></trd>,
    <trd>&#x4F7F;&#x5149;&#x6ED1; <pr>sh&#x1D0;gu&#x101;nghua</pr></trd>,
    <trd>&#x586B;&#x5751; <pr>ti&#xE1;nk&#x113;ng</pr></trd>,
    <trd>&#x88C2;&#x9699; <pr>li&#xE8;x&#xEC;</pr></trd>,
    <trd>&#x78E8;&#x5E73; <pr>m&#xF3;p&#xED;ng</pr></trd>,
    <trd>&#x88C2;&#x75D5; <pr>li&#xE8;h&#xE9;n</pr></trd>,
    <trd>&#x9699; <pr>x&#xEC;</pr></trd>,
    <trd>&#x88C2; <pr>li&#xE8;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fend.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="fend.0ego.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Granda, eventuale dan&gcirc;era <tld/>o, ekzemple en roko;
      <ref tip="vid" cel="fauxk.0o.granda">fa&ubreve;ko<sncref/></ref>:
      <ekz>
        la Izraelidoj faris al si la <tld/>egojn en la montoj
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 6:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ekmigris <klr>[&#x2026;]</klr> tra <tld/>egoj de rokoj kaj la&ubreve; vojoj, kiuj pendis &ccirc;e
        la &scirc;tona murego kvaza&ubreve; nestoj de hirundoj
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi falis en grandan <tld/>egon, kiu trairas la tutan glaciejon
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        malgra&ubreve; &scirc;ajna <tld/>ego, la diferenco inter ni, prudentaj instruituloj, estas
        surprize malgranda
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="abism.0o.profundo">abismo<sncref/></ref>,
      <ref cel="krut.inter0ejo">interkrutejo</ref>,
      <ref cel="ravin.0o">ravino</ref>
