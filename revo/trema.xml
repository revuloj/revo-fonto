<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>trema</rad>/o
    </kap>

    <drv mrk="trema.0o">
      <kap><tld/>o</kap>
      <snc>
        <uzo tip="fak">TIP</uzo>
	<dif>
          <ref tip="super" cel="diakrit.0ilo.LIN">Diakritilo</ref>
          supersigno en formo de horizontala dupunkto, kiel tiu super
          <em>a, e, i, o</em> en <em>&auml;, &euml;, &iuml;, &ouml;</em>, uzata i.a. por
          signi <ref cel="umlaut.0o.FON">umlauton</ref> kaj
          <ref cel="dierez.0o.FON">dierezon</ref>.
    	</dif>
     </snc>
     <trdgrp lng="de">
     	 <trd>Trema</trd>,
     	 <trd>Trennungspunkte</trd>,
     	 <trd>Umlautpunkte</trd>
      </trdgrp>
      <trd lng="en">diaeresis</trd>
      <trdgrp lng="nl">
     	 <trd>deelteken</trd>,
     	 <trd>trema</trd>
      </trdgrp>
      <trdgrp lng="ru">
     	 <trd>&c_t;&c_r;&c_je;&c_m;&c_a;</trd>,
     	 <trd>&c_d;&c_i;&c_je;&c_r;&c_je;&c_z;&c_i;&c_s;</trd>,
     	 <trd>&c_u;&c_m;&c_l;&c_ja;&c_u;&c_t;</trd>
      </trdgrp>
    </drv>
  </art>
