<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: android.xml,v 1.11 2023/10/28 08:33:06 revo Exp $">
<kap>
    <rad>android</rad>/o <fnt><bib>Viki</bib></fnt>
</kap>
<drv mrk="android.0o">
  <kap><tld/>o</kap>
  <snc mrk="android.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="robot.0o">Roboto</ref>, kiu tre similas al
      homo kaj kondutas simile al homo:
      <ekz>
        la <tld/>o <nom>Data</nom> &ccirc;iam strebas alpreni homajn
        karakteriza&jcirc;ojn, aparte homajn emociojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=287621">Stela
          Voja&gcirc;o: La Sekva Generacio</url></lok>
        </fnt>.
      </ekz>
     </dif>
     </snc>
    <trd lng="be">&c_a;&c_n;&c_d;&c_r;&c_o;&c_ib;&c_d;</trd>
  <trd lng="es">androide</trd>
  <trd lng="fr">andro&iuml;de</trd>
  <trd lng="pt">androide</trd>

</drv>
  
<drv mrk="android.A0o">
  <kap><tld lit="A"/>o</kap>
  <snc mrk="android.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <uzo tip="fak">TEL</uzo>
    <dif>
      Po&scirc;telefona <ref tip="super" cel="operac.0umo">operaciumo</ref>,
      kies kerno uzas <ref tip="super" cel="linuks.0o">Linukson</ref>:
      <ekz>
        Linukso a&ubreve; <tld lit="A"/>o estas ekzemploj de malfermkodaj
        programoj 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=509559">Malferma
          kodo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_A;&c_n;&c_d;&c_r;&c_o;&c_ib;&c_d;</trd>
  <trd lng="es">Android</trd>
  <trd lng="fr">Andro&iuml;d</trd>
  <trd lng="id">Android</trd>
  <trd lng="pt">Android</trd>
  <trdgrp lng="zh">
    <trd>&#x673A;&#x5668;&#x4EBA; <pr>j&#x12B;q&#xEC;r&#xE9;n</pr></trd>,
    <trd>&#x6A5F;&#x5668;&#x4EBA; <pr>j&#x12B;q&#xEC;r&#xE9;n</pr></trd>,
    <trd>&#x4EBA;&#x9020;&#x4EBA; <pr>r&#xE9;nz&#xE0;or&#xE9;n</pr></trd>,
    <trd>&#x4EFF;&#x751F;&#x4EBA; <pr>f&#x1CE;ngsh&#x113;ngr&#xE9;n</pr></trd>
  </trdgrp>
</drv>
</art>

