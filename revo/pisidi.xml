<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: pisidi.xml,v 1.5 2024/02/10 18:26:16 afido Exp $">
    <kap>
      <rad>Pisidi</rad>/o <fnt><bib>NT</bib></fnt>
    </kap>

<drv mrk="pisidi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo><uzo tip="fak">BIB</uzo>
  <snc mrk="pisidi.0o.antikva_regiono">
    <dif>
      Antikva <ref tip="lst" lst="voko:regionoj" cel="region.0o.land">regiono</ref>
      en Malgrandazio, norde de Likio kaj <ref tip="lst" lst="voko:regionoj"
      cel="pamfili.0o">Pamfilio</ref>,
      en kiu situas <ref tip="prt" cel="antiohxi.0o">Antio&hcirc;io</ref>:
      <ekz>
        ili, trapasante de <nom>Perga</nom> venis al Antio&hcirc;ia en <tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 13:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e Termeso, Aleksandro halt<klr>[&#8230;]</klr>is sed ne atakis tiun urbon de <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=11964">Aleksandro la Granda</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/8/8b/Putzger_Kleinasien_Zuschnitt.jpg/640px-Putzger_Kleinasien_Zuschnitt.jpg"
      prm="PD"
      >mapo de antikva Malgrandazio kun romiaj provincoj
        <fnt>
          <aut>Friedrich Wilhelm Putzger</aut>
          <lok><url ref="&WCW;/File:Putzger_Kleinasien_Zuschnitt.jpg"
              >&WCW;/File:Putzger_Kleinasien_Zuschnitt.jpg</url></lok>
        </fnt>
        <mrk stl="left:30%;top:55%;width:10%;height:5%;background:none;border: 3px dotted darkred;
        border-radius: 40%"/>
    </bld>
    <trd lng="de">Pisidien</trd>
    <trd lng="en">Pisidia</trd>
    <trd lng="fr">Pisidie</trd>
    <trd lng="grc">&Pi;&jota;&sigma;&jota;&delta;&jota_ton;&alfa;</trd>
    <trd lng="lat">Pisidia</trd>
    <trd lng="pl">Pizydia</trd>
    <trd lng="ro">Pisidia</trd>
  </snc>
<trd lng="be">&c_P;&c_ib;&c_s;&c_y;&c_d;&c_y;&c_ja;</trd>
</drv>

  </art>

