<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: objxet.xml,v 1.6 2022/03/24 15:12:20 revo Exp $">
<kap>
  <rad>ob&jcirc;et</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="objxet.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="objxet.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="parol.kontraux0i.KOMUNE">Kontra&ubreve;paroli</ref>
      kontra&ubreve; iu faro, kontra&ubreve; iu decido, provante &gcirc;in <ref tip="vid" cel="bar.0i.malhelpi">bari</ref>:
      <ekz>
        mi tamen ne <tld/>as, ke anka&ubreve; la mallogika senco 
        <klr>...</klr> estu registrita
        <fnt>
          <aut>S. Pokrovskij</aut>,
          <vrk>malvarma sango</vrk>,
          <lok><url ref="http://www.yahoogroups.com/group/revuloj">
          revuloj</url>
          2007-03-15</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="opon.0i">oponi</ref>,
      <ref cel="veto.0i">vetoi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ja;&c_r;&c_e;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_s;&c_p;&c_r;&c_e;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="es">objetar</trd>
  <trd lng="fr">objecter</trd>
  <trdgrp lng="nl">
    <trd>aanvoeren<klr tip="ind"> tegenwerpen</klr></trd>,
    <trd>tegenwerpen</trd>,
    <trd>inbrengen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wnosi&cacute;  <ind>obiekcje</ind></trd>,
    <trd>wnosi&cacute; zastrze&zdot;enia</trd>
  </trdgrp>
  <trd lng="ro">face obiec&#x21B;ii</trd>
  <trdgrp lng="uk">
    <trd>&#x437;&#x430;&#x43F;&#x435;&#x440;&#x435;&#x447;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x440;&#x43E;&#x442;&#x438;&#x441;&#x442;&#x430;&#x432;&#x43B;&#x44F;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="objxet.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="objxet.0o.KOMUNE">
    <dif>
      Kontra&ubreve;parolo, kontra&ubreve;argumentado:
      <ekz>
        pli serioza estas la <tld/>o la&ubreve; kiu la homaro ne kapablus,
        la&ubreve; 
        kvalita vidpunkto, funkciigi anar&hcirc;ian socion
        <fnt>
          <aut>S. Dagerman, trad. Phil</aut>,
          <vrk><url ref="http://www.satesperanto.org/La-anarhxiismo-kaj-mi.html">
          La anar&hcirc;iismo kaj mi</url></vrk>,
          <lok>[vidita en 2007-07]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ja;&c_r;&c_e;&c_ch;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="es">objeci&oacute;n</trd>
  <trd lng="fr">objection</trd>
  <trdgrp lng="nl">
    <trd>objectie</trd>,
    <trd>tegenwerping</trd>,
    <trd>bezwaar</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>obiekcja</trd>,
    <trd>zastrze&zdot;enie</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>obiec&#x21B;ie</trd>,
    <trd>exonerare de r&abreve;spundere</trd>
  </trdgrp>
</drv>
</art>

