<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: objxet.xml,v 1.11 2024/02/26 06:30:35 revo Exp $">
<kap>
  <rad>ob&jcirc;et</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="objxet.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="objxet.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="parol.kontraux0i.KOMUNE">Kontra&ubreve;paroli</ref>,
      <ref tip="sin" cel="argume.kontraux0i">kontra&ubreve;argumenti</ref>
      kontra&ubreve; iu faro, kontra&ubreve; iu decido, provante &gcirc;in <ref tip="vid"
      cel="bar.0i.malhelpi">bari</ref>:
      <ekz>
        mi tamen ne <tld/>as, ke anka&ubreve; la mallogika senco
        <klr>[&#8230;]</klr> estu registrita
        <fnt>
          <aut>S. Pokrovskij</aut>,
          <vrk>malvarma sango</vrk>,
          <lok><url ref="http://www.yahoogroups.com/group/revuloj">
          revuloj</url>
          2007-03-15</lok>
        </fnt>;
      </ekz>
      <ekz>
        nu, vi <tld/>is ke neniu povus malakcepti tion kion mi diris
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Ses</lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;pri tio vi eraras&rightquot;, mi <tld/>is
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Na&ubreve;</lok>
        </fnt>;
      </ekz>
      <ekz>
        tamen, tamen! &ndash; subite per severa vo&ccirc;o <tld/>is de ie <klr>[&#8230;]</klr> Ivano
        <fnt>
          <bib>MkM</bib>
          <lok>11</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konsen.mal0i">malkonsenti</ref>,
      <ref cel="opon.0i">oponi</ref>,
      <ref cel="opozic.0i">opozicii</ref>,
      <ref cel="veto.0i">vetoi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ja;&c_r;&c_e;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_s;&c_p;&c_r;&c_e;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>einwenden</trd>,
    <trd>entgegnen</trd>,
    <trd>einwerfen</trd>
  </trdgrp>
  <trd lng="es">objetar</trd>
  <trd lng="fr">objecter</trd>
  <trdgrp lng="nl">
    <trd>aanvoeren<klr tip="ind"> tegenwerpen</klr></trd>,
    <trd>tegenwerpen</trd>,
    <trd>inbrengen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wnosi&cacute;  <ind>obiekcje</ind></trd>,
    <trd>wnosi&cacute; zastrze&zdot;enia</trd>
  </trdgrp>
  <trd lng="ro">face obiec&#539;ii</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_p;&c_je;&c_r;&c_je;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_t;&c_i;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#29300;&#35302; <pr>d&#464;ch&ugrave;</pr></trd>,
    <trd>&#29300;&#35320; <pr>d&#464;ch&ugrave;</pr></trd>,
    <trd>&#39361; <pr>b&oacute;</pr></trd>,
    <trd>&#39539; <pr>b&oacute;</pr></trd>,
    <trd>&#38914;&#25758; <pr>d&#464;ngzhu&agrave;ng</pr></trd>,
    <trd>&#39030;&#25758; <pr>d&#464;ngzhu&agrave;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="objxet.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="objxet.0o.KOMUNE">
    <dif>
      Kontra&ubreve;parolo, kontra&ubreve;argumentado:
      <ekz>
        viajn <tld/>ojn mi malakceptas
        <fnt>
          <bib>MkM</bib>
          <lok>25</lok>
        </fnt>;
      </ekz>
      <ekz>
        la opozicio havis plurajn pravajn <tld/>ojn, el kiuj kelkaj eniris la
        definitivan reformon
        <fnt>
          <aut>Stefan Maul</aut>
            <vrk><url ref="&Monato;/2000/007124.php">Majstroverko, sed nur taktika</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 8</lok>
          </fnt>;
      </ekz>
      <ekz>
        pli serioza estas la <tld/>o la&ubreve; kiu la homaro ne kapablus,
        la&ubreve; kvalita vidpunkto, funkciigi anar&hcirc;ian socion
        <fnt>
          <aut>S. Dagerman, trad. Phil</aut>,
          <vrk><url ref="http://www.satesperanto.org/La-anarhxiismo-kaj-mi.html">
          La anar&hcirc;iismo kaj mi</url></vrk>,
          <lok>[vidita en 2007-07]</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="konsen.0o">konsento</ref>
    <refgrp tip="vid">
      <ref cel="opini.aparta0o">aparta opinio</ref>,
      <ref cel="ekscep.0o">ekscepcio</ref>,
      <ref cel="dir.kontraux0o.ago">kontra&ubreve;diro<sncref/></ref>,
      <ref cel="opozic.0o.POL">opozicio<sncref/></ref>,
      <ref cel="protes.0o">protesto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_ja;&c_r;&c_e;&c_ch;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="de">
    <trd>Einwand</trd>,
    <trd>Gegenargument</trd>,
    <trd>Entgegnung</trd>,
    <trd>Einwurf</trd>
  </trdgrp>
  <trd lng="es">objeci&oacute;n</trd>
  <trd lng="fr">objection</trd>
  <trdgrp lng="nl">
    <trd>objectie</trd>,
    <trd>tegenwerping</trd>,
    <trd>bezwaar</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>obiekcja</trd>,
    <trd>zastrze&zdot;enie</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>obiec&#539;ie</trd>,
    <trd>exonerare de r&abreve;spundere</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20108;&#35441; <pr>&egrave;rhu&agrave;</pr></trd>,
    <trd>&#20108;&#35805; <pr>&egrave;rhu&agrave;</pr></trd>,
    <trd>&#24322;&#35758; <pr>y&igrave;y&igrave;</pr></trd>,
    <trd>&#30064;&#35696; <pr>y&igrave;y&igrave;</pr></trd>,
    <trd>&#19981;&#21516;&#24847; <pr>b&ugrave;t&oacute;ngy&igrave;</pr></trd>
  </trdgrp>
</drv>
</art>

