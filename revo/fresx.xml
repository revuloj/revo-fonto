<drv mrk="fresx.re0igajxo">
  <kap>re<tld/>iga&jcirc;o</kap>
  <snc mrk="fresx.re0igajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Re<tld/>iga trinka&jcirc;o:
      <ekz>
        sur la ondanta akvo ni prenos iom da re<tld/>iga&jcirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        post la unua re<tld/>iga&jcirc;o ni pretigos nin
        <fnt>
          <vrk><url ref="https://esperanto.si/eo/rikoltado-de-olivoj-en-izolaisola">Rikoltado de
          olivoj en Izola/Isola</url></vrk>
          <lok>Slovenia Esperanto-Ligo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Erfrischungstrunk</trd>
    <trd lng="fr">rafraichissement<klr tip="ind"> (boisson)</klr></trd>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_mol;&c_v;&c_ja;&c_zh;&c_a;&c_l;&c_mol;&c_n;&c_y; &c_n;&c_a;&c_p;&c_ib;&c_t;&c_a;&c_k;</trd>
  <trd lng="ca">refresc</trd>
  <trd lng="de">Erfrischung</trd>
  <trd lng="es">refresco</trd>
  <trd lng="ja">&#x98F2;&#x307F;&#x7269;  <pr>&#x306E;&#x307F;&#x3082;&#x306E;</pr></trd>
  <trd lng="nl">frisdrank</trd>
  <trd lng="pl">nap&oacute;j orze&zacute;wiaj&aogonek;cy</trd>
  <trd lng="uk">&#x43E;&c_s;&c_v;&c_ib;&c_zh;&c_a;&#x44E;&c_ch;&c_i;&c_j; &#x43D;&c_a;&#x43F;&c_ib;&c_j;</trd>
