<drv mrk="fresx.0e">
  <kap>
    <tld/>e
  </kap>
  <snc mrk="fresx.0e.jxuse">
    <dif>
      &Jcirc;use:
      <ekz>
        <tld/>e falinta ne&gcirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">r&eacute;cemment</trd>
  <trdgrp lng="id">
    <trd><ind>baru</ind> saja</trd>,
    <trd>barusan</trd>
  </trdgrp>
  </snc>
  <snc mrk="fresx.0e.stato">
    <dif>
      En fre&scirc;a stato:
      <ekz>
        &ccirc;iuj floroj staris <tld/>e kaj bele en la varma lumo de la suno
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="id">segar</trd>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_v;&c_je;&c_zh;&c_a;</trd>
  <trd lng="de">frisch</trd>
  <trd lng="uk">&c_s;&c_v;&c_ib;&c_zh;&c_o;</trd>
  <trdgrp lng="zh">
    <trd>&#26032;&#39854;&#30340; <pr>x&imacron;nxi&amacron;nde</pr></trd>,
    <trd>&#40059; <pr>xian</pr></trd>,
    <trd>&#28152;&#29245; <pr>q&imacron;ngshu&#462;ng</pr></trd>,
    <trd>&#28732; <pr>r&aacute;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fresx.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="fresx.0eco.KOMUNE">
    <dif>
      Stato de io a&ubreve; iu <tld/>a:
      <ekz>
        Moseo havis la a&gcirc;on de cent dudek jaroj <klr>[&#8230;]</klr> kaj ne malaperis lia
        <tld/>eco
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 34:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i estis plena de <tld/>eco kaj &ccirc;armo
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> rigardis la tutan floran belecon kaj <tld/>econ en la &gcirc;ardeno
        <fnt>
          <bib>Fab2</bib>
          <lok>Abio</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_v;&c_je;&c_zh;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="ca">frescor</trd>
  <trd lng="de">Frische</trd>
  <trd lng="fr">fraicheur</trd>
  <trd lng="es">frescura</trd>
  <trd lng="id"><baz>segar</baz>kesegaran</trd>
  <trdgrp lng="ja">
    <trd>&#29983;&#27671;  <pr>&#12379;&#12356;&#12365;</pr></trd>,
    <trd>&#26032;&#39854;&#12373;  <pr>&#12375;&#12435;&#12379;&#12435;&#12373;</pr></trd>
  </trdgrp>
  <trd lng="nl">frisheid</trd>
  <trd lng="pl">&sacute;wie&zdot;o&sacute;&cacute;</trd>
  <trd lng="ru">&c_s;&c_v;&c_je;&c_zh;&c_je;&c_s;&c_t;&c_mol;</trd>
  <trd lng="uk">&c_s;&c_v;&c_ib;&c_zh;&c_ib;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="fresx.re0igi">
  <kap>re<tld/>igi</kap>
  <snc mrk="fresx.re0igi.KOMUNE">
    <dif>
      Fari ion denove <tld/>a:
      <ekz>
        fliki kaj re<tld/>igi vestojn<fnt>B</fnt>;
      </ekz>
      <ekz>
        re<tld/>igu min per pomsuko
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 2:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili re<tld/>igis mian spiriton
        <fnt>
          <bib>NT</bib>
          <lok>I. Korintanoj 16:18</lok>
        </fnt>.
      </ekz>
      <ekz>
        re<tld/>igi la kolorojn de pentra&jcirc;o, la aeron en la &ccirc;ambro,
        la memoron pri farita promeso;
      </ekz>
      <ekz>
        <ind>sin re<tld/>igi</ind>
        <klr>(sin meti en bonan, <tld/>an staton)</klr>;
        <trd lng="be">&c_a;&c_s;&c_mol;&c_v;&c_ja;&c_zh;&c_y;&c_c;&c_c;&c_a;</trd>
        <trd lng="ca">refrescar-se</trd>
        <trd lng="fr">se <ind>rafraichir</ind></trd>
        <trd lng="nl">zich <ind>verfrissen</ind></trd>
      </ekz>
      <ekz>
        <ind>re<tld/>iga</ind> spirita bano.
        <trd lng="be">&c_a;&c_s;&c_mol;&c_v;&c_ja;&c_zh;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
        <trd lng="ca">refrescant</trd>
        <trd lng="fr">rafraichissant</trd>
        <trd lng="nl">verfrissend</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_mol;&c_v;&c_ja;&c_zh;&c_y;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>refrescar</trd>,
    <trd>renovar</trd>
  </trdgrp>
  <trd lng="de">auffrischen</trd>
  <trd lng="es">refrescar</trd>
  <trd lng="fr">rafraichir</trd>
  <trdgrp lng="id">
    <trd><baz>segar</baz>menyegarkan</trd>,
    <trd><baz>segar</baz>mempersegar</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#26032;&#39854;&#12373;&#12434;&#34311;&#12425;&#12379;&#12427;  <pr>&#12375;&#12435;&#12379;&#12435;&#12373;&#12434;&#12424;&#12415;&#12364;&#12360;&#12425;&#12379;&#12427;</pr></trd>,
    <trd>&#20803;&#27671;&#12434;&#22238;&#24489;&#12373;&#12379;&#12427;  <pr>&#12370;&#12435;&#12365;&#12434;&#12363;&#12356;&#12405;&#12367;&#12373;&#12379;&#12427;</pr></trd>,
    <trd>&#12522;&#12501;&#12524;&#12483;&#12471;&#12517;&#12373;&#12379;&#12427;</trd>
  </trdgrp>
  <trd lng="nl">verfrissen</trd>
  <trd lng="pl">od&sacute;wie&zdot;y&cacute;</trd>
  <trd lng="ru">&c_o;&c_s;&c_v;&c_je;&c_zh;&c_i;&c_t;&c_mol;</trd>
  <trd lng="uk">&c_o;&c_s;&c_v;&c_ib;&c_zh;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#20351;&#28152;&#29245; <pr>sh&#464;q&imacron;ngshu&#462;ng</pr></trd>,
    <trd>&#29245;&#24555; <pr>shu&#462;ngku&agrave;i</pr></trd>,
    <trd>&#26377;&#31934;&#31070; <pr>y&#466;uj&imacron;ngsh&eacute;n</pr></trd>
