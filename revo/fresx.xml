<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>fre&scirc;</rad>/a
</kap>
<drv mrk="fresx.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc num="1">
    <dif>
      Pro &jcirc;useco bonkvalita, bonstata:
      <ekz><tld/>a pano<fnt>B</fnt>,</ekz>
      <ekz>viando, ovo, akvo, aero.</ekz>
    </dif>
    <trd lng="de">frisch</trd>
  </snc>
  <snc num="2">
    <dif>
      Nedifektita de mal&jcirc;useco, de eluziteco:
      <ekz>homo ankora&ubreve; <tld/>a malgra&ubreve; longa voja&gcirc;o;</ekz>
      <ekz><tld/>aj batalantoj;</ekz>
      <ekz><tld/>a vesto;</ekz>
      <ekz>&ccirc;iam <tld/>a estas la memoro pri lia morto;</ekz>
      <ekz><tld/>a entuziasmo.</ekz>
    </dif>
    <trd lng="de">frisch</trd>
  </snc>
  <snc num="3">
    <dif>
      &Jcirc;usa, ne malnova:
      <ekz><tld/>a modo, tombo<fnt>Z</fnt>;</ekz>
      <ekz><tld/>data informo;</ekz>
      <ekz>plej <tld/>aj nova&jcirc;oj de gazeto;</ekz>
      <ekz>krimulo kaptita &ccirc;e l' <tld/>a faro<fnt>Z</fnt>;</ekz>
      <ekz>&ccirc;u vi ricevis <tld/>a&jcirc;ojn (<tld/>ajn sciigojn) pri li?</ekz>
    </dif>
    <trdgrp lng="de">
     <trd>neu</trd>,
     <trd>aktuell</trd></trdgrp>
  </snc>
  <trd lng="cs">&ccaron;erstv&yacute;</trd>
  <trdgrp lng="fr">
    <trd>frais<klr tip="ind"> (adj.)</klr></trd>,
    <trd>r&eacute;cent</trd></trdgrp> 
  <trd lng="en">fresh</trd>
  <trd lng="ru">&c_s;&c_v;&c_je;&c_zh;&c_i;&c_j;</trd>
</drv>
<drv mrk="fresx.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Stato de io <tld/>a. 
    </dif>
  </snc>
  <trd lng="fr">fra&icirc;cheur</trd>
  <trd lng="ru">&c_s;&c_v;&c_je;&c_zh;&c_je;&c_s;&c_t;&c_mol;</trd>
</drv>
<drv mrk="fresx.re0igi">
  <kap>re<tld/>igi</kap>
  <snc>
    <dif>
      Fari ion denove <tld/>a:
      <ekz>fliki k re<tld/>igi vestojn<fnt>B</fnt>;</ekz>
      <ekz>
        re<tld/>igi la kolorojn de pentra&jcirc;o, la aeron en la &ccirc;ambro,
        la memoron pri farita promeso;
      </ekz>
      <ekz>
        <ind>sin re<tld/>igi</ind>
        <klr>(sin meti en bonan, <tld/>an staton)</klr>;
        <trd lng="fr">se <ind>rafra&icirc;chir</ind></trd>
      </ekz>
      <ekz>i
        <ind>re<tld/>iga</ind> spirita bano.
        <trd lng="fr">rafra&icirc;chissant</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="ru">&c_o;&c_s;&c_v;&c_je;&c_zh;&c_i;&c_t;&c_mol;</trd>
</drv>
<drv mrk="fresx.re0igajxo">
  <kap>re<tld/>iga&jcirc;o<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Re<tld/>iga trinka&jcirc;o. 
    </dif>
  </snc>
  <trd lng="fr">rafra&icirc;chissement<klr tip="ind"> (boisson)</klr></trd>
</drv>
<drv mrk="fresx.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Tute ne <tld/>a.
    </dif>
    <refgrp tip="vid">
      <ref cel="putr.0a">putra</ref>,
      <ref cel="arkaik.0a">arkaika</ref>,
      <ref cel="nov.mal0a">malnova</ref>,
      <ref cel="velk.0a">velka</ref>,
      <ref cel="kaduk.0a">kaduka</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>rassi<klr tip="ind"> (pas frais)</klr></trd>,
    <trd>pas <ind>frais</ind></trd>,
    <trd>d&eacute;fra&icirc;chi</trd>,
    <trd>fatigu&eacute;</trd>,
    <trd>rance</trd>,
    <trd>us&eacute;</trd></trdgrp> 
  <trd lng="ru">&c_n;&c_je;&c_s;&c_v;&c_je;&c_zh;&c_i;&c_j;</trd>
</drv>
</art>
