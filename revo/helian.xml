<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>heliant</rad>/o
</kap>
<drv mrk="helian.0o">
  <kap>
  <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="astera.0oj">asteracoj</ref>
      (<trd lng="la">Helianthus</trd>), kun granda flava floraro.
    </dif>
    <ref tip="sin" cel="sun.0floro">sunfloro</ref>
  </snc>
  <trd lng="de">Sonnenblume</trd>
  <trd lng="en">sunflower</trd>
  <trd lng="fr">tournesol</trd>
  <trd lng="hu">napraforg&oacute;</trd>
  <trd lng="nl">zonnebloem</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_s;&c_o;&c_l;&c_n;&c_u;&c_h;</trd>,
 
<trd>&c_p;&c_o;&c_d;&c_s;&c_o;&c_l;&c_n;&c_je;&c_ch;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="tr">ay&ccedil;i&ccedil;e&gbreve;i</trd>
</drv>

<drv mrk="helian.tubera0o">
  <kap>
  tubera <tld/>o
  </kap>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="helian.0o"><tld/>o</ref>
      (<trd lng="la">Helianthus tuberosus</trd>), kun tubera radiko,
      kreskanta precipe en norda Ameriko kaj uzata por nutri bestojn
      kaj kiel legomo.
    </dif>
  </snc>
  <bld lok="../bld/tuberaheliant.jpg">
    la man&gcirc;ebla radiko de tubera <tld/>o (teropiro)
  </bld>
  <trdgrp lng="de">
    <trd>Topinambur</trd>,
    <trd>Erdbirne</trd>
  </trdgrp>
  <trd lng="en">Jerusalem artichoke</trd>
  <trd lng="fr">topinambour</trd>
  <trd lng="hu">csics&oacute;ka</trd>
  <trd lng="nl">aardpeer</trd>
  <trd lng="ru">&c_t;&c_o;&c_p;&c_i;&c_n;&c_a;&c_m;&c_b;&c_u;&c_r;</trd>
</drv>
</art>
