<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>heliant</rad>/o
</kap>
<drv mrk="helian.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="astera.0oj">asteracoj</ref>
      (<trd lng="la">Helianthus</trd>), kun granda flava floraro.
    </dif>
    <ref tip="sin" cel="flor.sun0o">sunfloro</ref>
  </snc>
  <trd lng="de">Sonnenblume</trd>
  <trd lng="tr">ay&ccedil;i&ccedil;e&gbreve;i</trd>
</drv>
<drv mrk="helian.tubera0o">
  <kap>tubera <tld/>o</kap>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="helian.0o"><tld/>o</ref>
      (<trd lng="la">Helianthus tuberosus</trd>), kun tubera radiko,
      kreskanta precipe en Norda Ameriko kaj uzata por nutri bestojn kaj kiel
      legomo.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Topinambur</trd>,
    <trd>Erdbirne</trd>
  </trdgrp>
</drv>
</art>
