<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>putr</rad>/i
</kap>
<drv mrk="putr.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Malkomponi&gcirc;i, p. p. organisma substanco:
      <ekz>r
        forlasita ovo devas <tld/>i<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>ado de fi&scirc;o komenci&gcirc;as de l' kapo
        <klr>(f: altranguloj plej facile korupti&gcirc;as)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiuj kompatinduloj <tld/>as en malliberejo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la nomo de malpiuloj for<tld/>os<fnt>Z</fnt>
        <klr>(ekstermi&gcirc;os)</klr>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="velk.0i">velki</ref>,
      <ref cel="sxim.0i">&scirc;imi</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>faulen</trd>,
      <trd>verfaulen</trd>,
      <trd>verwesen</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>gni&cacute;</trd>
      <trd>rozk&lstroke;ada&cacute; si&eogonek;</trd>
      <trd>butwie&cacute;</trd>
      <trd>pr&oacute;chnie&cacute;</trd>
    </trdgrp>    
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Morale <tld/>i, morale difekti&gcirc;i, defali:
      <ekz>
        estos io <tld/>inta en la re&gcirc;imo
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="dekade.0i">dekadenci</ref>,
      <ref cel="degene.0i">degeneri</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>stinken</trd>,
      <trd>zerfallen</trd>,
      <trd><klr>sich </klr>zersetzen</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_g;&c_n;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="en">putrefy</trd>
  <trd lng="es">pudrirse</trd>
  <trd lng="hu">rothad</trd>
  <trd lng="nl">rotten</trd>
    <trdgrp lng="pl">
      <trd>gni&cacute;</trd>
      <trd>rozk&lstroke;ada&cacute; si&eogonek;</trd>
    </trdgrp>    
  <trdgrp lng="pt">
   <trd>apodrecer</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_l;&c_a;&c_g;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="putr.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      = <tld/>anta:
      <ekz>
        <tld/>a dento<fnt>Z</fnt>;
      </ekz>
      <ekz>
        de semo <tld/>a venas frukto ne nutra
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="mucid.0a">mucida</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_n;&c_ib;&c_l;&c_y;</trd>
  <trdgrp lng="de">
      <trd>faulend</trd>,
      <trd>faulig</trd>,
      <trd>verfaulend</trd>,
      <trd>verwesend</trd>
  </trdgrp>
  <trdgrp lng="es">
      <trd>podrido</trd>,
      <trd>putrefacto</trd>
  </trdgrp>
  <trd lng="hu">rothadt</trd>
  <trd lng="nl">rot</trd>
    <trdgrp lng="pl">
      <trd>gnilny</trd>
      <trd>gni&lstroke;y</trd>
      <trd>zbutwia&lstroke;y</trd>
      <trd>spr&oacute;chnia&lstroke;y</trd>
    </trdgrp>    
  <trd lng="ru">&c_g;&c_n;&c_i;&c_l;&c_o;&c_j;</trd>
</drv>

<drv mrk="putr.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Io <tld/>inta.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_n;&c_ib;&c_l;&c_mol;&c_l;&c_jo;</trd>
  <trdgrp lng="de">
     <trd>Aas</trd>,
     <trd>Faulendes</trd>,
     <trd>Verwesendes</trd>
  </trdgrp>
  <trd lng="hu">rohad&eacute;k</trd>
  <trdgrp lng="pl">
    <trd>zgnilizna</trd>
    <trd>pr&oacute;chno</trd>
  </trdgrp>    
  <trd lng="ru">&c_g;&c_n;&c_i;&c_l;&c_mol;</trd>
</drv>
</art>
