<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: putr.xml,v 1.36 2023/09/03 12:45:14 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>putr</rad>/i
</kap>
<drv mrk="putr.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="putr.0i.KOMUNE">
    <dif>
      Pri organika substanco: malkomponi&gcirc;i, disfali:
      <ekz>
        mia ha&ubreve;to krevis kaj <tld/>as
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 7:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> fi&scirc;oj <tld/>as pro senakveco
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 50:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i kun &ccirc;iuj malri&ccirc;uloj el la kvartalo en malbela tombejo
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la grizaj lignaj krucoj klini&gcirc;is kaj <tld/>as sub siaj tegmentoj
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XXVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        en &scirc;limo, kie <tld/>as algoj <klr>[&#x2026;]</klr> ku&scirc;as senmovaj aligatoroj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la popolo kapturni&gcirc;is kaj &scirc;anceli&gcirc;adis pro malsato, la man&gcirc;a&jcirc;oj kunamasigite
        <tld/>is en la Nutra kaj Vivdona Oficejo
        <fnt>
          <bib>VaK</bib>
          <lok>Deka &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en la malliberejo <klr>[&#x2026;]</klr> <tld/>i &gcirc;is 
        oni trumpetos la tagon de lasta ju&gcirc;o
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>ado de fi&scirc;o komenci&gcirc;as de l' kapo
        <klr>(altranguloj plej facile korupti&gcirc;as)</klr>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="de">der Fisch stinkt vom Kopfe her</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="erozi.0igxi">erozii&gcirc;i</ref>,
      <ref cel="gangre.0i">gangreni</ref>,
      <ref cel="sxim.0i">&scirc;imi</ref>,
      <ref cel="velk.0i">velki</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>faulen</trd>,
      <trd>verfaulen</trd>,
      <trd>verwesen</trd>
    </trdgrp>
    <trd lng="fr">pourrir<klr> (intr.)</klr></trd>
    <trdgrp lng="pl">
      <trd>gni&cacute;</trd>
      <trd>rozk&lstroke;ada&cacute; si&eogonek;</trd>
      <trd>butwie&cacute;</trd>
      <trd>pr&oacute;chnie&cacute;</trd>
    </trdgrp>    
  </snc>
  <snc mrk="putr.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Morale <tld/>i, morale difekti&gcirc;i,
      <ref tip="vid" cel="dekade.0i">dekadenci</ref>,
      <ref tip="vid" cel="degene.0i">degeneri</ref>:
      <ekz>
        estas io <tld/>inta en la re&gcirc;imo;
      </ekz>
      <ekz>
        la nobeleco komencis <tld/>i
        <fnt>
          <bib>Fab3</bib>
        </fnt>;
      </ekz>
      <ekz>
        Japanio jam anta&ubreve; jarcentoj <tld/>us sur la ruino de la familia sistemo
        <fnt>
          <bib>Monato</bib>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url ref="&Monato;/2005/008691.php">Familia nomo</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        nia demisio nepre komprenigos al Esperantujo ke io <tld/>as en UEA
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 8-9 (106-107)</lok>
        </fnt>;
      </ekz>
      <ekz>
        laboro homon nutras, sen laboro li <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>stinken</trd>,
      <trd>zerfallen</trd>,
      <trd><klr>sich </klr>zersetzen</trd>
    </trdgrp>
    <trd lng="fr">d&eacute;g&eacute;n&eacute;rer</trd>
  </snc>
  <trd lng="be">&c_g;&c_n;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="bg">
    <trd>&c_g;&c_r;&c_i;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_l;&c_a;&c_g;&c_a;&c_m; &c_s;&c_je;</trd>
  </trdgrp>
  <trd lng="bo">&t_ra;&t_shabchu;&t_la;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>hn&iacute;t</trd>,
    <trd>mravn&ecaron; upadat</trd>,
    <trd>rozkl&aacute;dat se</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>putrefy</trd>,
    <trd>putrify</trd>,
    <trd>decay</trd>,
    <trd>fester</trd>,
    <trd>decompose</trd>,
    <trd>rot</trd>
  </trdgrp>
  <trd lng="es">pudrirse</trd>
  <trd lng="hu">rothad</trd>
  <trdgrp lng="ja">
    <trd>&#x8150;&#x6557;&#x3059;&#x308B;  [<ind>&#x3075;&#x306F;&#x3044;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x8150;&#x308B;  [<ind>&#x304F;&#x3055;&#x308B;</ind>]</trd>,
    <trd>&#x9000;&#x5EC3;&#x3059;&#x308B;  [<ind>&#x305F;&#x3044;&#x306F;&#x3044;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">rotten</trd>
  <trdgrp lng="pl">
      <trd>gni&cacute;</trd>
      <trd>rozk&lstroke;ada&cacute; si&eogonek;</trd>
    </trdgrp>
  <trdgrp lng="pt">
   <trd>apodrecer</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_l;&c_a;&c_g;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pr&aacute;chnivie&tcaron;</trd>,
    <trd>tlie&tcaron;</trd>,
    <trd>zotlie&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="putr.0a">
  <kap><tld/>a</kap>
  <snc mrk="putr.0a.KOMUNE">
    <dif>
      <tld lit="P"/>anta, malkomponi&gcirc;anta, disfalanta:
      <ekz>
        <tld/>a dento
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 25:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a arbo
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 7:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj ovoj kaj rostitaj kokokrestoj estis la &ccirc;efaj man&gcirc;a&jcirc;oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne &ccirc;iam estis tiel <tld/>aj kaj ruinaj tiuj muregoj
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        alflugis al mi forta <tld/>a odoro, tiel abomena, ke mi volis forkuri
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a, pesta amaso da <ref cel="miasm.0o">miasmo</ref>j
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la <tld/>an vivon estis falanta la ombro de morto
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LI</lok>
        </fnt>;
      </ekz>
      <ekz>
        via koro estas <tld/>a abismo de malvirto
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        de semo <tld/>a venas frukto ne nutra
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mucid.0a">mucida</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_n;&c_ib;&c_l;&c_y;</trd>
  <trdgrp lng="bg">
    <trd>&c_g;&c_n;&c_i;&c_l;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_l;&c_o;&c_zh;&c_je;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hnilobn&yacute;</trd>,
    <trd>prohnil&yacute;</trd>,
    <trd>shnil&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>faulend</trd>,
      <trd>faulig</trd>,
      <trd>verfaulend</trd>,
      <trd>verwesend</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>putrescent</trd>,
    <trd>putrefying</trd>,
    <trd>putrifying</trd>,
    <trd>decaying</trd>,
    <trd>festering</trd>,
    <trd>rotting</trd>
  </trdgrp>
  <trdgrp lng="es">
      <trd>podrido</trd>,
      <trd>putrefacto</trd>
  </trdgrp>
  <trd lng="fr">pourri</trd>
  <trd lng="hu">rothadt</trd>
  <trd lng="ja">&#x8150;&#x3063;&#x305F;  [<ind>&#x304F;&#x3055;&#x3063;&#x305F;</ind>]</trd>
  <trd lng="nl">rot</trd>
  <trdgrp lng="pl">
      <trd>gnilny</trd>
      <trd>gni&lstroke;y</trd>
      <trd>zbutwia&lstroke;y</trd>
      <trd>spr&oacute;chnia&lstroke;y</trd>
    </trdgrp>
  <trd lng="ru">&c_g;&c_n;&c_i;&c_l;&c_o;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>hnilobn&yacute;</trd>,
    <trd>prehnit&yacute;</trd>,
    <trd>zhnit&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_n;&c_i;&c_l;&c_i;&c_j;</trd>,
    <trd>&c_t;&c_u;&c_h;&c_l;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="putr.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="putr.0ajxo.KOMUNE">
    <dif>
      Io <tld/>a:
      <ekz>
        akvon senmovan kovras <tld/>a&jcirc;o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi ja disfalas kiel <tld/>a&jcirc;o, kiel vesto disman&gcirc;ita de tineoj
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 13:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        Salamano insultadis sian hundon, li diris: <ctl>a&ccirc;ulo
        <tld/>a&jcirc;o!</ctl>
        <fnt>
          <aut>A. Camus, trad. M. Duc Goninaz</aut>,
          <vrk>La Fremdulo</vrk>,
          <lok>1993</lok>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <snc mrk="putr.0ajxo.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Io degenerinta, malbonstata:
      <ekz>
        la &ccirc;ifonproletaro, tiu pasiva <tld/>a&jcirc;o de la plej malaltaj tavoloj de la
        malnova socio estas parte entirata en la movadon per la proleta revolucio
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 41</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_n;&c_ib;&c_l;&c_mol;&c_l;&c_jo;</trd>
  <trdgrp lng="de">
     <trd>Aas</trd>,
     <trd>Faulendes</trd>,
     <trd>Verwesendes</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>carrion</trd>,
     <trd>decayed matter</trd>,
     <trd>filth</trd>
  </trdgrp>
  <trd lng="fr">pourriture</trd>
  <trd lng="hu">rohad&eacute;k</trd>
  <trd lng="ja">&#x8150;&#x6557;&#x7269;  [<ind>&#x3075;&#x306F;&#x3044;&#x3076;&#x3064;</ind>]</trd>
  <trd lng="nl">iets dat <ind>rot</ind> is</trd>
  <trdgrp lng="pl">
    <trd>zgnilizna</trd>
    <trd>pr&oacute;chno</trd>
  </trdgrp>    
  <trd lng="ru">&c_g;&c_n;&c_i;&c_l;&c_mol;</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_n;&c_i;&c_l;&c_mol;</trd>,
    <trd>&c_g;&c_n;&c_i;&c_l;&c_i;&c_z;&c_n;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="putr.0ema">
  <kap>
    <tld/>ema
  </kap>
  <snc mrk="putr.0ema.KOMUNE">
    <dif>
      Kiu balda&ubreve;, facile <tld/>as, ne konservi&gcirc;as longe:
      <ekz>
        &ccirc;i tiu putrema <klr>[&#x2026;]</klr>, mortema
        <fnt>
          <bib>NT</bib>
          <lok>I. Korintanoj 15:53</lok>
        </fnt>;
      </ekz>
      <ekz>
        la novtipa materialo detenas oksigenon de <tld/>emaj fruktoj kaj legomoj
        <fnt>
          <aut>-</aut>
          <vrk><url ref="https://www.lingq.com/en/learn-esperanto-online/courses/323534/ligno-kaj-kraboj-kiel-surogato-por-pl-1614787/">Ligno kaj kraboj kiel surogato por plasta&jcirc;o</url></vrk>
          <lok>Esperanta Retradio, 2018 </lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>leicht <ind>verderblich</ind></trd>,
    <trd>f&auml;ulnisanf&auml;llig</trd>
  </trdgrp>
  <trd lng="fr">putrescible</trd>
  <trd lng="ja">&#x8150;&#x308A;&#x3084;&#x3059;&#x3044;  [<ind>&#x304F;&#x3055;&#x308A;&#x3084;&#x3059;&#x3044;</ind>]</trd>
  <trd lng="uk">&c_sh;&c_v;&c_i;&c_d;&c_k;&c_o;&c_p;&c_s;&c_u;&c_v;&c_n;&c_i;&c_j;</trd>
</drv>

<drv mrk="putr.for0i">
  <kap>
    for<tld/>i,
    <var><kap>for<tld/>i&gcirc;i</kap></var>
  </kap>
  <snc>
    <dif>
      Tute <tld/>i&gcirc;i, nenii&gcirc;i a&ubreve; senvalori&gcirc;i <tld/>i&gcirc;ante:
      <ekz>
        for<tld/>is la grajnoj sub siaj terbuloj
        <fnt>
          <bib>MT</bib>
          <lok>Joel 1:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        ebonaj pordoj, kiuj neniam for<tld/>i&gcirc;os
        <fnt>
          <bib>IdO</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        jam ne unu universala genio, kiu povus reformi la mondon, for<tld/>is ja en la
        senfeligejo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ploras super viaj ostoj, vi jam longe for<tld/>inta
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>verfaulen</trd>,
    <trd>abfaulen</trd>,
    <trd>wegfaulen</trd>
  </trdgrp>
  <trd lng="es">pudrirse del todo</trd>
  <trdgrp lng="ja">
    <trd>&#x8150;&#x308A;&#x679C;&#x3066;&#x308B;  [<ind>&#x304F;&#x3055;&#x308A;&#x306F;&#x3066;&#x308B;</ind>]</trd>,
    <trd>&#x6D88;&#x6EC5;&#x3059;&#x308B;  [<ind>&#x3057;&#x3087;&#x3046;&#x3081;&#x3064;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">wegrotten</trd>
  <trd lng="ru">&c_s;&c_g;&c_n;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_ib;&c_g;&c_n;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_o;&c_g;&c_n;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
</drv>





<drv mrk="putr.ne0ema">
  <kap>
    ne<tld/>ema
  </kap>
  <snc mrk="putr.ne0ema.KOMUNE">
    <dif>
      Kiu ne facile <tld/>as, prefere seki&gcirc;as a&ubreve; iel konservi&gcirc;as longe:
      <ekz>
        akvo en vazo el tia argilo estas ne<tld/>ema kaj floroj en &gcirc;i floras pompe kaj
        la radikoj ne <tld/>as rapide
        <fnt>
          <aut>Xu Senrong, Niu Xueqin</aut>
          <vrk><url ref="http://www.espero.com.cn/se/txt/2012-10/31/content_495045.ht">Ceramiko</url></vrk>
          <lok>Espero.com.cn, 2012-10-31</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>best&auml;ndig</trd>,
    <trd>haltbar</trd>,
    <trd>f&auml;ulnisresistent</trd>
  </trdgrp>
  <trd lng="fr">imputrescible</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_n;&c_i;&c_l;&c_o;&c_s;&c_t;&c_ib;&c_j;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_t;&c_l;&c_ib;&c_n;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="putr.0igi">
  <kap>
    <tld/>igi
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="putr.0igi.KOMUNE">
    <dif>
      Ka&ubreve;zi <tld/>adon, malboni&gcirc;on de viva a&ubreve; vivdevena substanco:
      <ekz>
        mu&scirc;oj <tld/>igas kaj haladzigas la oleon de parfumisto
        <fnt>
          <bib>MT</bib>
          <lok>Predikanto 10:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tempo <klr>[&#x2026;]</klr> difektis la lignon kaj &gcirc;in tra<tld/>igis
        <fnt>
          <aut>H. Laxness, trad. B. Ragnarsson</aut>
          <vrk>Sendependaj homoj</vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  <trdgrp lng="de">
    <trd><ind>verfaulen</ind> lassen</trd>,
    <trd><ind>verwesen</ind> lassen</trd>
  </trdgrp>
  </snc>
  <snc mrk="putr.0igi.FIG">
    <dif>
      Morale malbonigi, <ref cel="korupt.0i.morale">korupti<sncref/></ref>:
      <ekz>
        sen le&gcirc;oj e&ccirc; en la plej memdisciplina socio aperas en la homo la sova&gcirc;eco de la
        prabesto, kiu plimalpli frue <tld/>igas la kulturon
        <fnt>
          <bib>VaK</bib>
          <lok>Sesa &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        laboro fortigas, ripozo <tld/>igas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">verderben</trd>
  <trdgrp lng="fr">
    <trd>corrompre</trd>,
