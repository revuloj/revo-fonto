<kap>
<rad>endoskop</rad>/o
</kap>
<drv mrk="endosk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="instru2.0o">Instrumento</ref> por ekzameni kavojn, kies aperturo estas
      mallar&gcirc;a:
      <ekz>
        <nom>Csaba T&oacute;th</nom> <klr>[&#x2026;]</klr> enkondukis en la hungara medicino la <tld/>on kontra&ubreve;
        nefrolitiazo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=528005">Csaba T&oacute;th</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        arkeologoj esploras la groton per <tld/>oj kaj laseroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3691">Romulo kaj Remo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="sond.0o">sondo</ref>
    <ref tip="vid" cel="endoskopi.0o">endoskopio</ref>
  </snc>
  <trd lng="be">&c_e;&c_n;&c_d;&c_a;&c_s;&c_k;&c_o;&c_p;</trd>
  <trd lng="cs">endoskop</trd>
  <trd lng="de">Endoskop</trd>
  <trd lng="es">endoscopio</trd>
  <trdgrp lng="fa">
    <trd>&a_d;&a_r;&a_w;&a_n;&zwnj;&a_b;&f_y;&a_n;</trd>,
    <trd>&a_A_madda;&a_n;&a_d;&a_w;&a_s;&f_k;&f_p;</trd>
  </trdgrp>
  <trd lng="fr">endoscope</trd>
  <trd lng="ja">&#x5185;&#x8996;&#x93E1;  <pr>&#x306A;&#x3044;&#x3057;&#x304D;&#x3087;&#x3046;</pr></trd>
  <trd lng="pl">endoskop</trd>
  <trd lng="pt">endosc&oacute;pio</trd>
  <trd lng="ru">&c_e;&c_n;&c_d;&c_o;&c_s;&c_k;&c_o;&c_p;</trd>
  <trd lng="sk">endoskop</trd>
  <trd lng="uk">&c_je;&c_n;&c_d;&c_o;&c_s;&c_k;&c_o;&c_p;</trd>
  <trdgrp lng="zh">
    <trd>&#x5167;&#x8996;&#x93E1; <pr>n&#xE8;ish&#xEC;j&#xEC;ng</pr></trd>,
    <trd>&#x5167;&#x7ABA;&#x93E1; <pr>n&#xE8;iku&#x12B;j&#xEC;ng</pr></trd>
  </trdgrp>
</drv>
</art>
