<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>armatur</rad>/o
</kap>
<drv mrk="armatu.0o">
  <kap><tld/>o</kap>
  <snc mrk="armatu.0o.en_masonajxo">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Metalaj stangoj, pecoj a&ubreve; fadenoj, kiuj plifortikigas
      masona&jcirc;on, &ccirc;arpenta&jcirc;on, fanda&jcirc;on a&ubreve; iun ajn
      ferlaboron.
    </dif>
  </snc>
  <snc mrk="armatu.0o.cxarpentajxo">
    <uzo tip="fak">TEK</uzo>
    <dif>
      &Ccirc;arpenta&jcirc;o, kiu subtenas konstrua&jcirc;on dum konstruado.
    </dif>
  </snc>
  <snc mrk="armatu.0o.de_kondensatoro">
    <uzo tip="fak">ELE</uzo>
    <dif>
      <klr>(de <ref cel="konden.0o.ELE">kondensatoro</ref>)</klr>
      &Ccirc;iu el la du platoj a&ubreve; metalfolioj, inter kiuj trovi&gcirc;as
      izolanta korpo, kaj kiuj kune formas la kondensatoron.
    </dif>
  </snc>
  <snc mrk="armatu.0o.de_magneto">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      <klr>(de magneto)</klr>
      Fera peco, kiu fermas la magnetan cirkviton.
    </dif>
  </snc>
  <trd lng="fr">armature</trd>
  <trd lng="ru">&#1072;&#1088;&#1084;&#1072;&#1090;&#1091;&#1088;&#1072;</trd>
</drv>

<drv mrk="armatu.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Provizi per <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">armer<klr tip="ind"> (d'une armature)</klr></trd>
</drv>
</art>
