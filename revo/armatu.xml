<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>armatur</rad>/o
</kap>
<drv mrk="armatu.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Metalaj stangoj, pecoj a&ubreve; fadenoj, kiuj plifortikigas
      masona&jcirc;on, &ccirc;arpenta&jcirc;on, fanda&jcirc;on a&ubreve; iun ajn
      ferlaboron.
    </dif>
  </snc>
  <snc>
    <dif>
      &Ccirc;arpenta&jcirc;o, kiu subtenas konstrua&jcirc;on dum konstruado.
    </dif>
  </snc>
  <trd lng="fr">armature</trd>
  <trd lng="ru">&#1072;&#1088;&#1084;&#1072;&#1090;&#1091;&#1088;&#1072;</trd>
</drv>

<drv mrk="armatu.magneta0o">
  <kap>magneta <tld/>o</kap>
  <snc>
    <dif>
      Fera peco, kiu fermas la magnetan cirkviton.
    </dif>
  </snc>
  <trd lng="fr">armature<klr tip="ind"> (d'un aimant)</klr></trd>
</drv>

<drv mrk="armatu.0odekondensatoro">
  <kap><tld/>o de kondensatoro</kap>
  <snc>
    <dif>
      &Ccirc;iu el la du platoj a&ubreve; metalfolioj, inter kiuj trovi&gcirc;as
      izolanta korpo, kaj kiuj kune formas la 
      <ref tip="malprt" cel="konden.0o.ELE">kondensatoron</ref>.
    </dif>
  </snc>
  <trd lng="fr">armature<klr tip="ind"> (d'un condensateur)</klr></trd>
</drv>

<drv mrk="armatu.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Provizi per <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">armer<klr tip="ind"> (d'une armature)</klr></trd>
</drv>
</art>
