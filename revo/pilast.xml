<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pilast.xml,v 1.25 2023/10/27 07:22:50 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>pilastr</rad>/o <fnt><bib>MT</bib></fnt>
</kap>
<drv mrk="pilast.0o">
  <kap><tld/>o</kap>
  <snc mrk="pilast.0o.ARKI">
  <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="dif" cel="kolon.mur0o.ARKI">Murkolono</ref>
      <ekz>
        el la santala ligno la re&gcirc;o faris <tld/>ojn por la domo
        <fnt>
          <bib>MT</bib>
          <lok>I. re&gcirc;oj 10:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la tuta naturo estis granda sankta pre&gcirc;ejo, kies <tld/>oj estis arboj
        <fnt>
          <bib>Fab2</bib>
          <lok>Sonorilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> nun vidis ku&scirc;i anta&ubreve; la pordego kaj inter la du <tld/>oj sennombrajn
        kraniojn de mortintoj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La tri Magiaj Pordegoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_l;&c_ja;&c_s;&c_t;&c_r;&c_a;</trd>
  <trd lng="cs">pilastr</trd>
  <trdgrp lng="de">
    <trd>Pilaster</trd>,
    <trd><klr>(Wand-)</klr>Pfeiler</trd>,
    <trd>viereckiger <ind>Wandpfeiler</ind></trd>
  </trdgrp>
  <trd lng="en">pilaster</trd>
  <trd lng="fr">pilastre</trd>
  <trd lng="hu">pilaszter</trd>
  <trdgrp lng="ja">
    <trd>&#x67F1;&#x5F62;  <pr>&#12385;&#12421;&#12358;&#x304B;&#x305F;</pr></trd>,
    <trd>&#x4ED8;&#12369;&#x67F1;  <pr>&#12388;&#12369;&#12385;&#12421;&#12358;</pr></trd>,
    <trd>&#x30D4;&#x30E9;&#x30B9;&#x30BF;&#x30FC;</trd>
  </trdgrp>
  <trd lng="nl">pilaster</trd>
  <trdgrp lng="pl">
    <trd>pilaster</trd>
    <trd>pilastr</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_i;&c_l;&c_ja;&c_s;&c_t;&c_r;</trd>
  <trd lng="sk">pilaster</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_ib;&#x43B;&#x44F;&c_s;&c_t;&c_r;</trd>,
    <trd>&#x43F;&c_ib;&#x43B;&#x44F;&c_s;&c_t;&c_r;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="pilast.apog0o">
  <kap>apog<tld/>o</kap>
  <snc mrk="pilast.apog0o.ARKI">
  <uzo tip="fak">ARKI</uzo>
    <dif>
      <tld lit="P"/>a <ref tip="super" cel="kolon.0o.ARKI">kolono</ref>
      kiu apogas &scirc;ar&gcirc;atan muron kontra&ubreve; ruini&gcirc;o:
      <ekz>
        la kontra&ubreve;forta sistemo konsistas el apog<tld/>oj kaj apogarkoj
        kaj utilas je baziliko por tio, forkapti la flankan volbo&scirc;ovon kaj la vent&scirc;ar&gcirc;on de meznavo
        kaj alt&hcirc;orejo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=203798">Gotika arkitekturo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_n;&c_t;&c_a;&c_r;&c_f;&c_o;&c_r;&c_s;</trd>
  <trd lng="de">St&uuml;tzpfeiler</trd>
  <trdgrp lng="en">
    <trd>abutment</trd>,
    <trd>buttress</trd>
  </trdgrp>
  <trd lng="fr">contrefort</trd>
  <trd lng="nl">steunbeer</trd>
  <trd lng="hu">t&aacute;mpill&eacute;r</trd>
  <trd lng="uk">&c_g;&#x43E;&#x43D;&c_t;&c_r;&c_f;&#x43E;&c_r;&c_s;</trd>
</drv>
</art>
