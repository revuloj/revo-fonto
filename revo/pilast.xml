<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>pilastr</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pilast.0o">
  <kap><tld/>o</kap>
  <snc mrk="pilast.0o.ARKI">
  <uzo tip="fak">ARKI</uzo>
    <ref tip="dif" cel="kolon.mur0o.ARKI">murkolono</ref>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_l;&c_ja;&c_s;&c_t;&c_r;&c_a;</trd>
  <trd lng="cs">pilastr</trd>
  <trdgrp lng="de">
    <trd>Pilaster</trd>,
    <trd>viereckiger <ind>Wandpfeiler</ind></trd>
  </trdgrp>
  <trd lng="en">pilaster</trd>
  <trd lng="fr">pilastre</trd>
  <trd lng="hu">pilaszter</trd>
  <trd lng="nl">pilaster</trd>
  <trdgrp lng="pl">
    <trd>pilaster</trd>
    <trd>pilastr</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_i;&c_l;&c_ja;&c_s;&c_t;&c_r;</trd>
  <trd lng="sk">pilaster</trd>
</drv>

<drv mrk="pilast.apog0o">
  <kap>apog<tld/>o</kap>
  <snc mrk="pilast.apog0o.ARKI">
  <uzo tip="fak">ARKI</uzo>
    <dif>
      <tld lit="P"/>a <ref tip="super" cel="kolon.0o.ARKI">kolono</ref> 
      kiu apogas &scirc;ar&gcirc;atan muron kontra&ubreve; ruini&gcirc;o. 
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>abutment</trd>,
    <trd>buttress</trd>
  </trdgrp>
  <trd lng="fr">contrefort</trd>
  <trd lng="nl">steunbeer</trd>
  <trd lng="hu">t&aacute;mpill&eacute;r</trd>
</drv>
</art>
