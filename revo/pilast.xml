<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>pilastr</rad>/o
</kap>
<drv mrk="pilast.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ARKI</uzo>
  <snc>
    <dif>
      Kvadrata kolono, plejofte parte enigita en muro.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Pilaster</trd>,
    <trd>viereckiger <ind>Wandpfeiler</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>pilastre</trd>,
    <trd>pilier</trd>
  </trdgrp>
  <trd lng="nl">pilaster</trd>
  <trd lng="ru">&c_p;&c_i;&c_l;&c_ja;&c_s;&c_t;&c_r;</trd>
</drv>

<drv mrk="pilast.apog0o">
  <kap>apog<tld/>o</kap>
  <uzo tip="fak">ARKI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="pilast.0o"><tld/>o</ref> kiu apogas
      &scirc;ar&gcirc;atan muron kontra&ubreve; ruini&gcirc;o. 
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>contrefort</trd>
  </trdgrp>
</drv>
</art>
