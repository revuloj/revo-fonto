  <trdgrp lng="pl">
    <trd>przebija&cacute;</trd>,
    <trd>zerwa&cacute;</trd>
  </trdgrp>   
  <trd lng="ro">str&abreve;punge</trd>
  <trd lng="sk">prielom</trd>
</drv>

