<drv mrk="romp.kol0a">
  <kap>kol<tld/>a</kap>
  <fnt><bib>PV</bib>, <lok>kapo: ~rompa</lok></fnt>
  <uzo tip="klr">(pri korpa lerta&jcirc;o)</uzo>
  <snc mrk="romp.kol0a.KOMUNE">
    <dif>
      Tre dan&gcirc;era, tre riska:
      <ekz>
        la perditoj ne kredis, ke tiom kol<tld/>a entrepreno povus
        sukcesi    
        <fnt>
          <aut>Franti&scaron;ek Omelka</aut>
          <vrk><url ref="https://eo.wikisource.org/wiki/La_Alaska_Stafeto_kaj_Kaptitoj_de_la_Glacirokoj/Kaptitoj_de_la_Glacirokoj/%C4%88apitro_VIII">Kaptitoj
          de la Glacirokoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i stumblas, maltrafas la lastan &scirc;tupon, kaj post kol<tld/>a
        cirklosalto &scirc;i retrovi&gcirc;as ku&scirc;anta sur la planko de la halo
        <fnt>
          <aut>Louis Beaucaire</aut>
          <vrk><url ref="https://pastebin.com/HwmRaYKy">Kruko kaj Baniko
          el Bervalo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        multminute li improvizis komence per kol<tld/>a virtuozeco
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Erroll_Garner">Erroll
          Garner</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">halsbrecherich</trd>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_o;&c_p;&c_a;&c_s;&c_je;&c_n;</trd>,
    <trd>&c_r;&c_i;&c_s;&c_k;&c_o;&c_v;&c_a;&c_n;</trd>,
    <trd>&c_s;&c_m;&c_malmol;&c_r;&c_t;&c_o;&c_n;&c_o;&c_s;&c_je;&c_n;</trd>
  </trdgrp>
  <trd lng="cs">krkolomn&yacute;</trd>
  <trd lng="en">fatal</trd>
  <trd lng="es">peligroso</trd>
  <trdgrp lng="fr">
    <trd>casse-cou<klr tip="ind"> (adj., p&eacute;rilleux)</klr></trd>,
    <trd>p&eacute;rilleux</trd>
  </trdgrp>
  <trd lng="hu">nyakt&ouml;r&odblac;</trd>
  <trdgrp lng="it">
      <trd>mortale</trd>,
      <trd>a <ind>rompicollo</ind></trd>
      <trd>a <ind>rotta di collo</ind></trd>,
      <trd>da <ind>rompersi il collo</ind></trd>
     </trdgrp>
  <trd lng="nl">fataal</trd>
  <trd lng="pl">karko&lstroke;omny</trd>     
  <trd lng="ro">riscant</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_p;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_i;&c_s;&c_k;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_g;&c_i;&c_b;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
