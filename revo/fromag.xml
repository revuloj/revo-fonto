<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fromag.xml,v 1.27 2024/09/17 21:25:21 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>froma&gcirc;</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="fromag.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:man&gcirc;a&jcirc;oj" cel="mangx.0ajxo">Man&gcirc;a&jcirc;o</ref>
      farita el <ref cel="kaze.0igxi">kazeigita</ref> lakto:
      <ekz>
        &ccirc;i tiujn dek <tld/>ojn alportu al la milestro
        <fnt>
          <bib>MT</bib>
          <lok>I. samuel 17:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        en fremda lando oni ne povas &ccirc;iutage trovi &scirc;elon de <tld/>o kaj lardha&ubreve;ton
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la provizejo, kie <tld/>oj ku&scirc;as sur la bretoj kaj &scirc;inkoj pendas sub la plafono
        <fnt>
          <bib>Fab2</bib>
          <lok>Abio</lok>
        </fnt>;
      </ekz>
      <ekz>
        nun denove venos sur nian tablon laktaj man&gcirc;a&jcirc;oj, butero kaj <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Kion la patro faras, estas &ccirc;iam &gcirc;usta</lok>
        </fnt>;
      </ekz>
      <ekz>
        penicilioj kiuj estas aplikataj &ccirc;e la produktado de kamemberta <tld/>o
        <klr>(vd <ref tip="sub" cel="kamembert.0o">kamemberto</ref>)</klr> <klr>[&#8230;]</klr> ne eltenas bluan lumon
        <fnt>
          <vrk><url ref="http://esperantaretradio.blogspot.com/2013/11/lumo-atakas-simon.html?view=snapshot"
              >Lumo atakas &scirc;imon</url></vrk>
          <lok>Esperanta Retradio, 2013-11-22</lok>
        </fnt>.
      </ekz>
      <ekz>
        nur pano kun <tld/>o, sed afabla viza&gcirc;o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        muelita &scirc;af<tld/>o nomi&gcirc;as slovake &leftquot;<nac>bryndza</nac>&rightquot;
        <klr>(vd <ref tip="lst" lst="voko:man&gcirc;a&jcirc;oj" cel="brindz.0o">brindzo</ref>)</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2013/011027.php">Oro blanka</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_r;</trd>
  <trdgrp lng="bg">
    <trd>&c_s;&c_i;&c_r;&c_je;&c_n;&c_je;</trd>,
    <trd>&c_k;&c_a;&c_sh;&c_k;&c_a;&c_v;&c_a;&c_l;</trd>
  </trdgrp>
  <trd lng="bo">&t_pha;&t_yata;&t_shabchu;&t_ra;&t_tsheg;&t_ba;&t_tsheg;</trd>
  <trd lng="cs">s&yacute;r</trd>
  <trd lng="de">K&auml;se</trd>
  <trd lng="en">cheese</trd>
  <trd lng="es">queso</trd>
  <trd lng="fa">&f_p;&a_n;&f_y;&a_r;</trd>
  <trd lng="fr">fromage</trd>
  <trd lng="hu">sajt</trd>
  <trd lng="id">keju</trd>
  <trd lng="it">formaggio</trd>
  <trd lng="ja">&#12481;&#12540;&#12474;  <pr>&#12385;&#12540;&#12378;</pr></trd>
  <trd lng="nl">kaas</trd>
  <trd lng="no">ost</trd>
  <trd lng="pl">ser<klr> (g&lstroke;&oacute;wnie chodzi o
  &zdot;&oacute;&lstroke;ty)</klr></trd>
  <trd lng="pt">queijo</trd>
  <trd lng="ro">br&acirc;nz&abreve;</trd>
  <trd lng="ru">&c_s;&c_y;&c_r;</trd>
  <trd lng="sl">sir</trd>
  <trd lng="tr">peynir</trd>
  <trd lng="uk">&c_s;&c_i;&c_r;</trd>
  <trd lng="zh">&#22902;&#37226;</trd>
</drv>

<drv mrk="fromag.rokforta_0o">
  <kap>
    rokforta <tld/>o
  </kap>
  <snc>
    <dif>
      &Scirc;aflakta <tld/>o kun blu&scirc;imo el la regiono &ccirc;irka&ubreve; la franca vila&gcirc;o
      <nom>Roquefort-sur-Soulzon</nom>:
      <ekz>
        la problemo de la malgranda diverseco de la mikroorganismoj anka&ubreve; ekzistas &ccirc;e
        aliaj froma&gcirc;ospecoj, ekzemple &ccirc;e la rokforta <tld/>o
        <fnt>
          <vrk><url ref="http://esperantaretradio.blogspot.com/2024/02/la-kamemberta-fungo-estas-finigonta.html"
              >La kamemberta fungo estas fini&gcirc;onta</url></vrk>
          <lok>Esperanta Retradio, 2024-02-24</lok>
        </fnt>;
      </ekz>
      <ekz>
        enmetu unu nukseton da &ccirc;okolada krema&jcirc;o, unu nukseton da rokforta <tld/>o kaj
        tran&ccirc;a&jcirc;eton da banano
        <fnt>
          <aut>Alain Fagot</aut>
          <vrk><url ref="http://fagot.alain.free.fr/kuirado/Rpf06/html/CxS21.php"
              >&Ccirc;okoladaj profiteroloj kun rokforto kaj banano</url></vrk>
        </fnt>;
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/9/92/Roquefort_cheese.jpg/602px-Roquefort_cheese.jpg"
      prm="CC BY-SA 3.0"
      >rokforta <tld/>o
        <fnt>
          <lok><url ref="&WCW;/File:Roquefort_cheese.jpg"
              >https://commons.wikimedia.org/wiki/File:Roquefort_cheese.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="de">Roquefortk&auml;se</trd>
  <trd lng="en">roquefort</trd>
  <trd lng="fr">roquefort <klr>(fromage)</klr></trd>
</drv>

</art>
