<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fromag.xml,v 1.31 2024/09/24 06:39:12 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>froma&gcirc;</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="fromag.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:man&gcirc;a&jcirc;oj" cel="mangx.0ajxo">Man&gcirc;a&jcirc;o</ref>
      farita el <ref cel="kaze.0igxi">kazeigita</ref> lakto:
      <ekz>
        &ccirc;i tiujn dek <tld/>ojn alportu al la milestro
        <fnt>
          <bib>MT</bib>
          <lok>I. samuel 17:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        en fremda lando oni ne povas &ccirc;iutage trovi &scirc;elon de <tld/>o kaj lardha&ubreve;ton
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la provizejo, kie <tld/>oj ku&scirc;as sur la bretoj kaj &scirc;inkoj pendas sub la plafono
        <fnt>
          <bib>Fab2</bib>
          <lok>Abio</lok>
        </fnt>;
      </ekz>
      <ekz>
        nun denove venos sur nian tablon laktaj man&gcirc;a&jcirc;oj, butero kaj <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Kion la patro faras, estas &ccirc;iam &gcirc;usta</lok>
        </fnt>;
      </ekz>
      <ekz>
        penicilioj kiuj estas aplikataj &ccirc;e la produktado de kamemberta <tld/>o
        <klr>(vd <ref tip="sub" cel="kamembert.0o">kamemberto</ref>)</klr> <klr>[&#8230;]</klr> ne eltenas
        bluan lumon
        <fnt>
          <vrk><url
          ref="http://esperantaretradio.blogspot.com/2013/11/lumo-atakas-simon.html?view=snapshot"
              >Lumo atakas &scirc;imon</url></vrk>
          <lok>Esperanta Retradio, 2013-11-22</lok>
        </fnt>.
      </ekz>
      <ekz>
        blank&scirc;ima, <ind>blu&scirc;ima <tld/>o</ind>;
        <trd lng="de">Blau&scirc;chimmelk&auml;se</trd>
      </ekz>
      <ekz>
        <ind>selakta <tld/>o</ind>
        <trd lng="de">Molkenk&auml;se</trd>
        <trd lng="en">whey cheese</trd>
        <trd lng="fr">fromage de lactos&eacute;rum</trd>
        <trd lng="ru">&c_s;&c_y;&c_v;&c_o;&c_r;&c_o;&c_t;&c_o;&c_ch;&c_n;&c_y;&c_j; &c_s;&c_y;&c_r;</trd>
      </ekz>
      <ekz>
        <ind>edama <tld/>o</ind>;
        <trd lng="de">Edamer</trd>
      </ekz>
      <ekz>
        kvin dekagramoj da <ind>ementala froma&gcirc;o</ind>
        <fnt>
          <aut> Jaroslav Ha&scaron;ek, trad. Vladim&iacute;r V&aacute;&ncaron;a</aut>
          <vrk><url ref="https://eduinf.waw.pl/esp/lern/librejo/0051/0026.php"
              >Aventuroj de la brava soldato &Scirc;vejk dum la mondmilito</url></vrk>
          <lok>2. En Budape&scirc;to</lok>
        </fnt>;
        <trd lng="de">Emmentaler</trd>
      </ekz>
      <ekz>
        muelita &scirc;affroma&gcirc;o nomi&gcirc;as slovake &leftquot;<nac>bryndza</nac>&rightquot;
        <klr>(vd <ref tip="lst" lst="voko:man&gcirc;a&jcirc;oj" cel="brindz.0o">brindzo</ref>)</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2013/011027.php">Oro blanka</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        nur pano kun <tld/>o, sed afabla viza&gcirc;o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_r;</trd>
  <trdgrp lng="bg">
    <trd>&c_s;&c_i;&c_r;&c_je;&c_n;&c_je;</trd>,
    <trd>&c_k;&c_a;&c_sh;&c_k;&c_a;&c_v;&c_a;&c_l;</trd>
  </trdgrp>
  <trd lng="bo">&t_pha;&t_yata;&t_shabchu;&t_ra;&t_tsheg;&t_ba;&t_tsheg;</trd>
  <trd lng="cs">s&yacute;r</trd>
  <trd lng="de">K&auml;se</trd>
  <trd lng="en">cheese</trd>
  <trd lng="es">queso</trd>
  <trd lng="fa">&f_p;&a_n;&f_y;&a_r;</trd>
  <trd lng="fr">fromage</trd>
  <trd lng="hu">sajt</trd>
  <trd lng="id">keju</trd>
  <trd lng="it">formaggio</trd>
  <trd lng="ja">&#12481;&#12540;&#12474;  <pr>&#12385;&#12540;&#12378;</pr></trd>
  <trd lng="nl">kaas</trd>
  <trd lng="no">ost</trd>
  <trd lng="pl">ser<klr> (g&lstroke;&oacute;wnie chodzi o
  &zdot;&oacute;&lstroke;ty)</klr></trd>
  <trd lng="pt">queijo</trd>
  <trd lng="ro">br&acirc;nz&abreve;</trd>
  <trd lng="ru">&c_s;&c_y;&c_r;</trd>
  <trd lng="sl">sir</trd>
  <trd lng="tr">peynir</trd>
  <trd lng="uk">&c_s;&c_i;&c_r;</trd>
  <trd lng="zh">&#22902;&#37226;</trd>
</drv>

<drv mrk="fromag.kapro0o">
  <kap>
    kapro<tld/>o, <var><kap>kaprin<tld/>o</kap></var>
  </kap>
  <snc>
    <dif>
      <tld/>o farita el kapra lakto:
      <ekz>
        toasto kun kapro<tld/>o
        <fnt>
          <vrk><url ref="https://gresillon.org/spip.php?page=article&amp;id_article=102&amp;lang=eo"
              >Menuoj dum sta&gcirc;oj </url></vrk>
          <lok>Kastelo Greziljono</lok>
        </fnt>;
      </ekz>
      <ekz>
        fortran&ccirc;u la krustojn de la kapro<tld/>o kaj pecetigu &gcirc;in sur la kantarelojn
        <fnt>
          <aut>Ingrid Vallin</aut>
          <vrk><url ref="https://www.esperanto-gbg.org/oraklubo/Ora_Jxurnalo_n-ro_21.pdf"
              >Kremeca kantarela pasta&jcirc;o</url></vrk>
          <lok>Ora &Jcirc;urnalo, n-ro 21, Septembro 2019, p. 44</lok>
        </fnt>;
      </ekz>
      <ekz>
        spertis ili pri <klr>[&#8230;]</klr> preparo de kaprin<tld/>o
        <fnt>
          <aut>Jean Ferrat, trad. Jo-Petro Danvy</aut>
          <vrk><url ref="https://kantaro.ikso.net/la_montaro"
              >La montaro</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Ziegenk&auml;se</trd>
  <trd lng="en">goat cheese</trd>
  <trd lng="fr">fromage de ch&egrave;vre</trd>
</drv>

<drv mrk="fromag.sxaf0o">
  <kap>
    &scirc;af<tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="F"/>o farita el &scirc;aflakto:
      <ekz>
        la slovakoj &scirc;atas muelitan &scirc;af<tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2013/011027.php"
              >Oro blanka</url></vrk>
          <lok>2013</lok>
        </fnt>.

      </ekz>
    </dif>
  </snc>
  <trd lng="de">Schafsk&auml;se</trd>
  <trd lng="en">sheep milk cheese</trd>
  <trd lng="fr">fromages au lait de brebis</trd>
</drv>

<drv mrk="fromag.gorgonzola_0o">
  <kap>
    gorgonzola <tld/>o
  </kap>
  <snc>
    <dif>
       Blu&scirc;ima froma&gcirc;o, nomita la&ubreve; la lombarda urbo <nom>Gorgonzola</nom>:
       <ekz>
        por la deserto piroj kaj gorgonzola <tld/>o
        <fnt>
          <aut>Hermann Hesse, trad. Wolfram Diestel</aut>
          <vrk><url ref="http://www.autodidactproject.org/other/hesse-klinsor.html"
              >La Lasta Somero de Klinsor </url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Gorgonzola</trd>
  <trd lng="en">gorgonzola</trd>
  <trd lng="fr">gorgonzola</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_g;&c_o;&c_n;&c_d;&c_z;&c_o;&c_l;&c_a;</trd>
</drv>

<drv mrk="fromag.parma_0o">
  <kap>
    parma <tld/>o
  </kap>
  <snc>
    <dif>
      Malmola <ref tip="super" cel="fromag.0o">froma&gcirc;o</ref> el senkremigita bovlakto,
      nomita la&ubreve; la itala regiono &ccirc;irka&ubreve; <nom>Parma</nom>, kiun oni metas
      raspita sur diversajn man&gcirc;a&jcirc;ojn, aparte nudelpladojn:
      <ekz>
        por doni pli akran guston, vi povas aldoni 50 &gcirc;is 100 g da raspita Parma <tld/>o
        <fnt>
          <aut>T&uacute;lio Flores</aut>
          <vrk><url ref="http://www.karavelo.net/html/pdf/LK_023_RV.pdf"
              >Froma&gcirc;bulkoj</url></vrk>
          <lok>La Karavelo, n-ro 23, okt. 2010, p. 24</lok>
        </fnt>;
      </ekz>
      <ekz>
        en iuj landoj a&ccirc;eteblas ementala, grujera, parma <tld/>o <klr>[&#8230;]</klr> <tld/>o
        jam raspita
        <fnt>
          <vrk><url ref="https://eo.wikibooks.org/wiki/Kuirlibro:Kapsikoj_kun_tinusa_far%C4%89o"
              >Kuirlibro:Kapsikoj kun tinusa far&ccirc;o</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        oni faras peston el bazilio, ajlo, pinsemoj, parma <tld/>o kaj olivoleo
        <fnt>
          <aut>Eriketo</aut>
          <vrk><url ref="https://eriketo.blogspot.com/2018/06/kreskas-duonligna-periljo.html"
              >Kreskas duonligna periljo </url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Parmesan</trd>
  <trd lng="en">parmesan</trd>
  <trd lng="fr">parmigiano</trd>
  <trd lng="it">parmigiano</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_m;&c_je;&c_z;&c_a;&c_n;</trd>
</drv>

<drv mrk="fromag.rokforta_0o">
  <kap>
    rokforta <tld/>o
  </kap>
  <snc>
    <dif>
      &Scirc;aflakta <tld/>o kun blu&scirc;imo el la regiono &ccirc;irka&ubreve; la franca vila&gcirc;o
      <nom>Roquefort-sur-Soulzon</nom>:
      <ekz>
        la problemo de la malgranda diverseco de la mikroorganismoj anka&ubreve; ekzistas &ccirc;e
        aliaj froma&gcirc;ospecoj, ekzemple &ccirc;e la rokforta <tld/>o
        <fnt>
          <vrk><url
          ref="http://esperantaretradio.blogspot.com/2024/02/la-kamemberta-fungo-estas-finigonta.html"
              >La kamemberta fungo estas fini&gcirc;onta</url></vrk>
          <lok>Esperanta Retradio, 2024-02-24</lok>
        </fnt>;
      </ekz>
      <ekz>
        enmetu unu nukseton da &ccirc;okolada krema&jcirc;o, unu nukseton da rokforta <tld/>o kaj
        tran&ccirc;a&jcirc;eton da banano
        <fnt>
          <aut>Alain Fagot</aut>
          <vrk><url ref="http://fagot.alain.free.fr/kuirado/Rpf06/html/CxS21.php"
              >&Ccirc;okoladaj profiteroloj kun rokforto kaj banano</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/9/92/Roquefort_cheese.jpg/602px-Roquefort_cheese.jpg"
      prm="CC BY-SA 3.0"
      >rokforta <tld/>o
        <fnt>
          <lok><url ref="&WCW;/File:Roquefort_cheese.jpg"
              >https://commons.wikimedia.org/wiki/File:Roquefort_cheese.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="de">Roquefortk&auml;se</trd>
  <trd lng="en">roquefort</trd>
  <trd lng="fr">roquefort <klr>(fromage)</klr></trd>
</drv>

</art>
