<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fromag.xml,v 1.26 2023/10/14 19:15:46 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>froma&gcirc;</rad>/o
</kap>
<drv mrk="fromag.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:man&gcirc;a&jcirc;oj" cel="mangx.0ajxo">Man&gcirc;a&jcirc;o</ref>
      farita el <ref cel="kaze.0igxi">kazeigita</ref> lakto:
      <ekz>
        &ccirc;i tiujn dek <tld/>ojn alportu al la milestro
        <fnt>
          <bib>MT</bib>
          <lok>I. samuel 17:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        en fremda lando oni ne povas &ccirc;iutage trovi &scirc;elon de <tld/>o kaj lardha&ubreve;ton
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la provizejo, kie <tld/>oj ku&scirc;as sur la bretoj kaj &scirc;inkoj pendas sub la plafono
        <fnt>
          <bib>Fab2</bib>
          <lok>Abio</lok>
        </fnt>;
      </ekz>
      <ekz>
        nun denove venos sur nian tablon laktaj man&gcirc;a&jcirc;oj, butero kaj <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Kion la patro faras, estas &ccirc;iam &gcirc;usta</lok>
        </fnt>;
      </ekz>
      <ekz>
        nur pano kun <tld/>o, sed afabla viza&gcirc;o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        muelita &scirc;affroma&gcirc;o nomi&gcirc;as slovake &leftquot;<nac>bryndza</nac>&rightquot;
        <klr>(vd <ref tip="lst" lst="voko:man&gcirc;a&jcirc;oj" cel="brindz.0o">brindzo</ref>)</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2013/011027.php">Oro blanka</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_r;</trd>
  <trdgrp lng="bg">
    <trd>&c_s;&c_i;&c_r;&c_je;&c_n;&c_je;</trd>,
    <trd>&c_k;&c_a;&c_sh;&c_k;&c_a;&c_v;&c_a;&c_l;</trd>
  </trdgrp>
  <trd lng="bo">&#xF55;&#xFB1;&#xF74;&#xF62;&#xF0B;&#xF56;&#xF0B;</trd>
  <trd lng="cs">s&yacute;r</trd>
  <trd lng="de">K&auml;se</trd>
  <trd lng="en">cheese</trd>
  <trd lng="es">queso</trd>
  <trd lng="fa">&f_p;&a_n;&f_y;&a_r;</trd>
  <trd lng="fr">fromage</trd>
  <trd lng="hu">sajt</trd>
  <trd lng="id">keju</trd>
  <trd lng="it">formaggio</trd>
  <trd lng="ja">&#x30C1;&#x30FC;&#x30BA;  <pr>&#x3061;&#x30FC;&#x305A;</pr></trd>
  <trd lng="nl">kaas</trd>
  <trd lng="no">ost</trd>
  <trd lng="pl">ser<klr> (g&lstroke;&oacute;wnie chodzi o
  &zdot;&oacute;&lstroke;ty)</klr></trd>
  <trd lng="pt">queijo</trd>
  <trd lng="ro">br&acirc;nz&abreve;</trd>
  <trd lng="ru">&c_s;&c_y;&c_r;</trd>
  <trd lng="sl">sir</trd>
  <trd lng="tr">peynir</trd>
  <trd lng="uk">&#x441;&#x438;&#x440;</trd>
  <trd lng="zh">&#x5976;&#x916A;</trd>
</drv>
</art>
