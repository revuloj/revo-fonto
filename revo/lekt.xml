<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lekt.xml,v 1.5 2023/05/14 16:29:08 revo Exp $">
<kap>
  <rad>lekt</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="lekt.0o">
  <kap><tld/>o</kap>
  <snc mrk="lekt.0o.MEBL">
    <dif>
      <ref tip="super" cel="kusx.0ejo.KOMUNE">Ku&scirc;ejo<sncref/></ref>,
      speco de <ref tip="vid" cel="sof.0o">sofo</ref>,
      kian uzis antikvaj romianoj dum man&gcirc;ado:
      <ekz>
        li ne sidis en la brakse&gcirc;o, sed duonku&scirc;is sur <tld/>o &ccirc;e malalta tableto,
        garnita je man&gcirc;a&jcirc;oj kaj vino
        <fnt>
          <bib>MkM</bib>
          <lok>25</lok>
        </fnt>;
      </ekz>
      <ekz>
        pezakorpaj altkastuloj <klr>...</klr> ku&scirc;is sur speco
        de portebla <tld/>o
        <fnt>
          <aut>T. Steele</aut>
          <vrk>Konvinka Kamufla&jcirc;o</vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_je;&c_k;&c_t;&c_u;&c_s;</trd>,
    <trd>&c_l;&c_o;&c_zh;&c_a;&c_k; <klr>(&c_d;&c_l;&c_ja; &c_b;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kline</trd>,
    <trd>Speisesofa</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>lectus</trd>,
    <trd>&lstroke;o&zdot;e</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>lectus</trd>,
    <trd>pat</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_o;&c_b;&c_ib;&c_d;&c_n;&c_jeu; &c_l;&c_o;&c_zh;&c_je;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_a;&c_p;&c_a;</trd>,
    <trd>&c_s;&c_o;&c_f;&c_a; (&c_d;&c_l;&c_ja; &c_b;&c_je;&c_n;&c_k;&c_je;&c_t;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;)</trd>
  </trdgrp>
</drv>
</art>

