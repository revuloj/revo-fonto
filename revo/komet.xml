<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>komet</rad>/o
</kap>
<drv mrk="komet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="astr.0o.1">Astro</ref>, kies vojo &ccirc;irka&ubreve;
      la suno estas tre longforma elipso, kaj kiu kuntrenas longan
      luman strion, kun formo de vosto a&ubreve; hararo.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>kometa</trd>,
    <trd>vlasatice</trd>
  </trdgrp>
  <trd lng="da">komet</trd>
  <trdgrp lng="de">
    <trd>Komet</trd>,
    <trd>Haarstern</trd>,
    <trd>Schweifstern</trd>
  </trdgrp>
  <trd lng="en">comet</trd>
  <trd lng="es">cometa</trd>
  <trdgrp lng="fi">
    <trd>pyrst&ouml;t&auml;hti</trd>,
    <trd>komeetta</trd>
  </trdgrp>
  <trd lng="fr">com&egrave;te</trd>
  <trd lng="hu">&uuml;st&ouml;k&ouml;s</trd>
  <trd lng="it">cometa</trd>
  <trd lng="ja">&#12377;&#12356;&#12379;&#12356; &#24407;&#26143; suisei</trd>
  <trd lng="nl">komeet</trd>
  <trd lng="pl">kometa</trd>
  <trd lng="pt">cometa</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_je;&c_t;&c_a;</trd>
  <trd lng="sv">komet</trd>
  <trd lng="tr">kuyruklu y&inodot;ld&inodot;z</trd>
</drv>
</art>
