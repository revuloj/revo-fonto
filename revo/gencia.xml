<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>gencian</rad>/o
</kap>
<drv mrk="gencia.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="gencia.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Gentiana</trd>),
      herbo treege amara, kun bluaj, flavaj a&ubreve; purpuraj floroj,
      uzata por fari infuza&jcirc;ojn, likvorojn.
    </dif>
  </snc>
  <trd lng="de">Enzian</trd>
</drv>
<drv mrk="gencia.0acoj">
  <kap><tld/>acoj</kap>
  <snc>
    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Gentianaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Enziangew&auml;chse</trd>
</drv>
</art>
