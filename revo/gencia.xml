<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>gencian</rad>/o
</kap>
<drv mrk="gencia.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Genro el la familio <ref tip="super"
      cel="gencia1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Gentiana</trd>),
      herbo treege amara, kun bluaj, flavaj a&ubreve; purpuraj floroj,
      uzata por fari infuza&jcirc;ojn, likvorojn.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_y;&c_ch;&c_k;&c_a;</trd>
  <trd lng="bg">&c_t;&c_i;&c_n;&c_t;&c_ja;&c_v;&c_a;</trd>
  <trd lng="ca">genciana</trd>
  <trd lng="de">Enzian</trd>
  <trd lng="en">gentian</trd>
  <trd lng="fr">gentiane</trd>
  <trdgrp lng="hu">
    <trd>t&aacute;rnics</trd>,
    <trd>enci&aacute;n</trd>
  </trdgrp>
  <trd lng="it">genziana</trd>
  <trd lng="nl">gentiaan</trd>
  <trd lng="pt">genciana</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_je;&c_ch;&c_a;&c_v;&c_k;&c_a;</trd><trdgrp lng="tr">
    <trd>k&inodot;z&inodot;lkantaron</trd>,
    <trd>centiyan</trd>
  </trdgrp>
</drv>

</art>
