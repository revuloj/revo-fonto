<vortaro>
<art mrk="$Id: fotokopi.xml,v 1.11 2023/11/10 06:08:00 revo Exp $">
<kap>
  <rad>fotokopi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="fotokopi.0o">
  <kap><tld/>o</kap>
  <snc mrk="fotokopi.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref tip="super" cel="kopi.0o.kopiajxo">Kopio</ref> de dokumento
      farita per <tld/>ilo:
      <ekz>
        <tld/>on vi ricevos post pago de sendokostoj
        <fnt>
        <bib>Viki</bib>,
        <lok>artikolo &leftquot;klavarskribo&rightquot;, 2005-10-17</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">fotoc&ograve;pia</trd>
  <trd lng="cs">fotokopie</trd>
  <trd lng="de">Fotokopie</trd>
  <trd lng="fa">&a_f;&a_t;&a_w;&f_k;&f_p;&f_y;</trd>
  <trd lng="fr">photocopie</trd>
  <trdgrp lng="ja">
    <trd>&#x5199;&#x771F;&#x8907;&#x5199;  <pr>&#x3057;&#x3083;&#x3057;&#x3093;&#x3075;&#x304F;&#x3057;&#x3083;</pr></trd>,
    <trd>&#x30D5;&#x30A9;&#x30C8;&#x30B3;&#x30D4;&#x30FC;</trd>,
    <trd>&#x5370;&#x753B;  <pr>&#x3044;&#x3093;&#x304C;</pr></trd>
  </trdgrp>
  <trd lng="pt">fotoc&oacute;pia</trd>
  <trd lng="sk">fotok&#xF3;pia</trd>
  <trdgrp lng="zh">
    <trd>&#x5F71;&#x5370;&#x4EF6; <pr>y&#x1D0;ngy&#xEC;nji&#xE0;n</pr></trd>,
    <trd>&#x5149;&#x7535;&#x590D;&#x5370; <pr>gu&#x101;ngdi&#xE0;nf&#xF9;y&#xEC;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fotokopi.0i">
  <kap><tld/>i</kap>
  <snc mrk="fotokopi.0i.TIP">
    <dif>
      Produkti <tld/>ojn de dokumento:
      <ekz>
        &leftquot;<tld/>u!&rightquot; iu sa&gcirc;ulo ekkriis
        <fnt>
        <aut>Wu Hua</aut>,
        <vrk>ekster ekzamenejo</vrk>,
        <lok><url ref="http://www.elerno.net/penseo/pen3.htm">Penseo,
        numero 3a, 1990-09</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">fotocopiar</trd>
  <trdgrp lng="cs">
    <trd>fotokop&#xED;rovat</trd>,
    <trd>kop&#xED;rovat fotograficky</trd>
  </trdgrp>
  <trd lng="de">fotokopieren</trd>
  <trd lng="fa">&a_f;&a_t;&a_w;&f_k;&f_p;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">photocopier</trd>
  <trdgrp lng="ja">
    <trd>&#x5199;&#x771F;&#x8907;&#x5199;&#x3059;&#x308B;  <pr>&#x3057;&#x3083;&#x3057;&#x3093;&#x3075;&#x304F;&#x3057;&#x3083;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x30D5;&#x30A9;&#x30C8;&#x30B3;&#x30D4;&#x30FC;&#x3059;&#x308B;  <pr>&#x30D5;&#x30A9;&#x30C8;&#x3053;&#x3074;&#x30FC;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="sk">fotokop&#xED;rova&#x165;</trd>
  <trdgrp lng="uk">
    <trd>&#x444;&#x43E;&#x442;&#x43E;&#x43A;&#x43E;&#x43F;&#x456;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x440;&#x43E;&#x431;&#x438;&#x442;&#x438; &#x444;&#x43E;&#x442;&#x43E;&#x43A;&#x43E;&#x43F;&#x456;&#x44E;</trd>
  </trdgrp>
  <trd lng="zh">&#x5F71;&#x5370; <pr>y&#x1D0;ngy&#xEC;n</pr></trd>
</drv>

<drv mrk="fotokopi.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="fotokopi.0ilo.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Ma&scirc;ino, kiu produktas kopiojn de dokumento per fotsimila procedo,
      farante lum&jcirc;etan bildon de la originalo.
    </dif>
  </snc>
  <trd lng="ca">fotocopiadora</trd>
  <trd lng="cs">fotografick&#xE1; kop&#xED;rka</trd>
  <trd lng="de">Fotokopierer</trd>
  <trd lng="fa">&a_d;&a_s;&a_t;&f_g;&a_A;&a_h; <ind>&a_f;&a_t;&a_w;&f_k;&f_p;&f_y;</ind></trd>
  <trdgrp lng="fr">
    <trd>photocopieur</trd>,
    <trd>photocopieuse</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5199;&#x771F;&#x8907;&#x5199;&#x6A5F;  <pr>&#x3057;&#x3083;&#x3057;&#x3093;&#x3075;&#x304F;&#x3057;&#x3083;&#x304D;</pr></trd>,
    <trd>&#x30D5;&#x30A9;&#x30C8;&#x30B3;&#x30D4;&#x30FC;&#x6A5F;  <pr>&#x30D5;&#x30A9;&#x30C8;&#x30B3;&#x30D4;&#x30FC;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="sk">fotokop&#xED;rka</trd>
  <trdgrp lng="uk">
    <trd>&#x444;&#x43E;&#x442;&#x43E;&#x43A;&#x43E;&#x43F;&#x456;&#x44E;&#x432;&#x430;&#x43B;&#x44C;&#x43D;&#x438;&#x439; &#x430;&#x43F;&#x430;&#x440;&#x430;&#x442;</trd>,
    <trd>&#x444;&#x43E;&#x442;&#x43E;&#x43A;&#x43E;&#x43F;&#x456;&#x44E;&#x432;&#x430;&#x43B;&#x44C;&#x43D;&#x430; &#x43C;&#x430;&#x448;&#x438;&#x43D;&#x430;</trd>
  </trdgrp>
  <trd lng="zh">&#x5149;&#x7535;&#x590D;&#x5370;&#x673A; <pr>gu&#x101;ngdi&#xE0;nf&#xF9;y&#xEC;nj&#x12B;</pr></trd>
</drv>
