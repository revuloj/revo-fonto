<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>fotokopi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="fotokopi.0o">
  <kap><tld/>o</kap>
  <snc mrk="fotokopi.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref tip="super" cel="kopi.0o.kopiajxo">Kopio</ref> de dokumento
      farita per <tld/>ilo:
      <ekz>
        <tld/>on vi ricevos post pago de sendokostoj
        <fnt>
        <bib>Viki</bib>,
        <lok>artikolo &leftquot;klavarskribo&rightquot;, 2005-10-17</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Fotokopie</trd>
  <trd lng="fr">photocopie</trd>
  <trd lng="pt">fotoc&#243;pia</trd>
</drv>

<drv mrk="fotokopi.0i">
  <kap><tld/>i</kap>
  <snc mrk="fotokopi.0i.TIP">
    <dif>
      Produkti <tld/>ojn de dokumento:
      <ekz>
        &leftquot;<tld/>u!&rightquot; iu sa&gcirc;ulo ekkriis
        <fnt>
        <aut>Wu Hua</aut>,
        <vrk>ekster ekzamenejo</vrk>,
        <lok><url ref="http://www.elerno.net/penseo/pen3.htm">Penseo,
        numero 3a, 1990-09</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">fotokopieren</trd>
  <trd lng="fr">photocopier</trd>
</drv>

<drv mrk="fotokopi.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="fotokopi.0ilo.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Ma&scirc;ino, kiu produktas kopiojn de dokumento per fotsimila procedo,
      farante lum&jcirc;etan bildon de la originalo.
    </dif>
  </snc>
  <trd lng="de">Fotokopierer</trd>
  <trdgrp lng="fr">
    <trd>photocopieur</trd>,
    <trd>photocopieuse</trd>
  </trdgrp>
</drv>
</art>

