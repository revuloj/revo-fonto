<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sufer</rad>/i
</kap>
<drv mrk="sufer.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="sufer.0i.dolore">
    <dif>
      <ref tip="super" cel="sent.0i">Senti</ref>
      doloron korpan a&ubreve; mensan:
      <ekz>
        la popolo tie <tld/>is pro manko de akvo
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 17:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i malsaton
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i de la suno
        <fnt>
          <bib>Fab2</bib>
          <lok>La ombro</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <klr>[&#8230;]</klr> <tld/>is de la frosto
        <fnt>
          <bib>Fab2</bib>
          <lok>Knabineto kun alumetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        li dum longa tempo <tld/>is en malliberejo kaj malvarmo
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia knabo ku&scirc;as paralizulo en la domo, kaj terure <tld/>as
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 8:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        kapo pekas, piedoj <tld/>as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind><tld/>plena</ind> &gcirc;emo
        <fnt>
          <bib>Marta</bib>
        </fnt>.
        <trdgrp lng="ca">
          <trd>desolador</trd>,
          <trd>planyent</trd>,
          <trd>dolen&ccedil;&oacute;s</trd>
        </trdgrp>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dolor.0o">doloro</ref>,
      <ref cel="turmen">turmento</ref>
    </refgrp>
  </snc>
  <snc mrk="sufer.0i.malfacilon">
    <dif>
      <ref tip="super" cel="spert.0i">Sperti</ref>
      malfeli&ccirc;a&jcirc;on, malagrabla&jcirc;on, malprofiton:
      <ekz>
        ni <tld/>is sufi&ccirc;e da malestimo
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 123:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili estis ju&gcirc;itaj indaj <tld/>i malhonoron
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 5:41</lok>
        </fnt>;
      </ekz>
      <ekz>
        longa vico da <tld/>itaj humili&gcirc;oj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>into pli valoras ol lerninto
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        nur <tld/>into &scirc;atas feli&ccirc;on
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        vidu, kion mi <tld/>as de miaj malamikoj
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 9:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        li multe <tld/>is pro siaj opinioj;
      </ekz>
      <ekz>
        multe ni devis <tld/>i de tiu malvasta
        pse&ubreve;dopatriotismo
        <fnt>
          <bib>LR</bib>
          <lok>4. Vortaroj</lok>
        </fnt>;
      </ekz>
    </dif>
    <subsnc mrk="sufer.0i.mankon">
      <dif>
        Esti en malbona stato pro nenormala manko de bezona&jcirc;o:
        <ekz>
          junuloj ofte tro <tld/>as je absoluta manko de
          singardo
          <fnt>
            <bib>Hamlet</bib>
          </fnt>;
        </ekz>
        <ekz>
          la krimromanoj la&ubreve; li <tld/>as pro similaj mankoj kiel aliaj Esperantaj
          verkoj: mallerta tekniko de literatura kreado, stranga lingva&jcirc;o
          <fnt>
            <vrk><url ref="http://www.liberafolio.org/arkivo/www.liberafolio.org/2006/krimromanoj/">Kvin 
            krimaj kvaropoj kaj kelkaj kromaj kadavroj &mdash; Libera Folio</url></vrk>
          </fnt>;
        </ekz>
        <ekz>
          ni estas nur homoj kaj <tld/>as pro homaj difektoj.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="sufer.0i.difektigxon">
      <dif>
        Difekti&gcirc;i, malprosperi:
        <ekz>
          nia negoco tre <tld/>as de tia situacio;
        </ekz>
        <ekz>
          <tld/>i pro, sub krizo;
        </ekz>
        <ekz>
          la internaciaj interrilatoj tre <tld/>as pro la multeco de
          lingvoj;
        </ekz>
        <ekz>
          la reputacio de la nomitaj landoj nun grave <tld/>is
          <fnt>
            <bib>Monato</bib>
          </fnt>;
        </ekz>
        <ekz>
          pro la dioksina skandalo en majo 1999 la belga terkultura industrio forte <tld/>is
          <fnt>
            <bib>Monato</bib>
            <vrk><url ref="http://steloj.de/esperanto/monato2/007272.html">Reen al la edena paradizo</url></vrk>
          </fnt>;
        </ekz>
        <ekz>
          en julio erupciis <nom>Oyama</nom>, sur la insulo <nom>Miyake</nom>, kaj dama&gcirc;is rikoltojn, same <tld/>is turismo
          <fnt>
            <bib>Monato</bib>
            <aut>Hori Jasuo</aut>
            <vrk><url ref="http://steloj.de/esperanto/monato2/007147.html">Erupcias maltrankvilo</url></vrk>
          </fnt>;
        </ekz>
        <ekz>
          la papeco de Romo <tld/>is gravan krizon <esc>depost</esc> la komenco de la 14a jarcento
          <fnt>
            <bib>Monato</bib>
            <aut>Gerrit Berveling</aut>
            <vrk><url ref="http://steloj.de/esperanto/monato2/007031.html">katolikismo: 
             Johano Hus &dash; &ccirc;u ne plu herezulo?</url></vrk>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="mizer.0o">mizero</ref>,
      <ref cel="port.0i">porti</ref>,
      <ref cel="spert.0i">sperti</ref>,
      <ref cel="viktim.0o">viktimo</ref>
    </refgrp>
  </snc>
  <snc mrk="sufer.0i.akcepti">
    <dif>
      Ricevi la efikon de ago, esti trafita de iu ago:
      <ekz>
        la akuzativon ni uzas nur en tri okazoj:
        a) por montri la <tld/>anton de la faro (t.e. post verboj havantaj sencon aktivan) <klr>[&#8230;]</klr>
        <fnt>
          <aut>L. L. Zamenhof</aut>
          <vrk><url ref="http://eduinf.waw.pl/esp/lern/librejo/0015/0008.php">Lingvaj respondoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la konferenco <tld/>is interrompon
        <klr>(estis interrompita)</klr>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_k;&c_u;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_u;&c_ch;&c_y;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>sofrir</trd>,
    <trd>patir</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>str&aacute;dat</trd>,
    <trd>trp&ecaron;t</trd>,
    <trd>trp&ecaron;t</trd>,
    <trd>utrp&ecaron;t</trd>
  </trdgrp>
  <trd lng="de">leiden</trd>
  <trd lng="en">suffer</trd>
  <trd lng="es">sufrir</trd>
  <trdgrp lng="fr">
    <trd>souffrir</trd>,
    <trd>subir</trd>
  </trdgrp>
  <trd lng="hu">szenved</trd>
  <trd lng="nl">lijden</trd>
  <trdgrp lng="pt">
    <trd>sofrer</trd>,
    <trd>penar</trd>,
    <trd>suportar</trd>,
    <trd>ag&uuml;entar</trd>,
    <trd>provar</trd>,
    <trd>aturar</trd>,
    <trd>padecer</trd>,
    <trd>tolerar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_m;&c_u;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>trpie&tcaron;</trd>,
    <trd>vyst&aacute;&tcaron;</trd>,
    <trd>vytrpie&tcaron;</trd>,
    <trd>zn&aacute;&scaron;a&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_t;&c_r;&c_a;&c_zh;&c_d;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="sufer.0o">
  <kap><tld/>o</kap>
  <snc mrk="sufer.0o.KOMUNE">
    <dif>
      Sento, dama&gcirc;o de tiu, kiu <tld/>as:
      <ekz>
        elporti tiom da feli&ccirc;oj kaj tiom da <tld/>oj mi ne
        povas<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mieno lia montris pli <tld/>on, sed ne
        koleron<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>ego lin atakis, doloro kiel &ccirc;e naskantino<fnt>Z</fnt>;
      </ekz>
      <ekz>
        transiro de <tld/>oj al &gcirc;ojo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>patiment</trd>,
    <trd>angoixa</trd>,
    <trd>malestar</trd>,
    <trd>dolen&ccedil;a</trd>
  </trdgrp>
  <trd lng="fr">souffrance</trd>
</drv>

<drv mrk="sufer.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sufer.0igi.KOMUNE">
    <dif>
      Ka&ubreve;zi <tld/>on:
      <ekz>
        tran&ccirc;i al si la piedojn kaj sin <tld/>igi<fnt>Z</fnt>
        (<ref tip="vid" cel="vund.0i">vundi</ref>);
      </ekz>
      <ekz>
        la varmego lin <tld/>igis<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tertremo <tld/>igis la insulon;
      </ekz>
      <ekz>
        la insulo estas <tld/>igita de la tertremo.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">faire <ind>souffrir</ind></trd>
</drv>
</art>
