    regulaj formoj ol &leftquot;da&ubreve;o&rightquot;, &leftquot;da&ubreve;ismo&rightquot;, &leftquot;da&ubreve;isto&rightquot;.
    <aut>&jcirc;v</aut>
  </rim>
  <trdgrp lng="fr">
    <trd>tao&iuml;sme</trd>
  </trdgrp>
  <trd lng="hu">taoizmus</trd>
  <trdgrp lng="pt">
   <trd>taoismo</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_a;&c_o;&c_s;&c_i;&c_z;&c_m;</trd>
  <trd lng="zh">&#36947;&#25945;<klr> [d&agrave;oji&#257;o]</klr></trd>
</drv>
