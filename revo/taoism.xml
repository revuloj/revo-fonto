<drv mrk="taoism.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">FIL</uzo>
    <uzo tip="fak">REL</uzo>
    <ref tip="dif" cel="daux.0ismo">da&ubreve;ismo</ref>
  </snc>
  <rim mrk="taoism.0o.RIM">
