<drv mrk="last.antaux0a">
  <kap>anta&ubreve;<tld/>a</kap>
  <snc>
    <dif>
      &Gcirc;uste anta&ubreve;anta la <tld/>an:
      <ekz>
        flugante preter la anta&ubreve;<tld/>a fenestro
        de la kvara eta&gcirc;o Margarita rigardis internen kaj vidis viron
        <fnt><bib>MkM</bib><lok>&ccirc;ap. 21</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_d;&c_a;&c_p;&c_o;&c_sh;&c_n;&c_ib;</trd>
  <trd lng="br">eil <ind>diwezha&ntilde;</ind></trd>
