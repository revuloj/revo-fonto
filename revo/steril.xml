<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>steril</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="steril.0a">
  <kap><tld/>a</kap>
  <snc mrk="steril.0a.KOMUNE">
    <dif>
      Senfrukta, nekapabla naski a&ubreve; produkti:
      <ekz>
        <tld/>a lando, bovino, planto, jaro, peno, diskuto.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_ja;&c_s;&c_p;&c_l;&c_o;&c_d;&c_n;&c_y;</trd>
    <trdgrp lng="hu">
      <trd>medd&odblac;</trd>,
      <trd>magtalan</trd>,
      <trd>term&eacute;ketlen</trd>
    </trdgrp>
    <trd
    lng="ru">&c_b;&c_je;&c_s;&c_p;&c_l;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="steril.0a.BIO">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">MED</uzo>
    <dif>
      Libera je &ccirc;ia viva &gcirc;ermo, speciale je
      <ref tip="vid" cel="mikrob.0o">mikroboj</ref>,
      <ref tip="vid" cel="bacil.0o">baciloj</ref>
      a&ubreve;
      <ref tip="vid" cel="bakter.0o">bakterioj</ref>:
      <ekz>
        <ind><tld/>a pansa&jcirc;o</ind>.
        <trd lng="hu">steril k&ouml;t&eacute;s</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_t;&c_e;&c_r;&c_y;&c_l;&c_mol;&c_n;&c_y;</trd>
    <trd lng="hu">steril</trd>    
    <trd
    lng="ru">&c_s;&c_t;&c_je;&c_r;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trd lng="de">steril</trd>
  <trd lng="fr">st&eacute;rile</trd>
  <trd lng="nl">steriel</trd>
</drv>

<drv mrk="steril.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="steril.0igi.BIO">
  <uzo tip="fak">BIO</uzo>
  <uzo tip="fak">MED</uzo>
    <dif>
      Igi <tld/>a:
      <ekz>
        <tld/>igi lakton;
      </ekz>
      <ekz>
        <ind><tld/>igilo</ind>.
        <trd lng="be">&c_s;&c_t;&c_e;&c_r;&c_y;&c_l;&c_ib;&c_z;&c_a;&c_t;&c_a;&c_r;</trd>
        <trd lng="fr">st&eacute;rilisateur</trd>
        <trd lng="hu">steriliz&aacute;tor</trd>
        <trd
        lng="ru">&c_s;&c_t;&c_je;&c_r;&c_i;&c_l;&c_i;&c_z;&c_a;&c_t;&c_o;&c_r;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_e;&c_r;&c_y;&c_l;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">sterilisieren</trd>
  <trd lng="fr">st&eacute;riliser</trd>
  <trdgrp lng="hu">
    <trd>cs&iacute;r&aacute;tlan&iacute;t</trd>,
    <trd>fert&odblac;tlen&iacute;t</trd>,
    <trd>steriliz&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">steriliseren</trd>
  <trd
  lng="ru">&c_s;&c_t;&c_je;&c_r;&c_i;&c_l;&c_i;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
