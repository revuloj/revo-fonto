<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>steril</rad>/a
</kap>
<drv mrk="steril.0a">
  <kap><tld/>a</kap>
  <snc num="1">
    <dif>
      Senfrukta, nekapabla naski a&ubreve; produkti:
      <ekz>
        <tld/>a lando, bovino, planto, jaro, peno, diskuto.
      </ekz>
    </dif>
    <trd lng="ru">&c_b;&c_je;&c_s;&c_p;&c_l;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc num="2">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">MED</uzo>
    <dif>
      Libera je &ccirc;ia viva &gcirc;ermo, speciale je
      <ref tip="vid" cel="mikrob.0o">mikroboj</ref>,
      <ref tip="vid" cel="bacil.0o">baciloj</ref> a&ubreve;
      <ref tip="vid" cel="bakter.0o">bakterioj</ref>:
      <ekz><tld/>a pansa&jcirc;o.</ekz>
    </dif>
    <trd lng="ru">&c_s;&c_t;&c_je;&c_r;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </snc>
</drv>
<drv mrk="steril.0igi">
  <kap><tld/>igi</kap> 
  <uzo tip="fak">BIO</uzo>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Igi <tld/>a:
      <ekz><tld/>igi lakton;</ekz>
      <ekz>
        <ind><tld/>igilo</ind>.
        <trd lng="ru">&c_s;&c_t;&c_je;&c_r;&c_i;&c_l;&c_i;&c_z;&c_a;&c_t;&c_o;&c_r;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_r;&c_i;&c_l;&c_i;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>

