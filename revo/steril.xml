<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: steril.xml,v 1.21 2023/10/21 07:46:48 revo Exp $">
<kap>
  <rad>steril</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="steril.0a">
  <kap><tld/>a</kap>
  <snc mrk="steril.0a.KOMUNE">
    <dif>
      <ref cel="frukt.sen0a.sterila">Senfrukta<sncref/></ref>,
      nekapabla naski a&ubreve; produkti:
      <ekz>
        <tld/>a bovino, planto;
      </ekz>
      <ekz>
        <tld/>a transgena sojo
        <fnt>
          <bib>MD02</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiam okazas mutacio, ne nur unu nova speco naski&gcirc;as, sed <klr>[pluraj]</klr>
        post iu tempo montri&gcirc;os <tld/>aj
        <fnt>
          <bib>BonaLingvo</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_ja;&c_s;&c_p;&c_l;&c_o;&c_d;&c_n;&c_y;</trd>
    <trdgrp lng="fa">
      <trd>&a_n;&a_A;&a_z;&a_A;</trd>,
      <trd>&a_ayn;&a_q;&f_y;&a_m;</trd>,
      <trd>&a_n;&a_A;&a_H;&a_A;&a_S;&a_l;&a_kh;&f_y;&a_z;</trd>,
      <trd>&a_b;&a_A;&f_y;&a_r;</trd>,
      <trd>&a_l;&a_m;&zwnj;&f_y;&a_z;&a_r;&a_ayn;</trd>,
      <trd>&a_b;&f_y;&zwnj;&a_th;&a_m;&a_r;</trd>,
      <trd>&a_b;&f_y;&zwnj;&a_H;&a_A;&a_S;&a_l;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>medd&odblac;</trd>,
      <trd>magtalan</trd>,
      <trd>term&eacute;ketlen</trd>
    </trdgrp>
    <trd lng="pl">sterylny</trd>
    <trd lng="ru">&c_b;&c_je;&c_s;&c_p;&c_l;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="steril.0a.BIO">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">MED</uzo>
    <dif>
      Libera je &ccirc;ia viva &gcirc;ermo, speciale je
      <ref tip="vid" cel="mikrob.0o">mikroboj</ref>,
      <ref tip="vid" cel="bacil.0o">baciloj</ref>
      a&ubreve;
      <ref tip="vid" cel="bakter.0o">bakterioj</ref>:
      <ekz>
        <ind><tld/>a pansa&jcirc;o</ind>;
        <trd lng="hu">steril k&ouml;t&eacute;s</trd>
      </ekz>
      <ekz>
        la marsa medio estas <klr>[&#8230;]</klr> <tld/>a kaj toksa
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2010/009989.php">Voja&gcirc;o unudirekta</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_t;&c_e;&c_r;&c_y;&c_l;&c_mol;&c_n;&c_y;</trd>
    <trdgrp lng="fa">
      <trd>&a_D;&a_d;&a_ayn;&a_f;&a_w;&a_n;&f_y; &a_sh;&a_d;&a_h;</trd>,
      <trd>&a_A;&a_s;&a_t;&a_r;&f_y;&a_l;</trd>
    </trdgrp>
    <trd lng="hu">steril</trd>
    <trd lng="pl">bezp&lstroke;odny</trd>
    <trd lng="ru">&c_s;&c_t;&c_je;&c_r;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="steril.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ne pu&scirc;anta la aferon anta&ubreve;en, <ref cel="frukt.sen0a.FIG">senfrukta<sncref/></ref>,
      <ref cel="hav.senen0a">senenhava</ref>, <ref cel="sprit.mal0a">malsprita</ref>:
      <ekz>
        movado militanta sur la kultura kampo, povas progresi ne per <tld/>aj disputoj,
        sed <klr>[nur]</klr> per grandaj fortostre&ccirc;oj por plenumi demokrate
        vo&ccirc;donitajn decidojn
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi devis mal&gcirc;oje konstati, ke la tuta teologio estas <tld/>a, senbaza kaj vana
        <fnt>
       <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2000/006847.php">Sobraj pensoj de sobrulo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 22</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fekund.mal0a">malfekunda</ref>,
      <ref cel="plen.mal0a">malplena</ref>
    </refgrp>
    <trd lng="be">&c_s;&c_t;&c_e;&c_r;&c_y;&c_l;&c_mol;&c_n;&c_y; <klr>(&c_p;&c_je;&c_r;&c_a;&c_n;.)</klr></trd>
  <trd lng="pl">ja&lstroke;owy</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>jalov&yacute;</trd>,
    <trd>neplodn&yacute;</trd>,
    <trd>steriln&iacute;</trd>
  </trdgrp>
  <trd lng="de">steril</trd>
  <trd lng="en">sterile</trd>
  <trd lng="es">est&eacute;ril</trd>
  <trd lng="fa">&a_s;&a_t;&a_r;&a_w;&a_n;</trd>
  <trd lng="fr">st&eacute;rile</trd>
  <trdgrp lng="ja">
    <trd>&#19981;&#27611;&#12398;  <pr>&#12405;&#12418;&#12358;&#12398;</pr></trd>,
    <trd>&#23455;&#12426;&#12398;&#12394;&#12356;  <pr>&#12415;&#12398;&#12426;&#12398;&#12394;&#12356;</pr></trd>,
    <trd>&#19981;&#31252;&#12398;  <pr>&#12405;&#12415;&#12398;&#12426;&#12398;</pr></trd>,
    <trd>&#23455;&#12398;&#12394;&#12425;&#12394;&#12356;  <pr>&#12376;&#12388;&#12398;&#12394;&#12425;&#12394;&#12356;</pr></trd>,
    <trd>&#19981;&#22922;&#12398;  <pr>&#12405;&#12395;&#12435;&#12398;</pr></trd>,
    <trd>&#23376;&#12364;&#12391;&#12365;&#12394;&#12356;  <pr>&#12371;&#12364;&#12391;&#12365;&#12394;&#12356;</pr></trd>,
    <trd>&#28961;&#33740;&#12398;  <pr>&#12416;&#12365;&#12435;&#12398;</pr></trd>,
    <trd>&#28357;&#33740;&#12375;&#12383;  <pr>&#12417;&#12387;&#12365;&#12435;&#12375;&#12383;</pr></trd>
  </trdgrp>
  <trd lng="nl">steriel</trd>
  <trdgrp lng="sk">
    <trd>neplodn&yacute;</trd>,
    <trd>steriln&yacute;</trd>,
    <trd>zdravotne nez&aacute;vadn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_je;&c_r;&c_i;&c_l;&c_mol;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_o;&c_ch;&c_i;&c_shch;&c_je;&c_n;&c_i;&c_j; &c_v;&c_ib;&c_d; &c_m;&c_ib;&c_k;&c_r;&c_o;&c_o;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_m;&c_ib;&c_v;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_p;&c_l;&c_ib;&c_d;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="steril.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="steril.0igi.BIO">
  <uzo tip="fak">BIO</uzo>
  <uzo tip="fak">MED</uzo>
    <dif>
      Igi <tld/>a:
      <ekz>
        <tld/>igi lakton, injektilon;
      </ekz>
      <ekz>
        infektaj prionoj <klr>[rezistas]</klr> al <tld/>igado
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2006/008861.php">Nova etapo en biologio kaj medicino</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ar tiaj fungoj kreskas en terenoj, kie trovi&gcirc;as sennombraj bakterioj, la
        ministerioj pri agrikulturo kaj sano postulas tiom da longda&ubreve;raj proceduroj
        por ilia <tld/>igo, ke la trufoj fari&gcirc;as malfre&scirc;aj kaj ne plu man&gcirc;eblaj
        <fnt>
          <bib>Kon11</bib>
        </fnt>;
      </ekz>
      <ekz>
        17-jara&gcirc;a virino <klr>[&#8230;]</klr> <tld/>igita sen sia konsento
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2012/010781.php">Kortumo kompensas</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>igilo</ind>.
        <trd lng="be">&c_s;&c_t;&c_e;&c_r;&c_y;&c_l;&c_ib;&c_z;&c_a;&c_t;&c_a;&c_r;</trd>
        <trd lng="fr">st&eacute;rilisateur</trd>
        <trd lng="hu">steriliz&aacute;tor</trd>
        <trd lng="ru">&c_s;&c_t;&c_je;&c_r;&c_i;&c_l;&c_i;&c_z;&c_a;&c_t;&c_o;&c_r;</trd>
      </ekz>
    </dif>
  <trd lng="pl">wyja&lstroke;awia&cacute;, sterylizowa&cacute;</trd>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_e;&c_r;&c_y;&c_l;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>sterilizovat</trd>,
    <trd>zp&uring;sobit neplodnost</trd>
  </trdgrp>
  <trd lng="de">sterilisieren</trd>
  <trd lng="en">sterilize</trd>
  <trd lng="es">esterilizar</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_t;&a_r;&a_w;&a_n; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_D;&a_d;&a_ayn;&a_f;&a_w;&a_n;&f_y;
    &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_A;&a_s;&a_t;&a_r;&f_y;&a_l; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">st&eacute;riliser</trd>
  <trdgrp lng="hu">
    <trd>cs&iacute;r&aacute;tlan&iacute;t</trd>,
    <trd>fert&odblac;tlen&iacute;t</trd>,
    <trd>steriliz&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#19981;&#22922;&#12395;&#12377;&#12427;  <pr>&#12405;&#12395;&#12435;&#12395;&#12377;&#12427;</pr></trd>,
    <trd>&#26029;&#31278;&#12377;&#12427;  <pr>&#12384;&#12435;&#12375;&#12421;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">steriliseren</trd>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_r;&c_i;&c_l;&c_i;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">sterilizova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_je;&c_r;&c_i;&c_l;&c_ib;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_b;&c_i;&c_t;&c_i; &c_n;&c_je;&c_z;&c_d;&c_a;&c_t;&c_n;&c_i;&c_m; &c_d;&c_o;
    &c_d;&c_ib;&c_t;&c_o;&c_r;&c_o;&c_d;&c_ib;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_s;&c_t;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
</art>
