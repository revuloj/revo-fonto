<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: artile.xml,v 1.22 2022/12/09 07:22:52 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>artileri</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="artile.0o">
  <kap><tld/>o</kap>
  <snc mrk="artile.0o.MIL_armiloj">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Diversaj <ref tip="prt" cel="paf.0ilego">pafilegoj</ref>
      kaj negrandaj raketoj servantaj por la milito:
      <ekz>
        armeo abunde provizita per <tld/>o;
      </ekz>
      <ekz>
        la soldatoj de la galeroj pafis per multnombra <tld/>o, al kiu respondadis tiuj,
        kiuj estis en la muregoj kaj fortika&jcirc;oj de la urbo kaj la peza <tld/>o
        rompadis la ventojn kun terura bruego, al kiu respondadis la ferdekaj kanonoj
        de la galeroj
        <fnt>
          <bib>DKM</bib>
          <lok>&Ccirc;apitro LXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu amaso estis <klr>[&#8230;]</klr> forlasanta <klr>[&#8230;]</klr> tiom rapide, kvaza&ubreve;
        <klr>[&#8230;]</klr> oni atendus <tld/>an bombardon
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="artile.0o.MIL_trupo">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Speciala parto de <ref tip="malprt" cel="arme.0o.sxtata">armeo</ref>, provizita je <tld/>aj armiloj kaj
      priservanta ilin:
      <ekz>
        baterio de <tld/>o;
      </ekz>
      <ekz>
        le&ubreve;tenanto de l&#8217; <tld/>o
        <fnt>
          <bib>IKr</bib>
          <lok>Neniam Plu!</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia <tld/>o premplatigos vin!
        <fnt>
          <aut>trad. senlime</aut>,
          <vrk><url ref="http://www.senlime.be/Kino/Diktator.htm">
          La Granda Diktatoro</url></vrk>,
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="arme.ter0o">terarmeo</ref>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_t;&c_y;&c_l;&c_je;&c_r;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_a;&c_r;&c_t;&c_i;&c_l;&c_je;&c_r;&c_i;&c_ja;</trd>
  <trd lng="ca">artilleria</trd>
  <trdgrp lng="cs">
    <trd>artilerie</trd>,
    <trd>d&ecaron;lost&rcaron;electvo</trd>
  </trdgrp>
  <trd lng="de">Artillerie</trd>
  <trd lng="en">artillery</trd>
  <trd lng="es">artiller&iacute;a</trd>
  <trd lng="fr">artillerie</trd>
  <trd lng="he">&alef;&#1464;&resh;&#1456;&tet;&#1460;&yod;&lamed;&#1462;&resh;&#1460;&yod;&#1464;&he;</trd>
  <trd lng="it">artiglieria</trd>
  <trd lng="nl">artillerie</trd>
  <trd lng="ru">&c_a;&c_r;&c_t;&c_i;&c_l;&c_l;&c_je;&c_r;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>artil&eacute;ria</trd>,
    <trd>delostrelectvo</trd>
  </trdgrp>
  <trd lng="sv">artilleri</trd>
  <trd lng="uk">&c_v;&c_ib;&c_j;&c_s;&c_mol;&c_k;.&nbsp;&c_a;&c_r;&c_t;&c_i;&c_l;&c_je;&c_r;&c_ib;&c_ja;</trd>
</drv>

<drv mrk="artile.0ano">
  <kap><tld/>ano, <var><kap><tld/>isto</kap></var></kap>
  <snc mrk="artile.0ano.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      <tld lit="A"/>a <ref tip="super" cel="soldat.0o.militisto">soldato</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_t;&c_y;&c_l;&c_je;&c_r;&c_y;&c_s;&c_t;</trd>
  <trd lng="bg">&c_a;&c_r;&c_t;&c_i;&c_l;&c_je;&c_r;&c_i;&c_s;&c_t;</trd>
  <trd lng="ca">artiller</trd>
  <trdgrp lng="cs">
    <trd>artilerista</trd>,
    <trd>d&ecaron;lost&rcaron;elec</trd>
  </trdgrp>
  <trd lng="de">Artillerist</trd>
  <trd lng="en">artilleryman</trd>
  <trd lng="es">artillero</trd>
  <trd lng="fr">artilleur</trd>
  <trd lng="he">&tav;&vav;&#1465;&tav;&#1456;&het;&#1464;&fnun;</trd>
  <trd lng="it">artiglieriere</trd>
  <trd lng="nl">artillerist</trd>
  <trd lng="ru">&c_a;&c_r;&c_t;&c_i;&c_l;&c_l;&c_je;&c_r;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">delostrelec</trd>
  <trd lng="sv">artillerist</trd>
</drv>
</art>
