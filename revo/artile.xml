<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>artileri</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="artile.0o">
  <kap><tld/>o</kap>
  <snc mrk="artile.0o.MIL_armiloj">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Diversaj <ref tip="prt" cel="paf.0ilego">pafilegoj</ref>
      kaj negrandaj raketoj servantaj por la milito:
      <ekz>
        armeo abunde provizita per <tld/>o.
      </ekz>
    </dif>
  </snc>
  <snc mrk="artile.0o.MIL_trupo">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Speciala parto de armeo, provizita je <tld/>aj pecoj kaj
      priservanta ilin:
      <ekz>
        baterio de <tld/>o;
      </ekz>
      <ekz>
        mia <tld/>o premplatigos vin!
        <fnt>
          <aut>trad. senlime</aut>,
          <vrk><url ref="http://www.senlime.be/Kino/Diktator.htm">
          La Granda Diktatoro</url></vrk>,
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_t;&c_y;&c_l;&c_je;&c_r;&c_y;&c_ja;</trd>
  <trd lng="ca">artilleria</trd>
  <trd lng="de">Artillerie</trd>
  <trd lng="es">artiller&iacute;a</trd>
  <trd lng="en">artillery</trd>
  <trd lng="fr">artillerie</trd>
  <trd lng="it">artiglieria</trd>
  <trd lng="ru">&c_a;&c_r;&c_t;&c_i;&c_l;&c_l;&c_je;&c_r;&c_i;&c_ja;</trd>
  <trd lng="sk">artil&eacute;ria</trd>
  <trd lng="sv">artilleri</trd>
</drv>

<drv mrk="artile.0ano">
  <kap><tld/>ano, <var><kap><tld/>isto</kap></var></kap>
  <snc mrk="artile.0ano.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      <tld/>a soldato.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_t;&c_y;&c_l;&c_je;&c_r;&c_y;&c_s;&c_t;</trd>
  <trd lng="ca">artiller</trd>
  <trd lng="de">Artillerist</trd>
  <trd lng="es">artillero</trd>
  <trd lng="en">artilleryman</trd>
  <trd lng="fr">artilleur</trd>
  <trd lng="it">artiglieriere</trd>
  <trd lng="ru">&c_a;&c_r;&c_t;&c_i;&c_l;&c_l;&c_je;&c_r;&c_i;&c_s;&c_t;</trd>
  <trd lng="sv">artillerist</trd>
</drv>
</art>
