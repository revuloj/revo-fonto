<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: suspir.xml,v 1.15 2019/12/24 09:10:14 revo Exp $">
<kap><rad>suspir</rad>/i <fnt><bib>SPV</bib></fnt></kap>

<drv mrk="suspir.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="suspir.0i.KOMUNE">
    <dif>
      <ref tip="dif" cel="spir.ve0i.KOMUNE">vespiri</ref>
      <ekz>
        malfeli&ccirc;aj, malfeli&ccirc;aj infanoj! - ek<tld/>is la bopatrino
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>, 
        <lok>[1898?]</lok></fnt>. 
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_z;&c_d;&c_y;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_z;&c_d;&c_y;&c_h;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_v;&c_malmol;&c_z;&c_d;&c_i;&c_sh;&c_a;&c_m;</trd>,
    <trd>&c_s;&c_t;&c_je;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">vzdychat</trd>
  <trdgrp lng="de">
    <trd>seufzen</trd>
  </trdgrp>
  <trd lng="en">sigh</trd>
  <trd lng="es">suspirar</trd>
  <trdgrp lng="fr">
    <trd>soupirer<klr> (pousser un soupir)</klr></trd>,
    <trd>pousser un <ind>soupir</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>s&oacute;hajt</trd>,
    <trd>foh&aacute;szkodik</trd>
  </trdgrp>
  <trd lng="it">sospirare</trd>
  <trdgrp lng="nl">
    <trd>zuchten</trd>,
    <trd>kreunen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_z;&c_d;&c_y;&c_h;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_z;&c_d;&c_o;&c_h;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">vzdycha&tcaron;</trd>
</drv>

<drv mrk="suspir.0o">
  <kap><tld/>o</kap>
  <snc mrk="suspir.0o.KOMUNE">
    <dif>
      A&ubreve;debla elspiro:
      <ekz>
        kian liberigan <tld/>on elmetis mia patrino, kiam nia najbarino
        rezignis la pozicion kaj la pordo fermi&gcirc;is
        <fnt><aut>A. Zecchin</aut>, <vrk>La kokino de nia
        najbarino</vrk>, <bib>Monato</bib>, <lok>1999:3,
        p. 27a</lok></fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_z;&c_d;&c_y;&c_h;</trd>
  <trdgrp lng="bg">
    <trd>&c_v;&c_malmol;&c_z;&c_d;&c_i;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_n;</trd>,
    <trd>&c_s;&c_t;&c_je;&c_n;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="cs">povzdech</trd>
  <trdgrp lng="de">
    <trd>Seufzer</trd>
  </trdgrp>
  <trd lng="en">sigh</trd>
  <trd lng="es">suspiro</trd>
  <trd lng="fr">soupir<klr> (expiration)</klr></trd>
  <trdgrp lng="hu">
    <trd>s&oacute;haj</trd>,
    <trd>foh&aacute;sz</trd>
  </trdgrp>
  <trd lng="it">sospiro</trd>
  <trdgrp lng="nl">
    <trd>zucht</trd>,
    <trd>kreun</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_z;&c_d;&c_o;&c_h;</trd>
  <trd lng="sk">vzdych</trd>
</drv>
</art>
