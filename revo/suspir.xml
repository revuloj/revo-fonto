<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: suspir.xml,v 1.21 2023/11/23 21:23:49 revo Exp $">
<kap><rad>suspir</rad>/i <fnt><bib>SPV</bib></fnt></kap>

<drv mrk="suspir.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="suspir.0i.KOMUNE">
    <dif>
      <ref tip="dif" cel="spir.ve0i.KOMUNE">Vespiri</ref>:
      <ekz>
        malfeli&ccirc;aj, malfeli&ccirc;aj infanoj! &ndash; ek<tld/>is la bopatrino
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>,
        <lok>[1898?]</lok></fnt>;
      </ekz>
      <ekz>
        la&ubreve; la aspekto de la muroj oni povus fari truon en ili per nura piedfrapo
        <klr>[&#8230;]</klr>, li &ccirc;irka&ubreve;rigardis, <tld/>egis
        <fnt>
          <bib>HsT</bib>
          <lok>Feli&ccirc;a homo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_z;&c_d;&c_y;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_z;&c_d;&c_y;&c_h;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_v;&c_malmol;&c_z;&c_d;&c_i;&c_sh;&c_a;&c_m;</trd>,
    <trd>&c_s;&c_t;&c_je;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">vzdychat</trd>
  <trdgrp lng="de">
    <trd>seufzen</trd>
  </trdgrp>
  <trd lng="en">sigh</trd>
  <trd lng="es">suspirar</trd>
  <trdgrp lng="fr">
    <trd>soupirer<klr> (pousser un soupir)</klr></trd>,
    <trd>pousser un <ind>soupir</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>s&oacute;hajt</trd>,
    <trd>foh&aacute;szkodik</trd>
  </trdgrp>
  <trd lng="it">sospirare</trd>
  <trd lng="ja">&#22022;&#24687;&#12377;&#12427;  <pr>&#12383;&#12435;&#12381;&#12367;&#12377;&#12427;</pr></trd>
  <trdgrp lng="nl">
    <trd>zuchten</trd>,
    <trd>kreunen</trd>
  </trdgrp>
  <trd lng="pl">wzdycha&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_z;&c_d;&c_y;&c_h;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_z;&c_d;&c_o;&c_h;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">vzdycha&tcaron;</trd>
  <trd lng="uk">&c_z;&c_ib;&c_t;&c_h;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="suspir.0o">
  <kap><tld/>o</kap>
  <snc mrk="suspir.0o.KOMUNE">
    <dif>
      A&ubreve;debla elspiro pro emocio:
      <ekz>
        <tld/>o de laceco kaj rezigno eli&gcirc;is el lia brusto
        <fnt>
          <bib>Tor</bib>
          <lok>IV.</lok>
        </fnt>;
      </ekz>
      <ekz>
        kian liberigan <tld/>on elmetis mia patrino, kiam nia najbarino
        rezignis la pozicion kaj la pordo fermi&gcirc;is
        <fnt><aut>A. Zecchin</aut>, <vrk>La kokino de nia
        najbarino</vrk>, <bib>Monato</bib>, <lok>1999:3,
        p. 27a</lok></fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_z;&c_d;&c_y;&c_h;</trd>
  <trdgrp lng="bg">
    <trd>&c_v;&c_malmol;&c_z;&c_d;&c_i;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_n;</trd>,
    <trd>&c_s;&c_t;&c_je;&c_n;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="cs">povzdech</trd>
  <trdgrp lng="de">
    <trd>Seufzer</trd>
  </trdgrp>
  <trd lng="en">sigh</trd>
  <trd lng="es">suspiro</trd>
  <trd lng="fr">soupir<klr> (expiration)</klr></trd>
  <trdgrp lng="hu">
    <trd>s&oacute;haj</trd>,
    <trd>foh&aacute;sz</trd>
  </trdgrp>
  <trd lng="it">sospiro</trd>
  <trdgrp lng="nl">
    <trd>zucht</trd>,
    <trd>kreun</trd>
  </trdgrp>
  <trd lng="pl">westchnienie</trd>
  <trd lng="ru">&c_v;&c_z;&c_d;&c_o;&c_h;</trd>
  <trd lng="sk">vzdych</trd>
  <trd lng="uk">&c_z;&c_ib;&c_t;&c_h;&c_a;&c_n;&c_n;&c_ja;</trd>
</drv>
</art>
