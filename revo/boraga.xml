<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: boraga.xml,v 1.9 2022/06/06 18:39:12 revo Exp $">
<kap>
  <rad>boragac</rad>/oj
</kap>
<drv mrk="boraga.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref>  de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Boraginaceae</trd>):
      <ekz>
        la skorpia cumo estas tre tipa de la familio <tld/>oj, 
        ekzemple la <ref tip="prt" cel="forges.ne0umino">neforgesumino</ref>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19918">Cumo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        regiono <klr>[&#8230;]</klr> speciale ri&ccirc;a je bulbkreska&jcirc;oj, kaj specioj de la familioj de la
        lamiacoj kaj de la <tld/>oj 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=423627">Sud-anatoliaj koniferaj kaj
          deciduaj montarbaroj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_r;&c_a;&c_ch;&c_n;&c_ib;&c_k;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  <trd lng="ca">borragin&agrave;cies</trd>
  <trd lng="de">Borretschgew&auml;chse</trd>
  <trd lng="fr">boraginac&eacute;es</trd>
  <trdgrp lng="hu">
    <trd>bor&aacute;g&oacute;f&eacute;l&eacute;k</trd>,
    <trd>&eacute;rdeslevel&udblac;ek</trd></trdgrp>
  <trd
lng="ru">&c_b;&c_u;&c_r;&c_a;&c_ch;&c_n;&c_i;&c_k;&c_o;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sv">str&auml;vbladiga v&auml;xter</trd>
</drv>
</art>
