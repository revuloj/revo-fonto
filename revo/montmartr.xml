<vortaro>
  <art mrk="$Id: montmartr.xml,v 1.2 2025/12/24 06:52:27 afido Exp $">
    <kap>
      <rad>Montmartr</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="montmartr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Pariza <ref tip="super" cel="kvarta.0o">kvartalo</ref> sur monteto, centro de artistoj:
      <ekz>
        ni estis kune en <tld/>o, en noktoklubo kun senvesti&gcirc;aj dancoj
        <fnt>
          <bib>KrB</bib>
          <lok>Vi kien flugas, papilio?</lok>
        </fnt>;
      </ekz>
      <ekz>
        spektaklo <klr>[&#8230;]</klr> en <tld/>a kabaredo
        <fnt>
          <bib>Kon11</bib>
          <lok>Intervjuo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Montmartre</trd>
    <trd lng="en">Montmartre</trd>
    <trd lng="fr">Montmartre</trd>
    <trd lng="ru">&c_M;&c_o;&c_n;&c_m;&c_a;&c_r;&c_t;&c_r;</trd>
  </snc>
<trd lng="be">&c_M;&c_a;&c_n;&c_m;&c_a;&c_r;&c_t;&c_r;</trd>
</drv>

  </art>

