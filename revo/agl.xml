  <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="agl.0o">
  <kap>
    <ofc>*</ofc>
    <tld/>o
  </kap>
  <snc mrk="agl.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio
      <ref tip="super" cel="akcipi1.0oj">akcipitredoj</ref>
      (<trd lng="la">Aquila</trd>), kaj similaj rabobirdoj kun
      kurba beko:
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>pasero kaptita estas pli bona, ol <tld/>o kaptota</ind>
        <trd lng="fr">un &leftquot;tiens&rightquot; vaut deux
        &leftquot;tu l'auras&rightquot;</trd>
        <trd lng="pl">lepszy wr&oacute;bel w gar&sacute;ci ni&zdot;
        <ind>go&lstroke;&aogonek;b</ind> na dachu</trd>
        <fnt>
          <bib>F</bib>,
          <lok>&FE; 22</lok>
        </fnt>;
      </ekz>
      <ekz>la <tld/>o &ccirc;irka&ubreve;&scirc;vebis &gcirc;in, trankvila kaj digna
        <fnt>
          <aut>Bj&oslash;rnstjerne Bj&oslash;rnson trad. &Eacute;dith kaj
          August Weide</aut>
          <vrk>Synn&oslash;ve Solbakken</vrk>
          <lok>&ccirc;apitro 3a</lok>.
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_o;&c_l;</trd>
  <trd lng="br">erer</trd>
  <trd lng="bg">&c_o;&c_r;&c_je;&c_l;</trd>
  <trdgrp lng="ca">
    <trd>&agrave;guila</trd>,
    <trd>&agrave;liga</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Adler</trd>,
    <trd>Aar<klr> [poezie]</klr></trd>
  </trdgrp>
  <trd lng="en">eagle</trd>
  <trd lng="es">&aacute;guila</trd>
  <trd lng="fr">aigle</trd>
  <trd lng="hu">sas</trd>
  <trd lng="it">aquila</trd>
  <trdgrp lng="nl">
    <trd>arend</trd>,
    <trd>adelaar</trd>
  </trdgrp>
  <trd lng="pl">orze&lstroke;</trd>
  <trd lng="pt">&aacute;guia</trd>
  <trd lng="tr">kartal</trd>
  <trd lng="ru">&c_o;&c_r;&c_jo;&c_l;</trd>
  <trd lng="sk">orol</trd>
  <trd lng="sv">&ouml;rn</trd>
</drv>

<drv mrk="agl.A0o">
  <kap><tld lit="A"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
  </kap>
  <mlg>Aqu</mlg>
  <snc mrk="agl.A0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Ekvatora <ref tip="lst" cel="stel.0figuro"
      lst="voko:stelfiguroj">stelfiguro</ref> (<trd
      lng="la">Aquila</trd>).
    </dif>
  </snc>
  <trd lng="bg">&c_O;&c_r;&c_je;&c_l;<klr tip="amb">
  (&c_s;&c_malmol;&c_z;&c_v;&c_je;&c_z;&c_d;&c_i;&c_je;)</klr></trd>
  <trd lng="ca">&Agrave;guila</trd>
  <trdgrp lng="cs">
    <trd>Orel</trd>,
    <trd>orel</trd>
  </trdgrp>
  <trd lng="de">Adler</trd>
  <trd lng="el">&Alfa;&epsilon;&tau;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">Eagle</trd>
  <trd lng="es">&Aacute;guila</trd>
  <trd lng="fr">Aigle<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="he">&nun;&shin;&resh;</trd>
  <trd lng="hu">Sas</trd>
  <trd lng="it">Aquila</trd>
  <trd lng="nl">Arend</trd>
  <trd lng="pl">Orze&lstroke;</trd>
  <trd lng="pt">&Aacute;guia</trd>
  <trd lng="sk">orol</trd>
  <trd lng="sv">&Ouml;rnen</trd>
  <trd lng="tr">Kartal</trd>
</drv>

<drv mrk="agl.0eca">
  <kap><tld/>eca</kap>
  <snc>
    <dif>
      Simila al <tld/>o:
      <ekz>
        <tld/>eca nazo, rigardo;
      </ekz>
      <ekz>lia nazo <klr>[estis]</klr> pli agleca <klr>[ol la &scirc;ia]</klr>
        <fnt>
          <aut>Henri Bernardin de Saint-Pierre, trad. Hector
          Hodler</aut>
          <vrk>Pa&ubreve;lo kaj Virginio</vrk>
        </fnt>;
      </ekz>
      <ekz>&Scirc;ia viza&gcirc;o estis klara, agleca, viza&gcirc;o kiun oni eble nomus
      nobla
        <fnt><aut>George Orwell, trad. Donald Broadribb</aut>
        <vrk>Mil Na&ubreve;cent Okdek Kvar</vrk>
        <lok>parto 1a, &ccirc;apitro 6a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_l;&c_ib;&c_n;&c_y;</trd>
  <trd lng="br">-<ind>erer</ind></trd>
  <trdgrp lng="ca">
    <trd>aguilenc</trd>,
    <trd>aquil&iacute;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>orl&iacute;</trd>,
    <trd>s vlastnostmi jako m&aacute; orel</trd>
  </trdgrp>
  <trd lng="de">Adler-</trd>
  <trd lng="en">aquiline</trd>
  <trd lng="es">aguile&ntilde;o</trd>
  <trdgrp lng="fr">
    <trd>aquilin <klr>(nez)</klr></trd>,
    <trd>d'<ind>aigle</ind></trd>
  </trdgrp>
  <trd lng="hu">sas-</trd>
  <trd lng="it">aquilino</trd>
  <trdgrp lng="nl">
    <trd>arends-</trd>,
    <trd>adelaars-</trd>
  </trdgrp>
  <trd lng="pl">orli</trd>
  <trdgrp lng="pt">
    <trd>aquilino</trd>,
    <trd>adunco</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_r;&c_l;&c_i;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>orl&iacute;</trd>,
    <trd>orlia <klr>(vlastnos&tcaron;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="agl.fisx0o">
  <kap>fi&scirc;<tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="agl.fisx0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda rabobirdo kaptanta fi&scirc;ojn,
      genro el familio <ref tip="super"
      cel="pandio1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Pandion</trd>):
      <ekz>
        fi&scirc;<tld/>o aktive ordigas kaj riparas la neston
        <fnt>
          <aut>H. Laine</aut>,
          <vrk>[blogo]</vrk>,
          <lok><url
          ref="http://www.ipernity.com/blog/harri/141911">2009-04-08</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="sin" cel="pandio.0o">pandiono</ref>
  <trd lng="bg">&c_o;&c_r;&c_je;&c_l; &c_r;&c_i;&c_b;&c_a;&c_r;</trd>
  <trd lng="br">erer-spluj</trd>
  <trdgrp lng="ca">
    <trd>&agrave;guila pescadora</trd>,
    <trd>&agrave;guila d'estany</trd>
  </trdgrp>
  <trd lng="cs">orel &rcaron;&iacute;&ccaron;n&iacute;</trd>
  <trd lng="de">Fischadler</trd>
  <trd lng="en">osprey</trd>
  <trd lng="es">&aacute;guila pescadora</trd>
  <trd lng="fr">balbuzard p&ecirc;cheur</trd>
  <trd lng="hu">hal&aacute;szsas</trd>
  <trd lng="it"><ind>falco</ind> pescatore</trd>
  <trd lng="la">Pandion haliaetus</trd>
  <trd lng="nl">visarend</trd>
  <trd lng="pl">rybo&lstroke;&oacute;w</trd>
  <trd lng="pt">&aacute;guia-pescadora</trd>
  <trd lng="ru">&c_s;&c_k;&c_o;&c_p;&c_a;</trd>
  <trd lng="sv">fiskgjuse</trd>
</drv>

<drv mrk="agl.mar0o">
  <kap>mar<tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <fnt>
       <aut>Wouter F. Pilger</aut>,
       <vrk>
         <url ref="http://www.xs4all.nl/~pilger/wfp/birdhvu8.htm">
           Birdonomoj en Esperanto por Hejma Vortaro</url>
       </vrk>
    </fnt>
    <dif>
      Genro <trd lng="la">Haliae&euml;tus</trd> de mar- a&ubreve;
      lag-bordaj a&ubreve; riveraj
      rabobirdoj el la familio de <ref tip="super"
      cel="akcipi1.0oj">akcipitredoj</ref>,
      grandaj kun duone plumkovritaj tarsoj.
    </dif>
    <ref tip="sin" cel="haliaet.0o">haliaeto</ref>
    <trdgrp lng="ca">
      <trd>&agrave;guila marina</trd>,
      <trd>pigarg</trd>
    </trdgrp>
    <trd lng="de">Seeadler</trd>
    <trd lng="en">sea <ind>eagle</ind></trd>
    <trd lng="fr">pygargue</trd>
    <trd lng="hu">r&eacute;tisas</trd>
    <trd lng="it"><ind>aquila</ind> di mare</trd>
    <trd lng="nl">zee-arend</trd>
    <trdgrp lng="pl">
      <trd>orze&lstroke; bielik</trd>,
      <trd>bielik</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">orel mo&rcaron;sk&yacute;</trd>
  <trd lng="sk">morsk&yacute; orol</trd>
</drv>

