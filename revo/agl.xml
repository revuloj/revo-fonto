<drv mrk="agl.0o">
  <kap>
    <ofc>*</ofc>
    <tld/>o
  </kap>
  <snc mrk="agl.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio
      <ref tip="super" cel="akcipi1.0oj">akcipitredoj</ref>
      (<trd lng="la">Aquila</trd>), kaj similaj rabobirdoj kun
      kurba beko:
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>pasero kaptita estas pli bona, ol <tld/>o kaptota</ind>
        <trd lng="fr">un &leftquot;tiens&rightquot; vaut deux &leftquot;tu l'auras&rightquot;</trd>
        <trd lng="pl">lepszy wr&oacute;bel w gar&sacute;ci ni&zdot; <ind>go&lstroke;&aogonek;b</ind> na dachu</trd>
        <fnt>
          <bib>F</bib>,
          <lok>&FE; 22</lok>
        </fnt>;
      </ekz>
      <ekz>la <tld/>o &ccirc;irka&ubreve;&scirc;vebis &gcirc;in, trankvila kaj digna
        <fnt>
          <aut>Bj&oslash;rnstjerne Bj&oslash;rnson trad. &Eacute;dith kaj
          August Weide</aut>
          <vrk>Synn&oslash;ve Solbakken</vrk>
          <lok>&ccirc;apitro 3a</lok>.
        </fnt>
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x963F;&#x5FB7;&#x52D2; <pr>&amacron;d&#xE9;l&emacron;i</pr></trd>,
    <trd>&#20803;&#x91D1;&#x5E01; <pr>yu&#xE1;nj&#x12B;nb&#xEC;</pr></trd>,
    <trd>&#20803;&#x91D1;&#x5E63; <pr>yu&#xE1;nj&#x12B;nb&#xEC;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_r;&c_o;&c_l;</trd>
  <trd lng="br">erer</trd>
  <trd lng="bg">&c_o;&c_r;&c_je;&c_l;</trd>
  <trdgrp lng="ca">
    <trd>&agrave;guila</trd>,
    <trd>&agrave;liga</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Adler</trd>,
    <trd>Aar<klr> [poezie]</klr></trd>
  </trdgrp>
  <trd lng="en">eagle</trd>
  <trd lng="es">&aacute;guila</trd>
  <trd lng="fr">aigle</trd>
  <trd lng="hu">sas</trd>
  <trd lng="it">aquila</trd>
  <trdgrp lng="nl">
    <trd>arend</trd>,
    <trd>adelaar</trd>
  </trdgrp>
  <trd lng="pl">orze&lstroke;</trd>
  <trd lng="pt">&aacute;guia</trd>
  <trd lng="tr">kartal</trd>
  <trd lng="ro">vultur</trd>
  <trd lng="ru">&c_o;&c_r;&c_jo;&c_l;</trd>
  <trd lng="sk">orol</trd>
  <trd lng="sv">&ouml;rn</trd>
</drv>

<drv mrk="agl.A0o">
  <kap><tld lit="A"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
  </kap>
  <mlg>Aqu</mlg>
  <snc mrk="agl.A0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Ekvatora <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref> (<trd
      lng="la">Aquila</trd>).
    </dif>
    <trdgrp lng="zh">
    <trd>&#22825;&#x9DF9;&#x5EA7; <pr>ti&amacron;ny&#x12B;ngzu&#xF2;</pr></trd>,
    <trd>&#22825;&#x9E70;&#x5EA7; <pr>ti&amacron;ny&#x12B;ngzu&#xF2;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_A;&c_r;&c_o;&c_l; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#8217;&c_je;)</klr></trd>
  <trd lng="bg">&c_O;&c_r;&c_je;&c_l;<klr tip="amb">
  (&c_s;&c_malmol;&c_z;&c_v;&c_je;&c_z;&c_d;&c_i;&c_je;)</klr></trd>
  <trd lng="bo">&#xF56;&#xFB1;&#xF0B;&#xF42;&#xFB3;&#xF42;&#xF0B;</trd>
  <trd lng="ca">&Agrave;guila</trd>
  <trdgrp lng="cs">
    <trd>Orel</trd>,
    <trd>orel</trd>
  </trdgrp>
  <trd lng="de">Adler</trd>
  <trd lng="el">&Alfa;&epsilon;&tau;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">Eagle</trd>
  <trd lng="es">&Aacute;guila</trd>
  <trd lng="fr">Aigle<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="he">&nun;&shin;&resh;</trd>
  <trd lng="hu">Sas</trd>
  <trd lng="it">Aquila</trd>
  <trdgrp lng="ja">
    <trd>&#x30EF;&#x30B7;</trd>,
    <trd>&#x9DF2;&#21360;  <pr>&#x308F;&#12375;&#12356;&#12435;</pr></trd>,
    <trd>&#x77E5;&#32773;  <pr>&#12385;&#12375;&#12419;</pr></trd>,
    <trd>&#20999;&#x308C;&#32773;  <pr>&#x304D;&#x308C;&#12418;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">Arend</trd>
  <trd lng="pl">Orze&lstroke;  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="pt">&Aacute;guia</trd>
  <trd lng="ro">Vultur <klr>(constela&#x21B;ie)</klr></trd>
  <trd lng="sk">orol</trd>
  <trd lng="sv">&Ouml;rnen</trd>
  <trd lng="tr">Kartal</trd>
  <trd lng="uk">&#x43E;&c_r;&c_je;&#x43B;</trd>
</drv>

<drv mrk="agl.0eca">
  <kap><tld/>eca</kap>
  <snc>
    <dif>
      Simila al <tld/>o:
      <ekz>
        <tld/>eca nazo, rigardo;
      </ekz>
      <ekz>lia nazo <klr>[estis]</klr> pli agleca <klr>[ol la &scirc;ia]</klr>
        <fnt>
          <aut>Henri Bernardin de Saint-Pierre, trad. Hector
          Hodler</aut>
          <vrk>Pa&ubreve;lo kaj Virginio</vrk>
        </fnt>;
      </ekz>
      <ekz>&Scirc;ia viza&gcirc;o estis klara, agleca, viza&gcirc;o kiun oni eble nomus
      nobla
        <fnt><aut>George Orwell, trad. Donald Broadribb</aut>
        <vrk>Mil Na&ubreve;cent Okdek Kvar</vrk>
        <lok>parto 1a, &ccirc;apitro 6a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_l;&c_ib;&c_n;&c_y;</trd>
  <trd lng="br">-<ind>erer</ind></trd>
  <trdgrp lng="ca">
    <trd>aguilenc</trd>,
    <trd>aquil&iacute;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>orl&iacute;</trd>,
    <trd>s vlastnostmi jako m&aacute; orel</trd>
  </trdgrp>
  <trd lng="de">Adler-</trd>
  <trd lng="en">aquiline</trd>
  <trd lng="es">aguile&ntilde;o</trd>
  <trdgrp lng="fr">
    <trd>aquilin <klr>(nez)</klr></trd>,
    <trd>d'<ind>aigle</ind></trd>
  </trdgrp>
  <trd lng="hu">sas-</trd>
  <trd lng="it">aquilino</trd>
  <trdgrp lng="ja">
    <trd>&#x30EF;&#x30B7;&#x306E;&#x304F;&#12385;&#12400;&#12375;&#x306E;&#12424;&#12358;&#x306B;&#x66F2;&#x304C;&#12387;&#x305F; 
    <pr>&#x308F;&#12375;&#x306E;&#x304F;&#12385;&#12400;&#12375;&#x306E;&#12424;&#12358;&#x306B;&#x307E;&#x304C;&#12387;&#x305F;</pr></trd>,
    <trd>&#x9DF2;&#x306E;&#12424;&#12358;&#x306A;  <pr>&#x308F;&#12375;&#x306E;&#12424;&#12358;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>arends-</trd>,
    <trd>adelaars-</trd>
  </trdgrp>
  <trd lng="pl">orli</trd>
  <trdgrp lng="pt">
    <trd>aquilino</trd>,
    <trd>adunco</trd>
  </trdgrp>
  <trd lng="ro">acvilin</trd>
  <trd lng="ru">&c_o;&c_r;&c_l;&c_i;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>orl&iacute;</trd>,
    <trd>orlia <klr>(vlastnos&tcaron;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="agl.fisx0o">
  <kap>fi&scirc;<tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="agl.fisx0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda rabobirdo kaptanta fi&scirc;ojn,
      genro el familio <ref tip="super" cel="pandio1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Pandion</trd>):
      <ekz>
        fi&scirc;<tld/>o aktive ordigas kaj riparas la neston
        <fnt>
          <aut>H. Laine</aut>,
          <vrk>[blogo]</vrk>,
          <lok><url ref="http://www.ipernity.com/blog/harri/141911">2009-04-08</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x96CE; <pr>j&#x16B;</pr></trd>
</snc>
  <ref tip="sin" cel="pandio.0o">pandiono</ref>
  <trd lng="be">&c_s;&c_k;&c_a;&c_p;&c_a;</trd>
  <trd lng="bg">&c_o;&c_r;&c_je;&c_l; &c_r;&c_i;&c_b;&c_a;&c_r;</trd>
  <trd lng="br">erer-spluj</trd>
  <trdgrp lng="ca">
    <trd>&agrave;guila pescadora</trd>,
    <trd>&agrave;guila d'estany</trd>
  </trdgrp>
  <trd lng="cs">orel &rcaron;&iacute;&ccaron;n&iacute;</trd>
  <trd lng="de">Fischadler</trd>
  <trd lng="en">osprey</trd>
  <trd lng="es">&aacute;guila pescadora</trd>
  <trd lng="fr">balbuzard p&ecirc;cheur</trd>
  <trd lng="hu">hal&aacute;szsas</trd>
  <trd lng="it"><ind>falco</ind> pescatore</trd>
  <trd lng="ja">&#x30DF;&#x30B5;&#x30B4;</trd>
  <trd lng="la">Pandion haliaetus</trd>
  <trd lng="nl">visarend</trd>
  <trd lng="pl">rybo&lstroke;&oacute;w</trd>
  <trd lng="pt">&aacute;guia-pescadora</trd>
  <trd lng="ro">uliganul pescar </trd>
  <trd lng="ru">&c_s;&c_k;&c_o;&c_p;&c_a;</trd>
  <trd lng="sv">fiskgjuse</trd>
  <trd lng="uk">&c_s;&#x43A;&#x43E;&#x43F;&c_a;</trd>
</drv>

<drv mrk="agl.mar0o">
  <kap>mar<tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <fnt>
       <aut>Wouter F. Pilger</aut>,
       <vrk>
         <url ref="http://www.xs4all.nl/~pilger/wfp/birdhvu8.htm">
           Birdonomoj en Esperanto por Hejma Vortaro</url>
       </vrk>
    </fnt>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">Genro</ref>
      (<trd lng="la">Haliae&euml;tus</trd>) de mar- a&ubreve;
      lag-bordaj a&ubreve; riveraj rabobirdoj el la familio de <ref tip="malprt"
      cel="akcipi1.0oj">akcipitredoj</ref>,
      grandaj kun duone plumkovritaj tarsoj:
      <ekz>
        tie havis siajn nestojn la mar<tld/>o, la arbaraj kolomboj, la blue-nigraj
        korvoj kaj e&ccirc; la nigra cikonio
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="haliaet.0o">haliaeto</ref>
    <trdgrp lng="ca">
      <trd>&agrave;guila marina</trd>,
      <trd>pigarg</trd>
    </trdgrp>
    <trd lng="de">Seeadler</trd>
    <trd lng="en">sea <ind>eagle</ind></trd>
    <trd lng="fr">pygargue</trd>
    <trd lng="hu">r&eacute;tisas</trd>
    <trd lng="it"><ind>aquila</ind> di mare</trd>
    <trd lng="nl">zee-arend</trd>
    <trdgrp lng="pl">
      <trd>orze&lstroke; bielik</trd>,
      <trd>bielik</trd>
    </trdgrp>
    <trd lng="ro">codalb </trd>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_l;&c_a;&c_n;</trd>
  <trd lng="cs">orel mo&rcaron;sk&yacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x30A6;&#x30DF;&#x30EF;&#x30B7;</trd>,
    <trd>&#x30AA;&#x30AA;&#x30EF;&#x30B7;</trd>
  </trdgrp>
  <trd lng="sk">morsk&yacute; orol</trd>
  <trd lng="uk">&#x43E;&c_r;&#x43B;&c_a;&#x43D;</trd>
</drv>

<drv mrk="agl.blankkola_mar0o">
  <kap>blankkola mar<tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <fnt>
       <aut>Wouter F. Pilger</aut>,
       <vrk>
         <url ref="http://www.xs4all.nl/~pilger/wfp/birdeou8.htm">
           Komunlingvaj nomoj de E&ubreve;ropaj birdoj</url>
       </vrk>
    </fnt>
    <dif>
      Specio de <ref tip="super" cel="agl.mar0o">maraglo</ref>
      (<trd lng="la">Haliae&euml;tus leucoryphus</trd> a&ubreve;
       <trd lng="la">Haliaeetus leucoryphus</trd>).
    </dif>
    <bld
    lok="&WCU;/thumb/f/f1/Haliaeetus_leucoryphus_-Corbett_National_Park%2C_Uttarakhand,_India-8.jpg/640px-Haliaeetus_leucoryphus_-Corbett_National_Park%2C_Uttarakhand%2C_India-8.jpg"
    prm="CC BY 2.0">Blankkola mar<tld/>o en Nacia Parko Corbett, Uttarakhand, Barato
        <fnt>
          <aut>Koshy Koshy el Faridabad, Haryana, Barato</aut>
          <lok><url
          ref="&WCW;/File:Haliaeetus_leucoryphus_-Corbett_National_Park,_Uttarakhand,_India-8.jpg">&WCW;/File:Haliaeetus_leucoryphus_-Corbett_National_Park,_Uttarakhand,_India-8.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="ca">pigarg americ&agrave;</trd>
    <trd lng="en">Pallas's fish <ind>eagle</ind></trd>
    <trd lng="it"><ind>aquila</ind> di mare testabianca</trd>
    <trd lng="pl"><ind>orze&lstroke;</ind>  bielik wschodni</trd>
    <trd lng="ro">vulturul ple&#537;uv</trd>
  </snc>
</drv>

<drv mrk="agl.blankvosta_mar0o">
  <kap>blankvosta mar<tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <fnt>
       <aut>Wouter F. Pilger</aut>,
       <vrk>
         <url ref="http://www.xs4all.nl/~pilger/wfp/birdeou8.htm">
           Komunlingvaj nomoj de E&ubreve;ropaj birdoj</url>
       </vrk>
    </fnt>
    <dif>
      Specio de <ref tip="super" cel="agl.mar0o">maraglo</ref>
      (<trd lng="la">Haliae&euml;tus albicilla</trd> a&ubreve;
       <trd lng="la">Haliaeetus albicilla</trd>):
      <ekz>
        mortis jam pro man&gcirc;o de venenitaj fi&scirc;oj du birdoj de rigore protektata specio:
        blankvosta mar<tld/>o, el kiuj oni kalkulas nur entute 10 parojn en tiu
        regiono, kaj multaj mutaj cignoj kaj kormoranoj
        <fnt><aut>Katalin Smid&eacute;liusz</aut>:
          <vrk><url ref="&Monato;/2000/006912.php">Cinike cianide -- &ccirc;u respondecas
          Okcidento?</url></vrk>,
          <bib>Monato</bib>
          <lok>2000/04, p. 15</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
    lok="&WCU;/thumb/a/a5/Haliaeetus_albicilla_(Svolv%C3%A6r%2C_2012).jpg/640px-Haliaeetus_albicilla_(Svolv%C3%A6r%2C_2012).jpg"
    prm="CC BY-SA 3.0">Blankvosta mar<tld/>o, en Svolvaer, Norvegujo
        <fnt>
          <aut>Yathin S Krishnappa, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Haliaeetus_albicilla_(Svolv%C3%A6r,_2012).jpg">&WCW;/File:Haliaeetus_albicilla_(Svolv&aelig;r,_2012).jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="ca">&agrave;guila marina de Pallas</trd>
    <trd lng="en">white-tailed <ind>eagle</ind></trd>
    <trd lng="it"><ind>aquila</ind> di mare a coda bianca</trd>
    <trdgrp lng="pl">
      <trd>orze&lstroke; bielik</trd>,
      <trd>bielik</trd>
    </trdgrp>
    <trd lng="ro">codalb </trd>
    <trd lng="sv">havs<ind>&ouml;rn</ind></trd>
  </snc>
  <trd lng="ja">&#x30AA;&#x30B8;&#x30ED;&#x30EF;&#x30B7;</trd>
</drv>

