<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>agl</rad>/o
</kap>
<drv mrk="agl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio
      <ref tip="super" cel="akcipi1.0oj">akcipitredoj</ref>
      (<trd lng="la">Aquila</trd>), kaj similaj rabobirdoj kun
      kurba beko:
      <ekz>
        pasero kaptita estas pli bona, ol <tld/>o kaptota
        <fnt><bib>F</bib>, <lok>ekzercaro, &para; 22</lok></fnt>;
      </ekz>
    </dif>
    <url ref="&Viki;/aglo">artikolo en Vikipedio</url>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_o;&c_l;</trd>
  <trd lng="br">erer</trd>
  <trd lng="bg">&c_o;&c_r;&c_je;&c_l;</trd>
  <trdgrp lng="de">
    <trd>Adler</trd>,
    <trd>Aar<klr> [poezie]</klr></trd>
  </trdgrp>
  <trd lng="en">eagle</trd>
  <trd lng="es">&aacute;guila</trd>
  <trd lng="fr">aigle</trd>
  <trd lng="hu">sas</trd>
  <trdgrp lng="nl">
    <trd>arend</trd>,
    <trd>adelaar</trd>
  </trdgrp>
  <trd lng="pl">orze&lstroke;</trd>
  <trd lng="pt">&aacute;guia</trd>
  <trd lng="tr">kartal</trd>
  <trd lng="ru">&c_o;&c_r;&c_jo;&c_l;</trd>
  <trd lng="sk">orol</trd>
  <trd lng="sv">&ouml;rn</trd>
</drv>

<drv mrk="agl.0eca">
  <kap><tld/>eca</kap>
  <snc>
    <dif>
      Simila al <tld/>o:
      <ekz>
        <tld/>eca nazo, rigardo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_l;&c_ib;&c_n;&c_y;</trd>
  <trd lng="br">-<ind>erer</ind></trd>
  <trd lng="de">Adler-</trd>
  <trd lng="en">aquiline</trd>
  <trd lng="es">agile&ntilde;o</trd>
  <trdgrp lng="fr">
    <trd>aquilin <klr>(nez)</klr></trd>,
    <trd>d'<ind>aigle</ind></trd>
  </trdgrp>
  <trd lng="hu">sas-</trd>
  <trdgrp lng="nl">
    <trd>arends-</trd>,
    <trd>adelaars-</trd>
  </trdgrp>
  <trd lng="pl">orli</trd>
  <trdgrp lng="pt">
    <trd>aquilino</trd>,
    <trd>adunco</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_r;&c_l;&c_i;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">orl&iacute;</trd>
</drv>

<drv mrk="agl.fisx0o">
  <kap>fi&scirc;<tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <ref tip="dif" cel="pandio.0o">pandiono</ref>
  </snc>
  <trd lng="br">erer-spluj</trd>
  <trd lng="de">Fischadler</trd>
  <trd lng="en">osprey</trd>
  <trd lng="fr">aigle bott&eacute;</trd>
  <trd lng="hu">hal&aacute;szsas</trd>
  <trd lng="pl">rybo&lstroke;&oacute;w</trd>
  <trd lng="pt">&aacute;guia-pescadora</trd>
  <trd lng="ru">&c_s;&c_k;&c_o;&c_p;&c_a;</trd>
</drv>
</art>
