    <trd lng="en">Bonelli&#8217;s eagle</trd>
    <trd lng="it"><ind>aquila</ind> del Bonelli</trd>
    <trd lng="la">Hieraaetus fasciatus</trd>
  </snc>
  <trd lng="cs">orel jest&rcaron;&aacute;b&iacute;</trd>
  <trd lng="sk">orol jastrabovit&yacute;</trd>
</drv>

<drv mrk="agl.bot0o">
  <kap>bot<tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <fnt>
       <aut>Wouter F. Pilger</aut>,
       <vrk>
         <url ref="http://www.xs4all.nl/~pilger/wfp/birdeou8.htm">
           Komunlingvaj nomoj de E&ubreve;ropaj birdoj</url>
       </vrk>
    </fnt>
    <dif>
      Specio de <ref tip="super" cel="agl.0o">aglo</ref>
      (
      <trdgrp lng="pl">
        <trd>Hieraa&euml;tus pennatus</trd> a&ubreve;
        <trd>Hieraaetus pennatus</trd>
      </trdgrp>
      ).
    </dif>
    <trd lng="ca">&agrave;guila cal&ccedil;ada</trd>
    <trd lng="en">booted eagle</trd>
    <trd lng="it"><ind>aquila</ind> pennata</trd>
    <trdgrp lng="la">
      <trd>Hieraaetus pennatus</trd>,
      <trd>Aquila pennata</trd>
    </trdgrp>
    <trd lng="pl">orze&lstroke;ek</trd>
    <trd lng="sv">dv&auml;rg<ind>&ouml;rn</ind></trd>
  </snc>
  <trd lng="cs">orel nejmen&scaron;&iacute;</trd>
  <trd lng="sk">orol mal&yacute;</trd>
</drv>

<drv mrk="agl.serpent0o">
  <kap>serpent<tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <fnt>
       <aut>Wouter F. Pilger</aut>,
       <vrk>
         <url ref="http://www.xs4all.nl/~pilger/wfp/birdeou8.htm">
           Komunlingvaj nomoj de E&ubreve;ropaj birdoj</url>
       </vrk>
    </fnt>
    <dif>
      Specio de <ref tip="super" cel="agl.0o">aglo</ref>
      (
      <trdgrp lng="pl">
        <trd>Circa&euml;tus gallicus</trd> a&ubreve;
        <trd>Circaetus gallicus</trd>
      </trdgrp>
      ).
    </dif>
    <ref tip="sin" cel="cirkaet.0o">cirkaeto</ref>
    <trd lng="ca">&agrave;guila marcenca</trd>
    <trd lng="en">short-toed <ind>eagle</ind></trd>
    <trd lng="it">biancone<klr> (zoo)</klr></trd>
    <trd lng="la">Circaetus gallicus</trd>
    <trd lng="pl">gado&zdot;er</trd>
    <trd lng="sv">orm<ind>&ouml;rn</ind></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>orel kr&aacute;tkoprst&yacute;</trd>,
    <trd>orl&iacute;k kr&aacute;tkoprst&yacute;</trd>
  </trdgrp>
  <trd lng="sk">hadiar kr&aacute;tkoprst&yacute;</trd>
</drv>

