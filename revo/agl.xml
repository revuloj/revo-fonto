<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>agl</rad>/o
</kap>
<drv mrk="agl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio
      <ref tip="super" cel="akcipi.0edoj">akcipitredoj</ref>
      (<trd lng="la">Aquila</trd>), kaj similaj rabobirdoj kun
      kurba beko.
    </dif>
  </snc>
  <trd lng="de">Adler</trd>
  <trd lng="en">eagle</trd>
  <trd lng="fr">aigle</trd>
  <trd lng="tr">kartal</trd>
  <trd lng="ru">&#1086;&#1088;&#1105;&#1083;</trd>
</drv>
<drv mrk="agl.0eca">
  <kap><tld/>eca</kap>
  <snc>
    <dif>
      Simila al <tld/>o:
      <ekz>
        <tld/>eca nazo, rigardo.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">aquiline</trd>
  <trdgrp lng="fr">
    <trd>aquilin <klr>(nez)</klr></trd>,
    <trd>d'<ind>aigle</ind></trd>
  </trdgrp>
  <trd lng="ru">&#1086;&#1088;&#1083;&#1080;&#1085;&#1099;&#1081;</trd>
</drv>
<drv mrk="agl.fisx0o">
  <kap>fi&scirc;<tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="pandio.0o">Pandiono</ref>.
    </dif>
  </snc>
  <trd lng="de">Fischadler</trd>
</drv>
</art>
