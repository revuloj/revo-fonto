<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>apena&ubreve;</rad>
</kap>
<drv mrk="apenau.0">
   <kap><ofc>*</ofc><tld/></kap>
   <subdrv>
     <dif>
       Adverbo, signifanta:
     </dif>
     <snc mrk="apenau.0.nePli">
       <dif>
         Ne pli multe ol:
         <ekz>
           tio kostas <tld/> kvin frankojn;
         </ekz>
         <ekz>
           mi restis <tld/> tri tagojn en Berlino.
         </ekz>
       </dif>
       <trdgrp lng="de">
         <trd>nicht mehr als</trd>,
         <trd>kaum</trd>
       </trdgrp>
       <trdgrp lng="en">
         <trd>no <ind>more</ind> than</trd>,
         <trd>barely</trd>
       </trdgrp>
     </snc>
     <snc mrk="apenau.0.preskauNe">
       <dif>
         Preska&ubreve; ne:
         <ekz>
           li <tld/> scias legi;
         </ekz>
         <ekz>
           &scirc;i povis <tld/> reteni siajn larmojn;
         </ekz>
         <ekz>
           <ind>la ventego <tld/> ne dronigis la &scirc;ipon</ind>
           <trd lng="fi">myrsky melkein hukutti laivan</trd>.
         </ekz>
       </dif>
       <trdgrp lng="en">
         <trd>hardly</trd>,
         <trd>barely</trd>
       </trdgrp>
       <trdgrp lng="de">
         <trd>fast nicht</trd>,
         <trd>kaum</trd>
       </trdgrp>
     </snc>
     <snc mrk="apenau.0.jxuse">
       <dif>
         Tute &jcirc;use:
         <ekz>
           li estis <tld/> foririnta, kiam ni venis.
         </ekz>
       </dif>
       <trdgrp lng="en">
         <trd>barely</trd>
       </trdgrp>
       <trd lng="de">kaum</trd>
     </snc>
     <trdgrp lng="be">
       <trd>&c_l;&c_je;&c_d;&c_z;&c_mol;</trd>,
       <trd>&c_l;&c_je;&c_d;&c_z;&c_mol;&c_v;&c_je;</trd>,
       <trd>&c_a;&c_m;&c_a;&c_l;&c_mol;</trd>
     </trdgrp>
     <trdgrp lng="br">
       <trd>a-boan</trd>,
       <trd>a-vec'h</trd>
     </trdgrp>
     <trd lng="cs">sotva</trd>
     <trd lng="es">apenas</trd>
     <trd lng="fr">&agrave; <ind>peine</ind>
     <klr tip="ind"> (adverbe)</klr></trd>
     <trdgrp lng="fi">
       <trd>tuskin</trd>
     </trdgrp>
     <trd lng="it">appena</trd>
     <trdgrp lng="pl">
       <trd>zaledwie</trd>,
       <trd>ledwie</trd>
       <trd>ledwo</trd>,
       <trd>prawie</trd>,
       <trd>niemal</trd>,
       <trd>nieomal</trd>,
       <trd>niemal&zdot;e</trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>&c_je;&c_d;&c_v;&c_a;</trd>,
       <trd>&c_je;&c_l;&c_je;</trd>
     </trdgrp>
     <trdgrp lng="sk">
       <trd>sotva</trd>,
       <trd>ledva</trd>
     </trdgrp>
     <trd lng="sv">knappt</trd>
     <trd lng="vo">kot&ouml;</trd>
   </subdrv>
   <subdrv>
     <dif>
       Subordiga konjunkcio, signifanta:
     </dif>
     <snc mrk="apenau.0.konjunkcio">
       <dif>
         Tuj post kiam, tuj de kiam:
         <ekz>
           <tld/> &scirc;i venis al la fonto, &scirc;i vidis unu
           sinjorinon, tre ri&ccirc;e vestitan
           <fnt><bib>F</bib><lok>&FE; 19</lok></fnt>;
         </ekz>
         <ekz>
           <tld/> li ricevis la leteron, li tuj sendis
           la respondon<fnt>K</fnt>;
         </ekz>
         <ekz>
           <tld/> li finis, jen <klr>(a&ubreve;: jam)</klr> li vidis.
         </ekz>
       </dif>
       <trdgrp lng="br">
         <trd>a-boan ma</trd>,
         <trd>a-vec'h ma</trd>
       </trdgrp>
       <trdgrp lng="fi">
         <trd>heti kun</trd>,
         <trd>tuskin ... kun jo</trd>
       </trdgrp>
       <trdgrp lng="fr">
         <trd>&agrave; <ind>peine</ind>
         <klr tip="ind"> (conj. sub.)</klr></trd>,
         <trd>d&egrave;s que</trd>
       </trdgrp>
       <trdgrp lng="pl">
         <trd>zaledwie</trd>,
         <trd>ledwo</trd>,
         <trd>ledwie</trd>
       </trdgrp>
       <trdgrp lng="ru">
         <trd>&c_je;&c_d;&c_v;&c_a;</trd>,
         <trd>&c_l;&c_i;&c_sh;&c_mol; &c_t;&c_o;&c_l;&c_mol;&c_k;&c_o;
         </trd>,
         <trd>&c_k;&c_a;&c_k; &c_t;&c_o;&c_l;&c_mol;&c_k;&c_o;</trd>
       </trdgrp>
     </snc>
   </subdrv>
</drv>
</art>
