<kap>
  <rad>akvamarin</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="akvamarin.0o">
  <kap><tld/>o</kap>
  <snc mrk="akvamarin.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      <ref tip="super" cel="sxton.juvel0o.MIN">Juvel&scirc;tono</ref>,
      diafana <ref tip="super" cel="beril.0o.MIN">berilo</ref>
      kun koloro marakva, tio estas varianta de helblua &gcirc;is verdablua:
      <ekz>
        al la plej valoraj juveloj apartenas <klr>...</klr> varia&jcirc;oj de
        berilo <klr>...</klr> smeraldo, <tld/>o, krizolito
        <fnt>
        <aut>Galina Beliakova</aut>,
        <vrk><url ref="http://www.linabel.de/Gem/Eo/Gemmologie800_eo.html">gemologio</url></vrk>,
        <lok>[vidita je 2006-02-17]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x7DA0;&#29577; <pr>l&#xFC;y&#xF9;</pr></trd>,
    <trd>&#x7EFF;&#29577; <pr>l&#xFC;y&#xF9;</pr></trd>,
    <trd>&#x84DD;&#29577; <pr>l&#xE1;ny&#xF9;</pr></trd>,
    <trd>&#x85CD;&#29577; <pr>l&#xE1;ny&#xF9;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_k;&c_v;&c_a;&c_m;&c_a;&c_r;&c_y;&c_n;</trd>
  <trd lng="bg">&c_a;&c_k;&c_v;&c_a;&c_m;&c_a;&c_r;&c_i;&c_n;</trd>
  <trd lng="cs">akvamar&#xED;n</trd>
  <trd lng="de">Aquamarin</trd>
  <trd lng="en">aquamarine</trd>
  <trd lng="es">aguamarina</trd>
  <trd lng="fa">&a_z;&a_m;&a_r;&a_d; &a_A_madda;&a_b;&f_y;</trd>
  <trd lng="fr">aigue-marine</trd>
  <trd lng="he">&#x5D0;&#x5D9;&#x5D6;&#x5DE;&#x5E8;&#x5D2;&#x5D3; (&#x5D0;&#x5D1;&#x5DF;
  &#x5D9;&#x5E7;&#x5E8;&#x5D4;)</trd>
  <trd lng="hu">akvamarin</trd>
  <trd lng="it">acquamarina</trd>
  <trd lng="nl">aquamarijn</trd>
  <trd lng="pl">akwamaryn</trd>
  <trd lng="pt">&aacute;gua-marinha</trd>
  <trd lng="ro">acvamarin</trd>
  <trd lng="sk">akvamar&#xED;n</trd>
  <trd lng="uk">&c_a;&#x43A;&c_v;&c_a;&#x43C;&c_a;&c_r;&c_i;&#x43D;</trd>
</drv>
