<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: tirs.xml,v 1.5 2024/01/27 21:19:26 revo Exp $">
    <kap>
      <rad>tirs</rad>/o
    </kap>

<drv mrk="tirs.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Kunmetita <ref tip="super" cel="inflor.0o">infloresko</ref>, &ccirc;e kiu
      <ref tip="prt" cel="cum.0o">cumo</ref>j ordi&gcirc;as &ccirc;irka&ubreve; &ccirc;efa akso
      kiel en <ref tip="prt" cel="grapol.0o">grapolo</ref>,
      <ref tip="prt" cel="spik.0o">spiko</ref> a&ubreve;
      <ref tip="prt" cel="kap.0o.BOT">kapo</ref>:
      <ekz>
        fakte, plantoscience, la floraro de la vito estas <tld/>o, tio estas grapolo de
        cumetoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16573">Grapolo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        fragmito <klr>[&#8230;]</klr> havas <klr>[&#8230;]</klr> 30&#x202F;cm longan <tld/>on el floroj
        &ccirc;irka&ubreve;ataj de ar&gcirc;ente brilaj haroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=23196">Fragmito</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
    lok="&WCU;/thumb/1/16/Inflorescence_morphology_thyrse.png/109px-Inflorescence_morphology_thyrse.png"
    prm="PD"><tld/>o
        <fnt>
          <aut>Michael G. Simpson, reporduktita de uzanto RoRo; el Simpson (2005) &leftquot;Plant
          Systematics&rightquot;</aut>
          <lok><url
          ref="&WCW;/File:Inflorescence_morphology_thyrse.png">&WCW;/File:Inflorescence_morphology_thyrse.png</url></lok>
        </fnt>
    </bld>
  </snc>
    <trd lng="be">&c_t;&c_y;&c_r;&c_s; <klr tip="amb">(&c_s;&c_u;&c_k;&c_v;&c_je;&c_c;&c_mol;&c_c;&c_je;)</klr></trd>
    <trd lng="de">Thyrsus</trd>
    <trd lng="en">thyrse</trd>
    <trd lng="fr">thyrse</trd>
    <trd lng="pl">tyrs</trd>
    <trd lng="uk">&c_t;&c_i;&c_r;&c_s;</trd>
</drv>
  </art>

