<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>baba</rad>/o
</kap>
<drv mrk="baba.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Franca speco de <ref tip="super" cel="kuk.0o">kuko</ref> 
      kun sekvinberoj, surver&scirc;ita per rumo.
    </dif>
  </snc>
  <trd lng="cs">b&aacute;bovka</trd>
  <trd lng="de">Gugelhupf</trd>
  <trd lng="fr">baba au rhum</trd>
  <trd lng="hu">rumos <ind>kugl&oacute;f</ind></trd>
  <trd lng="nl">baba</trd>
  <trd lng="pl">babeczka ponczowa</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_o;&c_m;&c_o;&c_v;&c_a;&c_ja;
    <ind>&c_b;&c_a;&c_b;&c_a;</ind></trd>,
    <trd>&c_k;&c_u;&c_l;&c_i;&c_ch;</trd>
  </trdgrp>
  <trd lng="sv">baba</trd>
</drv>
<adm>
  Provizore aldonis "franca speco de", &ccirc;ar la resto de la difino
  estas tro nepreciza por ekz. ekskludi "plumpudding".
  <aut>wd</aut>
</adm>
</art>
