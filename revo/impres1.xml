  <trd lng="pt">empres&aacute;rio<klr tip="amb"> (de representa&ccedil;&otilde;es teatrais,
  circenses, ou de concertos)</klr></trd>
  <trd lng="ru">&c_i;&c_m;&c_p;&c_r;&c_je;&c_s;&c_a;&c_r;&c_i;&c_o;</trd>
  <trdgrp lng="sk">
