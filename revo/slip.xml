<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>5</ofc>
  <rad>slip</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="slip.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Kartona a&ubreve; papera folieto, facile klasigebla, sur kiu oni skribas
      ian noton.
    </dif>
  </snc>
  <trd lng="de">Zettel</trd>
  <trdgrp lng="nl">
    <trd>kaart<klr tip="ind"> (in kaartsysteem)</klr></trd>,
    <trd>briefje</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_t;&c_o;&c_ch;&c_k;&c_a; <klr tip="amb">(&c_d;&c_l;&c_ja; &c_k;&c_a;&c_r;&c_t;&c_o;&c_t;&c_je;&c_k;&c_i;)</klr></trd>
</drv>

<drv mrk="slip.suflor0eto">
  <kap>suflor<tld/>eto, ka&scirc;<tld/>eto</kap>
  <snc>
    <dif>
      Kartona a&ubreve; papera folieto kun notoj, kiun oni ka&scirc;e uzas dum ekzameno.
    </dif>
  </snc>
  <trd lng="de">Spickzettel</trd>
  <trd lng="nl">spiekbriefje</trd>
  <trd lng="ru">&c_sh;&c_p;&c_a;&c_r;&c_g;&c_a;&c_l;&c_k;&c_a;</trd>
</drv>
</art>
