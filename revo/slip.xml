<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>5</ofc>
  <rad>slip</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="slip.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Kartona a&ubreve; papera folieto, facile klasigebla, sur kiu oni skribas
      ian noton:
      <ekz>
        la unua libreto, 87-pa&gcirc;a, prezentas ilustra&jcirc;ojn kaj tekstetojn; la
        dua &ndash; eltondotajn <tld/>ojn kun a&ubreve; sen vortoj
        dorsflanke<fnt><aut>Alessandra Di Donato</aut>: <vrk>Ni ludas
        por...lerni</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero 5a, p.
        23a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_r;&c_t;&c_k;&c_a;</trd>, 
    <trd>&c_k;&c_a;&c_r;&c_t;&c_a;&c_ch;&c_k;&c_a; 
      <klr tip="amb">(&c_d;&c_l;&c_ja; 
      &c_k;&c_a;&c_r;&c_t;&c_a;&c_t;&c_e;&c_k;&c_ib;)</klr></trd>
  </trdgrp>
  <trd lng="de">Zettel</trd>
  <trdgrp lng="fr">
    <trd>fiche</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kaart<klr tip="ind"> (in kaartsysteem)</klr></trd>,
    <trd>briefje</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_t;&c_o;&c_ch;&c_k;&c_a; <klr tip="amb">(&c_d;&c_l;&c_ja;
&c_k;&c_a;&c_r;&c_t;&c_o;&c_t;&c_je;&c_k;&c_i;)</klr></trd>
</drv>

<drv mrk="slip.suflor0eto">
  <kap>suflor<tld/>eto</kap>
  <snc>
    <dif>
      Kartona a&ubreve; papera folieto kun notoj, kiun oni ka&scirc;e uzas dum ekzameno.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="slip.kasx0eto">ka&scirc;<tld/>eto</ref>
  </refgrp>
  <trd lng="be">&c_sh;&c_p;&c_a;&c_r;&c_g;&c_a;&c_l;&c_k;&c_a;</trd>
  <trd lng="de">Spickzettel</trd>
  <trdgrp lng="fr">
    <trd>antis&egrave;che</trd>
  </trdgrp>
  <trd lng="nl">spiekbriefje</trd>
  <trd lng="ru">&c_sh;&c_p;&c_a;&c_r;&c_g;&c_a;&c_l;&c_k;&c_a;</trd>
</drv>

<drv mrk="slip.kasx0eto">
  <kap>ka&scirc;<tld/>eto</kap>
  <snc>
    <ref tip="dif" cel="slip.suflor0eto">suflor<tld/>eto</ref>
  </snc>
  <trd lng="be">&c_sh;&c_p;&c_a;&c_r;&c_g;&c_a;&c_l;&c_k;&c_a;</trd>
  <trd lng="de">Spickzettel</trd>
  <trdgrp lng="fr">
    <trd>antis&egrave;che</trd>
  </trdgrp>
  <trd lng="nl">spiekbriefje</trd>
  <trd lng="ru">&c_sh;&c_p;&c_a;&c_r;&c_g;&c_a;&c_l;&c_k;&c_a;</trd>
</drv>
</art>
