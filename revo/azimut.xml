<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>azimut</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="azimut.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Angulo inter la meridiano astronomia a&ubreve; magneta kaj la vertikala
      ebeno a&ubreve; la vertikalo, kiu trapasas tra difinita astro
      a&ubreve; tra difinita terpunkto.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_z;&c_ib;&c_m;&c_u;&c_t;</trd>
  <trd lng="de">Azimut</trd>
  <trd lng="en">azimuth</trd>
  <trdgrp lng="es">
    <trd>acimut</trd>,
    <trd>azimut</trd>
  </trdgrp>
  <trd lng="fr">azimut</trd>
  <trd lng="hu">azimut</trd>
  <trd lng="nl">azimut</trd>
  <trd lng="ru">&c_a;&c_z;&c_i;&c_m;&c_u;&c_t;</trd>
  <trd lng="sk">azimut</trd>
</drv>
</art>
