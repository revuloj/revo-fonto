<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>pirat</rad>/o
</kap>
<drv mrk="pirat.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Mara rabisto.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Pirat</trd>,
    <trd>Seer&auml;uber</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>piraat</trd>,
    <trd>zeerover</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_i;&c_r;&c_a;&c_t;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_s;&c_k;&c_o;&c_j; &c_r;&c_a;&c_z;&c_b;&c_o;&c_j;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>pirat</trd>,
    <trd>sj&ouml;r&ouml;vare</trd>
  </trdgrp>
</drv>

<drv mrk="pirat.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>o.
    </dif>
    <refgrp tip="sin">
      <ref cel="korsar.0o">korsaro</ref>,
      <ref cel="kaper.0i">kaperi</ref>.
    </refgrp>
  </snc>
  <trd lng="ru">&c_p;&c_i;&c_r;&c_a;&c_t;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
