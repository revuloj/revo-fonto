<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>fulm</rad>/o</kap>
<drv mrk="fulm.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fulm.0o.atmosfera">
    <uzo tip="fak">MET</uzo>
    <dif>
      Atmosfera fenomeno konsistanta el elektra mal&scirc;argo inter nuboj
      a&ubreve; nuboj kaj tero, akompanata de bran&ccirc;i&gcirc;anta,
      rapide pasanta, fajra kanalo:
      <ekz>
        altan arbon batas la <tld/>o
        <klr>(ju pli elstara, des pli en
        dan&gcirc;ero)</klr><fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ekbrilis sur la &ccirc;ielo grandaj zigzagaj <tld/>oj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        liaj okuloj &jcirc;etis <tld/>ojn<fnt>B</fnt>;
      </ekz>
      <ekz>
        en la da&ubreve;ro de <tld/>o <klr>(tre mallonge)</klr>;
      </ekz>
      <ekz>
        kun <tld/>a rapideco.
      <trd>vill&aacute;mgyorsan</trd>
      </ekz>
    </dif>
    <trd lng="vo">lelit</trd>
  </snc>
  <snc mrk="fulm.0o.ekbrilo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Subita kaj malda&ubreve;ra ekbrilo:
      <ekz>
        ek<tld/>oj de genio, de sa&gcirc;o.
      </ekz>
    </dif>
    <adm>
      Pli bone trovi ekz kun ~o anstataux ek~o. <aut>MB</aut>
    </adm>
  </snc>
  <snc mrk="fulm.0o.puno">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Timinda puno.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_a;&c_n;&c_k;&c_a;</trd>
  <trd lng="de">Blitz</trd>
  <trd lng="en">lightning</trd>
  <trd lng="es">rayo</trd>
  <trdgrp lng="fr">
    <trd>foudre</trd>,
    <trd>&eacute;clair</trd>
  </trdgrp>
  <trd lng="hu">vill&aacute;m</trd>
  <trd lng="nl">bliksem</trd>
  <trd lng="ru">&c_m;&c_o;&c_l;&c_n;&c_i;&c_ja;</trd>
</drv>

<drv mrk="fulm.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="fulm.0i.atmosfere">
    <uzo tip="fak">MET</uzo>
    <dif>
      Ekbrili, p.p. <tld/>o:
      <ekz>
        ek<tld/>as;
      </ekz>
      <ekz>
        aspektis kvaza&ubreve; la tuta mondo staris en flamoj, tiel
        forte <tld/>is<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="fr">y avoir des <ind>&eacute;clairs</ind></trd>
  </snc>
  <snc mrk="fulm.0i.brili">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Brili kiel <tld/>o:
      <ekz>
        <tld/>as lancoj
        <fnt><bib>MT</bib>, <lok>&Nah; 3:3</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a <klr>(bolanta)</klr> knabego<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind><tld/>anta</ind> rigardo;
        <trdgrp lng="fr">
          <trd>foudroyant</trd>,
          <trd>fulgurant</trd>
        </trdgrp>
      </ekz>
      <ekz>
        en la pupiloj <tld/>etadis briloj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        sur &scirc;iaj lipoj sen&ccirc;ese tremadis <tld/>etanta
        rideto<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>jeter des <ind>&eacute;clairs</ind></trd>,
      <trd>fulgurer</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_b;&c_l;&c_ib;&c_s;&c_n;&c_u;&c_c;&c_mol;</trd>
  <trd lng="de">blitzen</trd>
  <trd lng="hu">vill&aacute;mlik</trd>
  <trd lng="nl">bliksemen</trd>
  <trd lng="ru">&c_s;&c_v;&c_je;&c_r;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>
</drv>

<drv mrk="fulm.0obati0ofrapi">
  <kap><tld/>obati, <tld/>ofrapi</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      Trafi per <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ib;&c_c;&c_mol; <ind>&c_m;&c_a;&c_l;&c_a;&c_n;&c_k;&c_a;&c_j;</ind></trd>
  <trd lng="fr">foudroyer</trd>
  <trd lng="hu">vill&aacute;m s&uacute;jtja</trd>
  <trdgrp lng="nl">
    <trd>neerbliksemen</trd>,
    <trd>doodbliksemen</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_r;&c_a;&c_z;&c_i;&c_t;&c_mol; &c_m;&c_o;&c_l;&c_n;&c_i;&c_je;&c_j;</trd>
</drv>

<drv mrk="fulm.0oforigilo0osxirmilo">
  <kap><tld/>oforigilo, <tld/>o&scirc;irmilo</kap>
  <uzo tip="fak">TEK</uzo>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      Pinta fera vergo, starigita sur la supro de konstrua&jcirc;o por
      &gcirc;in &scirc;irmi kontra&ubreve; la <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_r;&c_o;&c_m;&c_a;&c_a;&c_d;&c_v;&c_o;&c_d;</trd>,
    <trd>&c_m;&c_a;&c_l;&c_a;&c_n;&c_k;&c_a;&c_a;&c_d;&c_v;&c_o;&c_d;</trd>
  </trdgrp>
  <trd lng="de">Blitzableiter</trd>
  <trd lng="es">pararrayos</trd>
  <trdgrp lng="fr">
    <trd>parafoudre</trd>,
    <trd>paratonnerre</trd>
  </trdgrp>
  <trd lng="hu">vill&aacute;mh&aacute;r&iacute;t&oacute;</trd>
  <trd lng="nl">bliksemafleider</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_o;&c_m;&c_o;&c_o;&c_t;&c_v;&c_o;&c_d;</trd>,
    <trd>&c_m;&c_o;&c_l;&c_n;&c_i;&c_je;&c_o;&c_t;&c_v;&c_o;&c_d;</trd>
  </trdgrp>
</drv>

<drv mrk="fulm.0otondro">
  <kap><tld/>otondro</kap>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      Neda&ubreve;ra pluvego kun <tld/>oj kaj tondroj:
      <ekz>
        kiu semas venton, rikoltos <tld/>otondron<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="uragan.0o">uragano</ref>,
      <ref cel="sxtorm.0o">&scirc;tormo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="de">Gewitter</trd>
  <trd lng="fr">orage</trd>
  <trdgrp lng="hu">
    <trd>zivatar</trd>,
    <trd>&eacute;gih&aacute;bor&uacute;</trd>
  </trdgrp>
  <trd lng="nl">onweer</trd>
  <trd lng="ru">&c_g;&c_r;&c_o;&c_z;&c_a;</trd>
</drv>
</art>
