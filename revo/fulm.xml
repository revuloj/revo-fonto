<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fulm.xml,v 1.48 2026/03/01 08:47:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fulm</rad>/o
<fnt><bib>UV</bib></fnt></kap>

<drv mrk="fulm.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fulm.0o.atmosfera">
    <uzo tip="fak">MET</uzo>
    <dif>
      Atmosfera <ref tip="super" cel="fenome.0o.FIL">fenomeno</ref>
      konsistanta el elektra mal&scirc;argo inter nuboj
      a&ubreve; nuboj kaj tero, akompanata de bran&ccirc;i&gcirc;anta,
      rapide pasanta, fajra kanalo:
      <ekz>
        tuj matene, komenci&gcirc;is tondroj kaj <tld/>oj, kaj densa nubo estis super la monto
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 19:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fajraj <tld/>oj zigzage sin &jcirc;etadis en la &scirc;a&ubreve;man maron
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        sentondra <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li eksentis subite kvaza&ubreve; radion de <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Persista stana soldato</lok>
        </fnt>;
      </ekz>
      <ekz>
        altan arbon batas la <tld/>o
        <klr>(ju pli elstara, des pli en
        dan&gcirc;ero)</klr><fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> <ind><tld/>orapide</ind> preterflugadis preter miaj oreloj
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
        <trd lng="hu">vill&aacute;mgyorsan</trd>
        <trd lng="nl">bliksemsnel</trd>
        <trd lng="no">lynrask</trd>
        <trd lng="pl">b&lstroke;yskawicznie</trd>
      </ekz>
      <ekz>
        dum parteto de sekundo &dash; nur tiom longe, kiom da&ubreve;ras la lumo de <tld/>o
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Flugo al la Ebura Turo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="id">
      <trd>kilat</trd>,
      <trd>halilintar</trd>,
      <trd>mata petir</trd>,
      <trd>petir</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>b&lstroke;yskawica</trd>,
      <trd>piorun</trd>,
      <trd>grom</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>rel&acirc;mpago</trd>,
      <trd>raio</trd>,
      <trd>corisco</trd>,
      <trd>fa&iacute;sca</trd>
    </trdgrp>
    <trd lng="ro">fulger</trd>
    <trd lng="vo">lelit</trd>
    <trd lng="sv">blixt</trd>
  </snc>

  <snc mrk="fulm.0o.ekbrilo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Subita kaj malda&ubreve;ra ekbrilo:
      <ekz>
        kie nun estas viaj sprita&jcirc;oj, viaj artifikoj, viaj kantoj, viaj <tld/>oj de
        gajeco, &ccirc;e kiuj la tuta tablo eksplodadis per rido
        <fnt>
          <bib>Hamlet</bib>
        </fnt>?
      </ekz>
    </dif>
    <adm>
      Pli bone trovi ekz kun ~o anstataux ek~o. <aut>MB</aut>
    </adm>
    <trdgrp lng="id">
      <trd>denyar</trd>,
      <trd>denyaran</trd>,
      <trd>kilatan</trd>
    </trdgrp>
    <trd lng="pl">b&lstroke;yskawica</trd>
    <trdgrp lng="pt">
      <trd>rel&acirc;mpago</trd>,
      <trd>raio</trd>,
      <trd>corisco</trd>,
      <trd>fa&iacute;sca</trd>
    </trdgrp>
    <trd lng="sv">blixt</trd>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_a;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="bg">
    <trd>&c_g;&c_r;&c_malmol;&c_m;</trd>,
    <trd>&c_m;&c_malmol;&c_l;&c_n;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="bo">&t_ga;&t_lata;&t_naro;&t_ga;&t_tsheg;&t_ga;&t_zha;&t_shabchu;&t_tsheg;</trd>
  <trd lng="cs">blesk</trd>
  <trd lng="de">Blitz</trd>
  <trd lng="en">lightning</trd>
  <trdgrp lng="es">
    <trd>rayo</trd>,
    <trd>rel&aacute;mpago</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_b;&a_r;&a_q;</trd>,
    <trd>&a_A_madda;&a_dh;&a_r;&a_kh;&a_sh;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>foudre</trd>,
    <trd>&eacute;clair</trd>
  </trdgrp>
  <trd lng="hu">vill&aacute;m</trd>
  <trdgrp lng="ja">
    <trd>&#x96F7;  <pr>&#x306A;&#x308B;&#x304B;&#x307F;</pr></trd>,
    <trd>&#x7A32;&#x59BB;  <pr>&#x3044;&#x306A;&#x305A;&#x307E;</pr></trd>,
    <trd>&#x96F7;&#x5149;  <pr>&#x3089;&#x3044;&#x3053;&#x3046;</pr></trd>,
    <trd>&#x3072;&#x3089;&#x3081;&#x304D;</trd>
  </trdgrp>
  <trd lng="nl">bliksem</trd>
  <trd lng="no">lyn</trd>
  <trd lng="ru">&c_m;&c_o;&c_l;&c_n;&c_i;&c_ja;</trd>
  <trd lng="sk">blesk</trd>
  <trd lng="uk">&c_b;&c_l;&c_i;&c_s;&c_k;&c_a;&c_v;&c_k;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x96F7;&#x9583; <pr>l&eacute;ish&#x1CE;n</pr></trd>,
    <trd>&#x5929;&#x706B; <pr>ti&amacron;nhu&#x1D2;</pr></trd>,
    <trd>&#x7535;&#x95EA; <pr>di&agrave;nsh&#x1CE;n</pr></trd>,
    <trd>&#x975D;&#x706B; <pr>ti&amacron;nhu&#x1D2;</pr></trd>,
    <trd>&#x9583;&#x96FB; <pr>sh&#x1CE;ndi&agrave;n</pr></trd>,
    <trd>&#x9583; <pr>sh&#x1CE;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fulm.0a">
  <kap>
    <tld/>a
  </kap>
  <snc mrk="fulm.0a.rilata">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        tra la nigraj nebuloj, gutigantaj pli kaj pli densan pluvon, komencis sin traigi
        <tld/>aj briloj
        <fnt>
          <bib>Lasta</bib>
          <lok>La Lasta</lok>
        </fnt>;
      </ekz>
    </dif>
    <trd lng="de">Blitz-</trd>
  </snc>
  <snc mrk="fulm.0a.kiel">
    <dif>
      <ref tip="super" cel="subit.0a">Subita</ref>, <ref tip="super" cel="rapid.0a.subita">rapida</ref>,
      <ref tip="super" cel="frap.0a">frapa</ref> kvaza&ubreve; <tld/>o:
      <ekz>
        <tld/>a komprenemo
        <fnt>
          <bib>ChV</bib>
          <lok>25</lok>
        </fnt>;
      </ekz>
      <ekz>
        morto terura, sed subita, <tld/>a
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        per <tld/>a movo &scirc;i for&jcirc;etis la tukon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        trafita de <tld/>a sort'
        <fnt>
          <bib>Monato</bib>
          <aut>Alejandro Cossavella</aut>
          <vrk><url ref="&Monato;/2005/008766.php"
              >Poemoj de Alejandro Cossavella</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a disvolvi&gcirc;o de elektronikaj rimedoj
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Schenkel</aut>
          <vrk><url ref="&Monato;/2010/009985.php"
              >Fluge tra la flughaveno</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rapid.0ega">fulmrapida</ref>
    </refgrp>
    <trd lng="de">blitzschnell</trd>
  </snc>
</drv>

<drv mrk="fulm.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="fulm.0i.atmosfere">
    <uzo tip="fak">MET</uzo>
    <dif>
      Ekbrili, p.p. <tld/>o:
      <ekz>
        ekstere tondris kaj <tld/>is
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        fari&gcirc;is granda uragano: <tld/>is kaj tondris, forte pluvegis
        <fnt>
          <bib>Fab1</bib>
          <lok>Re&gcirc;idino sur pizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        aspektis <klr>[&#x2026;]</klr> kvaza&ubreve; la tuta mondo staris en flamoj, tiel
        forte <tld/>is
        <fnt>
          <bib>Fab2</bib>
          <lok>Poligono</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fa">&a_A_madda;&a_dh;&a_r;&a_kh;&a_sh;
    &a_z;&a_d;&a_n;</trd>
    <trd lng="fr">y avoir des <ind>&eacute;clairs</ind></trd>
    <trd lng="id"><baz>petir</baz>berpetir</trd>
  </snc>
  <snc mrk="fulm.0i.brili">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Brili kiel <tld/>o:
      <ekz>
        brilas glavoj, fulmas lancoj
        <fnt>
          <bib>MT</bib>
          <lok>Na&hcirc;um 3:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a <klr>(bolanta)</klr> knabego
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        rigardoj brilegas <tld/>ante
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la pupiloj <tld/>etadis akraj briloj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        sur &scirc;iaj lipoj sen&ccirc;ese tremadis <tld/>etanta rideto
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fa">&a_d;&a_r;&a_kh;&a_sh;&f_y;&a_d;&a_n;</trd>
    <trdgrp lng="fr">
      <trd>jeter des <ind>&eacute;clairs</ind></trd>,
      <trd>fulgurer</trd>
    </trdgrp>
  <trdgrp lng="id">
    <trd><baz>denyar</baz>berdenyar</trd>,
    <trd><baz>kilat</baz>berkilat-kilat</trd>,
    <trd><baz>kilat</baz>mengilat</trd>
  </trdgrp>
  </snc>
  <trd lng="be">&c_b;&c_l;&c_ib;&c_s;&c_n;&c_u;&c_c;&c_mol;</trd>
  <trd lng="cs">bl&yacute;skat se</trd>
  <trd lng="de">blitzen</trd>
  <trd lng="es">relampaguear</trd>
  <trd lng="hu">vill&aacute;mlik</trd>
  <trd lng="id"><baz>kilat</baz>berkilat</trd>
  <trdgrp lng="ja">
    <trd>&#x7A32;&#x59BB;&#x304C;&#x8D70;&#x308B;
    <pr>&#x3044;&#x306A;&#x305A;&#x307E;&#x304C;&#x306F;&#x3057;&#x308B;</pr></trd>,
    <trd>&#x3072;&#x3089;&#x3081;&#x304F;</trd>
  </trdgrp>
  <trd lng="nl">bliksemen</trd>
  <trd lng="no">lyne</trd>
  <trd lng="pl">b&lstroke;yska&cacute;</trd>
  <trd lng="ru">&c_s;&c_v;&c_je;&c_r;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>
  <trd lng="sk">bl&yacute;ska&tcaron; sa</trd>
  <trd lng="sv">blixtra</trd>
  <trd lng="uk">&c_b;&c_l;&c_i;&c_s;&c_k;&c_a;&c_t;&c_i;</trd>
  <trd lng="zh">&#x5612; <pr>hu&igrave;</pr></trd>
</drv>

<drv mrk="fulm.0obati0ofrapi">
  <kap><tld/>obati, <var><kap><tld/>ofrapi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      Trafi (kvaza&ubreve;) per <tld/>o:
      <ekz>
        cin&hcirc;onamino, la plej agema veneno, kiun &gcirc;is nun malkovris &hcirc;emio, pli
        <tld/>ofrapanta e&ccirc; ol cianacido
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekkvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        la duko de Blasano mase falegis sur la tapi&scirc;on de sia &ccirc;ambro, kvaza&ubreve;
        <tld/>ofrapite
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekdua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bela <nom>Kunegond</nom> estas la preferata amatino de la sinjorego, tio estis kvaza&ubreve;
        <tld/>obato por <nom>Kandid</nom>
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>bate &ccirc;iu laco estas for
        <fnt>
          <bib>Monato</bib>
          <aut>Bernhard Pabst</aut>
          <vrk><url ref="&Monato;/2008/009376.php">Inter libroj posttagmeze</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>

  <trd lng="be">&c_b;&c_ib;&c_c;&c_mol;
  <ind>&c_m;&c_a;&c_l;&c_a;&c_n;&c_k;&c_a;&c_j;</ind></trd>
  <trd lng="cs">uhodit blesk</trd>
  <trdgrp lng="fa">
    <trd>&a_S;&a_A;&a_ayn;&a_q;&a_h; &a_z;&a_d;&a_n;</trd>,
    <trd>&a_b;&a_r;&a_q; &a_z;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">foudroyer</trd>
  <trd lng="hu">vill&aacute;m s&uacute;jtja</trd>
  <trdgrp lng="id">
    <trd>sambar</trd>,
    <trd><baz>sambar</baz>menyambar</trd>
  </trdgrp>
  <trd lng="ja">&#x96F7;&#x3067;&#x6253;&#x3064;  <pr>&#x304B;&#x307F;&#x306A;&#x308A;&#x3067;&#x3046;&#x3064;</pr></trd>
  <trdgrp lng="nl">
    <trd>neerbliksemen</trd>,
    <trd>doodbliksemen</trd>
  </trdgrp>
  <trd lng="no">lyne ned</trd>
  <trdgrp lng="pl">
    <trd>trafia&cacute;<klr> (o piorunie,
    b&lstroke;yskawicy)</klr></trd>,
    <trd>uderza&cacute;<klr> (o piorunie,
    b&lstroke;yskawicy)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_r;&c_a;&c_z;&c_i;&c_t;&c_mol;
  &c_m;&c_o;&c_l;&c_n;&c_i;&c_je;&c_j;</trd>
  <trd lng="sk">udrie&tcaron; blesk</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&c_d;&c_a;&c_r;&c_i;&c_t;&c_i; &c_b;&c_l;&c_i;&c_s;&c_k;&c_a;&c_v;&c_k;&c_o;&c_ju;</trd>,
    <trd>&c_u;&c_d;&c_a;&c_r;&c_i;&c_t;&c_i;/&c_p;&c_o;&c_b;&c_i;&c_t;&c_i; &c_g;&c_r;&c_o;&c_m;&c_o;&c_m;</trd>
  </trdgrp>
</drv>

<drv mrk="fulm.0ilo">
  <kap><tld/>ilo, <var><kap><tld/>olumilo</kap></var></kap>
  <snc>
    <uzo tip="fak">FOT</uzo>
    <dif>
      Ilo produktanta mallongan kaj intensan <tld/>on dum fotado:
      <ekz>
        tiu unua fotokompostilo estis maleleganta skatolo preska&ubreve; unu kuban metron
        granda, en kiu <tld/>ilo projekciis lumon sur sentiva fotopapero
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Peeraerts</aut>
          <vrk><url ref="&Monato;/2004/008280.html">Komposta revolucio kaj presada
          stabileco</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; duonjarcento fotistoj komencis uzi <tld/>olumilojn
        <fnt>
          <bib>Monato</bib>
          <aut>J&aacute;nos S&aacute;rk&ouml;zi</aut>
          <vrk><url ref="&Monato;/2005/008401.php">La fenomeno de ru&gcirc;aj okuloj</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_s;&c_p;&c_y;&c_sh;&c_k;&c_a; <klr tip="amb">(&c_p;&c_r;&c_y;&c_l;&c_a;&c_d;&c_a;)</klr></trd>
  <trdgrp lng="de">
    <trd>Fotoblitz</trd>,
    <trd>Blitzlicht</trd>
  </trdgrp>
  <trd lng="en">flash</trd>
  <trd lng="fr">flash</trd>
  <trdgrp lng="id">
    <trd>blitz</trd>,
    <trd>flash</trd>,
    <trd>lampu denyar</trd>,
    <trd>lampu kilat</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30D5;&#x30E9;&#x30C3;&#x30B7;&#x30E5;</trd>,
    <trd>&#x30B9;&#x30C8;&#x30ED;&#x30DC;</trd>
  </trdgrp>
  <trd lng="nl">flits</trd>
  <trdgrp lng="pl">
    <trd>lampa b&lstroke;yskowa</trd>,
    <trd>flesz</trd>
  </trdgrp>
  <trd lng="sk">blesk <klr>(pr&iacute;stroj)</klr></trd>
  <trdgrp lng="zh">
    <trd>&#x9583;&#x5149; <pr>sh&#x1CE;ngu&amacron;ng</pr></trd>,
    <trd>&#x9583;&#x5149;&#x71C8; <pr>sh&#x1CE;ngu&amacron;ngd&emacron;ng</pr></trd>
  </trdgrp>
</drv>
</art>
