<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Hieronim</rad>/o
    </kap>
    <drv mrk="hieronim.H0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">NOM</uzo>
      <snc>
	<dif>
	  Grekdevena vira anta&ubreve;nomo:
	  <ekz>
	    <uzo tip="fak">PERS</uzo>
	    <ref cel="vulgat.0o">Vulgaton</ref>
	    plejparte tradukis s-ta
	    <ind><tld/>o el Stridono</ind> (m.420).
            <trdgrp lng="de">
               <trd>Hl. Hieronymus zu Stridon</trd>,
               <trd>Hieronymus Sophronius Eusebius</trd>
            </trdgrp>
	    <trd lng="ru">&c_b;&c_l;. <ind>&c_I;&c_je;&c_r;&c_o;&c_n;&c_i;&c_m; &c_S;&c_t;&c_r;&c_i;&c_d;&c_o;&c_n;&c_s;&c_k;&c_i;&c_j;</ind></trd>
	  </ekz>
	</dif>
      </snc>
      <trdgrp lng="de">
         <trd>Hieronymus</trd>
      </trdgrp>
      <trd lng="en">Jerome</trd>
      <trd lng="fr">J&eacute;r&ocirc;me</trd>
      <trd lng="la">Hieronymus</trd>
      <trd lng="nl">Jeroen</trd>
      <trd lng="ru">&c_I;&c_je;&c_r;&c_o;&c_n;&c_i;&c_m;</trd>
    </drv>

  </art>

