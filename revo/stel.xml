<drv mrk="stel.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="stel.0eto.KOMUNE">
    <dif>
      Malgranda <ref tip="dif" cel="stel.0o.figuro">stelo<sncref/></ref>:
      <ekz>
        &ccirc;apo kun <tld/>eto
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> simboloj necesaj por &ccirc;iuj e&ubreve;ro-moneroj &ndash; 12 <tld/>etoj kaj la dato de
        stampado
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2006/008847.php">Kiam e&ubreve;ro venos al Litovio?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        okuloj &mdash; brilantaj <tld/>etoj
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="id"><ind>bintang</ind> kecil</trd>
  </snc>
  <snc mrk="stel.0eto.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref tip="dif" cel="asteri1.0o">Asterisko</ref>:
      <ekz>
        en la suba tabelo <tld/>eto montras la ar&hcirc;aikajn literojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1430">Greka alfabeto</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>eto (*) signifas, ke la lingvo estis unika kaj ne havas rektajn pralingvojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=30923">Programlingvo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">asterisc</trd>
