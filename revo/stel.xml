    <trd>asterism</trd>,
  </trdgrp>
  <trdgrp lng="es">
    <trd>asterismo</trd>,
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ast&eacute;risme</trd>,
  </trdgrp>
