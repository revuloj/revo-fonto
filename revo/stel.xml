<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>stel</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="stel.0o">
  <kap><tld/>o</kap>
  <snc mrk="stel.0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Memlumanta <ref tip="super" cel="astr.0o.AST">astro</ref>,
      kiu, pro malproksimegeco, aperas kiel brila sur&ccirc;iela punkto:
      <ekz>
        en la nokto ni vidas la palan lunon kaj la belajn <tld/>ojn
        <fnt><bib>F</bib>, <lok>&FE; 10</lok></fnt>;
      </ekz>
      <ekz>
        Li kreis la Grandan Ursinon, Orionon kaj Plejadojn
        kaj la <tld/>ojn de la sudo
        <fnt><bib>MT</bib>, <lok>&Ijo; 9:9</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>oj estas korpoj de universo
        <fnt><bib>Fab2</bib>, <lok>Dio de dormo</lok></fnt>;
      </ekz>
      <ekz>
        la polusa <tld/>o;
      </ekz>
      <ekz>
        <tld/>luma, <tld/>klara, <tld/>plena nokto.
      </ekz>
    </dif>
    <trd lng="bg">&c_z;&c_v;&c_je;&c_z;&c_d;&c_a;</trd>
    <trd lng="de">Stern</trd>
    <trd lng="es">estrella</trd>
    <trd lng="fr">&eacute;toile</trd>
    <trd lng="vo">stel</trd>
    <trd lng="pt">estrela</trd>
  </snc>
  <snc mrk="stel.0o.figuro">
    <dif>
      <ref tip="super" cel="figur.0o">Figuro</ref>
      kun kvin a&ubreve; pli da
      pintoj, ofte uzata kiel simbolo kaj
      <ref tip="super" cel="emblem.0o">emblemo</ref>:
      <ekz>
        <tld/>o el orsimila papero
        <fnt><bib>Fab2</bib>, <lok>Abio</lok></fnt>;
      </ekz>
      <ekz>
        milionoj da <tld/>formaj flokoj <klr>(ne&gcirc;o)</klr>
        <fnt><bib>Fab2</bib>, <lok>Ne&gcirc;a re&gcirc;ino</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Stern</trd>
    <trd lng="es">estrella</trd>
    <trd lng="fr">&eacute;toile</trd>
    <trd lng="pt">estrela</trd>
  </snc>
  <snc mrk="stel.0o.stelulo">
    <uzo tip="stl">FIG</uzo>
    <ref tip="dif" cel="stel.0ulo"><tld/>ulo</ref>
    <trdgrp lng="it">
      <trd>star</trd>
      <trd>stella<klr> (del cinema)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_z;&c_o;&c_r;&c_k;&c_a;</trd>
  <trd lng="bg">&c_z;&c_v;&c_je;&c_z;&c_d;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>hv&ecaron;zda</trd>,
    <trd>hv&ecaron;zdice</trd>
  </trdgrp>
  <trd lng="de">Stern</trd>
  <trd lng="en">star</trd>
  <trd lng="he">&kaf;&vav;&kaf;&bet;</trd>
  <trd lng="hu">csillag</trd>
  <trd lng="id">bintang</trd>
  <trd lng="it">stella</trd>
  <trd lng="ja">&#26143; [<ind>&#12411;&#12375;</ind>]</trd>
  <trd lng="nl">ster</trd>
  <trd lng="pl">gwiazda</trd>
  <trd lng="ru">&c_z;&c_v;&c_je;&c_z;&c_d;&c_a;</trd>
  <trd lng="sk">hviezda</trd>
  <trd lng="sv">stj&auml;rna</trd>
</drv>

<drv mrk="stel.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="stel.0aro.stelfiguro">
    <tezrad fak="AST"/>
    <uzo tip="fak">AST</uzo>
    <dif>
      &Scirc;ajna grupo da <tld/>oj apartenantaj al unu a&ubreve; pluraj
      <ref cel="konste.0o.stelaro">konstelacioj</ref>,
      kiu aparti&gcirc;as sur la &ccirc;ielsfero por desegni arbitran
      figuron (<ref
      cel="stel.0figuro">stelfiguron</ref>).
      &Gcirc;enerale, tiuj <tld/>oj ne estas gravite kunligitaj, nek
      havas
      komunan originon, diference de <ref
      cel="stel.0amaso">stelamasoj</ref>:
      <ekz>
        la &Ccirc;aro estas fama <tld/>aro, kiu apartenas al la
        konstelacio
        Granda Ursino;
      </ekz>
      <ekz>
        oni vidis la <tld/>aron de Leono en kies mezo brilis la
        <tld/>o de Regulus
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 24a</lok></fnt>;
      </ekz>
      <ekz>
        al la suno, al la luno, al la <tld/>aroj kaj al
        la tuta armeo de la &ccirc;ielo
        <fnt><bib>MT</bib>, <lok>&Re2; 23:5</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="stel.0figuro">stelfiguro</ref>,
      <ref cel="konste.0o">konstelacio</ref>
    </refgrp>
    <rim>
      Vidu la rimarkon &ccirc;e <ref
      cel="konste.0o.stelarorim">konstelacio</ref>.
    </rim>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_z;&c_o;&c_r;'&c_je;</trd>
  <trd
  lng="bg">&c_s;&c_malmol;&c_z;&c_v;&c_je;&c_z;&c_d;&c_i;&c_je;</trd>
  <trd lng="cs">souhv&ecaron;zd&iacute;</trd>
  <trd lng="de">Sternbild</trd>
  <trdgrp lng="en">
    <trd>asterism</trd>,
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ast&eacute;risme</trd>,
  </trdgrp>
  <trd lng="he">&qof;&bet;&vav;&tsadi;&tav;
  &kaf;&vav;&kaf;&bet;&yod;&fmem;</trd>
  <trd lng="hu">csillagzat</trd>
  <trd lng="nl">sterrenbeeld</trd>
  <trd lng="pl">gwiazdozbi&oacute;r</trd>
  <trd lng="ru">&c_s;&c_o;&c_z;&c_v;&c_je;&c_z;&c_d;&c_i;&c_je;</trd>
  <trd lng="sk">s&uacute;hvezdie</trd>
</drv>

<drv mrk="stel.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="stel.0eto.TIP">
    <uzo tip="fak">TIP</uzo>
    <ref tip="vid" cel="asteri1.0o">asterisko</ref>
    <dif>
    </dif>
    </snc>
  <trd lng="cs">hv&ecaron;zdi&ccaron;ka <klr>(symbol)</klr></trd>
  <trd lng="fr">ast&eacute;risque</trd>
  <trd lng="id">asterisk</trd>
  <trd lng="sk">hviezdi&ccaron;ka <klr>(symbol)</klr></trd>
</drv>

<drv mrk="stel.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="stel.0ulo.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Fama, elstara artisto:
      <ekz>
        tiuj aktoroj mem, nomataj &leftquot;steluloj&rightquot;, estis
        same naive
        diigataj, kiel sportistoj
        <fnt>
        <aut>I. Efremov, trad. J. Finkel</aut>,
        <vrk><url ref="http://www.esperanto.mv.ru/Efremov">Horo de
        Bovo</url></vrk>,
        <lok>&ccirc;apitro 3a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_o;&c_r;&c_k;&c_a;
  <klr>(&c_v;&c_ja;&c_d;&c_o;&c_m;&c_y;
  &c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;)</klr></trd>
  <trd lng="bg">&c_z;&c_v;&c_je;&c_z;&c_d;&c_a;</trd>
  <trd lng="cs">filmov&aacute; hv&ecaron;zda</trd>
  <trd lng="de">Star</trd>
  <trd lng="en">star</trd>
  <trdgrp lng="es">
    <trd>estrella</trd>,
    <trd>artista famoso</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>star</trd>,
    <trd>vedette<klr> (star)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csillag</trd>,
    <trd>szt&aacute;r</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>artis</trd>,
    <trd><ind>bintang</ind> film</trd>,
    <trd>selebritas</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>star</trd>,
    <trd>stella<klr> (del cinema)</klr></trd>
  </trdgrp>
  <trd lng="nl">ster</trd>
  <trdgrp lng="pl">
    <trd>gwiazda</trd>,
    <trd>gwiazdor</trd>,
    <trd>celebryta</trd>
  </trdgrp>
  <trd lng="pt">estrela</trd>
  <trd lng="ru">&c_z;&c_v;&c_je;&c_z;&c_d;&c_a; <klr>(&c_o;
  &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;&c_je;)</klr></trd>
  <trd lng="sk">filmov&aacute; hviezda</trd>
</drv>

<drv mrk="stel.fal0o">
  <kap>fal<tld/>o</kap>
  <snc mrk="stel.fal0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Brila meteoro, kiu &scirc;ajnas kvaza&ubreve; defalanta <tld/>o:
      <ekz>
        falis fal<tld/>o kaj la pensoj de la knabo
        levi&gcirc;is supren al la brilanta meteoro
        <fnt><bib>Fab4</bib>, <lok>Du fratoj</lok></fnt>;
      </ekz>
      <ekz>
        fal<tld/>o traflugis la &ccirc;ielon
        <fnt><bib>Fab1</bib>, <lok>Galo&scirc;oj de
        feli&ccirc;o</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="meteor.0o">meteoro</ref>
    <ref tip="vid" cel="aeroli.0o">aerolito</ref>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_t;&c_e;&c_a;&c_r;&c_y;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>meteorit</trd>,
    <trd>padaj&iacute;c&iacute; hv&ecaron;zda</trd>
  </trdgrp>
  <trd lng="de">Sternschnuppe</trd>
  <trd lng="es">estrella fugaz</trd>
  <trd lng="fr">&eacute;toile filante</trd>
  <trd lng="he">&kaf;&vav;&kaf;&bet; &nun;&vav;&pe;&lamed;</trd>
  <trd lng="hu">hull&oacute;csilllag</trd>
  <trd lng="id"><ind>bintang</ind> jatuh</trd>
  <trdgrp lng="pl">
    <trd>spadaj&aogonek;ca <ind>gwiazda</ind></trd>,
    <trd>meteoryt</trd>
  </trdgrp>
  <trd lng="pt">estrela cadente</trd>
  <trd lng="ru">&c_m;&c_je;&c_t;&c_je;&c_o;&c_r;&c_i;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>lietavica</trd>,
    <trd>meteor</trd>
  </trdgrp>
</drv>

<drv mrk="stel.mar0o">
  <kap>mar<tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="stel.mar0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Stelforma besto vivanta en maro,
      genro el <ref tip="super"
      cel="ehxinoderm.0oj.ZOO">e&hcirc;inodermoj</ref>
      (<trd lng="la">Asterias</trd>).
    </dif>
    <ref tip="sin" cel="asteri.0o.ZOO">asterio</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_s;&c_t;&c_e;&c_r;&c_y;&c_ja;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_s;&c_k;&c_a;&c_ja;
    &c_z;&c_o;&c_r;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mo&rcaron;sk&aacute; hv&ecaron;zda</trd>,
    <trd>mo&rcaron;sk&aacute; hv&ecaron;zdice</trd>
  </trdgrp>
  <trd lng="de">Seestern</trd>
  <trd lng="en">starfish</trd>
  <trd lng="es">estrella de mar</trd>
  <trdgrp lng="fr">
    <trd>ast&eacute;rie</trd>,
    <trd>&eacute;toile de mer</trd>
  </trdgrp>
  <trd lng="he">&kaf;&vav;&kaf;&bet;-&yod;&fmem;</trd>
  <trdgrp lng="hu">
    <trd>tengeri csillag</trd>,
    <trd>csillag&aacute;llatka</trd>
  </trdgrp>
  <trd lng="id"><ind>bintang</ind> laut</trd>
  <trd lng="it">stella marina</trd>
  <trdgrp lng="pl">
    <trd>gwiazda morska <klr>(rzadkie)</klr></trd>,
    <trd>rozgwiazda</trd>
  </trdgrp>
  <trd lng="pt">estrela do mar</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_s;&c_t;&c_je;&c_r;&c_i;&c_ja;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_s;&c_k;&c_a;&c_ja;
    &c_z;&c_v;&c_je;&c_z;&c_d;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">morsk&aacute; hviezdica</trd>
</drv>

<drv mrk="stel.maten0o">
  <kap>maten<tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="stel.maten0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      La planedo <ref tip="sin" cel="venus.0o.AST">Venuso</ref> vidata
      je
      frua mateno:
      <ekz>
        ektagi&gcirc;os kaj la maten<tld/>o eklevi&gcirc;os
        <fnt><bib>NT</bib>, <lok>&Pe2; 1:19</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">Jit&rcaron;enka</trd>
  <trdgrp lng="es">
    <trd>lucero del alba</trd>,
    <trd>estrella del alba</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;toile du berger</trd>,
    <trd>&eacute;toile du matin</trd>
  </trdgrp>
  <trd lng="id"><ind>bintang</ind> fajar</trd>
  <trd lng="it"><ind>stella</ind> del mattino</trd>
  <trdgrp lng="pl">
    <trd>gwiazda poranna</trd>,
    <trd>gwiazda zaranna</trd>
  </trdgrp>
  <trd lng="sk">Zorni&ccaron;ka</trd>
</drv>

<drv mrk="stel.polusa0o">
  <kap>Polusa <tld lit="S"/>o</kap>
  <snc mrk="stel.polusa0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      <tld lit="S"/>o en la stelaro
      <ref tip="malprt" cel="urs.MalgrandaU0ino">Malgranda Ursino</ref>,
      el la helaj <tld/>oj la plej proksima al la &ccirc;iela nordpoluso
      (je proksimume 1&#730;)
      (<trd lng="la">&alfa; Ursae Minoris</trd>):
      <ekz>
        la Polusa <tld lit="S"/>o, gvidanto de navigantoj,
        faras angulon je 25 gradoj al la horizonto
        <fnt>
          <aut>trad. J. R. Piton, E. Cid k. a.</aut>,
          <vrk><url ref="http://filmoj.net/karamuru">
          Karamuru</url></vrk>,
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_P;&c_a;&c_l;&c_ja;&c_r;&c_n;&c_a;&c_ja;
  <ind>&c_z;&c_o;&c_r;&c_k;&c_a;</ind></trd>
  <trd lng="bg">&c_P;&c_o;&c_l;&c_ja;&c_r;&c_n;&c_a;
  &c_z;&c_v;&c_je;&c_z;&c_d;&c_a;</trd>
  <trd lng="ca">Estel Polar</trd>
  <trdgrp lng="de">
    <trd>Nordstern</trd>,
    <trd>Polarstern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>North star</trd>,
    <trd>polestar</trd>,
    <trd>Cynosura</trd>,
    <trd>Polaris</trd>
  </trdgrp>
  <trd lng="es">estrella polar</trd>
  <trd lng="fr">&eacute;toile polaire</trd>
  <trdgrp lng="hu">
    <trd>Sarkcsillag</trd>,
    <trd>Polaris</trd>
  </trdgrp>
  <trd lng="pl">Gwiazda Polarna</trd>
  <trd lng="nl">Poolster</trd>
  <trd lng="ru">&c_P;&c_o;&c_l;&c_ja;&c_r;&c_n;&c_a;&c_ja;
  &c_z;&c_v;&c_je;&c_z;&c_d;&c_a;</trd>
</drv>

<drv mrk="stel.pulso0o">
  <kap>pulso<tld/>o</kap> <fnt><bib>PIV2</bib></fnt>
  <snc mrk="stel.pulso0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Kolapsinta <ref tip="super" cel="stel.0o.AST">stelo</ref> tre
      rapide
      turni&gcirc;anta, elsendante radiondojn je &ccirc;iu
      turni&gcirc;o.
    </dif>
  </snc>
  <trd lng="en">pulsar</trd>
  <trd lng="es">p&uacute;lsar</trd>
  <trd lng="fr">pulsar</trd>
  <trd lng="id">pulsar</trd>
  <trd lng="it">pulsar</trd>
  <trd lng="pl">pulsar</trd>
  <trd lng="pt">pulsar</trd>
</drv>

<drv mrk="stel.verda0o">
  <kap>verda <tld/>o</kap>
  <snc mrk="stel.verda0o.ESP">
    <uzo tip="fak">ESP</uzo>
    <dif>
      Figuro de verda, kvinpinta <tld/>o, tradicia
      emblemo de Esperantismo:
      <ekz>
        la flago de la Civito estas verda, kun blanka kvadrato en la 
        &ccirc;estanga supra angulo, kie situas simetria kvinpinta verda
        
        <tld/>o, konforme al la modelo adoptita de la unua
        
        Universala 
        Kongreso en Bulonjo-&ccirc;e-Maro, 1905
        <fnt>
          <aut>Esperanta Civito</aut>,
          <vrk><url ref="http://www.esperantio.net/index.php?id=15">
          Konstitucio</url></vrk>,
          <lok>2001-06-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd>estrella verde</trd>,
    <trd>estrella de esperanto</trd>
  </trdgrp>
  <trd lng="fr">&eacute;toile verte</trd>
  <trd lng="pl">zielona gwiazda</trd>
</drv>

<drv mrk="stel.vesper0o">
  <kap>vesper<tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="stel.vesper0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      La planedo <ref tip="sin" cel="venus.0o.AST">Venuso</ref> vidata
      je sunsubiro.
    </dif>
  </snc>
  <trd lng="cs">ve&ccaron;ernice</trd>
  <trdgrp lng="es">
    <trd>lucero vespertino</trd>,
    <trd>estrella de la tarde</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;toile du berger</trd>,
    <trd>&eacute;toile du soir</trd>
  </trdgrp>
  <trd lng="id"><ind>bintang</ind> senja</trd>
  <trd lng="it"><ind>stella</ind> della sera</trd>
  <trd lng="pl">gwiazda wieczorna</trd>
  <trd lng="sk">ve&ccaron;ernica</trd>
</drv>

<drv mrk="stel.0amaso">
  <kap><tld/>amaso</kap>
  <snc mrk="stel.0amaso.AST">
  <uzo tip="fak">AST</uzo>
    <dif>
      Grupo de multaj <tld/>oj, gravite kunligitaj:
      <ekz>
        globa <tld/>amaso a&ubreve; malferma <tld/>amaso<fnt>PIV</fnt>.
      </ekz>
    </dif>
    <trd lng="en">star cluster</trd>
    <trd lng="fr">amas d'&eacute;toiles</trd>
  </snc>
  <trd lng="cs">hv&ecaron;zdokupa</trd>
  <trd lng="es">c&uacute;mulo estelar</trd>
  <trd lng="sk">hviezdokopa</trd>
</drv>

<drv mrk="stel.0amasoglob">
  <kap>globa <tld/>amaso</kap>
  <snc mrk="stel.0amasoglob.AST">
  <uzo tip="fak">AST</uzo>
    <dif>
      Densa grupo de multaj tre maljunaj <tld/>oj, generale
      trovi&gcirc;anta
      en
      la haloo de galaksio.
    </dif>
    <trd lng="en">globular cluster</trd>
