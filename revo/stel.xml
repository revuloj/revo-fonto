    <trd>estrela verda</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>estrella verde</trd>,
    <trd>estrella de esperanto</trd>
  </trdgrp>
  <trd lng="fr">&eacute;toile verte</trd>
  <trd lng="id"><ind>bintang</ind> hijau<klr> (Esperanto)</klr></trd>
  <trd lng="ja">&#32209;&#12398;&#26143;  <pr>&#12415;&#12393;&#12426;&#12398;&#12411;&#12375;</pr></trd>
  <trd lng="pl">zielona gwiazda</trd>
</drv>

