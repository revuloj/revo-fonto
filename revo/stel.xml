    <trd>constellation figure</trd>,
    <trd>asterism figure</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>figura de una constelaci&oacute;n</trd>,
    <trd>figura de un asterismo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>figure de constellation</trd>,
    <trd>figure d'ast&eacute;risme</trd>
  </trdgrp>
  <trd lng="hu">csillagk&eacute;p</trd>
  <trdgrp lng="id">
    <trd><ind>figur</ind> konstelasi</trd>,
    <trd><ind>figur</ind> rasi bintang</trd>,
    <trd><ind>figur</ind> asterisme</trd>
  </trdgrp>
  <trd lng="ja">&#26143;&#24231;  <pr>&#12379;&#12356;&#12374;</pr></trd>
  <trd lng="nl">sterrenbeeld</trd>
  <trd lng="pl">gwiazdozbi&oacute;r</trd>
