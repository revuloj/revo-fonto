<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>stel</rad>/o
</kap>
<drv mrk="stel.0o">
  <kap><tld/>o</kap>
  <snc mrk="stel.0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Memlumanta &ccirc;iela <ref tip="super" cel="astr.0o.1">astro</ref>,
      kiu, pro malproksimegeco, aperas al nia okulo kiel brila
      nemezurebla punkto:
      <ekz>
        en la nokto ni vidas la palan lunon kaj la belajn <tld/>ojn
        <fnt><bib>F</bib>, <lok>Ekzercaro, &para; 10</lok></fnt>;
      </ekz>
      <ekz>
        Li kreis la Grandan Ursinon, Orionon kaj Plejadojn
        kaj la <tld/>ojn de la sudo  
        <fnt><bib>MT</bib>, <lok>&Ijo; 9:9</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>oj estas korpoj de universo
        <fnt><bib>Fab2</bib>, <lok>Dio de dormo</lok></fnt>;
      </ekz>
      <ekz>
        la polusa <tld/>o;
      </ekz>
      <ekz>
        <klr>(ev.)</klr>
        maten<tld/>o, vesper<tld/>o <klr>(Venuso)</klr>;
      </ekz>
      <ekz>
        <tld/>luma, <tld/>klara, <tld/>plena nokto.
      </ekz>
    </dif>
    <trd lng="de">Stern</trd>
    <trd lng="es">estrella</trd>
    <trd lng="fr">&eacute;toile</trd>
    <trd lng="vo">stel</trd>
  </snc>
  <snc mrk="stel.0o.figuro">
    <dif>
      <ref tip="super" cel="figur.0o">Figuro</ref>
      kun kvin a&ubreve; pli da
      pintoj, oft uzata kiel simbolo kaj 
      <ref tip="super" cel="emblem.0o">emblemo</ref>:
      <ekz>
        verda <tld/>o estas emblemo de Esperanto;
      </ekz>
      <ekz>
        <tld/>o el orsimila papero
        <fnt><bib>Fab2</bib>, <lok>Abio</lok></fnt>;
      </ekz>
      <ekz>
        milionoj da <tld/>formaj flokoj <klr>(ne&gcirc;o)</klr>
        <fnt><bib>Fab2</bib>, <lok>Ne&gcirc;a re&gcirc;ino</lok></fnt>.
      </ekz> 
    </dif>
    <trd lng="de">Stern</trd>
    <trd lng="es">estrella</trd>
    <trd lng="fr">&eacute;toile</trd>
  </snc>
  <snc mrk="stel.0o.stelulo">
    <uzo tip="stl">FIG</uzo>
    <ref tip="dif" cel="stel.0ulo"><tld/>ulo</ref>
  </snc>
  <trd lng="en">star</trd>
  <trd lng="he">&kaf;&vav;&kaf;&bet;</trd>
  <trd lng="hu">csillag</trd>
  <trd lng="nl">ster</trd>
  <trd lng="ru">&c_z;&c_v;&c_je;&c_z;&c_d;&c_a;</trd>
  <trd lng="sv">stj&auml;rna</trd>
</drv>
<drv mrk="stel.0aro">
  <kap><tld/>aro</kap>
  <snc>
    <dif>
      &Scirc;ajna grupo da <tld/>oj, prezentanta iun figuron kaj ricevinta
      apartan nomon:
      <ekz>
        oni vidis la <tld/>aron de Leono en kies mezo brilis la
        stelo de Regulus 
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 24</lok></fnt>;
      </ekz>
      <ekz>
        al la suno, al la luno, al la <tld/>aroj kaj al
        la tuta armeo de la &ccirc;ielo
        <fnt><bib>MT</bib>, <lok>II. re&gcirc;oj 23:5</lok></fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="stel.0figuro"><tld/>figuro</ref>,
      <ref cel="konste.0o">konstelacio</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Sternbild</trd>
  <trd lng="es">constelaci&oacute;n</trd>
  <trd lng="fr">constellation</trd>
  <trd lng="he">&qof;&bet;&vav;&tsadi;&tav; &kaf;&vav;&kaf;&bet;&yod;&fmem;</trd>
  <trd lng="hu">csillagzat</trd>
  <trd lng="nl">sterrenbeeld</trd>
  <trd lng="ru">&c_s;&c_o;&c_z;&c_v;&c_je;&c_z;&c_d;&c_i;&c_je;</trd>
</drv>
<drv mrk="stel.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Fama, elstara artisto.
    </dif>
  </snc>
  <trd lng="de">Star</trd>
  <trd lng="es">estrella</trd>
  <trdgrp lng="hu">
    <trd>csillag</trd>,
    <trd>szt&aacute;r</trd></trdgrp>
  <trd lng="nl">ster</trd>
  <trd lng="ru">&c_z;&c_v;&c_je;&c_z;&c_d;&c_a; <klr>(&c_o; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;&c_je;)</klr></trd>
</drv>
<drv mrk="stel.0figuro">
  <kap><tld/>figuro</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="stel.0aro">Stelaro</ref>:
      <ekz>
        la <tld/>oj de la &ccirc;ielo kaj &gcirc;iaj
        <tld/>figuroj ne donas sian lumon
         <fnt><bib>MT</bib>, <lok>&Jes; 13:10</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Sternbild</trd>
  <trd lng="hu">csillagk&eacute;p</trd>
  <trd lng="nl">sterrenbeeld</trd>
  <trd lng="ru">&c_s;&c_o;&c_z;&c_v;&c_je;&c_z;&c_d;&c_i;&c_je;</trd>
</drv>
<drv mrk="stel.fal0o">
  <kap>fal<tld/>o</kap>
  <snc>
    <dif>
      Brila meteoro, kiu &scirc;ajnas kvaza&ubreve; defalanta <tld/>o: 
      <ekz>
        falis fal<tld/>o kaj la pensoj de la knabo
        levi&gcirc;is supren al la brilanta meteoro  
        <fnt><bib>Fab4</bib>, <lok>Du fratoj</lok></fnt>;
      </ekz>
      <ekz>
        fal<tld/>o traflugis la &ccirc;ielon
        <fnt><bib>Fab1</bib>, <lok>Galo&scirc;oj de feli&ccirc;o</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="meteor.0o">meteoro</ref>
    <ref tip="vid" cel="aeroli.0o">aerolito</ref>
  </snc>
  <trd lng="de">Sternschnuppe</trd>
  <trd lng="es">estrella fugaz</trd>
  <trd lng="fr">m&eacute;t&eacute;orite</trd>
  <trd lng="he">&kaf;&vav;&kaf;&bet; &nun;&vav;&pe;&lamed;</trd>
  <trd lng="hu">hull&oacute;csilllag</trd>
  <trd lng="ru">&c_m;&c_je;&c_t;&c_je;&c_o;&c_r;&c_i;&c_t;</trd>
</drv>
<drv mrk="stel.mar0o">
  <kap>mar<tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="asteri.0o">Asterio</ref>.
    </dif>
  </snc>
  <trd lng="es">estrella de mar</trd>
  <trd lng="he">&kaf;&vav;&kaf;&bet;-&yod;&fmem;</trd>
  <trdgrp lng="hu">
    <trd>tengeri csillag</trd>,
    <trd>csillag&aacute;llatka</trd></trdgrp>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_s;&c_k;&c_a;&c_ja; &c_z;&c_v;&c_je;&c_z;&c_d;&c_a;</trd>
</drv>
</art>
