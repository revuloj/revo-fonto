<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: ken.xml,v 1.28 2023/10/18 14:48:10 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>ken</rad>/o
    </kap>

    <drv mrk="ken.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="ken.0o.lumigilo">
        <dif>
          Longa, seka, maldika lignosplito, uzata kiel lumigilo
          anstata&ubreve; <ref cel="kandel.0o">kandelo</ref>, a&ubreve; por
          disbruligi pli grandan lignofajron (kp
          <ref cel="alumet.0o">alumeto</ref>) ktp:
          <ekz>
            tiam ni <klr>(temas pri gnomoj)</klr> sidos en la &scirc;tona
            domo, kie brulas la <tld/>o, kaj ni trinkos mielvinon el
            la oraj kornoj de la antikvaj nordlandaj re&gcirc;oj
            <fnt><bib>Fab2</bib>, <lok>Monteto de elfoj</lok></fnt>;
          </ekz><ekz>
            tra la dormo li a&ubreve;dis, kiel la maljuna mastrino forigis
            de la fenestroj lignajn &jcirc;aluziojn, kiel &scirc;i per <tld/>oj
            bruligis la samovaron
            <fnt><bib>Metrop</bib></fnt>.
          </ekz>
        </dif>
        <trd lng="be">&c_l;&c_u;&c_ch;&c_y;&c_n;&c_a;</trd>
        <trdgrp lng="de">
              <trd>Kien</trd>,
              <trd>Kienspan</trd>
        </trdgrp>
        <trd lng="en">fatwood</trd>
        <trd lng="es">antorcha</trd>
        <trd lng="fr">bois gras</trd>
        <trdgrp lng="hu">
          <trd>fokla</trd>,
          <trd>vil&aacute;g&iacute;t&oacute;szil&aacute;nk</trd>
        </trdgrp>
        <trd lng="nl">kien</trd>
        <trd lng="pl">&lstroke;uczywo</trd>
        <trd lng="ru">&c_l;&c_u;&c_ch;&c_i;&c_n;&c_a;</trd>
        <trd lng="sv">tj&auml;rved</trd>
      </snc>

      <snc mrk="ken.0o.PV">
        <dif>
          Rezinori&ccirc;a ligno:
          <ekz>
            <tld/>a tor&ccirc;o.
          </ekz>
        </dif>                             
        <trd lng="be">&c_s;&c_m;&c_a;&c_l;&c_ja;&c_k;</trd>
        <trd lng="br">koad rousinek</trd>
        <trdgrp lng="de">
              <trd>Kienholz</trd>
        </trdgrp>
        <trd lng="en">resinous wood</trd>
        <trd lng="es">madera resinosa</trd>
        <trd lng="fr">bois r&eacute;sineux</trd>
        <trd lng="hu">gyant&aacute;s fa</trd>
        <trd lng="nl">harsrijk hout</trd>
        <trdgrp lng="ru">
          <trd>&c_s;&c_m;&c_o;&c_l;&c_mol;&c_jo;</trd>,
          <trd>&c_o;&c_s;&c_m;&c_o;&c_l;</trd>,
          <trd>&c_s;&c_m;&c_o;&c_l;&c_i;&c_s;&c_t;&c_a;&c_ja; &c_d;&c_r;&c_je;&c_v;&c_je;&c_s;&c_i;&c_n;&c_a;</trd>
        </trdgrp>
      </snc>
  <trdgrp lng="cs">
    <trd>lou&ccaron;</trd>,
    <trd>smoln&eacute; d&rcaron;evo</trd>
  </trdgrp>
  <trdgrp lng="de">
        <trd>Kien</trd>,
        <trd>Kienholz</trd>
  </trdgrp>
  <trd lng="ja">&#x6A39;&#x8102;&#x6750;  <pr>&#x3058;&#x3085;&#x3057;&#x3056;&#x3044;</pr></trd>
  <trdgrp lng="sk">
    <trd>l&uacute;&ccaron;</trd>,
    <trd>smolienka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43A;&c_ib;&#x43F;&c_a;</trd>,
    <trd>&c_s;&#x43A;&c_ib;&#x43F;&#x43A;&c_a;</trd>,
    <trd>&c_s;&#x43A;&c_a;&#x43B;&#x43A;&c_a;</trd>,
    <trd>&#x43B;&c_u;&c_ch;&c_i;&#x43D;&c_a;</trd>
  </trdgrp>
</drv>
  </art>
