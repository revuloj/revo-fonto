<kap>
  <ofc>1</ofc>
  <rad>car</rad>/o
</kap>
<drv mrk="car.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Regnestro en kelkaj slavaj landoj:
      <ekz>
        dukapa blanka aglo estis simbolo de multaj serbaj re&gcirc;oj
        kaj anka&ubreve;
        de <tld/>o Du&scaron;an<fnt><aut>K. Eckstadt</aut>:
        <vrk>Insultoj,
        misinformoj kaj malveroj</vrk>, <bib>Monato</bib><lok>jaro
        2001a,
        numero 3a, p. 8a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_c;&c_a;&c_r;</trd>
  <trd lng="ca">tsar</trd>
  <trd lng="cs">car</trd>
  <trd lng="de">Zar</trd>
  <trd lng="el">&tau;&sigma;&alfa_ton;&rho;&omikron;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>czar</trd>,
    <trd>csar</trd>,
    <trd>tsar</trd>
  </trdgrp>
  <trd lng="be">&c_c;&c_a;&c_r;</trd>
  <trd lng="br">tsar</trd>
  <trd lng="es">zar</trd>
  <trd lng="fa">&a_t;&a_z;&a_A;&a_r;</trd>
  <trd lng="fr">tsar</trd>
  <trd lng="hu">c&aacute;r</trd>
  <trd lng="it">zar</trd>
  <trd lng="nl">tsaar</trd>
  <trd lng="no">tsar</trd>
  <trd lng="pl">car</trd>
  <trd lng="pt">czar</trd>
  <trd lng="ru">&c_c;&c_a;&c_r;&c_mol;</trd>
  <trd lng="sk">c&aacute;r</trd>
  <trd lng="sv">tsar</trd>
</drv>
</art>
