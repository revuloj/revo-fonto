<art mrk="$Id: car.xml,v 1.29 2024/03/07 21:21:30 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>car</rad>/o
</kap>
<drv mrk="car.0o">
  <kap><tld/>o</kap>
  <snc mrk="car.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="monarh.0o">Monarko</ref> en kelkaj slavaj landoj:
      <ekz>
        kiu unufoje malpacis kun <tld/>o, kun li ne repaci&gcirc;os sur tero sincere
        <fnt>
          <bib>SrT</bib>
          <lok>libro 10a</lok>
        </fnt>;
      </ekz>
      <ekz>
        dukapa blanka aglo estis simbolo de multaj serbaj re&gcirc;oj
        kaj anka&ubreve; de <tld/>o <nom>Du&scaron;an</nom>
        <fnt>
        <aut>K. Eckstadt</aut>
        <vrk>Insultoj, misinformoj kaj malveroj</vrk>
        <bib>Monato</bib>
        <lok>2001/3, p. 8a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_c;&c_a;&c_r;</trd>
  <trd lng="ca">tsar</trd>
  <trd lng="cs">car</trd>
  <trd lng="de">Zar</trd>
  <trd lng="el">&tau;&sigma;&alfa_ton;&rho;&omikron;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>czar</trd>,
    <trd>csar</trd>,
    <trd>tsar</trd>
  </trdgrp>
  <trd lng="be">&c_c;&c_a;&c_r;</trd>
  <trd lng="br">tsar</trd>
  <trd lng="es">zar</trd>
  <trd lng="fa">&a_t;&a_z;&a_A;&a_r;</trd>
  <trd lng="fr">tsar</trd>
  <trdgrp lng="he">
    <trd>&#x5E6;&#x5D0;&#x5E8;</trd>,
    <trd>&#x5E9;&#x5DC;&#x5D9;&#x5D8; &#x5E1;&#x5DC;&#x5D0;&#x5D1;&#x5D9;</trd>
  </trdgrp>
  <trd lng="hu">c&aacute;r</trd>
  <trd lng="it">zar</trd>
  <trd lng="ja">&#x30C4;&#x30A1;&#x30FC;&#x30EA;</trd>
  <trd lng="nl">tsaar</trd>
  <trd lng="no">tsar</trd>
  <trd lng="pl">car</trd>
  <trd lng="pt">czar</trd>
  <trd lng="ru">&c_c;&c_a;&c_r;&c_mol;</trd>
  <trd lng="sk">c&aacute;r</trd>
  <trd lng="sv">tsar</trd>
  <trd lng="uk">&c_c;&c_a;&c_r;</trd>
  <trd lng="zh">&#x6C99;&#30343; <pr>sh&amacron;hu&#xE1;ng</pr></trd>
</drv>
