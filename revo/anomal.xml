<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>anomali</rad>/o
</kap>
<drv mrk="anomal.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Eksternorma&jcirc;o, deflanki&gcirc;o de la &gcirc;enerala regulo.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_n;&c_a;&c_m;&c_a;&c_l;&c_ib;&c_ja;</trd>,
    <trd>&c_a;&c_d;&c_h;&c_ib;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Abweichung</trd>,
      <trd>Anomalie</trd>
  </trdgrp>
  <trd lng="en">anomaly</trd>
  <trd lng="es">anomal&iacute;a</trd>
  <trd lng="fr">anomalie</trd>
  <trdgrp lng="hu">
    <trd>elt&eacute;r&eacute;s</trd>,
    <trd>rendelleness&eacute;g</trd>
    <trd>anom&aacute;lia</trd>
  </trdgrp>
  <trd lng="nl">anomalie</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_o;&c_m;&c_a;&c_l;&c_i;&c_ja;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_r;&c_a;&c_v;&c_i;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_k;&c_l;&c_o;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>anom&aacute;lia</trd>,
    <trd>nepravidelnos&tcaron;</trd>
  </trdgrp>
</drv>
</art>
