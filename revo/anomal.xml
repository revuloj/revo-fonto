    <trd>&#1085;&c_je;&#1087;&c_r;&c_a;&c_v;&c_i;&#1083;&#1100;&#1085;&c_ib;&c_s;&c_t;&#1100;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_h;&c_i;&#1083;&c_je;&#1085;&#1085;&#1103;</trd>
  </trdgrp>
</drv>

<drv mrk="anomal.0a">
  <kap><tld/>a</kap>
  <snc mrk="anomal.0a.SCI">
    <dif>
      Nekonforma al la atendebla, en ceteraj
      okazoj normala situacio:
      <ekz>
        iuj kavern-animaloj estas tre <tld/>aj
        <fnt>
          <bib>OriginoDeSpecioj</bib>,
          <lok>&ccirc;apitro 5a</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;is la fino de la 15a jarcento la portugala havis 
        pli ol cent <tld/>ajn verbojn
        <fnt>
          <bib>Monato</bib>
          <aut>Geraldo Mattos</aut>
          <vrk><url ref="&Monato;/2008/009422.php">Rezulto de preseraro</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="konfor.0a">konforma</ref>,
      <ref cel="normal.0a">normala</ref>,
      <ref cel="ordina.0a">ordinara</ref>,
      <ref cel="regul.0a">regula</ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#23214; <pr>t&#333;u</pr></trd>,
    <trd>&#23696; <pr>q&#237;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_n;&c_a;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trdgrp lng="de">
    <trd>abweichend</trd>,
    <trd>irregul&auml;r</trd>
  </trdgrp><trdgrp lng="fr">
    <trd>aberrant</trd>,
    <trd>d&eacute;viant</trd>
  </trdgrp>
  <trd lng="he">&alef;&#1458;&nun;&vav;&#1465;&mem;&#1463;&lamed;&#1460;&yod;&#1464;&he;</trd>
  <trd lng="ja">&#30064;&#20363;&#12398;  <pr>&#12356;&#12428;&#12356;&#12398;</pr></trd>
  <trdgrp lng="pl">
    <trd>anomalny</trd>,
    <trd>nieprawid&lstroke;owy</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#1085;&c_je;&#1087;&c_r;&c_a;&c_v;&c_i;&#1083;&#1100;&#1085;&c_i;&c_j;</trd>,
    <trd>&c_a;&#1085;&#1086;&#1084;&c_a;&#1083;&#1100;&#1085;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_h;&c_i;&#1083;&c_je;&#1085;&c_i;&c_j; &c_v;&c_ib;&c_d; &#1085;&#1086;&c_r;&#1084;&c_i;</trd>
  </trdgrp>
</drv>
