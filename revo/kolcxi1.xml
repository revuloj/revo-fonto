  <trd lng="uk">&#x43A;&#x43E;&#x43B;&c_h;&c_ib;&c_c;&c_i;&#x43D;</trd>
  <trd lng="zh">&#x79CB;&#x6C34;&#x4ED9;&#x7D20; <pr>qi&#x16B;shu&#x1D0;xi&amacron;ns&#xF9;</pr></trd>
</drv>
