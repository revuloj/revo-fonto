<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: singul1.xml,v 1.25 2022/03/27 17:06:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>singult</rad>/i
</kap>
<drv mrk="singul1.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Eligi specialan bruon, ka&ubreve;zitan de spasma kuntiri&gcirc;o de la
      diafragmo kaj subita trapaso de aero tra la laringo:
      <ekz>
        plorante <tld/>i pro akra doloro;
      </ekz>
      <ekz>
        Vetiha denove brakumis min kaj <tld/>is
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://www.esperanto.be/fel/mon/007140.html">de Julian Modest</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_h;&c_malmol;&c_l;&c_c;&c_a;&c_m;</trd>
  <trd lng="bo">&#xF68;&#xF72;&#xF42;&#xF0B;&#xF40;&#xF0B;&#xF62;&#xF92;&#xFB1;&#xF42;&#xF0B;</trd>
  <trd lng="cs">&scaron;kytat</trd>
  <trdgrp lng="de">
    <trd>hicksen</trd>,
    <trd>Schluckauf haben</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>hiccough</trd>,
    <trd>hiccup</trd>
  </trdgrp>
  <trd lng="es">hipo</trd>
  <trd lng="fr">avoir le <ind>hoquet</ind></trd>
  <trd lng="hu">csuklik</trd>
  <trd lng="nl">hikken</trd>
  <trdgrp lng="pl">
    <trd>czka&cacute;</trd>,
    <trd>mie&cacute; <ind>czkawk&eogonek;</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_k;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">&ccaron;ka&tcaron; <klr>(sa)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x433;&#x438;&#x43A;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x456;&#x43A;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="singul1.plor0i">
  <kap>plor<tld/>i</kap>
  <snc>
    <dif>
      <ref tip="super" cel="plor.0i">Ploregi</ref> kun <tld/>oj:
      <ekz>
        &scirc;i malla&ubreve;te plor<tld/>is, premante sin al lia brusto
        <fnt><bib>Metrop</bib></fnt>;
      </ekz>
      <ekz>
        mi &ccirc;esis plor<tld/>i kaj ree ekdormis en via mola brakumo
        <fnt>
          <vrk><url ref="http://miresperanto.com/biblioteko/jozefo/03.htm">plendoj de Jozefo arestita</url></vrk>
          <lok>miresperanto.com</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_r;&c_i;&c_d;&c_a;&c_ja;</trd>
  <trdgrp lng="be">
    <trd>&c_r;&c_y;&c_d;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_k;&c_a;&c_c;&c_mol;
    &c_n;&c_a;&c_w;&c_z;&c_r;&c_y;&c_d;</trd>,
    <trd>&c_u;&c_s;&c_h;&c_l;&c_ib;&c_p;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bre&ccaron;et</trd>,
    <trd>vzlykat</trd>,
    <trd>&scaron;tk&aacute;t</trd>
  </trdgrp>
  <trd lng="de">schluchzen</trd>
  <trd lng="en">to sob</trd>
  <trd lng="fr">sangloter<klr> (avec hoquet)</klr></trd>
  <trd lng="hu">zokog</trd>
  <trd lng="nl">snikken</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_y;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_k;&c_a;&c_t;&c_mol;
    &c_n;&c_a;&c_v;&c_z;&c_r;&c_y;&c_d;</trd>,
    <trd>&c_v;&c_s;&c_h;&c_l;&c_i;&c_p;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>f&ncaron;uka&tcaron;</trd>,
    <trd>narieka&tcaron;</trd>,
    <trd>vzlyka&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x433;&#x438;&#x43A;&#x430;&#x442;&#x438; &#x432;&#x456;&#x434; &#x43F;&#x43B;&#x430;&#x447;&#x443;</trd>,
    <trd>&#x437;&#x430;&#x445;&#x43B;&#x438;&#x43D;&#x430;&#x442;&#x438;&#x441;&#x44F; &#x432;&#x456;&#x434; &#x43F;&#x43B;&#x430;&#x447;&#x443;</trd>
  </trdgrp>
</drv>


<drv mrk="singul1.0o">
  <kap><tld/>o</kap>
  <snc mrk="singul1.0o.SNC">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Ago <tld/>i:
      <ekz>
        <tld/>o estas ripeta kaj nevolata kontrahi&gcirc;o de diafragmo, pro &scirc;an&gcirc;o de frenika nervo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=289624">Singulto</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o-sceno en la 2-a akto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=432696">Tristan-akordo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Schluckauf</trd>
  <trd lng="en">hiccup</trd>
  <trdgrp lng="fr">
    <trd>hoquet</trd>,
    <trd><ind>myoclonie</ind> phr&eacute;noglottique</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x433;&#x438;&#x43A;&#x430;&#x432;&#x43A;&#x430;</trd>,
    <trd>&#x456;&#x43A;&#x430;&#x432;&#x43A;&#x430;</trd>,
    <trd>&#x433;&#x438;&#x43A;&#x430;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x456;&#x43A;&#x430;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>


</art>
