<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>singult</rad>/i
</kap>
<drv mrk="singul1.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Eligi specialan bruon, ka&ubreve;zitan de spasma kuntiri&gcirc;o de la diafragmo
      kaj subita trapaso de aero tra la laringo:
      <ekz>
        plorante <tld/>i pro akra doloro.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Schluckauf haben</trd>
  <trd lng="en">hiccough</trd>
  <trd lng="es">hipo</trd>
  <trd lng="nl">hikken</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_k;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="singul1.plor0i">
  <kap>plor<tld/>i</kap>
  <snc>
    <dif>
      Pro plorego <tld/>i.
    </dif>
  </snc>
  <trd lng="de">schluchzen</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_y;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_k;&c_a;&c_t;&c_mol; &c_n;&c_a;&c_v;&c_z;&c_r;&c_y;&c_d;</trd>,
    <trd>&c_v;&c_s;&c_h;&c_l;&c_i;&c_p;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
