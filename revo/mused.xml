<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mused.xml,v 1.11 2022/03/23 14:02:27 revo Exp $">
<kap>
   <rad>mused</rad>/oj
</kap>
<drv mrk="mused.0oj">
   <kap><tld/>oj</kap>
   <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
       <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref tip="malprt" cel="rongx.0uloj">ron&gcirc;uloj</ref>
       (<trd lng="la">Muridae</trd>):
      <ekz>
        la <tld/>oj konsistigas la plej grandan familion de la mamuloj <klr>[&#x2026;]</klr>,
        en la familio estas la veraj <ref tip="prt" cel="mus.0o.ZOO">musoj</ref>
        kaj <ref tip="prt" cel="rat.0o">ratoj</ref>,
        <ref tip="prt" cel="gerbil.0o">gerbiloj</ref>, kaj iliaj parencoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=139715">Musedoj</url></lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trd lng="cs">my&scaron;ovit&iacute;</trd>
  <trd lng="de">M&auml;useartige</trd>
  <trd lng="fr">murid&eacute;s</trd>
  <trd lng="hu">eg&eacute;rf&eacute;l&eacute;k</trd>
  <trd lng="nl">muisachtigen</trd>
  <trd lng="pl">myszowate</trd>
  <trd lng="pt">mur&iacute;deos</trd>
  <trdgrp lng="ru">
     <trd>&c_m;&c_y;&c_sh;&c_i;&c_n;&c_y;&c_je;<klr tip="ind"> 
(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
     <trd>&c_m;&c_y;&c_sh;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
   </trdgrp>
  <trd lng="sk">my&scaron;ovit&iacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x438;&#x448;&#x430;&#x447;&#x456;</trd>,
    <trd>&#x43C;&#x438;&#x448;&#x438;&#x43D;&#x456;</trd>
  </trdgrp>
</drv>
</art>
