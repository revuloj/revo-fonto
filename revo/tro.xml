<!-- ............ tro II ........... -->
<subart>
   <dif>
     Samsenca vortero:
   </dif>
   <drv mrk="tro.0a">
   <kap><tld/>a</kap>
   <snc>
     <dif>
       Estanta en pli ol sufi&ccirc;a kvanto a&ubreve; grado; superanta la
       &gcirc;ustan mezuron:
       <ekz>
         <tld/>a laboro;
       </ekz>
      <ekz>
        sklavigitaj de <tld/>a vino
        <fnt>
          <bib>NT</bib>
          <lok>Tito 2:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        li klinis sin sen <tld/>a humileco
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        por gardi sin kontra&ubreve; <tld/>aj superakvoj, ili konstruis potencajn digojn
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a stre&ccirc;ado de la okuloj, <klr>[&#8230;]</klr> povas ka&ubreve;zi kronikan kataron
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        observante lian <tld/>an deziron esti la&ubreve;data kaj admirata, <klr>[li]</klr> sa&gcirc;e
        sin detenis
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la okazo de <tld/>a laboro mi cedus al li la <esc>prekeftan</esc> oficon
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LI</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu el ni estas <tld/>a, necese estas, ke unu el ni malaperu
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekdua</lok>
        </fnt>;
      </ekz>
      <ekz>
         <tld/>a festeno estas veneno
         <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="eksces.0a">ekscesa</ref>,
      <ref cel="mezur.sen0asuper0a">senmezura, pretermezura, supermezura</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
   <trd>&c_p;&c_r;&c_a;&c_z;&c_mol;&c_m;&c_je;&c_r;&c_n;&c_y;</trd>,
   <trd>&c_z;&c_a;&c_l;&c_ib;&c_sh;&c_n;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="cs">
