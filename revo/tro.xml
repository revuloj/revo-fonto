    <trd>&#x4F59;&#20998;  <pr>&#12424;&#12406;&#12435;</pr></trd>,
    <trd>&#x4F59;&#x308A;&#29289;  <pr>&#12354;&#x307E;&#x308A;&#12418;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
     <trd>iets <ind>overdrevens</ind></trd>,
     <trd>exces</trd>
   </trdgrp>
   <trd lng="ru">&c_i;&c_z;&c_l;&c_i;&c_sh;&c_je;&c_k;</trd>
