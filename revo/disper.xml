<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: disper.xml,v 1.16 2022/03/15 16:19:58 revo Exp $">
<kap>
  <rad>dispers</rad>/i
</kap>
<drv mrk="disper.0i">
  <kap><tld/>i</kap>
  <snc>
    <uzo tip="fak">FOT</uzo>
    <dif>
      Spektrigi <ref cel="lum.0o.FIZ">lumon</ref>
      per refrakto:
      <ekz>
         partikloj produktataj per radiadaj kaj <tld/>aj procesoj, kiel fotonoj, ne&ubreve;trinoj kaj muonoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19031">Partikla fiziko</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="disper.0i.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
       Bone <ref tip="super" cel="miks.0i.kunmiksi">miksi</ref>
       &hcirc;emia&jcirc;ojn, kiuj apena&ubreve; kombini&gcirc;as, t.e. reagas unu kun alia:
      <ekz>
        por ke la sablo ne sedimenti&gcirc;u tro frue, oni <tld/>as &gcirc;in per organikaj kemia&jcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Werner Fuss</aut>
          <vrk><url ref="&Monato;/2014/011348.php">Novtipa gasminado: &ccirc;u vere dan&gcirc;era?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        dufaza <ref cel="emulsi.0o">emulsio</ref> <tld/>igita
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=534672">Emulsio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_mol;&c_s;&c_je;&c_j;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>dispergovat</trd>,
    <trd>rozkl&aacute;dat <klr>(sv&ecaron;tlo)</klr></trd>,
    <trd>rozlo&zcaron;it</trd>,
    <trd>rozptylovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>dispergieren</trd>,
    <trd>feinst verteilen</trd>,
    <trd>zerstreuen</trd>
  </trdgrp>
  <trd lng="fr">disperser<klr tip="ind"> (la lumi&egrave;re)</klr></trd>
  <trdgrp lng="pl">
    <trd>rozszczepia&cacute;<klr> (&sacute;wiat&lstroke;o)</klr></trd>,
    <trd>rozprasza&cacute;<klr> (&sacute;wiat&lstroke;o)</klr></trd>
  </trdgrp>
  <trd lng="pt">dispersar</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_s;&c_je;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>rozklada&tcaron; <klr>(svetlo)</klr></trd>,
    <trd>rozpty&lcaron;ova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x440;&#x43E;&#x437;&#x441;&#x456;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x440;&#x43E;&#x437;&#x43F;&#x43E;&#x440;&#x43E;&#x448;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="disper.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Fenomeno de <tld/>ado:
      <ekz>
        aberacio <klr>[okazas pro]</klr> <tld/>o de lumradioj, kiu kontra&ubreve;staras formi&gcirc;on de la preciza bildo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=7767">Aberacio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        vitro enhavanta oksidon de torio prezentas altan refraktan indicon kaj malaltan <tld/>on
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=28705">Torio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="disper.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Miksa&jcirc;o <tld/>igita:
      <ekz>
        la jonoj jodidaj estas multe pli grandaj ol la ordinaraj halogenidoj kaj tio rezultas en pli granda spaco por <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=520881">Jodida acido</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <adm>
    Kontroli, &ccirc;u la termino ne povas apliki&gcirc;i anka&ubreve;
    en balistiko. <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_mol;&c_s;&c_je;&c_j;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_d;&c_y;&c_s;&c_p;&c_e;&c_r;&c_s;&c_ib;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dispergace</trd>,
    <trd>dispergov&aacute;n&iacute;</trd>,
    <trd>rozptylov&aacute;n&iacute;</trd>,
    <trd>rozpt&yacute;len&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Dispersion</trd>,
    <trd>Streuung</trd>
  </trdgrp>
  <trd lng="fr">dispersion<klr tip="ind"> (de la
  lumi&egrave;re)</klr></trd>
  <trd lng="he">&#x5E4;&#x5D9;&#x5D6;&#x5D5;&#x5E8; &#x5D0;&#x5D5;&#x5E8;</trd>
  <trdgrp lng="pl">
    <trd>dyspersja<klr> &sacute;wietlna</klr></trd>,
    <trd>rozszczepienie<klr> (&sacute;wiat&lstroke;a)</klr></trd>,
    <trd>rozproszenie<klr> (&sacute;wiat&lstroke;a)</klr></trd>
  </trdgrp>
  <trd lng="pt">dispers&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_s;&c_je;&c_ja;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_d;&c_i;&c_s;&c_p;&c_je;&c_r;&c_s;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rozklad</trd>,
    <trd>rozptyl</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x434;&#x438;&#x441;&#x43F;&#x435;&#x440;&#x441;&#x456;&#x44F;</trd>,
    <trd>&#x434;&#x438;&#x441;&#x43F;&#x435;&#x440;&#x433;&#x443;&#x432;&#x430;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="disper.0aangulo">
  <kap><tld/>a angulo</kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      Angulo formata de ekstremaj radioj, kiuj iras &gcirc;is la
      ekstremoj de la
      spektro.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_g;&c_a;&c_l;
  &c_r;&c_a;&c_s;&c_mol;&c_s;&c_je;&c_j;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Dispersionswinkel</trd>,
    <trd>Streuungswinkel</trd>
  </trdgrp>
  <trd lng="fr">angle de dispersion</trd>
  <trd lng="he">&#x5D6;&#x5D5;&#x5D5;&#x5D9;&#x5EA; &#x5E4;&#x5D9;&#x5D6;&#x5D5;&#x5E8;</trd>
  <trd lng="pt">&acirc;ngulo de dispers&atilde;o</trd>
  <trd lng="ru">&c_u;&c_g;&c_o;&c_l;
  &c_r;&c_a;&c_s;&c_s;&c_je;&c_ja;&c_n;&c_i;&c_ja;</trd>
</drv>

<drv mrk="disper.0eco">
  <kap><tld/>eco</kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      La propreca intenseco, kiun havas medio dum la okazo de <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_s;&c_p;&c_e;&c_r;&c_s;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>dispersn&iacute; schopnost</trd>,
    <trd>disperzita</trd>,
    <trd>disperznost</trd>,
    <trd>rozpt&yacute;lenost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Dispergierverm&ouml;gen</trd>,
    <trd>Streuungsverm&ouml;gen</trd>
  </trdgrp>
  <trd lng="fr">dispersivit&eacute;</trd>
  <trd lng="ru">&c_d;&c_i;&c_s;&c_p;&c_je;&c_r;&c_s;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">disperzn&aacute; schopnos&tcaron;</trd>
  <trd lng="uk">&#x434;&#x438;&#x441;&#x43F;&#x435;&#x440;&#x441;&#x43D;&#x456;&#x441;&#x442;&#x44C;</trd>
</drv>
</art>
