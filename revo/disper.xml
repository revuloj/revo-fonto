<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>dispers</rad>/i
</kap>
<drv mrk="disper.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      Ka&ubreve;zi <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>dispergieren</trd>,
    <trd>feinst verteilen</trd>,
    <trd>zerstreuen</trd>
  </trdgrp>
  <trd lng="fr">disperser<klr tip="ind"> (la lumi&egrave;re)</klr></trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_s;&c_je;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="disper.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Fenomeno konsistanta el tio, ke refrakti&gcirc;ante la fasko de blanka
      lumo malkomponi&gcirc;as en diverskolorajn radiojn.
    </dif>
    <adm>
      Kontroli, &ccirc;u la termino ne povas apliki&gcirc;i anka&ubreve;
      en balistiko. <aut>MB</aut>
    </adm>
  </snc>
  <trdgrp lng="de">
    <trd>Dispersion</trd>,
    <trd>Streuung</trd>
  </trdgrp>
  <trd lng="fr">dispersion<klr tip="ind"> (de la lumi&egrave;re)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_s;&c_je;&c_ja;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_d;&c_i;&c_s;&c_p;&c_je;&c_r;&c_s;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="disper.0aangulo">
  <kap><tld/>a angulo</kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      Angulo formata de ekstremaj radioj, kiuj iras &gcirc;is la ekstremoj de la
      spektro.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Dispersionswinkel</trd>,
    <trd>Streuungswinkel</trd>
  </trdgrp>
  <trd lng="fr">angle de dispersion</trd>
  <trd lng="ru">&c_u;&c_g;&c_o;&c_l; &c_r;&c_a;&c_s;&c_s;&c_je;&c_ja;&c_n;&c_i;&c_ja;</trd>
</drv>

<drv mrk="disper.0eco">
  <kap><tld/>eco</kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      La propreca intenseco, kiun havas medio dum la okazo de <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Dispergierverm&ouml;gen</trd>,
    <trd>Streuungsverm&ouml;gen</trd>
  </trdgrp>
  <trd lng="fr">dispersivit&eacute;</trd>
  <trd lng="ru">&c_d;&c_i;&c_s;&c_p;&c_je;&c_r;&c_s;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>
</art>
