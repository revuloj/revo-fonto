<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>ador</rad>/i
</kap>
<drv mrk="ador.0i">
   <kap><ofc>*</ofc><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc num="1">
     <dif>
       Respektegi kaj honoregi kiel dion:
       <ekz>
         la hebreoj <tld/>is en la dezerto oran bovidon;
       </ekz>
       <ekz>
         vivanton ni malhonoras, mortinton ni <tld/>as
         <fnt>
           (<aut>Zamenhof</aut>:
            <vrk>Proverbaro</vrk>)</fnt>.
       </ekz>
     </dif>
     <trd lng="de">anbeten</trd>
     <trdgrp lng="en">
        <trd>adore</trd>,
        <trd>worship</trd>
     </trdgrp>
     <trdgrp lng="pt">
        <trd>adorar</trd>,
        <trd>idolatrar</trd>,
        <trd>reverenciar</trd>
     </trdgrp>
     <trdgrp lng="sv">
        <trd>dyrka</trd>,
        <trd>tillbedja</trd>
     </trdgrp>
     <trd 
lng="ru">&#1087;&#1086;&#1082;&#1083;&#1086;&#1085;&#1103;&#1090;&#1100;&#1089;&#1103;</trd>
   </snc>
   <snc num="2">
     <dif>
       Amegi:
       <ekz>
         <tld/>i virinon, poezion<fnt>B</fnt>;
       </ekz>
       <ekz>
         &ccirc;iu abomena&jcirc;o trovas sian <tld/>anton
         <fnt>
           (<aut>Zamenhof</aut>:
            <vrk>Proverbaro</vrk>)</fnt>.
       </ekz>
     </dif>
     <trd lng="de">verehren</trd>
     <trdgrp lng="pt">
        <trd>adorar</trd>,
        <trd>idolatrar</trd>
     </trdgrp>
     <trd lng="ru">&#1086;&#1073;&#1086;&#1078;&#1072;&#1090;&#1100;</trd>
     <trd lng="en">adore</trd>
     <trdgrp lng="sv">
        <trd>dyrka</trd>,
        <trd>avguda</trd>
     </trdgrp>
   </snc>
   <trd lng="fr">adorer</trd>
   <trd lng="it">adorare</trd>
</drv>
</art>
