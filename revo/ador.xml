<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap><ofc>*</ofc><rad>ador</rad>/i</kap>
<drv mrk="ador.0i">
   <kap><ofc>*</ofc><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc num="1">
     <dif>
       Respektegi kaj honoregi kiel dion:
       <ekz>
         la hebreoj <tld/>is en la dezerto oran bovidon;
       </ekz>
       <ekz>vivanton ni malhonoras, mortinton ni <tld/>as<fnt><bib>PrV</bib></fnt>.</ekz>
     </dif>
     <trd lng="be">&c_p;&c_a;&c_k;&c_l;&c_a;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>
     <trd lng="de">anbeten</trd>
     <trdgrp lng="en">
        <trd>adore</trd>,
        <trd>worship</trd>
     </trdgrp>
     <trd lng="es">adorar</trd>
     <trdgrp lng="pt">
        <trd>adorar</trd>,
        <trd>idolatrar</trd>,
        <trd>reverenciar</trd>
     </trdgrp>
    <trd lng="sk">uctieva&tcaron;</trd>
     <trdgrp lng="sv">
        <trd>dyrka</trd>,
        <trd>tillbedja</trd>
     </trdgrp>
     <trd 
lng="ru">&c_p;&c_o;&c_k;&c_l;&c_o;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
   </snc>
   <snc num="2">
     <dif>
       Amegi:
       <ekz>
         <tld/>i virinon, poezion<fnt>B</fnt>;
       </ekz>
       <ekz>&ccirc;iu abomena&jcirc;o trovas sian <tld/>anton<fnt><bib>PrV</bib></fnt>.</ekz>
     </dif>
     <trdgrp lng="be">
       <trd>&c_a;&c_b;&c_a;&c_zh;&c_a;&c_c;&c_mol;</trd>, 
       <trd>&c_g;&c_o;&c_r;&c_a;&c_ch;&c_a; &c_l;&c_ju;&c_b;&c_ib;&c_c;&c_mol;</trd>
     </trdgrp>
     <trd lng="de">verehren</trd>
     <trdgrp lng="pt">
        <trd>adorar</trd>,
        <trd>idolatrar</trd>
     </trdgrp>
     <trd lng="ru">&c_o;&c_b;&c_o;&c_zh;&c_a;&c_t;&c_mol;</trd>
     <trd lng="en">adore</trd>
     <trdgrp lng="sv">
        <trd>dyrka</trd>,
        <trd>avguda</trd>
     </trdgrp>
   </snc>
   <trd lng="fr">adorer</trd>
   <trd lng="hu">im&aacute;d</trd>
   <trd lng="it">adorare</trd>
   <trd lng="nl">aanbidden</trd>
    <trd lng="sk">zbo&zcaron;&ncaron;ova&tcaron;</trd>
</drv>
</art>
