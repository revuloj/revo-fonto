<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ador.xml,v 1.37 2024/01/23 09:25:59 revo Exp $">
<kap>
  <ofc>*</ofc><rad>ador</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="ador.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ador.0i.religie">
    <uzo tip="fak">REL</uzo>
    <dif>
      <ref tip="sin" cel="respek.0egi">Respektegi</ref> kaj
      <ref tip="super" cel="honor.0i.respekti">honoregi</ref> kiel dion:
      <ekz>
        ne <tld/>u iliajn diojn kaj ne servu ilin, <klr>[&#8230;]</klr> sed frakasu ilin kaj
        detruu iliajn statuojn
        <fnt>
          <bib>MT</bib>
          <lok>&Eli; 23:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili faris al si fanditan bovidon kaj
          <ref cel="klin.ador0igxi">adorklini&gcirc;i</ref>s al &gcirc;i
        <fnt>
          <bib>MT</bib>
          <lok>&Eli; 32:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili ne bu&ccirc;u plu siajn oferbrutojn al la demonoj, kiujn ili mal&ccirc;aste <tld/>as
        <fnt>
          <bib>MT</bib>
          <lok>&Lev; 17:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        li faras <klr>[&#8230;]</klr> idolon kaj <tld/>as &gcirc;in
        <fnt>
          <bib>MT</bib>
          <lok>&Jes; 44:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        vivanton ni malhonoras, mortinton ni <tld/>as
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="apoteo.0i">apoteozi</ref>,
      <ref cel="kult.0i.REL">kulti</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_k;&c_l;&c_a;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_p;&c_a;&c_h;&c_v;&c_a;&c_l;&c_ja;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="br">azeuli&ntilde;</trd>
    <trd lng="ms">memuja</trd>
    <trdgrp lng="en">
      <trd>adore</trd>,
      <trd>worship</trd>
    </trdgrp>
    <trd lng="es">adorar</trd>
    <trdgrp lng="fr">
      <trd>adorer<klr tip="ind"> (v&eacute;n&eacute;rer)</klr></trd>,
      <trd>rendre un <ind>culte</ind> (&agrave;)</trd>,
      <trd>r&eacute;v&eacute;rer</trd>,
      <trd>v&eacute;n&eacute;rer</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>adorowa&cacute;</trd>,
      <trd>czci&cacute;</trd>,
      <trd>wielbi&cacute;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>adorar</trd>,
      <trd>idolatrar</trd>,
      <trd>reverenciar</trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>adora</trd>,
      <trd>diviniza</trd>,
      <trd>venera</trd>
    </trdgrp>
    <trd lng="sk">uctieva&tcaron;</trd>
    <trdgrp lng="sv">
      <trd>dyrka</trd>,
      <trd>tillbedja</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_o;&c_k;&c_l;&c_o;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
  <snc mrk="ador.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Honoregi kvaza&ubreve; dion a&ubreve; dia&jcirc;on;
      pasie inklinegi al iu a&ubreve; io:
      <ekz>
        mi <tld/>as la &ccirc;ampanon
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        homo esperon sen&ccirc;ese <tld/>as
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>as virinojn! ili estas la diinoj de mia koro
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i rekonis tiun elektroplenan okulfajron, kiu <tld/>is, kiu petegis, kiu ordonis
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kronprinco<klr>[n&#8230;]</klr> la armeo <tld/>is
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro 15a</lok>
        </fnt>;
      </ekz>
      <ekz>
        pasigi tutan duontagon en la arbaro <klr>[&#8230;ho kiel]</klr>,
        mi <tld/>as la arbaron
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro 2a</lok>
        </fnt>;
      </ekz>
      <ekz>
        vivanton ni malhonoras, mortinton ni <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu mem sin <tld/>as, nenion valoras
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="am.0egi">amegi</ref>,
      <ref cel="ard.0i.FIG">ardi<sncref/></ref>,
      <ref cel="sxat.0i">&scirc;ati</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_a;&c_b;&c_a;&c_zh;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_g;&c_o;&c_r;&c_a;&c_ch;&c_a; &c_l;&c_ju;&c_b;&c_ib;&c_c;&c_mol;
      </trd>
    </trdgrp>
    <trd lng="br">beza&ntilde; <ind>sod</ind> gant</trd>
    <trd lng="de">verehren</trd>
    <trdgrp lng="fr">
      <trd>adorer<klr tip="ind"> (ch&eacute;rir)</klr></trd>,
      <trd>aduler</trd>,
      <trd>ch&eacute;rir</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>adorowa&cacute;</trd>,
      <trd>wielbi&cacute;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>adorar</trd>,
      <trd>idolatrar</trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_b;&c_o;&c_zh;&c_a;&c_t;&c_mol;</trd>
    <trd lng="en">adore</trd>
    <trdgrp lng="sv">
      <trd>dyrka</trd>,
      <trd>avguda</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>adorovat</trd>,
    <trd>ct&iacute;t</trd>,
    <trd>ko&rcaron;it se</trd>,
    <trd>velebit</trd>,
    <trd>zbo&zcaron;n&ecaron; uct&iacute;vat</trd>,
    <trd>zbo&zcaron;&ncaron;ovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>anbeten</trd>,
    <trd>verg&ouml;ttern</trd>
  </trdgrp>
  <trd lng="he">&lamed;&he;&ayin;&resh;&yod;&ftsadi;</trd>
  <trd lng="hu">im&aacute;d</trd>
  <trd lng="it">adorare</trd>
  <trdgrp lng="ja">
    <trd>&#x793C;&#x62DD;&#12377;&#x308B;  <pr>&#x308C;&#12356;&#x306F;&#12356;&#12377;&#x308B;</pr></trd>,
    <trd>&#x5D07;&#x62DD;&#12377;&#x308B;  <pr>&#12377;&#12358;&#x306F;&#12356;&#12377;&#x308B;</pr></trd>,
    <trd>&#12354;&#x304C;&#12417;&#x308B;</trd>,
    <trd>&#x656C;&#24917;&#12377;&#x308B;  <pr>&#x305F;&#x304B;&#12375;&#12375;&#x305F;&#12377;&#x308B;</pr></trd>,
    <trd>&#12354;&#12371;&#x304C;&#x308C;&#x308B;</trd>,
    <trd>&#22823;&#x597D;&#x304D;&#12391;&#12354;&#x308B;  <pr>&#12384;&#12356;&#12377;&#x304D;&#12391;&#12354;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">aanbidden</trd>
  <trdgrp lng="sk">
    <trd>vz&yacute;va&tcaron;</trd>,
    <trd>zbo&zcaron;&ncaron;ova&tcaron;</trd>,
    <trd>zbo&zcaron;ne uctieva&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_o;&c_b;&c_o;&c_zh;&c_n;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_l;&c_o;&c_n;&c_ja;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="ador.0anto">
  <kap>
    <tld/>anto
  </kap>
  <snc mrk="ador.0anto.REL">
    <dif>
      Tiu, kiu religie <ref tip="vid" cel="ador.0i.religie"><tld/>as<sncref/></ref>:
      <ekz>
        mi malamas la <tld/>antojn de vantaj idoloj
        <fnt>
          <bib>MT</bib>
          <lok>&Psa; 31:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        veraj <tld/>antoj <tld/>os la Patron la&ubreve; spirito kaj vero
        <fnt>
          <bib>NT</bib>
          <lok>&Joh; 4:23</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Anbeter</trd>
    <trd lng="zh">&#x5D07;&#x62DC;&#32773; <pr>ch&#xF3;ngb&#xE0;izh&#x11B;</pr></trd>
</snc>
  <snc mrk="ador.0anto.FIG">
    <dif>
      Tiu, kiu pasie amas kaj respektas, <ref tip="vid" cel="ador.0i.FIG"><tld/>as<sncref/></ref>:
      <ekz>
        kolekti&gcirc;as la <tld/>antoj de <nom>Kama</nom>, nia pastrino
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro 6a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>antoj de <nom>Thorwaldsen</nom>
        <fnt>
          <bib>Fab2</bib>
          <lok>Najbaraj familioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        nu, diris <nom>Stella</nom>, <klr>[&#8230;]</klr> elser&ccirc;u la pekulon <klr>[&#8230;]</klr> kaj starigu
        lin en la vicon de miaj <tld/>antoj
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia humila <tld/>anto opiniis, ke for de &scirc;ia &ccirc;eesto la tempo pasas vere tro
        malrapide
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>&ccirc;iu abomena&jcirc;o trovas sian <tld/>anton</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="fr">tous les <ind>gout</ind>s sont dans la
        nature</trd>
      </ekz>
    </dif>
    <trd lng="de">Verehrer</trd>
    <trdgrp lng="zh">
    <trd>&#x4EF0;&#24917;&#32773; <pr>y&#x1CE;ngm&#xF9;zh&#x11B;</pr></trd>,
    <trd>&#x5D07;&#x62DC;&#32773; <pr>ch&#xF3;ngb&#xE0;izh&#x11B;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_h;&c_ib;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_p;&c_a;&c_k;&c_l;&c_o;&c_n;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="fr">adorateur</trd>
  <trd lng="ja">&#x5D07;&#x62DD;&#32773;  <pr>&#12377;&#12358;&#x306F;&#12356;&#12375;&#12419;</pr></trd>
</drv>

<drv mrk="ador.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc mrk="ador.0ajxo.REL">
    <dif>
      Tio, kion iu <ref tip="vid" cel="ador.0i.religie"><tld/>as<sncref/></ref>:
      <ekz>
        vero, jen la <tld/>a&jcirc;o de l&#8217; dervi&scirc;o <klr>(&#8230;)</klr> tion li adoris, tio estis
        lia sankta&jcirc;o kaj &ccirc;io&#8230;
        <fnt>
          <aut>T. Pogosjan, trad. V. Sarkisjan</aut>
          <vrk>La dervi&scirc;oj</vrk>
          <lok>en: Orienta Almanako, 1913</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="ador.0ajxo.FIG">
    <dif>
      Tio, kion iu <ref tip="vid" cel="ador.0i.FIG"><tld/>as<sncref/></ref>.
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="idol.0o">idolo</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&#8217;&c_je;&c_k;&c_t; <ind>&c_p;&c_a;&c_h;&c_v;&c_a;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_ja;</ind></trd>,
    <trd>&c_ib;&c_d;&c_a;&c_l;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>divinit&eacute;</trd>,
    <trd>idole</trd>
  </trdgrp>
</drv>
</art>
