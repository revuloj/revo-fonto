<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: parlam.xml,v 1.24 2023/10/20 14:25:20 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>parlament</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="parlam.0o">
  <kap><tld/>o</kap>
  <snc mrk="parlam.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Reprezentantaro de la regnanoj, kiu, en konstituciaj landoj, faras
      le&gcirc;ojn kaj vo&ccirc;donas bu&gcirc;eton:
      <ekz>
        interkonsili&gcirc;oj farataj en orda kaj <tld/>a
        maniero<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_l;&c_ja;&c_m;&c_e;&c_n;&c_t; <klr>(&c_v;&c_o;&c_r;&c_g;&c_a;&c_n;)</klr></trd>
  <trd lng="bg">&c_p;&c_a;&c_r;&c_l;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>
  <trd
  lng="bo">&#xF62;&#xF92;&#xFB1;&#xF63;&#xF0B;&#xF61;&#xF7C;&#xF44;&#xF66;&#xF0B;&#xF42;&#xFB2;&#xF7C;&#xF66;&#xF0B;&#xF5A;&#xF7C;&#xF42;&#xF66;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>N&aacute;rodn&iacute; shrom&aacute;&zcaron;d&ecaron;n&iacute;</trd>,
    <trd>parlament</trd>,
    <trd>sn&ecaron;m</trd>,
    <trd>sn&ecaron;movna</trd>
  </trdgrp>
  <trd lng="de">Parlament</trd>
  <trd lng="en">parliament</trd>
  <trd lng="fa">&f_p;&a_A;&a_r;&a_l;&a_m;&a_A;&a_n;</trd>
  <trd lng="fr">parlement</trd>
  <trd lng="hu">parlament</trd>
  <trd lng="it">parlamento</trd>
  <trdgrp lng="ja">
    <trd>&#x8B70;&#x4F1A;  <pr>&#x304E;&#x304B;&#12356;</pr></trd>,
    <trd>&#x56FD;&#x4F1A;  <pr>&#12371;&#12387;&#x304B;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">parlement</trd>
  <trd lng="pl">parlament</trd>
  <trd lng="pt">parlamento</trd>
  <trd lng="ro">parlament</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_l;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>parlament</trd>,
    <trd>snemov&ncaron;a</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&c_a;&c_r;&#x43B;&c_a;&#x43C;&c_je;&#x43D;&c_t;</trd>
</drv>

<drv mrk="parlam.0ano">
  <kap><tld/>ano</kap>
  <snc mrk="parlam.0ano.POL">
    <dif>
      Ano de <tld/>o:
      <ekz>
        Marco Cappato estas la unua <tld/>ano
        <klr>...</klr> kiu sukcesis devigi la
        EU-parlamenton esprimi sin pri esperanto
        <fnt>
          <aut>G. Silfer</aut>,
          <vrk><url ref="http://www.esperantio.net/index.php?id=883">
          Nia lingvo 122-jara</url></vrk>,
          <lok>HeKo, 2009-06-02 (391)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_l;&c_ja;&c_m;&c_e;&c_n;&c_t;&c_a;&c_r;&c_y;&c_j;</trd>
  <trd lng="fr">parlementaire<klr> (subst.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x8B70;&#x54E1;  <pr>&#x304E;&#12356;&#12435;</pr></trd>,
    <trd>&#x56FD;&#x4F1A;&#x8B70;&#x54E1;  <pr>&#12371;&#12387;&#x304B;&#12356;&#x304E;&#12356;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">parlementslid</trd>
  <trd lng="pl">parlamentarzysta</trd>
  <trd lng="ro">parlamentar</trd>
  <trdgrp lng="uk">
    <trd>&c_ch;&#x43B;&c_je;&#x43D; &#x43F;&c_a;&c_r;&#x43B;&c_a;&#x43C;&c_je;&#x43D;&c_t;&c_u;</trd>,
    <trd>&#x43F;&c_a;&c_r;&#x43B;&c_a;&#x43C;&c_je;&#x43D;&c_t;&c_a;&c_r;&c_ib;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="parlam.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="parlam.0ejo.POL">
    <dif>
      Kunsidejo de <tld/>o:
      <ekz>
        en la <tld/>ejo funkcias anka&ubreve; publika biblioteko
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://hungario.hu/bp.html">
          Budapest</url></vrk>,
          <lok>[vidita en 2006]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_l;&c_ja;&c_m;&c_e;&c_n;&c_t; <klr>(&c_m;&c_je;&c_s;&c_c;&c_a; &c_p;&c_a;&c_s;&c_je;&c_d;&c_zh;&c_a;&c_n;&c_mol;&c_n;&c_ja;&c_w;)</klr></trd>
  <trdgrp lng="cs">
    <trd>parlament <klr>(budova)</klr></trd>,
    <trd>sn&ecaron;movna</trd>
  </trdgrp>
  <trd lng="fr">parlement<klr> (b&acirc;timent)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x8B70;&#x4E8B;&#22530;  <pr>&#x304E;&#12376;&#12393;&#12358;</pr></trd>,
    <trd>&#x56FD;&#x4F1A;&#x8B70;&#x4E8B;&#22530;  <pr>&#12371;&#12387;&#x304B;&#12356;&#x304E;&#12376;&#12393;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">parlementsgebouw</trd>
  <trd lng="pl">parlament  <klr>(dom)</klr></trd>
  <trd lng="ro">parlament  <klr>(casa)</klr></trd>
  <trdgrp lng="sk">
    <trd>parlament</trd>,
    <trd>snemov&ncaron;a</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&c_a;&c_r;&#x43B;&c_a;&#x43C;&c_je;&#x43D;&c_t; (&c_b;&c_u;&c_d;&c_ib;&c_v;&#x43B;&#x44F;)</trd>
</drv>

<drv mrk="parlam.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="parlam.0ismo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Re&gcirc;imo de regno, kie registaro dependas de <tld/>o:
      <ekz>
        kelkaj regnoj havas <tld/>on, tute ne havante <tld/>isman
        re&gcirc;imon.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_l;&c_ja;&c_m;&c_e;&c_n;&c_t;&c_a;&c_r;&c_y;&c_z;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>parlamentn&iacute; syst&eacute;m</trd>,
    <trd>parlamentn&iacute; z&rcaron;&iacute;zen&iacute;</trd>
  </trdgrp>
  <trd lng="de">Parlamentarismus</trd>
  <trd lng="fa">&f_p;&a_A;&a_r;&a_l;&a_m;&a_A;&a_n;&a_t;&a_A;&a_r;&f_y;&a_s;&a_m;</trd>
  <trd lng="fr">parlementarisme</trd>
  <trd lng="hu">parlamentarizmus</trd>
  <trd lng="it">parlamentarismo</trd>
  <trdgrp lng="ja">
    <trd>&#x8B70;&#x4F1A;&#x653F;&#x6CBB;  <pr>&#x304E;&#x304B;&#12356;&#x305B;&#12356;&#12376;</pr></trd>,
    <trd>&#x8B70;&#x4F1A;&#21046;&#x5EA6;  <pr>&#x304E;&#x304B;&#12356;&#x305B;&#12356;&#12393;</pr></trd>,
    <trd>&#x8B70;&#x4F1A;&#x4E3B;&#x7FA9;  <pr>&#x304E;&#x304B;&#12356;&#12375;&#12421;&#x304E;</pr></trd>
  </trdgrp>
  <trd lng="nl">parlementarisme</trd>
  <trd lng="pl">parlamentaryzm</trd>
  <trd lng="ro">parlamentarism</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_l;&c_a;&c_m;&c_je;&c_n;&c_t;&c_a;&c_r;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">parlamentn&yacute; syst&eacute;m</trd>
  <trd lng="uk">&#x43F;&c_a;&c_r;&#x43B;&c_a;&#x43C;&c_je;&#x43D;&c_t;&c_a;&c_r;&c_i;&c_z;&#x43C;</trd>
</drv>
