<art mrk="$Id: endivi.xml,v 1.22 2023/12/06 10:33:04 revo Exp $">
<kap>
<ofc>2</ofc>
<rad>endivi</rad>/o
</kap>
<drv mrk="endivi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_specioj" cel="speci.0o.BIO">Specio</ref>
      de <ref tip="super" cel="cikori.0o">cikorio</ref>
      (<trd lng="la">Cichorium endivia</trd>), uzata kiel <ref tip="malprt"
      cel="salat.0o">salato</ref>:
      <ekz>
        endivio <klr>[&#8230;]</klr> ekzistas kun malsamaj formoj kaj gustoj, Tutfolia <tld/>o
        estas konata pro lar&gcirc;aj krem-koloraj amaraj folioj, Krispa <tld/>o havas
        verdajn krispajn foliojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=21979">Endivio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/f/f7/Cichorium_endivia_J3.jpg/640px-Cichorium_endivia_J3.jpg" prm="CC    
    BY-SA 3.0"><tld/>o (Cichorium endivia), sur kampo en Belgujo
        <fnt>
          <aut>Jamain, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Cichorium_endivia_J3.jpg">&WCW;/File:Cichorium_endivia_J3.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_k;&c_o;&c_r;&c_y;&c_ja; &c_s;&c_a;&c_l;&c_ja;&c_t;&c_n;&c_a;&c_ja;</trd>
  <trd lng="bg">&c_je;&c_n;&c_d;&c_i;&c_v;&c_i;&c_ja;</trd>
  <trd lng="ca">end&iacute;via</trd>
  <trdgrp lng="cs">
    <trd>&ccaron;ekanka &scaron;t&ecaron;rb&aacute;k</trd>,
    <trd>&scaron;t&ecaron;rb&aacute;k zahradn&iacute;</trd>
  </trdgrp>
  <trd lng="de">Endivie</trd>
  <trd lng="en">endive</trd>
  <trd lng="es">endibia</trd>
  <trd lng="fr">endive</trd>
  <trdgrp lng="ja">
    <trd>&#x30AD;&#x30AF;&#x30C2;&#x30B7;&#x30E3;</trd>,
    <trd>&#x30A8;&#x30F3;&#x30C0;&#x30A4;&#x30D6;</trd>
  </trdgrp>
  <trd lng="nl">andijvie</trd>
  <trd lng="pl">endywia</trd>
  <trdgrp lng="pt">
    <trd>end&iacute;via</trd>,
    <trd>almeir&atilde;o</trd>,
    <trd>chic&oacute;ria</trd>,
    <trd>escarola</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_n;&c_d;&c_i;&c_v;&c_i;&c_j;</trd>
  <trd lng="sk">&ccaron;akanka</trd>
  <trdgrp lng="uk">
    <trd>&c_c;&c_i;&#x43A;&#x43E;&c_r;&c_ib;&c_j; &c_s;&#x43F;&c_r;&c_a;&c_v;&c_zh;&#x43D;&c_ib;&c_j;</trd>,
    <trd>&c_je;&#x43D;&c_d;&c_i;&c_v;&c_ib;&c_j;</trd>
  </trdgrp>
</drv>
</art>
