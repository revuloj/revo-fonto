<kap>
  <ofc>*</ofc>
  <rad>graved</rad>/a
</kap>
<drv mrk="graved.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Portanta idon en la utero:
      <ekz>
        &scirc;i estas <tld/>a de nur tri monatoj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        nigraj nuboj <tld/>aj je pluvo
        <klr>(pretaj ellasi pluvon)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        decido <tld/>a je multaj sekvoj;
      </ekz>
      <ekz>
        <tld/>eco.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/9/98/Pregnant_woman.jpg/360px-Pregnant_woman.jpg" prm="PD">graveda virino
        <fnt>
          <aut>Ken Hammond</aut>
          <lok><url ref="&WCW;/File:Pregnant_woman.jpg">https://commons.wikimedia.org/wiki/File:Pregnant_woman.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_c;&c_ja;&c_zh;&c_a;&c_r;&c_n;&c_a;&c_ja;</trd>
  <trd lng="bg">&c_b;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_a;</trd>
  <trd lng="bo">&t_pha;&t_rata;&t_shabchu;&t_tsheg;&t_ga;&t_shabchu;&t_tsheg;&t_sa;&t_kata;&t_yata;&t_drengbu;&t_tsheg;&t_ya;&t_ga;&t_tsheg;&t_ya;&t_naro;&t_da;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trdgrp lng="ca">
    <trd>embarassada</trd>,
    <trd>prenyada</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>gravidn&iacute;</trd>,
    <trd>t&ecaron;hotn&aacute;</trd>,
    <trd>v jin&eacute;m stavu</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schwanger</trd>,
    <trd>in anderen <ind>Umst&auml;nden</ind></trd>
  </trdgrp>
  <trd lng="en">pregnant</trd>
  <trd lng="es">embarazada</trd>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_b;&a_s;&a_t;&a_n;</trd>,
    <trd>&a_b;&a_A;&a_r;&a_d;&a_A;&a_r;</trd>,
    <trd>&a_H;&a_A;&a_m;&a_l;&a_h;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>enceinte<klr tip="ind"> (adj.)</klr></trd>,
    <trd>gestante</trd>,
    <trd>gravide</trd>,
    <trd>grosse<klr tip="ind"> (enceinte)</klr></trd>,
    <trd>pleine<klr tip="ind"> (gravide)</klr></trd>
  </trdgrp>
  <trd lng="fy">swier</trd>
  <trdgrp lng="hu">
    <trd>terhes<klr tip="ind"> (&aacute;llapotos)</klr></trd>,
    <trd>&aacute;llapotos</trd>
  </trdgrp>
  <trd lng="io">gravida</trd>
  <trdgrp lng="ja">
    <trd>&#x598A;&#x5A20;&#x3057;&#x305F;  <pr>&#x306B;&#x3093;&#x3057;&#x3093;&#x3057;&#x305F;</pr></trd>,
    <trd>&#x306F;&#x3089;&#x3093;&#x3060;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zwanger</trd>,
    <trd>in <ind>verwachting</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>gr&aacute;vido</trd>,
   <trd>prenhe</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_a;&c_ja;</trd>,
    <trd>&c_ch;&c_r;&c_je;&c_v;&c_a;&c_t;&c_y;&c_j;<klr>(fig.)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>samodruh&aacute;</trd>,
    <trd>tehotn&aacute;</trd>
  </trdgrp>
  <trd lng="sv">gravid</trd>
  <trd lng="uk">&c_v;&c_a;&c_g;&c_ib;&c_t;&c_n;&c_a;</trd>
  <trd lng="vo">grodik</trd>
  <trd lng="zh">&#x53CC;&#x8EAB;&#x5B50; <pr>shu&#x101;ngsh&#x113;nzi</pr></trd>
</drv>

<drv mrk="graved.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Stato de iu <tld/>a:
      <ekz>
        Pro droguzado de la patrino dum la <tld/>eco, kaj pro malfacila
        aku&scirc;o, la infano estas cerbe kaj korpe tre kripla, kaj oni
        a&ubreve;guras al &gcirc;i mallongan sensencan vivon
         <fnt><vrk><url ref="http://esperanto.net/literaturo/roman/libr/krisilentrec.html">recenzo
         de Sten Johansson pri la libro "Krias la
         silento"</url></vrk>,</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_b;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t; &c_p;&c_r;&c_i; &c_ch;&c_o;&c_v;&c_je;&c_k;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>embar&agrave;s</trd>,
    <trd>gestaci&oacute;</trd>
  </trdgrp>
  <trd lng="cs">t&ecaron;hotenstv&iacute;</trd>
  <trd lng="de">Schwangerschaft</trd>
  <trd lng="en">pregnancy</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_A;&a_r;&a_d;&a_A;&a_r;&f_y;</trd>,
    <trd>&a_H;&a_A;&a_m;&a_l;&f_g;&f_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>grossesse<klr tip="ind"> (gravidit&eacute;)</klr></trd>,
    <trd>gestation</trd>,
    <trd>gravidit&eacute;</trd>
  </trdgrp>
  <trd lng="fy">swierw&ecirc;zen</trd>
  <trd lng="hu">terhess&eacute;g</trd>
  <trd lng="io">gravideso</trd>
  <trd lng="ja">&#x598A;&#x5A20;  <pr>&#x306B;&#x3093;&#x3057;&#x3093;</pr></trd>
  <trd lng="nl">zwangerschap</trd>
  <trd lng="ru">&c_b;&c_je;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">tehotnos&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_a;&c_g;&c_ib;&c_t;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_t;&c_ja;&c_zh;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x598A; <pr>r&#xE8;n</pr></trd>,
    <trd>&#x6000;&#x5B55;&#x671F; <pr>hu&#xE1;iy&#xF9;nq&#x12B;</pr></trd>,
    <trd>&#x6000;&#x5B55; <pr>hu&#xE1;iy&#xF9;n</pr></trd>,
    <trd>&#x598A;&#x5A20; <pr>r&#xE8;nsh&#x113;n</pr></trd>,
    <trd>&#x5B55; <pr>y&#xF9;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="graved.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari <tld/>a:
      <ekz>
        viroj <tld/>igas virinojn, por ke tiuj povu naski infanojn
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Viro">Viro</url></vrk>
        </fnt>;
      </ekz>
            <ekz>
        &ccirc;u ne estis necese, ke via patro <tld/>igu vian patrinon por vin naski
        <fnt>
          <aut>Moli&egrave;re, tradukis &Eacute;mile Boirac</aut>
          <vrk><url ref="https://archive.org/details/DonJuanAuLaShtonaFesteno">Don Juan a&ubreve; la &scirc;tona
          festeno</url></vrk>
          <lok>Akto Tria</lok>
        </fnt>?
      </ekz>
      <ekz>
        Anna naive diris al nobela sekvantino ke &scirc;i kredis ke kisoj sufi&ccirc;as por
        <tld/>igi virinon
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Anna_de_Kleve">Anna de Kleve</url></vrk>
        </fnt>;
        </ekz>
      <ekz>
        ekde la 60aj jaroj esploristoj uzis artefaritan ensemadon kaj aliajn metodojn
        por <tld/>igi kaj reprodukti la grandan pandon
        <fnt>
          <bib>Monato</bib>
          <aut>Peng Jianjun</aut>
          <vrk><url ref="&Monato;/2001/007372.html">Kloni grandan pandon</url></vrk>
          <lok>2001/04, p. 25</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="nask.0igi">naskigi</ref>,
      <ref cel="patr">patrinigi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>embarassar</trd>,
    <trd>prenyar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>oplodnit</trd>,
    <trd>p&rcaron;iv&eacute;st do jin&eacute;ho stavu</trd>
  </trdgrp>
  <trd lng="de">schw&auml;ngern</trd>
  <trd lng="en">impregnate</trd>
  <trd lng="es">embarazar</trd>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_b;&a_s;&a_t;&a_n; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_b;&a_A;&a_r;&a_d;&a_A;&a_r; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_H;&a_A;&a_m;&a_l;&a_h; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>mettre <ind>enceinte</ind></trd>,
    <trd>engrosser</trd>
  </trdgrp>
  <trd lng="hu">teherbe ejt</trd>
  <trd lng="io">gravidigar</trd>
  <trd lng="ja">&#x598A;&#x5A20;&#x3055;&#x305B;&#x308B;  <pr>&#x306B;&#x3093;&#x3057;&#x3093;&#x3055;&#x305B;&#x308B;</pr></trd>
  <trdgrp lng="nl">
    <trd><ind>zwanger</ind> maken</trd>,
    <trd>bevruchten<klr tip="ind"> (zwanger maken)</klr></trd>
  </trdgrp>
  <trd lng="pt">engravidar</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; <ind>&c_b;&c_je;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_o;&c_j;</ind></trd>,
    <trd>&c_o;&c_b;&c_r;&c_ju;&c_h;&c_a;&c_t;&c_i;&c_t;&c_mol;<klr> (&c_r;&c_a;&c_z;&c_g;.)</klr></trd>
  </trdgrp>
  <trd lng="sk">oplodni&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_r;&c_o;&c_b;&c_i;&c_t;&c_i; &c_v;&c_a;&c_g;&c_ib;&c_t;&c_n;&c_o;&c_ju;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_l;&c_ib;&c_d;&c_n;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="graved.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Fari&gcirc;i <tld/>a:
      <ekz>
        amba&ubreve; filinoj de Lot <tld/>i&gcirc;is de sia patro
        <fnt><bib>MT</bib>, <lok>&Gen; 19:36</lok></fnt>;
      </ekz>
      <ekz>
        monto <tld/>i&gcirc;is, muso naski&gcirc;is
        <klr>(grandioza peno, eta efiko)</klr><fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        oni <tld/>i&gcirc;as per malhonesta&jcirc;o, kaj naskas krimon
        <fnt><bib>MT</bib>, <lok>&Jes; 59:4</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu <tld/>i&gcirc;is per malbono kaj naskis mensogon
        <fnt><bib>MT</bib>, <lok>&Psa; 7:14</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="koncip.0i">koncipi</ref>,
      <ref cel="nask.0i">naski</ref>,
      <ref cel="akusx.0i">aku&scirc;i</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_c;&c_ja;&c_zh;&c_a;&c_r;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">concebre</trd>
  <trd lng="de">schwanger werden</trd>
  <trd lng="en">get pregnant</trd>
  <trdgrp lng="es">
    <trd>quedarse <ind>embarazada</ind></trd>,
    <trd>embarazarse</trd>
    <trd>quedarse <ind>encinta</ind></trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_b;&a_s;&a_t;&a_n; &a_sh;&a_d;&a_n;</trd>,
    <trd>&a_b;&a_A;&a_r;&a_d;&a_A;&a_r; &a_sh;&a_d;&a_n;</trd>,
    <trd>&a_H;&a_A;&a_m;&a_l;&a_h; &a_sh;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>devenir <ind>enceinte</ind></trd>,
    <trd>devenir <ind>gravide</ind></trd>,
    <trd>concevoir<klr tip="ind"> (devenir enceinte)</klr></trd>
  </trdgrp>
  <trd lng="hu">teherbe esik</trd>
  <trd lng="io">gravideskar</trd>
  <trdgrp lng="ja">
    <trd>&#x598A;&#x5A20;&#x3059;&#x308B;  <pr>&#x306B;&#x3093;&#x3057;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x306F;&#x3089;&#x3080;</trd>
  </trdgrp>
  <trd lng="nl"><ind>zwanger</ind> worden</trd>
  <trd lng="pt">engr&aacute;vidar-se</trd>
  <trd lng="ru">&c_z;&c_a;&c_b;&c_je;&c_r;&c_je;&c_m;&c_je;&c_n;&c_je;&c_t;&c_mol;</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_v;&c_a;&c_g;&c_ib;&c_t;&c_n;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_j;&c_t;&c_i;&nbsp;&c_v; &c_t;&c_ja;&c_zh;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x53D7;&#x80CE; <pr>sh&#xF2;ut&#x101;i</pr></trd>,
    <trd>&#x7372;&#x5F97;&#x598A;&#x5A20; <pr>hu&#xF2;d&#xE9;r&#xE8;nsh&#x113;n</pr></trd>,
    <trd>&#x6000; <pr>hu&#xE1;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="graved.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Graveda persono:
      <ekz>
        &ccirc;e <tld/>uloj <klr>[la manko de folia acido]</klr> povas ka&ubreve;zi misformadon de la embrio
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Foliata_acido">Foliata acido</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Schwangere</trd>
  <trd lng="en">pregnant person</trd>
  <trd lng="nl">zwangere</trd>
  <trd lng="io">gravido</trd>
  <trd lng="vo">grodan</trd>
  <trd lng="zh">&#x5B55;&#x5987; <pr>y&#xF9;nf&#xF9;</pr></trd>
</drv>
</art>
