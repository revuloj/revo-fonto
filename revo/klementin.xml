<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: klementin.xml,v 1.7 2022/05/01 08:09:24 revo Exp $">
<kap>
  <rad>klementin</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="klementin.0o">
  <kap><tld/>o</kap>
  <snc mrk="klementin.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="frukt.0o.KUI">Frukto</ref>
      de <tld/>arbo, dol&ccirc;a, sensema oran&gcirc;eto:
      <ekz>
        mi kutime nomas tiajn oran&gcirc;etojn <tld/>oj
        <fnt>
          <aut>E. Piateski</aut>,
          <vrk>en: <url ref="http://vortaro-blogo.blogspot.fr/2009/08/la-nomado-de-diversaj-citrusfruktoj.html">
          vortaro-blogo</url></vrk>,
          <lok>2009-09-06</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_je;&c_m;&c_e;&c_n;&c_t;&c_y;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>klementina</trd>,
    <trd>klementinka</trd>
  </trdgrp>
  <trd lng="en">clementine</trd>
  <trd lng="fr">cl&eacute;mentine</trd>
  <trdgrp lng="pl">
    <trd>klementynka</trd>,
    <trd>mandarynka &sacute;r&oacute;dziemnomorska</trd>
  </trdgrp>
  <trd lng="sk">klement&iacute;nka</trd>
  <trd lng="uk">&#x43F;&#x456;&#x437;&#x43D;&#x44C;&#x43E;&#x441;&#x442;&#x438;&#x433;&#x43B;&#x438;&#x439; &#x431;&#x435;&#x437;&#x43A;&#x456;&#x441;&#x442;&#x43E;&#x447;&#x43A;&#x43E;&#x432;&#x438;&#x439; &#x43C;&#x430;&#x43D;&#x434;&#x430;&#x440;&#x438;&#x43D; (&#x43F;&#x43B;&#x456;&#x434;)</trd>
</drv>

<drv mrk="klementin.0arbo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="klementin.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="citrus.0o">Citruso</ref>
      rezultanta de krucigo de mandarino kaj
      dol&ccirc;a oran&gcirc;o (<trd lng="la">Citrus clementina</trd>):
      <ekz>
        <tld/>ujo estas specio de citruso
        proksime parenca al la mandarinujo
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Klementinujo">
          klementinujo</url>, 2013-03-12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">mandarinka <klr>(strom)</klr></trd>
  <trd lng="fr">cl&eacute;mentinier</trd>
  <trdgrp lng="pl">
    <trd>klementynka</trd>,
    <trd>mandarynka &sacute;r&oacute;dziemnomorska</trd>
  </trdgrp>
  <trd lng="sk">mandar&iacute;nka <klr>(strom)</klr></trd>
  <trd lng="uk">&#x43C;&#x430;&#x43D;&#x434;&#x430;&#x440;&#x438;&#x43D; (&#x434;&#x435;&#x440;&#x435;&#x432;&#x43E;)</trd>
</drv>
</art>

