<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>tun</rad>/o
</kap>
<drv mrk="tun.0o">
  <kap><tld/>o</kap>
  <snc mrk="tun.0o.pezunuo">
    <mlg>t</mlg>
    <dif>
      Komunuza <ref tip="lst" cel="unu.mezur0o"
      lst="voko:kvantoj">mezurunuo</ref> de
      maso,
      egala al 1000 <ref tip="prt" cel="kilogr.0o">kilogramoj</ref>
      <klr>(la&ubreve; metra <tld/>o)</klr> a&ubreve; 1016 kilogramoj
      <klr>(la&ubreve; angla <tld/>o)</klr>:
      <ekz>
        sendube estus pli facile kolekti po&scirc;tmarkojn a&ubreve;
        iujn aliajn
        eta&jcirc;ojn ol verajn sonorilegojn pre&gcirc;ejajn, kiuj povas
        pezi 
        e&ccirc; plurajn <tld/>ojn<fnt><aut>Saliko</aut>: <vrk>Kolekti
        sonorilojn</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 2a,
        p.
        24a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_o;&c_n;&c_a;</trd>
    <trd lng="ca">tona<klr> (m&egrave;trica)</klr></trd>
    <trd lng="de">Tonne</trd>
    <trd lng="en">metric ton</trd>
    <trd lng="fa">&a_t;&a_damma;&a_n;</trd>
    <trd lng="fr">tonne</trd>
    <trd lng="hu">tonna</trd>
    <trd lng="nl">ton</trd>
    <trd lng="pl">tona</trd>
    <trd lng="pt">tonelada</trd>
    <trd lng="ru">&c_t;&c_o;&c_n;&c_n;&c_a;</trd>
  </snc>
  <snc mrk="tun.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <uzo tip="stl">ARK</uzo>
    <fnt><bib>PV</bib></fnt>
    <ref tip="dif" cel="tinus.0o">tinuso</ref>
    <trd lng="ca">tonyina</trd>
    <trd lng="fr">thon</trd>
  </snc>
  <trd lng="sk">tona</trd>
</drv>
</art>
