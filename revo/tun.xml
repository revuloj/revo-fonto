<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>tun</rad>/o
</kap>
<drv mrk="tun.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Komunuza <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de maso,
      egala al mil <ref tip="prt" cel="kilogr.0o">kilogramoj</ref>
      <klr>(metra <tld/>o)</klr> a&ubreve; 1016 kilogramoj
      <klr>(angla <tld/>o)</klr>; simb. t.
    </dif>
    <trd lng="de">Tonne</trd>
    <trd lng="fr">tonne</trd>
    <trd lng="nl">ton</trd>
    <trd lng="ru">&c_t;&c_o;&c_n;&c_n;&c_a;</trd>
  </snc>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <uzo tip="stl">ARK</uzo>
    <fnt><bib>PV</bib></fnt>
    <ref tip="dif" cel="tinus.0o">tinuso.</ref>
    </snc>
</drv>
</art>
