<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bosk.xml,v 1.15 2021/03/05 15:48:38 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>bosk</rad>/o
</kap>
<drv mrk="bosk.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Belaran&gcirc;ita arbareto en parko:
      <ekz>
        el figa <tld/>o elglitis homo en nigra mantelo
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 17a</lok></fnt>
      </ekz>;
       <ekz>
        la okuloj de &ccirc;iuj direkti&gcirc;is al la cipresa <tld/>o
        <fnt>
          <aut>H. A. Luyken</aut>
          <vrk><url ref="http://www.gutenberg.org/ebooks/55954">Stranga
          hereda&jcirc;o</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">arbreda</trd>
  <trdgrp lng="cs">
    <trd>h&aacute;j</trd>,
    <trd>les&iacute;k</trd>,
    <trd>parkov&yacute; h&aacute;j</trd>
  </trdgrp>
  <trd lng="de">Boskett</trd>
  <trdgrp lng="en">
    <trd>grove</trd>,
    <trd>copse</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bosquet</trd>,
    <trd>bois <klr>(dans un parc)</klr></trd>,
    <trd>massif <klr>(d'arbres)</klr></trd>
  </trdgrp>
  <trd lng="he">&#x5D7;&#x5D5;&#x5E8;&#x5E9;&#x5D4; &#x5D1;&#x5E4;&#x5D0;&#x5E8;&#x5E7;</trd>
  <trdgrp lng="pl">
    <trd>gaj</trd>,
    <trd>gaik</trd>,
    <trd>zagajnik</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_o;&c_shch;&c_a; <klr>(&c_v;
  &c_p;&c_a;&c_r;&c_k;&c_je;)</klr></trd>
  <trd lng="sk">h&aacute;j</trd>
</drv>
</art>
