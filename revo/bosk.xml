<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bosk.xml,v 1.22 2023/10/14 08:39:48 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>bosk</rad>/o
</kap>
<drv mrk="bosk.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Belaran&gcirc;ita <ref tip="super" cel="arb.0aro">arbar</ref>eto en <ref tip="malprt" cel="park.0o">parko</ref>:
      <ekz>
        el figa <tld/>o elglitis homo en nigra mantelo
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 17a</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;tonaj tereta&gcirc;aj konstrua&jcirc;oj, &ccirc;irka&ubreve;itaj de <tld/>oj el diversaj arboj.
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;teliris junulo kaj junulino al la silenta <tld/>o kun la pla&ubreve;danta fonteto por
        flustri unu al la alia <klr>[&#x2026;]</klr> hezitemajn vortojn
        <fnt>
          <bib>Tor</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> la unuan fojon en la vivo vidis tiujn grandiozajn &gcirc;ardenojn,
        <klr>[kie]</klr> floris tutaj <tld/>oj da rozoj, sur&scirc;prucataj de fontana
        akvopolvo
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la okuloj de &ccirc;iuj direkti&gcirc;is al la cipresa <tld/>o
        <fnt>
          <aut>H. A. Luyken</aut>
          <vrk><url ref="&Gutenberg;ebooks/55954">Stranga
          hereda&jcirc;o</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_j;</trd>,
    <trd>&c_l;&c_ja;&c_s;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">arbreda</trd>
  <trdgrp lng="cs">
    <trd>h&aacute;j</trd>,
    <trd>les&iacute;k</trd>,
    <trd>parkov&yacute; h&aacute;j</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Boskett</trd>,
    <trd>Hain</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>grove</trd>,
    <trd>copse</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bosquet</trd>,
    <trd>bois <klr>(dans un parc)</klr></trd>,
    <trd>massif <klr>(d'arbres)</klr></trd>
  </trdgrp>
  <trd lng="he">&het;&vav;&resh;&shin;&he; &bet;&pe;&alef;&resh;&qof;</trd>
  <trdgrp lng="ja">
    <trd>&#x690D;&#x8FBC;&#x307F;  <pr>&#x3046;&#x3048;&#x3053;&#x307F;</pr></trd>,
    <trd>&#x6728;&#x7ACB;  <pr>&#x3053;&#x3060;&#x3061;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>gaj</trd>,
    <trd>gaik</trd>,
    <trd>zagajnik</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_o;&c_shch;&c_a; <klr>(&c_v;
  &c_p;&c_a;&c_r;&c_k;&c_je;)</klr></trd>
  <trd lng="sk">h&aacute;j</trd>
  <trd lng="uk">&c_g;&c_a;&c_j;&#xA0;(&c_u; &#x43F;&c_a;&c_r;&#x43A;&c_u;)</trd>
</drv>
</art>
