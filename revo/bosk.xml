<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>bosk</rad>/o
</kap>
<drv mrk="bosk.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Belaran&gcirc;ita arbareto en parko:
      <ekz>
        el figa <tld/>o elglitis homo en nigra mantelo
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 17a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>bosquet</trd>,
    <trd>bois <klr>(dans un parc)</klr></trd>,
    <trd>massif <klr>(d'arbres)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_o;&c_shch;&c_a; <klr>(&c_v; &c_p;&c_a;&c_r;&c_k;&c_je;)</klr></trd>
</drv>
</art>
