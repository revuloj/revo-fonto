<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>an&gcirc;el</rad>/o
</kap>
<drv mrk="angxel.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc num="1">
     <uzo tip="fak">REL</uzo>
     <dif>
       &Ccirc;iela spirito adoranta k servanta Dion; &ccirc;iela sendito:
       <ekz>
         malbona <tld/>o <klr>(Diablo)</klr>.
       </ekz>
     </dif>
   </snc>
   <snc num="2">
     <dif>
       Bonkorega, tolerema homo:
       <ekz>
         &scirc;i estas <tld/>o de mildeco;
       </ekz>
       <ekz>
         mi ne kredas, ke ne&ubreve;trala fundamento  faros el la
         homoj <tld/>ojn<fnt>Z</fnt>
         <klr>(<ref tip="vid" cel="nigr.0igi">nigrigi</ref>)</klr>;
       </ekz>
       <ekz>
         parolo <tld/>a, sed penso  pri &scirc;telo
         <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="hipokr.0i">hipokriti</ref>,
       <ref cel="kis.0o">kiso</ref>.
     </refgrp>
   </snc>
   <trd lng="be">&c_a;&c_n;&c_jo;&c_l;</trd>
   <trd lng="de">Engel</trd>
   <trd lng="en">angel</trd>
   <trd lng="fr">ange</trd>
   <trd lng="it">angelo</trd>
   <trd lng="ru">&#1072;&#1085;&#1075;&#1077;&#1083;</trd>
   <trd lng="tr">melek</trd>
</drv>
</art>
