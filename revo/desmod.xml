<art mrk="$Id: desmod.xml,v 1.18 2023/08/31 10:43:59 revo Exp $">
<kap>
<rad>desmodi</rad>/o
</kap>
<drv mrk="desmod.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio
      <ref tip="malprt" cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Desmodium</trd>):
      <ekz> 
        origine oni klasigis la telegrafoplanton
        <fnt>
          <vrk><url ref="https://eo.wiktionary.org/wiki/Vortaro_germana-Esperanta_t">Vortaro germana-Esperanta</url></vrk>
        </fnt>, tiel nomata pro sia formo, sub la genro <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_s;&c_m;&c_o;&c_d;&c_y;&c_ju;&c_m;</trd>
  <trdgrp lng="de">
    <trd>Desmodium</trd>
  </trdgrp>
  <trd lng="fr">desmodium</trd>
  <trd lng="he">&#x5DB;&#x5E8;&#x5D1;&#x5D5;&#x5DC;&#x5EA;</trd>
  <trdgrp lng="hu">
    <trd>h&uuml;velycsom&oacute;</trd>,
    <trd>koldusgyom</trd>
  </trdgrp>
  <trd lng="ja">&#x30CC;&#x30B9;&#x30D3;&#x30C8;&#x30CF;&#x30AE;</trd>
  <trd lng="nl">desmodium</trd>
  <trd lng="pl">desmodium</trd>
  <trdgrp lng="pt">
    <trd>desm&oacute;dio</trd>,
    <trd>carrapicho</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_je;&c_s;&c_m;&c_o;&c_d;&c_i;&c_u;&c_m;</trd>
  <trd lng="uk">&#x434;&#x435;&#x441;&#x43C;&#x43E;&#x434;&#x456;&#x443;&#x43C;</trd>
</drv>
</art>

