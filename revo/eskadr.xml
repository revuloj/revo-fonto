<kap>
  <ofc>*</ofc>
  <rad>eskadr</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="eskadr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="eskadr.0o.mara">
    <uzo tip="fak">MAR</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Aro da <ref cel="sxip.milit0o">milit&scirc;ipoj</ref>
      partoprenantaj en iu operacio:
      <ekz>
        la oficiroj de <tld/>o surbordi&gcirc;inte en diversaj
        lokoj multe man&gcirc;is kaj trinkis
        <fnt>
         <aut>L. N. Tolstoj, trad. I. Ma&ccirc;ernis</aut>,
         <vrk><url ref="http://cindymckee.com/librejo/TOLSTOJ_Kristanismo_kaj_patriotismo.pdf">
         Kristanismo kaj Patriotismo</url></vrk>,
         <lok>1931</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">haj&oacute;raj</trd>
    <trd lng="pl">eskadra</trd>
    <trd lng="pt">esquadra</trd>
  </snc>
  <snc mrk="eskadr.0o.aera">
    <uzo tip="fak">AVI</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Aro da <ref cel="aviad.milit0ilo">militaviadiloj</ref>
      partoprenantaj en iu operacio.
    </dif>
    <trd lng="de">Pulk</trd>
    <trd lng="hu">rep&uuml;l&odblac;ezred</trd>
    <trd lng="pl">eskadra</trd>
    <trd lng="pt">esquadrilha</trd>
  </snc>
  <trd lng="be">&c_e;&c_s;&c_k;&c_a;&c_d;&c_r;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>eskadra</trd>,
    <trd>odd&#xED;l lo&#x10F;stva</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Geschwader</trd>,
    <trd>Staffel [aviadiloj]</trd>
  </trdgrp>
  <trd lng="en">squadron</trd>
  <trd lng="fr">escadre</trd>
  <trdgrp lng="it">
    <trd>squadra<klr> (mil.)</klr></trd>,
    <trd>squadriglia<klr> (aer.)</klr></trd>,
    <trd>stormo<klr> (aer.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#33382;&#x968A;  <pr>&#x304B;&#12435;&#x305F;&#12356;</pr></trd>,
    <trd>&#x98DB;&#x884C;&#22823;&#x968A;  <pr>&#12402;&#12371;&#12358;&#12384;&#12356;&#x305F;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">eskader</trd>
  <trd lng="ru">&c_e;&c_s;&c_k;&c_a;&c_d;&c_r;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>oddiel <klr>( letectva, lo&#x10F;stva)</klr></trd>,
    <trd>oddiel <klr>( letectva| lo&#x10F;stva)</klr></trd>
  </trdgrp>
  <trd lng="sv">flotteskader</trd>
  <trd lng="uk">&c_je;&c_s;&#x43A;&c_a;&c_d;&c_r;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#20998;&#36963;&#x961F; <pr>f&#xE8;nqi&#x1CE;ndu&#xEC;</pr></trd>,
    <trd>&#x98DB;&#x884C;&#x4E2D;&#x968A; <pr>f&emacron;ix&#xED;ngzh&#x14D;ngdu&#xEC;</pr></trd>,
    <trd>&#x4E2D;&#x961F; <pr>zh&#x14D;ngdu&#xEC;</pr></trd>,
    <trd>&#x7A7A;&#x519B;&#x4E2D;&#x961F; <pr>k&#x14D;ngj&#x16B;nzh&#x14D;ngdu&#xEC;</pr></trd>,
    <trd>&#x9A0E;&#20853;&#x4E2D;&#x968A; <pr>q&#xED;b&#x12B;ngzh&#x14D;ngdu&#xEC;</pr></trd>,
    <trd>&#x7A7A;&#x519B;&#22823;&#x961F; <pr>k&#x14D;ngj&#x16B;nd&#xE0;du&#xEC;</pr></trd>,
    <trd>&#32852;&#x961F; <pr>li&#xE1;ndu&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>
