<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>eskadr</rad>/o
</kap>
<drv mrk="eskadr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc mrk="eskadr.0o.mara">
    <dif>
      Aro da <ref cel="sxip.milit0o">milit&scirc;ipoj</ref>
      partoprenantaj en iu operacio.  
    </dif>
    <trd lng="hu">haj&oacute;raj</trd>
    <trd lng="pl">eskadra</trd>
    <trd lng="pt">esquadra</trd>
  </snc>
  <snc mrk="eskadr.0o.aera">
    <dif>
      Aro da <ref cel="aviad.milit0ilo">militaviadiloj</ref>
      partoprenantaj en iu operacio.  
    </dif>
    <trd lng="hu">rep&uuml;l&odblac;ezred</trd>
    <trd lng="pl">eskadra</trd>
    <trd lng="pt">esquadrilha</trd>
  </snc>
  <trd lng="be">&c_e;&c_s;&c_k;&c_a;&c_d;&c_r;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Geschwader</trd>,
    <trd>Staffel [aviadiloj]</trd>
  </trdgrp>
  <trd lng="en">squadron</trd>
  <trd lng="fr">escadre</trd>
  <trdgrp lng="it">
    <trd>squadra<klr> (mil.)</klr></trd>,
    <trd>squadriglia<klr> (aer.)</klr></trd>,
    <trd>stormo<klr> (aer.)</klr></trd>
  </trdgrp>
  <trd lng="nl">eskader</trd>
  <trd lng="sv">flotteskader</trd>
  <trd lng="ru">&c_e;&c_s;&c_k;&c_a;&c_d;&c_r;&c_a;</trd>
</drv>
</art>
