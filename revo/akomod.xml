<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>akomod</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="akomod.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Al&gcirc;ustigi, adapti ion, por ke &gcirc;i povu funkcii:
      <ekz>
        la homaj
        okuloj tre rapide <tld/>i&gcirc;as al diversaj distancoj;
      </ekz>
      <ekz>
        sin <tld/>i al nova situacio.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="konfor">alkonformi&gcirc;i</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>accommoder</trd>,
    <trd>adapter</trd>
  </trdgrp>
  <trd lng="nl">aanpassen</trd>
  <trdgrp lng="pt">
    <trd>adaptar</trd>,
    <trd>ajustar</trd>,
    <trd>acomodar</trd>,
    <trd>arranjar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_s;&c_p;&c_o;&c_s;&c_o;&c_b;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_l;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_a;&c_k;&c_k;&c_o;&c_m;&c_o;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">anpassa</trd>
</drv>
</art>
