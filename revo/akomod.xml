<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>akomod</rad>/i <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="akomod.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="akomod.0i.KOMUNE">
    <dif>
      <ref tip="vid" cel="gxust.al0igi.KOMUNE">Al&gcirc;ustigi</ref>, <ref 
      tip="vid" cel="adapt.0i">adapti</ref> ion, por ke &gcirc;i povu 
      funkcii:
      <ekz>
        sin <tld/>i al nova situacio;
      </ekz>
      <ekz>
        gravas <tld/>i liajn instruojn al la nuntempa reala&jcirc;o
        <fnt>
          <aut>L. F. Pereira Garupe, trad. &Gcirc;. Vrajto</aut>,
          <vrk><url ref="http://www.soulfoodministry.org/docs/Esperanto/Esp_Practices.htm">
          La Kutimoj Jesuaj...</url></vrk>,
          <lok>2007-04</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_s;&c_t;&c_a;&c_s;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_l;&c_a;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><klr>sich </klr>einstellen</trd>,
    <trd><klr>sich </klr>anpassen</trd>,
    <trd>akkommodieren</trd>
  </trdgrp>
  <trd lng="en">accommodate</trd>
  <trdgrp lng="fr">
    <trd>accommoder</trd>,
    <trd>adapter</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>adaptar</trd>,
    <trd>acomodar</trd>
  </trdgrp>
  <trd lng="nl">aanpassen</trd>
  <trdgrp lng="pt">
    <trd>adaptar</trd>,
    <trd>ajustar</trd>,
    <trd>acomodar</trd>,
    <trd>arranjar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_s;&c_p;&c_o;&c_s;&c_o;&c_b;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_l;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_a;&c_k;&c_k;&c_o;&c_m;&c_o;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">prisp&ocirc;sobi&tcaron;</trd>
  <trd lng="sv">anpassa</trd>
</drv>

<drv mrk="akomod.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="akomod.0igxi.KOMUNE">
    <dif>
      Bone sin akordigi kun io:
      <ekz>
        la homaj okuloj tre rapide <tld/>i&gcirc;as al diversaj distancoj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konfor.0igxi.KOMUNE">konformi&gcirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="fr">s'<ind>adapter</ind></trd>
</drv>
</art>
