<kap>
  <rad>akomod</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="akomod.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="akomod.0i.KOMUNE">
    <dif>
      <ref cel="gxust.al0igi.KOMUNE">Al&gcirc;ustigi</ref>,
      <ref tip="sin" cel="adapt.0i">adapti</ref> ion, por ke &gcirc;i povu 
      funkcii:
      <ekz>
        <tld/>i sin al la nova situacio kaj esti optimisto
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="&Monato;/2013/011057.php">Nerezisteble altira
          poluso</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        gravas <tld/>i liajn instruojn al la nuntempa reala&jcirc;o
        <fnt>
          <aut>L. F. Pereira Garupe, trad. &Gcirc;. Vrajto</aut>,
          <vrk><url ref="http://www.soulfoodministry.org/docs/Esperanto/Esp_Practices.htm">
          La Kutimoj Jesuaj...</url></vrk>,
          <lok>2007-04</lok>
        </fnt>;
      </ekz>
      <ekz>
        muzeoj bezonas propran ekonomian enspezfonton por <tld/>i sin al la &ccirc;instila
        merkata ekonomio
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="&Monato;/2005/008682.php">Senmonaj muzeoj</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        la a&ubreve;toroj de la <klr>[&#x2026;]</klr> slavona alfabeto Cirilo kaj Metodo <tld/>is la
        grekan alfabeton
        <fnt>
          <bib>Monato</bib>
          <aut>Mihail Korotkov</aut>
          <vrk><url ref="&Monato;/2013/010760.php">Plej granda invento de la
          homaro</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x5165;&#x4E61;&#x968F;&#x4FD7; <pr>r&#xF9;xi&#x101;ngsu&#xED;s&#xFA;</pr></trd>,
    <trd>&#x5165;&#x9109;&#x96A8;&#x4FD7; <pr>r&#xF9;xi&#x101;ngsu&#xED;s&#xFA;</pr></trd>,
    <trd>&#x56E0;&#x5730;&#x5236;&#x5B9C; <pr>y&#x12B;nd&#xEC;zh&#xEC;y&#xED;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_s;&c_t;&c_a;&c_s;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_l;&c_a;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>acomodar</trd>,
    <trd>adaptar</trd>,
    <trd>adequar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nastavit</trd>,
    <trd>se&rcaron;&iacute;dit</trd>,
    <trd>upravit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>anpassen</trd>,
    <trd>akkommodieren</trd>,
  </trdgrp>
  <trdgrp lng="en">
    <trd>accommodate</trd>,
    <trd>adapt</trd>,
    <trd>adjust</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>adaptar</trd>,
    <trd>acomodar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>accommoder</trd>,
    <trd>adapter</trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5D4;&#x5EA;&#x5D0;&#x5D9;&#x5DD;</trd>
  <trdgrp lng="ja">
    <trd>&#x9069;&#x5FDC;&#x3055;&#x305B;&#x308B;  <pr>&#x3066;&#x304D;&#x304A;&#x3046;&#x3055;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x8ABF;&#x7BC0;&#x3059;&#x308B;  <pr>&#x3061;&#x3087;&#x3046;&#x305B;&#x3064;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">aanpassen</trd>
  <trdgrp lng="pt">
    <trd>adaptar</trd>,
    <trd>ajustar</trd>,
    <trd>acomodar</trd>,
    <trd>arranjar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_s;&c_p;&c_o;&c_s;&c_o;&c_b;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_l;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_a;&c_k;&c_k;&c_o;&c_m;&c_o;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>akomodova&tcaron;</trd>,
    <trd>prisp&ocirc;sobi&tcaron;</trd>,
    <trd>prisp&ocirc;sobi&tcaron; sa</trd>
  </trdgrp>
  <trd lng="sv">anpassa</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&c_i;&c_s;&c_t;&#x43E;&c_s;&#x43E;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_r;&c_i;&#x43B;&c_a;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="akomod.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        plej ofte lingva homogeneco esti&gcirc;as per <tld/>o kaj adapti&gcirc;o al la lingvo de la
        plej forta
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2013/010677.php">Lingva konfuzo kaj
          Konfuceo</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_s;&c_t;&c_a;&c_s;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="de">Anpassung</trd>
  <trd lng="he">&#x5D4;&#x5EA;&#x5D0;&#x5DE;&#x5D4;</trd>
</drv>

<drv mrk="akomod.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="akomod.0igxi.KOMUNE">
    <dif>
      Bone sin akordigi kun io:
      <ekz>
        rapide <tld/>i&gcirc;i al novaj situacioj
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="&Monato;/2009/009886.php">Ekzameno ekzamenata</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la indi&gcirc;enoj bobelas tensioj inter tiuj, kiuj volas <tld/>i&gcirc;i al la limoj kaj
        ebloj de submeti&gcirc;o, kaj tiuj, kiuj konstatas la minacon al ilia kulturo kaj
        vivado kaj strebas <klr>[&#x2026;]</klr> iel rezisti la blankulojn
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan MacGill</aut>
          <vrk><url ref="&Monato;/2012/010164.php">Panorama soro super
          kontinento</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="konfor.0igxi.KOMUNE">konformi&gcirc;i</ref>,
      <ref cel="adapt.0igxi">adapti&gcirc;i</ref>,
      <ref cel="akord.0igxi">akordi&gcirc;i</ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#x5165;&#x5883;&#x554F;&#x4FD7; <pr>r&#xF9;j&#xEC;ngw&#xE8;ns&#xFA;</pr></trd>,
    <trd>&#x5165;&#x5883;&#x95EE;&#x4FD7; <pr>r&#xF9;j&#xEC;ngw&#xE8;ns&#xFA;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_s;&c_t;&c_a;&c_s;&c_o;&c_w;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>acomodar-se</trd>,
    <trd>adaptar-se</trd>,
    <trd>emmotllar-se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><klr>sich </klr>einstellen</trd>,
    <trd><klr>sich </klr>anpassen</trd>
  </trdgrp>
  <trd lng="fr">s'<ind>adapter</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x9069;&#x5FDC;&#x3059;&#x308B;  <pr>&#x3066;&#x304D;&#x304A;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x9806;&#x5FDC;&#x3059;&#x308B;  <pr>&#x3058;&#x3085;&#x3093;&#x306E;&#x3046;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="akomod.0igxema">
  <kap><tld/>i&gcirc;ema</kap>
  <snc mrk="akomod.0igxema.PSI">
    <dif>
      Kiu facile a&ubreve; volonte <tld/>i&gcirc;as al
      homoj, al kondi&ccirc;oj:
      <ekz>
        instruitulo <nom>Zhang</nom> restis &ccirc;iam kvieta,
        <tld/>i&gcirc;ema kaj neniam malbonkonduta
        <fnt>
          <aut>Yuan Zhen, trad. Guozhu</aut>,
          <vrk><url ref="www.elerno.cn/elibro/cuiying.pdf">
          La Amo de Fra&ubreve;lino Cui YingYing</url></vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
       <ref cel="fleks.0ebla">fleksebla</ref>,
       <ref cel="elast.0a.FIG">elasta<sncref/></ref>,
       <ref cel="obe.0ema">obeema</ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#x5706;&#x901A; <pr>yu&#xE1;nt&#x14D;ng</pr></trd>,
    <trd>&#x5713;&#x901A; <pr>yu&#xE1;nt&#x14D;ng</pr></trd>,
    <trd>&#x5584;&#x53D8; <pr>sh&#xE0;nbi&#xE0;n</pr></trd>,
    <trd>&#x5584;&#x8B8A; <pr>sh&#xE0;nbi&#xE0;n</pr></trd>,
    <trd>&#x5BB9;&#x6613;&#x6559; <pr>r&#xF3;ngy&#xEC;ji&#xE0;o</pr></trd>,
    <trd>&#x62FF;&#x5F97;&#x8D77;&#x653E;&#x5F97;&#x4E0B; <pr>n&#xE1;deq&#x1D0;f&#xE0;ngdexi&#xE0;</pr></trd>,
    <trd>&#x53EF;&#x5F2F;&#x66F2; <pr>k&#x11B;w&#x101;nq&#x1D4;</pr></trd>,
    <trd>&#x53EF;&#x5F4E;&#x66F2; <pr>k&#x11B;w&#x101;nq&#x1D4;</pr></trd>,
    <trd>&#x53EF;&#x53D8; <pr>k&#x11B;bi&#xE0;n</pr></trd>,
    <trd>&#x53EF;&#x8B8A; <pr>k&#x11B;bi&#xE0;n</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_s;&c_t;&c_a;&c_s;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_c;&c_k;&c_ib;</trd>,
    <trd>&c_g;&c_n;&c_u;&c_t;&c_k;&c_ib; <klr tip="amb">(&c_u; &c_p;&c_a;&c_v;&c_o;&c_d;&c_z;&c_ib;&c_n;&c_a;&c_h;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>f&uuml;gsam</trd>,
    <trd>flexibel</trd>,
    <trd>anpassungsf&auml;hig</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>accomodant</trd>,
    <trd>souple<klr> (caract&egrave;re)</klr></trd>
  </trdgrp>
</drv>
