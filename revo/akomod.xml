<kap>
  <rad>akomod</rad>/i <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="akomod.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="akomod.0i.KOMUNE">
    <dif>
      <ref cel="gxust.al0igi.KOMUNE">Al&gcirc;ustigi</ref>,
      <ref tip="sin" cel="adapt.0i">adapti</ref> ion, por ke &gcirc;i povu
      funkcii:
      <ekz>
        <tld/>i sin al la nova situacio kaj esti optimisto
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="&Monato;/2013/011057.php">Nerezisteble altira
          poluso</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        gravas <tld/>i liajn instruojn al la nuntempa reala&jcirc;o
        <fnt>
          <aut>L. F. Pereira Garupe, trad. &Gcirc;. Vrajto</aut>,
          <vrk><url ref="http://www.soulfoodministry.org/docs/Esperanto/Esp_Practices.htm">
          La Kutimoj Jesuaj...</url></vrk>,
          <lok>2007-04</lok>
        </fnt>;
      </ekz>
      <ekz>
        muzeoj bezonas propran ekonomian enspezfonton por <tld/>i sin al la &ccirc;instila
        merkata ekonomio
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="&Monato;/2005/008682.php">Senmonaj muzeoj</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        la a&ubreve;toroj de la <klr>[&#8230;]</klr> slavona alfabeto Cirilo kaj Metodo <tld/>is la
        grekan alfabeton
        <fnt>
          <bib>Monato</bib>
          <aut>Mihail Korotkov</aut>
          <vrk><url ref="&Monato;/2013/010760.php">Plej granda invento de la
          homaro</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#20837;&#20065;&#38543;&#20439; <pr>r&#249;xi&amacron;ngsu&#237;s&#250;</pr></trd>,
    <trd>&#20837;&#37129;&#38568;&#20439; <pr>r&#249;xi&amacron;ngsu&#237;s&#250;</pr></trd>,
    <trd>&#22240;&#22320;&#21046;&#23452; <pr>y&#299;nd&#236;zh&#236;y&#237;</pr></trd>
  </trdgrp>
</snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>al&#285;ustigi</ke>, <ke>adapti</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_s;&c_t;&c_a;&c_s;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_l;&c_a;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>acomodar</trd>,
    <trd>adaptar</trd>,
    <trd>adequar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nastavit</trd>,
    <trd>se&rcaron;&iacute;dit</trd>,
    <trd>upravit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>anpassen</trd>,
    <trd>akkommodieren</trd>,
    <trd><ind>zuschneiden</ind> auf</trd>,
    <trd>anbequemen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>accommodate</trd>,
    <trd>adapt</trd>,
    <trd>adjust</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>adaptar</trd>,
    <trd>acomodar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>accommoder</trd>,
    <trd>adapter</trd>
  </trdgrp>
  <trd lng="he">&#1500;&#1492;&#1514;&#1488;&#1497;&#1501;</trd>
  <trdgrp lng="ja">
    <trd>&#36969;&#24540;&#12373;&#12379;&#12427;  <pr>&#12390;&#12365;&#12362;&#12358;&#12373;&#12379;&#12427;</pr></trd>,
    <trd>&#35519;&#31680;&#12377;&#12427;  <pr>&#12385;&#12423;&#12358;&#12379;&#12388;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">aanpassen</trd>
  <trdgrp lng="pl">
     <trd>akomodowa&cacute;</trd>,
     <trd>dostosowywa&cacute;</trd>,
     <trd>adaptowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>adaptar</trd>,
    <trd>ajustar</trd>,
    <trd>acomodar</trd>,
    <trd>arranjar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_s;&c_p;&c_o;&c_s;&c_o;&c_b;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_l;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_a;&c_k;&c_k;&c_o;&c_m;&c_o;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>akomodova&tcaron;</trd>,
    <trd>prisp&ocirc;sobi&tcaron;</trd>,
    <trd>prisp&ocirc;sobi&tcaron; sa</trd>
  </trdgrp>
  <trd lng="sv">anpassa</trd>
  <trdgrp lng="uk">
    <trd>&#1087;&c_r;&c_i;&c_s;&c_t;&#1086;&c_s;&#1086;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#1087;&c_r;&c_i;&#1083;&c_a;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="akomod.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        plej ofte lingva homogeneco esti&gcirc;as per <tld/>o kaj adapti&gcirc;o al la lingvo de la
        plej forta
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2013/010677.php">Lingva konfuzo kaj
          Konfuceo</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_s;&c_t;&c_a;&c_s;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="de">
    <trd>Anpassung</trd>,
    <trd>Zuschnitt <klr>(Anpassung)</klr></trd>,
    <trd>Akkomodation</trd>
  </trdgrp>
  <trd lng="he">&#1492;&#1514;&#1488;&#1502;&#1492;</trd>
  <trdgrp lng="pl">
     <trd>akomodacja</trd>,
     <trd>dostosowanie</trd>,
     <trd>adaptacja</trd>,
     <trd>przystosowanie</trd>
  </trdgrp>
</drv>

<drv mrk="akomod.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="akomod.0igxi.KOMUNE">
    <dif>
      Bone sin akordigi kun io:
      <ekz>
        rapide <tld/>i&gcirc;i al novaj situacioj
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="&Monato;/2009/009886.php">Ekzameno ekzamenata</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la indi&gcirc;enoj bobelas tensioj inter tiuj, kiuj volas <tld/>i&gcirc;i al la limoj kaj
        ebloj de submeti&gcirc;o, kaj tiuj, kiuj konstatas la minacon al ilia kulturo kaj
        vivado kaj strebas <klr>[&#8230;]</klr> iel rezisti la blankulojn
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan MacGill</aut>
          <vrk><url ref="&Monato;/2012/010164.php">Panorama soro super
          kontinento</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="konfor.0igxi.KOMUNE">konformi&gcirc;i</ref>,
      <ref cel="adapt.0igxi">adapti&gcirc;i</ref>,
      <ref cel="akord.0igxi">akordi&gcirc;i</ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#20837;&#22659;&#21839;&#20439; <pr>r&#249;j&#236;ngw&#232;ns&#250;</pr></trd>,
    <trd>&#20837;&#22659;&#38382;&#20439; <pr>r&#249;j&#236;ngw&#232;ns&#250;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_s;&c_t;&c_a;&c_s;&c_o;&c_w;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>acomodar-se</trd>,
    <trd>adaptar-se</trd>,
    <trd>emmotllar-se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><klr>sich </klr>einstellen</trd>,
    <trd><klr>sich </klr>anpassen</trd>
  </trdgrp>
  <trd lng="fr">s'<ind>adapter</ind></trd>
  <trdgrp lng="ja">
    <trd>&#36969;&#24540;&#12377;&#12427;  <pr>&#12390;&#12365;&#12362;&#12358;&#12377;&#12427;</pr></trd>,
    <trd>&#38918;&#24540;&#12377;&#12427;  <pr>&#12376;&#12421;&#12435;&#12398;&#12358;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>upodabnia&cacute; si&eogonek;</trd>,
    <trd>akomodowa&cacute; si&eogonek;</trd>
  </trdgrp>
</drv>

<drv mrk="akomod.0igxema">
  <kap><tld/>i&gcirc;ema</kap>
  <snc mrk="akomod.0igxema.PSI">
    <dif>
      Kiu facile a&ubreve; volonte <tld/>i&gcirc;as al
      homoj, al kondi&ccirc;oj:
      <ekz>
        instruitulo <nom>Zhang</nom> restis &ccirc;iam kvieta,
        <tld/>i&gcirc;ema kaj neniam malbonkonduta
        <fnt>
          <aut>Yuan Zhen, trad. Guozhu</aut>,
          <vrk><url ref="www.elerno.cn/elibro/cuiying.pdf">
          La Amo de Fra&ubreve;lino Cui YingYing</url></vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
       <ref cel="fleks.0ebla">fleksebla</ref>,
       <ref cel="elast.0a.FIG">elasta<sncref/></ref>,
       <ref cel="obe.0ema">obeema</ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#22278;&#36890; <pr>yu&#225;nt&#333;ng</pr></trd>,
    <trd>&#22291;&#36890; <pr>yu&#225;nt&#333;ng</pr></trd>,
    <trd>&#21892;&#21464; <pr>sh&#224;nbi&#224;n</pr></trd>,
    <trd>&#21892;&#35722; <pr>sh&#224;nbi&#224;n</pr></trd>,
    <trd>&#23481;&#26131;&#25945; <pr>r&#243;ngy&#236;ji&#224;o</pr></trd>,
    <trd>&#25343;&#24471;&#36215;&#25918;&#24471;&#19979;
    <pr>n&#225;deq&#464;f&#224;ngdexi&#224;</pr></trd>,
    <trd>&#21487;&#24367;&#26354; <pr>k&#283;w&amacron;nq&#468;</pr></trd>,
    <trd>&#21487;&#24398;&#26354; <pr>k&#283;w&amacron;nq&#468;</pr></trd>,
    <trd>&#21487;&#21464; <pr>k&#283;bi&#224;n</pr></trd>,
    <trd>&#21487;&#35722; <pr>k&#283;bi&#224;n</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_s;&c_t;&c_a;&c_s;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_c;&c_k;&c_ib;</trd>,
    <trd>&c_g;&c_n;&c_u;&c_t;&c_k;&c_ib; <klr tip="amb">(&c_u; &c_p;&c_a;&c_v;&c_o;&c_d;&c_z;&c_ib;&c_n;&c_a;&c_h;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>f&uuml;gsam</trd>,
    <trd>flexibel</trd>,
    <trd>anpassungsf&auml;hig</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>accomodant</trd>,
    <trd>souple<klr> (caract&egrave;re)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przystosowuj&aogonek;cy si&eogonek;</trd>,
    <trd>akomoduj&aogonek;cy si&eogonek;</trd>
  </trdgrp>
</drv>
