<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tercet.xml,v 1.22 2022/03/28 12:03:04 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>tercet</rad>/o
</kap>
<drv mrk="tercet.0o">
  <kap><tld/>o</kap>
  <snc mrk="tercet.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzika&jcirc;o por 3 vo&ccirc;oj a&ubreve; vo&ccirc;oj:
      <ekz>
        &leftquot;Estu nun indulga&rightquot; &dash; <tld lit="T"/>o kaj &hcirc;oro 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=275218">Die Jahreszeiten
          (Haydn)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="tri.0o.ensemblo">trio</ref>
    <trd lng="be">&c_t;&c_e;&c_r;&c_c;&c_e;&c_t;</trd>
    <trdgrp lng="ca">
      <trd>trio</trd>,
      <trd>tercet</trd>
    </trdgrp>
    <trd lng="de">Terzett</trd>
    <trd lng="en">trio</trd>
    <trd lng="fr">trio<klr> (pi&egrave;ce de musique)</klr></trd>
    <trd lng="hu">tercett</trd>
    <trd lng="nl">terzet</trd>
    <trd lng="pl">tercet</trd>
    <trdgrp lng="pt">
      <trd>terceto</trd>
    </trdgrp>
    <trd lng="ru">&c_t;&c_je;&c_r;&c_c;&c_je;&c_t;</trd>
    <trd lng="sv">terzett</trd>
  </snc>
  <snc mrk="tercet.0o.POE">
    <uzo tip="fak">POE</uzo>
    <dif>
      Triversa <ref tip="super" cel="strof.0o">strofo</ref>:
      <ekz>
        soneto (Ar&ccirc;oj, p. 100. La Laguna: Stafeto, 1967),
        kies du <tld/>oj estas citataj de <nom>William Auld</nom> en unu el liaj libroj 
        <fnt>
          <bib>Monato</bib>
          <aut>Geraldo Mattos</aut>
          <vrk><url ref="&Monato;/2008/009422.php">Rezulto de preseraro</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>tercet</trd>,
    <trd>terceto</trd>
  </trdgrp>
  <trd lng="sk">terceto</trd>
  <trdgrp lng="uk">
    <trd>&#x442;&#x435;&#x440;&#x446;&#x435;&#x442;</trd>,
    <trd>&#x442;&#x440;&#x456;&#x43E;</trd>
  </trdgrp>
</drv>
</art>
