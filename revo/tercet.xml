<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tercet.xml,v 1.28 2024/10/31 10:34:31 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>tercet</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="tercet.0o">
  <kap><tld/>o</kap>
  <snc mrk="tercet.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="muzik.0ajxo">Muzika&jcirc;o</ref> por tri vo&ccirc;oj a&ubreve; instrumentoj:
      <ekz>
        &leftquot;Estu nun indulga&rightquot; &dash; <tld lit="T"/>o kaj &hcirc;oro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=275218">Die Jahreszeiten
          (Haydn)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_e;&c_r;&c_c;&c_e;&c_t; <klr>(&c_m;&c_u;&c_z;.)</klr></trd>
    <trdgrp lng="ca">
      <trd>trio</trd>,
      <trd>tercet</trd>
    </trdgrp>
    <trd lng="de">Terzett</trd>
    <trd lng="en">trio</trd>
    <trd lng="fr">trio<klr> (pi&egrave;ce de musique)</klr></trd>
    <trd lng="hu">tercett</trd>
    <trd lng="nl">terzet</trd>
    <trd lng="pl">tercet</trd>
    <trdgrp lng="pt">
      <trd>terceto</trd>
    </trdgrp>
    <trd lng="ru">&c_t;&c_je;&c_r;&c_c;&c_je;&c_t;</trd>
    <trd lng="sv">terzett</trd>
  </snc>
  <snc mrk="tercet.0o.trio">
    <dif>
      <ref tip="super" cel="tri.0o.ensemblo">Trio</ref> muzikanta:
      <ekz>
        koncerto de tri instrumentoj kaj du vo&ccirc;oj (la <tld/>o &leftquot;<nom>La Rocha</nom>&rightquot;), kiuj ne nur
        prezentis klasikan muzikon, sed anka&ubreve; tiun de diversaj filmoj kaj kantistoj
        <fnt>
          <aut>Roland Schnell</aut>
          <vrk><url ref="https://esperanto.berlin/printempo-szczecin-2016/"
              >Jubilea Esperanto-printempo en Szczecin</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_r;&c_y;&c_jo;</trd>
    <trd lng="de">Trio</trd>
  </snc>
  <snc mrk="tercet.0o.POE">
    <uzo tip="fak">POE</uzo>
    <dif>
      Triversa <ref tip="super" cel="strof.0o">strofo</ref>:
      <ekz>
        soneto (Ar&ccirc;oj, p. 100. La Laguna: Stafeto, 1967),
        kies du <tld/>oj estas citataj de <nom>William Auld</nom> en unu el liaj libroj
        <fnt>
          <bib>Monato</bib>
          <aut>Geraldo Mattos</aut>
          <vrk><url ref="&Monato;/2008/009422.php">Rezulto de preseraro</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_e;&c_r;&c_c;&c_e;&c_t; <klr>(&c_l;&c_ib;&c_t;.)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>tercet</trd>,
    <trd>terceto</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#19977;&#34892;&#36899;&#21477;  <pr>&#12373;&#12435;&#12366;&#12423;&#12358;&#12428;&#12435;&#12367;</pr></trd>,
    <trd>&#19977;&#34892;&#35433;  <pr>&#12415;&#12387;&#12371;&#12358;&#12375;</pr></trd>,
    <trd>&#19977;&#37096;&#21512;&#21809;&#26354;  <pr>&#12415;&#12387;&#12406;&#12364;&#12387;&#12375;&#12423;&#12358;&#12365;&#12423;&#12367;</pr></trd>,
    <trd>&#19977;&#37325;&#22863;&#26354;  <pr>&#12415;&#12387;&#12376;&#12421;&#12358;&#12381;&#12358;&#12365;&#12423;&#12367;</pr></trd>
  </trdgrp>
  <trd lng="sk">terceto</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_je;&c_r;&c_c;&c_je;&c_t;</trd>,
    <trd>&c_t;&c_r;&c_ib;&c_o;</trd>
  </trdgrp>
</drv>
</art>
