<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: domagx.xml,v 1.30 2023/11/05 21:21:46 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>doma&gcirc;</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="domagx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="domagx.0o.bedauro">
    <dif>
      Sento de beda&ubreve;ro pro perdo a&ubreve; difekto; beda&ubreve;rinda&jcirc;o:
      <ekz>
        kia <tld/>o perdi tutan jaron
        <fnt>
          <bib>IKr</bib>
          <lok>Neniam Plu!</lok>
        </fnt>!
      </ekz>
      <ekz>
        sen <tld/>o iu vidis, ke havas li botojn kun truoj
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i nun e&ccirc; ne permesas al mi lerni la muzikon &ndash; tio estas granda <tld/>o
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>kia <tld/>o</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>!
        <trd lng="de">wie <ind>bedauerlich</ind></trd>
        <trd lng="fr">quel dommage !</trd>
      </ekz>
      <ekz>
        <ind>post <tld/>o venas sa&gcirc;o</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="de"><ind>Schaden</ind> macht klug</trd>
      </ekz>
    </dif>
    <trd lng="de"><ind>bedauerlich</ind>e Sache</trd>
    <trdgrp lng="en">
      <trd>a pity</trd>,
      <trd>something regrettable</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd><ofc>*</ofc>szkoda</trd>,
      <trd>przykro&sacute;&cacute;</trd>,
      <trd>&zdot;al</trd>,
      <trd>niefart<klr> (pot.)</klr></trd>,
      <trd>kaszana<klr> (pot.)</klr></trd>,
      <trd>kicha<klr> (pot.)</klr></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>coisa <ind>inconveniente</ind></trd>,
      <trd>coisa <ind>lament&aacute;vel</ind></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_zh;&c_a;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_o;&c_zh;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc mrk="domagx.0o.damagxo">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="damagx.0o">Dama&gcirc;o</ref>:
      <ekz>
        alveni kun helpo je propra kosto kaj <klr>[&#8230;]</klr> trudi justan riparon de la
        <tld/>oj
        <fnt>
          <bib>FSp</bib>
          <lok>Dua Parto: Svisa Sperto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la itala registaro ne perdis presti&gcirc;on en la solvo pri <nom>Korfu</nom>, &gcirc;i ricevis
        <tld/>pagon kaj forlasis la grekan insulon sen humili&gcirc;o
        <fnt>
          <bib>FSp</bib>
          <lok>Tria Parto: Ligo De Nacioj</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="bon">malbona&jcirc;o</ref>,
      <ref cel="perd">perdo</ref>,
      <ref cel="averi.0o">averio</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_a;&c_l;&c_mol;</trd>,
    <trd>&c_sh;&c_k;&c_a;&c_d;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>calamidad</trd>,
    <trd>tragedia</trd>,
    <trd>problema</trd>,
    <trd>drama</trd>,
    <trd>perdida</trd>,
    <trd>desgracia</trd>,
    <trd>desdicha</trd>
  </trdgrp>
  <trd lng="fr"><ofc>*</ofc>dommage</trd>
  <trdgrp lng="ja">
    <trd>&#24796;&#12375;&#12356;&#12371;&#12392;  <pr>&#12362;&#12375;&#12356;&#12371;&#12392;</pr></trd>,
    <trd>&#27531;&#24565;&#12394;&#12371;&#12392;  <pr>&#12374;&#12435;&#12397;&#12435;&#12394;&#12371;&#12392;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_zh;&c_a;&c_l;&c_mol;</trd>,
    <trd>&c_d;&c_o;&c_s;&c_a;&c_d;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="domagx.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Beda&ubreve;riga:
      <ekz>
        Li estas nun freneza,&mdash;estas vero, &parsl; Kaj vere tio estas tre <tld/>a
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a afero estas, ke mia dokumento malaperis
        <fnt>
          <bib>ChV</bib>
          <lok>11</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi iel plu kredas je la eblo de harmonia kaj viv-<tld/>a rilato inter bestoj kaj
        homoj
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="bedaux.0inda">beda&ubreve;rinda</ref>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_r;&c_t;&c_y; &c_zh;&c_a;&c_l;&c_ju;</trd>
  <trd lng="bo">&t_da;&t_ga;&t_naro;&t_nga;&t_sa;&t_tsheg;&t_da;&t_ga;&t_tsheg;</trd>
  <trdgrp lng="de">
    <trd>bedauerlich</trd>,
    <trd>schade</trd>
  </trdgrp>
  <trd lng="en">regrettable</trd>
  <trdgrp lng="es">
    <trd>tr&aacute;gico</trd>,
    <trd>lamentable</trd>,
    <trd>triste</trd>,
    <trd>dram&aacute;tico</trd>,
    <trd>desastroso</trd>
  </trdgrp>
  <trd lng="fr">regrettable</trd>
  <trdgrp lng="ja">
    <trd>&#24796;&#12375;&#12416;&#12409;&#12365;  <pr>&#12362;&#12375;&#12416;&#12409;&#12365;</pr></trd>,
    <trd>&#36986;&#25022;&#12394;  <pr>&#12356;&#12363;&#12435;&#12394;</pr></trd>,
    <trd>&#27531;&#24565;&#12394;  <pr>&#12374;&#12435;&#12397;&#12435;&#12394;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>spijtig</trd>,
    <trd>jammer</trd>,
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przykry</trd>,
    <trd>&zdot;a&lstroke;osny</trd>,
    <trd>nieprzyjemny</trd>,
    <trd>niefajny<klr> (pot.)</klr></trd>
  </trdgrp>
  <trd lng="zh">&#20196;&#20154;&#24779;&#24796; <pr>l&igrave;ngr&eacute;nw&#462;nx&#299;</pr></trd>
</drv>

<drv mrk="domagx.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Beda&ubreve;rige:
      <ekz>
        silentu! diris al li la edzino, multaj kura&gcirc;uloj jam perdis tie la vivon,
        <tld/>e estus, se &ccirc;i tiuj belaj okuloj ne plu vidus la tagan lumon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fabelo pri iu, kiu migris por ekkoni timon</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estas senviva, <tld/>e, tiel bela viro! diris la re&gcirc;o
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fabelo pri iu, kiu migris por ekkoni timon</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u oni povas tiel konstante faradi facilanima&jcirc;ojn <klr>[&#8230;]</klr>? estas efektive
        <tld/>e pri via bona koro
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi posedas nedubeblan talenton, estas <tld/>e, ke vi ne lernis pli
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="bedaux.0inde">beda&ubreve;rinde</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_k;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_zh;&c_a;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_zh;&c_a;&c_l;&c_k;&c_o;</trd>
  <trdgrp lng="de">
    <trd>bedauerlich</trd>,
    <trd>schade</trd>
  </trdgrp>
  <trd lng="en">regrettably</trd>
  <trdgrp lng="es">
    <trd>lamentablemente</trd>,
    <trd>desgraciadamente</trd>,
    <trd>desafortunadamente</trd>
  </trdgrp>
  <trd lng="he">&het;&bet;&lamed;</trd>
  <trd lng="ja">&#27531;&#24565;&#12395;&#12418;  <pr>&#12374;&#12435;&#12397;&#12435;&#12395;&#12418;</pr></trd>
  <trd lng="nl">jammer genoeg</trd>
  <trdgrp lng="pl">
    <trd>przykro</trd>,
    <trd>&zdot;a&lstroke;osnnie</trd>,
    <trd>nieprzyjemnie</trd>,
    <trd>niefajnie<klr> (pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_a;&c_l;&c_mol;</trd>,
    <trd>&c_zh;&c_a;&c_l;&c_k;&c_o;</trd>,
    <trd>&c_d;&c_o;&c_s;&c_a;&c_d;&c_n;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd> &c_sh;&c_k;&c_o;&c_d;&c_a;</trd>,
    <trd>&c_zh;&c_a;&c_l;&c_mol;</trd>,
    <trd>&c_zh;&c_a;&c_l;&c_k;&c_o;</trd>,
    <trd>&c_d;&c_o;&c_s;&c_a;&c_d;&c_n;&c_o;</trd>
  </trdgrp>
  <trd lng="zh">&#21487;&#24796; <pr>k&ecaron;x&#299;</pr></trd>
</drv>

<drv mrk="domagx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="domagx.0i.avari">
    <dif>
      Ne voli elspezi, ne voli perdi, ne voli difekti:
      <ekz>
        li ne a&ccirc;etis la libron, &ccirc;ar li <tld/>is la monon
        <fnt>
          <bib>LR</bib>
          <lok>Unua Parto &ndash; Leksikologio</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i tute ne <tld/>is la monon, &mdash; kontra&ubreve;e, &scirc;i estis e&ccirc; &gcirc;oja, ke &scirc;i povas helpi
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        venis gasto al la ri&ccirc;a homo, kaj &ccirc;i tiu doma&gcirc;is preni el siaj &scirc;afoj
        <klr>[&#8230;]</klr>, por prepari ion por la gasto
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 12:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        pafu sur &gcirc;in, ne <tld/>u sagojn
        <fnt><bib>MT</bib>, <lok>&Jer; 50:14</lok></fnt>;
      </ekz>
      <ekz>
        kiu <tld/>as gro&scirc;on perdas la tutan
        po&scirc;on
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="spez.el0i">elspezi</ref>,
      <ref cel="ofer.0i.kontribui">oferi</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="avar.0i">avari</ref>,
      <ref cel="konser.0i.pluhavi">konservi</ref>,
      <ref cel="sxpar.0i.moderi">&scirc;pari</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>zu <ind>schade</ind> sein</trd>,
      <trd>schonen</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>preocuparse <klr>por</klr></trd>,
      <trd>temer <klr>por</klr></trd>,
      <trd>llevar cuidado con</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>be anxious about</trd>,
      <trd>be concerned for</trd>,
      <trd>fear for</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>m&eacute;nager</trd>,
      <trd>&eacute;conomiser</trd>,
      <trd>&eacute;pargner</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>&zdot;a&lstroke;owa&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_o;&c_k;&c_r;&c_u;&c_sh;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_zh;&c_a;&c_l;&c_je;&c_t;&c_mol; <klr>(&c_o;
      &c_ch;&c_jo;&c_m;-&c_l;.)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="domagx.0i.kompati">
    <dif>
      Timi malutili, timi ofendi a&ubreve; dolorigi, voli indulgi:
      <ekz>
        la abelo diris: mi <tld/>as vin, urso
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tablo, la orazeno kaj bastono el sako</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi <tld/>as la hederon, pri kiu vi ne laboris
        <klr>[&#8230;]</klr> kaj &ccirc;u mi povus ne <tld/>i
        <nom>Nineve</nom>n, la grandan urbon, en kiu
        trovi&gcirc;as pli ol cent dudek mil homoj
        ne povantaj distingi inter sia dekstra mano kaj sia
        maldekstra
        <fnt><bib>MT</bib>, <lok>&Jon; 4:10-11.</lok></fnt>;
      </ekz>
      <ekz>
        pro vorta ludo li e&ccirc; patron ne <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kompat.0i">kompati</ref>,
      <ref cel="indulg.0i">indulgi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>dauern</trd>,
      <trd><klr>(jemanden)</klr> bedauern</trd>,
      <trd><ind>R&uuml;cksicht</ind> nehmen</trd>
    </trdgrp>
    <trd lng="es">ser cuidadoso con</trd>
    <trd lng="fr">m&eacute;nager</trd>
    <trdgrp lng="pl">
      <trd>&zdot;a&lstroke;owa&cacute;</trd>,
      <trd>oszcz&eogonek;dza&cacute;<klr> (kogo&sacute;,
      czego&sacute;)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_b;&c_je;&c_r;&c_je;&c_ch;&c_mol;</trd>,
      <trd>&c_shch;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="domagx.0i.damagxi">
    <uzo tip="stl">EVI</uzo>
    <dif>
      Difekti, <ref tip="dif" cel="damagx.0i">dama&gcirc;i</ref>.
    </dif>
    <trd lng="es"><klr>(evitar usar en el sentido de
    da&ntilde;ar)</klr></trd>
  </snc>
  <trd lng="be">&c_sh;&c_k;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="he">&lamed;&he;&tsadi;&tet;&ayin;&resh;</trd>
  <trdgrp lng="ja">
    <trd>&#24796;&#12375;&#12416;  <pr>&#12362;&#12375;&#12416;</pr></trd>,
    <trd>&#22823;&#20107;&#12395;&#12377;&#12427;  <pr>&#12384;&#12356;&#12376;&#12395;&#12377;&#12427;</pr></trd>,
    <trd>&#12356;&#12383;&#12431;&#12427;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_sh;&c_k;&c_o;&c_d;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_d;&c_b;&c_a;&c_j;&c_l;&c_i;&c_v;&c_o; &c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_b;&c_je;&c_r;&c_je;&c_g;&c_t;&c_i;</trd>,
    <trd>&c_shch;&c_a;&c_d;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_zh;&c_u;&c_r;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#30171;&#24796; <pr>t&ograve;ngx&iacute;</pr></trd>,
    <trd>&#24833; <pr>ch&oacute;u</pr></trd>
  </trdgrp>
</drv>
