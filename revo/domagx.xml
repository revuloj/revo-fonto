<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: domagx.xml,v 1.25 2021/05/29 14:32:19 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>doma&gcirc;</rad>/o
</kap>
<drv mrk="domagx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Beda&ubreve;rinda&jcirc;o:
      <ekz>
        kia <tld/>o!<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        estas <tld/>o, ke vi perdis vian libron.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>a pity</trd>,
      <trd>something regrettable</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>szkoda</trd>,
      <trd>przykro&sacute;&cacute;</trd>,
      <trd>&zdot;al</trd>,
      <trd>niefart<klr> (pot.)</klr></trd>,
      <trd>kaszana<klr> (pot.)</klr></trd>,
      <trd>kicha<klr> (pot.)</klr></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>coisa <ind>inconveniente</ind></trd>,
      <trd>coisa <ind>lament&aacute;vel</ind></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_zh;&c_a;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_o;&c_zh;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">EVI</uzo>
    <dif>
      Malprofito, difekto:
      <ekz>
        la hajlo ka&ubreve;zis grandajn <tld/>ojn;
      </ekz>
      <ekz>
        ripari la <tld/>ojn ka&ubreve;zitajn de la milito;
      </ekz>
      <ekz>
        post <tld/>o venas
        sa&gcirc;o<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        domo brul<tld/>igita.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="bon">malbona&jcirc;o</ref>,
      <ref cel="perd">perdo</ref>,
      <ref cel="averi.0o">averio</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_a;&c_l;&c_mol;</trd>,
    <trd>&c_sh;&c_k;&c_a;&c_d;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>calamidad</trd>,
    <trd>tragedia</trd>,
    <trd>problema</trd>,
    <trd>drama</trd>,
    <trd>perdida</trd>,
    <trd>desgracia</trd>,
    <trd>desdicha</trd>
  </trdgrp>
  <trd lng="de">Schaden</trd>
  <trd lng="fr">dommage</trd>
</drv>

<drv mrk="domagx.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Beda&ubreve;rinda:
      <ekz>
        tio estas <tld/>a afero.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_r;&c_t;&c_y; &c_zh;&c_a;&c_l;&c_ju;</trd>
  <trd lng="bo">&#xF51;&#xF42;&#xF7C;&#xF44;&#xF66;&#xF0B;&#xF51;&#xF42;&#xF0B;</trd>
  <trdgrp lng="de">
    <trd>bedauerlich</trd>,
    <trd>schade</trd>
  </trdgrp>
  <trd lng="en">regrettable</trd>
  <trdgrp lng="es">
    <trd>tr&aacute;gico</trd>,
    <trd>lamentable</trd>,
    <trd>triste</trd>,
    <trd>dram&aacute;tico</trd>,
    <trd>desastroso</trd>
  </trdgrp>
  <trd lng="fr">regrettable</trd>
  <trdgrp lng="nl">
    <trd>spijtig</trd>,
    <trd>jammer</trd>,
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przykry</trd>,
    <trd>&zdot;a&lstroke;osny</trd>,
    <trd>nieprzyjemny</trd>,
    <trd>niefajny<klr> (pot.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="domagx.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Beda&ubreve;rinde:
      <ekz>
        estas <tld/>e, ke;
      </ekz>
      <ekz>
        kiel <tld/>e!
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_k;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_zh;&c_a;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_zh;&c_a;&c_l;&c_k;&c_o;</trd>
  <trdgrp lng="de">
    <trd>bedauerlich</trd>,
    <trd>schade</trd>
  </trdgrp>
  <trd lng="en">regrettably</trd>
  <trdgrp lng="es">
    <trd>lamentablemente</trd>,
    <trd>desgraciadamente</trd>,
    <trd>desafortunadamente</trd>
  </trdgrp>
  <trd lng="he">&#x5D7;&#x5D1;&#x5DC;</trd>
  <trd lng="nl">jammer genoeg</trd>
  <trdgrp lng="pl">
    <trd>przykro</trd>,
    <trd>&zdot;a&lstroke;osnnie</trd>,
    <trd>nieprzyjemnie</trd>,
    <trd>niefajnie<klr> (pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_a;&c_l;&c_mol;</trd>,
    <trd>&c_zh;&c_a;&c_l;&c_k;&c_o;</trd>,
    <trd>&c_d;&c_o;&c_s;&c_a;&c_d;&c_n;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="domagx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="domagx.0i.avari">
    <dif>
      Timi beda&ubreve;rinda&jcirc;on, difekton, perdon pri io;
      deziri
      &scirc;pari, konservi, ne elspezi, ne difekti ion:
      <ekz>
        li ne a&ccirc;etis la libron, &ccirc;ar li <tld/>is la
        monon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li ne eliris en la pluva vetero,
        &ccirc;ar li <tld/>is sian veston;
      </ekz>
      <ekz>
        kiu <tld/>as gro&scirc;on perdas la tutan
        po&scirc;on<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        pafu sur &gcirc;in, ne <tld/>u sagojn
        <fnt><bib>MT</bib>, <lok>&Jer; 50:14</lok></fnt>;
      </ekz>
      <ekz>
        ne <tld/>i sian penon;
      </ekz>
      <ekz>
        li forkuris, &ccirc;ar li <tld/>is sian vivon
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="ofer">maloferi</ref>,
      <ref cel="avar.0i">avari</ref>.
    </refgrp>
    <trdgrp lng="es">
      <trd>preocuparse <klr>por</klr></trd>,
      <trd>temer <klr>por</klr></trd>,
      <trd>llevar cuidado con</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>be anxious about</trd>,
      <trd>be concerned for</trd>,
      <trd>fear for</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>m&eacute;nager</trd>,
      <trd>&eacute;conomiser</trd>,
      <trd>&eacute;pargner</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>&zdot;a&lstroke;owa&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_o;&c_k;&c_r;&c_u;&c_sh;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_zh;&c_a;&c_l;&c_je;&c_t;&c_mol; <klr>(&c_o;
      &c_ch;&c_jo;&c_m;-&c_l;.)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="domagx.0i.kompati">
    <dif>
      Timi malutili, timi ofendi a&ubreve; dolorigi, voli indulgi:
      <ekz>
        pro sprita&jcirc;o li e&ccirc; patron ne
        <tld/>us<fnt>Z</fnt>.
      </ekz>
      <ekz>
        vi <tld/>as la hederon, pri kiu vi ne laboris
        kaj kiun vi ne edukis, kiu dum unu nokto elkreskis kaj dum
        unu nokto pereis --- kaj &ccirc;u mi povus ne <tld/>i
        Nineven, la
        grandan urbon, en kiu trovi&gcirc;as pli ol cent dudek mil
        homoj
        ne povantaj distingi inter sia dekstra mano kaj sia
        maldekstra, kaj krom tio multe da brutoj
        <fnt><bib>MT</bib>, <lok>&Jon; 4:10-11.</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kompat.0i">kompati</ref>,
      <ref cel="indulg.0i">indulgi</ref>
    </refgrp>
    <trd lng="es">ser cuidadoso con</trd>
    <trd lng="fr">m&eacute;nager</trd>
    <trdgrp lng="pl">
      <trd>&zdot;a&lstroke;owa&cacute;</trd>,
      <trd>oszcz&eogonek;dza&cacute;<klr> (kogo&sacute;,
      czego&sacute;)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_b;&c_je;&c_r;&c_je;&c_ch;&c_mol;</trd>,
      <trd>&c_shch;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="domagx.0i.damagxi">
    <uzo tip="stl">EVI</uzo>
    <dif>
      Difekti, <ref tip="dif" cel="damagx.0i">dama&gcirc;i</ref>.
    </dif>
    <trd lng="es"><klr>(evitar usar en el sentido de
    da&ntilde;ar)</klr></trd>
  </snc>
  <trd lng="be">&c_sh;&c_k;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="he">&#x5DC;&#x5D4;&#x5E6;&#x5D8;&#x5E2;&#x5E8;</trd>
</drv>
</art>
