<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fraj.xml,v 1.26 2025/12/18 18:49:56 revo Exp $">
<kap><ofc>*</ofc>
<rad>
  fraj</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fraj.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fraj.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="super" cel="ov.0o">Ovoj</ref> de
      <ref tip="malprt" cel="fisx.0o">fi&scirc;oj</ref> kaj
      <ref tip="malprt" cel="ran.0o">ranoj</ref>:
      <ekz>
        la kaptitajn fi&scirc;ojn oni tuj tran&ccirc;as, apartigas la <tld/>on
        por <ref tip="sub" cel="kaviar.0o">kaviaro</ref>, forigas
        malbezonajn interna&jcirc;ojn
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
        </fnt>;
      </ekz>
      <ekz>
        sova&gcirc;aj anasoj <klr>[&#8230;]</klr> per siaj piedoj alportis etajn fi&scirc;etojn a&ubreve; <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2004/008267.html">Estigi lagetojn</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lakt.0umo">laktumo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_ib;&c_k;&c_r;&c_a;</trd>
  <trd lng="bg">&c_m;&c_i;&c_c;&c_je;&c_l;</trd>
  <trd lng="ca">fresa</trd>
  <trdgrp lng="de">
    <trd>Laich</trd>,
    <trd>Fischbrut</trd>,
    <trd>Rogen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>fry</trd>,
    <trd>spawn</trd>,
    <trd>roe</trd>,
    <trd>fish eggs</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_t;&a_kh;&a_m; &a_m;&a_A;&a_h;&f_y;</trd>,
    <trd>&a_A;&a_sh;&f_p;&a_l;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>frai<klr tip="ind"> (&oelig;ufs)</klr></trd>,
    <trd>&oelig;ufs<klr tip="ind"> (de poisson)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#21365;&#x584A;  <pr>&#12425;&#12435;&#x304B;&#12356;</pr></trd>
  <trdgrp lng="nl">
    <trd>kuit<klr tip="ind"> (viskuit)</klr></trd>,
    <trd>viskuit</trd>,
    <trd>kikkerdril</trd>,
    <trd>kikkerrit</trd>
  </trdgrp>
  <trd lng="hu">ikra</trd>
  <trdgrp lng="pl">
    <trd>ikra rybia</trd>,
    <trd>skrzek &zdot;abi</trd>
  </trdgrp>
  <trd lng="pt">ova</trd>
  <trd lng="ru">&c_i;&c_k;&c_r;&c_a;</trd>
  <trdgrp lng="uk">
    <trd>&c_ib;&#x43A;&c_r;&c_a; (&c_r;&c_i;&c_b; &c_ib; &c_zh;&c_a;&c_b;</trd>,
    <trd>&c_zh;&c_i;&c_v;&c_a;)</trd>,
    <trd>&c_ib;&#x43A;&c_r;&c_i;&#x43D;&#x43A;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#x9B5A;&#21365; <pr>y&#xFA;lu&#x1CE;n</pr></trd>
</drv>

<drv mrk="fraj.0i">
  <kap><tld/>i</kap>
  <snc mrk="fraj.0i.ZOO">
    <dif>
      Demeti sian <tld/>on:
      <ekz>
        vigle nun <tld/>as la karasoj en la kanaro de Linkolago
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep
        fratoj</vrk>, <lok>1947</lok></fnt>;
      </ekz>
      <ekz>
        aparta &hcirc;elonio, kies scienca nomo estas Eretmochelys imbricata povas konservi
        sian spermon dum 75 tagoj, kun la celo &gcirc;in utiligi en posta momento, kiam &gcirc;i
        <tld/>os en diversaj marbordaj lokoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011217.php">Nekovrilaj malkovroj</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <adm>
    &Ccirc;u temas pri la nefekundigitaj ovoj? &Ccirc;u anka&ubreve; demeti spermon
    fare de masklaj fi&scirc;oj estas ~i? <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_k;&c_l;&c_a;&c_d;&c_v;&c_a;&c_c;&c_mol; <ind>&c_ib;&c_k;&c_r;&c_u;</ind></trd>,
    <trd>&c_k;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_ib; <ind>&c_ib;&c_k;&c_r;&c_u;</ind></trd>
  </trdgrp>
  <trd lng="bg">&c_z;&c_a;&c_s;&c_a;&c_zh;&c_d;&c_a;&c_m; &c_s; <ind>&c_m;&c_i;&c_c;&c_je;&c_l;</ind></trd>
  <trd lng="ca">fresar</trd>
  <trd lng="de">laichen</trd>
  <trd lng="en">spawn</trd>
  <trd lng="fa">&a_t;&a_kh;&a_m;&#8204;&a_r;&f_y;&a_z;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">frayer</trd>
  <trd lng="ja">&#29987;&#21365;&#12377;&#x308B;  <pr>&#12373;&#12435;&#12425;&#12435;&#12377;&#x308B;</pr></trd>
  <trd lng="nl">kuit schieten</trd>
  <trdgrp lng="pl">
    <trd>z&lstroke;o&zdot;y&cacute; <ind>ikr&eogonek;</ind></trd>,
    <trd>ikrzy&cacute; si&eogonek;</trd>
  </trdgrp>
  <trd lng="hu">ikr&aacute;t rak</trd>
  <trd lng="ru">&c_m;&c_je;&c_t;&c_a;&c_t;&c_mol; &c_i;&c_k;&c_r;&c_u;</trd>
  <trd lng="uk">&#x43C;&c_je;&c_t;&c_a;&c_t;&c_i;/&c_v;&c_i;&#x43A;&c_i;&c_d;&c_a;&c_t;&c_i; &c_ib;&#x43A;&c_r;&c_u;</trd>
  <trdgrp lng="zh">
    <trd>&#x4E0B;&#x86CB; <pr>xi&#xE0;d&#xE0;n</pr></trd>,
    <trd>&#25684;&#x5B50; <pr>shu&amacron;iz&#x1D0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fraj.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="fraj.0ejo.ZOO">
    <dif>
      Loko, kie fi&scirc;oj <tld/>as:
      <ekz>
        en kelkaj regionoj de Kaza&hcirc;io kaj Turkmenio, la marbordo jam malanta&ubreve;eni&gcirc;is
        dekojn da kilometroj for de la lando, kio <klr>[&#8230;]</klr> sekigas <tld/>ejojn,
        kaj mortigas multajn specojn de akvaj vivuloj
        <fnt>
          <aut>N. Bekmurzaev, trad. A. D. Prakasa</aut>
          <vrk><url ref="https://eo.globalvoices.org/2025/04/15176/">
          Kiel savi la Kaspian Maron por ke &gcirc;i ne i&gcirc;os katastrofo por la natura vivmedio</url></vrk>
          <lok>GlobalVoices, 2025-04-11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_s;&c_c;&c_a; &c_a;&c_d;&c_k;&c_l;&c_a;&c_d;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ja; <ind>&c_ib;&c_k;&c_r;&c_y;</ind></trd>
  <trd lng="fr">zone de frai</trd>
  <trdgrp lng="pl">
    <trd>staw hodowlany</trd>,
    <trd>tarlisko</trd>
  </trdgrp>
</drv>
</art>
