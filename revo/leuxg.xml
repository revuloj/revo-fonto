<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>le&ubreve;g</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="leuxg.0o">
  <kap><tld/>o</kap>
  <snc mrk="leuxg.0o.MEZ">
    <dif>
      Malnova okcidente&ubreve;ropa kaj sudamerika <ref tip="super"
      cel="unu.mezur0o.SCI">mezurunuo</ref> de vojlongo, la&ubreve;
      la lokoj inter 3 kaj 6 kilometrojn valora:
      <ekz>
        <ctl>dudek mil <tld/>oj sub la maroj</ctl>
        <klr>(romano de Julio Vernes)</klr>;
      </ekz>
      <ekz>
        &gcirc;i pluiris tra <tld/>o inter du fumantaj fendega&jcirc;oj
        <fnt>
          <aut>J. R. R. Tolkien, trad. W. Auld</aut>,
          <vrk>La Mastro de l'Ringoj</vrk>,
          <lok>3, La reveno de la Re&gcirc;o, &ccirc;apitro 3a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">legua</trd>
  <trd lng="fr">lieue</trd>
  <trd lng="hu">m&eacute;rf&ouml;ld</trd>
  <trd lng="pt">l&eacute;gua</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_mol;&c_je;</trd>,
    <trd>&c_l;&c_i;&c_g;&c_a;<klr tip="amb"> 
(&c_je;&c_d;&c_i;&c_n;&c_i;&c_c;&c_a; &c_d;&c_l;&c_i;&c_n;&c_y;)</klr></trd>
  </trdgrp>
</drv>
</art>

