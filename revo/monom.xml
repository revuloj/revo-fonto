<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: monom.xml,v 1.21 2024/01/26 19:05:53 revo Exp $">
<kap>
  <rad>monom</rad>/o
</kap>
<drv mrk="monom.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PV</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="polino.0o">Polinomo</ref> kun unu
      <ref cel="term.0o.polinomo">termo</ref>; alidire, en elementa
      algebro: algebra esprimo, kiu entenas nek adicion, nek subtrahon:
      <ekz>
        <frm am="c(x-a)^rho"><k>c</k>(<k>x&#x202F;-&#x202F;a</k>)<sup>&rho;</sup></frm>
        estas
        nomata potenca monomo de <frm am="x-a"><k>x</k>&#x202F;-&#x202F;<k>a</k></frm>
        kun la potenca indico <frm am="rho">&rho;</frm>
        <fnt>
          <aut>Masuo Hukuhara</aut>
          <vrk><url ref="http://fe.math.kobe-u.ac.jp/FE/Free/vol04/fe04-3.pdf">Transformo de Euler kaj &gcirc;ia aplikado al la diferencialaj
              ekvacioj de Fuhcsa tipo &#x2026;</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="term.n0o">n-termo</ref>,
       <ref cel="binom.0o">binomo</ref>,
       <ref cel="trinom.0o">trinomo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_n;&c_o;&c_m;</trd>,
    <trd>&c_a;&c_d;&c_n;&c_a;&c_s;&c_k;&c_l;&c_a;&c_d;</trd>
  </trdgrp>
  <trd lng="ca">monomi</trd>
  <trdgrp lng="cs">
    <trd>jedno&ccaron;len</trd>,
    <trd>monom</trd>
  </trdgrp>
  <trd lng="de">Monom</trd>
  <trd lng="en">monomial</trd>
  <trd lng="es">monomio</trd>
  <trd lng="fr">mon&ocirc;me</trd>
  <trdgrp lng="hu">
    <trd>egytag&uacute; kifejez&eacute;s</trd>,
    <trd>mon&oacute;m</trd>
  </trdgrp>
  <trd lng="ja">&#x5358;&#x9805;&#x5F0F;  <pr>&#x305F;&#x3093;&#x3053;&#x3046;&#x3057;&#x304D;</pr></trd>
  <trdgrp lng="nl">
    <trd>eenterm</trd>,
    <trd>monoom</trd>
  </trdgrp>
  <trd lng="pl">jednomian</trd>
  <trd lng="pt">mon&ocirc;mio</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_d;&c_n;&c_o;&c_ch;&c_l;&c_je;&c_n;</trd>,
    <trd>&c_m;&c_o;&c_n;&c_o;&c_m;</trd></trdgrp>
  <trdgrp lng="sk">
    <trd>jedno&ccaron;len</trd>,
    <trd>mon&oacute;m</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x43E;&#x43D;&#x43E;&#x43C;</trd>,
    <trd>&#x43E;&#x434;&#x43D;&#x43E;&#x447;&#x43B;&#x435;&#x43D;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5355;&#x9879;&#x5F0F; <pr>d&#x101;nxi&#xE0;ngsh&#xEC;</pr></trd>,
    <trd>&#x55AE;&#x9805;&#x5F0F; <pr>d&#x101;nxi&#xE0;ngsh&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>
