<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>monom</rad>/o
</kap>
<drv mrk="monom.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="polino.0o">Polinomo</ref> kun unu
      <ref cel="term.0o.polinomo">termo</ref>; alidire, en elementa algebro:
      algebra esprimo, kiu entenas nek adicion, nek subtrahon.
    </dif>
  </snc>
  <trd lng="fr">mon&ocirc;me</trd>
  <trdgrp lng="nl">
    <trd>eenterm</trd>,
    <trd>monoom</trd>
  </trdgrp>
</drv>
</art>
