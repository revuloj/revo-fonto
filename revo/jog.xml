  <trd lng="hu">j&oacute;ga</trd>
  <trd lng="it">yoga</trd>
  <trd lng="ja">&#x30E8;&#x30FC;&#x30AC;</trd>
  <trd lng="nl">joga</trd>
  <trd lng="pl">joga</trd>
  <trd lng="pt">ioga</trd>
  <trd lng="ru">&c_j;&c_o;&c_g;&c_a;</trd>
