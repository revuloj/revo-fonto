<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>artikol</rad>/o
</kap>

<drv mrk="artiko.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="artiko.0o.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      <ref tip="super" cel="determ.0ilo.GRA">Determinilo</ref>,
      vorteto (a&ubreve;, en iuj lingvoj, fleksiilo) uzata &ccirc;e substantivo
      (a&ubreve; &ccirc;e noma adjektivo, prepozicio, fraza&jcirc;o) por &gcirc;in
      <ref cel="determ.0i.GRA">determini</ref>:
      <ekz>
        la <tld/>o <ctl>la</ctl> estas uzata tiam, kiam ni
        parolas pri personoj a&ubreve; objektoj konataj
        <fnt><bib>F</bib>, <lok>&FE;27.</lok></fnt>;
      </ekz><ekz>
        <ind>difina <tld/>o</ind>;
        <trd lng="be">&c_a;&c_z;&c_n;&c_a;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_y; 
          <ind>&c_a;&c_r;&c_t;&c_y;&c_k;&c_l;&c_mol;</ind></trd>   
        <trd lng="br">ger-mell strizh</trd>
        <trd lng="ru">&c_o;&c_p;&c_r;&c_je;&c_d;&c_je;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j; 
          &c_a;&c_r;&c_t;&c_i;&c_k;&c_l;&c_mol;</trd>
      </ekz><ekz>
        <ind>nedifina <tld/>o</ind>;
        <trd lng="be">&c_n;&c_je;&c_a;&c_z;&c_n;&c_a;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_y; 
          <ind>&c_a;&c_r;&c_t;&c_y;&c_k;&c_l;&c_mol;</ind></trd> 
        <trd lng="br">ger-mell amstrizh</trd>
        <trd lng="ru">&c_n;&c_je;&c_o;&c_p;&c_r;&c_je;&c_d;&c_je;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j; 
          &c_a;&c_r;&c_t;&c_i;&c_k;&c_l;&c_mol;</trd>
      </ekz><ekz>
        <ind>nul<tld/>o</ind>.
        <trd lng="be">&c_n;&c_u;&c_l;&c_ja;&c_v;&c_y; 
          <ind>&c_a;&c_r;&c_t;&c_y;&c_k;&c_l;&c_mol;</ind></trd>
        <trd lng="ru">&c_n;&c_u;&c_l;&c_je;&c_v;&c_o;&c_j; &c_a;&c_r;&c_t;&c_i;&c_k;&c_l;&c_mol;</trd>
      </ekz>
    </dif>
    <rim> Por la uzado de ia nedifina artikolo vidu &ccirc;e <ref tip="sin"
    cel="iu.0.elektajxo">iu</ref>, <ref tip="sin"
    cel="unu.0.artikolo">unu</ref>, <ref tip="sin"
    cel="cert.0a.difinebla">certa</ref>.</rim>
    <trd lng="be">&c_a;&c_r;&c_t;&c_y;&c_k;&c_l;&c_mol;</trd>  
    <trd lng="br">ger-mell</trd>
    <trd lng="ru">&c_a;&c_r;&c_t;&c_i;&c_k;&c_l;&c_mol;</trd>
    <trd lng="sk">&ccaron;len</trd>
  </snc>
  <snc mrk="artiko.0o.teksto">
    <dif>
      Tekstopeco el pli granda tuto:
    </dif>
    <subsnc mrk="artiko.0o.gazeta">
      <dif>
        Relative negranda verko scienca, politika, kritika ktp en
        gazeto, revuo, <tld/>aro, bro&scirc;uro:
        <ekz>
          en la <tld/>o <ctl>Esenco kaj Estonteco</ctl>,
          kiun multaj el vi legis en la
          <ctl>Fundamenta Krestomatio</ctl>, mi analizis tiun
          demandon detale
          <fnt>
            <aut>&Z;</aut>:
            <vrk>
              Parolado anta&ubreve; la Sesa Kongreso Esperantista,
              (Washington, 1910-08-15).
            </vrk>
          </fnt>;
        </ekz>
        <ekz>
          anonima <tld/>o;
        </ekz><ekz>
          redakcia <tld/>o, &ccirc;ef<tld/>o.
        </ekz>
    </dif>     
    <trd lng="br">pennad<klr tip="ind"> (skrid)</klr></trd>
  </subsnc>
  <subsnc mrk="artiko.0o.sekcio">
    <dif>
      Sekcio, &ccirc;apitro a&ubreve; paragrafo en le&gcirc;o,
            vortaro, kontrakto, rezolucio ktp:
      <ekz>
        la <tld/>o estas divida&jcirc;o meza inter &ccirc;apitro kaj paragrafo
        <fnt>PIV</fnt>.
      </ekz>
    </dif>
    <trd lng="br">mellad</trd>
  </subsnc>
    <trd lng="be">&c_a;&c_r;&c_t;&c_y;&c_k;&c_u;&c_l;</trd>
    <trd lng="ru">&c_s;&c_t;&c_a;&c_t;&c_mol;&c_ja;</trd>
    <trd lng="sk">&ccaron;l&aacute;nok</trd>
  </snc>
  <snc mrk="artiko.0o.artiklo">
    <dif>
      <ref tip="dif" cel="artikl.0o">Artiklo</ref>.
    </dif>
    <trd lng="ru">&c_a;&c_r;&c_t;&c_i;&c_k;&c_u;&c_l;</trd>
  </snc>
  <trd lng="ca">article</trd>
  <trd lng="de">Artikel</trd>
  <trd lng="en">article</trd>
  <trd lng="es">art&iacute;culo</trd>
  <trd lng="fr">article</trd>
  <trd lng="it">articolo</trd>
  <trd lng="nl">artikel</trd>
  <trd lng="sv">artikel</trd>
</drv>

<drv mrk="artiko.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="artiko.0aro.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref tip="super" cel="tekst.0aro">Tekstaro</ref>, libro en
        kiu estas kolektitaj
      <tld/>oj,<sncref ref="artiko.0o.gazeta"/>
      de diversaj a&ubreve;toroj pri iu temo:
      <ekz>
        &leftquot;Radioteknika <tld/>aro&rightquot;,
        eldonita en Budape&scirc;to en 1984.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_b;&c_o;&c_r;&c_n;&c_ib;&c_k; &c_a;&c_r;&c_t;&c_y;&c_k;&c_u;&c_l;&c_a;&c_w;</trd>       
    <trdgrp lng="br">
      <trd>dastumad<klr tip="ind"> (skrido&ugrave;)</klr></trd>,  
      <trd>teskad<klr tip="ind"> (skrido&ugrave;)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_b;&c_o;&c_r;&c_n;&c_i;&c_k; &c_s;&c_t;&c_a;&c_t;&c_je;&c_j;</trd>
  </snc>
</drv>

</art>
