<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ar&scirc;in</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="arsxin.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Malnova orienta mezurunuo de longo.
      Rusa <tld/>o = 0,7112m.
      Bulgara, estona, litova, rumana, serba kaj historia turka
      <tld/>oj longis inter 0,68 kaj 0,69m.
      Moderna turka <tld/>o = 1m.
    </dif>
  </snc>
  <trd lng="de">Arschin</trd>
  <trd lng="en">archine</trd>
  <trd lng="fr">archine</trd>
  <trd lng="hu">arsin</trd>
  <trd lng="ru">&c_a;&c_r;&c_sh;&c_i;&c_n;</trd>
  <trdgrp lng="tr">
    <trd>ar&#x15F;&inodot;n</trd>,
    <trd>endaze</trd>
  </trdgrp>
</drv>
</art>
