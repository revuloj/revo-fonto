<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sturg.xml,v 1.24 2024/03/25 07:28:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sturg</rad>/o
</kap>
<drv mrk="sturg.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Primitiva rabo<ref tip="lst" cel="fisx.0o" lst="voko:fi&scirc;oj">fi&scirc;o</ref>,
      iom &scirc;arkoforma, vivanta en e&ubreve;ropaj maroj, sed preska&ubreve; formortinta
      kaj pro tio protektita de 1982;
      <ref tip="lst" lst="voko:zoologiaj_specioj" cel="speci.0o">specio</ref>
      de <ref tip="super" cel="acipenser.0o">acipensero</ref>
      (<trd lng="la">Acipenser sturio</trd>):
      <ekz>
        infrastrukturaj projektoj en la malsupra Danubo povus tute
        malaperigi la forte minacatajn populaciojn de <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2010/010038.php">Priatenti la naturprotekton</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <ref cel="kaviar.0o">kaviaro</ref>
       estas franda&jcirc;o, farita el salita frajo de <tld/>o a&ubreve; similaj fi&scirc;oj (huzo)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=26047">Kaviaro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/4/4c/St%C3%B6r_Sturgeon.jpg/640px-St%C3%B6r_Sturgeon.jpg" prm="CC BY     2.0">E&ubreve;ropa <tld/>o (Acipenser sturio)

        <fnt>
          <aut>Aah-Yeah, Flickr: St&ouml;r Sturgeon</aut>
          <lok><url ref="&WCW;/File:St%C3%B6r_Sturgeon.jpg">&WCW;/File:St%C3%B6r_Sturgeon.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_je;&c_t;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>jeseter</trd>,
    <trd>jeseter velk&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Europ&auml;ischer <ind>St&ouml;r</ind></trd>,
    <trd>Baltischer <ind>St&ouml;r</ind></trd>
  </trdgrp>
  <trd lng="en">sturgeon</trd>
  <trd lng="fr">esturgeon</trd>
  <trd lng="hu">k&ouml;z&ouml;ns&eacute;ges <ind>tok</ind></trd>
  <trd lng="it">storione<klr> (genere sturgo)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x30E8;&#x30FC;&#x30ED;&#x30C3;&#x30D1;&#x30C1;&#x30E7;&#x30A6;&#x30B6;&#x30E1;</trd>,
    <trd>&#x30C1;&#x30E7;&#x30A6;&#x30B6;&#x30E1;</trd>
  </trdgrp>
  <trd lng="nl">steur</trd>
  <trd lng="pl">jesiotr zachodni</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_t;&c_l;&c_a;&c_n;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;
<ind>&c_o;&c_s;&c_jo;&c_t;&c_r;</ind></trd>,
    <trd>&c_b;&c_a;&c_l;&c_t;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j;
<ind>&c_o;&c_s;&c_jo;&c_t;&c_r;</ind></trd>
  </trdgrp>
  <trd lng="sk">jeseter ve&lcaron;k&yacute;</trd>
  <trd lng="sv">st&ouml;r</trd>
  <trd lng="uk">&#x43E;&c_s;&c_je;&c_t;&c_je;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#x9C58; <pr>x&#xFA;n</pr></trd>,
    <trd>&#x9C9F; <pr>x&#xFA;n</pr></trd>
  </trdgrp>
</drv>
</art>
