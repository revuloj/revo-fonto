<art mrk="$Id: kaspi.xml,v 1.17 2023/12/03 08:00:24 revo Exp $">
<kap>
  <rad>Kaspi</rad>/o
</kap>
<drv mrk="kaspi.K0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Eksmaro, la plej granda <ref tip="super" cel="lag.0o">lago</ref>
      en la mondo, salakva, 27,9 m sub la oceannivelo (1986), situanta
      en
      <ref tip="malprt" cel="azi.A0o">Azio</ref> kaj
      &ccirc;e la sud-orienta limo de E&ubreve;ropo, okcidente de
      Ka&ubreve;kazo, inter <ref cel="rus.R0ujo">Rusujo</ref>
      norde kaj Irano sude:
      <ekz>
        la Golfo <nom>Garabogazk&ouml;l</nom> estas salenhava orienta interna
        golfeto a&ubreve; aparta lageto
        de <tld/>o, kiu estas parto de Turkmenio kaj foje estis lago pro
        la istmo kiu
        separas &gcirc;in el <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2196">Kaspio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_K;&c_a;&c_s;&c_mol;&c_p;&c_ib;&c_j;&c_s;&c_k;&c_a;&c_je; &c_m;&c_o;&c_r;&c_a;</trd>
  <trd lng="ca">Mar <ind>Caspi</ind></trd>
  <trd lng="cs">Kaspick&eacute; mo&rcaron;e</trd>
  <trd lng="de">Kaspisee</trd>
  <trd lng="en">Caspian Sea</trd>
  <trd lng="es">Mar <ind>Caspio</ind></trd>
  <trd lng="fr">Mer <ind>Caspienne</ind></trd>
  <trd lng="hu">Kaszpi-tenger</trd>
  <trd lng="ja">&#x30AB;&#x30B9;&#x30D4;&#x6D77;  <pr>&#x30AB;&#x30B9;&#x30D4;&#x304B;&#x3044;</pr></trd>
  <trd lng="nl">Kaspische Zee</trd>
  <trd lng="pl">Morze  <ind>Kaspijskie</ind></trd>     
  <trd lng="ro">Marea Caspic&abreve;</trd>
  <trd lng="ru">&c_K;&c_a;&c_s;&c_p;&c_i;&c_j;&c_s;&c_k;&c_o;&c_je; &c_m;&c_o;&c_r;&c_je;</trd>
  <trd lng="sk">Kaspick&eacute; more</trd>
  <trdgrp lng="tr">
    <trd>Hazar denizi</trd>,
    <trd>Hazer denizi</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x41A;&#x430;&#x441;&#x43F;&#x456;&#x439;</trd>,
    <trd>&#x41A;&#x430;&#x441;&#x43F;&#x456;&#x439;&#x441;&#x44C;&#x43A;&#x435; &#x43C;&#x43E;&#x440;&#x435;</trd>
  </trdgrp>
  <trd lng="zh">&#x88E1;&#x6D77; <pr>L&#x1D0;H&#x1CE;i</pr></trd>
</drv>
</art>
