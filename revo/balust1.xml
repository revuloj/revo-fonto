<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: balust1.xml,v 1.21 2021/03/04 19:18:48 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>balustrad</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="balust1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="balust1.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="super" cel="bar.0ilo">Barilo</ref> a&ubreve; <ref tip="super" cel="apog.0ilo">apogilo</ref>
      konsistanta el vico da kolonetoj a&ubreve; <ref tip="prt" cel="palis.0eto">paliseto</ref>j 
      kunigitaj per supra horizontala trabo:
      <ekz>
        <tld/>o de teraso, balkono, &scirc;tuparo, &hcirc;orejo;
      </ekz>
      <ekz>
        <tld/>o de verando
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam vi konstruos novan domon, faru <tld/>on &ccirc;irka&ubreve; via tegmento
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 22:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;tuparo kun malpuraj &scirc;tupoj kaj glata &scirc;nuro anstata&ubreve; <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fera <tld/>o aspektas kiel la pordo de tomba arka&jcirc;o kaj krom tio &gcirc;i havas
        ankora&ubreve; latunajn butonojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi longe staris senmove, min apoginte al fera <tld/>o, kaj
        observis la riveron
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>,
        <lok>&ccirc;apitro 33a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="balust.0aro">balustraro</ref>,
      <ref cel="barier.0o">bariero</ref>,
      <ref cel="parape.0o">parapeto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_l;&c_ju;&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;</trd>
  <trd lng="bg">&c_b;&c_a;&c_l;&c_ju;&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;</trd>
  <trd lng="br">aspled</trd>
  <trd lng="ca">barana</trd>
  <trdgrp lng="cs">
    <trd>balustr&aacute;da</trd>,
    <trd>z&aacute;bradl&iacute; <klr>(zvl. sloupov&eacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Balustrade</trd>,
    <trd>Br&uuml;stung</trd>,
    <trd>S&auml;ulengel&auml;nder</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>balustrade</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>barandilla</trd>,
    <trd>baranda</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>balustrade</trd>,
    <trd>garde-corps</trd>,
    <trd>parapet</trd>
  </trdgrp>
  <trd lng="he">&#x5DE;&#x5B7;&#x5E2;&#x5B2;&#x5E7;&#x5B6;&#x5D4;</trd>
  <trdgrp lng="hu">
    <trd>balusztr&aacute;d</trd>,
    <trd>korl&aacute;t</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>balustrade</trd>,
    <trd>borstwering</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>balustrada</trd>,
    <trd>por&eogonek;cz</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_l;&c_ju;&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_i;&c_l;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_a;&c_p;&c_je;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>balustr&aacute;da</trd>,
    <trd>z&aacute;bradlie</trd>
  </trdgrp>
</drv>
</art>
