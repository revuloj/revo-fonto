<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>balustrad</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="balust1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="balust1.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Barilo a&ubreve; apogilo konsistanta el vico da kolonetoj
      a&ubreve;
      palisetoj kunigitaj per supra horizontala trabo:
      <ekz>
        <tld/>o de teraso, balkono, &scirc;tuparo, &hcirc;orejo;
      </ekz>
      <ekz>
        mi longe staris senmove, min apoginte al fera <tld/>o, kaj
        observis la riveron
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>,
        <lok>&ccirc;apitro 33a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="balust.0aro">balustraro</ref>,
      <ref cel="barier.0o">bariero</ref>,
      <ref cel="bar.0ilo">barilo</ref>,
      <ref cel="parape.0o">parapeto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_l;&c_ju;&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;</trd>
  <trd lng="bg">&c_b;&c_a;&c_l;&c_ju;&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;</trd>
  <trd lng="br">aspled</trd>
  <trd lng="ca">barana</trd>
  <trdgrp lng="de">
    <trd>Balustrade</trd>,
    <trd>Br&uuml;stung</trd>,
    <trd>S&auml;ulengel&auml;nder</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>balustrade</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>barandilla</trd>,
    <trd>baranda</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>balustrade</trd>,
    <trd>garde-corps</trd>,
    <trd>parapet</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>balusztr&aacute;d</trd>,
    <trd>korl&aacute;t</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>balustrade</trd>,
    <trd>borstwering</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>balustrada</trd>,
    <trd>por&eogonek;cz</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_l;&c_ju;&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_i;&c_l;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_a;&c_p;&c_je;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>balustráda</trd>,
    <trd>zábradlie</trd>
  </trdgrp>
</drv>
</art>
