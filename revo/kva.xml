<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kva.xml,v 1.4 2023/04/20 10:30:35 revo Exp $">
<kap>
  <rad>kva</rad> <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kva.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="kva.0.SON">
    <dif>
      <ref tip="lst" cel="blek.0o" lst="voko:blekoj">Bleko</ref>
      rana a&ubreve; simila;
      <ref tip="dif" cel="kvak.0.SON">kvak</ref>:
      <ekz>
        vi <klr>...</klr> faros viziton al la ranoj;
        ili <klr>...</klr> kantos <ctl><tld/> <tld/></ctl>
        <fnt>
          <bib>Fab1</bib>, <vrk>Cikonioj</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_v;&c_a;!</trd>
  <trdgrp lng="cs">
    <trd>kv&aacute;</trd>,
    <trd>kv&aacute;</trd>
  </trdgrp>
  <trd lng="fr">co&acirc; !</trd>
  <trd lng="ru">&c_k;&c_v;&c_a;!</trd>
  <trdgrp lng="sk">
    <trd>kv&aacute;</trd>,
    <trd>kv&aacute;</trd>
  </trdgrp>
</drv>
</art>

