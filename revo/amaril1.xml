<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: amaril1.xml,v 1.11 2019/02/25 16:10:20 revo Exp $">
<kap>
  <rad>amarilidac</rad>/oj
</kap>
<drv mrk="amaril1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref>  de <ref tip="malprt"
      cel="kotile.unu0aj">unukotiledonaj</ref>
      vegeta&jcirc;oj (<trd lng="la">Amaryllidaceae</trd>):
      <ekz>
        <tld/>oj <klr>[&#8230;]</klr> ampleksas proksimume 60 genroj kaj pli ol 800 specioj,
        kiuj estas nature disvasti&gcirc;antaj aparte en pli varmaj klimataj zonoj kaj
        kreskas preska&ubreve; tutmonde
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=246269"
              >Amarilidacoj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        narcisoj estas genro da bulboherboj el la familio de <tld lit="A"/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=246267"
              >Narciso (floro)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="amaril.0o">amarilido</ref>,
      <ref cel="galant.0o">galanto</ref>,
      <ref cel="narcis.0o">narciso</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_a;&c_r;&c_y;&c_l;&c_ib;&c_s;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_k;&c_o;&c_k;&c_i;&c_ch;&c_je;&c_v;&c_i;</trd>
  <trd lng="ca">amaril&middot;lid&agrave;cies </trd>
  <trd lng="de">Amaryllisgew&auml;chse</trd>
  <trd lng="en">crinum</trd>
  <trd lng="fr">amaryllidac&eacute;es</trd>
  <trd lng="hu">amarilliszf&eacute;l&eacute;k</trd>
  <trd lng="nl">narcissen <klr tip="amb">(familie)</klr></trd>
  <trd lng="ru">&c_a;&c_m;&c_a;&c_r;&c_i;&c_l;&c_l;&c_i;&c_s;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sv">amaryllisv&auml;xter</trd>
</drv>
</art>
