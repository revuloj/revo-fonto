<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>laktuk</rad>/o
</kap>
<drv mrk="laktuk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="aster.0acoj">asteracoj</ref>
      (<trd lng="la">Lactuca</trd>),
      legoma vegeta&jcirc;o uzata kiel salato.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Lattich</trd>,
    <trd>Salat</trd>
  </trdgrp>
  <trd lng="en">lettuce</trd>
  <trd lng="fr">laitue</trd>
</drv>
</art>
