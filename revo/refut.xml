<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>refut</rad>/i
</kap>
<drv mrk="refut.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Venke rebati opinion per trafaj argumentoj:
      <ekz>
        <tld/>i al iu, ke...;
      </ekz>
      <ekz>
        montri per ne <tld/>eblaj faktoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="argume.0i">argumenti</ref>,
      <ref cel="pruv.0i">pruvi</ref>,
      <ref cel="kritik..0i">kritiki</ref>,
      <ref cel="kontes.0i">kontesti</ref>,
      <ref cel="frakas.0i">frakasi</ref>,
      <ref cel="batal.0i">batali</ref>,
      <ref cel="pusx.re0i">repu&scirc;i</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_k;&c_ib;&c_d;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_h;&c_ib;&c_l;&c_ja;&c_c;&c_mol; <klr>(&c_a;&c_r;&c_g;&c_u;&c_m;&c_e;&c_n;&c_t;&c_y;, &c_d;&c_o;&c_v;&c_a;&c_d;&c_y;)</klr></trd>
  </trdgrp>
  <trd lng="de">widerlegen</trd>
  <trdgrp lng="hu">
    <trd>megc&aacute;fol</trd>,
    <trd>visszautas&iacute;t</trd>,
    <trd>ellenvet</trd>
  </trdgrp>
  <trd lng="nl">weerleggen</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_v;&c_je;&c_r;&c_g;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_k;&c_l;&c_o;&c_n;&c_ja;&c_t;&c_mol; <klr tip="amb">(&c_a;&c_r;&c_g;&c_u;&c_m;&c_je;&c_n;&c_t;&c_y;)</klr></trd>
  </trdgrp>
</drv>
</art>
