<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>refut</rad>/i
</kap>
<drv mrk="refut.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Venke rebati opinion per trafaj argumentoj:
      <ekz>
        <tld/>i al iu, ke...;
      </ekz>
      <ekz>
        montri per ne <tld/>eblaj faktoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="argume.0i">argumenti</ref>,
      <ref cel="pruv.0i">pruvi</ref>,
      <ref cel="kritik.0i">kritiki</ref>,
      <ref cel="kontes.0i">kontesti</ref>,
      <ref cel="frakas.0i">frakasi</ref>,
      <ref cel="batal.0i">batali</ref>,
      <ref cel="pusx.re0i">repuŝi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>адкідаць</trd>,
    <trd>адхіляць <klr>(аргумэнты, довады)</klr></trd>
  </trdgrp>
  <trd lng="de">widerlegen</trd>
  <trd lng="en">refute</trd>
  <trdgrp lng="hu">
    <trd>megcáfol</trd>,
    <trd>visszautasít</trd>,
    <trd>ellenvet</trd>
  </trdgrp>
  <trd lng="nl">weerleggen</trd>
  <trdgrp lng="ru">
    <trd>отвергать</trd>,
    <trd>отклонять <klr tip="amb">(аргументы)</klr></trd>
  </trdgrp>
</drv>
</art>
