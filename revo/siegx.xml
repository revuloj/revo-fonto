<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: siegx.xml,v 1.30 2024/03/24 20:21:56 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sie&gcirc;</rad>/i
  <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="siegx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="siegx.0i.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Restadigi militistajn trupojn &ccirc;irka&ubreve;
      fortika&jcirc;o por preni &gcirc;in:
      <ekz>
        ili <tld/>is A&hcirc;azon, sed ne povis venkopreni
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 16:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        estis granda malsato en Samario, kaj ili <tld/>is &gcirc;in tiel longe, &gcirc;is kapo de
        azeno ricevis la prezon de okdek sikloj
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 6:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        dufoje la turkoj <tld/>is Vienon
        <fnt>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2000/006903.php">Arabaj vortoj en la germana kaj esperanta
          lingvoj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/02, p. 14</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>oblega&cacute;</trd>,
      <trd>prowadzi&cacute; <ind>obl&eogonek;&zdot;enie</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="siegx.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Amase a&ubreve; longe minaci, tedi:
      <ekz>
        almozpetantaj kripluloj <tld/>is <klr>[la]</klr> pordon
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ekskuis la kapon, kvaza&ubreve; &scirc;i volus forpeli la amason da kordoloroj kaj timoj,
        kiuj &scirc;in <tld/>is
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        a&ubreve;dinte la bruon, la mona&hcirc;oj komprenos, ke la tentoj <tld/>as iun fraton, kaj
        &ccirc;iuj kune iros pre&gcirc;i
        <fnt>
          <bib>IKr</bib>
          <lok>Tri Musketeroj</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ne &ccirc;esis <tld/>i min per siaj petoj &gcirc;is mi konsentis vo&ccirc;legi al li la tutan
        romanon
        <fnt>
          <bib>MkM</bib>
          <lok>13</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili tute min &ccirc;irka&ubreve;<tld/>as
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 88:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        dan&gcirc;ero <tld/>as, al Dio ni pre&gcirc;as, dan&gcirc;ero
        &ccirc;esas, ni Dion forgesas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="es">
      <trd>sitiar</trd>,
      <trd>asediar</trd>
    </trdgrp>
    <trd lng="pl">oblega&cacute;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_l;&c_o;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_s;&c_a;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_o;&c_b;&c_s;&c_a;&c_zh;&c_d;&c_a;&c_m;</trd>
  <trd lng="de">belagern</trd>
  <trd lng="en">besiege</trd>
  <trd lng="fa">&a_m;&a_H;&a_A;&a_S;&a_r;&a_h;
  &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>assi&eacute;ger</trd>,
    <trd>faire le <ind>si&egrave;ge</ind> de</trd>
  </trdgrp>
  <trd lng="hu">ostromol</trd>
  <trd lng="it">assediare</trd>
  <trdgrp lng="ja">
    <trd>&#x5305;&#x56F2;&#x3059;&#x308B;  <pr>&#x307B;&#x3046;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x653B;&#x56F2;&#x3059;&#x308B; <pr>&#x3053;&#x3046;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x53D6;&#x308A;&#x56F2;&#x3080;  <pr>&#x3068;&#x308A;&#x304B;&#x3053;&#x3080;</pr></trd>,
    <trd>&#x6BBA;&#x5230;&#x3059;&#x308B;  <pr>&#x3055;&#x3063;&#x3068;&#x3046;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">belegeren</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_s;&c_a;&c_d;&c_i;&c_t;&c_mol;<klr tip="ind">
    (&c_v;&c_o;&c_je;&c_n;.)</klr></trd>,
    <trd>&c_o;&c_s;&c_a;&c_zh;&c_d;&c_a;&c_t;&c_mol; <klr tip="amb">(&c_v;&c_o;&c_je;&c_n;.)</klr></trd>
  </trdgrp>
  <trd lng="uk">&c_o;&c_b;&c_l;&c_ja;&c_g;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x56F4;&#x57CE; <pr>w&#xE9;ich&#xE9;ng</pr></trd>,
    <trd>&#x570D;&#x57CE; <pr>w&#xE9;ich&#xE9;ng</pr></trd>,
    <trd>&#x56F4;&#x653B; <pr>w&#xE9;ig&#x14D;ng</pr></trd>,
    <trd>&#x570D;&#x653B; <pr>w&#xE9;ig&#x14D;ng</pr></trd>,
    <trd>&#x56F4;&#x56F0; <pr>w&#xE9;ik&#xF9;n</pr></trd>,
    <trd>&#x570D;&#x56F0; <pr>w&#xE9;ik&#xF9;n</pr></trd>,
    <trd>&#x5305;&#x56F4; <pr>b&#x101;ow&#xE9;i</pr></trd>,
    <trd>&#x5305;&#x570D; <pr>b&#x101;ow&#xE9;i</pr></trd>,
    <trd>&#x56F4;&#x51FB; <pr>w&#xE9;ij&#x12B;</pr></trd>,
    <trd>&#x570D;&#x64CA; <pr>w&#xE9;ij&#x12B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="siegx.0o">
  <kap><tld/>o</kap>
  <snc mrk="siegx.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref cel="bar.cxirkaux0i">&Ccirc;irka&ubreve;baro</ref> de fortika&jcirc;o, urbo, haveno per
      seninterrompa gardo, por venki &gcirc;in:
      <ekz>
        provizu al vi akvon por la tempo de <tld/>o, fortigu viajn fortika&jcirc;ojn
        <fnt>
          <bib>MT</bib>
          <lok>Na&hcirc;um 3:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        en 1683 venis <klr>[&#x2026;]</klr> 200-mil-kapa armeo denove al Vieno, nun la <tld/>o
        da&ubreve;ris preska&ubreve; du monatojn
        <fnt>
          <bib>Monato</bib>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk>Pruntoj: Arabaj vortoj en la germana kaj esperanta lingvoj</vrk>
        </fnt>;
      </ekz>
      <ekz>
        nurajn 11 jarojn post la rusa <tld/>o fininta la germanan okupadon, granda parto
        de Budape&scirc;to denove ku&scirc;is en ruinoj
        <fnt>
          <bib>Monato</bib>
          <aut>Istv&aacute;n Ertl</aut>
          <vrk><url ref="&Monato;/2003/008140.html">Pri orienti&gcirc;o kaj okcidenti&gcirc;o</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="blokad.0o">blokado</ref>
    <trd lng="be">&c_a;&c_b;&c_l;&c_o;&c_g;&c_a;</trd>
    <trd lng="bg">&c_o;&c_b;&c_s;&c_a;&c_d;&c_a;</trd>
    <trd lng="de">Belagerung</trd>
    <trd lng="en">siege</trd>
    <trd lng="fa">&a_m;&a_H;&a_A;&a_S;&a_r;&a_h;</trd>
    <trd lng="it">assedio</trd>
    <trdgrp lng="pl">
      <trd>okr&aogonek;&zdot;enie</trd>,
      <trd>obl&eogonek;&zdot;enie</trd>
    </trdgrp>
    <trd lng="fr">si&egrave;ge</trd>
    <trdgrp lng="zh">
    <trd>&#x56F4;&#x653B; <pr>w&#xE9;ig&#x14D;ng</pr></trd>,
    <trd>&#x570D;&#x653B; <pr>w&#xE9;ig&#x14D;ng</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="ja">
    <trd>&#x5305;&#x56F2;&#x653B;&#x6483;  <pr>&#x307B;&#x3046;&#x3044;&#x3053;&#x3046;&#x3052;&#x304D;</pr></trd>,
    <trd>&#x57CE;&#x653B;&#x3081;  <pr>&#x3057;&#x308D;&#x305B;&#x3081;</pr></trd>
  </trdgrp>
  <trd lng="nl">belegering</trd>
  <trd lng="uk">&c_o;&c_b;&c_l;&c_o;&c_g;&c_a;</trd>
</drv>
</art>
