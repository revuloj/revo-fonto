<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: siegx.xml,v 1.27 2023/09/24 16:28:24 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sie&gcirc;</rad>/i
</kap>
<drv mrk="siegx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="siegx.0i.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Restadigi militistajn trupojn &ccirc;irka&ubreve;
      fortika&jcirc;o por preni &gcirc;in.
    </dif>
    <trdgrp lng="pl">
      <trd>oblega&cacute;</trd>,
      <trd>prowadzi&cacute; <ind>obl&eogonek;&zdot;enie</ind></trd>
    </trdgrp>
    <ekz>dufoje la turkoj <tld/>is Vienon
    <fnt>
    <aut>Walter Klag</aut>
    <vrk><url ref="&Monato;/2000/006903.php">Arabaj vortoj en la germana kaj esperanta
    lingvoj</url></vrk>
    <bib>Monato</bib>
    <lok>2000/02, p. 14</lok></fnt>.
    </ekz>
  </snc>
  <snc mrk="siegx.0i.FIG">
    <dif>
      Amase a&ubreve; longe minaci, tedi:
      <ekz>
        <tld/>i ies pordon
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        botpurigistoj are <tld/>as la trapasantojn;
      </ekz>
      <ekz>
        ili tute min &ccirc;irka&ubreve;<tld/>as<fnt>Z</fnt>;
      </ekz>
      <ekz>
        dan&gcirc;ero <tld/>as, al Dio ni pre&gcirc;as, dan&gcirc;ero
        &ccirc;esas, ni Dion forgesas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="es">
      <trd>sitiar</trd>,
      <trd>asediar</trd>
    </trdgrp>
    <trd lng="pl">oblega&cacute;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_l;&c_o;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_s;&c_a;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_o;&c_b;&c_s;&c_a;&c_zh;&c_d;&c_a;&c_m;</trd>
  <trd lng="de">belagern</trd>
  <trd lng="en">besiege</trd>
  <trd lng="fa">&a_m;&a_H;&a_A;&a_S;&a_r;&a_h;
  &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>assi&eacute;ger</trd>,
    <trd>faire le <ind>si&egrave;ge</ind> de</trd>
  </trdgrp>
  <trd lng="hu">ostromol</trd>
  <trd lng="it">assediare</trd>
  <trdgrp lng="ja">
    <trd>&#21253;&#x56F2;&#12377;&#x308B;  [<ind>&#x307B;&#12358;&#12356;&#12377;&#x308B;</ind>]</trd>,
    <trd>&#x653B;&#x56F2;&#12377;&#x308B; [<ind>&#12371;&#12358;&#12356;&#12377;&#12427;</ind>]</trd>,
    <trd>&#x53D6;&#x308A;&#x56F2;&#12416;  [<ind>&#12392;&#x308A;&#x304B;&#12371;&#12416;</ind>]</trd>,
    <trd>&#x6BBA;&#21040;&#12377;&#x308B;  [<ind>&#12373;&#12387;&#12392;&#12358;&#12377;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">belegeren</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_s;&c_a;&c_d;&c_i;&c_t;&c_mol;<klr tip="ind">
    (&c_v;&c_o;&c_je;&c_n;.)</klr></trd>,
    <trd>&c_o;&c_s;&c_a;&c_zh;&c_d;&c_a;&c_t;&c_mol; <klr tip="amb">(&c_v;&c_o;&c_je;&c_n;.)</klr></trd>
  </trdgrp>
  <trd lng="uk">&#x43E;&c_b;&#x43B;&#x44F;&c_g;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="siegx.0o">
  <kap><tld/>o</kap>
  <snc mrk="siegx.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      &Ccirc;irka&ubreve;baro de fortika&jcirc;o, urbo, haveno per
      seninterrompa gardo, por venki &gcirc;in:
      <ekz>
        La germana komendantaro retirigis el Varsovio la plimulton de
        siaj soldatoj timante <tld/>on;
        <fnt>
        <vrk>Pola Radio</vrk>,
        <lok>"Liberigo de Varsovio", 2008-01-17</lok>
        </fnt>;
      </ekz>
      <ekz>
        Dum 6-semajna <tld/>o de la ru&gcirc;armeanoj &gcirc;i estis
        detruita
        95-procente
        <fnt>
        <vrk>Pola Radio, "Pollando invitas"</vrk>,
        <lok>G&lstroke;og&oacute;w</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="blokad.0o">blokado</ref>
    <trd lng="be">&c_a;&c_b;&c_l;&c_o;&c_g;&c_a;</trd>
    <trd lng="bg">&c_o;&c_b;&c_s;&c_a;&c_d;&c_a;</trd>
    <trd lng="de">Belagerung</trd>
    <trd lng="en">siege</trd>
    <trd lng="fa">&a_m;&a_H;&a_A;&a_S;&a_r;&a_h;</trd>
    <trd lng="it">assedio</trd>
    <trdgrp lng="pl">
      <trd>okr&aogonek;&zdot;enie</trd>,
      <trd>obl&eogonek;&zdot;enie</trd>
    </trdgrp>
    <trd lng="fr">si&egrave;ge</trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#21253;&#x56F2;&#x653B;&#25731;  [<ind>&#x307B;&#12358;&#12356;&#12371;&#12358;&#12370;&#x304D;</ind>]</trd>,
    <trd>&#x57CE;&#x653B;&#12417;  [<ind>&#12375;&#x308D;&#x305B;&#12417;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">belegering</trd>
  <trd lng="uk">&#x43E;&c_b;&#x43B;&#x43E;&c_g;&c_a;</trd>
</drv>
</art>
