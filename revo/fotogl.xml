<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fotogl.xml,v 1.14 2025/01/22 10:28:40 revo Exp $">
  <kap>
  <rad>fotogliptik</rad>/o
  </kap>
<drv mrk="fotogl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="proced.0o">Procedo</ref> uzata precipe inter 1870 kaj
      1900 por altkvalite reprodukti fotojn en presado:
      <ekz>
        &ccirc;e la <tld/>o oni eksponas impresi&gcirc;eman &hcirc;roman gelatena&jcirc;on al lumo kaj poste
        premegas &gcirc;in sur plumban platon kreante reliefan strukturon, tiun oni uzas
        post kolorigo por represi la bildon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fotogr2.0i">fotogravuri</ref>,
      <ref cel="fototi.0o">fototipio</ref>,
      <ref cel="glipti.0o">gliptiko</ref>.
    </refgrp>
  </snc>
    <trd lng="be">&c_f;&c_a;&c_t;&c_a;&c_g;&c_l;&c_ib;&c_f;&c_ib;&c_ja;</trd>
    <trdgrp lng="cs">
      <trd>fotoglyptika</trd>,
      <trd>sv&ecaron;tlorytectv&iacute;</trd>
    </trdgrp>
    <trd lng="de">Photoglyptie</trd>
  <trd lng="en">photoglyphy</trd>
  <trd lng="fr">photoglyptie</trd>
    <trd lng="ja">&#20889;&#x771F;&#x5F6B;&#x523B;&#29256;&#x6CD5; 
    <pr>&#12375;&#12419;&#12375;&#12435;&#12385;&#12423;&#12358;&#12371;&#x304F;&#x306F;&#12435;&#x307B;&#12358;</pr></trd>
  <trd lng="nl">fotoglyptie<klr tip="ind"> (arch.)</klr></trd>
    <trdgrp lng="pt">
      <trd>fotogliptia</trd>,
      <trd>fotoplastografia</trd>
    </trdgrp>
    <trd lng="sk">fotoglyptika</trd>
</drv>
</art>
