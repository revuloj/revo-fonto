  <trd lng="my">&#x1001;&#x1036;&#x101A;&#x1030;</trd>
  <trd lng="ne">&#x92A;&#x94D;&#x930;&#x93E;&#x92A;&#x94D;&#x924;</trd>
  <trd lng="ny">alandire</trd>
  <trd lng="pa">&#xA2A;&#xA4D;&#xA30;&#xA3E;&#xA2A;&#xA24;</trd>
  <trd lng="ps">&#x62A;&#x631; &#x644;&#x627;&#x633;&#x647;</trd>
  <trd lng="pt">receber</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_l;&c_u;&c_ch;&c_i;&c_t;&c_mol;</trd>,
