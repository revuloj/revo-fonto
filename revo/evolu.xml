<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>evolu</rad>/i
</kap>
<drv mrk="evolu.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="evolu.0i.disvolvigxi">
    <dif>
      Iompostiome disvolvi&gcirc;i; regule kaj la&ubreve;nature kreskadi aliformi&gcirc;ante:
      <ekz>
        Esperanto <tld/>as la&ubreve; natura kaj rekta vojo<fnt>Z</fnt>..
      </ekz>
    </dif>
    <trd lng="de">sich <ind>entwickeln</ind></trd>
  </snc>
  <snc mrk="evolu.0i.sxangxigxi">
    <dif>
      Pasi tra diversaj iompostiomaj &scirc;an&gcirc;oj:
      <ekz>
        kun la tempo, lia karaktero <tld/>adis &gcirc;is ekstrema mildeco.
      </ekz>
    </dif>
    <trd lng="de">sich <ind>ver&auml;ndern</ind></trd>
  </snc>
  <trd lng="es">evolucionar</trd>
  <trd lng="en">evolve</trd>
  <trd lng="fr">&eacute;voluer</trd>
  <trd lng="hu">fejl&odblac;dik</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_v;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_e;&c_v;&c_o;&c_l;&c_ju;&c_c;&c_i;&c_o;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">utvecklas</trd>
</drv>

<drv mrk="evolu.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Igi ion evoluanta.
    </dif>
    <trd lng="de">entwickeln</trd>
    <trd lng="hu">fejleszt</trd>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_v;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
</drv>

<drv mrk="evolu.0o">
  <kap><tld/>o</kap>
  <snc mrk="evolu.0o.disvolvigxo">
    <dif>
      Iompostioma la&ubreve;natura disvolvi&gcirc;o kaj aliformi&gcirc;o:
      <ekz>
        <tld/>o de la bestoj, plantoj, lingvoj;
      </ekz>
      <ekz>
        la teorio de la <tld/>ado.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Entwicklung</trd>,
      <trd>Evolution<klr> [vivogenezo]</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="evolu.0o.sxangxigxo">
    <dif>
      Iompostioma &scirc;an&gcirc;i&gcirc;o:
      <ekz>
        la <tld/>o de la lingvaj teorioj, de la militaj metodoj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="proces.0o">proceso 2</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Ver&auml;nderung</trd>,
      <trd>Sichentwickeln</trd>,
      <trd>Evolution<klr> [fak]</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="es">evoluci&oacute;n</trd>
  <trd lng="fr">&eacute;volution</trd>
  <trd lng="hu">fejl&odblac;d&eacute;s</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_v;&c_o;&c_l;&c_ju;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_i;&c_t;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sv">utveckling</trd>
</drv>
</art>
