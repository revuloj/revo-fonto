  <trd lng="ro">evolua</trd>
  <trd lng="sk">vyv&iacute;ja&tcaron; sa</trd>
  <trd lng="sv">utvecklas</trd>
  <trdgrp lng="th">
    <trd>&#xE1E;&#xE31;&#xE12;&#xE19;&#xE32;</trd>,
    <trd>&#xE40;&#xE01;&#xE34;&#xE14;&#xE01;&#xE32;&#xE23;&#xE1E;&#xE31;&#xE12;&#xE19;&#xE32;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_z;&c_v;&c_i;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_je;&c_v;&c_o;&c_l;&c_ju;&c_c;&c_ib;&c_o;&c_n;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_b;&c_i;&c_t;&c_i; &c_je;&c_v;&c_o;&c_l;&c_ju;&c_c;&c_ib;&c_ju;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x53D8; <pr>bi&#xE0;n</pr></trd>,
    <trd>&#24930;&#24615; <pr>m&#xE0;nx&#xEC;ng</pr></trd>,
    <trd>&#x6ECB;&#38263; <pr>z&#x12B;zh&#x1CE;ng</pr></trd>,
    <trd>&#x6E10;&#x8FDB; <pr>ji&#xE0;nj&#xEC;n</pr></trd>,
    <trd>&#x751F;&#38263; <pr>sh&emacron;ngzh&#x1CE;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="evolu.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Igi ion <tld/>anta:
      <ekz>
        &scirc;i <tld/>igis la kverelon je arto
        <fnt>
          <bib>LGA</bib>
        </fnt>;
      </ekz>
      <ekz>
        li amis mediti, la vivo en la kamparo <tld/>igis en li &ccirc;i tiun kapablon
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>igis grandegan lertecon de la anta&ubreve;aj membroj kaj fari&gcirc;is veraj
        akrobatoj
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        la popoloj de la iam vasta Imperio <klr>[&#8230;]</klr> <tld/>igis siajn proprajn
        regionajn lingvojn
        <fnt>
          <bib>Ret</bib>
