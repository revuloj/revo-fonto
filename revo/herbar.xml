  <rad>herbari</rad>/o <fnt>Z</fnt> <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="herbar.0o">
  <kap><tld/>o</kap>
  <snc mrk="herbar.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Kolekto de vegeta&jcirc;oj, sekigitaj kaj ordigitaj
      kun klasifikaj informoj;
      <ref tip="dif" cel="kolekt.herbo0o">herbokolekto</ref>:
      <ekz>
        la fungojn &scirc;i jam malsekigis, poste rearan&gcirc;is sian <tld/>on
        <fnt>
          <aut>K. Buli&ccirc;ov, trad. E. Tikhonov</aut>
          <vrk><url ref="http://sxakludanto.narod.ru/la_setlejo.htm">
          La setlejo</url></vrk>
          <lok>[2011?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_je;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_g;&c_je;&c_r;&c_b;&c_a;&c_r;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="bg">&c_h;&c_je;&c_r;&c_b;&c_a;&c_r;&c_i;&c_j;</trd>
  <trdgrp lng="de">
    <trd>Herbarium</trd>,
    <trd>Sammlung getrockneter Pflanzen</trd>
  </trdgrp>
  <trd lng="el">&beta;&omikron;&tau;&alfa;&ny;&omikron;&lambda;&omikron_ton;&gamma;&jota;&omikron;</trd>
  <trd lng="en">herbarium</trd>
