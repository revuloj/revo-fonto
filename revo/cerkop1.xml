<art mrk="$Id: cerkop1.xml,v 1.14 2023/10/31 13:37:03 revo Exp $">
<kap>
  <rad>cerkopiteked</rad>/oj
</kap>
<drv mrk="cerkop1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref tip="malprt" cel="simi.0oj">simioj</ref>
      (<trd lng="la">Cercopithecidae</trd>):
      <ekz>
        la ursmakako a&ubreve; asama makako el la <klr>[&#x2026;]</klr> familio
        de <tld/>oj vivas en Junano kaj G<klr>[v]</klr>ang&scirc;io 
        de &Ccirc;inio kaj anka&ubreve; en Birmo, Vjetnamio, Barato, Nepalo,
        Butano kaj Sikimo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=49900">Ursmakako</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_t;&c_y;&c_sh;&c_k;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja;
  <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">cercopit&egrave;cids</trd>
  <trd lng="nl">meerkat</trd>
  <trdgrp lng="de">
    <trd>Meerkatzen</trd>,
    <trd>Hundsaffen</trd>
  </trdgrp>
  <trd lng="en">old world <ind>monkeys</ind></trd>
  <trd lng="fr">cercopith&egrave;que</trd>
  <trd lng="hu">cerk&oacute;fmajomf&eacute;l&eacute;k</trd>
  <trd lng="it">cercopitecini</trd>
  <trdgrp lng="pl">
    <trd>w&aogonek;skonose<klr> (nadrodzina)</klr></trd>,
    <trd>ma&lstroke;py Starego &Sacute;wiata<klr>
    (nadrodzina)</klr></trd>,
    <trd>makakowate<klr> (rodzina)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_a;&c_r;&c_t;&c_y;&c_sh;&c_k;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;
  <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="zh">&#x6D77;&#x732B; <pr>h&#x1CE;im&#x101;o</pr></trd>
</drv>
</art>

