  <art mrk="$Id: tuniz.xml,v 1.9 2024/05/10 10:43:48 revo Exp $">
    <kap>
      <rad>Tuniz</rad>/o <fnt><bib>PIV1</bib></fnt>,
      <var><kap><rad var="s">Tunis</rad>/o <fnt><bib>FK</bib></fnt></kap></var>
    </kap>

<drv mrk="tuniz.0o">
  <kap><tld/>o, <var><kap><tld var="s"/>o</kap></var></kap>
  <snc mrk="tuniz.0o.GEOG">
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&Ccirc;efurbo</ref> 
      de <ref tip="malprt" cel="tunizi.T0o">Tunizio</ref>:
      <ekz>
        la Neapola re&gcirc;o faras lin estro de la ekspedicio kontra&ubreve; 
        la piratoj de <tld var="s"/>o
        <fnt><bib>FK</bib></fnt>;
      </ekz><ekz>
        <tld var="s"/>o&#x202F;&#x2010;&#x202F;Tunisio
        <fnt><bib>LR</bib>. <lok>2. Vortuzo</lok></fnt>;
      </ekz><ekz>
        monda pintkonferenco pri komunikada socio en <tld/>o en novembro
        2005
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2006/008867.php">Primitive</url></vrk>
        </fnt>;
      </ekz>  
      <ekz>
        <nom>Ibn-&Hcirc;aldun</nom> <klr>[&#x2026;]</klr> estis naskita en <tld/>o en
        1332
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=7023">Ibn-&Hcirc;aldun</url></lok>
        </fnt>.
      </ekz>    
    </dif>
    <trd lng="be">&c_T;&c_u;&c_n;&c_ib;&c_s;<klr> (&c_g;&c_o;&c_r;&c_a;&c_d;)</klr></trd>
    <trd lng="de">Tunis</trd>
    <trd lng="fr">Tunis</trd>
    <trd lng="ru">&c_T;&c_u;&c_n;&c_i;&c_s;<klr> (&c_g;&c_o;&c_r;&c_o;&c_d;)</klr></trd>
    <trd lng="zh">&#x7A81;&#x5C3C;&#x65AF; <pr>T&#x16B;n&#xED;s&#x12B;</pr></trd>
</snc>
  <trd lng="ja">&#x30C1;&#x30E5;&#x30CB;&#x30B9;</trd>
  <trd lng="uk">&#x422;&#x443;&#x43D;&#x456;&#x441;</trd>
