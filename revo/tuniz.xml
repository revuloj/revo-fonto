<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: tuniz.xml,v 1.4 2021/04/15 13:07:25 revo Exp $">
    <kap>
      <rad>Tuniz</rad>/o <fnt><bib>PIV1</bib></fnt>,
      <var><kap><rad var="s">Tunis</rad>/o <fnt><bib>FK</bib></fnt></kap></var>
    </kap>

<drv mrk="tuniz.0o">
  <kap><tld/>o, <var><kap><tld var="s"/>o</kap></var></kap>
  <snc mrk="tuniz.0o.GEOG">
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&ccirc;efurboj"
      cel="urb.cxef0o.POL">&Ccirc;efurbo</ref> 
      de <ref tip="malprt" cel="tunizi.T0o">Tunizio</ref>:
      <ekz>
        la Neapola re&gcirc;o faras lin estro de la ekspedicio kontra&ubreve; 
        la piratoj de <tld var="s"/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld var="s"/>o&#8239;&dash;&#8239;Tunisio
        <fnt>
          <bib>LR</bib>
          <lok>2. Vortuzo</lok>
        </fnt>;
      </ekz>
      <ekz>
        monda pintkonferenco pri komunikada socio en <tld/>o en novembro
        2005
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.net/2006/008867.php"
              >Primitive</url></vrk>
        </fnt>;
      </ekz>  
      <ekz>
        <nom>Ibn-&Hcirc;aldun</nom> <klr>[&#8230;]</klr> estis naskita en <tld/>o en
        1332
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=7023"
              >Ibn-&Hcirc;aldun</url></lok>
        </fnt>.
      </ekz>    
    </dif>
    <trd lng="be">&c_T;&c_u;&c_n;&c_ib;&c_s; <klr>(&c_g;&c_o;&c_r;&c_a;&c_d;)</klr></trd>
    <trd lng="de">Tunis</trd>
    <trd lng="fr">Tunis</trd>
  </snc>
</drv>

  </art>

