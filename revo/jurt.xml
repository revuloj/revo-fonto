<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: jurt.xml,v 1.13 2023/12/03 07:09:19 revo Exp $">
<kap>
  <rad>jurt</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="jurt.0o">
  <kap><tld/>o</kap>
  <snc mrk="jurt.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Granda familia <ref tip="super" cel="tend.0o">tendo</ref> el ligna
      framo kaj felta tego tradicie uzata de centraziaj nomadoj:
      <ekz>
        la nomadaj mongoloj kaj kaza&hcirc;oj lo&gcirc;as en munteblaj
        <tld/>oj
        <fnt>
          <bib>EncCxi</bib>,
          <lok>&ccirc;ap. &leftquot;Lo&gcirc;ejoj&rightquot;, p. 149a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="super" cel="logx.0ejo">lo&gcirc;ejo</ref>
  <trd lng="be">&c_ju;&c_r;&c_t;&c_a;</trd>
  <trd lng="de">Jurte</trd>
  <trd lng="fr">yourte</trd>
  <trd lng="it">yurta</trd>
  <trd lng="hu">jurta</trd>
  <trdgrp lng="pt">
    <trd>iurta</trd>,
    <trd>iurte</trd>
  </trdgrp>
  <trd lng="ru">&c_ju;&c_r;&c_t;&c_a;</trd>
  <trd lng="sv">jurt</trd>
  <trd lng="sk">jurta</trd>
  <trdgrp lng="cs">
    <trd>ch&#xFD;&#x161;e</trd>,
    <trd>jurta</trd>,
    <trd>stan</trd>
  </trdgrp>
  <trd lng="uk">&#x44E;&#x440;&#x442;&#x430;</trd>
  <trdgrp lng="zh">
    <trd>&#x5ED9; <pr>y&#xEC;</pr></trd>,
    <trd>&#x5706;&#x9876;&#x5E10;&#x7BF7; <pr>yu&#xE1;nd&#x1D0;ngzh&#xE0;ngpeng</pr></trd>,
    <trd>&#x8499;&#x53E4;&#x5305; <pr>m&#xE9;ngg&#x1D4;b&#x101;o</pr></trd>,
    <trd>&#x6BE1;&#x623F; <pr>zh&#x101;nf&#xE1;ng</pr></trd>
  </trdgrp>
</drv>
</art>

