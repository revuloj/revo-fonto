<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>jurt</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>

    <drv mrk="jurt.0o">
      <kap><tld/>o</kap>
      <snc>
    	<dif>
    	  Granda familia <ref tip="super" cel="tend.0o">tendo</ref> el ligna
    	  framo kaj felta tego tipe uzata de centraziaj nomadoj:
    	  <ekz>
    	    la nomadaj mongoloj kaj kaza&hcirc;oj lo&gcirc;as en munteblaj
    	    <tld/>oj<fnt><bib>EncCxi</bib> <lok>&ccirc;ap. &leftquot;Lo&gcirc;ejoj&rightquot;,
    	    p. 149a</lok></fnt>.
    	  </ekz>
    	</dif>
      </snc>
      <ref tip="super" cel="logx.0ejo">lo&gcirc;ejo</ref>
      <trd lng="be">&c_ju;&c_r;&c_t;&c_a;</trd>
      <trdgrp lng="fr">
    	<trd>yourte</trd>
      </trdgrp>
      <trd lng="hu">jurta</trd>
  <trdgrp lng="pt">
    <trd>iurta</trd>,
    <trd>iurte</trd>
  </trdgrp>
      <trd lng="ru">&c_ju;&c_r;&c_t;&c_a;</trd>
      <trd lng="sv">jurt</trd>
    </drv>
  </art>

