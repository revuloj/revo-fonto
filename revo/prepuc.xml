<art mrk="$Id: prepuc.xml,v 1.26 2024/02/17 07:52:36 revo Exp $">
<kap>
  <rad>prepuci</rad>/o
  <fnt><bib>MT</bib></fnt>
</kap>
<drv mrk="prepuc.0o">
  <kap><tld/>o</kap>
  <snc mrk="prepuc.0o.penisa">
    <uzo tip="fak">ANA</uzo>
    <dif>
      La retirebla ha&ubreve;tfaldo, kiu kovras kaj protektas la <ref tip="malprt" cel="penis.0o">penisan</ref> <ref tip="vid" cel="glan.0o.penisa">glanon<sncref/></ref>:
      <ekz>
        cirkumcidu la karnon de via <tld/>o, kaj tio estu signo de
        interligo inter Mi kaj vi
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 17:11</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="af">voorhuid</trd>
    <trdgrp lng="be">
      <trd>&c_k;&c_r;&c_a;&c_j;&c_n;&c_ja;&c_ja; &c_p;&c_l;&c_o;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_k;&c_u;&c_r;&c_a;&c_v;&c_ib;&c_n;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="cs">p&rcaron;edko&zcaron;ka</trd>
    <trd lng="de">Vorhaut</trd>
    <trd lng="en">foreskin</trd>
    <trd lng="fa">&a_gh;&a_l;&a_f;&a_h;</trd>
    <trd lng="fy">foarh&ucirc;d</trd>
    <trd lng="he">&ayin;&vav;&resh;&lamed;&he;</trd>
    <trdgrp lng="hu">
      <trd>fityma</trd>,
      <trd>el&odblac;b&odblac;r</trd>
    </trdgrp>
    <trd lng="is">forh&uacute;&#xF0;</trd>
    <trd lng="iu">&#x1439;&#x14EF;&#x14C2;&#x1585;</trd>
    <trd lng="nl">voorhuid</trd>
    <trd lng="pl">napletek</trd>
    <trd lng="ru">&c_k;&c_r;&c_a;&c_j;&c_n;&c_ja;&c_ja; &c_p;&c_l;&c_o;&c_t;&c_mol;</trd>
    <trd lng="sk">predko&zcaron;ka</trd>
    <trd lng="sv">f&ouml;rhud</trd>
    <trd lng="sw">govi</trd>
  </snc>
  <snc mrk="prepuc.0o.klitora">
    <uzo tip="fak">ANA</uzo>
    <dif>
      La ha&ubreve;tfaldo, kiu kovras kaj protektas la <ref tip="vid" cel="klitor.0o">klitoran</ref> <ref tip="vid" cel="glan.0o.klitora">glanon<sncref/></ref>.
    </dif>
    <trd lng="ca">prepuci del cl&iacute;toris</trd>
    <trd lng="en">clitoral hood</trd>
    <trd lng="es">prepucio del cl&iacute;toris</trd>
    <trd lng="fr">capuchon du clitoris</trd>
    <trd lng="it">prepuzio clitorideo</trd>
    <trd lng="nl">clitorishoed</trd>
  </snc>
  <trd lng="ca">prepuci</trd>
  <trd lng="en">prepuce</trd>
  <trd lng="es">prepucio</trd>
  <trd lng="fr">pr&eacute;puce</trd>
  <trd lng="io">prepuco</trd>
  <trd lng="it">prepuzio</trd>
  <trd lng="ja">&#x5305;&#x76AE;  <pr>&#x307B;&#x3046;&#x3072;</pr></trd>
  <trd lng="pt">prep&uacute;cio</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x440;&#x430;&#x439;&#x43D;&#x44F; &#x43F;&#x43B;&#x43E;&#x442;&#x44C;</trd>,
    <trd>&#x43F;&#x440;&#x435;&#x43F;&#x443;&#x446;&#x456;&#x443;&#x43C;</trd>,
    <trd>&#x43F;&#x440;&#x435;&#x43F;&#x443;&#x446;&#x456;&#x439;</trd>
  </trdgrp>
  <trd lng="vo">prep&uuml;d</trd>
  <trd lng="zh">&#x5305;&#x76AE; <pr>b&#x101;op&#xED;</pr></trd>
</drv>
</art>
