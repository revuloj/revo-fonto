<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kantabr.xml,v 1.5 2022/05/01 08:09:23 revo Exp $">
<kap>
<rad>kantabr</rad>/o
</kap>

<drv mrk="kantabr.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ano de la &ccirc;efa gento de <tld lit="K"/>ujo.
      <ekz>
        <nom>Li&eacute;rganes</nom> &dash; homoj en Kadizo ne konis tiun &ccirc;i loknomon, sed <tld/>o laboranta en
        Kadizo menciis, ke temas pri vila&gcirc;o en Kantabrio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=447864">Fi&scirc;homo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_t;&c_a;&c_b;&c_r;&c_y;&c_je;&c_c;</trd>
  <trd lng="en">Cantabria</trd>
  <trdgrp lng="io">
    <trd>Kantabriano</trd>,
    <trd>Kantabro</trd>
  </trdgrp>
  <trd lng="tok">jan Kantapija</trd>
</drv>

<drv mrk="kantabr.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj a&ubreve; <tld lit="K"/>ujo:
      <ekz>
        la&ubreve; la <tld/>a marbordo la pilgrimantoj venis al Ribadeo, galega urbo, proksima al la astura
        limo
        <fnt>
          <bib>Monato</bib>
          <aut>Rafaela Urue&ntilde;a</aut>
          <vrk><url ref="&Steloj;monato2/006809.html">Hispanio: Vojoj al la
          sankta urbo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        post la <tld lit="K"/>aj militoj kaj malvenko de <tld/>oj kontra&ubreve; Romio (en la jaro -19)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=447905">Cantabrum</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_t;&c_a;&c_b;&c_r;&c_y;&c_j;&c_s;&c_k;&c_ib;</trd>
  <trd lng="de">kantabrisch</trd>
  <trd lng="en">Cantabrian</trd>
  <trdgrp lng="io">
    <trd>Kantabriana</trd>,
    <trd>Kantabra</trd>
  </trdgrp>
  <trd lng="nl">Cantabrisch</trd>
  <trd lng="no">kantabrisk</trd>
</drv>

<drv mrk="kantabr.K0ujo">
  <kap><tld lit="K"/>ujo</kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      A&ubreve;tonoma regiono situanta en Hispanujo:
      <ekz>
        busto de <nom>Federico Garc&iacute;a Lorca</nom> en <nom>Santo&ntilde;a</nom>, <tld lit="K"/>ujo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1284">Federico Garc&iacute;a Lorca</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_K;&c_a;&c_n;&c_t;&c_a;&c_b;&c_r;&c_y;&c_ja;</trd>
  <trd lng="ca">Cant&agrave;bria</trd>
  <trd lng="de">Kantabrien</trd>
  <trd lng="en">Cantabria</trd>
  <trd lng="es">Cantabria</trd>
  <trd lng="eu">Kantabria</trd>
  <trd lng="fa">&f_k;&a_A;&a_n;&a_t;&a_A;&a_b;&a_r;&f_y;&a_A;</trd>
  <trd lng="fr">Cantabrie</trd>
  <trd lng="fy">Kantaabrje</trd>
  <trd lng="gl">Cantabria</trd>
  <trd lng="io">Kantabria</trd>
  <trd lng="it">Cantabria</trd>
  <trd lng="ko">&#52856;&#53440;&#48652;&#47532;&#50500; &#51648;&#48169;</trd>
  <trd lng="nl">Cantabri&euml;</trd>
  <trdgrp lng="no">
    <trd>Cantabria</trd>,
    <trd>Kantabria</trd>
  </trdgrp>
  <trd lng="oc">Cant&agrave;bria</trd>
  <trd lng="pl">Kantabria</trd>
  <trd lng="pt">Cant&aacute;bria</trd>
  <trd lng="ru">&c_K;&c_a;&c_n;&c_t;&c_a;&c_b;&c_r;&c_i;&c_ja;</trd>
  <trd lng="sk">Kant&aacute;bria</trd>
  <trd lng="sv">Kantabrien</trd>
  <trd lng="tok">ma Kantapija</trd>
  <trd lng="tr">Kantabria</trd>
</drv>
</art>

