<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: manipu.xml,v 1.18 2020/06/23 21:10:54 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>manipul</rad>/i
</kap>
<drv mrk="manipu.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="manipu.0i.KOMUNE">
    <dif>
      Mane preni, uzi, funkciigi ilon, aparaton:
      <ekz>
        <tld/>i penikon;
      </ekz>
      <ekz>
        la subhejtisto <klr>...</klr> montris al mi kiel <tld/>i la
        vin&ccirc;on
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>dua libro, &ccirc;apitro
        28a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>bedienen</trd>,
      <trd>handhaben</trd>
    </trdgrp>
    <trd lng="en">manipulate</trd>
    <trd lng="es">manipular</trd>
    <trd lng="hu">kezel</trd>
    <trdgrp lng="nl">
      <trd>hanteren</trd>,
      <trd>manipuleren</trd>
    </trdgrp>
    <trd lng="pt">manipular</trd>
  </snc>
  <snc mrk="manipu.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      La&ubreve;cele kaj la&ubreve;vole uzi ion:
      <ekz>
        lerte <tld/>i la lingvon, la homojn;
      </ekz>
      <ekz>
        malproksime de la re&gcirc;urbo Kopenhago kaj de la kleraj
        bonintencaj
        regantoj, oni ofte ankora&ubreve; <tld/>adis kun la le&gcirc;o arbitre
        <fnt><bib>Fab4</bib>, <lok>historio el la dunoj</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="manovr.0i">manovri</ref>.
    </refgrp>
    <trdgrp lng="hu">
      <trd>kezel</trd>,
      <trd>manipul&aacute;l</trd>
    </trdgrp>
    <trd lng="nl">omgaan met</trd>
    <trd lng="pt">manipular</trd>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_ib;&c_p;&c_u;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="br">embreger</trd>
  <trd lng="ca">manipular</trd>
  <trdgrp lng="cs">
    <trd>manipulovat</trd>,
    <trd>ovl&aacute;dat n&ecaron;co</trd>,
    <trd>signalizovat telegrafn&iacute;m kl&iacute;&ccaron;em</trd>,
    <trd>&ccaron;achrovat</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>handle</trd>,
    <trd>manage</trd>,
    <trd>use</trd>
  </trdgrp>
  <trd lng="fr">manipuler</trd>
  <trdgrp lng="pl">
    <trd>manipulowa&cacute;</trd>,
    <trd>obchodzi&cacute; si&eogonek;</trd>,
    <trd>w&lstroke;ada&cacute;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>manipula</trd>,
    <trd>trata</trd>,
    <trd>guverna</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_a;&c_n;&c_i;&c_p;&c_u;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>manipulova&tcaron;</trd>,
    <trd>nar&aacute;ba&tcaron;</trd>,
    <trd>zaobch&aacute;dza&tcaron;</trd>
  </trdgrp>
</drv>
</art>
