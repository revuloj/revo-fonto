<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>manipul</rad>/i
</kap>
<drv mrk="manipu.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Funkciigi ilon, aparaton uzante la manojn, manuzi:
      <ekz>
        <tld/>i penikon.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>bedienen</trd>,
      <trd>handhaben</trd>
    </trdgrp>
    <trd lng="hu">kezel</trd>
    <trdgrp lng="nl">
      <trd>hanteren</trd>,
      <trd>manipuleren</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      La&ubreve;cele praktike uzi ion:
      <ekz>
        lerte <tld/>i la lingvon, la homojn.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="manovr.0i">manovri</ref>.
    </refgrp>
    <trdgrp lng="hu">
      <trd>kezel</trd>,
      <trd>manipul&aacute;l</trd>
    </trdgrp>
    <trd lng="nl">omgaan met</trd>
  </snc>
  <trd lng="ru">&c_m;&c_a;&c_n;&c_i;&c_p;&c_u;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
