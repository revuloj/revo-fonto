<vortaro>
<art mrk="$Id: konkrement.xml,v 1.8 2023/03/10 06:35:52 revo Exp $">
<kap>
  <rad>konkrement</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="konkrement.0o">
  <kap><tld/>o</kap>
  <snc mrk="konkrement.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Nenormala solida&jcirc;o iom post iom formi&gcirc;anta
      en kava organo a&ubreve; en dukto:
      <ref tip="sub" cel="bezoar.0o.FIZL">bezoaro</ref>,
      <ref tip="sub" cel="sxton.0o.MED">&scirc;tono</ref>...:
      <ekz>
        <nom>Eisenbarth</nom> <klr>[&#8230;]</klr> speciali&gcirc;is pri <klr>[&#8230;i.a.]</klr> <tld/>a kirurgio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=612964"
              >Johann Andreas Eisenbarth</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>oj <klr>[&#8230;]</klr> demeti&gcirc;as &ccirc;irka&ubreve; la artikoj
        <fnt>
          <aut>-</aut>,
          <vrk><url
          ref="http://www.naturkuracado.info/51088195f60b7be04/51088195f60cec936/51088195f70f946fa.html">
          Podagra artrito</url></vrk>,
          <lok>Naturkuracado.info, [2007]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="konkreci.0o.KEM">konkrecio</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_n;&c_k;&c_r;&c_e;&c_m;&c_e;&c_n;&c_t;</trd>,
    <trd>&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol; <klr tip="amb">(&c_u; &c_n;&c_y;&c_r;&c_c;&c_y;, &c_p;&c_je;&c_ch;&c_a;&c_n;&c_ib;)</klr></trd>
  </trdgrp>
  <trd lng="ca">c&agrave;lcul<klr> (patol.)</klr></trd>
  <trdgrp lng="cs">
    <trd>kam&iacute;nek</trd>,
    <trd>konkrement</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Konkrement</trd>,
    <trd>Stein <klr>(im K&ouml;rper)</klr></trd>
  </trdgrp>
  <trd lng="fr">calcul<klr> (m&eacute;d.)</klr></trd>
  <trd lng="pl">konkrement</trd>     
  <trd lng="ro">concre&#x021b;iilor</trd>
  <trd lng="sk">zhluk kry&scaron;t&aacute;lov</trd> 
</drv>
</art>

