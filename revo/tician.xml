<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: tician.xml,v 1.7 2022/03/28 12:03:04 revo Exp $">
    <kap><rad>Tician</rad>/o<fnt>Z</fnt></kap>

    <drv mrk="tician.0o">
      <kap><tld/>o</kap>
      <snc>
      <uzo tip="fak">PERS</uzo>
        <dif>
          Granda venecia pentristo (itale
          <trd lng="it">Tiziano Vecellio</trd>, 1488/1490&ndash;1576):
          <ekz>
            montri&gcirc;is la bildo de Venero, de la surtera Venero,
            tiel vivoplena kaj flama, kiel <tld/>o tion vidis
            <fnt><bib>Fab1</bib>, <lok>Kupra porko</lok></fnt>.         
              
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_T;&c_y;&c_c;&c_y;&c_ja;&c_n;</trd>
  <trd lng="ca">Tici&agrave;</trd>
  <trd lng="cs">Tizian <klr>(Tiziano Vecellio)</klr></trd>
  <trd lng="en">Titian</trd>
  <trdgrp lng="fr">
    <trd>Titien</trd>,
    <trd>le Titien</trd>
  </trdgrp>
  <trd lng="ru">&c_T;&c_i;&c_c;&c_i;&c_a;&c_n;</trd>
  <trd lng="sk">Tizian</trd>
  <trd lng="uk">&#x422;&#x456;&#x446;&#x456;&#x430;&#x43D; (&#x445;&#x443;&#x434;&#x43E;&#x436;&#x43D;&#x438;&#x43A;)</trd>
</drv>

  </art>

