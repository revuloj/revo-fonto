<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: brod.xml,v 1.23 2021/05/29 10:56:40 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>brod</rad>/i
</kap>
<drv mrk="brod.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <uzo tip="fak">TEKS</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kudrornami teksa&jcirc;ojn per reliefaj desegna&jcirc;oj:
      <ekz>
        el la lano panjo <tld/>is por mi leporon<fnt><aut>Spomenka
        &Scirc;timec</aut>: <vrk>Morto en Melburna tramo</vrk>,
        <bib>Monato</bib><lok>jaro 1997a, numero 8a, p. 24a</lok></fnt>.
      </ekz>
    </dif>
  <trd lng="be">&c_v;&c_y;&c_sh;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">brodar</trd>
  <trd lng="en">embroider</trd>
  <trd lng="es">bordar</trd>
  <trdgrp lng="fa">
    <trd>&f_g;&a_l;&a_d;&a_w;&a_z;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_q;&a_l;&a_A;&a_b;&zwnj;&a_d;&a_w;&a_z;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_s;&a_w;&a_z;&a_n;&zwnj;&a_d;&a_w;&a_z;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_b;&a_r;&a_w;&a_d;&a_r;&f_y;&zwnj;&a_d;&a_w;&a_z;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">broder</trd>
  <trd lng="hu">h&iacute;mez</trd>
  <trdgrp lng="pl">
    <trd>haftowa&cacute;</trd>,
    <trd>wyszywa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">bordar</trd>
  <trd lng="ru">&c_v;&c_y;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">brodera</trd>
  </snc>
  <trd lng="cs">vy&scaron;&iacute;vat</trd>
  <trd lng="he">&#x5DC;&#x5E8;&#x5E7;&#x5D5;&#x5DD;</trd>
  <trd lng="sk">vy&scaron;&iacute;va&tcaron;</trd>
</drv>

<drv mrk="brod.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Brodita desegna&jcirc;o.
    </dif>
  <trd lng="be">&c_v;&c_y;&c_sh;&c_y;&c_w;&c_k;&c_a;</trd>
  <trd lng="ca">brodat</trd>
  <trd lng="en">embroidery</trd>
  <trdgrp lng="es">
    <trd>bordado</trd>,
    <trd>bordadura</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&f_g;&a_l;&a_d;&a_w;&a_z;&f_y;</trd>,
    <trd>&a_q;&a_l;&a_A;&a_b;&zwnj;&a_d;&a_w;&a_z;&f_y;</trd>,
    <trd>&a_s;&a_w;&a_z;&a_n;&zwnj;&a_d;&a_w;&a_z;&f_y;</trd>,
    <trd>&a_b;&a_r;&a_w;&a_d;&a_r;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">broderie</trd>
  <trd lng="hu">h&iacute;mz&eacute;s</trd>
  <trd lng="pl">haft</trd>
  <trd lng="pt">bordado</trd>  
  <trd lng="ru">&c_v;&c_y;&c_sh;&c_i;&c_v;&c_k;&c_a;</trd>
  <trd lng="sv">broderi</trd>
  </snc>
  <trd lng="bo">&#xF55;&#xF62;&#xF0B;&#xF45;&#xF72;&#xF53;&#xF0B;</trd>
  <trd lng="he">&#x5E8;&#x5B4;&#x5E7;&#x5DE;&#x5B8;&#x5D4;</trd>
</drv>

<drv mrk="brod.0teksi">
  <kap><tld/>teksi</kap>
  <uzo tip="fak">TEKS</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Traplekti diverskolorajn metalajn a&ubreve; silkajn fadenojn, por
      formi
      desegna&jcirc;ojn.
    </dif>
    <refgrp tip="sin">
      <ref cel="brok.0i">broki</ref>.
    </refgrp>
  </snc>
  <trd lng="ca">fer un brocat</trd>
  <trd lng="cs">protkat</trd>
  <trd lng="en">brocade</trd>
  <trd lng="es">briscar</trd>
  <trdgrp lng="fa">
    <trd>&a_kh;&a_A;&a_m;&a_h;&zwnj;&a_d;&a_w;&a_z;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_z;&a_r;&a_d;&a_w;&a_z;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">brocher <klr tip="ind">(faire un brocart)</klr></trd>
  <trd lng="hu">&aacute;tsz&odblac; <klr tip="ind">(brok&aacute;tot k&eacute;sz&iacute;t)</klr></trd>
  <trd lng="pt">tecer brocado</trd>  
  <trd lng="sk">pretk&aacute;va&tcaron;</trd>
</drv>
</art>
