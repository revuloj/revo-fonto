<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: brod.xml,v 1.30 2023/10/30 09:28:05 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>brod</rad>/i
</kap>
<drv mrk="brod.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <uzo tip="fak">TEKS</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="kudr.0i">Kudr</ref>ornami teksa&jcirc;ojn per fadenaj figuroj:
      <ekz>
        faru <tld/>itan zonon
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 28:39</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ita vesto
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 16:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        vesto stel<tld/>ita
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        sur la &ccirc;ebrusta&jcirc;o vidi&gcirc;is skarlata,
        sur<tld/>ita figuro de fluganta kolombo
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        perlo<tld/>itaj &scirc;uoj
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        sur la sofo ku&scirc;is du <tld/>itaj kusenoj, sufi&ccirc;e beletaj
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;parmonujo</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la kurteno estis <tld/>ita per oro flugila globo kun du serpentoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;i <tld/>is mensogojn kaj trikis senpripensajn vortojn
        <fnt>
          <bib>Fab4</bib>
          <lok>Knabino, kiu pa&scirc;is sur panon</lok>
        </fnt>;
      </ekz>
      <ekz>
        jes, &scirc;i povosciis kudri, <tld/>i kaj triki, la maljuna avino
        <fnt>
          <bib>Fab4</bib>
          <lok>Knabino, kiu pa&scirc;is sur panon</lok>
        </fnt>!
      </ekz>
      <ekz>
        el la lano panjo <tld/>is por mi leporon
        <fnt><aut>Spomenka
        &Scirc;timec</aut>: <vrk>Morto en Melburna tramo</vrk>,
        <bib>Monato</bib><lok>jaro 1997a, numero 8a, p. 24a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_y;&c_sh;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="ca">brodar</trd>
    <trd lng="en">embroider</trd>
    <trd lng="es">bordar</trd>
    <trdgrp lng="fa">
      <trd>&f_g;&a_l;&a_d;&a_w;&a_z;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>,
      <trd>&a_q;&a_l;&a_A;&a_b;&#x200C;&a_d;&a_w;&a_z;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>,
      <trd>&a_s;&a_w;&a_z;&a_n;&#x200C;&a_d;&a_w;&a_z;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>,
      <trd>&a_b;&a_r;&a_w;&a_d;&a_r;&f_y;&#x200C;&a_d;&a_w;&a_z;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
    </trdgrp>
    <trd lng="fr">broder</trd>
    <trd lng="hu">h&iacute;mez</trd>
    <trdgrp lng="pl">
      <trd>haftowa&cacute;</trd>,
      <trd>wyszywa&cacute;</trd>
    </trdgrp>
    <trd lng="pt">bordar</trd>
    <trd lng="ru">&c_v;&c_y;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
    <trd lng="sv">brodera</trd>
  </snc>
  <trd lng="cs">vy&scaron;&iacute;vat</trd>
  <trd lng="de">sticken</trd>
  <trd lng="he">&lamed;&resh;&qof;&vav;&fmem;</trd>
  <trd lng="ja">&#x523A;&#x7E4D;&#12377;&#x308B;  <pr>&#12375;&#12375;&#12421;&#12358;&#12377;&#x308B;</pr></trd>
  <trd lng="sk">vy&scaron;&iacute;va&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_g;&c_a;&#x43F;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="brod.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Brodita desegna&jcirc;o:
      <ekz>
        bisino kun <tld/>a&jcirc;oj
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 26:36</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;apeletoj kun ri&ccirc;a perla <tld/>a&jcirc;o
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        li surhavis blankan &ccirc;efpastran mantelon kun orfadenaj sur<tld/>a&jcirc;oj,
        reprezentantaj drakosimilajn serpentojn
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_y;&c_sh;&c_y;&c_w;&c_k;&c_a;</trd>
    <trd lng="ca">brodat</trd>
    <trd lng="en">embroidery</trd>
    <trdgrp lng="es">
      <trd>bordado</trd>,
      <trd>bordadura</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&f_g;&a_l;&a_d;&a_w;&a_z;&f_y;</trd>,
      <trd>&a_q;&a_l;&a_A;&a_b;&#x200C;&a_d;&a_w;&a_z;&f_y;</trd>,
      <trd>&a_s;&a_w;&a_z;&a_n;&#x200C;&a_d;&a_w;&a_z;&f_y;</trd>,
      <trd>&a_b;&a_r;&a_w;&a_d;&a_r;&f_y;</trd>
    </trdgrp>
    <trd lng="fr">broderie</trd>
    <trd lng="hu">h&iacute;mz&eacute;s</trd>
    <trd lng="pl">haft</trd>
    <trd lng="pt">bordado</trd>
    <trd lng="ru">&c_v;&c_y;&c_sh;&c_i;&c_v;&c_k;&c_a;</trd>
    <trd lng="sv">broderi</trd>
  </snc>
  <trd lng="bo">&t_pha;&t_ra;&t_tsheg;&t_ca;&t_gigu;&t_na;&t_tsheg;</trd>
  <trd lng="de">Stickerei</trd>
  <trd lng="he">&resh;&#x5B4;&qof;&mem;&#x5B8;&he;</trd>
  <trdgrp lng="ja">
    <trd>&#x523A;&#x7E4D;&#x54C1;  <pr>&#12375;&#12375;&#12421;&#12358;&#12375;&#x306A;</pr></trd>,
    <trd>&#x523A;&#x7E4D;&#x7D30;&#x5DE5;  <pr>&#12375;&#12375;&#12421;&#12358;&#12373;&#12356;&#x304F;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_sh;&c_i;&c_v;&#x43A;&c_a;</trd>,
    <trd>&c_g;&c_a;&#x43F;&c_t;&c_u;&c_v;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_g;&c_a;&#x43F;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="brod.0teksi">
  <kap><tld/>teksi</kap>
  <uzo tip="fak">TEKS</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Interteksi diverskolorajn metalajn a&ubreve; silkajn fadenojn, por
      formi desegna&jcirc;ojn:
      <ekz>
        vestitaj per ore <tld/>teksitaj &scirc;tofoj
        <fnt>
          <bib>Kandid</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="brok.0i">broki</ref>
    </refgrp>
    <ref tip="vid" cel="gobelen.0o">gobeleno</ref>
  </snc>
  <trd lng="be">&c_t;&c_k;&c_a;&c_c;&c_mol; <klr>(&c_r;&c_o;&c_z;&c_n;&c_a;&c_k;&c_a;&c_l;&c_ja;&c_r;&c_o;&c_v;&c_y;&c_m;&c_ib; &c_n;&c_ib;&c_t;&c_k;&c_a;&c_m;&c_ib;)</klr></trd>
  <trd lng="ca">fer un brocat</trd>
  <trd lng="cs">protkat</trd>
  <trd lng="de">wirken <klr>(Teppich)</klr></trd>
  <trd lng="en">brocade</trd>
  <trd lng="es">briscar</trd>
  <trdgrp lng="fa">
    <trd>&a_kh;&a_A;&a_m;&a_h;&#x200C;&a_d;&a_w;&a_z;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_z;&a_r;&a_d;&a_w;&a_z;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">brocher <klr tip="ind">(faire un brocart)</klr></trd>
  <trd lng="hu">&aacute;tsz&odblac; <klr tip="ind">(brok&aacute;tot k&eacute;sz&iacute;t)</klr></trd>
  <trd lng="ja">&#x7D0B;&#x7E54;&#x308A;&#x306B;&#12377;&#x308B; 
  <pr>&#12418;&#12435;&#x304A;&#x308A;&#x306B;&#12377;&#x308B;</pr></trd>
  <trd lng="pt">tecer brocado</trd>
  <trd lng="sk">pretk&aacute;va&tcaron;</trd>
</drv>
</art>
