<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: diakil.xml,v 1.16 2019/04/25 06:10:15 revo Exp $">
<kap>
  <rad>diakil</rad>/o,
  <var><kap><rad>diakilon</rad>/o <fnt><bib>PV</bib></fnt></kap></var>
</kap>
<drv mrk="diakil.0o">
  <kap><tld/>o,
    <var><kap>diakilono</kap></var>
  </kap>
  <snc mrk="diakil.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="plastr.0o">Plastro</ref> el aglutina substanco
      de plumbo(II)-oksiko <klr>(<frm>2PbO</frm>)</klr> kaj plantaj substancoj,
      uzita en E&ubreve;ropo precipe en la 19a jc:
      <ekz>
        fine de la 19a jc laboristinoj eltrovis, ke <tld/>on oni ne nur povas meti sur
        vundon, sed anka&ubreve; gluti por aborto per la plumba veneni&gcirc;o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_h;&c_ib;&c_l;&c_jo;&c_n;</trd>
  <trd lng="ca">diaquil&oacute;</trd>
  <trd lng="cs">n&aacute;plast</trd>
  <trdgrp lng="de">
    <trd>Diachylonpflaster</trd>,
    <trd>Bleipflaster</trd>
  </trdgrp>
  <trd lng="es">diaquil&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>diachylon</trd>,
    <trd>diachylum</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&oacute;lomtapasz</trd>,
    <trd>diachylum</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>plaster<klr> samoprzylepny</klr></trd>,
    <trd>przylepiec</trd>
  </trdgrp>
  <trd lng="pt">diaquil&atilde;o</trd>
  <trd lng="ru">&c_d;&c_i;&c_a;&c_h;&c_i;&c_l;&c_o;&c_n;</trd>
  <trd lng="sk">n&aacute;plas&tcaron;</trd>
</drv>
</art>
