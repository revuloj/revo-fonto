<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: silikv.xml,v 1.23 2024/03/24 20:21:57 revo Exp $">
<kap>
  <rad>silikv</rad>/o
</kap>
<drv mrk="silikv.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Dufaka <ref tip="super" cel="frukt.malferm0o">malfermfrukto</ref>
      de la <ref tip="malprt" cel="brasik1.0oj">brasikacoj</ref>,
      esti&gcirc;anta el du fruktofolioj kaj havanta mezan vandon
      sur kiu situas la semgrajnoj:
      <ekz>
        sinapoj estas unu a&ubreve; plurjaraj herboj kun flavaj odoraj floroj kaj kun <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4017">Sinapo</url></lok>
        </fnt>.
      </ekz>   
    </dif>
    <rim>
      Ne konfuzu kun <ref tip="vid" cel="gusx.0o">gu&scirc;o</ref>.
    </rim>
    <bld lok="&WCU;/thumb/9/9b/Diplotaxis_tenuifolia_sl4.jpg/180px-Diplotaxis_tenuifolia_sl4.jpg" prm="CC BY-SA 4.0">malfermita <tld/>o de Diplotaxis tenuifolia
        <fnt>
          <aut>Stefan.lefnaer, propra verko</aut>
          <lok><url ref="&WCW;/File:Diplotaxis_tenuifolia_sl4.jpg">&WCW;/File:Diplotaxis_tenuifolia_sl4.jpg</url></lok>
        </fnt>
    </bld>    
  </snc>
  <snc>
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="vid" cel="gusx.0o">Gu&scirc;o</ref>:
      <ekz>
        kvin verdaj pizoj <klr>[&#x2026;]</klr> en unu <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Kvin en unu silikvo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_r;&c_u;&c_k;</trd>
  <trd lng="bg">&c_sh;&c_u;&c_sh;&c_u;&c_l;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>lusk</trd>,
    <trd>&scaron;e&scaron;ule</trd>,
    <trd>&scaron;e&scaron;ulka</trd>
  </trdgrp>
  <trd lng="de">Schote</trd>
  <trdgrp lng="en">
    <trd>pod</trd>,
    <trd>siliqua</trd>
  </trdgrp>
  <trd lng="fr">silique</trd>
  <trd lng="hu">bec&odblac;</trd>
  <trd lng="ja">&#x89D2;&#x679C;  <pr>&#x304B;&#x3069;&#x304B;</pr></trd>
  <trd lng="nl">hauw</trd>
  <trd lng="pl">&lstroke;uszczyna</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_u;&c_ch;&c_o;&c_k;</trd>
  <trd lng="sk">struk <klr>(dvojkomorov&yacute;)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x442;&#x440;&#x443;&#x447;&#x43E;&#x43A;</trd>,
    <trd>&c_s;&#x442;&#x440;&#x443;&#x447;&#x435;&#x447;&#x43E;&#x43A;</trd>
  </trdgrp>
  <trd lng="zh">&#x7A03; <pr>f&#x16B;</pr></trd>
</drv>
</art>
