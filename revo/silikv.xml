<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: silikv.xml,v 1.17 2018/03/28 10:10:32 revo Exp $">
<kap>
  <rad>silikv</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="silikv.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Dufaka <ref tip="super" cel="frukt.malferm0o">malfermfrukto</ref>
      de la <ref tip="malprt" cel="brasik1.0oj">brasikacoj</ref>,
      esti&gcirc;anta el du fruktofolioj kaj havanta mezan vandon
      sur kiu situas la semgrajnoj:
      <ekz>
        <tld/>o de sinapo<fnt>B</fnt>
      </ekz>
    </dif>
    <rim>
      Ne konfuzu kun <ref tip="vid" cel="gusx.0o">gu&scirc;o</ref>.
    </rim>
  </snc>
  <snc>
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="vid" cel="gusx.0o">Gu&scirc;o</ref>:
      <ekz>
        5 pizoj en unu <tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_r;&c_u;&c_k;</trd>
  <trd lng="bg">&c_sh;&c_u;&c_sh;&c_u;&c_l;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>lusk</trd>,
    <trd>&scaron;e&scaron;ule</trd>,
    <trd>&scaron;e&scaron;ulka</trd>
  </trdgrp>
  <trd lng="de">Schote</trd>
  <trdgrp lng="en">
    <trd>pod</trd>,
    <trd>siliqua</trd>
  </trdgrp>
  <trd lng="fr">silique</trd>
  <trd lng="hu">bec&odblac;</trd>
  <trd lng="nl">hauw</trd>
  <trd lng="pl">&lstroke;uszczyna</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_u;&c_ch;&c_o;&c_k;</trd>
  <trd lng="sk">struk <klr>(dvojkomorov&yacute;)</klr></trd>
</drv>
</art>
