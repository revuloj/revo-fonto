<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  -<rad>in</rad>
</kap>
<subart>
  <dif>
    Sufikso markanta &ccirc;e la vivuloj la sekson organizitan por
    naski idojn:
  </dif>
  <snc>
    <ekz>
      hero<tld/>o
      <fnt><bib>F</bib>, <lok>&FE; 3, ekzerco de legado</lok></fnt>;
    </ekz>
    <ekz>
      Pa&ubreve;l<tld/>o
      <fnt><bib>F</bib>, <lok>&FE; 3, ekzerco de legado</lok></fnt>;
    </ekz>
    <ekz>
      patr<tld/>o;
    </ekz>
    <ekz>
      frat<tld/>o;
    </ekz>
    <ekz>
      &ccirc;eval<tld/>o;
    </ekz>
    <ekz>
      kok<tld/>o;
    </ekz>
    <ekz>
      amat<tld/>o;
    </ekz>
    <ekz>
      belul<tld/>o;
    </ekz>
    <ekz>
      virgul<tld/>o.
    </ekz>
  </snc>
  <trd lng="de"><klr>-</klr>in</trd>
  <trdgrp lng="fr">
    <trd>-e<klr tip="ind"> (suffixe f&eacute;minin)</klr></trd>,
    <trd>-esse<klr tip="ind"> (suffixe f&eacute;minin)</klr></trd>,
    <trd>-euse<klr tip="ind"> (suffixe f&eacute;minin)</klr></trd>
  </trdgrp>
  <trd lng="hu"><klr>-</klr>n&odblac;</trd>
  <trd lng="vo">ji-</trd>
</subart>

<subart>
  <dif>
    Samsignifa vortero uzata memstare:
  </dif>
  <drv mrk="in.0o">
    <kap><tld/>o</kap>
    <snc>
      <dif>
        Vivulo de idonaska sekso:
        <ekz>
          prenu po sep paroj virbestojn kaj iliajn <tld/>ojn
          <fnt><bib>MT</bib>, <lok>&Gen; 7:2</lok></fnt>;
        </ekz>
        <ekz>
          la plej dan&gcirc;era homo - malbona <tld/>' en domo
          <fnt><bib>PrV</bib></fnt>.
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="de">
      <trd>Weibchen</trd>,
      <trd>Weib</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>femme</trd>,
      <trd>femelle<klr tip="ind"> (subst.)</klr></trd>
    </trdgrp>
    <trd lng="en">feminine</trd>
    <trdgrp lng="hu">
      <trd>n&odblac;</trd>,
      <trd>n&odblac;st&eacute;ny</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>vrouw</trd>,
      <trd>vrouwtje</trd>,
      <trd>wijfje</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_zh;&c_je;&c_n;&c_shch;&c_i;&c_n;&c_a;</trd>,
      <trd>&c_s;&c_a;&c_m;&c_k;&c_a;</trd>,
      <trd>&c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o; &c_zh;&c_je;&c_n;&c_s;&c_k;&c_o;&c_g;&c_o; &c_p;&c_o;&c_l;&c_a;</trd>
    </trdgrp>
  </drv>
  <drv mrk="in.0a">
    <kap>ina</kap>
    <snc>
      <dif>
        Havanta la sekson de ino:
        <ekz>
          <tld/>aj floroj, organoj.
        </ekz>
      </dif>
      <trdgrp lng="fr">
        <trd>f&eacute;minin</trd>,
        <trd>femelle<klr tip="ind"> (adj.)</klr></trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>n&odblac;i</trd>,
        <trd>n&odblac;st&eacute;ny-</trd>
      </trdgrp>
    </snc>
    <snc mrk="in.0a.POE">
      <uzo tip="fak">POE</uzo>
      <dif>
        <klr>(pri <ref cel="rim.0o.versparto">rimo</ref>)</klr>
        Fini&gcirc;anta per senakcenta silabo;
        <klr>(pri <ref cel="cezur.0o">cezuro</ref>)</klr>
        sekvanta senakcentan silabon:
        <ekz>
          &leftquot;oro&rightquot;, &leftquot;koro&rightquot; konsistigas
          <tld/>an rimon.
        </ekz>
      </dif>
      <ref tip="vid" cel="vir.0a.POE">vira</ref>
      <trd lng="fr"><klr tip="amb">(rime) </klr>f&eacute;minine</trd>
      <trd lng="hu">n&odblac;i<klr tip="amb">(r&iacute;m)</klr></trd>
    </snc>
    <trd lng="de">weiblich</trd>
    <trd lng="nl">vrouwelijk</trd>
    <trd lng="ru">&c_zh;&c_je;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
    <trd lng="vo">jilik</trd>
  </drv>
  <drv mrk="in.0ismo">
    <kap><tld/>ismo</kap>
    <snc>
      <ref tip="dif" cel="femini.0o">feminismo</ref>
      <ekz>
        estas pluraj specoj de praktika &leftquot;<tld/>ismo&rightquot;, de pli malfortaj al
        pli radikalaj
        <fnt>
        <aut>R. Corsetti</aut>,
        <vrk>A question...</vrk>,
        <lok>soc.culture.esperanto, 1999-07-26</lok>
        </fnt>.
      </ekz>
    </snc>
    <trd lng="de">Feminismus</trd>
    <trd lng="es">feminismo</trd>
    <trd lng="fr">f&eacute;minisme</trd>
    <trd lng="hu">feminizmus</trd>
    <trd lng="ru">&c_f;&c_je;&c_m;&c_i;&c_n;&c_i;&c_z;&c_m;</trd>
  </drv>
</subart>
</art>
