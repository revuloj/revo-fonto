<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  -<rad>in</rad>
</kap>
<subart>
  <dif>
    Sufikso markanta &ccirc;e la vivuloj la sekson organizitan por
    naski idojn:
  </dif>
  <snc>
    <ekz>
      patr<tld/>o, frat<tld/>o, hund<tld/>o,
      &ccirc;eval<tld/>o, kok<tld/>o;
    </ekz>
    <ekz>
      amat<tld/>o, belul<tld/>o, virgul<tld/>o.
    </ekz>
  </snc>
  <trdgrp lng="fr">
    <trd>-esse<klr> (suffixe f&eacute;minin)</klr></trd>,
    <trd>-euse<klr> (suffixe f&eacute;minin)</klr></trd>
  </trdgrp>
</subart>

<subart>
  <dif>
    Samsignifa vortero uzata memstare:
  </dif>
  <drv mrk="in.0o">
    <kap><tld/>o</kap>
    <snc>
      <dif>
        Vivulo de idonaska sekso:
        <ekz>
          prenu po sep paroj virbestojn kaj iliajn <tld/>ojn
          <fnt><bib>MT</bib>, <lok>&Gen; 7:2</lok></fnt>;
        </ekz>
        <ekz>la plej dan&gcirc;era homo - malbona <tld/>' en domo
          <fnt><bib>PrV</bib></fnt></ekz>
      </dif>
    </snc>
    <trdgrp lng="de">
      <trd>Weibchen</trd>,
      <trd>Weib</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>femme</trd>,
      <trd>femelle<klr> (subst.)</klr></trd>
    </trdgrp>
    <trd lng="en">feminine</trd>
    <trdgrp lng="nl">
      <trd>vrouw</trd>,
      <trd>vrouwtje</trd>,
      <trd>wijfje</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_zh;&c_je;&c_n;&c_shch;&c_i;&c_n;&c_a;</trd>,
      <trd>&c_s;&c_a;&c_m;&c_k;&c_a;</trd>,
      <trd>&c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o; &c_zh;&c_je;&c_n;&c_s;&c_k;&c_o;&c_g;&c_o; &c_p;&c_o;&c_l;&c_a;</trd>
    </trdgrp>
  </drv>
  <drv mrk="in.0a">
    <kap>ina</kap>
    <snc>
      <dif>
        Havanta la sekson de ino:
        <ekz>
          <tld/>aj floroj, organoj.
        </ekz>
      </dif>
      <trdgrp lng="fr">
        <trd>f&eacute;minin</trd>,
        <trd>femelle<klr> (adj.)</klr></trd>
      </trdgrp>
    </snc>
    <snc mrk="in.0a.POE">
      <uzo tip="fak">POE</uzo>
      <dif>
        <klr>(pp <ref cel="rim.0o.versparto">rimo</ref>)</klr>
        Fini&gcirc;anta per senakcenta silabo;
        <klr>(pp <ref cel="cezur.0o">cezuro</ref>)</klr>
        sekvanta senakcentan silabon:
        <ekz>
          &leftquot;oro&rightquot;, &leftquot;koro&rightquot; konsistigas <tld/>ajn rimojn.
        </ekz>
      </dif>
      <ref tip="vid" cel="vir.0a.POE">vira</ref>
      <trdgrp lng="fr">
        <trd>f&eacute;minine<klr> (rime)</klr></trd>
      </trdgrp>
    </snc>
    <trd lng="de">weiblich</trd>
    <trd lng="nl">vrouwelijk</trd>
    <trd lng="ru">&c_zh;&c_je;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
  </drv>
</subart>
</art>
