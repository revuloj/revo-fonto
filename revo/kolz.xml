<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kolz</rad>/o
</kap>
<drv mrk="kolz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Varia&jcirc;o de <ref tip="super" cel="nap.0o">napo</ref>
      (<trd lng="la">Brassica napus var. napus</trd>), 
      kies semoj donas sengustan oleon.  
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_p;&c_s;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_mol;&c_z;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Raps</trd>
  <trdgrp lng="en">
    <trd>colza</trd>,
    <trd>winter <ind>rape</ind></trd>
  </trdgrp>
  <trd lng="fa">&f_k;&a_damma;&a_l;&a_z;&a_A;</trd>
  <trd lng="fr">colza</trd>
  <trd lng="hu">repce</trd>
  <trd lng="nl">koolzaad</trd>
  <trd lng="pt">colza</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_p;&c_s;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_mol;&c_z;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_a;&c_p;&c_s;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_mol;&c_z;&c_a;</trd>
  </trdgrp>
</drv>
</art>
