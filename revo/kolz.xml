<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kolz</rad>/o
</kap>
<drv mrk="kolz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Varia&jcirc;o de <ref tip="super" cel="nap.0o">napo</ref>
      (<trd lng="la">Brassica napus var. napus</trd>), 
      kies semoj donas sengustan oleon.  
    </dif>
  </snc>
  <trd lng="de">Raps</trd>
  <trdgrp lng="en">
    <trd>colza</trd>,
    <trd>winter <ind>rape</ind></trd>
  </trdgrp>
  <trd lng="nl">koolzaad</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_p;&c_s;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_mol;&c_z;&c_a;</trd>
  </trdgrp>
</drv>
</art>
