<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>program</rad>/o
    </kap>
    <drv mrk="progra.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Tabelo anoncanta la detalojn  de teatra&jcirc;o, koncerto,
          ceremonio, festo ktp.
        </dif>
      </snc>
      <snc>
        <dif>
          Tabelo de la temoj pritraktotaj  en kurso a&ubreve; pridemandotaj
          en ekzameno (instru<tld/>o, lerno<tld/>o).
        </dif>
      </snc>
      <snc>
        <dif>
          Listo de la demandoj pridiskutotaj en kunveno.
        </dif>
        <ref tip="sin" cel="ord.tag0o">tagordo</ref>
      </snc>
      <snc>
        <dif>
          Resumo de la politikaj intencoj de partio, ministraro a&ubreve;
          individuo.
        </dif>
        <refgrp tip="vid">
          <ref cel="deviz.0o">devizo</ref>,
          <ref cel="cel.0o">celo</ref>,
          <ref cel="plan.0o">plano 2</ref>.
        </refgrp>
      </snc>
      <snc mrk="progra.0o.komp">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Priskribo de la agoj plenumotaj de komputilo, en
          programlingvo akceptata de tiu komputilo a&ubreve; en &gcirc;ia
          ma&scirc;inkodo:
          <ekz>
            verki <tld/>on en C;
          </ekz>
          <ekz>
            traduki <tld/>on el C;
          </ekz>
          <ekz>
            bindi <tld/>on el moduloj;
          </ekz><ekz>
            lan&ccirc;i, plenumi, ruli <tld/>on;
          </ekz>
          <ekz mrk="progra.aplika.0o">
            <ind>aplika <tld/>o</ind>
            <klr>(kiun uzas ne<tld/>istoj)</klr>;
            <trd lng="en">application program</trd>
            <trd lng="ru">&c_p;&c_r;&c_i;&c_k;&c_l;&c_a;&c_d;&c_n;&c_a;&c_ja; &c_p;&c_r;&c_o;&c_g;&c_r;&c_a;&c_m;&c_m;&c_a;</trd>
          </ekz>
          <ekz mrk="progra.il0o">
            <ind>il<tld/>o</ind>
            <klr>(uzata por krei a&ubreve; funkciigi aplikajn <tld/>ojn)</klr>.
            <trd lng="en">tool program</trd>
            <trd lng="ru">&c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_p;&c_r;&c_o;&c_g;&c_r;&c_a;&c_m;&c_m;&c_a;</trd>
          </ekz>
        </dif>
        <ref tip="super" cel="algor.0o">algoritmo</ref>
        <refgrp tip="vid">
          <ref cel="traduk.0ilo">tradukilo</ref>,
          <ref cel="interp2.0ilo">interpretilo</ref>,
          <ref cel="bind.0ilo">bindilo</ref>.
        </refgrp>
      </snc>
      <trd lng="en">program</trd>
      <trd lng="ru">&c_p;&c_r;&c_o;&c_g;&c_r;&c_a;&c_m;&c_m;&c_a;</trd>
    </drv>
    <drv mrk="progra.0i">
      <kap><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Verki <tld/>on kiu faru ion:
          <ekz>
            <tld/>i sintaksan analizon, formorekonon;
          </ekz><ekz>
            <tld/>i por (sub, en) Unikso en C;
          </ekz><ekz>
            &ccirc;u tiu funkcio estas <tld/>ita a&ubreve; realigita aparate?
          </ekz>
        </dif>
      </snc>
      <trd lng="en">program</trd>
      <trd lng="ru">&c_p;&c_r;&c_o;&c_g;&c_r;&c_a;&c_m;&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </drv>
    <drv mrk="progra.0ado">
      <kap><tld/>ado</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="progra.0ado.KOMP">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Projektado, verkado, dokumentado kaj vartado de komputilaj
          <tld/>oj:
          <ekz>
            metodoj de <tld/>ado;
          </ekz>
          <ekz>
            strukturema, modulema, objektema <tld/>o.
          </ekz>
        </dif>
        <trdgrp lng="en">
          <trd>programming</trd>,
          <trd>software engineering</trd>
        </trdgrp>
        <trd lng="ru">&c_p;&c_r;&c_o;&c_g;&c_r;&c_a;&c_m;&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
      </snc>
      <snc mrk="progra.0ado.MAT">
        <uzo tip="fak">MAT</uzo>
        <dif>
          <klr>(anka&ubreve; <ctl>matematika <tld/>ado</ctl>)</klr> Parto
          de <ref tip="super" cel="operac.0esploro">operaciesploro</ref>,
          studanta problemojn pri funkciekstremoj super aroj de
          finidimensia vektorspaco, kiujn arojn difinas egala&jcirc;oj a&ubreve;
          kompara&jcirc;oj:
          <ekz>
            la <ctl>dorssakproblemo</ctl> kaj
            <ctl>la problemo pri voja&gcirc;a komizo</ctl> estas
            konataj problemoj de matematika <tld/>ado;
          </ekz>
          <ekz>
            dinamika, diskreta,
            konkava, konveksa, lineara, nelineara <tld/>o.
          </ekz>
        </dif>
        <trd lng="en">mathematical programming</trd>
        <trd lng="ru">&c_m;&c_a;&c_t;&c_je;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; &c_p;&c_r;&c_o;&c_g;&c_r;&c_a;&c_m;&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
      </snc>
    </drv>
    <drv mrk="progra.0aro">
      <kap><tld/>aro</kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          La programoj uzataj en komputilo (ofte kontraste al
          <ref tip="ant" cel="aparat.0aro.KOMP">aparataro</ref>):
          <ekz>
            <ref tip="sub" cel="progra.sistema.0aro">sistema <tld/>aro</ref>
          </ekz><ekz>
            <ref tip="sub" cel="progra.aplika.0aro">aplika <tld/>aro</ref>
          </ekz><ekz>
            komerca, &ccirc;iesa, senpaga, libera <tld/>aro;
          </ekz>
          <ekz>
            portebla <tld/>aro;
          </ekz>
          <ekz>
            vivciklo de <tld/>aro;
          </ekz>
          <ekz>
            <tld/>ara krizo.
          </ekz>
        </dif>
      </snc>
      <trd lng="en">software</trd>
      <trd lng="fr">logiciel</trd>
      <trd lng="pl">oprogramowanie</trd>
      <trd lng="ru">&c_p;&c_r;&c_o;&c_g;&c_r;&c_a;&c_m;&c_m;&c_n;&c_o;&c_je; &c_o;&c_b;&c_je;&c_s;&c_p;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
      <trd lng="tr">yaz&inodot;l&inodot;m</trd>
    </drv>
    <drv mrk="progra.0isto">
      <kap><tld/>isto</kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Fakulo pri <tld/>ado.
        </dif>
      </snc>
      <trd lng="en">programmer</trd>
      <trd lng="ru">&c_p;&c_r;&c_o;&c_g;&c_r;&c_a;&c_m;&c_m;&c_i;&c_s;&c_t;</trd>
    </drv>
    <drv mrk="progra.cel0o">
      <kap>cel<tld/>o</kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Komputila <tld/>o produkita per
          <ref tip="vid" cel="traduk.0ilo">tradukilo</ref> el
          <ref tip="ant" cel="progra.font0o">fontprogramo</ref>.
        </dif>
      </snc>
      <trdgrp lng="en">
        <trd>object program</trd>,
        <trd>target program</trd>
      </trdgrp>
      <trd lng="ru">&c_v;&c_y;&c_h;&c_o;&c_d;&c_n;&c_a;&c_ja; &c_p;&c_r;&c_o;&c_g;&c_r;&c_a;&c_m;&c_m;&c_a;</trd>
    </drv>
    <drv mrk="progra.font0o">
      <kap>font<tld/>o</kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Komputila <tld/>o verkita en programlingvo, kiun
          <ref tip="vid" cel="traduk.0ilo">tradukilo</ref>
          transformas en
          <ref tip="ant" cel="progra.cel0o">celprogramon</ref>.
        </dif>
      </snc>
      <trd lng="en">source program</trd>
      <trd lng="ru">&c_i;&c_s;&c_h;&c_o;&c_d;&c_n;&c_a;&c_ja; &c_p;&c_r;&c_o;&c_g;&c_r;&c_a;&c_m;&c_m;&c_a;</trd>
    </drv>
    <drv mrk="progra.sistema.0aro">
      <kap>sistema <tld/>aro</kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          <ref cel="progra.il0o">Il<tld/>oj</ref> de
          <ref cel="komput.0a.sistemo">komputa sistemo</ref>
          necesaj por funkciigi
          <ref cel="progra.aplika.0o">aplikajn <tld/>ojn</ref>.
        </dif> 
      </snc>
      <trd lng="en">system software</trd>
      <trd lng="ru">&c_s;&c_i;&c_s;&c_t;&c_je;&c_m;&c_n;&c_o;&c_je; &c_p;&c_r;&c_o;&c_g;&c_r;&c_a;&c_m;&c_m;&c_n;&c_o;&c_je; &c_o;&c_b;&c_je;&c_s;&c_p;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
    </drv>
    <drv mrk="progra.aplika.0aro">
      <kap>aplika <tld/>aro</kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Kolekto da <ref cel="progra.aplika.0o">aplikaj <tld/>oj</ref>
          por iu ekstera aplika tereno (kontraste al
          <ref cel="progra.sistema.0aro">sistema <tld/>aro</ref>).
       </dif>
      </snc>
      <trd lng="en">application software</trd>
      <trd lng="ru">&c_p;&c_r;&c_i;&c_k;&c_l;&c_a;&c_d;&c_n;&c_o;&c_je; &c_p;&c_r;&c_o;&c_g;&c_r;&c_a;&c_m;&c_m;&c_n;&c_o;&c_je; &c_o;&c_b;&c_je;&c_s;&c_p;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
    </drv>
  </art>
