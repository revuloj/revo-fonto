<art mrk="$Id: teni.xml,v 1.20 2024/01/16 21:22:49 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>teni</rad>/o
</kap>
<drv mrk="teni.0o">
  <kap><tld/>o</kap>
    <snc>
   <uzo tip="fak">ZOO</uzo>
  <uzo tip="fak">MED</uzo>
    <dif>
      Longa rubandoforma <ref tip="super" cel="parazi.0o">parazita</ref>
      <ref tip="super" cel="verm.0o.ZOO">vermo</ref>
      vivanta en intesto de homo a&ubreve; besto:
      <ekz>
         <tld/>oj apartenas al la biologia klaso de cestodoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=391934">Tenio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_s;&c_t;&c_u;&c_zh;&c_k;&c_a;&c_v;&c_y; <ind>&c_g;&c_l;&c_ib;&c_s;&c_t;</ind></trd>,
    <trd>&c_ib;&c_s;&c_t;&c_u;&c_zh;&c_k;&c_a;&c_v;&c_y; <ind>&c_r;&c_a;&c_b;&c_a;&c_k;</ind></trd>
  </trdgrp>
  <trd lng="ca">t&egrave;nia</trd>
  <trd lng="cs">tasemnice</trd>
  <trd lng="de">Bandwurm</trd>
  <trdgrp lng="en">
    <trd>tape-worm</trd>,
    <trd>tapeworm</trd>
  </trdgrp>
  <trd lng="fr">t&eacute;nia</trd>
  <trd lng="hu">garandf&eacute;reg</trd>
  <trdgrp lng="ja">
    <trd>&#x30B8;&#x30E7;&#x30A6;&#x30C1;&#x30E5;&#x30A6;</trd>,
    <trd>&#x30B5;&#x30CA;&#x30C0;&#x30E0;&#x30B7;</trd>
  </trdgrp>
  <trd lng="nl">lintworm</trd>
  <trdgrp lng="pl">
    <trd>soliter</trd>,
    <trd>tasiemiec</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>t&ecirc;nia</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_je;&c_n;&c_t;&c_o;&c_ch;&c_n;&c_y;&c_j; &c_g;&c_l;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">p&aacute;somnica</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&#x44C;&#x43E;&c_zh;&#x43A;&#x43E;&c_v;&c_i;&c_j; &c_g;&#x43B;&c_i;&c_s;&c_t;</trd>,
    <trd>&c_c;&c_ib;&#x43F;&#8217;&#x44F;&#x43A;</trd>
  </trdgrp>
</drv>
</art>
