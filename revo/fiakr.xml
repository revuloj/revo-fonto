<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fiakr.xml,v 1.19 2019/06/30 18:21:01 revo Exp $">
<kap>
<ofc>2</ofc>
<rad>fiakr</rad>/o
</kap>
<drv mrk="fiakr.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Luebla <ref tip="super" cel="kalesx.0o">kale&scirc;o</ref> je dispono
      de publiko:
      <ekz>
        <tld/>oj estas &ccirc;evaltirataj &ccirc;aroj, kiuj veturas tra la historia centro de la urbo,
        <klr>[&#8230;]</klr> en Parizo estis atendejo de tiaj veturiloj en la strato <tld lit="F"/>o,
        kiu akiris sian nomon de la mona&hcirc;o <nom>Fiakrius</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2012/010753.php">Tramo sen enir-dramo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="super">
      <ref cel="drosxk.0o">dro&scirc;ko</ref>,
      <ref cel="vetur.0ilo">veturilo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_ja;&c_k;&c_r;</trd>
  <trd lng="ca">fiacre</trd>
  <trd lng="cs">fiakr</trd>
  <trdgrp lng="de">
    <trd>Fiaker</trd>,
    <trd>Droschke</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>hackney coach</trd>,
    <trd>hackney carriage</trd>,
    <trd>coach for hire</trd>,
    <trd>carriage for hire</trd>,
    <trd>horse-drawn carriage</trd>,
    <trd>cab</trd>,
    <trd>fiacre</trd>
  </trdgrp>
  <trd lng="fr">fiacre</trd>
  <trdgrp lng="hu">
    <trd>fi&aacute;ker</trd>,
    <trd>b&eacute;rkocsi</trd>
  </trdgrp>
  <trd lng="nl">fiaker</trd>
  <trd lng="pt">fiacre</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_i;&c_a;&c_k;&c_r;</trd>,
    <trd>&c_n;&c_a;&c_jo;&c_m;&c_n;&c_y;&c_j; &c_e;&c_k;&c_i;&c_p;&c_a;&c_zh;</trd>
  </trdgrp>
  <trd lng="sk">fiaker</trd>
</drv>
<drv mrk="fiakr.motor0o">
  <kap>motor<tld/>o <fnt><bib>PV</bib></fnt></kap>
  <snc>
    <uzo tip="stl">ARK</uzo>
    <dif>
       Tia veturilo kun motoro.
    </dif>
    <ref tip="super" cel="taksi.0o">taksio</ref>
    <trd lng="ca">taxi</trd>
    <trd lng="fr">taxi<klr tip="ind"> (v&eacute;hicule)</klr></trd>
    <trd lng="hu">taxi</trd>
    <trd lng="ru">&c_t;&c_a;&c_k;&c_s;&c_i;</trd>
  </snc>
    <bld
      lok="&WCU;/thumb/1/16/Bundesarchiv_Bild_183-J0716-500-003%2C_Berlin%2C_erste_Kraftdroschke.jpg/768px-Bundesarchiv_Bild_183-J0716-500-003%2C_Berlin%2C_erste_Kraftdroschke.jpg"
      prm="CC BY-SA 3.0 de"
      >unua motor<tld/>o en Berlino, je 1905
        <fnt>
          <aut>Bundesarchiv, Bildo 183-J0716-500-003</aut>
          <lok><url ref="&WCW;/File:Bundesarchiv_Bild_183-J0716-500-003,_Berlin,_erste_Kraftdroschke.jpg"
              >https://commons.wikimedia.org/wiki/File:Bundesarchiv_Bild_183-J0716-500-003,_Berlin,_erste_Kraftdroschke.jpg</url></lok>
        </fnt>
    </bld>
  <trd lng="be">&c_t;&c_a;&c_k;&c_s;&c_ib;</trd>
  <trd lng="de">Motordroschke</trd>
  <trd lng="nl">taxi</trd>
</drv>
</art>
