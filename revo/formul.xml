  <trd lng="zu">ifomu</trd>
</drv>

<drv mrk="formul.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="formul.0i.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Esprimi per <tld/>oj:
      <ekz>
        <tld/>i la komponon de la aero.
      </ekz>
    </dif>
    <trd lng="hu">k&eacute;plettel kifejez</trd>    
    <trd lng="ru">&c_v;&c_y;&c_r;&c_a;&c_z;&c_i;&c_t;&c_mol; 
    <ind>&c_f;&c_o;&c_r;&c_m;&c_u;&c_l;&c_o;&c_j;</ind>
    </trd>
    <trdgrp lng="pl">
      <trd>formu&lstroke;owa&cacute;</trd>,
      <trd>okre&sacute;la&cacute;</trd>
    </trdgrp> 
  </snc>
  <snc>
    <dif>
      Mallonge, precize esprimi ion:
      <ekz>
        <tld/>i demandon, deziron<fnt>Z</fnt>,
      </ekz>
      <ekz>
        sian opinion;
      </ekz>
      <ekz>
        io ankora&ubreve; ne precize <tld/>ita, sed de &ccirc;iuj bone
        sentata...<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="redakt.0i">redakti</ref>,
      <ref cel="formal.0a">formala</ref>,
      <ref cel="form.0o">formo</ref>,
      <ref cel="tekst.0o">teksto</ref>.
    </refgrp>
    <trdgrp lng="hu">
      <trd>megfogalmaz</trd>,
      <trd>szavakba &ouml;nt</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>formu&lstroke;owa&cacute;</trd>,
      <trd>okre&sacute;la&cacute;</trd>
    </trdgrp> 
    <trd lng="ru">
    &c_f;&c_o;&c_r;&c_m;&c_u;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;
    </trd>
  </snc>
  <trd lng="be">
  &c_f;&c_a;&c_r;&c_m;&c_u;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;
  </trd>
  <trdgrp lng="cs">
    <trd>formulovat</trd>,
    <trd>koncipovat</trd>,
    <trd>utv&aacute;&rcaron;et</trd>
  </trdgrp>
  <trd lng="en">formulate</trd>
  <trd lng="es">formular</trd>
  <trdgrp lng="fr">
    <trd>formuler</trd>,
    <trd>exprimer</trd>,
    <trd>&eacute;noncer</trd>
  </trdgrp>
  <trd lng="nl">formuleren</trd>
  <trd lng="sk">sformulova&tcaron;</trd>
</drv>
