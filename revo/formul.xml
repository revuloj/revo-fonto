  <trd lng="km">&#x179F;&#x17C6;&#x178E;&#x17BB;&#x17C6;&#x1794;&#x17C2;&#x1794;&#x1794;&#x1791;</trd>
  <trd lng="ko">&#xC591;&#xC2DD;</trd>
  <trd lng="lo">&#xEAE;&#xEB9;&#xE9A;&#xEC1;&#xE9A;&#xE9A;</trd>
  <trd lng="ml">&#xD30;&#xD42;&#xD2A;&#xD02;</trd>
  <trd lng="mr">&#x92B;&#x949;&#x930;&#x94D;&#x92E;</trd>
  <trd lng="my">&#x1015;&#x102F;&#x1036;&#x101E;&#x100F;&#x1039;&#x100C;&#x102C;&#x1014;&#x103A;</trd>
  <trd lng="ny">mawonekedwe</trd>
