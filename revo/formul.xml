  <trd lng="ps">&#x641;&#x648;&#x631;&#x645;&#x647;</trd>
  <trd lng="sd">&#x641;&#x627;&#x631;&#x645;</trd>
  <trd lng="si">&#xD86;&#xD9A;&#xDD8;&#xDAD;&#xDD2; &#xDB4;&#xDAD;&#xDCA;&#xDBB;&#xDBA;</trd>
  <trd lng="sk">kniha vzorcov</trd>
  <trd lng="sq">form&#xEB;</trd>
  <trd lng="st">foromo</trd>
  <trd lng="ta">&#xBB5;&#xB9F;&#xBBF;&#xBB5;&#xBAE;&#xBCD;</trd>
