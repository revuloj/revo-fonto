<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: jokoham.xml,v 1.7 2023/12/03 07:09:18 revo Exp $">
    <kap>
      <rad>Jokoham</rad>/o
    </kap>

<drv mrk="jokoham.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
   <dif>
      Haven<ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      &ccirc;e la okcidenta flanko
      de la Golfo de Tokio, dua plej granda urbo de <ref tip="malprt" cel="japan.0ujo">Japanujo</ref>:
      <ekz>
        partoprenintoj en la Universala Kongreso en <tld/>o certe
        rimarkis
        <klr>[&#x2026;]</klr> ekzemple veturadon sur la maldekstra flanko,
        <klr>[&#x2026;]</klr> e&ccirc;
        homoj sur trotuaroj kuras maldekstre
        <fnt>
          <bib>Monato</bib>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url ref="&Monato;/2007/009290.php">Busoj,
          biciklantoj kaj kavaliraj kutimoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kiam Japanio moderni&gcirc;is en la 19a jarcento, la rolon de
        <nom>De&jcirc;ima</nom> kaj Nagasako
        transprenis <tld/>o kaj Kobeo
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Yasuo</aut>
          <vrk><url ref="&Monato;/2008/009358.php">Nagasako</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        liaj notoj pri voja&gcirc;o al <tld/>o tra Rusio estas konsiderataj
        plejparte senutilaj
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2014/011151.php">Kiu &ccirc;i mi?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_Jo;&c_k;&c_a;&c_g;&c_a;&c_m;&c_a;</trd>
  <trd lng="de">Yokohama</trd>
  <trd lng="fr">Yokohama</trd>
  <trd lng="ja">&#x6A2A;&#x6D5C;  <pr>&#x3088;&#x3053;&#x306F;&#x307E;</pr></trd>
  <trdgrp lng="zh">
    <trd>&#x6A2A;&#x6EE8; <pr>h&#xE9;ngb&#x12B;n</pr></trd>,
    <trd>&#x6A2A;&#x6EE8;&#x5E02; <pr>h&#xE9;ngb&#x12B;nsh&#xEC;</pr></trd>
  </trdgrp>
</drv>

  </art>

