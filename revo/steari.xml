<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: steari.xml,v 1.16 2023/09/04 15:14:31 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>stearin</rad>/o
</kap>
<drv mrk="steari.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Senkolora kaj senodora <ref tip="super" cel="substa.0o.FIZ">substanco</ref>
      trovi&gcirc;anta en preska&ubreve; &ccirc;iuj bestaj <ref tip="malprt" cel="gras.0o.besta">grasoj</ref>, 
      precipe en &scirc;afa graso, kaj iam uzata por fabriki <ref tip="malprt" cel="kandel.0o">kandelojn</ref>:
      <ekz>
        komence <nom>Alfred Krupp</nom> insistis, ke ene de la domo oni nur rajtas uzi oleo-, <tld/>o- a&ubreve; vakslumigadojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=223094">Vilao H&uuml;gel</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="parafi.0o">parafino</ref>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_e;&c_a;&c_r;&c_y;&c_n;</trd>
  <trd lng="de">Stearin</trd>
  <trd lng="en">stearin</trd>
  <trd lng="fr">st&eacute;arine</trd>
  <trd lng="hu">sztearin</trd>
  <trd lng="ja">&#x30B9;&#x30C6;&#x30A2;&#x30EA;&#x30F3;</trd>
  <trd lng="nl">stearine</trd>
  <trd lng="pl">stearyna</trd>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_a;&c_r;&c_i;&c_n;</trd>
  <trd lng="sv">stearin</trd>
  <trd lng="uk">&#x441;&#x442;&#x435;&#x430;&#x440;&#x438;&#x43D;</trd>
</drv>
</art>
