  <art mrk="$Id: erevan.xml,v 1.6 2023/11/07 20:34:11 afido Exp $">
    <kap>
      <rad>Erevan</rad>/o
    </kap>

<drv mrk="erevan.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc mrk="erevan.0o.cxefurbo">
    <dif>
      <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&Ccirc;efurbo</ref> 
      de <ref tip="dif" cel="armen.A0ujo">Armenujo</ref>:
      <ekz>
        <tld/>o, la &ccirc;efurbo de Armenio, estas unu el la plej malnovaj urboj de la mondo,
        sama&gcirc;a kiel Romo kaj Samarkando
        <fnt>
          <bib>Monato</bib>
          <aut>Lida Elbakjan</aut>
          <vrk><url ref="&Monato;/2003/007590.html">Armenio festis 1700 jarojn da
          kristanismo</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        eksplodis <klr>[&#x2026;]</klr> diskuto pri tio, &ccirc;u la partoprenantoj de iu
        Esperanto-aran&gcirc;o, okazinta en printempo 2013 en <tld/>o, Armenio, vizitu la
        surlokan monumenton pri la genocido kontra&ubreve; la armena popolo a&ubreve; &ccirc;u oni
        prefere rezignu pri tiu programero &leftquot;por ne ofendi ies nacian senton&rightquot;
        <fnt>
          <bib>Monato</bib>
          <aut>Andreas K&uuml;nzli</aut>
          <vrk><url ref="&Monato;/2013/011020.php">Atentinda memuaro</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Jerewan</trd>
    <trd lng="en">Yerevan</trd>
    <trd lng="fr">Erevan</trd>
    <trd lng="zh">&#x57C3;&#x88E1;&#x6EAB; <pr>&#x101;il&#x1D0;w&#x113;n</pr></trd>
</snc>
  <trd lng="be">&c_Je;&c_r;&c_e;&c_v;&c_a;&c_n;</trd>
  <trd lng="ja">&#x30A8;&#x30EC;&#x30D0;&#x30F3;</trd>
  <trd lng="uk">&c_Jeu;&c_r;&c_je;&c_v;&c_a;&#x43D;</trd>
</drv>

