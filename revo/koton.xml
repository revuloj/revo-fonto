<drv mrk="koton.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="koton.0o.lanugo">
  <uzo tip="fak">TEKS</uzo>
    <dif>
      Teksebla blanka delikata lanugo &ccirc;irka&ubreve;volvanta
      la grajnojn de <tld/>arbo kaj el kiu oni fabrikas
      tolsimilan &scirc;tofon:
      <ekz>
        <tld/>a drapo, <tld/>hava &scirc;tofo;
      </ekz>
      <ekz>
        pro kultivado de <tld/>o, maizo, batatoj kaj manioko necesis
        multaj manoj, nome sklavoj
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="&Monato;/2014/011219.php">Observanto
          observata</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        sub la flankojn de la birdo &scirc;i metis molan <tld/>on
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        simpla T-&ccirc;emizo el <tld/>o englutas 2500 litrojn <klr>[da
        akvo]</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="&Monato;/2012/010696.php">&Ccirc;u
          vivofonto a&ubreve; profitfonto?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="katun.0o">katuno</ref>,
      <ref cel="vat.0o">vato</ref>.
    </refgrp>
  <trd lng="id">kapas</trd>
  </snc>
  <snc mrk="koton.0o.planto">
    <dif>
      <tld lit="K"/>ujo:
      <ekz>
        sur la kampoj svarmis nudaj homoj, kiuj kolektis kukumojn
        a&ubreve; semis <tld/>on
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        plantoj, kiel <tld/>o, kafo kaj kakao, estas zorge kultivataj
        por industria uzado
        <fnt>
          <bib>Monato</bib>
          <aut>Sagbadjelou Kokou/pg</aut>
          <vrk><url ref="&Monato;/2005/008658.php">Pro
          pluvo plu</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="id">pohon kapas</trd>
  </snc>
