<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>koton</rad>/o
</kap>
<drv mrk="koton.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="koton.0o.lanugo">
  <uzo tip="fak">TEKS</uzo>  
    <dif>
      Teksebla blanka delikata lanugo &ccirc;irka&ubreve;volvanta
      la grajnojn de <tld/>arbo kaj el kiu oni fabrikas
      tolsimilan &scirc;tofon:
      <ekz>
        <tld/>a drapo, <tld/>hava &scirc;tofo;
      </ekz>
      <ekz>
        pro kultivado de <tld/>o, maizo, batatoj kaj manioko necesis multaj manoj, nome sklavoj
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="http://www.monato.net/2014/011219.php">Observanto observata</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        sub la flankojn de la birdo &scirc;i metis molan <tld/>on
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        simpla T-&ccirc;emizo el <tld/>o englutas 2500 litrojn <klr>[da akvo]</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="http://www.monato.net/2012/010696.php">&Ccirc;u vivofonto a&ubreve; profitfonto?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="katun.0o">katuno</ref>,
      <ref cel="vat.0o">vato</ref>.
    </refgrp>
  </snc>
  <snc mrk="koton.0o.planto">
    <dif>
      <tld lit="K"/>ujo:
      <ekz>
        sur la kampoj svarmis nudaj homoj, kiuj kolektis kukumojn a&ubreve; semis <tld/>on
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        plantoj, kiel <tld/>o, kafo kaj kakao, estas zorge kultivataj por industria uzado
        <fnt>
          <bib>Monato</bib>
          <aut>Sagbadjelou Kokou/pg</aut>
          <vrk><url ref="http://www.monato.net/2005/008658.php">Pro pluvo plu</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_v;&c_o;&c_w;&c_n;&c_a;</trd>
  <trd lng="cs">bavlna</trd>
  <trd lng="de">Baumwolle</trd>
  <trd lng="en">cotton</trd>
  <trd lng="fr">coton</trd>
  <trdgrp lng="hu">
    <trd>gyapot</trd>,
    <trd>pamut</trd>
  </trdgrp>
  <trd lng="id">kapas</trd>
  <trd lng="pl">bawe&lstroke;na</trd>
  <trd lng="pt">algod&atilde;o</trd>
  <trd lng="ru">&c_h;&c_l;&c_o;&c_p;&c_o;&c_k;</trd>
  <trd lng="sk">bavlna</trd>
</drv>

<drv mrk="koton.0arbo0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <uzo tip="fak">BOT</uzo>  
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio de 
      de <ref tip="lst" lst="voko:malvoparencoj" cel="malvac.0oj">malvacoj</ref> 
      (<trd lng="la">Gossypium</trd>) de unu a&ubreve; plurjaraj herboj, arbustoj kaj arbedoj 
      kun mane ripaj, 3-7-lobaj folioj, grandaj unuopaj floroj, kapsuloj juglandgrandaj, 3-5 &ccirc;ambraj, enhavantaj 
      brune nigrajn grajnojn, kiuj &ccirc;e la industrie uzataj specioj estas dense harkovritaj:
      <ekz>
        oni sukcesis fari transgenajn tabakplanton kaj <tld/>ujon kun mikroba geno, 
        kiu respondecas pri enzimo malkomponanta la herbicidon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=21185">Gentekniko</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        oni elprenis el tiuj riveroj ekde la erao de Stalin 
        (komenco de la intensa kultivado de <tld/>ujo en Uzbekio kaj Kaza&hcirc;io) multan akvon por irigacio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=24473">Aralo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_v;&c_o;&c_w;&c_n;&c_a; <klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trd lng="cs">bavln&iacute;k</trd>
  <trd lng="de">Baumwollstrauch</trd>
  <trdgrp lng="en">
    <trd>cotton</trd>,
    <trd>cotton plant</trd>
  </trdgrp>
  <trd lng="fr">cotonnier</trd>
  <trd lng="hu">gyapotcserje</trd>
  <trd lng="id"><ind>kapas</ind> (tumbuhan)</trd>
  <trd lng="pl">bawe&lstroke;na <klr>(ro&sacute;lina)</klr></trd>
  <trd lng="ru">&c_h;&c_l;&c_o;&c_p;&c_o;&c_k; <klr>(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">bavln&iacute;k</trd>
</drv>

<drv mrk="koton.fulm0o">
  <kap>fulm<tld/>o</kap>
  <snc>
    <dif>
      Kombita <tld/>o preparita kun azotacido kaj flagranta kiel pulvo.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ib;&c_r;&c_a;&c_k;&c_s;&c_ib;&c_l;&c_ib;&c_n;</trd>,
    <trd>&c_n;&c_ib;&c_t;&c_r;&c_a;&c_c;&c_e;&c_l;&c_ju;&c_l;&c_o;&c_z;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Blitzwolle</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>guncotton</trd>,
    <trd>nitrocellulose</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fulmicoton</trd>,
    <trd>coton-poudre</trd>
  </trdgrp>
  <trd lng="hu">l&odblac;gyapot</trd>  
  <trd lng="id">nitroselulosa</trd>
  <trd lng="pl">bawe&lstroke;na strzelnicza</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_i;&c_r;&c_o;&c_k;&c_s;&c_i;&c_l;&c_i;&c_n;</trd>,
    <trd>&c_n;&c_i;&c_t;&c_r;&c_o;&c_c;&c_je;&c_l;&c_l;&c_ju;&c_l;&c_o;&c_z;&c_a;</trd>
  </trdgrp>
</drv>
</art>
