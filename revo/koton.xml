<drv mrk="koton.0arbo0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio de
      de <ref tip="lst" lst="voko:malvoparencoj" cel="malvac.0oj">malvacoj</ref>
      (<trd lng="la">Gossypium</trd>) de unu a&ubreve; plurjaraj herboj,
      arbustoj kaj arbedoj
      kun mane ripaj, 3-7-lobaj folioj, grandaj unuopaj floroj, kapsuloj
      juglandgrandaj, 3-5 &ccirc;ambraj, enhavantaj
      brune nigrajn grajnojn, kiuj &ccirc;e la industrie uzataj specioj
      estas dense harkovritaj:
      <ekz>
        oni sukcesis fari transgenajn tabakplanton kaj <tld/>ujon kun
        mikroba geno,
        kiu respondecas pri enzimo malkomponanta la herbicidon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=21185">Gentekniko</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        oni elprenis el tiuj riveroj ekde la erao de <nom>Stalin</nom>
        (komenco de la intensa kultivado de <tld/>ujo en Uzbekio kaj
        Kaza&hcirc;io) multan akvon por irigacio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=24473">Aralo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_v;&c_o;&c_w;&c_n;&c_a;
  <klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trd lng="ca">cotoner</trd>
  <trd lng="cs">bavln&iacute;k</trd>
  <trd lng="de">Baumwollstrauch</trd>
  <trdgrp lng="en">
    <trd>cotton</trd>,
    <trd>cotton plant</trd>
  </trdgrp>
  <trd lng="fr">cotonnier</trd>
  <trd lng="hu">gyapotcserje</trd>
  <trd lng="id">pohon kapas</trd>
  <trd lng="ja">&#12527;&#12479;</trd>
  <trd lng="pl">bawe&lstroke;na <klr>(ro&sacute;lina)</klr></trd>
  <trd lng="ru">&c_h;&c_l;&c_o;&c_p;&c_o;&c_k;
  <klr>(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">bavln&iacute;k</trd>
  <trd lng="zh">&#26825;&#27611; <pr>mi&aacute;nm&aacute;o</pr></trd>
</drv>

<drv mrk="koton.fulm0o">
  <kap>fulm<tld/>o</kap>
  <snc>
    <dif>
      Kombita <tld/>o preparita kun azotacido kaj flagranta kiel pulvo,
      &hcirc;emie t.e. <ref cel="celulo1.0o">celuloza</ref>
      <ref tip="super" cel="nitrat.0o">nitrato</ref> a&ubreve; piroksilino:
      <ekz>
        solvante fulm<tld/>on en etero a&ubreve; alkoholo, oni ricevas <ref
        cel="celoid.0o">celoidinon</ref>, kiun
        oni uzas por prepari specimenojn por mikroskopado;
      </ekz>
      <ekz>
        el <ref tip="malprt" cel="celulo.0o">celuloido</ref>, kombina&jcirc;o
        de <tld/>o kun kamforo,
        &gcirc;is 1950 oni produktis filmbendojn.
      </ekz>
    </dif>
    <refgrp tip="malprt">
      <ref cel="kolodi.0o">kolodio</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ib;&c_r;&c_a;&c_k;&c_s;&c_ib;&c_l;&c_ib;&c_n;</trd>,
    <trd>&c_n;&c_ib;&c_t;&c_r;&c_a;&c_c;&c_e;&c_l;&c_ju;&c_l;&c_o;&c_z;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>nitrocel&middot;lulosa</trd>,
    <trd>cot&oacute; p&oacute;lvora</trd>,
    <trd>fulmicot&oacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Blitzwolle</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>guncotton</trd>,
    <trd>nitrocellulose</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fulmicoton</trd>,
    <trd>coton-poudre</trd>
  </trdgrp>
  <trd lng="hu">l&odblac;gyapot</trd>
  <trd lng="id">nitroselulosa</trd>
  <trd lng="ja">&#32191;&#28779;&#34220;  <pr>&#12417;&#12435;&#12363;&#12420;&#12367;</pr></trd>
  <trd lng="pl">bawe&lstroke;na strzelnicza</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_i;&c_r;&c_o;&c_k;&c_s;&c_i;&c_l;&c_i;&c_n;</trd>,
    <trd>&c_n;&c_i;&c_t;&c_r;&c_o;&c_c;&c_je;&c_l;&c_l;&c_ju;&c_l;&c_o;&c_z;&c_a;</trd>
  </trdgrp>
</drv>
