<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>krokodil</rad>/o
</kap>
<drv mrk="krokod.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="krokod1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Crocodylus</trd>), granda kvarpieda dan&gcirc;era
      rampulo, vivanta en grandaj riveroj de tropikaj landoj:
      <ekz>
        <tld/>a ledo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_a;&c_k;&c_a;&c_d;&c_z;&c_ib;&c_l;</trd>
  <trd lng="de">Krokodil</trd>
  <trd lng="en">crocodile</trd>
  <trd lng="hu">krokodil</trd>
  <trd lng="pt">crocodilo</trd>
  <trd lng="tr">timsah</trd>
  <trd lng="ru">&c_k;&c_r;&c_o;&c_k;&c_o;&c_d;&c_i;&c_l;</trd>
  <trd lng="sv">krokodil</trd>
</drv>

<drv mrk="krokod.0uloj">
  <kap>
    <tld/>uloj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="ramp.0uloj">rampuloj</ref>
      (<trd lng="la">Crocodylia</trd>).
    </dif>
  </snc>
  <trd lng="de">Krokodile</trd>
  <trd lng="ru">&c_k;&c_r;&c_o;&c_k;&c_o;&c_d;&c_i;&c_l;&c_y; <klr>(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
</drv>

<drv mrk="krokod.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">ESP</uzo>
  <uzo tip="stl">FRAZ</uzo>
  <snc>
    <dif>
      Paroli kun esperantistoj ne en Esperanto.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>krokodilisch sprechen</trd>,
     <trd>unter Esperantisten nicht Esperanto, sondern die Muttersprache benutzen</trd>
  </trdgrp>
  <trd lng="hu">krokodilkodik</trd>
  <trd lng="ru">&c_k;&c_r;&c_o;&c_k;&c_o;&c_d;&c_i;&c_l;&c_i;&c_t;&c_mol;</trd>
</drv>

</art>
