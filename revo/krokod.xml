<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>krokodil</rad>/o
</kap>
<drv mrk="krokod.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="krokod.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Crocodylus</trd>), granda kvarpieda dan&gcirc;era
      rampulo, vivanta en grandaj riveroj de tropikaj landoj:
      <ekz>
        <tld/>a ledo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Krokodil</trd>
  <trd lng="en">crocodile</trd>
  <trd lng="tr">timsah</trd>
</drv>
<drv mrk="krokod.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="krokod.0uloj"><tld/>uloj</ref>
      (<trd lng="la">Crocodylidae</trd>), &ccirc;e kiuj la kvara dento estas
      videbla &ccirc;e fermita bu&scirc;o.
    </dif>
  </snc>
  <trd lng="de">Echte <ind>Krokodile</ind></trd>
</drv>
<drv mrk="krokod.0uloj">
  <kap><tld/>uloj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="ramp.0uloj">rampuloj</ref>
      (<trd lng="la">Crocodylia</trd>).
    </dif>
  </snc>
  <trd lng="de">Krokodile</trd>
</drv>
</art>
