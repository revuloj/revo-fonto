<vortaro>
<art mrk="$Id: alcxem.xml,v 1.26 2023/08/30 18:23:32 revo Exp $">
<kap>
  <rad>al&ccirc;emil</rad>/o <fnt><bib>PV</bib></fnt>,
  <var><kap><rad var="k">alkemil</rad>/o</kap></var>
</kap>
<drv mrk="alcxem.0o">
  <kap><tld/>o,
    <var><kap><tld var="k"/>o</kap></var>
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="rozac.0oj">rozacoj</ref>
      (<trd lng="la">Alchemilla</trd>), kun renformaj folioj kaj
      verdetaj floroj:
      <ekz>
        <tld var="k"/>o estas <klr>[&#x2026;]</klr> populara &gcirc;ardenplanto kun la komuna nomo dammantelo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=390212">Alkemilo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="lotus.0efiko">lotusefiko</ref>
    <bld lok="&WCU;/thumb/3/3d/Alchemilla_xanthochlora_001.JPG/360px-Alchemilla_xanthochlora_001.JPG" prm="CC BY-SA 3.0"><tld/>o, Botanika &Gcirc;ardeno, Karlsruho, Germanujo
        <fnt>
          <aut>H. Zell, propra verko</aut>
          <lok><url ref="&WCW;/File:Alchemilla_xanthochlora_001.JPG">&WCW;/File:Alchemilla_xanthochlora_001.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_r;&c_d;&c_e;&c_ch;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_ib;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_y;&c_p;&c_je;&c_r;&c_s;&c_mol;&c_n;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">alquemil&middot;la</trd>
  <trd lng="de">Frauenmantel</trd>
  <trd lng="en">rosacea</trd>
  <trd lng="fa">&f_p;&a_A;&f_y; &a_sh;&f_y;&a_r;</trd>
  <trd lng="fr">alch&eacute;mille</trd>
  <trd lng="hu">pal&aacute;stf&udblac;</trd>
  <trd lng="ja">&#x30CF;&#x30B4;&#x30ED;&#x30E2;&#x30B0;&#x30B5;</trd>
  <trd lng="nl">vrouwenmantel</trd>
  <trd lng="pt">alquemila</trd>
  <trd lng="ru">&c_m;&c_a;&c_n;&c_zh;&c_je;&c_t;&c_k;&c_a; <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="sv">daggk&aring;pa</trd>
  <trd lng="uk">&#x43F;&#x440;&#x438;&#x432;&#x43E;&#x440;&#x43E;&#x442;&#x435;&#x43D;&#x44C;</trd>
</drv>
</art>
