<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: diktam.xml,v 1.2 2024/03/11 09:24:35 afido Exp $">
    <kap>
      <rad>diktamn</rad>/o
      <fnt><bib>PVEF</bib></fnt>
    </kap>

<drv mrk="diktam.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="diktam.0o.genro">
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio <ref tip="malprt" cel="rutac.0oj">rutacoj</ref> kun nur
      unu konata specio (<trd lng="la">Dictamnus albus</trd>),
      venena planto kies floroj eligas i.a. facile flamigeblan izopropenon:
      <ekz>
        kiel ka&ubreve;zon de tiuj eksplodoj oni konstatis eteran oleon en la floroj de l&#8217;
        diktamno kiu, sub la influo de la varmo, povas elradii bruleblan gason
        <fnt>
          <vrk><url ref="https://dlibra.kul.pl/Content/43278/48160.pdf"
              >Plantoj eksplodantaj</url></vrk>
          <lok>Heroldo de Esperanto, n-ro 17 (565), 1930-04-25, p. 3</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/a/ae/0_Dictamnus_albus_-_Fraxinelle_(1).JPG/320px-0_Dictamnus_albus_-_Fraxinelle_(1).JPG"
      prm="CC BY-SA 3.0"
      ><tld/>o (Dictamnus albus)
        <fnt>
          <aut>Jean-Pol Grandmont, propra verko</aut>
          <lok><url ref="&WCW;/File:0_Dictamnus_albus_-_Fraxinelle_(1).JPG"
              >https://commons.wikimedia.org/wiki/File:0_Dictamnus_albus_-_Fraxinelle_(1).JPG</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="de">
      <trd>Diptam</trd>,
      <trd>Aschwurz</trd>,
      <trd>Spechtwurz</trd>,
      <trd>Brennender Busch</trd>
    </trdgrp>
    <trd lng="en">Dictamnus</trd>
    <trd lng="fr">Fraxinelle</trd>
  </snc>
<trdgrp lng="be">
  <trd>&c_ja;&c_s;&c_ja;&c_n;&c_je;&c_c;</trd>,
  <trd>&c_d;&c_z;&c_ib;&c_k;&c_ib; <ind>&c_b;&c_a;&c_d;&c_z;&c_ja;&c_n;</ind></trd>
</trdgrp>
</drv>

  </art>

