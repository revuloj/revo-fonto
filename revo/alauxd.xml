<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>ala&ubreve;d</rad>/o
</kap>
<drv mrk="alauxd.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
  <dif>
    Birdo el la familio <tld/>edoj.
  </dif>
  <trd lng="cs">sk&rcaron;ivan</trd>
  <trd lng="de">Lerche</trd>
  <trd lng="en">lark</trd>
  <trd lng="hu">pacsirta</trd>
  <trd lng="ru">&c_zh;&c_a;&c_v;&c_o;&c_r;&c_o;&c_n;&c_o;&c_k;</trd>
  </snc>
  <snc num="2">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio 
      <ref tip="super" cel="alauxd.0edoj">ala&ubreve;dedoj</ref>
      (<trd lng="la">Alauda</trd>).
    </dif>
  </snc>
  <trd lng="cs">sk&rcaron;ivan polni</trd>
  <trd lng="de">Feldlerche</trd>
  <trd lng="en">skylark</trd>
  <trd lng="hu">mezei pacsirta</trd>
  <trd lng="pl">skowronek</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_je;&c_v;&c_o;&c_j;
   &c_zh;&c_a;&c_v;&c_o;&c_r;&c_o;&c_n;&c_o;&c_k;</trd>
</drv>
<drv mrk="alauxd.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo 
      <ref tip="super" cel="paser.0oformaj">paseroformaj</ref> birdoj
      (<trd lng="de">Alaudidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Lerchen</trd>
</drv>
  
</art>
