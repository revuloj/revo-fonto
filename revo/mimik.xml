<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mimik.xml,v 1.30 2024/04/03 15:23:12 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>mimik</rad>/o
</kap>
<drv mrk="mimik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEA</uzo>
  <snc mrk="mimik.0o.arto">
    <dif>
      <ref tip="lst" lst="voko:artoj" cel="art.0o.belarto">Arto</ref> esprimi
      per <ref tip="prt" cel="gest.0o">gesto</ref>j pensojn kaj sentojn:
      <ekz>
        mi neniam ankora&ubreve; renkontis homon, kiu povus tiel libere klari&gcirc;i per <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de ebrio
        <fnt>
          <aut>Marjorie Boulton</aut>
          <vrk><url ref="&Tekstaro;?nomo=ne-nur-leteroj-de-plumamikoj">Ne nur leteroj de
          plumamikoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        malgra&ubreve; la foresto de sceno, <tld/>o, gestoj,
        kostumoj kaj orkestro [la opero] faris treege profundan impreson
        <fnt>
          <aut>Paul Bennemann</aut>,
          <vrk>La Vendreda Klubo</vrk>,
          <lok><url ref="&Gutenberg;ebooks/47259">
        gutenberg.org</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="pantom.0o">pantomimo</ref>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_m;&c_ib;&c_k;&c_a;</trd>
  <trd lng="ca">m&iacute;mica</trd>
  <trd lng="cs">um&ecaron;n&iacute; mimiky</trd>
  <trdgrp lng="de">
    <trd>Mimik</trd>,
    <trd>Geb&auml;rdenspiel</trd>,
    <trd>Mienenspiel</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>mime</trd>,
    <trd>mimicry</trd>
  </trdgrp>
  <trd lng="es">m&iacute;mica</trd>
  <trd lng="fr">mimique</trd>
  <trdgrp lng="hu">
    <trd>mimika</trd>,
    <trd>arcj&aacute;t&eacute;k</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30B8;&#x30A7;&#x30B9;&#x30C1;&#x30E3;&#x30FC;</trd>,
    <trd>&#x8EAB;&#12406;&#x308A;  <pr>&#x307F;&#12406;&#x308A;</pr></trd>,
    <trd>&#29289;&#x307E;&#x306D;  <pr>&#12418;&#x306E;&#x307E;&#x306D;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>mimiek</trd>,
    <trd>gebarenspel</trd>
  </trdgrp>
  <trd lng="pl">mimika</trd>
  <trd lng="pt">m&iacute;mica</trd>
  <trd lng="ru">&c_m;&c_i;&c_m;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">umenie mimiky</trd>
  <trd lng="uk">&c_m;&c_ib;&c_m;&c_ib;&c_k;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#34920;&#x60C5; <pr>bi&#x1CE;oq&#xED;ng</pr></trd>,
    <trd>&#38754;&#x90E8;&#34920;&#x60C5; <pr>mi&#xE0;nb&#xF9;bi&#x1CE;oq&#xED;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mimik.0isto">
  <kap>
    <tld/>isto
  </kap>
  <uzo tip="fak">TEA</uzo>
  <snc mrk="mimik.0isto.artisto">
    <dif>
      <ref tip="super" cel="art.0isto">Artisto</ref> esprimi&gcirc;anta nur
      per <tld/>o, <ref tip="dif" cel="pantom.0isto">pantomimisto</ref>:
      <ekz>
        instruita <tld/>isto <klr>[&#8230;]</klr> publike anoncis, ke li povas per diversaj
        signoj de manoj paroladi ne sole pri ordinaraj objektoj de la &ccirc;iutaga vivo,
        sed e&ccirc; pri objektoj de filozofio
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iaspeca &ccirc;ifonularo, konsistanta el kantistoj, <tld/>istoj, muzikistoj,
        dancistoj kaj dancistinoj
        <fnt>
          <aut>Henryk Sienkiewicz, trad. Lidia Zamenhof </aut>
          <vrk><url ref="&Tekstaro;?nomo=quo-vadis">Quo Vadis</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="mim.0o">mimo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ib;&c_m;</trd>,
    <trd>&c_m;&c_ib;&c_m;&c_ib;&c_s;&c_t;</trd>
  </trdgrp>
  <trd lng="de">Mime</trd>
  <trd lng="en">mime</trd>
  <trdgrp lng="ja">   <trd>&#x30D1;&#x30F3;&#x30C8;&#x30DE;&#x30A4;&#x30E0;&#x4FF3;&#x512A;
    <pr>&#x30D1;&#x30F3;&#x30C8;&#x30DE;&#x30A4;&#x30E0;&#x306F;&#12356;&#12422;&#12358;</pr></trd>,
    <trd>&#29289;&#x307E;&#x306D;&#x82B8;&#x4EBA;  <pr>&#12418;&#x306E;&#x307E;&#x306D;&#12370;&#12356;&#x306B;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">mimekunstenaar</trd>
  <trd lng="pl">mim</trd>
  <trdgrp lng="zh">
    <trd>&#x54D1;&#21095;&#x6F14;&#21592; <pr>y&#x1CE;j&#xF9;y&#x1CE;nyu&#xE1;n</pr></trd>,
    <trd>&#x555E;&#21127;&#x6F14;&#x54E1; <pr>y&#x1CE;j&#xF9;y&#x1CE;nyu&#xE1;n</pr></trd>
  </trdgrp>
</drv>
</art>
