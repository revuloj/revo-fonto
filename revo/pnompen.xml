    <trd lng="be">&c_P;&c_n;&c_a;&c_m;&c_p;&c_e;&c_n;&c_mol;</trd>
    <trd lng="de">Phnom Penh</trd>
    <trd lng="en">Phnom Penh</trd>
    <trd lng="fr">Phnom Penh</trd>
    <trd lng="pl">Phnom Penh</trd>
    <trd lng="ro">Phnom Penh </trd>
