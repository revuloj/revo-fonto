  <trd lng="ja">&#x30D7;&#x30CE;&#x30F3;&#x30DA;&#x30F3;</trd>
  <trd lng="uk">&#x41F;&#x43D;&#x43E;&#x43C;&#x43F;&c_je;&#x43D;&#x44C; (&c_s;&c_t;&#x43E;&#x43B;&c_i;&c_c;&#x44F;)</trd>
  <trdgrp lng="zh">
    <trd>&#x91D1;&#x8FB9; <pr>J&#x12B;nbi&#x101;n</pr></trd>,
    <trd>&#x91D1;&#x908A; <pr>J&#x12B;nbi&#x101;n</pr></trd>
  </trdgrp>
