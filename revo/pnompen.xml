<drv mrk="pnompen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&Ccirc;efurbo</ref> 
      de <ref tip="malprt" cel="kambogx.K0o">Kambo&gcirc;o</ref>:
      <ekz>
        en <tld/>o, bonvena malvarmeto akompanas la nokti&gcirc;on
        <fnt>
          <aut>Christine Chaumeau</aut>
          <vrk><url ref="&Mondediplo;article2632.html">La monarkisma revo de la
          Kambo&gcirc;a &ccirc;efministro</url></vrk>
          <lok>Le Monde diplomatique' en Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la 1-an de septembro 1966, en <tld/>o, li notas ke la milito en Vjetnamio &leftquot;ne
        havos militan solvon&rightquot;
        <fnt>
          <aut>Dominique Vidal</aut>
          <vrk><url ref="&Mondediplo;article1348.html">NATO - Tio kion volis de Gaulle
          en 1966</url></vrk>
          <lok>Le Monde diplomatique' en Esperanto</lok>
