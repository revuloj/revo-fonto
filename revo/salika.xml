<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: salika.xml,v 1.17 2023/09/03 12:09:43 revo Exp $">
<kap>
  <rad>salikari</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="salika.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_specioj" cel="speci.0o.BIO">Specio</ref>
      de <ref tip="super" cel="litrum.0o">litrumo</ref>
      (<trd lng="la">Lythrum salicaria</trd>), kun rozkoloraj floroj:
      <ekz>
        <tld/>o estas <klr>[&#x2026;]</klr> plurjara herbo alta inter 30 kaj 200&#x202F;cm, kun paraj
        a&ubreve; verticilaj folioj, la sespetalaj, purpurrozaj floroj formas longajn,
        spikaspektajn infloreskojn, la semoj, el kapsulo, disvasti&gcirc;as plejofte la&ubreve; la
        akvofluo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=531104">Salikario</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/f/f8/Lythrum_salicaria_Rhine.jpg/640px-Lythrum_salicaria_Rhine.jpg" prm="CC-BY-SA-3.0"><tld/>oj &ccirc;e meza Rejno, Germanujo
        <fnt>
          <aut>I, Manfred Heyde, propra verko</aut>
          <lok><url ref="&WCW;/File:Lythrum_salicaria_Rhine.jpg">&WCW;/File:Lythrum_salicaria_Rhine.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_l;&c_mol;&c_ch;&c_a;&c_k; &c_v;&c_je;&c_r;&c_b;&c_a;&c_l;&c_ib;&c_s;&c_t;&c_y;</trd>
  <trd lng="ca">salic&agrave;ria</trd>
  <trd lng="de">Blutweiderich</trd>
  <trd lng="fr">salicaire</trd>
  <trd lng="hu">r&eacute;ti <ind>f&uuml;z&eacute;ny</ind></trd>
  <trd lng="it">salicaria</trd>
  <trd lng="ja">&#x30A8;&#x30BE;&#x30DF;&#x30BD;&#x30CF;&#x30AE;</trd>
  <trd lng="nl">grote <ind>kattenstaart</ind><klr tip="ind">
  (plant)</klr></trd>
  <trd lng="pl">krwawnica pospolita</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_a;&c_k;&c_u;&c_n;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_k;&c_u;&c_n;-&c_t;&c_r;&c_a;&c_v;&c_a;</trd>,
    <trd>&c_d;&c_je;&c_r;&c_b;&c_je;&c_n;&c_n;&c_i;&c_k; &c_i;&c_v;&c_o;&c_l;&c_i;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x43B;&c_a;&#x43A;&c_u;&#x43D; &c_v;&c_je;&c_r;&c_b;&#x43E;&#x43B;&c_i;&c_s;&c_t;&c_i;&c_j;</trd>
</drv>
</art>
