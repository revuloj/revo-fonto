<drv mrk="instru.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="instru.0igxi.PED">
    <dif>
      I&gcirc;i <tld/>ita:
      <ekz>
        multaj &ccirc;eestas paroladon, &ccirc;ar ili deziras <tld/>i&gcirc;i kaj &gcirc;ui la oratora&jcirc;on
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili balda&ubreve; lernis fari &ccirc;iuspecojn da objektoj el kano <klr>[&#8230;]</klr>, ili &ccirc;ion
        faris lerte, post kiam <tld/>i&gcirc;is
        <fnt>
          <bib>Rob</bib>
          <lok>Parto V</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;u, ju&gcirc;istoj de la tero
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 2:10</lok>
        </fnt>!
      </ekz>
      <ekz>
        tradicie knabinoj ne tiom bone <tld/>i&gcirc;is kiom knaboj kaj do malmultas virinoj
        ekzemple en oficaj kaj registaraj postenoj
        <fnt>
          <bib>Monato</bib>
          <aut>Sagbadjelou Kokou/pg</aut>
          <vrk><url ref="https://www.monato.be/2006/008955.php"
              >In-iciato kontentiga</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_v;&c_u;&c_ch;&c_y;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_v;&c_u;&c_ch;&c_y;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><ind>belehrt</ind> werden</trd>,
    <trd><ind>unterrichtet</ind> werden</trd>,
    <trd>lernen</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd><ind>formar</ind>se<klr tip="ind">
    (instruirse)</klr></trd>,
    <trd><ind>educar</ind>se</trd>,
    <trd><ind>instruir</ind>se</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>former</ind><klr tip="ind">
    (s'instruire)</klr></trd>,
    <trd>s'<ind>instruire</ind></trd>
  </trdgrp>
  <trd lng="hu">tanul</trd>
  <trd lng="id">bel<ind>ajar</ind></trd>
  <trd lng="nl">leren</trd>
  <trdgrp lng="pl">
    <trd>uczy&cacute; si&eogonek;</trd>,
    <trd>kszta&lstroke;ci&cacute; si&eogonek;</trd>,
    <trd>wyci&aogonek;gn&aogonek;&cacute; <ind>nauk&eogonek;</ind>
      <klr> (z czego&sacute;)</klr>
    </trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>instruir-se</trd>,
    <trd>aprender</trd>
  </trdgrp>
  <trdgrp lng="ru">
