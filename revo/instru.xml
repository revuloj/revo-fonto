  </trdgrp>
  <trd lng="pt">escola</trd>
  <trd lng="ru">&c_u;&c_ch;&c_je;&c_b;&c_n;&c_o;&c_je; &c_z;&c_a;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">u&ccaron;eb&ncaron;a</trd>
  <trdgrp lng="uk">
    <trd>&c_n;&c_a;&c_v;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_je; &c_p;&c_r;&c_i;&c_m;&c_ib;&c_shch;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_a; &c_k;&c_ib;&c_m;&c_n;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_k;&c_l;&c_a;&c_s;</trd>,
