    <trd>Lehrer</trd>,
    <trd>Lehrerin</trd>
  </trdgrp>
  <trd lng="es">profesor</trd>
  <trdgrp lng="fr">
    <trd>enseignant</trd>,
    <trd>professeur<klr tip="ind"> (enseignant en 
g&eacute;n&eacute;ral)</klr></trd>,
    <trd>prof<klr tip="ind"> (enseignant en 
