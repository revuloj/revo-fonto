    <trd>le&ccedil;on<klr tip="ind"> (enseignement)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tanult ismeret</trd>,
    <trd>tanuls&aacute;g</trd>
  </trdgrp>
