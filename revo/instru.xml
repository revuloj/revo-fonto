  </trdgrp>
  <trd lng="de">Lehre</trd>
  <trd lng="es">ense&ntilde;anza</trd>
  <trdgrp lng="hu">
    <trd>tanult ismeret</trd>,
    <trd>tanuls&aacute;g</trd>
  </trdgrp>
  <trd lng="nl">onderwezen <ind>kennis</ind></trd>
