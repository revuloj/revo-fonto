</drv>

<drv mrk="instru.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="instru.0igxi.PED">
    <dif>
      I&gcirc;i <tld/>ita:
      <ekz>
        <tld/>i&gcirc;i sen profesoro;
      </ekz>
      <ekz>
        per fremda sperto neniu <tld/>i&gcirc;as;
      </ekz>
      <ekz>
        <tld/>i&gcirc;u, ju&gcirc;istoj de la tero<fnt>Z</fnt>,
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_v;&c_u;&c_ch;&c_y;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_v;&c_u;&c_ch;&c_y;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>lernen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>former</ind><klr tip="ind"> (s'instruire)</klr></trd>,
    <trd>s'<ind>instruire</ind></trd>
  </trdgrp>
  <trd lng="hu">tanul</trd>
  <trd lng="nl">leren</trd>
  <trdgrp lng="pt">
    <trd>instruir-se</trd>,
    <trd>aprender</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_u;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_n;&c_a;&c_u;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="instru.0isto">
  <kap><tld/>isto<fnt>Z</fnt></kap>
  <snc mrk="instru.0isto.PED">
    <tezrad fak="PED"/>
    <uzo tip="fak">PED</uzo>
    <dif>
      Homo, kiu profesie <tld/>as:
      <ekz>
        la patro, <tld/>isto de vila&gcirc;a lernejo
        <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 6a, p. 34a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_s;&c_t;&c_a;&c_w;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_v;&c_y;&c_k;&c_l;&c_a;&c_d;&c_ch;&c_y;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Lehrer</trd>,
    <trd>Lehrerin</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>teacher</trd>,
    <trd>instructor</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Lehrer</trd>,
    <trd>Lehrerin</trd>
  </trdgrp>
  <trd lng="es">profesor</trd>
  <trdgrp lng="fr">
    <trd>enseignant</trd>,
    <trd>professeur<klr tip="ind"> (enseignant)</klr></trd>,
    <trd>prof<klr tip="ind"> (enseignant)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tan&iacute;t&oacute;</trd>,
    <trd>tan&aacute;r</trd>,
    <trd>oktat&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>onderwijzer</trd>,
    <trd>leraar</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>professor</trd>,
    <trd>professora</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_p;&c_o;&c_d;&c_a;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_u;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="vo">tidel</trd>
</drv>

<drv mrk="instru.0iteco">
  <kap><tld/>iteco</kap>
  <snc mrk="instru.0iteco.KOMUNE">
    <dif>
