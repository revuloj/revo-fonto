<drv mrk="instru.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="instru.0ejo.lernejo">
    <uzo tip="fak">PED</uzo>
    <dif>
      <ref cel="lern.0ejo" tip="dif">Lernejo</ref>;
      salono ekipita por <tld/>ado:
      <ekz>
        <tld/>ejo, kongresejo malfermos siajn pordojn
        <fnt>
          <bib>VivZam</bib>
          <lok>Vivo de Zamenhof</lok>
        </fnt>;
      </ekz>
      <ekz>
        la E&ubreve;ropaj Lernejoj estas specifaj <tld/>ejoj, fonditaj anta&ubreve; pli ol 60 jaroj
        por doni edukon al la etuloj de e&ubreve;ropaj funkciuloj
        <fnt>
          <bib>Kon11</bib>
          <lok>Tiel la mondo...</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj kolekti&gcirc;is silente en la granda <tld/>ejo
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Stelmona&hcirc;ejo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu <tld/>ejo, &ccirc;u unugrada, &ccirc;u duagrada, disponas pri aroj da plej modernaj
        elektronikaj ekipa&jcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="https://www.monato.be/2010/010124.php"
              >Revo realigita</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum 40 jaroj en diversaj <tld/>ejoj mi kutimis &ndash; &ccirc;u bone, &ccirc;u malbone &ndash; suvereni
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="https://www.monato.be/2015/011570.php"
              >Po&scirc;komputiloj kaj klas&ccirc;ambroj</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="auxdit.0o">a&ubreve;ditorio</ref>,
      <ref cel="cxambr.klas0o">klas&ccirc;ambro</ref>,
      <ref cel="preleg.0ejo">prelegejo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_v;&c_u;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_w;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_a;</trd>
  <trd lng="cs">u&ccaron;ebna</trd>
  <trdgrp lng="de">
    <trd>Schule</trd>,
    <trd>Unterrichtsraum</trd>,
    <trd>Seminarraum</trd>
  </trdgrp>
  <trd lng="en">school</trd>
  <trdgrp lng="es">
    <trd>colegio</trd>,
    <trd>escuela</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;cole</trd>,
    <trd>&eacute;tablissement scolaire</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>iskola</trd>,
    <trd>oktat&oacute;terem</trd>,
    <trd>oktat&aacute;si helyis&eacute;g</trd>,
