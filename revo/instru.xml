<drv mrk="instru.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      I&gcirc;i <tld/>ita:
      <ekz>
        <tld/>i&gcirc;i sen profesoro;
      </ekz>
      <ekz>
        per fremda sperto neniu <tld/>i&gcirc;as;
      </ekz>
      <ekz>
        <tld/>i&gcirc;u, ju&gcirc;istoj de la tero<fnt>Z</fnt>,
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_v;&c_u;&c_ch;&c_y;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_v;&c_u;&c_ch;&c_y;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>lernen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>former</ind><klr tip="ind"> (s'instruire)</klr></trd>,
    <trd>s'<ind>instruire</ind></trd>
  </trdgrp>
  <trd lng="hu">tanul</trd>
  <trd lng="nl">leren</trd>
  <trdgrp lng="pt">
   <trd>instruir-se</trd>,
   <trd>aprender</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_u;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_n;&c_a;&c_u;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

