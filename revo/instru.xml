    <trd>oktat&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>onderwijzer</trd>,
    <trd>leraar</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>professor</trd>,
   <trd>professora</trd>
  </trdgrp>
  <trdgrp lng="ru">
    
<trd>&c_p;&c_r;&c_je;&c_p;&c_o;&c_d;&c_a;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_u;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="vo">tidel</trd>
</drv>

<drv mrk="instru.0iteco">
  <kap><tld/>iteco</kap>
  <snc>
    <dif>
      Tuto de la akiritaj konoj, scioj; stato de <tld/>ita homo:
      <ekz>
        havi ampleksan <tld/>itecon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
