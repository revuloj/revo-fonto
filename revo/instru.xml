<drv mrk="instru.0a">
  <kap><tld/>a</kap>
  <snc mrk="instru.0a.0plena">
    <dif>
      <tld lit="I"/>anta, plena je <tld/>oj:
      <ekz>
        <tld/>a leciono;
      </ekz><ekz>
        <tld/>a ekzemplo
        <fnt><bib>MT</bib>, <lok>&Rea; 28:37</lok></fnt>,
      </ekz><ekz>
        rakonto, libro.
      </ekz>
    </dif>
    <ref tip="sin" cel="instru.0plena"><tld/>plena</ref>
    <trd lng="be">&c_p;&c_a;&c_v;&c_u;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
    <trdgrp lng="de">
      <trd>lehrreich</trd>,
      <trd>instruktiv</trd>
    </trdgrp>
    <trd lng="en">instructive</trd>
    <trd lng="es">instructivo</trd>
    <trdgrp lng="fr">
      <trd>instructif</trd>
    </trdgrp>
    <trd lng="hu">tanuls&aacute;gos</trd>
    <trd lng="id">instruktif</trd>
    <trd lng="nl">leerrijk</trd>
    <trdgrp lng="pl">
      <trd>pouczaj&aogonek;cy</trd>,
      <trd>instrukta&zdot;owy</trd>,
      <trd>kszta&lstroke;c&aogonek;cy</trd>
    </trdgrp>
    <trd lng="pt">instrutivo</trd>
    <trdgrp lng="ru">
      <trd>
        &c_p;&c_o;&c_u;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;
      </trd>,
      <trd>&c_o;&c_b;&c_u;&c_ch;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>
    </trdgrp>
    <trd lng="vo">tidik</trd>
  </snc>
  <snc mrk="instru.0a.0rilata">
    <dif>
      Rilata al <tld/>ado:
      <ekz>
        al la bordo de Nesku&ccirc;nyj sin apogis <tld/>a &scirc;ipo
        <fnt><bib>Metrop</bib><lok>&ccirc;apitro 11a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="instru.pri0a">pri<tld/>a</ref>
    <trdgrp lng="en">
      <trd>instructional</trd>,
      <trd>teaching</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>de <ind>instrucci&oacute;n</ind></trd>,
      <trd>de <ind>ense&ntilde;anza</ind></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d'<ind>instruction</ind></trd>,
      <trd>d'<ind>enseignement</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>tan&iacute;t&aacute;si</trd>,
      <trd>tan-</trd>,
      <trd>oktat&aacute;si</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Lehr-</trd>,
      <trd>Schul-</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>pouczaj&aogonek;cy</trd>,
      <trd>instrukta&zdot;owy</trd>,
      <trd>kszta&lstroke;c&aogonek;cy</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_u;&c_ch;&c_je;&c_b;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_o;&c_b;&c_u;&c_ch;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>instruktivn&iacute;</trd>,
    <trd>pou&ccaron;n&yacute;</trd>,
    <trd>vyu&ccaron;ovac&iacute;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pou&ccaron;n&yacute;</trd>,
    <trd>vyu&ccaron;ovac&iacute;</trd>
  </trdgrp>
</drv>

<drv mrk="instru.0o">
  <kap><tld/>o</kap>
  <snc mrk="instru.0o.instruado">
    <dif>
      <ref tip="dif" cel="instru.0ado"><tld lit="I"/>ado</ref>:
      <ekz>
        doni <tld/>on;
      </ekz><ekz>
        preni de io <tld/>on<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="es">ense&ntilde;anza</trd>
    <trd lng="hu">tan&iacute;t&aacute;s</trd>
    <trdgrp lng="pl">
      <trd>uczenie</trd>,
      <trd>nauczanie</trd>,
      <trd>pouczanie</trd>,
      <trd>instruowanie</trd>,
      <trd>kszta&lstroke;cenie</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>instru&ccedil;&atilde;o<klr tip="amb"> (a&ccedil;&atilde;o de
          instruir)</klr></trd>,
      <trd>ensino<klr tip="amb"> (a&ccedil;&atilde;o de
          instruir)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="instru.0o.instruajxo">
    <dif>
      <ref tip="dif" cel="instru.0ajxo"><tld lit="I"/>a&jcirc;o</ref>:
      <ekz>
        kano <klr>(batado)</klr> kaj <tld/>o donas sa&gcirc;on
        <fnt><bib>MT</bib>, <lok>&Sen; 29:15</lok></fnt>;
      </ekz><ekz>
        la bu&scirc;o de sa&gcirc;uloj semas <tld/>on
        <fnt><bib>MT</bib>, <lok>&Sen; 15:7</lok></fnt>;
      </ekz><ekz>
        a&ubreve;skulti, observi, mal&scirc;ati ies <tld/>ojn;
      </ekz><ekz>
        profiti la <tld/>ojn de profesoro;
      </ekz><ekz>
        la&ubreve; la plej fre&scirc;aj <tld/>oj de la scienco;
      </ekz><ekz>
        a&ubreve;skulti la <tld/>ojn de la vivo
          <fnt>
            <bib>MT</bib>
            <lok>Sentencoj 15:31</lok>
          </fnt>, de la
        historio;
      </ekz><ekz>
        tiu terura ekzemplo servu al vi kiel <tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="es">
      <trd>ense&ntilde;anza</trd>,
      <trd>conocimiento</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>enseignement<klr tip="ind"> (ce qu'on
      enseigne)</klr></trd>,
      <trd>le&ccedil;on<klr tip="ind"> (enseignement)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>tanult ismeret</trd>,
      <trd>tanuls&aacute;g</trd>
    </trdgrp>
    <trd lng="id">peng<ind>ajar</ind>an</trd>
    <trdgrp lng="pl">
      <trd>nauka</trd>,
      <trd>pouczenie</trd>,
      <trd>instrukta&zdot;</trd>,
      <trd>kszta&lstroke;cenie</trd>,
      <trd>wskaz&oacute;wka</trd>,
      <trd>nauczka<klr> (przen.)</klr></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>uma <ind>li&ccedil;&atilde;o</ind></trd>,
      <trd>um <ind>ensinamento</ind></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>instrukce</trd>,
    <trd>instrukt&aacute;&zcaron;</trd>,
    <trd>instruov&aacute;n&iacute;</trd>,
    <trd>pou&ccaron;en&iacute;</trd>,
    <trd>u&ccaron;en&iacute;</trd>,
    <trd>vyu&ccaron;ov&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>in&scaron;trukcia</trd>,
    <trd>n&aacute;uka</trd>,
    <trd>ponau&ccaron;enie</trd>,
    <trd>u&ccaron;enie</trd>,
    <trd>vyu&ccaron;ovanie</trd>
  </trdgrp>
  <trd lng="vo">tidam</trd>
  <adm>
     Indus reverki &ccirc;i tiun deriva&jcirc;on la&ubreve; PIV, kiu
     konservas
     nur la sencon "instru(it/at)a&jcirc;(ar)o". Efektive la ekzemploj
     por la senco "instruado" tute ne konvinkas,
     ke ili ne povas esti interpretataj per "instrua&jcirc;o". Cetere,
     cxar "instrua&jcirc;o" estas tre malofta, oni prefere difinu
     &gcirc;in
     referencante al "instruo".
     <aut>MB</aut>
   </adm>
</drv>

<drv mrk="instru.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="instru.0ado.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      Ago, <ref tip="lst" cel="art.0o.tehxniko" lst="voko:artoj">arto</ref> 
      de tiu, kiu <tld/>as:
      <ekz>
        nova metodo por <tld/>ado;
      </ekz><ekz>
        la ministro de la publika <tld/>ado
        <fnt>(<aut>Zamenhof</aut>: <vrk>Parolo anta&ubreve;
            la Unua Kongreso Esperantista en Boulogne sur Mer,
            1905</vrk>)</fnt>;
      </ekz><ekz>
        ricevi bonan <tld/>adon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_v;&c_u;&c_ch;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_u;&c_ch;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>
      &c_v;&c_y;&c_k;&c_l;&c_a;&c_d;&c_a;&c_n;&c_mol;&c_n;&c_je;
    </trd>,
    <trd>&c_a;&c_d;&c_u;&c_k;&c_a;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>instruov&aacute;n&iacute;</trd>,
    <trd>pou&ccaron;ov&aacute;n&iacute;</trd>,
    <trd>u&ccaron;en&iacute;</trd>,
    <trd>vyu&ccaron;ov&aacute;n&iacute;</trd>,
    <trd>v&yacute;uka</trd>,
    <trd>&scaron;kolen&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Lehre</trd>,
    <trd>Unterricht</trd>,
    <trd>P&auml;dagogik</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>teaching</trd>,
    <trd>instruction</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>ense&ntilde;anza</trd>,
    <trd>instrucci&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>enseignement<klr tip="ind"> (fait d'enseigner)</klr></trd>,
    <trd>instruction<klr tip="ind"> (fait d'instruire)</klr></trd>
  </trdgrp>
