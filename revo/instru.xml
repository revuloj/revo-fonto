<drv mrk="instru.0itulo">
  <kap><tld/>itulo</kap>
  <snc mrk="instru.0itulo.klerulo">
    <dif>
      <ref tip="dif" cel="kler.0ulo">Klerulo</ref>:
      <ekz>
        <tld/>itulo entreprenis gravan sciencan laboron, sed ne havis la rimedojn por
        &gcirc;in efektivigi
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>itulo eliris sur la balkonon, <klr>[&#8230;]</klr> kaj apena&ubreve; la steloj
        ekellumis el la klara etero, li eksentis novan vivon
        <fnt>
          <bib>DL</bib>
          <lok>Dua Libro de l' Lingvo Internacia</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel la <tld/>ituloj scii&gcirc;is pri la nomo de &ccirc;iu stelo &mdash; tion &ccirc;i mi jam neniel
        povas kompreni
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la homoj, kiuj okupas sin je &ccirc;ia plej sencela kaj senutila
        sensenca&jcirc;o, se &gcirc;i nur estas en modo kaj konforma al la rutinaj ideoj de la
        amaso, &gcirc;uas <klr>[...]</klr> la honoran nomon de <tld/>ituloj
        <fnt>
          <bib>EE</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_u;&c_k;&c_a;&c_v;&c_a;&c_n;&c_y;
    &c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;</trd>
  <trd lng="cs">v&ecaron;dec</trd>
  <trd lng="de">Gelehrter</trd>
  <trdgrp lng="es">
    <trd>erudito</trd>,
    <trd>cient&iacute;fico</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>clerc<klr tip="ind"> (personne cultiv&eacute;e)</klr></trd>,
    <trd>lettr&eacute;<klr tip="ind"> (subst.)</klr></trd>,
    <trd>savant<klr tip="ind"> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tanult ember</trd>,
    <trd>m&udblac;velt ember</trd>
  </trdgrp>
  <trd lng="nl">ontwikkeld man</trd>
  <trdgrp lng="pl">
    <trd><klr>kto&sacute;</klr> <ind>wyszkolony</ind></trd>,
    <trd><klr>kto&sacute;</klr> <ind>wyedukowany</ind></trd>,
    <trd><klr>kto&sacute;</klr> <ind>wykszta&lstroke;cony</ind></trd>,
    <trd><klr>kto&sacute;</klr> <ind>nauczony</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>s&aacute;bio</trd>,
    <trd>homem <ind>instru&iacute;do</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_b;&c_r;&c_a;&c_z;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>
  <trd lng="sk">vedec</trd>
  <trd lng="uk">&c_u;&c_ch;&c_je;&c_n;&c_i;&c_j;</trd>
</drv>

<drv mrk="instru.mal0i">
