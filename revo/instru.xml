<drv mrk="instru.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="instru.0i.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      Transdoni al iu sciencajn metodajn konojn a&ubreve; utilajn 
      precizajn sciojn:
      <ekz>
        oni instruis gramatikon, dialektikon kaj retorikon
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili intencis min <tld/>i pri sia lingvo
        <fnt>
          <bib>VaK</bib>
          <lok>Tria &Ccirc;apitro</lok>
        </fnt>;
      </ekz>      
      <ekz>
        la Eternulo ordonis <tld/>i al vi la le&gcirc;ojn
        <fnt><bib>MT</bib>, <lok>&Rea; 4:14</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i distingi inter sankta&jcirc;o kaj nesankta&jcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 44:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiujn do objektojn vi povas kaj deziras <tld/>i
        <fnt>
          <bib>Marta</bib>
        </fnt>?
      </ekz>
      <ekz>
        mi volus <tld/>i la komencojn de historio, de geografio, de la historio de nia
        literaturo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        por ke la <tld/>itaj lingvistoj povu tuj kompreni la
        tekston
        <fnt><bib>EE</bib>, <lok>&ccirc;apitro 7a</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>ita mondo
        <fnt>
          <bib>FK</bib>
          <lok>Nokto</lok>
        </fnt>;
      </ekz>
      <ekz>
        tre alte <tld/>ita homo
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiaj <tld/>itaj <klr>(de <tld/>itulo)</klr> paroloj tre
        imponas
        <fnt><bib>EE</bib>, <lok>&ccirc;apitro 3a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="eduk.0i">eduki</ref>,
      <ref cel="lern.0igi">lernigi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>unterrichten</trd>,
      <trd>lehren</trd>,
      <trd>unterweisen</trd>
    </trdgrp>
  </snc>
  <snc mrk="instru.0i.ekzerci">
    <dif>
      Metode <ref tip="super" cel="ekzerc.0i">ekzerci</ref> iun en iu arto, okupo kc:
      <ekz>
        li <tld/>as miajn brakojn stre&ccirc;i pafarkon
        <fnt><bib>MT</bib>, <lok>&Sa2; 22:35</lok></fnt>;
      </ekz>
      <ekz>
        li <tld/>is al ili la &ccirc;asadon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        nun vi <tld/>is al mi la arton, kiel mi devas vin turmenti
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        virinoj <tld/>is al mi sa&gcirc;on kaj prudenton
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        al la fi&scirc;o ne <tld/>u na&gcirc;arton
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="trejn.0i">trejni</ref>
    <trdgrp lng="de">
      <trd>beibringen</trd>,
      <trd>trainieren</trd>
    </trdgrp>
  </snc>
  <snc mrk="instru.0i.kiel_agi">
    <dif>
      <ref tip="super" cel="montr.0i.parole">Montri</ref> al iu precize kiel agi, konduti:
      <ekz>
        mi <tld/>os vin, kion vi devas paroli
        <fnt><bib>MT</bib>, <lok>&Eli; 4:12</lok></fnt>;
      </ekz>
      <ekz>
        mi al ili &ccirc;iuj <tld/>is disciplinon
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        se vi kontra&ubreve;parolos al mi, mi vin <tld/>os, kio estas moki nin
        <fnt>
          <bib>GDn</bib>
          <lok>AKTO TRIA</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="mentor.0i">mentori</ref>
    <trdgrp lng="de">
      <trd>vorf&uuml;hren</trd>,
      <trd>vormachen</trd>,
      <trd>zeigen</trd>
    </trdgrp>
  </snc>
  <snc mrk="instru.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <klr>(p.p. okaza&jcirc;oj)</klr>
      <ref tip="super" cel="montr.0i.parole">Montri</ref>
      al iu la la&ubreve;celan konduton:
      <ekz>
        tio <tld/>os al vi singardemon
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 18a</lok></fnt>;
      </ekz>
      <ekz>
        mi instruos al vi, kio estas timo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fabelo pri iu, kiu migris por ekkoni timon</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>as mizero man&gcirc;i panon sen butero
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        nin <tld/>as eraro, kiun faras najbaro
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        e&ccirc; malsa&gcirc;ulon sperto <tld/>os
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="instru1">instrukcii</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>lehren</trd>,
      <trd>beibringen</trd>
    </trdgrp>
  </snc>
  <trd lng="af">leer</trd>
  <trd lng="am">&#4768;&#4661;&#4720;&#4637;&#4653;</trd>
