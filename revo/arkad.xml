<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>arkad</rad>/o
</kap>
<drv mrk="arkad.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Arka&jcirc;o sur kolonoj, prezentanta sub si
      malplena&jcirc;on:
      <ekz>
        promeni sub la <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">arcade</trd>
  <trd lng="ru">&c_a;&c_r;&c_k;&c_a;&c_d;&c_a;</trd>
</drv>

<drv mrk="arkad.0aro">
  <kap><tld/>aro</kap>
  <snc>
    <dif>
      Vico da <tld/>oj.
    </dif>
  </snc>
</drv>
</art>
