<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kamel</rad>/o
</kap>
<drv mrk="kamel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="kameled.0oj"><tld/>edoj</ref>
      (<trd lng="la">Camelus</trd>), kun unu a&ubreve; du &gcirc;iboj
      &ccirc;e la dorso, vivanta en Azio kaj Afriko.  
    </dif>
  </snc>
  <trd lng="de">Kamel</trd>
  <trd lng="en">camel</trd>
  <trd lng="fr">chameau</trd>
  <trd lng="nl">kameel</trd>
  <trd lng="ru">&c_v;&c_je;&c_r;&c_b;&c_l;&c_ju;&c_d;</trd>
  <trd lng="sv">kamel</trd>
  <trd lng="tr">deve</trd>
</drv>

</art>
