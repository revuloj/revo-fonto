<art mrk="$Id: aklam.xml,v 1.18 2022/03/11 13:29:02 revo Exp $">
<kap>
  <ofc>3</ofc><rad>aklam</rad>/i
</kap>
<drv mrk="aklam.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Saluti, akcepti, aprobi per favoraj krioj:
      <ekz>
        <tld/>i iun, al iu;
      </ekz>
      <ekz>
        <tld/>e oni nomis lin prezidanto.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kri.0i">krii</ref>,
      <ref cel="aplaux.0i">apla&ubreve;di</ref>,
      <ref cel="ovaci.0i">ovacii</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_u;&c_r;&c_a;&c_ch;&c_y;&c_s;&c_t;&c_a; &c_v;&c_ib;&c_t;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>aklamac&#xED; schvalovat</trd>,
    <trd>aklamovat</trd>,
    <trd>provol&#xE1;vat sl&#xE1;vu</trd>,
    <trd>ve&#x159;ejn&#x11B; hlasovat</trd>,
    <trd>ve&#x159;ejn&#x11B; souhlasit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>freudig <ind>begr&uuml;&szlig;en</ind></trd>,
    <trd>bejubeln</trd>,
    <trd>zustimmen</trd>,
    <trd>Beifall spenden</trd>
  </trdgrp>
  <trd lng="en">acclaim</trd>
  <trd lng="es">aclamar</trd>
  <trd lng="fr">acclamer</trd>
  <trd lng="he">&#x5DC;&#x5D4;&#x5E8;&#x5D9;&#x5E2;</trd>
  <trd lng="nl">toejuichen</trd>
  <trd lng="pt">aclamar</trd>
  <trd lng="ru">&c_t;&c_o;&c_r;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o; <ind>&c_p;&c_r;&c_i;&c_v;&c_je;&c_t;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</ind></trd>
  <trdgrp lng="sk">
    <trd>prevol&#xE1;va&#x165; sl&#xE1;vu</trd>,
    <trd>verejne hlasova&#x165;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>bifallsrop</trd>,
    <trd>acklamation</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x443;&#x440;&#x43E;&#x447;&#x438;&#x441;&#x442;&#x43E; &#x432;&#x456;&#x442;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x443;&#x43B;&#x430;&#x448;&#x442;&#x443;&#x432;&#x430;&#x442;&#x438; &#x43E;&#x432;&#x430;&#x446;&#x456;&#x44E;</trd>,
    <trd>&#x437;&#x443;&#x441;&#x442;&#x440;&#x456;&#x442;&#x438; &#x440;&#x430;&#x434;&#x456;&#x441;&#x43D;&#x438;&#x43C;&#x438; &#x43E;&#x43A;&#x43B;&#x438;&#x43A;&#x430;&#x43C;&#x438;</trd>
  </trdgrp>
</drv>
</art>
