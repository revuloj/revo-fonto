<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kasaci.xml,v 1.15 2023/10/18 14:48:10 revo Exp $">
<kap>
  <rad>kasaci</rad>/i
  <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="kasaci.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      <klr>(tribunalan ju&gcirc;on, dekreton, proceduron, verdikton)</klr>
      Le&gcirc;e nuligi:
      <ekz>
        <tld/>an plendon la kortumo ne permesis
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2008/009472.php"
              >Kaptukoj kaj kapdoloroj</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        la franclingva Ligo por la Homaj Rajtoj jam anoncis, ke &gcirc;i apelacios &ccirc;e la <ind><tld
        lit="K"/>a Kortumo</ind> kaj provos nuligi la el&scirc;tatigon de krimuloj
        <fnt>
          <bib>Monato</bib>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="&Monato;/2010/010000.php"
              >Belgio eksportas siajn krimulojn</url></vrk>
          <lok>2010</lok>
        </fnt>;
        <trd lng="de">Kassationsgerichtshof</trd>
      </ekz>
      <ekz>
        ekster &ccirc;ia dubo, post deko da jaroj e&ubreve;ropa <tld/>a tribunalo malvalidigos la
        le&gcirc;on
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos I Font</aut>
          <vrk><url ref="&Monato;/2003/008145.html"
              >150 000 orfoj</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="abol.0i">aboli</ref>,
      <ref cel="apelac.0i.JUR">apelacii<sncref/></ref>,
      <ref cel="nul0.0igi">nuligi</ref>,
      <ref cel="revizi.0i.JUR">revizii<sncref/></ref>,
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_s;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
     <trd><klr>(ein Urteil) </klr>kassieren</trd>,
     <trd><klr>(ein Urteil) </klr>aufheben</trd>,
     <trd><klr>(ein Urteil) </klr>f&uuml;r <ind>nichtig</ind> erkl&auml;ren</trd>,
     <trd><klr>(ein Urteil) </klr>f&uuml;r <ind>ung&uuml;ltig</ind> erkl&auml;ren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>overturn</trd>,
    <trd>quash</trd>,
    <trd>vacate</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>casser<klr tip="ind"> (un jugement)</klr></trd>,
    <trd>annuler<klr tip="ind"> (en cassation)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>megsemmis&iacute;t <klr tip="ind">(jogilag)</klr></trd>,
     <trd>&eacute;rv&eacute;nytelen&iacute;t</trd>,
     <trd>hat&aacute;lyon k&iacute;v&uuml;l helyez</trd>,
     <trd>hat&aacute;lytalan&iacute;t</trd>
  </trdgrp>
  <trd lng="ja">&#30772;&#26820;&#12377;&#12427;  <pr>&#12399;&#12365;&#12377;&#12427;</pr></trd>
  <trdgrp lng="nl">
    <trd>casseren</trd>,
    <trd>nietig verklaren</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>anulowa&cacute;</trd>,
    <trd>kasowa&cacute;</trd>,
    <trd>uniewa&zdot;nia&cacute;</trd>,
    <trd>zerowa&cacute;</trd>,
    <trd>znosi&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>anula</trd>,
    <trd>abroga</trd>,
    <trd>reduce la zero</trd>,
    <trd>aboli</trd>,
    <trd>revoca</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_s;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_m;&c_je;&c_n;&c_i;&c_t;&c_mol; <klr>(&c_r;&c_je;&c_sh;&c_je;&c_n;&c_i;&c_je; &c_s;&c_u;&c_d;&c_a; &c_i; &c_t;.&c_p;.)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_s;&c_o;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i; (&c_r;&c_ib;&c_sh;&c_je;&c_n;&c_n;&c_ja;)</trd>
  </trdgrp>
</drv>

<drv mrk="kasaci.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Le&gcirc;a nuligo al tribunala ju&gcirc;o, dekreto, proceduro, verdikto.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_s;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="de">
     <trd>Kassation</trd>,
     <trd>Aufhebung</trd>,
     <trd>Nichtigkeitserkl&auml;rung</trd>,
     <trd>Ung&uuml;ltigmachung</trd>
  </trdgrp>
  <trd lng="fr">cassation<klr tip="ind"> (droit)</klr></trd>
  <trdgrp lng="hu">
     <trd>megsemmis&iacute;t&eacute;s <klr tip="ind">(jogilag)</klr></trd>,
     <trd>&eacute;rv&eacute;nytelen&iacute;t&aacute;s</trd>,
     <trd>hat&aacute;lyon k&iacute;v&uuml;l helyez&eacute;s</trd>,
     <trd>hat&aacute;lytalan&iacute;t&aacute;s</trd>
  </trdgrp>
  <trd lng="ja">&#30772;&#26820;  <pr>&#12399;&#12365;</pr></trd>
  <trd lng="nl">cassatie</trd>
    <trdgrp lng="pl">
    <trd>anulowanie</trd>,
    <trd>kasacja</trd>,
    <trd>uniewa&zdot;nienie</trd>,
    <trd>zniesienie</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>anulare</trd>,
    <trd>abolire</trd>,
    <trd>invalidare</trd>,
    <trd>revocare</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_s;&c_s;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trd lng="uk">&c_k;&c_a;&c_s;&c_a;&c_c;&c_ib;&c_ja;</trd>
</drv>
</art>
