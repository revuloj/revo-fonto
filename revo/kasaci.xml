<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kasaci.xml,v 1.13 2023/09/01 13:58:57 revo Exp $">
<kap>
  <rad>kasaci</rad>/i
</kap>
<drv mrk="kasaci.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      <klr>(tribunalan ju&gcirc;on, dekreton, proceduron, verdikton)</klr>
      Le&gcirc;e nuligi:
      <ekz>
        la <tld/>a kortumo <tld/>is la verdikton de la unua instanco.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_s;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
     <trd>kassieren</trd>,
     <trd>aufheben</trd>,
     <trd>f&uuml;r nichtig erkl&auml;ren</trd>,
     <trd>f&uuml;r ung&uuml;ltig erkl&auml;ren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>overturn</trd>,
    <trd>quash</trd>,
    <trd>vacate</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>casser<klr tip="ind"> (un jugement)</klr></trd>,
    <trd>annuler<klr tip="ind"> (en cassation)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>megsemmis&iacute;t <klr tip="ind">(jogilag)</klr></trd>,
     <trd>&eacute;rv&eacute;nytelen&iacute;t</trd>,
     <trd>hat&aacute;lyon k&iacute;v&uuml;l helyez</trd>,
     <trd>hat&aacute;lytalan&iacute;t</trd>
  </trdgrp>
  <trd lng="ja">&#x7834;&#x68C4;&#x3059;&#x308B;  [<ind>&#x306F;&#x304D;&#x3059;&#x308B;</ind>]</trd>
  <trdgrp lng="nl">
    <trd>casseren</trd>,
    <trd>nietig verklaren</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>anulowa&cacute;</trd>,
    <trd>kasowa&cacute;</trd>,
    <trd>uniewa&zdot;nia&cacute;</trd>,
    <trd>zerowa&cacute;</trd>,
    <trd>znosi&cacute;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>anula</trd>,
    <trd>abroga</trd>,
    <trd>reduce la zero</trd>,
    <trd>aboli</trd>,
    <trd>revoca</trd>
  </trdgrp>                 
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_s;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_m;&c_je;&c_n;&c_i;&c_t;&c_mol; <klr>(&c_r;&c_je;&c_sh;&c_je;&c_n;&c_i;&c_je; &c_s;&c_u;&c_d;&c_a; &c_i; &c_t;.&c_p;.)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x430;&#x441;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x441;&#x43A;&#x430;&#x441;&#x43E;&#x432;&#x443;&#x432;&#x430;&#x442;&#x438; (&#x440;&#x456;&#x448;&#x435;&#x43D;&#x43D;&#x44F;)</trd>
  </trdgrp>
</drv>

<drv mrk="kasaci.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Le&gcirc;a nuligo al tribunala ju&gcirc;o, dekreto, proceduro, verdikto.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_s;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="de">
     <trd>Kassation</trd>,
     <trd>Aufhebung</trd>,
     <trd>Nichtigkeitserkl&auml;rung</trd>,
     <trd>Ung&uuml;ltigmachung</trd>
  </trdgrp>
  <trd lng="fr">cassation<klr tip="ind"> (droit)</klr></trd>
  <trdgrp lng="hu">
     <trd>megsemmis&iacute;t&eacute;s <klr tip="ind">(jogilag)</klr></trd>,
     <trd>&eacute;rv&eacute;nytelen&iacute;t&aacute;s</trd>,
     <trd>hat&aacute;lyon k&iacute;v&uuml;l helyez&eacute;s</trd>,
     <trd>hat&aacute;lytalan&iacute;t&aacute;s</trd>
  </trdgrp>
  <trd lng="ja">&#x7834;&#x68C4;  [<ind>&#x306F;&#x304D;</ind>]</trd>
  <trd lng="nl">cassatie</trd>
    <trdgrp lng="pl">
    <trd>anulowanie</trd>,
    <trd>kasacja</trd>,
    <trd>uniewa&zdot;nienie</trd>,
    <trd>zniesienie</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>anulare</trd>,
    <trd>abolire</trd>,
    <trd>invalidare</trd>,
    <trd>revocare</trd>
  </trdgrp> 
  <trd lng="ru">&c_k;&c_a;&c_s;&c_s;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trd lng="uk">&#x43A;&#x430;&#x441;&#x430;&#x446;&#x456;&#x44F;</trd>
</drv>
</art>
