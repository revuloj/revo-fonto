<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kasaci</rad>/i
</kap>
<drv mrk="kasaci.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Le&gcirc;e nuligi tribunalan ju&gcirc;on, dekreton, proceduron, verdikton:
      <ekz>
        la <tld/>a kortumo <tld/>is la verdikton de la unua instanco.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>overturn</trd>,
    <trd>quash</trd>,
    <trd>vacate</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>casseren</trd>,
    <trd>nietig verklaren</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_s;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_m;&c_je;&c_n;&c_i;&c_t;&c_mol; <klr>(&c_r;&c_je;&c_sh;&c_je;&c_n;&c_i;&c_je; &c_s;&c_u;&c_d;&c_a; &c_i; &c_t;.&c_p;.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="kasaci.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Le&gcirc;a nuligo al tribunala ju&gcirc;o, dekreto, proceduro, verdikto.
    </dif>
  </snc>
  <trd lng="nl">cassatie</trd>
  <trd lng="ru">&c_k;&c_a;&c_s;&c_s;&c_a;&c_c;&c_i;&c_ja;</trd>
</drv>
</art>
