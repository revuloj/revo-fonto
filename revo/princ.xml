<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>princ</rad>/o
    </kap>
    <drv mrk="princ.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="princ.0o.regnestro">
        <dif>
          Regnestro de malgranda &scirc;tato. 
        </dif>
      </snc>
      <snc mrk="princ.0o.regxido">
        <dif>
          Membro a&ubreve; posteulo de re&gcirc;a a&ubreve; imperiestra familio. 
        </dif>
        <ref tip="sin" cel="regx.0ido">re&gcirc;ido</ref>
      </snc>
      <snc mrk="princ.0o.nobelo">
        <dif>
          Nobelo plej altranga. 
        </dif>
      </snc>
      <trd lng="de">Prinz</trd>
      <trdgrp lng="ru">
        <trd>&c_k;&c_n;&c_ja;&c_z;&c_mol;</trd>,
        <trd>&c_p;&c_r;&c_i;&c_n;&c_c;</trd></trdgrp>
    </drv>
    <drv mrk="princ.kron0o">
      <kap>kron<tld/>o</kap>
      <snc>
        <dif>
          Re&gcirc;ido heredonta la re&gcirc;econ.  
        </dif>
      </snc>
      <trdgrp lng="ru">
        <trd>&c_n;&c_a;&c_s;&c_l;&c_je;&c_d;&c_n;&c_i;&c_k; &c_t;&c_r;&c_o;&c_n;&c_a;</trd>,
        <trd>&c_c;&c_je;&c_s;&c_a;&c_r;&c_je;&c_v;&c_i;&c_ch;</trd>,
        <trd>&c_k;&c_r;&c_o;&c_n;&c_p;&c_r;&c_i;&c_n;&c_c;</trd></trdgrp>
    </drv>
    <drv mrk="princ.0ido">
      <kap><tld/>ido</kap>
      <snc>
        <dif>
          Juna filo de <tld/>o.
        </dif>
      </snc>
      <trd lng="ru">&c_k;&c_n;&c_ja;&c_zh;&c_i;&c_ch;</trd>
    </drv>
    <drv mrk="princ.0idino">
      <kap><tld/>idino</kap>
      <snc>
        <dif>
          Needzini&gcirc;inta filino de <tld/>o:
          <ekz>
            <ctl><tld lit="P"/>idino Mary</ctl>
            <klr>(romano de M. Lermontov)</klr>
          </ekz>
        </dif>
      </snc>
      <trd lng="ru">&c_k;&c_n;&c_ja;&c_zh;&c_n;&c_a;</trd>
    </drv>
  </art>
