<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ekskoriaci</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ekskor.0o">
  <kap><tld/>o</kap>
  <snc mrk="ekskor.0o.MED">
    <uzo tip="fak">MED</uzo>
    <ref tip="sin" cel="vund.skrap0o.MED">skrapvundo</ref>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_a;&c_p;&c_ib;&c_n;&c_a;</trd>
  <trd lng="ca">excoriaci&oacute;</trd>
  <trdgrp lng="de">
    <trd>Hautabsch&uuml;rfung</trd>
  </trdgrp>
  <trd lng="fr">excoriation</trd>
  <trdgrp lng="hu">
    <trd>b&odblac;rhorzsol&aacute;s</trd>,
    <trd>b&odblac;rkimar&oacute;d&aacute;s</trd>
  </trdgrp>
  <trd lng="it">escoriazione</trd>
  <trdgrp lng="pl">
    <trd>otarcie<klr> nask&oacute;rka</klr></trd>,
    <trd>zadrapanie</trd>
  </trdgrp>
  <trd lng="pt">escoria&ccedil;&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_s;&c_a;&c_d;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_c;&c_a;&c_r;&c_a;&c_p;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="ekskor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ekskor.0i.MED">
    <ref tip="sin" cel="vund.skrap0i.MED">skrapvundi</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_r;&c_a;&c_p;&c_a;&c_c;&c_mol;
  <klr>(&c_s;&c_k;&c_u;&c_r;&c_u;)</klr></trd>
  <trd lng="ca">excoriar</trd>
  <trdgrp lng="de">
    <trd>die Haut <ind>absch&uuml;rfen</ind></trd>,
    <trd>wund reiben</trd>
  </trdgrp>
  <trd lng="fr">excorier</trd>
  <trd lng="hu">lehorzsolja a b&odblac;rt</trd>
  <trdgrp lng="pl">
    <trd>obetrze&cacute;<klr> nask&oacute;rek</klr></trd>,
    <trd>zadrapa&cacute;</trd>,
    <trd>podrapa&cacute;</trd>
  </trdgrp>
  <trd lng="it">escoriare</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_n;&c_je;&c_s;&c_t;&c_i;
    &c_s;&c_s;&c_a;&c_d;&c_i;&c_n;&c_u;</trd>,
    <trd>&c_p;&c_o;&c_c;&c_a;&c_r;&c_a;&c_p;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<adm>
  PIV forigis la radikon kaj anstata&ubreve;igis &gcirc;in per
  "ekskori" kun la respondaj deriva&jcirc;oj: ~a&jcirc;o, ~i.
  <aut>MB</aut>
</adm>
</art>
