<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ekskor.xml,v 1.19 2023/11/07 19:22:36 revo Exp $">
<kap>
  <rad>ekskoriaci</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ekskor.0o">
  <kap><tld/>o</kap>
  <snc mrk="ekskor.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="sin" cel="vund.skrap0o.MED">Skrapvundo</ref>:
      <ekz>
        la konstanta malseki&gcirc;ado de la ha&ubreve;to en la anguloj de la palpebroj kondukas al
        la formi&gcirc;ado de <tld/>oj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        jukado kaj gratado povas ka&ubreve;zi dermatomojn, <tld/>ojn, ne&ubreve;rozon, nervozecon,
        sendormecon <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=227441">Oksiurozo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_a;&c_p;&c_ib;&c_n;&c_a;</trd>
  <trdgrp lng="bg">
    <trd>&c_o;&c_b;&c_je;&c_l;&c_v;&c_a;&c_n;&c_je;</trd>,
    <trd>&c_je;&c_k;&c_s;&c_k;&c_o;&c_r;&c_i;&c_a;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="ca">excoriaci&oacute;</trd>
  <trdgrp lng="cs">
    <trd>od&ecaron;rka</trd>,
    <trd>od&rcaron;enina</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hautabsch&uuml;rfung</trd>
  </trdgrp>
  <trd lng="en">excoriation</trd>
  <trd lng="fr">excoriation</trd>
  <trdgrp lng="hu">
    <trd>b&odblac;rhorzsol&aacute;s</trd>,
    <trd>b&odblac;rkimar&oacute;d&aacute;s</trd>
  </trdgrp>
  <trd lng="it">escoriazione</trd>
  <trdgrp lng="pl">
    <trd>otarcie<klr> nask&oacute;rka</klr></trd>,
    <trd>zadrapanie</trd>
  </trdgrp>
  <trd lng="pt">escoria&ccedil;&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_s;&c_a;&c_d;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_c;&c_a;&c_r;&c_a;&c_p;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">&scaron;krabnutie na ko&zcaron;i</trd>
</drv>

<drv mrk="ekskor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ekskor.0i.MED">
    <ref tip="sin" cel="vund.skrap0i.MED">skrapvundi</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_r;&c_a;&c_p;&c_a;&c_c;&c_mol;
  <klr>(&c_s;&c_k;&c_u;&c_r;&c_u;)</klr></trd>
  <trd lng="ca">excoriar</trd>
  <trdgrp lng="cs">
    <trd>od&rcaron;&iacute;t</trd>,
    <trd>pohmo&zcaron;dit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>die Haut <ind>absch&uuml;rfen</ind></trd>,
    <trd>wund reiben</trd>
  </trdgrp>
  <trd lng="fr">excorier</trd>
  <trd lng="he">&#x5DC;&#x5D4;&#x5EA;&#x5E2;&#x5DC;&#x5DC;</trd>
  <trd lng="hu">lehorzsolja a b&odblac;rt</trd>
  <trd lng="it">escoriare</trd>
  <trdgrp lng="pl">
    <trd>obetrze&cacute;<klr> nask&oacute;rek</klr></trd>,
    <trd>zadrapa&cacute;</trd>,
    <trd>podrapa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_n;&c_je;&c_s;&c_t;&c_i;
    &c_s;&c_s;&c_a;&c_d;&c_i;&c_n;&c_u;</trd>,
    <trd>&c_p;&c_o;&c_c;&c_a;&c_r;&c_a;&c_p;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">porani&tcaron; ko&zcaron;u &scaron;krabnut&iacute;m</trd>
  <trd lng="zh">&#x64E6;&#x4F24; <pr>c&#x101;sh&#x101;ng</pr></trd>
</drv>
<adm>
  PIV forigis la radikon kaj anstata&ubreve;igis &gcirc;in per
  "ekskori" kun la respondaj deriva&jcirc;oj: ~a&jcirc;o, ~i.
  <aut>MB</aut>
</adm>
</art>
