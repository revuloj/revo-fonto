<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pranc.xml,v 1.17 2022/06/21 08:45:32 revo Exp $">
<kap>
  <rad>pranc</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pranc.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="pranc.0i.RAJ">
    <uzo tip="klr">(pri &ccirc;evalo)</uzo>
    <dif>
      Ribele starigi sin sur postaj piedoj.
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_t;&c_a;&c_n;&c_a;&c_v;&c_ib;&c_c;&c_c;&c_a; &c_n;&c_a; &c_d;&c_y;&c_b;&c_k;&c_ib;</trd>,
      <trd>&c_s;&c_t;&c_a;&c_n;&c_a;&c_v;&c_ib;&c_c;&c_c;&c_a; &c_d;&c_y;&c_b;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="de"><klr>sich </klr>b&auml;umen</trd>
    <trd lng="en">prance</trd>
    <trd lng="fr">se <ind>cabrer</ind></trd>
    <trd lng="hu">&aacute;gaskodik</trd>
    <trd lng="nl">steigeren</trd>
    <trd lng="pl">stawa&cacute; d&eogonek;ba  <klr>(o koniu)</klr></trd>
    <trdgrp lng="pt">
      <trd>empinar-se</trd>,
      <trd>encabritar-se</trd>
    </trdgrp>
    <trd lng="ro">se  <ind>ridica</ind>  <klr>(despre un cal)</klr></trd>
    <trd lng="ru">&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_i;&c_t;&c_mol;&c_s;&c_ja; &c_n;&c_a; &c_d;&c_y;&c_b;&c_y;</trd>
  </snc>
  <snc mrk="pranc.0i.AMOR">
    <uzo tip="stl">VULG</uzo>
    <uzo tip="klr">(precipe pri viro, pri peniso)</uzo>
    <dif>
      Sperti erekti&gcirc;on,
      <ref tip="dif" cel="erekt.0igxi.FIZL">erekti&gcirc;i</ref>:
      <ekz>
        dum <tld/>is mia kompatinda Stango
        <fnt>
        <aut>P. Peneter</aut>,
        <vrk><url ref="http://eo.wikisource.org/wiki/Sekretaj_Sonetoj/I">
        Sekretaj Sonetoj</url></vrk>,
        <lok>1932</lok>
        </fnt>.
      </ekz>
      </dif>
      <trd lng="be">&c_u;&c_s;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_p;&c_r;&c_a; &c_ch;&c_e;&c_l;&c_ja;&c_s;)</klr></trd>
    <trd lng="fr">bander<klr> (vulg.)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>dupat</trd>,
    <trd>hrabat</trd>,
    <trd>kopat</trd>,
    <trd>naparovat se</trd>,
    <trd>vzp&#xED;nat se <klr>(k&#x16F;&ncaron;)</klr></trd>
  </trdgrp>
  
  <trd lng="sk">vzp&#xED;na&tcaron; sa</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_a;&c_v;&c_a;&c_t;&c_i; &c_d;&c_i;&c_b;&#x43A;&c_i;</trd>,
    <trd>&c_g;&c_a;&c_r;&c_c;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_h;&#x43E;&c_d;&c_i;&c_t;&c_i; &c_g;&#x43E;&c_g;&#x43E;&#x43B;&c_je;&#x43C;</trd>,
    <trd>&#x43A;&#x43E;&c_z;&c_i;&c_r;&c_i;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_i;&c_r;&c_a;&c_t;&c_i; &#x43D;&#x43E;&c_s;&c_a;/&#x43D;&c_ib;&c_s;</trd>
  </trdgrp>
</drv>
</art>
