<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pranc.xml,v 1.22 2023/10/20 16:38:17 revo Exp $">
<kap>
  <rad>pranc</rad>/i
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pranc.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="pranc.0i.RAJ">
    <uzo tip="klr">(pri &ccirc;evalo)</uzo>
    <dif>
      <ref tip="super" cel="stamf.0i">Stamfi</ref> per la anta&ubreve;aj piedoj:
      <ekz>
        la sova&gcirc;a bovo stampfas, <tld/>as, ba&ubreve;mas, provas &ccirc;iel skufaligi la vireton
        <fnt>
          <bib>KrB</bib>
          <lok>Ripetado estas plej bona lernado</lok>
        </fnt>;
      </ekz>
      <ekz>
        en Grekio <tld/>as ne nur koleri&gcirc;intaj &ccirc;evaloj kaj artifike modifitaj
        motorcikloj, sed, de post la komenco de la ekonomia krizo, anka&ubreve; senlaboreco
        kaj <klr>[&#x2026;]</klr> senespero
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.be/2012/010708.php">Mil senlaboruloj tage</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bauxm.0i.RAJ">ba&ubreve;mi<sncref/></ref>,
      <ref cel="kalcitr.0i">kalcitri</ref>,
      <ref cel="karako.0i">karakoli</ref>
    </refgrp>
    <rim>
      En <fnt><bib>PV</bib></fnt> la vorto estis ankora&ubreve; difinita samsignife kiel &leftquot;ba&ubreve;mi&rightquot;,
      dum <fnt><bib>PIV1</bib></fnt> &scirc;an&gcirc;is &gcirc;in pli malpli samsignife al
      &leftquot;karakoli&rightquot;, kiu respondas al la angla &leftquot;<nac>to prance</nac>&rightquot;, sed kiu tamen ne plu
      harmonias kun la vulgara senco. Do prefere oni uzu la referencitajn vortojn, kiuj
      ne trairis tian metamorfozon.
    </rim>
    <trdgrp lng="be">
      <trd>&c_s;&c_t;&c_a;&c_n;&c_a;&c_v;&c_ib;&c_c;&c_c;&c_a; &c_n;&c_a; <ind>&c_d;&c_y;&c_b;&c_k;&c_ib;</ind></trd>,
      <trd>&c_u;&c_z;&c_mol;&c_n;&c_ib;&c_m;&c_a;&c_c;&c_c;&c_a; &c_n;&c_a; <ind>&c_d;&c_y;&c_b;&c_k;&c_ib;</ind></trd>,
      <trd>&c_s;&c_t;&c_a;&c_n;&c_a;&c_v;&c_ib;&c_c;&c_c;&c_a; <ind>&c_d;&c_y;&c_b;&c_k;&c_a;</ind></trd>
    </trdgrp>
    <trd lng="de">t&auml;nzeln</trd>
    <trd lng="en">prance</trd>
    <trd lng="fr">caracoler</trd>
<!-- tradukoj kontrolendaj post reverko:

    <trd lng="hu">&aacute;gaskodik</trd>
    <trd lng="nl">steigeren</trd>
    <trd lng="pl">stawa&cacute; d&eogonek;ba  <klr>(o koniu)</klr></trd>
    <trdgrp lng="pt">
      <trd>empinar-se</trd>,
      <trd>encabritar-se</trd>
    </trdgrp>
    <trd lng="ro">se  <ind>ridica</ind>  <klr>(despre un cal)</klr></trd>
    <trd lng="ru">&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_i;&c_t;&c_mol;&c_s;&c_ja; &c_n;&c_a; &c_d;&c_y;&c_b;&c_y;</trd>
-->
  </snc>
  <snc mrk="pranc.0i.AMOR">
    <uzo tip="stl">VULG</uzo>
    <uzo tip="klr">(precipe pri viro, pri peniso)</uzo>
    <dif>
      Sperti erekti&gcirc;on,
      <ref tip="dif" cel="erekt.0igxi.FIZL">erekti&gcirc;i</ref>:
      <ekz>
        dum <tld/>is mia kompatinda Stango
        <fnt>
          <aut>P. Peneter</aut>,
          <vrk><url ref="http://eo.wikisource.org/wiki/Sekretaj_Sonetoj/I">
          Sekretaj Sonetoj</url></vrk>,
          <lok>1932</lok>
        </fnt>;
      </ekz>
      <ekz>
        romanverkisto <klr>[kaj]</klr> art&scirc;atanto, kiu pli <tld/>as anta&ubreve; pentra&jcirc;o ol
        anta&ubreve; porna TTT-pa&gcirc;aro
        <fnt>
          <bib>Monato</bib>
          <aut>Paulo Silas</aut>
          <vrk><url ref="https://www.monato.be/2015/011507.php">La lastaj tagoj de Lillo</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_u;&c_s;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_p;&c_r;&c_a; &c_ch;&c_e;&c_l;&c_ja;&c_s;)</klr></trd>
    <trd lng="de">einen <ind>Steifen</ind> bekommen</trd>
    <trd lng="fr">bander<klr> (vulg.)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>dupat</trd>,
    <trd>hrabat</trd>,
    <trd>kopat</trd>,
    <trd>naparovat se</trd>,
    <trd>vzp&iacute;nat se <klr>(k&uring;&ncaron;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8DF3;&#x306D;&#x56DE;&#x308B;  <pr>&#x306F;&#x306D;&#x307E;&#x308F;&#x308B;</pr></trd>,
    <trd>&#x304A;&#x3069;&#x308A;&#x8DF3;&#x306D;&#x3066;&#x9032;&#x3080;  <pr>&#x304A;&#x3069;&#x308A;&#x306F;&#x306D;&#x3066;&#x3059;&#x3059;&#x3080;</pr></trd>,
    <trd>&#x6A2A;&#x67C4;&#x306A;&#x632F;&#x308B;&#x821E;&#x3044;&#x3092;&#x3059;&#x308B;  <pr>&#x304A;&#x3046;&#x3078;&#x3044;&#x306A;&#x3075;&#x308B;&#x307E;&#x3044;&#x3092;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="sk">vzp&iacute;na&tcaron; sa</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_a;&c_v;&c_a;&c_t;&c_i; &c_d;&c_i;&c_b;&c_k;&c_i;</trd>,
    <trd>&c_g;&c_a;&c_r;&c_c;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_h;&c_o;&c_d;&c_i;&c_t;&c_i; &c_g;&c_o;&c_g;&c_o;&c_l;&c_je;&c_m;</trd>,
    <trd>&c_k;&c_o;&c_z;&c_i;&c_r;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_i;&c_r;&c_a;&c_t;&c_i; &c_n;&c_o;&c_s;&c_a;/&c_n;&c_ib;&c_s;</trd>
  </trdgrp>
</drv>
</art>
