    <trd lng="ru">&c_t;&c_je;&c_k;&c_s;&c_t;&c_i;&c_l;&c_mol;</trd>
  </snc>
<trd lng="be">&c_t;&c_e;&c_k;&c_s;&c_t;&c_y;&c_l;&c_mol;</trd>
