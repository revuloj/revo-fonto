  <trd lng="pt">desonrado</trd>
  <trd lng="ru">&c_b;&c_je;&c_s;&c_s;&c_t;&c_y;&c_d;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>hanebn&yacute;</trd>,
    <trd>ned&ocirc;stojn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_je;&c_z;&c_ch;&c_je;&c_s;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43F;&#x43E;&c_z;&c_b;&c_a;&c_v;&#x43B;&c_je;&#x43D;&c_i;&c_j; &c_ch;&c_je;&c_s;&c_t;&c_ib;</trd>,
