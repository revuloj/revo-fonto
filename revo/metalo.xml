<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>metaloid</rad>/o
</kap>
<drv mrk="metalo.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia elemento, kiu ne havas specialan brilon, estas malbona
      kondukanto de varmo kaj elektro, kaj kies oksigenaj kombinoj estas
      a&ubreve; ne&ubreve;traj a&ubreve; acidaj oksidoj. La &ccirc;efaj
<tld/>oj estas:
      <ref tip="sub" cel="antimo.0o">antimono</ref>,
      <ref tip="sub" cel="arsen.0o">arseno</ref>,
      <ref tip="sub" cel="azot.0o">azoto</ref>,
      <ref tip="sub" cel="bor1.0o">boro</ref>,
      <ref tip="sub" cel="brom.0o">bromo</ref>,
      <ref tip="sub" cel="fluor.0o">fluoro</ref>,
      <ref tip="sub" cel="fosfor.0o">fosforo</ref>,
      <ref tip="sub" cel="jod.0o">jodo</ref>,
      <ref tip="sub" cel="karbon.0o">karbono</ref>,
      <ref tip="sub" cel="klor.0o">kloro</ref>,
      <ref tip="sub" cel="oksige.0o">oksigeno</ref>,
      <ref tip="sub" cel="selen.0o">seleno</ref>,
      <ref tip="sub" cel="silici.0o">silicio</ref>,
      <ref tip="sub" cel="sulfur.0o">sulfuro</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_t;&c_a;&c_l;&c_jo;&c_ib;&c_d;</trd>
  <trdgrp lng="ca">
    <trd>no-metall</trd>,
    <trd>metal&middot;loide</trd>
  </trdgrp>
  <trd lng="de">Metalloid</trd>
  <trd lng="en">metalloid</trd>
  <trdgrp lng="es">
    <trd>metaloide</trd>
    <trd>semimetal</trd>
  </trdgrp>
  <trd lng="fr">m&eacute;tallo&iuml;de</trd>
  <trdgrp lng="id">
    <trd>metaloid</trd>,
    <trd>semilogam</trd>
  </trdgrp>
  <trd lng="nl">metallo&iuml;de</trd>
  <trd lng="pt">metal&oacute;ide</trd>
  <trd lng="ru">&c_m;&c_je;&c_t;&c_a;&c_l;&c_l;&c_o;&c_i;&c_d;</trd>
</drv>

<drv mrk="metalo.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      De <tld/>o, el <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_t;&c_a;&c_l;&c_jo;&c_ib;&c_d;&c_n;&c_y;</trd>
  <trd lng="ca">no-met&agrave;l&middot;lic</trd>
  <trd lng="de">metalloidisch</trd>
  <trd lng="en">metalloid</trd>
  <trdgrp lng="hu">
    <trd>metalloid</trd>,
    <trd>f&eacute;lf&eacute;m</trd></trdgrp>
  <trd lng="nl">metallo&iuml;de-</trd>
  <trd lng="ru">&c_m;&c_je;&c_t;&c_a;&c_l;&c_l;&c_o;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
