<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tribord</rad>/o
</kap>
<drv mrk="tribor.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Dekstra flanko de &scirc;ipo rigardante al la pruo:
      <ekz>
        la <tld/>o estas signita per verda fajro.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="babord.0o">babordo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Steuerbord</trd>
  <trd lng="fr">tribord</trd>
  <trd lng="nl">stuurboord</trd>
  <trdgrp lng="pt">
    <trd>boreste</trd>,
    <trd>estibordo</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_t;&c_i;&c_r;&c_b;&c_o;&c_r;&c_t;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_v;&c_y;&c_j; &c_b;&c_o;&c_r;&c_t;</trd>
  </trdgrp>
</drv>
</art>
