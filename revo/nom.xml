<drv mrk="nom.bapto0o">
  <kap>bapto<tld/>o</kap>
  <snc mrk="nom.bapto0o.KRI">
    <uzo tip="fak">GEN</uzo>
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">KRI</uzo>
    <dif>
      Persona <ref tip="super" cel="nom.0o.de.io"><tld/>o</ref>
      donita
      &ccirc;e <ref tip="vid" cel="bapt.0o">bapto</ref>.
    </dif>
    <trd lng="en">baptismal <ind>name</ind></trd>
    <trd lng="es"><ind>nombre</ind> de pila</trd>
    <trd lng="hu">keresztn&eacute;v</trd>
    <trd lng="nl">doopnaam</trd>
    <trd lng="ru">&c_k;&c_r;&c_jo;&c_s;&c_t;&c_n;&c_o;&c_je; <ind>&c_i;&c_m;&c_ja;</ind></trd>
  </snc>
  <snc mrk="nom.bapto0o.antauxnomo">
    <ref tip="dif" cel="nom.antaux0o">anta&ubreve;nomo</ref>
    <trd lng="en">first <ind>name</ind></trd>
    <trd lng="es">nombre</trd>
    <trd lng="hu">ut&oacute;n&eacute;v</trd>
    <trd lng="nl">voornaam<klr tip="ind"> (doopnaam)</klr></trd>
  </snc>
  <rim>
    E&ccirc; inter kristanoj ne &ccirc;iam kaj ne &ccirc;ie baptonomo
    koincidas kun anta&ubreve;nomo. Ekzemple la Kieva &ccirc;efprinco Vladimiro
    Monoma&hcirc;o havis la laikan <ctl>princan anta&ubreve;nomon</ctl>
    Vladimiro kaj la baptonomon Bazilo. Kutime oni &scirc;an&gcirc;as la
    anta&ubreve;nomon &ccirc;e mona&hcirc;i&gcirc;o a&ubreve; papi&gcirc;o ktp.
  </rim>
  <trd lng="br">anv-badez</trd>
  <trd lng="de">Taufname</trd>
  <trd lng="es">nombre</trd>
  <trdgrp lng="fr">
    <trd>pr&eacute;nom</trd>,
    <trd>nom de bapt&ecirc;me</trd>
  </trdgrp>
  <trd lng="id"><ind>nama</ind> baptis</trd>
  <trd lng="ru">&c_l;&c_i;&c_ch;&c_n;&c_o;&c_je; <ind>&c_i;&c_m;&c_ja;</ind></trd>
  <trd lng="sk">krstn&eacute; meno</trd>
  <trd lng="sv">f&ouml;rnamn</trd>
  <trdgrp lng="cs">
    <trd>k&rcaron;estn&iacute; jm&eacute;no</trd>,
    <trd>rodn&eacute; jm&eacute;no</trd>
  </trdgrp>
</drv>

<drv mrk="nom.famili0o">
  <kap>famili<tld/>o</kap>
  <snc mrk="nom.famili0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Komuna <ref tip="super" cel="nom.0o.de.io"><tld/>o</ref> de
      samfamilianoj:
      <ekz>
        sur la koverto &scirc;i skribis unu el la grafaj famili<tld/>oj
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_o;&c_z;&c_mol;&c_v;&c_ib;&c_sh;&c_ch;&c_a;</trd>
  <trdgrp lng="br">
    <trd>anv familh</trd>,
    <trd>anv-tiegezh</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>p&rcaron;&iacute;jmen&iacute;</trd>,
    <trd>rodov&eacute; jm&eacute;no</trd>
  </trdgrp>
  <trd lng="de">Familienname</trd>
  <trd lng="es">apellidos</trd>
  <trd lng="fr">nom de famille</trd>
  <trd lng="id"><ind>nama</ind> keluarga</trd>
  <trd lng="nl">familienaam</trd>
  <trd lng="ru">&c_f;&c_a;&c_m;&c_i;&c_l;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>priezvisko</trd>,
    <trd>rodov&eacute; meno</trd>
  </trdgrp>
</drv>

<drv mrk="nom.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="nom.for0i.KOMUNE">
    <dif>
      <ref tip="vid" cel="konfes.mal0i.agnoski">Malkonfesi</ref>;
      rifuzi iun <tld/>on al iu:
      <ekz>
        se vi tion faros, mi for<tld/>os vin mia amiko.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je; <ind>&c_n;&c_a;&c_z;&c_y;&c_v;&c_a;&c_c;&c_mol;</ind>
      &c_b;&c_o;&c_l;&c_je;&c_j; <klr>(&c_k;&c_ib;&c_m;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>,
    <trd>&c_n;&c_je; <ind>&c_w;&c_v;&c_a;&c_zh;&c_a;&c_c;&c_mol;</ind>
      &c_b;&c_o;&c_l;&c_je;&c_j; <klr>(&c_z;&c_a;
        &c_k;&c_a;&c_g;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>,
    <trd>&c_n;&c_je; <ind>&c_l;&c_ib;&c_ch;&c_y;&c_c;&c_mol;</ind> &c_b;&c_o;&c_l;&c_je;&c_j;
      <klr>(&c_k;&c_ib;&c_m;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  </trdgrp>
  <trd lng="br">dizanzav</trd>
  <trd lng="cs">odm&iacute;tat jmenovat</trd>
  <trd lng="de">verleugnen</trd>
  <trd lng="en">disown</trd>
  <trd lng="es">renegar</trd>
  <trd lng="fr">d&eacute;savouer</trd>
  <trd lng="hu">megtagad</trd>
  <trdgrp lng="id">
    <trd>[<ind>ingkar</ind>] mengingkari</trd>,
    <trd>[<ind>mungkir</ind>] memungkiri</trd>,
    <trd>[<ind>sangkal</ind>] menyangkal</trd>
  </trdgrp>
  <trd lng="pt">n&atilde;o <ind>chamar</ind> de</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je; <ind>&c_s;&c_ch;&c_i;&c_t;&c_a;&c_t;&c_mol;</ind>
      &c_b;&c_o;&c_l;&c_mol;&c_sh;&c_je;<klr> (&c_k;&c_o;&c_g;&c_o;-&c_l;. &c_k;&c_je;&c_m;-&c_l;.)</klr></trd>,
    <trd>&c_n;&c_je; <ind>&c_n;&c_a;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</ind>
      &c_b;&c_o;&c_l;&c_mol;&c_sh;&c_je;<klr> (&c_k;&c_o;&c_g;&c_o;-&c_l;. &c_k;&c_a;&c_k;-&c_l;.)</klr></trd>
  </trdgrp>
  <trd lng="sk">odmieta&tcaron; menova&tcaron;</trd>
</drv>

<drv mrk="nom.kares0o">
  <kap>kares<tld/>o</kap>
  <snc mrk="nom.kares0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="super" cel="nom.0o.de.io"><tld lit="N"/>o</ref>
      intima, karesa, uzata por iu &scirc;atata:
      <ekz>
        per rusaj sufiksoj mi povas krei multajn kares<tld/>ojn de
        &ccirc;iu virina nomo <klr>...</klr> ekzemple Rita, 
        Rito&ccirc;ka, Ritusja, Ritusjka, Ritu&scirc;a
        <fnt>
          <aut>Terur&ccirc;jo</aut>,
          <vrk><url ref="http://eo.lernu.net/komunikado/forumo/temo.php?t=1735">
              Romantika&jcirc;oj</url></vrk>,
          <lok>lernu.net, 2008-01-16</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxj.0">&ccirc;j</ref>,
      <ref cel="nj.0">nj</ref>
    </refgrp>
  </snc>
  <trd lng="cs">zdrobn&ecaron;lina</trd>
  <trdgrp lng="es">
    <trd>apelativo cari&ntilde;oso</trd>,
    <trd>hipocor&iacute;stico</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>diminutif<klr> (petit nom)</klr></trd>,
    <trd>petit nom</trd>
  </trdgrp>
  <trd lng="id"><ind>juluk</ind>an</trd>
  <trd lng="nl">koosnaam</trd>
  <trd lng="sk">zdrobnenina <klr>(mena)</klr></trd>
</drv>

<drv mrk="nom.kasx0o">
  <kap>ka&scirc;<tld/>o</kap>
  <snc mrk="nom.kasx0o.KOMUNE">
    <dif>
      Memelektita <ref tip="super" cel="nom.0o.de.io"><tld/>o</ref>
      de iu, malsama disde la kutima, oficiala nomo;
      <ref tip="sin" cel="pseuxd1.0o.KOMUNE">pse&ubreve;donimo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_s;&c_e;&c_w;&c_d;&c_a;&c_n;&c_ib;&c_m;</trd>
  <trd lng="br">anv-pluenn</trd>
  <trdgrp lng="de">
    <trd>Pseudonym</trd>,
    <trd>Deckname</trd>
  </trdgrp>
  <trd lng="en">pseudonym</trd>
  <trd lng="es">seud&oacute;nimo</trd>
  <trd lng="fr">pseudonyme</trd>
  <trdgrp lng="hu">
    <trd>&aacute;ln&eacute;v</trd>,
    <trd>fed&odblac;n&eacute;v</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>nama</ind> samaran</trd>,
    <trd>pseudonim</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schuilnaam</trd>,
    <trd>pseudoniem</trd>
  </trdgrp>
  <trd lng="pl">pseudonim</trd>
  <trdgrp lng="pt">
    <trd>pseud&ocirc;nimo</trd>,
    <trd>pseud&oacute;nimo</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_s;&c_je;&c_v;&c_d;&c_o;&c_n;&c_i;&c_m;</trd>
</drv>

<drv mrk="nom.lok0o">
  <kap>lok<tld/>o</kap>
  <snc mrk="nom.lok0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <tld lit="N"/>o de geografia loko;
      <ref tip="sin" cel="toponim.0o.GEOG">toponimo</ref>:
      <ekz>
        la fra&ubreve;linoj kiuj ilin <klr>(la informbultenojn)</klr>
        vo&ccirc;legas malklare prononcas lok<tld/>ojn
        <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 22a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">m&iacute;stn&iacute; jm&eacute;no</trd>
  <trdgrp lng="de">
    <trd>Ortsname</trd>,
    <trd>Toponym</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>place name</trd>,
    <trd>toponym</trd>
  </trdgrp>
  <trd lng="es">top&oacute;nimo</trd>
  <trdgrp lng="fr">
    <trd>nom de lieu</trd>,
    <trd>toponyme</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>nama</ind> tempat</trd>,
    <trd>toponimi</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_je;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; <ind>&c_n;&c_a;&c_z;&c_v;&c_a;&c_n;&c_i;&c_je;</ind></trd>,
    <trd>&c_t;&c_o;&c_p;&c_o;&c_n;&c_i;&c_m;</trd>
  </trdgrp>
  <trd lng="sk">miestne meno</trd>
</drv>

<drv mrk="nom.mok0o">
  <kap>mok<tld/>o</kap>
  <snc mrk="nom.mok0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Al<ref tip="super" cel="nom.0o.de.io"><tld/>o</ref> donita al
      iu kun moka intenco.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ja;&c_n;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trd lng="cs">posm&ecaron;&scaron;n&aacute; p&rcaron;ezd&iacute;vka</trd>
  <trdgrp lng="de">
    <trd>Spottname</trd>,
    <trd>Neckname</trd>
  </trdgrp>
  <trd lng="es">apodo</trd>
  <trd lng="fr">sobriquet</trd>
  <trd lng="hu">g&uacute;nyn&eacute;v</trd>
  <trd lng="id"><ind>juluk</ind>an</trd>
  <trd lng="nl">spotnaam</trd>
  <trd lng="pt">apelido</trd>
  <trd lng="ru">&c_k;&c_l;&c_i;&c_ch;&c_k;&c_a;</trd>
  <trd lng="sk">prez&yacute;vka <klr>(v&yacute;sme&scaron;n&aacute;)</klr></trd>
</drv>

<drv mrk="nom.persona0o">
  <kap>persona <tld/>o</kap>
  <snc mrk="nom.persona0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="super" cel="nom.0o.de.io"><tld lit="N"/>o</ref> de
      unuopa homo, aldonita al la familia <tld/>o por distingi &ccirc;iun
      familianon:
      <ekz>
        ver&scirc;ajne la sola inversa konstruo estas la ...
        orda numeralo post persona <tld/>o, ekzemple
        Re&gcirc;o Luizo la Dekkvara
        <fnt>
          <aut>J. Wells</aut>,
          <vrk>Lingvistikaj aspektoj de Esperanto</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="prt" cel="nom.antaux0o.GEN">anta&ubreve;nomo</ref>
  <trd lng="be">&c_ib;&c_m;&c_ja; <klr tip="amb">(&c_a;&c_s;&c_a;&c_b;&c_ib;&c_s;&c_t;&c_a;&c_je;)</klr></trd>
  <trdgrp lng="br">
    <trd>anv bihan</trd>,
    <trd>kentanv</trd>
  </trdgrp>
  <trd lng="de">Vorname</trd>
  <trd lng="en">first name</trd>
  <trd lng="es">nombre</trd>
  <trd lng="fr">pr&eacute;nom</trd>
  <trdgrp lng="hu">
    <trd>ut&oacute;n&eacute;v</trd>,
    <trd>keresztn&eacute;v</trd>
  </trdgrp>
  <trd lng="id"><ind>nama</ind> depan</trd>
  <trd lng="nl">voornaam</trd>
  <trdgrp lng="pt">
    <trd>nome</trd>,
    <trd>prenome</trd>,
    <trd><ind>nome</ind> de batismo</trd>
  </trdgrp>
  <trd lng="pl">imi&eogonek;</trd>
  <trd lng="ru">&c_i;&c_m;&c_ja; <klr tip="amb">(&c_l;&c_i;&c_ch;&c_n;&c_o;&c_je;)</klr></trd>
  <trd lng="sv">f&ouml;rnamn</trd>
  <trd lng="sk">krstn&eacute; meno</trd>
  <trdgrp lng="cs">
    <trd>k&rcaron;estn&iacute; jm&eacute;no</trd>,
    <trd>rodn&eacute; jm&eacute;no</trd>
  </trdgrp>
</drv>

<drv mrk="nom.plum0o">
  <kap>plum<tld/>o</kap>
  <snc mrk="nom.plum0o.BELE">
    <uzo tip="fak">BELE</uzo>
    <dif>
      <ref tip="super" cel="nom.kasx0o.KOMUNE">Ka&scirc;<tld/>o</ref>
      de verkisto a&ubreve; artisto, kiu deziras ne konigi sian
      oficialan nomon:
      <ekz>
        ... konatan per la plum<tld/>o La&ubreve;lum.
        <fnt>
          <aut>D. Keefe</aut>,
          <vrk>-</vrk>,
          <lok><url ref="http://uea.org/revuo/index.html">
          Esperanto</url>, 2010-04, p. 80a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">pseudonym</trd>
  <trd lng="es">seud&oacute;nimo</trd>
  <trd lng="fr">nom de plume</trd>
  <trd lng="id"><ind>nama</ind> pena</trd>
  <trd lng="sk">pseudonym</trd>
</drv>

<drv mrk="nom.sam0ulo">
  <kap>sam<tld/>ulo</kap>
  <snc mrk="nom.sam0ulo.KOMUNE">
    <dif>
      Havanto de samforma nomo:
      <ekz>
        sam<tld/>ulo kun sia patro estis baptata la knabeto
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">jmenovec</trd>
  <trd lng="es">tocayo</trd>
  <trd lng="fr">homonyme<klr> (porteur du m&ecirc;me nom)</klr></trd>
  <trd lng="sk">menovec</trd>
</drv>
