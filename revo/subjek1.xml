<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>subjektiv</rad>/a
</kap>
<drv mrk="subjek1.0a">
  <kap><tld/>a</kap>
  <snc>
    <uzo tip="stl">EVI</uzo>
    <dif>
      Nur mensa, nur psika, subjekta.
    </dif>
  <trdgrp lng="hu">
    <trd>szubjekt&iacute;v</trd>,
    <trd>alanyi</trd>
   </trdgrp>
    <trd lng="nl">als <ind>subject</ind><klr tip="ind">
    (ontr.)</klr></trd>
  </snc>
  <snc>
    <dif>
      Arbitra, dependanta de individuaj pla&ccirc;oj:
      <ekz>
      perfekta objektiveco ne eblas, &ccirc;ar &ccirc;iu homa verko iel 
      tiel estas <tld/>a
      <fnt>
      <aut>Stefan Maul</aut>,
      <vrk>El verva vivo &jcirc;urnalista</vrk>,
      <lok>p. 8</lok>
      </fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="objekt1.0a">objektiva</ref>.
    </refgrp>
    <trd lng="be">&c_s;&c_u;&c_b;'&c_je;&c_k;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>
    <trd lng="bg">&c_s;&c_u;&c_b;&c_je;&c_k;&c_t;&c_i;&c_v;&c_je;&c_n;</trd>
    <trdgrp lng="de">
      <trd>subjektiv</trd>,
      <trd>einseitig</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>subjective</trd>
    </trdgrp>
    <trd lng="fr">subjectif</trd>
  <trdgrp lng="hu">
    <trd>szubjekt&iacute;v</trd>,
    <trd>egy&eacute;ni</trd>,
    <trd>mag&aacute;n-</trd>
    <trd>egyoldal&uacute;</trd>,
    <trd>elfogult</trd>
  </trdgrp>
    <trdgrp lng="nl">
      <trd>subjectief</trd>,
      <trd>eenzijdig</trd>
    </trdgrp>
  </snc>
  <trd lng="ru">&c_s;&c_u;&c_b;&c_malmol;&c_je;&c_k;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
