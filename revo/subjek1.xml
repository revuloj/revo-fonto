<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: subjek1.xml,v 1.16 2015/11/12 23:10:22 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>subjektiv</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="subjek1.0a">
  <kap><tld/>a</kap>
  <snc mrk="subjek1.0a.individua">
    <dif>
      Sentata, pensata, decidata de individuo, eventuale arbitre,
      la&ubreve; propra pla&ccirc;o:
      <ekz>
        perfekta objektiveco ne eblas, &ccirc;ar &ccirc;iu homa verko iel 
        tiel estas <tld/>a
        <fnt>
          <aut>S. Maul</aut>,
          <vrk>El verva vivo &jcirc;urnalista</vrk>,
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="objekt1.0a">objektiva</ref>.
    </refgrp>
    <trd lng="be">&c_s;&c_u;&c_b;'&c_je;&c_k;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>
    <trd lng="bg">&c_s;&c_u;&c_b;&c_je;&c_k;&c_t;&c_i;&c_v;&c_je;&c_n;</trd>
    <trdgrp lng="de">
      <trd>subjektiv</trd>,
      <trd>einseitig</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>subjective</trd>
    </trdgrp>
    <trd lng="fr">subjectif</trd>
    <trdgrp lng="hu">
      <trd>szubjekt&iacute;v</trd>,
      <trd>egy&eacute;ni</trd>,
      <trd>mag&aacute;n-</trd>
      <trd>egyoldal&uacute;</trd>,
      <trd>elfogult</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>subjectief</trd>,
      <trd>eenzijdig</trd>
    </trdgrp>
  </snc>
  <snc mrk="subjek1.0a.subjekta">
    <uzo tip="stl">EVI</uzo>
    <dif>
      Nur mensa, nur psika, subjekta.
    </dif>
    <trdgrp lng="hu">
      <trd>szubjekt&iacute;v</trd>,
      <trd>alanyi</trd>
    </trdgrp>
    <trd lng="nl">als <ind>subject</ind><klr tip="ind">
    (ontr.)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>osobn&iacute;</trd>,
    <trd>subjektivn&iacute;</trd>,
    <trd>zaujat&yacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_u;&c_b;&c_malmol;&c_je;&c_k;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>osobn&yacute;</trd>,
    <trd>subjekt&iacute;vny</trd>
  </trdgrp>
</drv>

<drv mrk="subjek1.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="subjek1.0ismo.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Vidpunkto, kiu gravigas precipe la <tld/>aj impresoj, sentadoj
      de homa menso,
      kontraste al <ref tip="ant"
      cel="poziti.0ismo.FIL">pozitivismo</ref>:
      <ekz>
        &ccirc;iu homa scio estas pri&scirc;ar&gcirc;ita per <tld/>ismo; la mia anka&ubreve;
        <fnt>
          <aut>J. Zielonka</aut>,
          <vrk><url
          ref="http://www.malnova.esperanto.pl/page.php?tid=310400">
          Predikoj en Kromie&jcirc;i&scirc; 2002</url></vrk>,
          <lok>[vidita en 2015]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">subjectivisme</trd>
</drv>
</art>
