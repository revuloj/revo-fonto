<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>kin</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KIN</uzo>
  <snc mrk="kin.0o.KIN.arto">
    <dif>
      <ref tip="super" cel="art.bel0o">Belarto</ref> fari verkojn el
      memmovaj bildoj por projekcio sur grandan ekranon.
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_ib;&c_n;&c_o;</trd>,
      <trd>&c_k;&c_ib;&c_n;&c_e;&c_m;&c_a;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Kinovorf&uuml;hrung</trd>
    </trdgrp>
    <trd lng="en">cinematography</trd>
    <trd lng="fr">cin&eacute;ma<klr tip="ind"> (art)</klr></trd>
    <trd lng="he">&qof;&vav;&lamed;&nun;&vav;&ayin;</trd>
    <trdgrp lng="hu">
      <trd>mozi</trd>,
      <trd>filmm&udblac;v&eacute;szet</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>filmkunst</trd>,
      <trd>cinematografie</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_i;&c_n;&c_o;</trd>,
      <trd>&c_k;&c_i;&c_n;&c_je;&c_m;&c_a;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
    </trdgrp>
  </snc>
  <rim mrk="kin.0o.RIM_HOM">
    Ne konfuzu kun <ref tip="hom" cel="kin1.0o">kino</ref> (kinkono).
  </rim>
</drv>

<drv mrk="kin.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="kin.0ajxo.KIN_filmo">
    <uzo tip="fak">KIN</uzo>
    <dif>
      <tld lit="K"/>ofilmo, verko el serio de memmovaj
      bildoj ordinare akompanata de sonoj:
      <ekz>
        &jcirc;us surekranigita <tld/>a&jcirc;o
        <fnt><aut>A. Giridhar Rao</aut>: <vrk>La formi&gcirc;o de
        Mahatmo</vrk>,
        <bib>Monato</bib>, <lok>jaro 1997a, numero 7a, p.
        26a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="kinema1.0ajxo">kinematografa&jcirc;o</ref>
  </snc>
  <trd lng="de">Kinofilm</trd>
  <trdgrp lng="fr">
    <trd>film<klr tip="ind"> (&oelig;uvre cin&eacute;matographique)</klr></trd>,
    <trd>&oelig;uvre cin&eacute;matographique</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>film</trd>,
    <trd>filmm&udblac;v&eacute;szeti alkot&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>film<klr tip="ind"> (kunstwerk)</klr></trd>,
  </trdgrp>
</drv>

<drv mrk="kin.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="kin.0ejo.KIN">
    <uzo tip="fak">KIN</uzo>
    <dif>
      Loko, ordinare <ref tip="super"
      cel="konstr.0ajxo">konstrua&jcirc;o</ref>,
      en kiu oni publike projekcias filmojn:
      <ekz>
        ni kune dancis kaj vizitis restoraciojn kaj <tld/>ejojn
        <fnt><aut>G. Kama&ccirc;o</aut>: <vrk>Berveling, poeto sen
        tabua</vrk>, <bib>Monato</bib>, <lok>jaro 1994a, numero 12a, p.
        22a</lok></fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="teatr.film0o">filmteatro</ref>,
      <ref cel="teatr.kino0o">kinoteatro</ref>,
      <ref cel="kinema1.0ejo">kinematografejo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_n;&c_o;</trd>,
    <trd>&c_k;&c_ib;&c_n;&c_a;&c_t;&c_e;&c_a;&c_t;&c_a;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kino</trd>,
    <trd>Filmtheater</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>movie theater</trd>,
    <trd>cinema</trd>,
    <trd>movie house</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cin&eacute;ma<klr tip="ind"> (lieu)</klr></trd>,
    <trd>salle de <ind>cin&eacute;ma</ind></trd>
  </trdgrp>
  <trd lng="he">&bet;&yod;&tav; &qof;&vav;&lamed;&nun;&vav;&ayin;</trd>
  <trdgrp lng="hu">
    <trd>mozi</trd>,
    <trd>filmsz&iacute;nh&aacute;z</trd>
  </trdgrp>
  <trd lng="nl">bioscoop</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_n;&c_o;</trd>,
    <trd>&c_k;&c_i;&c_n;&c_o;&c_t;&c_je;&c_a;&c_t;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="kin.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="kin.0isto.KIN">
    <uzo tip="fak">KIN</uzo>
    <dif>
      Faristo de <tld/>ofilmoj, <ref tip="vid" 
      cel="regxis.0o.TEA">re&gcirc;isoro</ref> a&ubreve; alia &ccirc;effaranto:
      <ekz>
        infanoj en Israelo malamas infanojn en Palestino nur &ccirc;ar ili 
        estas arabaj <klr>...</klr>  sed la <tld/>isto kunigis ilin
        <fnt>
          <aut>A. Mejia</aut>,
          <vrk>RE: Cenzuro</vrk>,
          <lok><url
          ref="http://groups.google.be/group/soc.culture.esperanto?hl=eo">
          soc.culture.esperanto</url>, 2004-02-16</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">cin&eacute;aste</trd>
  <trd lng="nl">cineast</trd>
</drv>
</art>
