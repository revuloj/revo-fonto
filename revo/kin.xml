<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>kin</rad>/o
</kap>
<drv mrk="kin.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <fnt>V</fnt>
    <ref tip="dif" cel="kinkon.0o.2">Kinkono 2</ref>
    <trd lng="be">&c_h;&c_ib;&c_n;&c_a;</trd>
    <trdgrp lng="de">
       <trd>Chinarinde</trd>
    </trdgrp>
     <trd lng="en">cinema</trd>
     <trd lng="nl">kinabast</trd>
     <trd lng="ru">&c_h;&c_i;&c_n;&c_a;</trd>
  </snc>
  <snc num="2">
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="sin" cel="kinema1.0o">Kinematografo</ref>:
      <ekz><tld/>oprezentado.</ekz>
    </dif>
    <trdgrp lng="be">
       <trd>&c_k;&c_ib;&c_n;&c_o;</trd>,
       <trd>&c_k;&c_ib;&c_n;&c_e;&c_m;&c_a;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
    </trdgrp>
    <trdgrp lng="de">
       <trd>Kinovorf&uuml;hrung</trd>
    </trdgrp>
  <trd lng="en">cinematography</trd>
  <trd lng="fr">cin&eacute;ma</trd>
     <trd lng="nl">filmprojector</trd>
     <trdgrp lng="ru">
      <trd>&c_k;&c_i;&c_n;&c_o;</trd>,
      <trd>&c_k;&c_i;&c_n;&c_je;&c_m;&c_a;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
    </trdgrp>
  </snc>
</drv>
<drv mrk="kin.0ejo0oteatro">
  <kap><tld/>ejo, <tld/>oteatro</kap>
  <uzo tip="fak">TEA</uzo>
  <uzo tip="fak">KIN</uzo>
  <snc>
    <dif>
      <ref tip="sin" cel="kinema1.0ejo">Kinematografejo</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_n;&c_o;</trd>,
    <trd>&c_k;&c_ib;&c_n;&c_a;&c_t;&c_e;&c_a;&c_t;&c_a;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kino</trd>,
    <trd>Filmtheater</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>movie theater</trd>,
    <trd>cinema</trd>,
    <trd>movie house</trd>
  </trdgrp>
  <trd lng="fr">cin&eacute;ma</trd>
  <trd lng="nl">bioscoop</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_n;&c_o;</trd>,
    <trd>&c_k;&c_i;&c_n;&c_o;&c_t;&c_je;&c_a;&c_t;&c_r;</trd>
  </trdgrp>
</drv>
</art>
