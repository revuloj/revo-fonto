<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>kin</rad>/o
</kap>
<drv mrk="kin.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <fnt>V</fnt>
    <ref tip="dif" cel="kinkon.0o.2">Kinkono 2</ref>
    <trdgrp lng="de">
       <trd>Chinarinde</trd>
    </trdgrp>
    <trd lng="ru">&c_h;&c_i;&c_n;&c_a;</trd>
  </snc>
  <snc num="2">
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="sin" cel="kinema1.0o">Kinematografo</ref>:
      <ekz><tld/>oprezentado.</ekz>
    </dif>
    <trdgrp lng="de">
       <trd>Kinovorf&uuml;hrung</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_i;&c_n;&c_o;</trd>,
      <trd>&c_k;&c_i;&c_n;&c_je;&c_m;&c_a;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
    </trdgrp>
  </snc>
</drv>
<drv mrk="kin.0ejo0oteatro">
  <kap><tld/>ejo, <tld/>oteatro</kap>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <dif>
      <ref tip="sin" cel="kinema1.0ejo">Kinematografejo</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Kino</trd>,
    <trd>Filmtheater</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>movie house</trd>,
    <trd>movie theater</trd>,
    <trd>theater</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_n;&c_o;</trd>,
    <trd>&c_k;&c_i;&c_n;&c_o;&c_t;&c_je;&c_a;&c_t;&c_r;</trd>
  </trdgrp>
</drv>
</art>
