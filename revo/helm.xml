<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>helm</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="helm.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <uzo tip="fak">MIL</uzo>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Plen<ref tip="super" cel="kask.0o">kasko</ref> kovranta
      &ccirc;iujn flankojn de la kapo, kiajn uzis interalie batalistoj
      de malfrua e&ubreve;ropa mezepoko:
      <ekz>
        viziero <klr>...</klr>, fiksita al la kasko, donis al la tuto
        aspekton de kompleta <tld/>o<fnt><bib>DonKihxoto</bib>,
        <lok>&ccirc;apitro 1a, p. 30a-31a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_l;&c_je;&c_m;</trd>
  <trd lng="bg">&c_sh;&c_l;&c_je;&c_m;</trd>
  <trd lng="ca">elm</trd>
  <trd lng="de">Helm</trd>
  <trd lng="fa">&f_k;&a_l;&a_A;&a_h;&a_kh;&a_w;&a_d;</trd>
  <trd lng="fr">heaume</trd>
  <trd lng="hu">lovagsisak</trd>
  <trd lng="it">elmo</trd>
  <trd lng="pl">he&lstroke;m</trd>
  <trd lng="pt">elmo</trd>
  <trd lng="ru">&c_sh;&c_l;&c_je;&c_m;</trd>
</drv>
</art>

