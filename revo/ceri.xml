<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ceri</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="ceri.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
   <fnt><bib>ZR</bib></fnt>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref>
      kun atomnumero 58, atompezo 140,12, simbolo Ce. Malofta
      <ref tip="super" cel="metal.0o">metalo</ref> mola kaj griza.
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_e;&c_r;&c_y;&c_j;</trd>
  <trd lng="de">Cer</trd>
  <trd lng="el">&delta;&eta;&my;&eta_ton;&tau;&rho;&jota;&omikron;</trd>
  <trd lng="fr">c&eacute;rium</trd>
  <trd lng="hu">c&eacute;rium</trd>
  <trd lng="nl">cerium</trd>
  <trd lng="pt">c&eacute;rio</trd>
  <trd lng="ru">&c_c;&c_je;&c_r;&c_i;&c_j;</trd>
  <trd lng="sk">c&eacute;rium</trd>
</drv>
</art>
