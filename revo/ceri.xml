<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ceri</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="ceri.0o">
  <kap><tld/>o</kap>
  <fnt><bib>ZR</bib></fnt>
  <uzo tip="fak">KEM</uzo>
  <mlg kod="IUPAC">Ce</mlg>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
      lst="voko:&hcirc;emiaj_elementoj" val="58">elemento</ref>
      kun atomnumero 58, atompezo 140,12; malofta
      <ref tip="super" cel="metal.0o">metalo</ref> mola kaj griza; 
      lantanoido.
    </dif>
  </snc>                        
  <trd lng="be">&c_c;&c_e;&c_r;&c_y;&c_j;</trd>
  <trd lng="bg">&c_c;&c_je;&c_r;&c_i;&c_j;</trd>
  <trd lng="br">seriom</trd>
  <trd lng="ca">ceri</trd>
  <trd lng="de">Cer</trd>
  <trd lng="el">&delta;&eta;&my;&eta_ton;&tau;&rho;&jota;&omikron;</trd>
  <trd lng="en">cerium</trd>
  <trd lng="es">cerio</trd>
  <trd lng="fa">&a_s;&a_r;&f_y;&a_m;</trd>
  <trd lng="fr">c&eacute;rium</trd>
  <trd lng="hu">c&eacute;rium</trd>
  <trd lng="id">serium</trd>
  <trd lng="it">cerio</trd>
  <trd lng="pl">Cerium</trd>
  <trd lng="nl">cerium</trd>
  <trd lng="pl">cer</trd>
  <trd lng="pt">c&eacute;rio</trd>
  <trd lng="ru">&c_c;&c_je;&c_r;&c_i;&c_j;</trd>
  <trd lng="sk">c&eacute;rium</trd>
  <trd lng="sv">cerium</trd>
</drv>
</art>
