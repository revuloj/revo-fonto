  <trdgrp lng="pl">
  <trd>okazywa&cacute; <ind>nieufno&sacute;&cacute;</ind></trd>,
  <trd>okazywa&cacute; brak <ind>zaufania</ind></trd>
</trdgrp>
  <trd lng="sk">ned&ocirc;verova&tcaron;</trd>
  <trdgrp lng="zh">
    <trd>&#x6000;&#x7591; <pr>hu&#xE1;iy&#xED;</pr></trd>,
    <trd>&#x4FE1;&#x4E0D;&#x8FC7; <pr>x&#xEC;nb&#xF9;gu&#xF2;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="konfid.mal0o">
  <kap>mal<tld/>o</kap>
  <snc mrk="konfid.mal0o.KOMUNE">
    <dif>
      Sento de tiu, kiu mal<tld/>as:
      <ekz>
        la samtempuloj renkontas <klr>[novajn ideojn]</klr> ne sole
        kun rimarkinde obstina mal<tld/>o, sed e&ccirc; kun ia
        neklarigebla malamikeco
        <fnt><bib>EE</bib></fnt>;
      </ekz><ekz>
        <uzo tip="fak">POL</uzo>
        parlamenta komitato decidis <klr>[&#x2026;]</klr> prepari
        vo&ccirc;donon pri mal<tld/>o al la registaro
        <fnt>
          <aut>A. Ibraimova, trad. J. Bauer</aut>,
          <vrk><url ref="https://eo.globalvoices.org/2014/11/1945">
          Kirgizio: La amaskomunikiloj suferas meze de kreskanta
          politika malakordo</url></vrk>,
          <lok>GlobalVoices, 2016-04-07</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_d;&c_a;&c_v;&c_je;&c_r;</trd>
  <trd lng="cs">ned&uring;v&ecaron;ra</trd>
  <trd lng="de">Misstrauen</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;fiance</trd>,
    <trd>m&eacute;fiance</trd>,
    <trd>r&eacute;serve<klr tip="ind"> (m&eacute;fiance)</klr></trd>
  </trdgrp>
  <trd lng="hu">bizalmatlans&aacute;g</trd>
  <trdgrp lng="ja">
    <trd>&#x7591;&#x5FF5;  <pr>&#x304E;&#x306D;&#x3093;</pr></trd>,
    <trd>&#x4E0D;&#x4FE1;  <pr>&#x3075;&#x3057;&#x3093;</pr></trd>,
    <trd>&#x8B66;&#x6212;&#x5FC3;  <pr>&#x3051;&#x3044;&#x304B;&#x3044;&#x3057;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="pl">brak <ind>zaufania</ind></trd>
  <trd lng="sk">ned&ocirc;vera</trd>
  <trdgrp lng="zh">
    <trd>&#x4E0D;&#x4FE1;&#x4EFB; <pr>b&#xF9;x&#xEC;nr&#xE8;n</pr></trd>,
    <trd>&#x4E0D;&#x4FE1; <pr>b&#xF9;x&#xEC;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="konfid.mal0ema">
  <kap>mal<tld/>ema</kap>
  <snc>
    <dif>
      Kiu emas mal<tld/>i:
      <ekz>
        Tiberio <klr>[&#x2026;]</klr> da&ubreve;re restis vivanta kiel privata civitano, timema kaj mal<tld/>ema,
        evitante ricevi vizitojn en la insulo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17193">Tiberio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_d;&c_a;&c_v;&c_je;&c_r;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_a;&c_z;&c_r;&c_o;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">misstrauisch</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;fiant</trd>,
    <trd>m&eacute;fiant</trd>,
    <trd>soup&ccedil;onneux</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bizalmatlan</trd>,
    <trd>gyanakv&oacute;</trd>
  </trdgrp>
  <trd lng="pl">niefuny</trd>
  <trdgrp lng="zh">
    <trd>&#x4E0D;&#x4FE1;&#x4EFB; <pr>b&#xF9;x&#xEC;nr&#xE8;n</pr></trd>,
    <trd>&#x591A;&#x5FC3; <pr>du&#x14D;x&#x12B;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="konfid.mem0o">
  <kap>mem<tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="fid.mem0o">Memfido</ref>:
      <ekz>
        tia mem<tld/>o estis legebla sur ilia viza&gcirc;o, kvaza&ubreve;
        &ccirc;iu el ili posedus je sia dispono nevenkeblajn regimentojn
        de spiritoj
        <fnt><bib>Far3</bib><lok>&ccirc;ap. 7</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_m;&c_a;&c_w;&c_p;&c_e;&c_w;&c_n;&c_je;&c_n;&c_a;&c_s;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_a; &c_w; &c_s;&c_v;&c_a;&c_je; &c_s;&c_ib;&c_l;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>sebed&uring;v&ecaron;ra</trd>,
    <trd>sebejistota</trd>
  </trdgrp>
  <trd lng="de">Selbstvertrauen</trd>
  <trdgrp lng="fr">
    <trd>assurance<klr tip="ind"> (confiance en soi)</klr></trd>,
    <trd><ind>confiance</ind> en soi</trd>
  </trdgrp>
  <trd lng="hu">&ouml;nbizalom</trd>
  <trd lng="pl">pewno&sacute;&cacute; siebie</trd>
  <trd lng="sk">sebaistota</trd>
  <trdgrp lng="zh">
    <trd>&#x81EA;&#x4FE1;&#x5FC3; <pr>z&#xEC;x&#xEC;nx&#x12B;n</pr></trd>,
    <trd>&#x81EA;&#x4FE1; <pr>z&#xEC;x&#xEC;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="konfid.havi_0on">
  <kap>havi <tld/>on</kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc>
    <dif>
