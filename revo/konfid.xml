    <trd>&#x4E49;&#x6C14; <pr>y&#xEC;q&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="konfid.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <dif>
      Kiu volonte
      <ref cel="konfid.0i.havi_0on"><tld/>as<sncref/></ref>:
        <ekz>
        la birdoj estis tiel <tld/>emaj, ke ili preska&ubreve;
        sidi&gcirc;adis sur la &scirc;ultroj de Elizo
        <fnt><bib>Fab1</bib><lok>Sova&gcirc;aj cignoj</lok></fnt>;
      </ekz><ekz>
        la maniero, kiel ili sidis unu apud la alia, esprimis <tld/>eman
        proksimi&gcirc;on
        <fnt><bib>PatrojFiloj</bib><lok>&ccirc;ap. 25</lok></fnt>;
      </ekz><ekz>
        <nom>Katja</nom> <klr>[...]</klr> rifuzis nek al li, nek al si la senpekan
        plezuron de duonhontema, duon<tld/>ema amikeco
        <fnt><bib>PatrojFiloj</bib><lok>&ccirc;ap. 17</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_v;&c_je;&c_r;&c_l;&c_ib;&c_v;&c_y;</trd>
  <trd lng="cs">d&uring;v&ecaron;&rcaron;iv&yacute;</trd>
  <trdgrp lng="de">
    <trd>zutraulich</trd>,
    <trd>vertraut</trd>
  </trdgrp>
  <trd lng="es">confiado</trd>
  <trdgrp lng="fr">
    <trd>confiant</trd>
  </trdgrp>
  <trd lng="hu">j&oacute;hiszem&udblac;</trd>
  <trd lng="ja">&#x4EBA;&#x3092;&#x4FE1;&#x3058;&#x3084;&#x3059;&#x3044;  <pr>&#x3058;&#x3093;&#x3092;&#x3057;&#x3093;&#x3058;&#x3084;&#x3059;&#x3044;</pr></trd>
  <trd lng="pl">ufny</trd>
  <trd lng="ru">&c_d;&c_o;&c_v;&c_je;&c_r;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>
  <trd lng="sk">d&ocirc;ver&ccaron;iv&yacute;</trd>
</drv>

<drv mrk="konfid.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi <ref cel="konfid.0i.havi_0on"><tld/>i<sncref/></ref>,
      veki <tld/>on:
      <ekz>
        <klr>[lia viza&gcirc;o]</klr> estis severa, sed tamen
        <ind><tld/>iga</ind>
        <fnt><bib>Fab4</bib><lok>Infano en la tombo</lok></fnt>;
        <trdgrp lng="fr">
          <trd>qui inspire <ind>confiance</ind></trd>,
          <trd>engageant<klr tip="ind"> (qui inspire
          confiance)</klr></trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>bizalomgerjeszt&odblac;</trd>,
          <trd>bizalom&eacute;p&iacute;t&odblac;</trd>
        </trdgrp>
      </ekz><ekz>
        mi e&ccirc; devis timi en miaj revadoj, ke mia imago, kiun
        miaj malfeli&ccirc;oj <ind>mal<tld/>igi</ind>s, fine turnos
        tiuflanken sian aktivecon
        <fnt><bib>Revadoj</bib><lok>Sepa promenado</lok></fnt>.
        <trdgrp lng="fr">
          <trd>effaroucher<klr tip="ind"> (inspirer
          m&eacute;fiance)</klr></trd>,
          <trd>rendre <ind>d&eacute;fiant</ind></trd>,
          <trd>inspirer <ind>m&eacute;fiance</ind></trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>bizalmatlann&aacute; tesz</trd>,
          <trd>gyanakv&oacute;v&aacute; tesz</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol; <ind>&c_d;&c_a;&c_v;&c_je;&c_r;</ind></trd>
  <trd lng="de"><ind>Vertrauen</ind> erweckend</trd>
  <trd lng="fr">inspirer <ind>confiance</ind></trd>
  <trdgrp lng="hu">
    <trd>bizalmat kelt</trd>,
    <trd>bizakod&oacute;v&aacute; tesz</trd>
  </trdgrp>
</drv>

<drv mrk="konfid.0inda">
  <kap><tld/>inda</kap>
  <snc>
    <dif>
      Inda je <tld/>o:
      <ekz>
        <nom>Asarhadon</nom> sendis en la straton de l' Tomboj <tld/>indan homon,
        ordonante al li atente observi
        <fnt><bib>Far1</bib><lok>&ccirc;ap. 20</lok></fnt>;
      </ekz><ekz>
        mi liveris pruvon, ke al atestantoj por ni agrablaj ni kredas
        ne kontrolante ilian <tld/>indecon
        <fnt>
        <aut>trad. K. Bein</aut>
        <vrk>Internacia Krestomatio</vrk>
        <lok>Rakonto pri spiritoj</lok></fnt>;
      </ekz><ekz>
        dum 25 jaroj ili suriradis &ccirc;iujn brakojn de la Neva kune kun
        la kanaloj de Peterburgo <klr>[...]</klr> sen pli ol unu
        malfeli&ccirc;a&jcirc;o: bona pruvo de l&#x2019; ta&ubreve;geco kaj
        <tld/>indeco de l&#x2019; finna gento
        <fnt><bib>Voja&gcirc;Imp</bib><lok>En Peterburgo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_o;&c_d;&c_n;&c_y; <ind>&c_d;&c_a;&c_v;&c_je;&c_r;&c_u;</ind></trd>
  <trd lng="cs">d&uring;v&ecaron;ryhodn&yacute;</trd>
  <trd lng="de">vertrauensw&uuml;rdig</trd>
  <trdgrp lng="fr">
    <trd>digne de <ind>confiance</ind></trd>,
    <trd>fiable<klr tip="ind"> (&agrave; qui on peut se fier)</klr></trd>,
    <trd>honn&ecirc;te<klr tip="ind"> (&agrave; qui on peut se fier)</klr></trd>,
    <trd>loyal<klr tip="ind"> (&agrave; qui on peut se fier)</klr></trd>,
    <trd>recommandable<klr tip="ind"> (&agrave; qui on peut se
    fier)</klr></trd>,
    <trd>s&eacute;rieux<klr tip="ind"> (&agrave; qui on peut se fier)</klr></trd>,
    <trd>s&ucirc;r<klr tip="ind"> (&agrave; qui on peut se fier)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bizalomra m&eacute;lt&oacute;</trd>,
    <trd>bizalomra &eacute;rdemes</trd>
  </trdgrp>
  <trd lng="pl">godny <ind>zaufania</ind></trd>
  <trd lng="sk">d&ocirc;veryhodn&yacute;</trd>
  <trd lng="zh">&#x4FE1;&#x5F97;&#x8FC7; <pr>x&#xEC;ndegu&#xF2;</pr></trd>
</drv>

<drv mrk="konfid.0itajxo">
  <kap><tld/>ita&jcirc;o</kap>
  <snc>
    <dif>
      A&jcirc;o, kiun oni al iu <tld/>is <sncref ref="konfid.0i.pro_honesteco"/>:
      <ekz>
        mi konas tiun, al kiu mi kredis, kaj mi konvinki&gcirc;is, ke li havas
        la povon gardi mian <tld/>ita&jcirc;on &gcirc;is tiu tago
        <fnt><bib>NT</bib><lok>&Ti2; 1:12</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Vertrautes</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;p&ocirc;t<klr tip="ind"> (chose laiss&eacute;e en d&eacute;p&ocirc;t)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>r&aacute;b&iacute;zott holmi</trd>,
    <trd>let&eacute;t</trd>
  </trdgrp>
</drv>

<drv mrk="konfid.0ulo">
  <kap><tld/>ulo, <var><kap><tld/>ulino</kap></var></kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="konfid.0ato"><tld lit="K"/>ato, <tld/>atino</ref>;
      al kiu oni povas <tld/>i:
      <ekz>
        <nom>Walter de Stetten</nom>, oficiro husara, estis la fian&ccirc;o de <nom>Mathilde</nom>,
        kaj pro tio li estis anka&ubreve; la <tld/>ulo de la du amikinoj
        <fnt><bib>FK</bib> <lok>Nur unu vorton!</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Vertrauter</trd>
  <trdgrp lng="fr">
    <trd>confident(e)</trd>,
    <trd>personne de <ind>confiance</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vki <ind>bizalmasa</ind></trd>,
    <trd>bizalmi ember</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5BC6;&#x53CB; <pr>m&#xEC;y&#x1D2;u</pr></trd>,
    <trd>&#x77E5;&#x5DF1; <pr>zh&#x12B;j&#x1D0;</pr></trd>,
    <trd>&#x5FC3;&#x8179; <pr>x&#x12B;nf&#xF9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="konfid.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Certi pri malhonesteco, malkapablo, malvaloro de iu:
      <ekz>
        en malnova turo <klr>[...]</klr> lo&gcirc;is gardisto kaj strigo:
        mi mal<tld/>is al ili amba&ubreve;, precipe al la strigo
        <fnt><bib>Fab3</bib><lok>Supo el kolbasaj bastonetoj</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="konfid.0i.havi_0on"><tld/>i <sncref/></ref>
  </snc>
  <trd lng="be">&c_n;&c_je; <ind>&c_d;&c_a;&c_v;&c_ja;&c_r;&c_a;&c_c;&c_mol;</ind></trd>
