<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: auxrik.xml,v 1.22 2023/10/29 18:30:17 revo Exp $">
<kap>
  <rad>a&ubreve;rikl</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="auxrik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc mrk="auxrik.0o.orelo">
    <dif>
      La ekstera parto de <ref tip="malprt" cel="orel.0o.ANA">orelo</ref>:
      <ekz>
        se la surkapa a&ubreve;dilo ne <klr>[&#8230;]</klr> estas hermeta sur a&ubreve;riklo, &gcirc;ia efiko ne
        estas sufi&ccirc;a por basaj frekvencoj
        <fnt>
          <aut>Miroslav Baro&scaron;</aut>
          <vrk><url ref="https://scienca-revuo.info/files/1211-1-1349-1-10-20140426.pdf">Altfidele
          sonreproduktanta miniatura intrameata a&ubreve;dilo</url></vrk>
          <lok>Scienca Revuo, vol. 32 (1981), n-ro 1 (137)</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ar <klr>[hipopotamo]</klr> pasigas plimulton de tempo en la akvo (proksimume 18
        horoj &ccirc;iutage), &gcirc;i havas fermeblajn <tld/>oj kaj naztruojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=115888">Amfibia hipopotamo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_u;&c_sh;&c_n;&c_a;&c_ja; <ind>&c_r;&c_a;&c_k;&c_a;&c_v;&c_ib;&c_n;&c_a;</ind></trd>
    <trd lng="de">Ohrmuschel</trd>
    <trd lng="zh">&#22806;&#32819; <pr>w&#xE0;i&#x11B;r</pr></trd>
</snc>
  <snc mrk="auxrik.0o.koro">
    <subsnc mrk="auxrik.0o.etendajxo">
      <dif>
        Orelsimila etenda&jcirc;o de <ref tip="malprt" cel="atri.0o.ANA">atrio<sncref/></ref>.
      </dif>
      <trd lng="de">Herzohr</trd>
    </subsnc>
    <subsnc mrk="auxrik.0o.korkavo">
      <dif>
        Malnova nomo de <ref cel="atri.0o.ANA">atrio<sncref/></ref>,
        la supraj kava&jcirc;oj de la koro (&ccirc;e kelkaj simplaj bestoj, ekzemple moluskoj, la termino
        ankora&ubreve; uzi&gcirc;as):
        <ekz>
          komuniktruo inter la <tld/>o kaj la ventriklo
          <fnt>
            <aut>Agostinho da Silva</aut>
            <vrk><url
            ref="http://www.eventoj.hu/steb/zoologio/la-araneoj-1944.pdf">Araneoj</url></vrk>
            <lok>&Ccirc;irka&ubreve; la Mondo, 1944, p. 18</lok>
          </fnt>;
        </ekz>
        <ekz>
          la <tld/>o estas &ccirc;ambro en kiu la sango eniras al la koro, male al la ventriklo,
          el kiu &gcirc;i estas pelita ekster la organon
          <fnt>
            <bib>Viki</bib>
            <lok><url ref="https://eo.wikipedia.org/?curid=580361">A&ubreve;riklo (koro)</url></lok>
          </fnt>.
        </ekz>
      </dif>
    <trd lng="de">Vorhof <klr>(veraltet)</klr></trd>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="ventri">ventriklo</ref>,
      <ref cel="vestib.0o.ANA">vestiblo</ref>
    </refgrp>
    <trd lng="be">&c_v;&c_u;&c_sh;&c_k;&c_a; <klr tip="amb">(&c_s;&c_e;&c_r;&c_c;&c_a;)</klr></trd>
    <trd lng="ca">aur&iacute;cula</trd>
    <trd lng="en">auricle</trd>
    <trd lng="es">aur&iacute;cula</trd>
    <trd lng="fr">oreillette</trd>
    <trdgrp lng="hu">
      <trd>f&uuml;lcse</trd>,
      <trd>sz&iacute;vf&uuml;lecske</trd>
    </trdgrp>
    <trd lng="it">auricola</trd>
    <trd lng="la">Auricula cordis</trd>
    <trd lng="pl">przedsionek serca</trd>
    <trd lng="ru">&c_u;&c_sh;&c_k;&c_o; <klr tip="amb">(&c_s;&c_je;&c_r;&c_d;&c_c;&c_a;)</klr></trd>
  </snc>
  <trd lng="he">&tav;&nun;&vav;&fkaf; &alef;&vav;&zayin;&fnun;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_u;&c_sh;&c_k;&c_o;&nbsp;(&c_s;&c_je;&c_r;&c_c;&c_ja;)</trd>,
    <trd>&c_v;&c_u;&c_sh;&c_n;&c_a;&nbsp;&c_r;&c_a;&c_k;&c_o;&c_v;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_m;&c_o;&c_ch;&c_k;&c_a; &c_v;&c_u;&c_h;&c_a;</trd>
  </trdgrp>
</drv>
</art>
