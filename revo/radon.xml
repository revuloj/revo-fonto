<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>radon</rad>/o
</kap>
<drv mrk="radon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 86, atompezo 222,0176, simbolo Rn. <ref tip="super"
      cel="gas.nobla0o">Nobla gaso</ref>. Eksa nomo: <ref
      tip="sin" cel="niton.0o">nitono</ref>. &Ccirc;iuj
      izotopoj de tiu elemento estas radioaktivaj.
    </dif>
  </snc>
  <trd lng="de">Radon</trd>
  <trd lng="hu">radon</trd>
  <trd lng="nl">radon</trd>
  <trd lng="ru">&c_r;&c_a;&c_d;&c_o;&c_n;</trd>
</drv>
</art>
