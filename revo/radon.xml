<vortaro>
<art mrk="$Id$">
<kap>
  <rad>radon</rad>/o
</kap>
<drv mrk="radon.0o">
  <kap><tld/>o</kap>
  <mlg>Rn</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
      lst="voko:&hcirc;emiaj_elementoj">elemento</ref> kun
      atomnumero 86, atompezo 222,0176, simbolo <frm>Rn</frm>. <ref
tip="super"
      cel="gas.nobla0o">Nobla gaso</ref>. Eksa nomo: <ref
      tip="sin" cel="niton.0o">nitono</ref>. &Ccirc;iuj
      <ref tip="super" cel="izotop.0o">izotopoj</ref> 
      de tiu elemento estas radioaktivaj.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_d;&c_o;&c_n;</trd>  
  <trd lng="br">radon</trd>
  <trd lng="de">Radon</trd>
  <trd lng="en">radon</trd>
  <trd lng="es">rad&oacute;n</trd>
  <trd lng="fr">radon</trd>
  <trd lng="hu">radon</trd>
  <trd lng="it">radon</trd>
  <trd lng="la">Radon</trd>
  <trd lng="nl">radon</trd>
  <trd lng="pl">radon</trd>
  <trd lng="pt">rad&ocirc;nio</trd>
  <trd lng="ru">&c_r;&c_a;&c_d;&c_o;&c_n;</trd>
  <trd lng="sv">radon</trd>
</drv>
</art>
