<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: tonik1.xml,v 1.3 2022/03/28 14:44:11 afido Exp $">
    <kap>
      <rad>tonik</rad>/o
    </kap>

<drv mrk="tonik1.0o">
  <kap><tld/>o</kap>
  <snc mrk="tonik1.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      La <ref tip="sin" cel="ton.0o.MUZ_baza">baza tono</ref>
      de <ref tip="super" cel="gam.0o.MUZ">gamo</ref>:
      <ekz>
        en antikva kaj mezepoka Barato la arto de muziko kaj danco &ccirc;iam estis tre rekte
        ligita al <klr>[&#x2026;]</klr> fiksa sono, <tld/>o
        <fnt>
          <vrk><url ref="https://esperantaretradio.blogspot.com/2017/10/kion-ludas-la-mondo.html">Bakit anka&ubreve;
          estis komponistino kaj muzika instruistino en la egipta...</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Tonika</trd>,
      <trd>Grundton</trd>
    </trdgrp>
    <trd lng="fr">tonique<klr> (ina subst.)</klr><klr tip="ind">
    (subst.)</klr></trd>
    <trd lng="it">tonica<klr> (sost.)</klr></trd>
    <trd lng="nl">stamtoon</trd>
    <trdgrp lng="pl">
      <trd>tonika</trd>,
      <trd>tonacja</trd>
    </trdgrp>
    <trd lng="pt">t&ocirc;nica</trd>
  </snc>
  <ref tip="hom" cel="tonik.0o.MED">toniko (vigligilo)</ref>
  <trd lng="cs">t&oacute;nika</trd>
  <trd lng="sk">tonikum</trd>
  <trdgrp lng="uk">
    <trd>&#x442;&#x43E;&#x43D;&#x456;&#x43A;&#x430;</trd>,
    <trd>&#x43E;&#x441;&#x43D;&#x43E;&#x432;&#x43D;&#x438;&#x439; &#x442;&#x43E;&#x43D;</trd>,
    <trd>&#x43E;&#x441;&#x43D;&#x43E;&#x432;&#x43D;&#x430; &#x447;&#x430;&#x441;&#x442;&#x43E;&#x442;&#x430;</trd>,
    <trd>&#x442;&#x43E;&#x43D;&#x456;&#x437;&#x443;&#x44E;&#x447;&#x430; &#x440;&#x435;&#x447;&#x43E;&#x432;&#x438;&#x43D;&#x430;</trd>,
    <trd>&#x442;&#x43E;&#x43D;&#x456;&#x437;&#x443;&#x44E;&#x447;&#x438;&#x439; &#x437;&#x430;&#x441;&#x456;&#x431;</trd>,
    <trd>&#x442;&#x43E;&#x43D;&#x456;&#x43A; (&#x43D;&#x430;&#x43F;&#x456;&#x439;)</trd>
  </trdgrp>
</drv>

<drv mrk="tonik1.0a">
  <kap>
    <tld/>a
  </kap>
  <snc mrk="tonik1.0a.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Referanta al la <ref tip="vid" cel="tonik1.0o.MUZ">toniko</ref>.
    </dif>
    <trd lng="nl">betr. hebbend op de <ind>stamtoon</ind></trd>
    <trdgrp lng="pl">
      <trd>toniczny</trd>,
      <trd>w <ind>tonacji</ind></trd>
    </trdgrp>
    <trd lng="pt">t&ocirc;nico</trd>
  </snc>
  <trd lng="fr">tonique<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="it">tonico<klr> (agg.)</klr></trd>
</drv>


  </art>

