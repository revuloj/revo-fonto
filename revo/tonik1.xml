<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: tonik1.xml,v 1.6 2023/10/21 14:37:42 afido Exp $">
    <kap>
      <rad>tonik</rad>/o
    </kap>

<drv mrk="tonik1.0o">
  <kap><tld/>o</kap>
  <snc mrk="tonik1.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      La <ref tip="sin" cel="ton.0o.MUZ_baza">baza tono</ref>
      de <ref tip="super" cel="gam.0o.MUZ">gamo</ref>:
      <ekz>
        en antikva kaj mezepoka Barato la arto de muziko kaj danco &ccirc;iam estis tre rekte
        ligita al <klr>[&#x2026;]</klr> fiksa sono, <tld/>o
        <fnt>
          <vrk><url ref="https://esperantaretradio.blogspot.com/2017/10/kion-ludas-la-mondo.html">Bakit anka&ubreve;
          estis komponistino kaj muzika instruistino en la egipta...</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_o;&c_n;&c_ib;&c_k;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Tonika</trd>,
      <trd>Grundton</trd>
    </trdgrp>
    <trd lng="fr">tonique<klr> (ina subst.)</klr><klr tip="ind">
    (subst.)</klr></trd>
    <trd lng="it">tonica<klr> (sost.)</klr></trd>
    <trd lng="nl">stamtoon</trd>
    <trdgrp lng="pl">
      <trd>tonika</trd>,
      <trd>tonacja</trd>
    </trdgrp>
    <trd lng="pt">t&ocirc;nica</trd>
  </snc>
  <ref tip="hom" cel="tonik.0o.MED">toniko (vigligilo)</ref>
  <trd lng="cs">t&oacute;nika</trd>
  <trdgrp lng="ja">
    <trd>&#x4E3B;&#x97F3;  <pr>&#x3057;&#x3085;&#x304A;&#x3093;</pr></trd>,
    <trd>&#x30AD;&#x30FC;&#x30CE;&#x30FC;&#x30C8;</trd>,
    <trd>&#x5F37;&#x58EE;&#x5264;  <pr>&#x304D;&#x3087;&#x3046;&#x305D;&#x3046;&#x3056;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="sk">tonikum</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&#x43E;&#x43D;&c_ib;&#x43A;&c_a;</trd>,
    <trd>&#x43E;&c_s;&#x43D;&#x43E;&c_v;&#x43D;&c_i;&c_j; &c_t;&#x43E;&#x43D;</trd>,
    <trd>&#x43E;&c_s;&#x43D;&#x43E;&c_v;&#x43D;&c_a; &c_ch;&c_a;&c_s;&c_t;&#x43E;&c_t;&c_a;</trd>,
    <trd>&c_t;&#x43E;&#x43D;&c_ib;&c_z;&c_u;&#x44E;&c_ch;&c_a; &c_r;&c_je;&c_ch;&#x43E;&c_v;&c_i;&#x43D;&c_a;</trd>,
    <trd>&c_t;&#x43E;&#x43D;&c_ib;&c_z;&c_u;&#x44E;&c_ch;&c_i;&c_j; &c_z;&c_a;&c_s;&c_ib;&c_b;</trd>,
    <trd>&c_t;&#x43E;&#x43D;&c_ib;&#x43A; (&#x43D;&c_a;&#x43F;&c_ib;&c_j;)</trd>
  </trdgrp>
</drv>

<drv mrk="tonik1.0a">
  <kap>
    <tld/>a
  </kap>
  <snc mrk="tonik1.0a.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Referanta al la <ref tip="vid" cel="tonik1.0o.MUZ">toniko</ref>.
    </dif>
    <trd lng="be">&c_t;&c_a;&c_n;&c_ib;&c_ch;&c_n;&c_y;</trd>
    <trd lng="nl">betr. hebbend op de <ind>stamtoon</ind></trd>
    <trdgrp lng="pl">
      <trd>toniczny</trd>,
      <trd>w <ind>tonacji</ind></trd>
    </trdgrp>
    <trd lng="pt">t&ocirc;nico</trd>
  </snc>
  <trd lng="fr">tonique<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="it">tonico<klr> (agg.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x4E3B;&#x97F3;&#x306E;  <pr>&#x3042;&#x308B;&#x3058;&#x304A;&#x3068;&#x306E;</pr></trd>,
    <trd>&#x5F37;&#x58EE;&#x306B;&#x3059;&#x308B;  <pr>&#x304D;&#x3087;&#x3046;&#x305D;&#x3046;&#x306B;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
</drv>


  </art>

