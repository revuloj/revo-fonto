<kap>
  <rad>stilet</rad>/o
</kap>
<drv mrk="stilet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Malgranda <ref tip="lst" lst="voko:bataliloj" cel="ponard.0o">ponardo</ref>
      kun <ref tip="prt" cel="kling.0o">klingo</ref> tre akrapinta:
      <ekz>
        nadl-pinta klingo <klr>[&#x2026;]</klr> estas tre malvasta lanc-pinta 
        klingo <klr>[&#x2026;]</klr> ne por tran&ccirc;ilo sed por pikilo, ekzemple <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=126498">Klingo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/0/06/Gunners_stiletto_01.jpg/640px-Gunners_stiletto_01.jpg" prm="PD"><tld/>o 
        <fnt>
          <aut>Jerryk50 &ccirc;e English Wikipedia</aut>
          <lok><url ref="&WCW;/File:Gunners_stiletto_01.jpg">&WCW;/File:Gunners_stiletto_01.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_sh;&c_t;&c_y;&c_l;&c_je;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>graphium</trd>,
    <trd>stilus</trd>
  </trdgrp>
  <trd lng="de">Stilett</trd>
  <trd lng="en">stiletto</trd>
  <trdgrp lng="fa">
    <trd>&a_d;&a_sh;&a_n;&a_h;</trd>,
    <trd>&a_kh;&a_n;&a_j;&a_r;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>stil&eacute;t</trd>,
    <trd>gyilok</trd></trdgrp>
  <trd lng="it">stiletto</trd>
  <trd lng="nl">stilet</trd>
  <trd lng="ru">&c_s;&c_t;&c_i;&c_l;&c_je;&c_t;</trd>
  <trd lng="sk">kr&aacute;tka ostr&aacute; d&yacute;ka</trd>
  <trd lng="uk">&#x441;&#x442;&#x438;&#x43B;&#x435;&#x442;</trd>
</drv>
