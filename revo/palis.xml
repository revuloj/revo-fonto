<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: palis.xml,v 1.28 2025/10/05 22:27:08 revo Exp $">
<kap>
  <ofc>*</ofc><rad>palis</rad>/o
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="palis.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="palis.0o.KOMUNE">
    <dif>
      Forta ligna <ref tip="super" cel="stang.0o">stango</ref>, malsupre pintigita, kiun oni
      starigas en
      teron:
      <ekz>
        vastigu la spacon de via tendo <klr>[&#x2026;]</klr> etendu pli longe viajn &scirc;nurojn,
        kaj viajn <tld/>ojn fortikigu
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 54:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        korvo <klr>[&#x2026;]</klr> sidis sur barila <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de la jaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Arne</nom> fosas truojn por la <tld/>oj de la nova barilparto
        <fnt><bib>InfanTorent2</bib>, <lok>dua parto, &ccirc;apitro
        3a</lok></fnt>;
      </ekz>
      <ekz>
        oblikvaj muroj sur oblikvaj betonaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2008/009361.php">Oblikve-&scirc;ike sed prefere dike-brike</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="fost.0o.paliso">fosto<sncref/></ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_o;&c_l;</trd>,
    <trd>&c_zh;&c_e;&c_r;&c_d;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pilota</trd>,
    <trd>za&scaron;pi&ccaron;at&ecaron;l&yacute; k&uring;l</trd>
  </trdgrp>
    <trdgrp lng="de">
      <trd>Pfahl</trd>,
      <trd>Pfosten</trd>,
      <trd>Stickel</trd>
    </trdgrp>
  <trdgrp lng="en">
    <trd>stake</trd>,
    <trd>post</trd>,
    <trd>pale</trd>,
    <trd>stick</trd>
  </trdgrp>
  <trd lng="es">poste</trd>
  <trdgrp lng="fr">
    <trd>pal</trd>,
    <trd>palis</trd>,
    <trd>pieu</trd>,
    <trd>piquet</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>c&ouml;l&ouml;p</trd>,
    <trd>kar&oacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x676D;  <pr>&#x304F;&#x3044;&#x305C;</pr></trd>,
    <trd>&#x68D2;&#x676D;  <pr>&#x307C;&#x3046;&#x3050;&#x3044;</pr></trd>,
    <trd>&#x652F;&#x67F1;  <pr>&#x3057;&#x3061;&#x3085;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">paal</trd>
  <trdgrp lng="no">
    <trd>p&aring;l</trd>,
    <trd>staur</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pal</trd>,
    <trd>s&lstroke;up</trd>,
    <trd>tyczka podpieraj&aogonek;ca ro&sacute;lin&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>estaca</trd>,
    <trd>tanch&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>par</trd>,
    <trd>st&acirc;lp</trd>,
    <trd>un  <ind>st&acirc;lp</ind>  care sus&tcomma;ine planta</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_l;</trd>
  <trd lng="sk">p&auml;tn&iacute;k</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_ib;&c_l; (&c_ch;&c_a;&c_s;&c_t;&c_o;&c_k;&c_o;&c_l;&c_u;)</trd>,
    <trd>&c_zh;&c_je;&c_r;&c_d;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_t;&c_i;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_p;&c_ib;&c_r;&c_k;&c_a; (&c_d;&c_l;&c_ja; &c_r;&c_o;&c_s;&c_l;&c_i;&c_n;)</trd>,
    <trd>&c_s;&c_t;&c_o;&c_v;&c_p;</trd>
  </trdgrp>
  <trd lng="zh">&#x6A34; <pr>zh&iacute;</pr></trd>
</drv>

<drv mrk="palis.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="palis.0aro.KOMUNE">
    <dif>
      <ref tip="super" cel="bar.0ilo">Barilo</ref> formita de vico da <tld/>oj:
      <ekz>
        flor&gcirc;ardeneto kun bele kolorigita <tld/>aro
        <fnt>
          <bib>Fab1</bib>
          <lok>Lekanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        malgranda <tld/>ara barilo
        <fnt>
          <bib>Fab2</bib>
          <lok>Abio</lok>
        </fnt>;
      </ekz>
      <ekz>
        templo <klr>...</klr> kun nigre farbita <tld/>aro anta&ubreve; sia
        kvinspana pordo
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 53a, volumo 2a, p.
        190a</lok></fnt>;
      </ekz>
      <ekz>
        la&ubreve; la hungara flanko trovi&gcirc;is tiel nomata fera kurteno kun <tld/>aro,
        pikdrato,
        pafistoj kaj minoj
        <fnt>
          <bib>Monato</bib>
          <aut>Karlo Juh&aacute;sz</aut>
          <vrk><url ref="&Monato;/2008/009423.php">Historia momento</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_s;&c_t;&c_a;&c_k;&c_o;&c_l;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_ib;&c_s;&c_a;&c_d;</trd>
  </trdgrp>
  <trd lng="cs">palis&aacute;da</trd>
    <trdgrp lng="de">
      <trd>Palisade</trd>,
      <trd>Pfahlmauer</trd>
    </trdgrp>
  <trdgrp lng="en">
    <trd>fence</trd>,
    <trd>palisade</trd>
  </trdgrp>
  <trd lng="es">cerca</trd>
  <trdgrp lng="fr">
    <trd>palissade</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>c&ouml;l&ouml;psor</trd>,
    <trd>c&ouml;l&ouml;pfal</trd>,
    <trd>palisz&aacute;d</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x67F5;  <pr>&#x304D;</pr></trd>,
    <trd>&#x77E2;&#x6765;  <pr>&#x3084;&#x3089;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">palissade</trd>
  <trdgrp lng="pl">
    <trd>palisada</trd>,
    <trd>cz&eogonek;stok&oacute;&lstroke;</trd>,
    <trd>ostrok&oacute;&lstroke;</trd>
  </trdgrp>
  <trd lng="ro">palisad&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_a;&c_s;&c_t;&c_o;&c_k;&c_o;&c_l;</trd>,
    <trd>&c_i;&c_z;&c_g;&c_o;&c_r;&c_o;&c_d;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_i;&c_s;&c_a;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kolov&aacute; ohrada</trd>,
    <trd>palis&aacute;da</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ch;&c_a;&c_s;&c_t;&c_o;&c_k;&c_ib;&c_l;</trd>,
    <trd>&c_o;&c_g;&c_o;&c_r;&c_o;&c_zh;&c_a;</trd>,
    <trd>&c_d;&c_o;&c_shch;&c_a;&c_n;&c_i;&c_j; &c_p;&c_a;&c_r;&c_k;&c_a;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x56F4;&#x7BF1; <pr>w&eacute;il&iacute;</pr></trd>,
    <trd>&#x570D;&#x7C6C; <pr>w&eacute;il&iacute;</pr></trd>,
    <trd>&#x7826; <pr>zh&agrave;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="palis.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="palis.0eto.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      <ref tip="super" cel="baston.0o">Bastono</ref> malsupre pintigita, kiun oni starigas en teron,
      ekz. por subteni
      vegeta&jcirc;on:
      <ekz>
        ili <klr>[&#x2026;]</klr> &ccirc;irka&ubreve;baris per <tld/>etoj sur la tero malgrandan
        &gcirc;ardenon
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="en">stake <klr>(horticulture)</klr></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_l;&c_o;&c_k;</trd>,
    <trd>&c_zh;&c_e;&c_r;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_p;&c_o;&c_r;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kol&iacute;k</trd>,
    <trd>sloupek</trd>,
    <trd>ty&ccaron;</trd>
  </trdgrp>
  <trd lng="de">Stange</trd>
  <trd lng="es">estaca</trd>
  <trdgrp lng="fr">
    <trd>tuteur<klr> (hort.)</klr></trd>
  </trdgrp>
  <trd lng="hu">kar&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x652F;&#x67F1;  <pr>&#x3057;&#x3061;&#x3085;&#x3046;</pr></trd>,
    <trd>&#x6DFB;&#x3048;&#x6728;  <pr>&#x305D;&#x3048;&#x304E;</pr></trd>
  </trdgrp>
  <trd lng="nl">stok</trd>
  <trd lng="no">stokk</trd>
  <trdgrp lng="pl">
    <trd>palik</trd>,
    <trd>sztacheta</trd>,
    <trd>s&lstroke;upek</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>b&abreve;&tcomma;</trd>,
    <trd>pistil</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_je;&c_r;&c_d;&c_mol;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_y;&c_sh;&c_je;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kol&iacute;k</trd>,
    <trd>st&lacute;pik</trd>,
    <trd>ty&ccaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_k;&c_ib;&c_l;&c_o;&c_k;</trd>,
    <trd>&c_t;&c_i;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="palis.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="palis.0umi.TOR">
    <dif>
      Turmentege <ref tip="super" cel="mort.0igi.malvivigi">mortigi<sncref/></ref> iun, sidigante
      lin sur pinta stango, kiun oni
      enigas en lin:
      <ekz>
        ili <klr>[la asirianoj]</klr> <tld/>umas la venkitojn
        a&ubreve; de&scirc;iras de ili vivantaj la ha&ubreve;ton
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 20a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;
&c_n;&c_a; <ind>&c_k;&c_o;&c_l;</ind></trd>
  <trd lng="cs">nabodnout na k&uring;l narazit na k&uring;l</trd>
  <trd lng="de">pf&auml;hlen</trd>
  <trd lng="en">impale</trd>
  <trd lng="es">empalar</trd>
  <trdgrp lng="fr">
    <trd>empaler</trd>
  </trdgrp>
  <trd lng="hu">kar&oacute;ba h&uacute;z</trd>
  <trd lng="ja">&#x304F;&#x3057;&#x523A;&#x3057;&#x306E;&#x5211;&#x306B;&#x51E6;&#x3059;&#x308B;  <pr>&#x304F;&#x3057;&#x3055;&#x3057;&#x306E;&#x3051;&#x3044;&#x306B;&#x3057;&#x3087;&#x3059;&#x308B;</pr></trd>
  <trd lng="nl">doden door de <ind>paalstraf</ind></trd>
  <trd lng="pl">wbija&cacute; na pal</trd>
  <trd lng="ro">tras &icirc;n &tcomma;eap&abreve;</trd>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_a;&c_d;&c_i;&c_t;&c_mol; &c_n;&c_a;
<ind>&c_k;&c_o;&c_l;</ind></trd>
  <trd lng="sk">napichn&uacute;&tcaron; na k&ocirc;l</trd>
  <trd lng="uk">&c_s;&c_a;&c_d;&c_zh;&c_a;&c_t;&c_i; &c_n;&c_a; &c_p;&c_a;&c_l;&c_ju;/&c_k;&c_ib;&c_l;</trd>
  <trd lng="zh">&#x523A;&#x7A7F; <pr>c&igrave;chu&amacron;n</pr></trd>
</drv>
</art>
