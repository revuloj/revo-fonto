<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>boa</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="boa.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="boa.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda <ref tip="lst" cel="serpen.0o" 
      lst="voko.serpentoj">serpento</ref>, ne venena, sed dan&gcirc;era pro 
      sia premsufokanta fortego; 
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> 
      el familio <ref tip="malprt" cel="boaed.0oj"><tld/>edoj</ref> 
      (<trd lng="la">Boa</trd>).
    </dif>
    <trd lng="be">&c_b;&c_o;&c_a;
    <klr>(&c_z;&c_mol;&c_m;&c_ja;&c_ja;)</klr></trd>
    <trd lng="br">boa</trd>
    <trd lng="ca">boa</trd>
    <trd lng="de">Boa (Schlange)</trd>
    <trd lng="es">boa</trd>
    <trd lng="fr">boa <klr tip="ind">(serpent)</klr></trd>
    <trd lng="hu">boa</trd>
    <trd lng="pl">boa dusiciel</trd>
    <trdgrp lng="ru">
      <trd>&c_b;&c_o;&c_a;</trd>,
      <trd>&c_u;&c_d;&c_a;&c_v;</trd>
    </trdgrp>
    <trd lng="sv">boaorm</trd>
  </snc>
  <snc mrk="boa.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Longa virina pelta&jcirc;o, kiu &ccirc;irka&ubreve;volvas la kolon
      kiel serpento.
    </dif>
    <trd lng="be">&c_b;&c_o;&c_a;
    <klr>(&c_sh;&c_a;&c_l;&c_ib;&c_k;)</klr></trd>
    <trd lng="ca">bo&agrave;</trd>
    <trd lng="de">Boa (Schal)</trd>
    <trd lng="fr">boa <klr tip="ind">(&eacute;charpe)</klr></trd>
    <trd lng="hu">boa <klr tip="ind">(s&aacute;l)</klr></trd>
    <trd lng="pl">boa<klr> (szal)</klr></trd>
    <trd lng="ru">&c_b;&c_o;&c_a;
    <klr>(&c_sh;&c_a;&c_r;&c_f;)</klr></trd>
  </snc>
  <trd lng="en">boa</trd>
  <trd lng="de">Boa</trd>
</drv>
</art>

