<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>boa</rad>/o
</kap>
<drv mrk="boa.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <tld/>edoj (<trd lng="la">Boa</trd>), 
      serpentego, ne venena, sed dan&gcirc;era pro 
      sia premsufokanta fortego. 
    </dif>
    <trdgrp lng="ru">
      <trd>&c_b;&c_o;&c_a;</trd>,
      <trd>&c_u;&c_d;&c_a;&c_v;</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <dif>
      Longa virina pelta&jcirc;o, kiu &ccirc;irka&ubreve;volvas la kolon kiel
      serpento.
    </dif>
    <trd lng="ru">&c_b;&c_o;&c_a; <klr>(&c_sh;&c_a;&c_r;&c_f;)</klr></trd>
  </snc>
  <trd lng="de">Boa</trd>
</drv>
</art>
