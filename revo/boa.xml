<drv mrk="boa.0o">

<kap><ofc>*</ofc><tld/>o</kap>

<snc num="1">

<uzo tip="fak">ZOO</uzo>

<dif>

Genro el familio <tld/>edoj (<trd lng="la">Boa</trd>), 

serpentego, ne venena, sed dan&gcirc;era pro 

sia premsufokanta fortego. 

</dif>

<trd lng="be">&c_b;&c_o;&c_a; <klr>(&c_z;&c_mol;&c_m;&c_ja;&c_ja;)</klr></trd>

<trd lng="br">boa</trd>

<trd lng="es">boa</trd>

<trd lng="fr">boa <klr tip="ind">(serpent)</klr></trd>

<trd lng="hu">boa</trd>

<trdgrp lng="ru">

<trd>&c_b;&c_o;&c_a;</trd>,

<trd>&c_u;&c_d;&c_a;&c_v;</trd>

</trdgrp>

<trd lng="sv">boaorm</trd>

</snc>

<snc num="2">

<uzo tip="fak">TEKS</uzo>

<dif>

Longa virina pelta&jcirc;o, kiu &ccirc;irka&ubreve;volvas la kolon

kiel

serpento.

</dif>

<trd lng="be">&c_b;&c_o;&c_a; <klr>(&c_sh;&c_a;&c_l;&c_ib;&c_k;)</klr></trd>

<trd lng="fr">boa <klr tip="ind">(&eacute;charpe)</klr></trd>

<trd lng="hu">boa <klr tip="ind">(s&aacute;l)</klr></trd>

<trd lng="ru">&c_b;&c_o;&c_a; <klr>(&c_sh;&c_a;&c_r;&c_f;)</klr></trd>

</snc>

<trd lng="de">Boa</trd>

</drv>

</art>

