<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>2</ofc>
      <rad>Epifani</rad>/o
    </kap>

    <drv mrk="epifan.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">KRI</uzo>
      <snc>
        <dif>
          Evangelia epizodo kaj la granda
          kristana <ref tip="lst" cel="fest.0o"
          lst="voko:festoj">festo</ref>
          al &gcirc;i dedi&ccirc;ita, festata je la 6a de januaro, kiu
          por
          plimulto de la kristanaj eklezioj estas la 12a tago post la
          <ref cel="krist.0nasko">Kristnasko</ref>.
        </dif>
        <subsnc mrk="epifan.0o.KAT">
          <dif>
            En la
            <ref cel="krist.okcidenta0anismo">okcidenta
            kristanismo</ref>
            temas pri la veno de <ctl>sa&gcirc;uloj el la oriento</ctl>
            [&Mat; 2], a&ubreve;, la&ubreve; apokrifa tradicio, <ctl>Tri
            Re&gcirc;oj</ctl>,
            kiuj salutis <ref cel="jesu.0o.homo">Jesuon</ref> kiel
            <ref cel="krist.0o">Kriston</ref>.
          </dif>
          <trd lng="de">Dreik&ouml;nigsfest</trd>
          <trdgrp lng="la">
            <trd>festum <ind>magorum</ind></trd>,
            <trd>festum <ind>regum</ind></trd>
          </trdgrp>
          <trd lng="pt">Epifania</trd>
          <trd
          lng="ru">&c_p;&c_o;&c_k;&c_l;&c_o;&c_n;&c_je;&c_n;&c_i;&c_je;
          &c_v;&c_o;&c_l;&c_h;&c_v;&c_o;&c_v;</trd>
        </subsnc>
        <subsnc mrk="epifan.0o.ORT">
          <dif>
            En la <ref cel="krist.orienta0anismo">orienta
            kristanismo</ref>
            temas pri Dia apero en la tri
            <ref cel="person.0o.KRI">personoj</ref> de la
            <ref tip="vid" cel="tri.0unuo">Triunuo</ref> dum
            <ctl>la bapto de la Sinjoro</ctl> [&Mat; 3:16-17], kiu
            epizodo
            malfermas la publikan agadon de Jesuo.
          </dif>
          <trd lng="de">Fest der Erscheinung des Herrn</trd>
          <trd
          lng="grc">&theta;&epsilon;&omikron;&phi;&alfa_acute;&ny;&epsilon;&jota;&alfa;</trd>
          <trd lng="ru">&c_K;&c_r;&c_je;&c_shch;&c_je;&c_n;&c_i;&c_je;
          &c_G;&c_o;&c_s;&c_p;&c_o;&c_d;&c_n;&c_je;</trd>
        </subsnc>
      </snc>
      <trd lng="bg">&c_B;&c_o;&c_g;&c_o;&c_ja;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
      <trdgrp lng="de">
         <trd>Epiphanie</trd>,
         <trd>Epiphaniasfest</trd>
      </trdgrp>
      <trd lng="en">Epiphany</trd>
      <trd lng="es">Epifan&iacute;a</trd>
      <trd lng="fr">&Eacute;piphanie</trd>
      <trd lng="id">Epifani</trd>
      <trd
      lng="grc">&epsilon_psili;&pi;&jota;&phi;&alfa_acute;&ny;&epsilon;&jota;&alfa;</trd>
      <trdgrp lng="pl">
        <trd>&sacute;wi&eogonek;to <ind>Trzech Kr&oacute;li</ind></trd>,
        <trd>Epifania</trd>
      </trdgrp>
      <trd
      lng="ru">&c_B;&c_o;&c_g;&c_o;&c_ja;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
    </drv>
  </art>
