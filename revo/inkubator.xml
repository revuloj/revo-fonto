<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
   <kap>
    <rad>inkubator</rad>/o
   </kap>

   <drv mrk="inkubator.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">MED</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <fnt><bib>KrED</bib></fnt>
    <snc>
     <dif>
       Aparato uzata por kreskigi mikrobojn a&ubreve; por kovi ovojn,
a&ubreve; por bredi frunaskitajn infanetojn.
     </dif>
    </snc>
    <trdgrp lng="de">
       <trd>Brutkasten</trd>,
       <trd>Inkubator<klr> [fak]</klr></trd>
    </trdgrp>
  <trd lng="en">incubator</trd>
  <trd lng="es">incubadora</trd>
  <trd lng="fr">incubateur</trd>
    <trdgrp lng="hu">
       <trd>inkub&aacute;tor</trd>,
       <trd>keltet&odblac;g&eacute;p</trd>
    </trdgrp>

   </drv>

  </art>

