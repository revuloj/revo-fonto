<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: inkubator.xml,v 1.11 2018/12/15 18:10:19 revo Exp $">
<kap>
  <rad>inkubator</rad>/o
</kap>
<drv mrk="inkubator.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <uzo tip="fak">AGR</uzo>
  <uzo tip="fak">BIO</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="dif" cel="kov.0ilo">Kovilo</ref>:
      <ekz>
        <klr>[&#8230;]</klr> multaj a&jcirc;oj kiuj estis konstruitaj 
        de <nom>Drebbel</nom>, kiel sunenergia sistemo per kiu oni hejtis
        Londonon kaj per kiu oni povis kuiri, <tld/>o por ovoj,
        akvopurigilo kaj submar&scirc;ipo kun klimatizilo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=311408"
              >Cornelis Drebbel</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_k;&c_u;&c_b;&c_a;&c_t;&c_a;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>inkub&aacute;tor</trd>,
    <trd>um&ecaron;l&aacute; l&iacute;he&ncaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Brutkasten</trd>,
    <trd>Inkubator<klr> [fak]</klr></trd>
  </trdgrp>
  <trd lng="en">incubator</trd>
  <trd lng="es">incubadora</trd>
  <trdgrp lng="fr">
    <trd>incubateur</trd>,
    <trd>couveuse</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>inkub&aacute;tor</trd>,
    <trd>keltet&odblac;g&eacute;p</trd>
  </trdgrp>
  <trd lng="pt">incubadora</trd>
  <trd lng="ru">&c_i;&c_n;&c_k;&c_u;&c_b;&c_a;&c_t;&c_o;&c_r;</trd>
  <trd lng="sk">umel&aacute; liahe&ncaron;</trd>
</drv>
</art>
