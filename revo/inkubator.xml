<art mrk="$Id: inkubator.xml,v 1.14 2025/12/05 22:34:26 revo Exp $">
<kap>
  <rad>inkubator</rad>/o
</kap>
<drv mrk="inkubator.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <uzo tip="fak">AGR</uzo>
  <uzo tip="fak">BIO</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="dif" cel="kov.0ilo">Kovilo</ref>:
      <ekz>
        <klr>[&#8230;]</klr> multaj a&jcirc;oj kiuj estis konstruitaj
        de <nom>Drebbel</nom>, kiel sunenergia sistemo per kiu oni hejtis
        Londonon kaj per kiu oni povis kuiri, <tld/>o por ovoj,
        akvopurigilo kaj submar&scirc;ipo kun klimatizilo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=311408">Cornelis Drebbel</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_k;&c_u;&c_b;&c_a;&c_t;&c_a;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>inkub&aacute;tor</trd>,
    <trd>um&ecaron;l&aacute; l&iacute;he&ncaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Brutkasten</trd>,
    <trd>Inkubator<klr> [fak]</klr></trd>
  </trdgrp>
  <trd lng="en">incubator</trd>
  <trd lng="es">incubadora</trd>
  <trdgrp lng="fr">
    <trd>incubateur</trd>,
    <trd>couveuse</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>inkub&aacute;tor</trd>,
    <trd>keltet&odblac;g&eacute;p</trd>
  </trdgrp>
  <trd lng="pl">inkubator</trd>
  <trd lng="pt">incubadora</trd>
  <trd lng="ru">&c_i;&c_n;&c_k;&c_u;&c_b;&c_a;&c_t;&c_o;&c_r;</trd>
  <trd lng="sk">umel&aacute; liahe&ncaron;</trd>
  <trd lng="uk">&c_ib;&#x43D;&#x43A;&c_u;&c_b;&c_a;&c_t;&#x43E;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#x4FDD;&#x80B2;&#x7BB1; <pr>b&#x1CE;oy&#xF9;xi&amacron;ng</pr></trd>,
    <trd>&#x5B75;&#21270;&#22120; <pr>f&#x16B;hu&#xE0;q&#xEC;</pr></trd>,
    <trd>&#x5B75;&#21270;&#x573A; <pr>f&#x16B;hu&#xE0;ch&#x1CE;ng</pr></trd>
  </trdgrp>
</drv>
</art>
