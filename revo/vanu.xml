<art mrk="$Id: vanu.xml,v 1.15 2025/04/08 19:13:07 revo Exp $">
<kap>
  <rad>vanu</rad>/i <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="vanu.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <uzo tip="stl">POE</uzo>
  <snc mrk="vanu.0i.KOMUNE">
    <dif>
      <ref tip="dif" cel="aper.mal0i.nevidebligxi">Malaperi
      <sncref/></ref>:
        <ekz>
          lastaj steloj de l&#8217; &ccirc;iel&#8217; <tld/>as
          <fnt>
            <aut>Igorj Guberman, trad. Valentin Melnikov</aut>,
            <vrk>Mi vivas pie...</vrk>,
            <lok><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/ckk/ckk9912.htm">Cerbe
            kaj Kore, 1999-12</url></lok>
          </fnt>;
        </ekz>
        <ekz>
          la hiera&ubreve;aj emocioj <tld/>is
          <fnt>
            <aut>Ivan Turgenev, trad. A. Kor&jcirc;enkov</aut>,
            <vrk><url ref="&LOdE;Rn-turg1.htm">La unua
            amo</url></vrk>,
            <lok>Jekaterinburg: Sezonoj, 1993</lok>
          </fnt>.
        </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>malaperi</ke>.
  </rim>

  <trd lng="be">&c_z;&c_mol;&c_n;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">esvair-se</trd>
  <trdgrp lng="cs">
    <trd>mizet</trd>,
    <trd>zmizet</trd>,
    <trd>ztratit se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verblassen</trd>,
    <trd><klr>(ver)</klr>schwinden</trd>,
    <trd><klr>sich </klr>verfl&uuml;chtigen</trd>,
    <trd>verfliegen</trd>
  </trdgrp>
  <trd lng="en">vanish</trd>
  <trd lng="es">desvanecerse</trd>
  <trdgrp lng="fr">
    <trd>s'<ind>estomper</ind><klr> (disparaitre)</klr></trd>,
    <trd>s'<ind>&eacute;vanouir</ind><klr> (disparaitre)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>elhal<klr> (&aacute;tv)</klr></trd>,
    <trd>elt&udblac;nik<klr> (&aacute;tv)</klr></trd>,
  </trdgrp>
  <trd lng="pl">znika&cacute; <klr> (poet.)</klr></trd>
  <trdgrp lng="pt">
    <trd>desvanecer</trd>,
    <trd>esvanecer</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mizn&uacute;&tcaron;</trd>,
    <trd>strati&tcaron; sa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_shch;&c_je;&c_z;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&#1085;&c_i;&#1082;&c_a;&c_t;&c_i;</trd>,
    <trd>&#1087;&c_r;&#1086;&#1087;&c_a;&c_d;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#28067;&#28982;&#20912;&#37322; <pr>hu&#224;nr&#225;nb&#299;ngsh&#236;</pr></trd>,
    <trd>&#28185;&#28982;&#20912;&#37323; <pr>hu&#224;nr&#225;nb&#299;ngsh&#236;</pr></trd>,
    <trd>&#28040;&#36893; <pr>xi&amacron;osh&#236;</pr></trd>,
    <trd>&#23490;&#28357; <pr>j&#236;mi&#232;</pr></trd>,
    <trd>&#23490;&#28781; <pr>j&#236;mi&#232;</pr></trd>,
    <trd>&#28040;&#22833; <pr>xi&amacron;osh&#299;</pr></trd>
  </trdgrp>
</drv>
</art>

