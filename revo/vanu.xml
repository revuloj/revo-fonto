  <trdgrp lng="fr">
    <trd>s'<ind>estomper</ind><klr> (disparaitre)</klr></trd>,
    <trd>s'<ind>&eacute;vanouir</ind><klr> (disparaitre)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>desvanecer</trd>,
   <trd>esvanecer</trd>
  </trdgrp>
</drv>
</art>

