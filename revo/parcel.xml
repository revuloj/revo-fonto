<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: parcel.xml,v 1.21 2024/02/23 18:26:25 revo Exp $">
<kap>
  <rad>parcel</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="parcel.0o">
  <kap><tld/>o</kap>
  <snc mrk="parcel.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="are.0o.KOMUNE">Areo</ref>, kiun oni povas aparte
      a&ccirc;eti, farmi, heredi:
      <ekz>
        <nom>Manase</nom> ricevis dek <tld/>ojn
        <fnt>
          <bib>MT</bib>
          <lok>Josuo 17:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ka&scirc;e a&ccirc;etis <tld/>on da grundo proksime al la konstruotaj domoj
        <fnt>
          <bib>KrM</bib>
          <lok>4. Konstante Frapante.</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="katastr.0o">katastro</ref>
    <refgrp tip="vid">
      <ref cel="akre.0o">akro, akreo</ref>,
      <ref cel="hub.0o">hubo</ref>,
      <ref cel="ter.0peco">terpeco</ref>
    </refgrp>
  </snc>
  <snc mrk="parcel.0o.de_bieno">
    <dif>
      <ref tip="super" cel="are.0o.KOMUNE">Areo</ref>, apartigita de tuta
      <ref tip="malprt" cel="bien.0o">bieno</ref> por prizorgado:
      <ekz>
        la&ubreve;longe-la&ubreve;lar&gcirc;e etendi&gcirc;is tuboj <klr>[&#8230;]</klr> per tiuj oni kondukis la akvon
        por akvumi la <tld/>ojn, &ccirc;ar la rizo postulas tre malsekan humon
        <fnt>
          <bib>VaK</bib>
          <lok>Dua &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la florkampo dispeci&gcirc;is al <tld/>oj: bluaj, flavaj kaj ru&gcirc;aj kvadratoj amasi&gcirc;is
        unu apud la alia
        <fnt>
          <bib>VaK</bib>
          <lok>Tria &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        en sia apuddoma <tld/>o el 9 aroj li aran&gcirc;is japanstilan
        &gcirc;ardenon
        <fnt>
          <aut>L. Straznickas</aut>,
          <vrk>Entuziasmulo pri Japanio</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2002a, numero 5a, p. 26a</lok>
         </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bed.0o">bedo</ref>,
      <ref cel="kamp.0o.AGR">kampo<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_d;&c_z;&c_je;&c_l; <klr>(&c_z;&c_ja;&c_m;&c_l;&c_ib;)</klr></trd>,
    <trd>&c_d;&c_z;&c_ja;&c_l;&c_ja;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>parcela</trd>,
    <trd>pozemek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Parzelle</trd>,
    <trd>Flurst&uuml;ck</trd>,
    <trd>Grundst&uuml;ck</trd>
  </trdgrp>
  <trd lng="en">parcel<klr> (of land)</klr></trd>
  <trdgrp lng="fr">
    <trd>parcelle<klr> (terrain)</klr></trd>,
    <trd>terrain</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>telek</trd>,
    <trd>parcella</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#19968;&#21306;&#30011;  <pr>&#12402;&#12392;&#12367;&#12364;</pr></trd>,
    <trd>&#19968;&#31558;  <pr>&#12356;&#12387;&#12404;&#12388;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>perceel</trd>,
    <trd>kavel</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>parcela</trd>,
    <trd>dzia&lstroke;ka</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>parcela <klr>(de terreno)</klr></trd>,
    <trd>lote</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_je;&c_m;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; <ind>&c_u;&c_ch;&c_a;&c_s;&c_t;&c_o;&c_k;</ind></trd>,
    <trd>&c_n;&c_a;&c_d;&c_je;&c_l;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_c;&c_je;&c_l;&c_l;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">pozemok</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_a;&c_r;&c_c;&c_je;&c_l;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_c;&c_je;&c_l;&c_ja;</trd>,
    <trd>&c_z;&c_je;&c_m;&c_je;&c_l;&c_mol;&c_n;&c_a; &c_d;&c_ib;&c_l;&c_ja;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_ib;&c_l;</trd>
  </trdgrp>
  <trd lng="zh">&#23447;&#22320; <pr>z&omacron;ngd&igrave;</pr></trd>
</drv>

<drv mrk="parcel.dis0igi">
  <kap>dis<tld/>igi, <var><kap>dis<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="parcel.dis0igi.KOMUNE">
    <dif>
      Dividi bienon en <tld/>ojn.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_ja;&c_l;&c_ib;&c_c;&c_mol; <klr>(&c_z;&c_ja;&c_m;&c_l;&c_ju; &c_n;&c_a; &c_n;&c_a;&c_d;&c_z;&c_je;&c_l;&c_y;)</klr></trd>
  <trdgrp lng="de">
    <trd>parzellieren</trd>,
    <trd>in <ind>Flurst&uuml;ck</ind>e aufteilen</trd>
  </trdgrp>
  <trd lng="en">divide<klr> (land)</klr> into lots</trd>
  <trdgrp lng="fr">
    <trd>lotir<klr> (un terrain)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>parcell&aacute;z</trd>,
    <trd>kim&eacute;r <klr tip="amb">(f&ouml;ldet)</klr></trd>
  </trdgrp>
  <trd lng="nl">verkavelen</trd>
  <trd lng="pl">rozparcelowa&cacute;</trd>
</drv>
</art>
