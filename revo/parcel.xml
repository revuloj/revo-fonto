<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>parcel</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="parcel.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Parto de tero apartigita de tuta bieno:
      <ekz>
        en sia apuddoma <tld/>o el 9 aroj li aran&gcirc;is japanstilan
        &gcirc;ardenon<fnt><aut>Laimius Straznickas</aut>: <vrk>Entuziasmulo pri
        Japanio</vrk>, <bib>Monato</bib><lok>jaro 2002a, numero 5a, p.
        26a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>parcelle<klr> (terrain)</klr></trd>,
    <trd>terrain</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>perceel</trd>,
    <trd>kavel</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_je;&c_m;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; <ind>&c_u;&c_ch;&c_a;&c_s;&c_t;&c_o;&c_k;</ind></trd>,
    <trd>&c_n;&c_a;&c_d;&c_je;&c_l;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_c;&c_je;&c_l;&c_l;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="parcel.dis0igi">
  <kap>dis<tld/>(ig)i</kap>
  <snc>
    <dif>
      Dividi bienon en <tld/>ojn.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>lotir<klr> (un terrain)</klr></trd>
  </trdgrp>
  <trd lng="nl">verkavelen</trd>
</drv>
</art>
