<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: arauxk1.xml,v 1.11 2019/02/15 16:10:18 revo Exp $">
<kap>
  <rad>ara&ubreve;kariac</rad>/oj
</kap>
<drv mrk="arauxk1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <snc mrk="arauxk1.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref> de 
      <ref tip="malprt" cel="konifer.0oj">koniferoj</ref>
      (<trd lng="la">Araucariaceae</trd>):
      <ekz>
        la <tld/>oj estas facile rekoneblaj pro ties dikaj, skvamosimilaj trunkoj kaj
        lar&gcirc;aj pinglofolioj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=318065"
              >Ara&ubreve;kariacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
       <ref cel="agatid.0o">agatido</ref>,
       <ref cel="arauxk.0o">ara&ubreve;kario</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_a;&c_w;&c_k;&c_a;&c_r;&c_y;&c_je;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_a;&c_r;&c_a;&c_u;&c_k;&c_a;&c_r;&c_i;&c_je;&c_v;&c_i;</trd>
  <trd lng="ca">araucari&agrave;cies</trd>
  <trd lng="da">abeskr&aelig;k-familien</trd>
  <trd lng="de">Araukariengew&auml;chse</trd>
  <trd lng="en">araucarians</trd>
  <trd lng="fr">araucariac&eacute;es</trd>
  <trd lng="hu">arauk&aacute;riaf&eacute;l&eacute;k</trd>
  <trd lng="nl">apenboomfamilie</trd>
  <trd lng="pl">araukariowate</trd>
  <trd lng="ru">&c_a;&c_r;&c_a;&c_u;&c_k;&c_a;&c_r;&c_i;&c_je;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&c_a;&c_r;&c_a;&c_u;&c_k;&c_a;&c_r;&c_ib;&c_jeu;&c_v;&c_ib;</trd>
</drv>
</art>

