<art mrk="$Id: labi.xml,v 1.21 2024/01/12 17:28:14 revo Exp $">
<kap>
   <rad>labi</rad>/o
   <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="labi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Unupetala <ref tip="super" cel="korol.0o">korolo</ref>,
      kies rando estas partigita en du kvaza&ubreve;lipojn:
      <ekz>
        la floro de la zingibracoj estas &#8208; simile al tiuj de la
        orkidacoj &#8208; <tld/>a
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18069">Zingibracoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lamiac.0oj">lamiacoj</ref>
    </refgrp>
  </snc>
  <trd lng="ca">corol&middot;la unilabiada</trd>
  <trdgrp lng="cs">
    <trd>labium</trd>,
    <trd>pysk</trd>,
    <trd>ret</trd>
  </trdgrp>
  <trd lng="de">Labium</trd>
  <trd lng="fr">corolle <ind>labi&eacute;e</ind></trd>
  <trd lng="hu">ajak<klr tip="ind"> (vir&aacute;g&eacute;)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#19978;&#21767; <pr>&#12358;&#12431;&#12367;&#12385;&#12403;&#12427;</pr></trd>,
    <trd>&#19979;&#21767; <pr>&#12375;&#12383;&#12367;&#12385;&#12403;&#12427;</pr></trd>,
    <trd>&#21767;&#24418;&#12398;&#12418;&#12398;  <pr>&#12367;&#12385;&#12403;&#12427;&#12363;&#12383;&#12398;&#12418;&#12398;</pr></trd>
  </trdgrp>
  <trd lng="nl">lipvormige <ind>bloemkroon</ind></trd>
  <trd lng="pl">warga</trd>
  <trdgrp lng="pt">
    <trd>l&aacute;bio</trd>,
    <trd>corola labial</trd>
   </trdgrp>
   <trd lng="ro">buz&abreve;</trd>
  <trd lng="ru">&c_n;&c_i;&c_zh;&c_n;&c_ja;&c_ja;
&c_g;&c_u;&c_b;&c_a;<klr tip="amb">
(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">pysk</trd>
  <trd lng="uk">&c_g;&c_u;&c_b;&c_a;</trd>
</drv>
</art>
