<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: labi.xml,v 1.19 2023/09/01 19:10:57 revo Exp $">
<kap>
   <rad>labi</rad>/o
</kap>
<drv mrk="labi.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       Unupetala <ref tip="super" cel="korol.0o">korolo</ref>, 
       kies rando estas partigita en du kvaza&ubreve;lipojn:
      <ekz>
        la floro de la zingibracoj estas &#x2010; simile al tiuj de la
        orkidacoj &#x2010; <tld/>a
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18069">Zingibracoj</url></lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trd lng="ca">corol&middot;la unilabiada</trd>
  <trdgrp lng="cs">
    <trd>labium</trd>,
    <trd>pysk</trd>,
    <trd>ret</trd>
  </trdgrp>
  <trd lng="de">Labium</trd>
  <trd lng="fr">corolle <ind>labi&eacute;e</ind></trd>
  <trd lng="hu">ajak<klr tip="ind"> (vir&aacute;g&eacute;)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x4E0A;&#x5507;  [<ind>&#x3046;&#x308F;&#x304F;&#x3061;&#x3073;&#x308B;</ind>]</trd>,
    <trd>&#x4E0B;&#x5507;  [<ind>&#x3057;&#x305F;&#x304F;&#x3061;&#x3073;&#x308B;</ind>]</trd>,
    <trd>&#x5507;&#x5F62;&#x306E;&#x3082;&#x306E;  [<ind>&#x304F;&#x3061;&#x3073;&#x308B;&#x304B;&#x305F;&#x306E;&#x3082;&#x306E;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">lipvormige <ind>bloemkroon</ind></trd>
  <trd lng="pl">warga</trd> 
  <trdgrp lng="pt">
    <trd>l&aacute;bio</trd>,
    <trd>corola labial</trd>
   </trdgrp>
   <trd lng="ro">buz&abreve;</trd>
  <trd lng="ru">&c_n;&c_i;&c_zh;&c_n;&c_ja;&c_ja; 
&c_g;&c_u;&c_b;&c_a;<klr tip="amb"> 
(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">pysk</trd>
  <trd lng="uk">&#x433;&#x443;&#x431;&#x430;</trd>
</drv>
</art>
