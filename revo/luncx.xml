<art mrk="$Id: luncx.xml,v 1.19 2024/01/15 19:32:09 revo Exp $">
<kap>
  <rad>lun&ccirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="luncx.0o">
  <kap><tld/>o</kap>
  <snc mrk="luncx.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Malabunda, rapida
      <ref tip="super" cel="mangx.0o.regula">tagman&gcirc;o</ref>:
      <ekz>
        la tuta klubo revenis en la &gcirc;ardenon nur post <tld/>o
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 70a, p. 485a</lok></fnt>;
      </ekz>      
      <ekz>
        oni servis al li la lun&ccirc;on, sed li ne faris ian progreson pri la plano
        <fnt>
          <bib>KKl</bib>
        </fnt>;
      </ekz>
      <ekz>
        la koktelojn sekvas lun&ccirc;o, &ccirc;ampano, konjako, dol&ccirc;a muziko
        <fnt>
          <bib>KrB</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mangx.inter0eto">interman&gcirc;eto</ref>,
      <ref cel="mangx.te0o">teman&gcirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_n;&c_ch;</trd>
  <trdgrp lng="de">
    <trd>Mittagessen</trd>,
    <trd>Imbiss</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>lunch</trd>,
    <trd>snack</trd>
  </trdgrp>  
  <trd lng="es">merienda</trd>
  <trdgrp lng="fr">
    <trd>casse-croute</trd>,
    <trd>collation</trd>,
    <trd>en-cas<klr> (casse-croute)</klr></trd>,
    <trd>lunch</trd>
  </trdgrp>
  <trd lng="hu">uzsonna</trd>
  <trdgrp lng="ja">
    <trd>&#x8EFD;&#x98DF;  <pr>&#x3051;&#x3044;&#x3057;&#x3087;&#x304F;</pr></trd>,
    <trd>&#x30E9;&#x30F3;&#x30C1;  <pr>&#x3089;&#x3093;&#x3061;</pr></trd>,
    <trd>&#x5F01;&#x5F53;  <pr>&#x3079;&#x3093;&#x3068;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">lunch</trd>
  <trdgrp lng="pl">
    <trd>lunch</trd>,
    <trd>posi&lstroke;ek po&lstroke;udniowy</trd>
  </trdgrp> 
  <trdgrp lng="pt">
    <trd>lanche</trd>,
    <trd>merenda</trd>
  </trdgrp>
  <trd lng="ro">masa de pr&acirc;nz</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_je;&c_n;&c_ch;</trd>,
    <trd>&c_v;&c_t;&c_o;&c_r;&c_o;&c_j; &c_z;&c_a;&c_v;&c_t;&c_r;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x435;&#x43D;&#x447;</trd>,
    <trd>&#x43B;&#x430;&#x43D;&#x447;</trd>,
    <trd>&#x43F;&#x43E;&#x43B;&#x443;&#x434;&#x435;&#x43D;&#x43E;&#x43A;</trd>,
    <trd>&#x434;&#x440;&#x443;&#x433;&#x438;&#x439; &#x441;&#x43D;&#x456;&#x434;&#x430;&#x43D;&#x43E;&#x43A;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x96F6;&#x98DF; <pr>l&#xED;ngsh&#xED;</pr></trd>,
    <trd>&#x4E2D;&#x98EF; <pr>zh&#x14D;ngf&#xE0;n</pr></trd>,
    <trd>&#x4E2D;&#x996D; <pr>zh&#x14D;ngf&#xE0;n</pr></trd>,
    <trd>&#x5348;&#x98EF; <pr>w&#x1D4;f&#xE0;n</pr></trd>,
