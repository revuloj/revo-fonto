<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: parfum.xml,v 1.21 2016/09/28 20:10:41 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>parfum</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="parfum.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="parfum.0o.bonodoro">
    <dif>
      Agrabla odoro.
    </dif>             
    <trd lng="br">frond</trd>
    <trd lng="de">Duft</trd>
    <trd lng="he">&nun;&yod;&het;&vav;&het;</trd>
    <trd lng="hu">illat</trd>
    <trd lng="pl">zapach</trd>
  </snc>
  <snc mrk="parfum.0o.odorilo">
    <dif>
      Substanco, ordinare solvita en alkoholo kaj eliganta agrablan
      odoron.
    </dif>                   
    <trd lng="br">porfum</trd>
    <trd lng="he">&bet;&vav;&shin;&fmem;</trd>
    <trdgrp lng="hu">
      <trd>illatszer</trd>,
      <trd>parf&uuml;m</trd>
    </trdgrp>
    <trd lng="pl">perfumy</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_f;&c_u;&c_m;&c_a;</trd>,
    <trd>&c_d;&c_u;&c_h;&c_ib;</trd>
  </trdgrp>
  <trd lng="br">c'hwezh-vat</trd>
  <trdgrp lng="cs">
    <trd>parf&eacute;m</trd>,
    <trd>vo&ncaron;avka</trd>
  </trdgrp>
  <trd lng="de">Parf&uuml;m</trd>
  <trd lng="en">perfume</trd>
  <trd lng="es">perfume</trd>
  <trd lng="fr">parfum</trd>
  <trd lng="id">parfum</trd>
  <trdgrp lng="nl">
    <trd>parfum</trd>,
    <trd>geur</trd>
  </trdgrp>
  <trd lng="pt">perfume</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_u;&c_h;&c_i;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_f;&c_ju;&c_m;</trd>
  </trdgrp>
  <trd lng="sk">vo&ncaron;avka</trd>
</drv>

<drv mrk="parfum.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="parfum.0i.KOS">
    <dif>
      Agrable odorigi.
    </dif>
  </snc>
  <trd lng="br">frondi&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>navon&ecaron;t</trd>,
    <trd>parf&eacute;movat</trd>
  </trdgrp>
  <trd lng="de">parf&uuml;mieren</trd>
  <trdgrp lng="en">
    <trd>to perfume</trd>,
    <trd>to scent</trd>
  </trdgrp>
  <trd lng="es">perfumar</trd>
  <trd lng="fr">parfumer</trd>
  <trd lng="he">&lamed;<ind>&bet;&shin;&fmem;</ind></trd>
  <trd lng="nl">parfumeren</trd>
  <trd lng="pl">perfumowa&cacute;</trd>
  <trd lng="ru">&c_d;&c_u;&c_sh;&c_i;&c_t;&c_mol; <klr
  tip="amb">(&c_d;&c_u;&c_h;&c_a;&c_m;&c_i;)</klr></trd>
  <trdgrp lng="sk">
    <trd>naparfumova&tcaron;</trd>,
    <trd>navo&ncaron;avkova&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="parfum.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="parfum.0ujo.KOS">
    <dif>
      Vazo a&ubreve; alia ujo destinita al enteno de <tld/>o:
      <ekz>
        la fumo de la <tld/>ujoj plenigis la ri&ccirc;egan &ccirc;ambron
        <fnt>
          <aut>G. A. Becquer, trad. A. M. Botella</aut>,
          <vrk><url
          ref="https://sites.google.com/site/zaragozafrateco/antonio-marco/beletraj-pagoj/becquer">
          La militestro de la ru&gcirc;aj manoj</url></vrk>,
          <lok>1955</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="pl">flakonik <ind>perfum</ind></trd>
  </snc>
</drv>
</art>
