<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>parfum</rad>/o
</kap>
<drv mrk="parfum.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Agrabla odoro.
    </dif>
    <trd lng="de">Duft</trd>
  </snc>
  <snc>
    <dif>
      Substanco, ordinare solvita en alkoholo kaj eliganta agrablan odoron.
    </dif>
  </snc>
  <trd lng="es">perfume</trd>
  <trd lng="de">Parf&uuml;m</trd>
  <trd lng="en">perfume</trd>
  <trdgrp lng="nl">
    <trd>parfum</trd>,
    <trd>geur</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_u;&c_h;&c_i;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_f;&c_ju;&c_m;</trd>
  </trdgrp>
</drv>

<drv mrk="parfum.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Agrable odorigi.
    </dif>
  </snc>
  <trd lng="de">parf&uuml;mieren</trd>
  <trd lng="es">perfumar</trd>
  <trd lng="nl">parfumeren</trd>
  <trd lng="ru">&c_d;&c_u;&c_sh;&c_i;&c_t;&c_mol; <klr tip="amb">(&c_d;&c_u;&c_h;&c_a;&c_m;&c_i;)</klr></trd>
</drv>
</art>
