  <trd lng="fr">Linux</trd>
  <trd lng="hu">Linux</trd>
  <trdgrp lng="ja">
    <trd>&#x30EA;&#x30CA;&#x30C3;&#x30AF;&#x30B9;</trd>,
    <trd>&#x30EA;&#x30CC;&#x30C3;&#x30AF;&#x30B9;</trd>,
    <trd>Linux</trd>
  </trdgrp>
  <trd lng="nl">Linux</trd>
