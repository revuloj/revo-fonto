<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Linuks</rad>/o
</kap>
<drv mrk="linuks.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KOMP</uzo>
  <snc mrk="linuks.0o.kerno">
    <dif>
      Propre, populara <ref cel="klon.0o.KOMP">klono</ref> de la
      operaciumkerno de <ref tip="super" cel="uniks.0o">Unikso</ref>.
    </dif>
  </snc>
  <snc mrk="linuks.0o.tuto">
    <dif>
      En la &gcirc;enerala, kvankam jure mal&gcirc;usta uzo, la tuta operaciumo
      konstruita &ccirc;irka&ubreve; <tld/>o<sncref ref="linuks.0o.kerno"/>:
      <ekz>
	senkostaj programoj haveblas &ccirc;efe por <tld/>o<fnt><aut>Frank
	Arnaud</aut>: <vrk>Preterle&gcirc;a televidado en E&ubreve;ropo</vrk>,
	<bib>Monato</bib><lok>jaro 1999a, numero 3a, p. 22a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_n;&c_u;&c_k;&c_s;</trd>
  <trd lng="ca">Linux</trd>
  <trd lng="de">Linux</trd>
  <trd lng="en">Linux</trd>
  <trd lng="fa">&a_l;&f_y;&a_n;&a_w;&f_k;&a_s;</trd>
  <trdgrp lng="fr">
    <trd>Linux</trd>
  </trdgrp>
  <trd lng="hu">Linux</trd>
  <trd lng="nl">Linux</trd>
  <trd lng="pt">Linux</trd>
  <trd lng="ru">&c_L;&c_i;&c_n;&c_u;&c_k;&c_s;</trd>
  <trd lng="sk">Linux</trd>
  <trd lng="sv">Linux</trd>
</drv>
</art>
