  <trd lng="mt">kwozjent</trd>
  <trd lng="my">&#x101C;&#x1012;&#x103A;</trd>
  <trd lng="ne">&#x92D;&#x93E;&#x917;&#x92B;&#x932;</trd>
  <trd lng="pa">&#xA38;&#xA2E;&#xA30;&#xA71;&#xA25;&#xA3E;</trd>
  <trd lng="ps">&a_q;&a_s;&a_m;&#x62A; &#x62F;</trd>
  <trdgrp lng="pt">
    <trd>quociente</trd>,
    <trd>cociente</trd>
  </trdgrp>
  <trd lng="rw">igipimo</trd>
  <trd lng="sd">&a_A;&a_n;&a_s;</trd>
  <trd lng="si">&#xDBD;&#xDB6;&#xDCA;&#xDB0;&#xDD2;&#xDBA;</trd>
