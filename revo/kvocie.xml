<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>kvocient</rad>/o
</kap>
<drv mrk="kvocie.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="kvocie.0o.ekzakta">
    <fnt><bib>PV</bib></fnt>
    <dif>
      Rezulto de <ref cel="divid.0o.MAT">divido</ref>:
      <ekz>
        la <tld/>o de <frm>10</frm> per <frm>5</frm> estas <frm>2</frm>.
      </ekz>
    </dif>
    <ref tip="sin" cel="rilat.0o.kvociento">rilato.</ref>
    <rim>
      Tiun &ccirc;i <tld/>on oni foje kvalifikas <ctl>ekzakta</ctl>
      por distingi &gcirc;in de <tld/>o <sncref ref="kvocie.0o.euxklida"/>.
    </rim>
    <trdgrp lng="be">
      <trd>&c_d;&c_z;&c_je;&c_l;&c_mol;</trd>,
      <trd>&c_s;&c_t;&c_a;&c_s;&c_u;&c_n;&c_a;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Quotient</trd>,
      <trd>Verh&auml;ltnis</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>quotient</trd>,
      <trd>ratio</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>quotient<klr tip="ind"> (exact)</klr></trd>,
      <trd>rapport<klr tip="ind"> (entre deux nombres)</klr></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>quoti&euml;nt</trd>,
      <trd>verhouding</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>iloraz</trd>,
      <trd>stosunek</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_a;&c_s;&c_t;&c_n;&c_o;&c_je;<klr tip="ind">
(&c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_t;
&c_d;&c_je;&c_l;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>,
      <trd>&c_o;&c_t;&c_n;&c_o;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kvocie.0o.euxklida">
    <dif>
      <klr>(&ccirc;e <ref cel="divid.euxklida0o">e&ubreve;klida divido</ref>
de
      <frm><k>a</k></frm> per <frm><k>b</k></frm> en
      <ref cel="ring.euxklida0o">e&ubreve;klida ringo</ref>)</klr>
      &Ccirc;iu el la eblaj elementoj <frm><k>q</k></frm>, por kiuj
      <frm><k>a</k> = <k>b</k>&#215;<k>q</k>+<k>r</k></frm> kaj
      <frm>&phi;(<k>r</k>) &#8804; &phi;(<k>b</k>)</frm>:
      <ekz>
        &ccirc;e e&ubreve;klida divido de
        <frm><g>X</g><sup>2</sup>+<g>1</g></frm>
        per <frm><g>X</g></frm> la <tld/>o estas <frm><g>X</g></frm>.
      </ekz>
    </dif>
    <rim>
      Tiun &ccirc;i <tld/>on oni foje kvalifikas <ctl>e&ubreve;klida</ctl>
      por distingi &gcirc;in de <tld/>o <sncref ref="kvocie.0o.ekzakta"/>.
    </rim>
    <trd lng="be">&c_n;&c_ja;&c_p;&c_o;&c_w;&c_n;&c_y;
<ind>&c_d;&c_z;&c_je;&c_l;&c_mol;</ind></trd>
    <trd lng="de">Quotient<klr tip="ind"> (beim euklidischen
Algorithmus)</klr></trd>
    <trd lng="en">quotient<klr tip="ind"> (in a division
algorithm)</klr></trd>
    <trd lng="fr">quotient<klr tip="ind"> (euclidien)</klr></trd>
    <trd lng="pl">iloraz<klr tip="ind"> (przez dzielenie z
reszt&aogonek;)</klr></trd>
    <trd lng="ru">&c_n;&c_je;&c_p;&c_o;&c_l;&c_n;&c_o;&c_je;
<ind>&c_ch;&c_a;&c_s;&c_t;&c_n;&c_o;&c_je;</ind><klr tip="ind">
(&c_p;&c_r;&c_i; &c_d;&c_je;&c_l;&c_je;&c_n;&c_i;&c_i; &c_s;
&c_o;&c_s;&c_t;&c_a;&c_t;&c_k;&c_o;&c_m;)</klr></trd>
  </snc>
  <snc mrk="kvocie.0o.progresio">
    <fnt><bib>MatVort</bib></fnt>
    <dif>
      <klr>(de <ref cel="progre1.geometria0o">geometria
progresio</ref>)</klr>
      La konstanta <tld/>o <sncref ref="kvocie.0o.ekzakta"/> de
      &ccirc;iu termo de &gcirc;i per la anta&ubreve;a.
    </dif>
    <trd lng="be">&c_n;&c_a;&c_z;&c_o;&c_w;&c_n;&c_ib;&c_k; <klr
tip="ind">(&c_g;&c_je;&c_a;&c_m;&c_e;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_j;
&c_p;&c_r;&c_a;&c_g;&c_r;&c_e;&c_s;&c_ib;&c_ib;)</klr></trd>
    <trd lng="de">Quotient<klr tip="ind"> (einer geometrischen
Progression)</klr></trd>
    <trd lng="en">quotient<klr tip="ind"> (of a geometric
progression)</klr></trd>
    <trd lng="fr">raison<klr tip="ind"> (d'une progression
g&eacute;om&eacute;trique)</klr></trd>
    <trd lng="pl">iloraz<klr tip="ind"> (ci&aogonek;gu
geometrycznego)</klr></trd>
    <trd
lng="ru">&c_z;&c_n;&c_a;&c_m;&c_je;&c_n;&c_a;&c_t;&c_je;&c_l;&c_mol; <klr
tip="ind">
(&c_g;&c_je;&c_o;&c_m;&c_je;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_j;
&c_p;&c_r;&c_o;&c_g;&c_r;&c_je;&c_s;&c_s;&c_i;&c_i;)</klr></trd>
  </snc>
  <trdgrp lng="hu">
    <trd>h&aacute;nyados</trd>,
    <trd>kv&oacute;ciens</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>quociente</trd>,
    <trd>cociente</trd>
  </trdgrp>
</drv>
</art>
