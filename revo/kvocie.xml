  <trd lng="mk">&#x43A;&#x43E;&#x43B;&c_i;&c_ch;&#x43D;&c_i;&#x43A;</trd>
  <trd lng="ml">&#xD18;&#xD1F;&#xD15;&#xD17;&#xD4D;&#xD30;&#xD42;&#xD2A;&#xD4D;&#xD2A;&#xD41;&#xD15;&#xD7E;</trd>
  <trd lng="mn">&#x43A;&#x43E;&#x44D;&c_f;&c_f;&c_i;&c_c;&c_je;&#x43D;&c_t;&c_i;&c_j;&c_g;</trd>
  <trd lng="mr">&#x92D;&#x93E;&#x917;&#x93E;&#x915;&#x93E;&#x930;</trd>
