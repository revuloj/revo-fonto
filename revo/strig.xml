<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>strig</rad>/o
</kap>
<drv mrk="strig.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="strig.0o.komune">
    <dif>
      Iu el <ref tip="malprt" cel="strig.0edoj"><tld/>edoj</ref>.
    </dif>
    <trd lng="cs">sowa <klr>(kaj.a.)</klr></trd>
    <trd lng="de">Eule</trd>
    <trd lng="en">owl</trd>
    <trd lng="hu">bagoly</trd>
    <trd lng="pl">sowa</trd>
    <trd lng="ru">&c_s;&c_o;&c_v;&c_a;</trd>
  </snc>
  <snc mrk="strig.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="strig.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Strix</trd>).
    </dif>
    <trd lng="de">Kauz</trd>
  </snc>
</drv>
<drv mrk="strig.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo de <ref tip="super" cel="strig.0oformaj"><tld/>oformaj
      birdoj</ref> (<trd lng="la">Strigidae</trd>), viandman&gcirc;antaj noktobirdoj
      kun anta&ubreve;endirektitaj okuloj kaj kun plumkovritaj piedfingroj.
    </dif>
  </snc>
</drv>
<drv mrk="strig.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Striges</trd>).
    </dif>
  </snc>
  <trd lng="de">Eulen</trd>
</drv>
<drv mrk="strig.orel0o">
  <kap>orel<tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio el la familio <ref tip="super" cel="strig.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Asio otus</trd>), kun orelaspektaj plumtufoj.
    </dif>
  </snc>
  <bld lok="../bld/strig.jpg">orel<tld/>o</bld>
  <trd lng="de">Waldohreule</trd>
</drv>
</art>
