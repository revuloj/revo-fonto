<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: strig.xml,v 1.42 2024/03/25 07:28:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>strig</rad>/o
</kap>
<drv mrk="strig.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="strig.0o.komune">
    <dif>
      Iu el <ref tip="malprt" cel="strige.0oj"><tld/>edoj</ref>:
      granda nokte &ccirc;asanta birdo, ofte asociata kun <ref tip="vid" cel="sagx.0eco">sa&gcirc;eco</ref>:
      <ekz>
        &ccirc;i tiujn abomenu inter la birdoj, ili ne estu man&gcirc;ataj <klr>[&#x2026;]</klr>
        la struton kaj la <tld/>on kaj la mevon kaj la akcipitron kun &gcirc;ia speco
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 11:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sonado venas de eksterordinare granda <tld/>o en kavo de arbo <klr>[&#x2026;]</klr> tio estas
        sa&gcirc;ega <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Sonorilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i estas tiel respektinda <tld/>ino
        <fnt>
          <bib>Fab3</bib>
          <lok>Estas tute certe</lok>
        </fnt>!
      </ekz>
      <ekz>
        respektinda, tre klera maljuna <tld/>o, &gcirc;i sciis pli ol la gardisto
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi &ccirc;iufoje flugadis tien kaj reen tra tuta nokto, kvaza&ubreve; mi estus <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        maljuna <tld/>o, kiu kriis trifoje uhu
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>;
      </ekz>
      <ekz>
        grandaj <tld/>okuloj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Stelmona&hcirc;ejo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>viza&gcirc;a
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Stelmona&hcirc;ejo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_a;&c_v;&c_a;</trd>
    <trd lng="br">kaouenn</trd>
    <trd lng="ca">estr&iacute;gid</trd>
    <trd lng="cs">sova</trd>
    <trd lng="de">Eule</trd>
    <trd lng="en">owl</trd>
    <trdgrp lng="es">
      <trd>b&uacute;ho</trd>,
      <trd>estr&iacute;gido</trd>
    </trdgrp>
    <trd lng="fr">hibou</trd>
    <trd lng="hu">bagoly</trd>
    <trd lng="nl">uil</trd>
    <trd lng="pl">sowa</trd>
    <trd lng="ru">&c_s;&c_o;&c_v;&c_a;</trd>
    <trdgrp lng="zh">
    <trd>&#x9D69; <pr>f&#xFA;</pr></trd>,
    <trd>&#x591C;&#x67AD; <pr>y&#xE8;xi&#x101;o</pr></trd>,
    <trd>&#x591C;&#x689F; <pr>y&#xE8;xi&#x101;o</pr></trd>,
    <trd>&#x9D1F;&#x9D1E; <pr>ch&#x12B;xi&#x101;o</pr></trd>,
    <trd>&#x9E31;&#x9E2E; <pr>ch&#x12B;xi&#x101;o</pr></trd>,
    <trd>&#x9D42; <pr>xi&#x16B;</pr></trd>,
    <trd>&#x9E3A; <pr>xi&#x16B;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="strig.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="strige.0oj"><tld/>edoj</ref>
      (<trd lng="la">Strix</trd>):
      <ekz>
        Arbar<tld/>o (<nom>Strix aluco</nom>) estas mezgranda <klr>[&#x2026;]</klr>
        nokta rabobirdo, kiu apartenas al genro <tld lit="S"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18879">Arbarstrigo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/2b/Tawny_wiki_edit1.jpg/323px-Tawny_wiki_edit1.jpg" prm="CC BY-SA         2.5">Arbar<tld/>o (Strix aluco)
         <fnt>
           <aut>K.-M. Hansche - prilaborita de: Arad, propra verko</aut>
           <lok><url ref="&WCW;/File:Tawny_wiki_edit1.jpg">&WCW;/File:Tawny_wiki_edit1.jpg</url></lok>
         </fnt>
    </bld>
    <trd lng="be">&c_n;&c_ja;&c_ja;&c_s;&c_y;&c_c;&c_mol; <klr>(&c_r;&c_o;&c_d;)</klr></trd>
    <trd lng="ca">g&egrave;nere Strix</trd>
    <trd lng="de">Kauz</trd>
    <trd lng="es">g&eacute;nero Strix</trd>
    <trd lng="nl">uil</trd>
    <trd lng="ru">&c_n;&c_je;&c_ja;&c_s;&c_y;&c_t;&c_mol; <klr tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>
    <trdgrp lng="zh">
    <trd>&#x9CF4;&#x89D2;&#x9D1E; <pr>m&#xED;ngji&#x1CE;oxi&#x101;o</pr></trd>,
    <trd>&#x9E23;&#x89D2;&#x9D1E; <pr>m&#xED;ngji&#x1CE;oxi&#x101;o</pr></trd>
  </trdgrp>
</snc>
  <trd lng="bo">&t_-a;&t_shabchu;&t_ga;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trd lng="ja">&#x30D5;&#x30AF;&#x30ED;&#x30A6;  <pr>&#x3075;&#x304F;&#x308D;&#x3046;</pr></trd>
  <trd lng="sk">sova</trd>
  <trd lng="uk">(&c_s;&c_ib;&c_r;&c_a;) &c_s;&#x43E;&c_v;&c_a;</trd>
</drv>

<drv mrk="strig.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref> el la klaso <ref tip="malprt" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Strigiformes</trd>):
      <ekz>
        <tld lit="S"/>oformaj birdoj konsistas el du familioj: Titonedoj, Strigedoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18885">Strigoformaj birdoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_v;&c_ib;&c_n;&c_y;&c_ja; <klr>(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="ca">estrigiformes</trd>
  <trd lng="cs">sovy</trd>
  <trd lng="de">Eulen</trd>
  <trd lng="en">owl</trd>
  <trdgrp lng="es">
    <trd>estrigiformes</trd>,
    <trd>rapaces nocturnas</trd>
  </trdgrp>
  <trd lng="fr">Strigiformes</trd>
  <trd lng="hu">baglyok<klr tip="amb"> (rend)</klr></trd>
  <trd lng="nl">uilen</trd>
  <trd lng="pl">sowy</trd>
  <trd lng="ro">strigiform&abreve;</trd>
  <trd lng="ru">&c_s;&c_o;&c_v;&c_y; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trd lng="sk">sovy</trd>
  <trd lng="uk">&c_s;&#x43E;&c_v;&#x43E;&#x43F;&#x43E;&c_d;&c_ib;&c_b;&#x43D;&c_ib;</trd>
  <trdgrp lng="zh">
    <trd>&#x732B;&#x5934;&#x9E70; <pr>m&#x101;ot&#xF3;uy&#x12B;ng</pr></trd>,
    <trd>&#x8C93;&#x982D;&#x9DF9; <pr>m&#x101;ot&#xF3;uy&#x12B;ng</pr></trd>,
    <trd>&#x67AD; <pr>xi&#x101;o</pr></trd>,
    <trd>&#x689F; <pr>xi&#x101;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="strig.orel0o">
  <kap>orel<tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_specioj" cel="speci.0o">Specio</ref>
      el la familio <ref tip="super" cel="strige.0oj"><tld/>edoj</ref>
      (<trd lng="la">Asio otus</trd>), kun orelaspektaj plumtufoj:
      <ekz>
        la noktuo estas simila al la malgranda orel<tld/>o, sed <klr>[&#x2026;]</klr> estas pli fortika kaj
        ne havas ties oreltufojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18886">Noktuo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/0/06/B%C3%BAho_chico_%28Asio_otus%29%2C_Arcos_de_la_Frontera%2C_C%C3%A1diz%2C_Espa%C3%B1a%2C_2015-12-08%2C_DD_05.jpg/454px-B%C3%BAho_chico_%28Asio_otus%29%2C_Arcos_de_la_Frontera%2C_C%C3%A1diz%2C_Espa%C3%B1a%2C_2015-12-08%2C_DD_05.jpg" prm="CC BY-SA 4.0">orel<tld/>o (Asio otus), Arcos de la Frontera, C&aacute;diz, Hispanujo
         <fnt>
           <aut>Diego Delso, propra verko</aut>
           <lok><url ref="&WCW;/File:B%C3%BAho_chico_(Asio_otus),_Arcos_de_la_Frontera,_C%C3%A1diz,_Espa%C3%B1a,_2015-12-08,_DD_05.jpg">&WCW;/File:B%C3%BAho_chico_(Asio_otus),_Arcos_de_la_Frontera,_C%C3%A1diz,_Espa%C3%B1a,_2015-12-08,_DD_05.jpg</url></lok>
         </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_sh;&c_a;&c_s;&c_t;&c_a;&c_ja; <ind>&c_s;&c_a;&c_v;&c_a;</ind></trd>
  <trdgrp lng="br">
    <trd>penn-kazh</trd>,
    <trd>toud<klr tip="ind"> (evn&gt;</klr></trd>
  </trdgrp>
  <trd lng="ca">mussol banyut</trd>
  <trd lng="de">Waldohreule</trd>
  <trd lng="en">long-eared owl</trd>
  <trdgrp lng="fr">
    <trd>hibou moyen duc</trd>,
    <trd>hibou &agrave; aigrettes longues</trd>
  </trdgrp>
  <trd lng="es">b&uacute;ho chico</trd>
  <trd lng="hu">erdei <ind>f&uuml;lesbagoly</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x30C8;&#x30E9;&#x30D5;&#x30BA;&#x30AF;</trd>,
    <trd>&#x30DF;&#x30DF;&#x30BA;&#x30AF;</trd>
  </trdgrp>
  <trd lng="nl">ransuil</trd>
  <trd lng="pl">uszatka</trd>
  <trd lng="ro">ciuf de p&abreve;dure</trd>
  <trd lng="ru">&c_u;&c_sh;&c_a;&c_s;&c_t;&c_a;&c_ja;
    <ind>&c_s;&c_o;&c_v;&c_a;</ind></trd>
  <trd lng="sk">my&scaron;iarka u&scaron;at&aacute;</trd>
  <trd lng="cs">kalous u&scaron;at&yacute;</trd>
  <trd lng="uk">&c_v;&c_u;&c_h;&c_a;&c_t;&c_a; &c_s;&#x43E;&c_v;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x9577;&#x8033;&#x9D1E; <pr>ch&#xE1;ng&#x11B;rxi&#x101;o</pr></trd>,
    <trd>&#x957F;&#x8033;&#x9E2E; <pr>ch&#xE1;ng&#x11B;rxi&#x101;o</pr></trd>
  </trdgrp>
</drv>
</art>
