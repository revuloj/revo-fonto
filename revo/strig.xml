<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: strig.xml,v 1.44 2025/01/22 08:34:12 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>strig</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="strig.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="strig.0o.komune">
    <dif>
      Iu el <ref tip="malprt" cel="strige.0oj"><tld/>edoj</ref>:
      granda nokte &ccirc;asanta birdo, ofte asociata kun <ref tip="vid" cel="sagx.0eco">sa&gcirc;eco</ref>:
      <ekz>
        &ccirc;i tiujn abomenu inter la birdoj, ili ne estu man&gcirc;ataj <klr>[&#8230;]</klr>
        la struton kaj la <tld/>on kaj la mevon kaj la akcipitron kun &gcirc;ia speco
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 11:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sonado venas de eksterordinare granda <tld/>o en kavo de arbo <klr>[&#8230;]</klr> tio estas
        sa&gcirc;ega <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Sonorilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i estas tiel respektinda <tld/>ino
        <fnt>
          <bib>Fab3</bib>
          <lok>Estas tute certe</lok>
        </fnt>!
      </ekz>
      <ekz>
        respektinda, tre klera maljuna <tld/>o, &gcirc;i sciis pli ol la gardisto
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi &ccirc;iufoje flugadis tien kaj reen tra tuta nokto, kvaza&ubreve; mi estus <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        maljuna <tld/>o, kiu kriis trifoje uhu
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>;
      </ekz>
      <ekz>
        grandaj <tld/>okuloj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Stelmona&hcirc;ejo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>viza&gcirc;a
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Stelmona&hcirc;ejo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_a;&c_v;&c_a;</trd>
    <trd lng="br">kaouenn</trd>
    <trd lng="ca">estr&iacute;gid</trd>
    <trd lng="cs">sova</trd>
    <trd lng="de">Eule</trd>
    <trd lng="en">owl</trd>
    <trdgrp lng="es">
      <trd>b&uacute;ho</trd>,
      <trd>estr&iacute;gido</trd>
    </trdgrp>
    <trd lng="fr">hibou</trd>
    <trd lng="hu">bagoly</trd>
    <trd lng="nl">uil</trd>
    <trd lng="pl">sowa</trd>
    <trd lng="ru">&c_s;&c_o;&c_v;&c_a;</trd>
    <trdgrp lng="zh">
    <trd>&#40297; <pr>f&uacute;</pr></trd>,
    <trd>&#22812;&#26541; <pr>y&egrave;xi&amacron;o</pr></trd>,
    <trd>&#22812;&#26783; <pr>y&egrave;xi&amacron;o</pr></trd>,
    <trd>&#40223;&#40222; <pr>ch&imacron;xi&amacron;o</pr></trd>,
    <trd>&#40497;&#40494; <pr>ch&imacron;xi&amacron;o</pr></trd>,
    <trd>&#40258; <pr>xi&umacron;</pr></trd>,
    <trd>&#40506; <pr>xi&umacron;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="strig.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="strige.0oj"><tld/>edoj</ref>
      (<trd lng="la">Strix</trd>):
      <ekz>
        Arbar<tld/>o (<nom>Strix aluco</nom>) estas mezgranda <klr>[&#8230;]</klr>
        nokta rabobirdo, kiu apartenas al genro <tld lit="S"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18879">Arbarstrigo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/2b/Tawny_wiki_edit1.jpg/323px-Tawny_wiki_edit1.jpg"
         prm="CC BY-SA 2.5">Arbar<tld/>o (Strix aluco)
         <fnt>
           <aut>K.-M. Hansche - prilaborita de: Arad, propra verko</aut>
           <lok><url
           ref="&WCW;/File:Tawny_wiki_edit1.jpg">&WCW;/File:Tawny_wiki_edit1.jpg</url></lok>
         </fnt>
    </bld>
    <trd lng="be">&c_n;&c_ja;&c_ja;&c_s;&c_y;&c_c;&c_mol; <klr>(&c_r;&c_o;&c_d;)</klr></trd>
    <trd lng="ca">g&egrave;nere Strix</trd>
    <trd lng="de">Kauz</trd>
    <trd lng="es">g&eacute;nero Strix</trd>
    <trd lng="nl">uil</trd>
    <trd lng="ru">&c_n;&c_je;&c_ja;&c_s;&c_y;&c_t;&c_mol; <klr tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>
    <trdgrp lng="zh">
    <trd>&#40180;&#35282;&#40222; <pr>m&iacute;ngji&#462;oxi&amacron;o</pr></trd>,
    <trd>&#40483;&#35282;&#40222; <pr>m&iacute;ngji&#462;oxi&amacron;o</pr></trd>
  </trdgrp>
</snc>
  <trd lng="bo">&t_-a;&t_shabchu;&t_ga;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trd lng="ja">&#12501;&#12463;&#12525;&#12454;  <pr>&#12405;&#12367;&#12429;&#12358;</pr></trd>
  <trd lng="sk">sova</trd>
  <trd lng="uk">(&c_s;&c_ib;&c_r;&c_a;) &c_s;&c_o;&c_v;&c_a;</trd>
</drv>

<drv mrk="strig.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref> el la klaso <ref
      tip="malprt" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Strigiformes</trd>):
      <ekz>
        <tld lit="S"/>oformaj birdoj konsistas el du familioj: Titonedoj, Strigedoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18885">Strigoformaj birdoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_v;&c_ib;&c_n;&c_y;&c_ja; <klr>(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="ca">estrigiformes</trd>
  <trd lng="cs">sovy</trd>
  <trd lng="de">Eulen</trd>
  <trd lng="en">owl</trd>
  <trdgrp lng="es">
    <trd>estrigiformes</trd>,
    <trd>rapaces nocturnas</trd>
  </trdgrp>
  <trd lng="fr">Strigiformes</trd>
  <trd lng="hu">baglyok<klr tip="amb"> (rend)</klr></trd>
  <trd lng="nl">uilen</trd>
  <trd lng="pl">sowy</trd>
  <trd lng="ro">strigiform&abreve;</trd>
  <trd lng="ru">&c_s;&c_o;&c_v;&c_y; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trd lng="sk">sovy</trd>
  <trd lng="uk">&c_s;&c_o;&c_v;&c_o;&c_p;&c_o;&c_d;&c_ib;&c_b;&c_n;&c_ib;</trd>
  <trdgrp lng="zh">
    <trd>&#29483;&#22836;&#40560; <pr>m&amacron;ot&oacute;uy&imacron;ng</pr></trd>,
    <trd>&#35987;&#38957;&#40441; <pr>m&amacron;ot&oacute;uy&imacron;ng</pr></trd>,
    <trd>&#26541; <pr>xi&amacron;o</pr></trd>,
    <trd>&#26783; <pr>xi&amacron;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="strig.nizo0o">
  <kap>
    nizo<tld/>o
    <fnt>
      <aut>Wouter F. Pilger</aut>
      <vrk><url ref="http://www.eventoj.hu/steb/vortaroj/komunlingvaj-nomoj-de-europaj-birdoj.htm"
          >Komunlingvaj nomoj de e&ubreve;ropaj birdoj</url></vrk>
    </fnt>
  </kap>
  <snc>
    <dif>
      Strigo kun aspekto kaj flugbildo simila al nizo (<trd lng="la">Surnia ulula</trd>),
      kun horizontalaj strioj &ccirc;e la ventro kaj ulula kanto:
      <ekz>
        la nizo<tld/>o kovas la idojn en Norrlando, sed aperas kiel pasbirdo kelkajn
        jarojn a&ubreve;tune kaj vintre en suda Svedio
        <fnt>
          <aut>Jan Hellstr&ouml;m</aut>
          <vrk><url ref="https://www.esperanto-gbg.org/oraklubo/Ora_Jxurnalo_n-ro_28.pdf"
              >Nizostrigo</url></vrk>
          <lok>Ora &Jcirc;urnalo, n-ro 28, Aprilo 2020, p. 18</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="ulul.0i">ululi</ref>
    <bld
      lok="&WCU;/thumb/e/ef/Northern_hawk_owl_(Surnia_ulula)_(Schleswig_Hostein).jpg/320px-Northern_hawk_owl_(Surnia_ulula)_(Schleswig_Hostein).jpg"
      prm="CC0"
      >nizo<tld/>o St. Peter-Ording, Germanujo
        <fnt>
          <aut>Hobbyfotowiki, propra verko</aut>
          <lok><url ref="&WCW;/File:Northern_hawk_owl_(Surnia_ulula)_(Schleswig_Hostein).jpg"
              >https://commons.wikimedia.org/wiki/File:Northern_hawk_owl_(Surnia_ulula)_(Schleswig_Hostein).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_v;&c_a;-&c_b;&c_ja;&c_r;&c_o;&c_z;&c_a;&c_w;&c_k;&c_a;</trd>
  <trd lng="de">Sperbereule</trd>
  <trd lng="en">northern hawk-owl</trd>
  <trd lng="fr">chouette &eacute;pervi&egrave;re</trd>
  <trd lng="uk">&c_s;&c_o;&c_v;&c_a; &c_ja;&c_s;&c_t;&c_r;&c_u;&c_b;&c_i;&c_n;&c_a;</trd>
  <trd lng="ru">&c_ja;&c_s;&c_t;&c_r;&c_je;&c_b;&c_i;&c_n;&c_a;&c_ja; &c_s;&c_o;&c_v;&c_a;</trd>
</drv>

<drv mrk="strig.orel0o">
  <kap>orel<tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_specioj" cel="speci.0o">Specio</ref>
      el la familio <ref tip="super" cel="strige.0oj"><tld/>edoj</ref>
      (<trd lng="la">Asio otus</trd>), kun orelaspektaj plumtufoj:
      <ekz>
        la noktuo estas simila al la malgranda orel<tld/>o, sed <klr>[&#8230;]</klr> estas pli fortika kaj
        ne havas ties oreltufojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18886">Noktuo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
    lok="&WCU;/thumb/0/06/B%C3%BAho_chico_%28Asio_otus%29%2C_Arcos_de_la_Frontera%2C_C%C3%A1diz%2C_Espa%C3%B1a%2C_2015-12-08%2C_DD_05.jpg/454px-B%C3%BAho_chico_%28Asio_otus%29%2C_Arcos_de_la_Frontera%2C_C%C3%A1diz%2C_Espa%C3%B1a%2C_2015-12-08%2C_DD_05.jpg"
    prm="CC BY-SA 4.0">orel<tld/>o (Asio otus), Arcos de la Frontera, C&aacute;diz, Hispanujo
         <fnt>
           <aut>Diego Delso, propra verko</aut>
           <lok><url
           ref="&WCW;/File:B%C3%BAho_chico_(Asio_otus),_Arcos_de_la_Frontera,_C%C3%A1diz,_Espa%C3%B1a,_2015-12-08,_DD_05.jpg">&WCW;/File:B%C3%BAho_chico_(Asio_otus),_Arcos_de_la_Frontera,_C%C3%A1diz,_Espa%C3%B1a,_2015-12-08,_DD_05.jpg</url></lok>
         </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_sh;&c_a;&c_s;&c_t;&c_a;&c_ja; <ind>&c_s;&c_a;&c_v;&c_a;</ind></trd>
  <trdgrp lng="br">
    <trd>penn-kazh</trd>,
    <trd>toud<klr tip="ind"> (evn&gt;</klr></trd>
  </trdgrp>
  <trd lng="ca">mussol banyut</trd>
  <trd lng="de">Waldohreule</trd>
  <trd lng="en">long-eared owl</trd>
  <trdgrp lng="fr">
    <trd>hibou moyen duc</trd>,
    <trd>hibou &agrave; aigrettes longues</trd>
  </trdgrp>
  <trd lng="es">b&uacute;ho chico</trd>
  <trd lng="hu">erdei <ind>f&uuml;lesbagoly</ind></trd>
  <trdgrp lng="ja">
    <trd>&#12488;&#12521;&#12501;&#12474;&#12463;</trd>,
    <trd>&#12511;&#12511;&#12474;&#12463;</trd>
  </trdgrp>
  <trd lng="nl">ransuil</trd>
  <trd lng="pl">uszatka</trd>
  <trd lng="ro">ciuf de p&abreve;dure</trd>
  <trd lng="ru">&c_u;&c_sh;&c_a;&c_s;&c_t;&c_a;&c_ja;
    <ind>&c_s;&c_o;&c_v;&c_a;</ind></trd>
  <trd lng="sk">my&scaron;iarka u&scaron;at&aacute;</trd>
  <trd lng="cs">kalous u&scaron;at&yacute;</trd>
  <trd lng="uk">&c_v;&c_u;&c_h;&c_a;&c_t;&c_a; &c_s;&c_o;&c_v;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#38263;&#32819;&#40222; <pr>ch&aacute;ng&ecaron;rxi&amacron;o</pr></trd>,
    <trd>&#38271;&#32819;&#40494; <pr>ch&aacute;ng&ecaron;rxi&amacron;o</pr></trd>
  </trdgrp>
</drv>
</art>
