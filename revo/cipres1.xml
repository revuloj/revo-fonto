<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cipres1.xml,v 1.18 2021/03/06 06:20:01 revo Exp $">
<kap>
  <rad>cipresac</rad>/oj
</kap>
<drv mrk="cipres1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <snc mrk="cipres1.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt" cel="konifer.0oj">koniferoj</ref>
      (<trd lng="la">Cupressaceae</trd>), al kiuj apartenas ekzemple
      <refgrp tip="prt">
        <ref cel="cipres.0o">cipreso</ref> kaj
        <ref cel="junipe.0o">junipero</ref>:
      </refgrp>
      <ekz>
        la giganta <ref tip="prt" cel="tuj1.0o">tujo</ref>
        estas tujo el la familio de <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=164338">Giganta tujo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_p;&c_a;&c_r;&c_y;&c_s;&c_a;&c_v;&c_y;&c_ja;
  <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">cupress&agrave;cies</trd>
  <trd lng="de">Zypressengew&auml;chse</trd>
  <trdgrp lng="en">
    <trd>Cupressaceae</trd>,
    <trd>cypress <klr tip="amb">(family of trees)</klr></trd>
  </trdgrp>
  <trd lng="fr">cupressac&eacute;es</trd>
  <trd lng="he">&#x5D1;&#x5E8;&#x5D5;&#x5E9;&#x5D9;&#x5DD;</trd>
  <trd lng="hu">ciprusf&eacute;l&eacute;k</trd>
  <trd lng="it">cipressacee</trd>
  <trd lng="nl">cipresfamilie</trd>
  <trd lng="pl">cyprysowate</trd>
  <trd lng="pt">cupress&aacute;ceas</trd>
  <trd lng="ru">&c_k;&c_i;&c_p;&c_a;&c_r;&c_i;&c_s;&c_o;&c_v;&c_y;&c_je;
  <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>

