<art mrk="$Id$">
<kap>
  <rad>cipresac</rad>/oj
</kap>
<drv mrk="cipres1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <snc mrk="cipres1.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Familio de <ref tip="super"
      cel="konifer.0oplantoj">koniferoplantoj</ref>
      (<trd lng="la">Cupressaceae</trd>), al kiuj apartenas ekzemple
