<vortaro>
  <art mrk="$Id: bulgur.xml,v 1.1 2023/07/09 15:25:18 afido Exp $">
    <kap>
      <rad>bulgur</rad>/o
    </kap>

<drv mrk="bulgur.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Vaporkuirita kaj hakita
      <ref tip="super" cel="tritik.0o">tritiko</ref>:
      <ekz>
        bazaj tritikaj mediteraneaj man&gcirc;a&jcirc;oj inkluzivas
        produktojn kiel pasta&jcirc;oj kaj
        <ref cel="semol.0o">semol</ref>a&jcirc;oj kiel kuskuso kaj <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=312621">Mediteranea kuirarto</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        por kvar personoj necesas: unu glaso da ru&gcirc;aj
        lentoj, unu cepo, du supkuleroj da
        <tld/>o a&ubreve; faruno, olivoleo, tomatoka&ccirc;o, salo,
        papriko, seka mento
        <fnt>
          <aut>Dominique Karaca</aut>
          <vrk><url ref="http://esperanto-gep.asso.fr/informiloj/informilo_111.pdf"
              >Supo de ru&gcirc;aj (koralkoloraj) lentoj</url></vrk>
          <lok>Esperanto P&eacute;rigord, n-ro 111, printempo 2018</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Bulgur</trd>
    <trd lng="en">bulgur</trd>
    <trd lng="fr">boulghour</trd>
  </snc>
</drv>

  </art>

