<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>la&ccirc;</rad>/o
</kap>
<drv mrk="lacx.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      &Scirc;nureto el kotono, silko, ledo ktp, kiun oni trapasigas tra
      specialaj truetoj por stre&ccirc;i kaj kunligi du partojn de vestoj,
      korseto, &scirc;uoj kc.
    </dif>
    <trd lng="en">lace</trd>
    <trdgrp lng="es">
      <trd>lazo</trd>,
      <trd>cord&oacute;n</trd>
    </trdgrp>
    <trd lng="fr">lacet</trd>
    <trd lng="nl">veter</trd>
    <trd lng="ru">&c_sh;&c_n;&c_u;&c_r;&c_o;&c_k;</trd>
  </snc>
  <snc mrk="lacx.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <klr>(en <ref cel="spac.topologia0o">topologia spaco</ref>
      <em>E</em>)</klr>
      Tia <ref tip="super" cel="ir.0ejo.MAT">irejo</ref> &phi;, ke
      &gcirc;iaj origino kaj fino estas identaj; alidire &phi;(0)=&phi;(1).
    </dif>
    <trdgrp lng="en">
      <trd>loop</trd>,
      <trd>closed <ind>path</ind></trd>
    </trdgrp>
    <trd lng="de">geschlossener <ind>Weg</ind></trd>
    <trdgrp lng="fr">
      <trd>lacet<klr tip="ind"> (math.)</klr></trd>,
      <trd>chemin ferm&eacute;<klr tip="ind"> (math.)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_je;&c_t;&c_l;&c_ja;<klr tip="ind"> (&c_m;&c_a;&c_t;.)</klr></trd>,
      <trd>&c_z;&c_a;&c_m;&c_k;&c_n;&c_u;&c_t;&c_y;&c_j; <ind>&c_p;&c_u;&c_t;&c_mol;</ind><klr tip="ind"> (&c_m;&c_a;&c_t;)</klr></trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="lacx.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kunligi per <tld/>o:
      <ekz>
        <tld/>i &scirc;uojn
        <klr>(f)</klr>;
      </ekz>
      <ekz>
        mi ne volas <tld/>i mian volon per le&gcirc;oj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">lace</trd>
  <trd lng="es">enlazar</trd>
  <trd lng="fr">lacer</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_n;&c_u;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_v;&c_ja;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_v; &c_p;&c_je;&c_r;&c_je;&c_n;&c_o;&c_s;&c_n;&c_o;&c_m; &c_s;&c_m;&c_y;&c_s;&c_l;&c_je;)</klr></trd>
  </trdgrp>
</drv>
</art>
