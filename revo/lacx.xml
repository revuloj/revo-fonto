<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lacx.xml,v 1.33 2024/01/15 16:14:07 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>la&ccirc;</rad>/o
</kap>
<drv mrk="lacx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lacx.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref cel="sxnur.0o.KOMUNE">&Scirc;nureto</ref>, kiun oni trapasigas tra specialaj truetoj por stre&ccirc;i
      kaj
      kunligi du partojn de vesto, ekz-e <ref tip="malprt" cel="korset.0o">korseto</ref>,
      <ref tip="malprt" cel="sxu.0o">&scirc;uo</ref> kc:
      <ekz>
        alligu la surbrusta&jcirc;on per &gcirc;iaj ringoj al la ringoj de la efodo per <tld/>o el
        blua &scirc;tofo
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 28:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        bela estas <klr>[&#x2026;]</klr> via kolo sub la <tld/>oj de perloj
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 1:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        iufoje <tld/>o da koloraj koraletoj &ccirc;irka&ubreve;is la kolon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la patrino sendis la knabinojn en la urbon por a&ccirc;eti fadenojn, kudrilojn,
        <tld/>ojn kaj rubandojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino kaj Rozulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        enkuris <klr>[&#x2026;]</klr> dekdujara knabineto, <klr>[&#x2026;]</klr> tiranta kun si
        longan puncan <tld/>on, kun kiu &scirc;i anta&ubreve; momento ver&scirc;ajne faradis
        gimnastikajn ekzercojn
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        tirilo de sonorilo <klr>[estis]</klr> bona dika <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Bona humoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi e&ccirc; ne povas atingi viajn <ind>&scirc;u<tld/>o</ind>jn, kiam vi
        klini&gcirc;as
        <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;apitro 4a</lok></fnt>.
        <trd lng="ca">cord&oacute; de les sabates</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="galon.0o">galono</ref>,
      <ref cel="kordon.0o.TEKS">kordono<sncref/></ref>,
      <ref cel="ruband.0o.ornamo">rubando<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_n;&c_u;&c_r;</trd>
  <trd lng="bo">&t_ja;&t_shabchu;&t_ra;&t_tsheg;&t_sa;&t_gata;&t_rata;&t_naro;&t_ga;&t_tsheg;&t_ra;&t_gata;&t_yata;&t_ga;&t_tsheg;</trd>
  <trd lng="br">las</trd>
  <trd lng="ca">cord&oacute;</trd>
  <trdgrp lng="de">
    <trd>Schn&uuml;rband</trd>,
    <trd>Schn&uuml;rriemen</trd>,
    <trd>Schn&uuml;rsenkel</trd>,
  </trdgrp>
  <trd lng="en">lace</trd>
  <trdgrp lng="es">
    <trd>cinta</trd>,
    <trd>cord&oacute;n</trd>
  </trdgrp>
  <trd lng="fr">lacet</trd>
  <trdgrp lng="hu">
     <trd>f&udblac;z&odblac;</trd>,
     <trd>cip&odblac;f&udblac;z&odblac;</trd>,
     <trd>ruhaf&udblac;z&odblac;</trd>,
     <trd>f&udblac;z&odblac;zsin&oacute;r</trd>
  </trdgrp>
  <trd lng="id"><ind>tali</ind><klr tip="amb"> (sepatu)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x3072;&#x3082;</trd>,
    <trd>&#x7DE8;&#x307F;&#x3072;&#x3082;  <pr>&#x3042;&#x307F;&#x3072;&#x3082;</pr></trd>,
    <trd>&#x7F60;  <pr>&#x308F;&#x306A;</pr></trd>
  </trdgrp>
  <trd lng="nl">veter</trd>
  <trd lng="pl">sznur&oacute;wka</trd>
  <trdgrp lng="pt">
     <trd>cadar&ccedil;o</trd>,
     <trd>cord&atilde;o</trd>,
     <trd>atilho</trd>,
       </trdgrp>
  <trd lng="ru">&c_sh;&c_n;&c_u;&c_r;&c_o;&c_k;</trd>
  <trdgrp lng="uk">
    <trd>&c_sh;&#x43D;&c_u;&c_r;</trd>,
    <trd>&c_sh;&#x43D;&c_u;&c_r;&#x43E;&#x43A;</trd>,
    <trd>&c_sh;&#x43D;&c_u;&c_r;&c_ib;&c_v;&#x43A;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x978B;&#x5E26; <pr>xi&#xE9;d&#xE0;i</pr></trd>,
    <trd>&#x978B;&#x5E36; <pr>xi&#xE9;d&#xE0;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lacx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="lacx.0i.TEKS">
    <dif>
      <ref tip="super" cel="lig.kun0i">Kunligi</ref> per <tld/>o:
      <ekz>
        <tld/>i &scirc;uojn;
      </ekz>
      <ekz>
        rimarkinte, ke &scirc;i estas tro forte <tld/>ita, tratran&ccirc;is la <tld/>on, &scirc;i komencis
        spiri, kaj iom post iom revenis la vivo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        estas nun modo porti &ccirc;e la pantalonoj bukojn, per kiuj oni povas la&ubreve;vole <tld/>i
        ilin pli vaste a&ubreve; pli malvaste
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        pro iaj kelkaj miloj da favaj dukatoj <klr>[&#x2026;]</klr> mi <klr>[ne]</klr> <tld/>u
        mian volon per le&gcirc;oj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>!
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lig.0i.malhelpimovon">ligi<sncref/></ref>,
      <ref cel="plekt.0i">plekti</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_n;&c_u;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="br">lasa&ntilde;</trd>
  <trdgrp lng="ca">
    <trd>lligar</trd>,
    <trd>nuar</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schn&uuml;ren</trd>,
    <trd>zuschn&uuml;ren</trd>,
    <trd>einschn&uuml;ren</trd>,
  </trdgrp>
  <trd lng="en">lace</trd>
  <trdgrp lng="es">
    <trd>anudar</trd>,
    <trd>enlazar</trd>,
    <trd>atar</trd>
  </trdgrp>
  <trd lng="fr">lacer</trd>
  <trdgrp lng="hu">
    <trd>&ouml;sszef&udblac;z</trd>,
    <trd>&ouml;sszezsin&oacute;roz</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#xFF5E;<ind>&#x306E;&#x3072;&#x3082;&#x3092;&#x7D50;&#x3076;  </ind><pr>&#x306E;&#x3072;&#x3082;&#x3092;&#x3080;&#x3059;&#x3076;</pr></trd>,
    <trd>&#x3072;&#x3082;&#x3067;&#x7DE0;&#x3081;&#x308B;  <pr>&#x3072;&#x3082;&#x3067;&#x3057;&#x3081;&#x308B;</pr></trd>,
    <trd>&#x675F;&#x7E1B;&#x3059;&#x308B;  <pr>&#x305D;&#x304F;&#x3070;&#x304F;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">rijgen<klr> (met een veter)</klr></trd>
  <trdgrp lng="pl">
    <trd>sznurowa&cacute; <klr>(buty)</klr></trd>,
    <trd>zawi&aogonek;za&cacute; <klr>(buty)</klr></trd>
  </trdgrp>
  <trd lng="pt">amarrar</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_n;&c_u;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_v;&c_ja;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_v; &c_p;&c_je;&c_r;&c_je;&c_n;&c_o;&c_s;&c_n;&c_o;&c_m; &c_s;&c_m;&c_y;&c_s;&c_l;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="uk">&c_sh;&#x43D;&c_u;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x624E; <pr>z&#x101;</pr></trd>,
    <trd>&#x7D2E; <pr>z&#x101;</pr></trd>,
    <trd>&#x7E6F; <pr>hu&#xE1;n</pr></trd>,
    <trd>&#x7F33; <pr>hu&#xE1;n</pr></trd>
  </trdgrp>
</drv>
</art>
