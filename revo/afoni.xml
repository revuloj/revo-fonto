<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: afoni.xml,v 1.20 2022/03/11 12:50:04 revo Exp $">
<kap>
  <rad>afoni</rad>/o
</kap>
<drv mrk="afoni.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref cel="vocx.sen0a">Senvo&ccirc;eco</ref>:
      <ekz>
        la <tld/>o povas havi multan ka&ubreve;zojn, sed &gcirc;i &ccirc;efe estas ka&ubreve;zita de tra&ubreve;mo de la
        laringo, anka&ubreve; la timo povas fari <tld/>on
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=471182">Afonio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_f;&c_o;&c_n;&c_ib;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_t;&c_a; &c_g;&c_o;&c_l;&c_a;&c_s;&c_u;</trd>
  </trdgrp>
  <trd lng="ca">afonia</trd>
  <trdgrp lng="cs">
    <trd>afonie</trd>,
    <trd>bezhlasnost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Verlust der Stimme</trd>,
    <trd>Stimmverlust</trd>,
    <trd>Stimmlosigkeit</trd>,
    <trd>Aphonie</trd>
  </trdgrp>
  <trd lng="en">aphonia</trd>
  <trd lng="es">afon&iacute;a</trd>
  <trd lng="fr">aphonie</trd>
  <trdgrp lng="hu">
     <trd>af&oacute;nia</trd>,
     <trd>hangveszt&eacute;s</trd>,
     <trd>hangtalans&aacute;g</trd>
  </trdgrp>
  <trd lng="it">afonia</trd>
  <trd lng="nl">sprakeloosheid</trd>
  <trd lng="pt">afonia</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_f;&c_o;&c_n;&c_i;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_t;&c_je;&c_r;&c_ja; &c_g;&c_o;&c_l;&c_o;&c_s;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>af&oacute;nia</trd>,
    <trd>bezhlasnos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x430;&#x444;&#x43E;&#x43D;&#x456;&#x44F;</trd>,
    <trd>&#x443;&#x442;&#x440;&#x430;&#x442;&#x430; &#x433;&#x43E;&#x43B;&#x43E;&#x441;&#x443;</trd>
  </trdgrp>
</drv>
</art>
