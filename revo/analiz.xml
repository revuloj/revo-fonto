<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>1</ofc>
      <rad>analiz</rad>/i
    </kap>

    <drv mrk="analiz.0i">
      <kap><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Diserigi tuta&jcirc;on, por pli bone esplori ties erojn:
          <ekz>
            <tld/>i &hcirc;emian solva&jcirc;on, frazon, libron.
          </ekz>
        </dif>
      </snc>
      <ref tip="ant" cel="sintez.0o">sintezo</ref>
      <trd lng="be">&c_a;&c_n;&c_a;&c_l;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
      <trdgrp lng="de">
        <trd>analysieren</trd>,
        <trd>zergliedern</trd>,
        <trd>zerlegen</trd></trdgrp>
      <trd lng="en">analyse</trd>
      <trd lng="es">analisar</trd>
      <trd lng="fr">analyser</trd>
      <trd lng="nl">analyseren</trd>
      <trd lng="it">analizzare</trd>
      <trd lng="ru">&c_a;&c_n;&c_a;&c_l;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
      <trd lng="sk">analyzova&tcaron;</trd>
      <trd lng="sv">analysera</trd>
    </drv>

    <drv mrk="analiz.0a">
      <kap><tld/>a</kap>
      <snc mrk="analiz.0a.KOMUNE">
	<dif>
	  Rilata al <tld/>o:
	  <ekz>
	    &leftquot;Plena <tld/>a gramatiko de Esperanto&rightquot;.
	  </ekz>
	</dif>
	<trd lng="ru">&c_a;&c_n;&c_a;&c_l;&c_i;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
      </snc>

      <snc mrk="analiz.0a.LIN">
	<uzo tip="fak">LIN</uzo>
	<dif>
	  Uzanta apartan vorton por gramatika funkcio; esprimanta
	  <ref cel="fleksi.0o.paradigmo">fleksiojn</ref> plurvorte:
	  <ekz>
	    la pasivo en Esperanto estas esprimata per
	    <ind><tld/>a formo</ind>;
	    <trd lng="ru">&c_s;&c_l;&c_o;&c_zh;&c_n;&c_a;&c_ja; &c_f;&c_o;&c_r;&c_m;&c_a;</trd>
	  </ekz><ekz>
	    <ind><tld/>(em)a lingvo</ind>
	    <klr>(kontraste al <ref tip="ant"
		cel="fand.kun0ema.lingvo">kunfandema lingvo</ref>)</klr>;
	    <trd lng="ru">&c_a;&c_n;&c_a;&c_l;&c_i;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; &c_ja;&c_z;&c_y;&c_k;</trd>
	  </ekz><ekz>
	    dativon Esperanto esprimas per
	    <ind><tld/>a kazo</ind>: <em>al + nomo</em>;
	    <trd lng="ru">&c_a;&c_n;&c_a;&c_l;&c_i;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; &c_p;&c_a;&c_d;&c_je;&c_zh;</trd>
	  </ekz><ekz>
	    <tld/>a verbotempo, superlativo, komparativo.
	  </ekz>
	</dif>
	<trdgrp lng="ru">
	  <trd>&c_a;&c_n;&c_a;&c_l;&c_i;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
	  <trd>&c_s;&c_l;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>
	</trdgrp>
      </snc>
    </drv>

    <drv mrk="analiz.0ilo">
      <kap><tld/>ilo</kap>
      <snc mrk="analiz.0ilo.KOMP">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Programa a&ubreve; aparata rimedo por trovi atributojn a&ubreve;
          komponantojn de datumo:
          <ekz>
            leksika analizilo,
          </ekz><ekz>
            <ind>sintaksa analizilo</ind>.
            <trd lng="en">parser</trd>
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_a;&c_n;&c_a;&c_l;&c_ib;&c_z;&c_a;&c_t;&c_a;&c_r;</trd>
      <trd lng="en">analyser</trd>
      <trd lng="nl">analysator</trd>
      <trd lng="ru">&c_a;&c_n;&c_a;&c_l;&c_i;&c_z;&c_a;&c_t;&c_o;&c_r;</trd>
    </drv>
  </art>
