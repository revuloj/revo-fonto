<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kanjon.xml,v 1.21 2022/03/21 17:51:56 revo Exp $">
<kap>
  <rad>kanjon</rad>/o
</kap>

<drv mrk="kanjon.0o">
  <kap><tld/>o</kap>
  <snc mrk="kanjon.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Vasta kaj profunda <ref tip="super" cel="val.0o.GEOG">valo</ref>,
      kies bordoj estas krutegaj:
      <ekz>
        li a&ubreve;dacis deklari, ke la <tld/>o de Kolorado estas pli bela ol
        la
        valo de la Tajo en Hispanio
        <fnt><bib>SatirRak</bib>, <lok>la guarbo</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fauxk.0o">fa&ubreve;ko</ref>,
      <ref cel="ravin.0o">ravino</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_mol;&c_jo;&c_n;</trd>
  <trd lng="ca">congost, engorjat</trd>
  <trd lng="cs">ka&ncaron;on</trd>
  <trd lng="de">Canyon</trd>
  <trd lng="en">canyon</trd>
  <trd lng="es">ca&ntilde;on <klr>(geog.)</klr></trd>
  <trdgrp lng="fr">
    <trd>canyon</trd>,
    <trd>ca&ntilde;on</trd>,
    <trd>gorges</trd>
  </trdgrp>
  <trd lng="hu">kanyon</trd>
  <trdgrp lng="nl">
    <trd>canyon</trd>,
    <trd>ca&ntilde;on</trd>,
    <trd>ravijn</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kanion</trd>,
    <trd>prze&lstroke;om <klr>(rzeki)</klr></trd>
  </trdgrp>   
  <trd lng="ro">defileu</trd>
  <trd lng="ru">&c_k;&c_a;&c_n;&c_mol;&c_o;&c_n;</trd>
  <trd lng="sk">ka&ncaron;on</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x430;&#x43D;&#x44C;&#x439;&#x43E;&#x43D;</trd>,
    <trd>&#x433;&#x43B;&#x438;&#x431;&#x43E;&#x43A;&#x430; &#x443;&#x449;&#x435;&#x43B;&#x438;&#x43D;&#x430;</trd>
  </trdgrp>
</drv>
</art>
