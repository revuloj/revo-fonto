<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: alburn1.xml,v 1.19 2023/10/12 18:05:09 revo Exp $">
<kap>
  <rad>alburn</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="alburn1.0o">
  <kap><tld/>o</kap>
  <snc mrk="alburn1.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Eta <ref tip="lst" cel="fisx.0o" lst="voko:fi&scirc;oj">riverfi&scirc;o</ref> 
      el e&ubreve;ropaj riveroj, <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref>
      el familio 
      <ref tip="malprt" cel="ciprin1.0oj">ciprinedoj</ref> 
      (<trd lng="la">Alburnus</trd>) precipe la komuna specio 
      <trd lng="la">Alburnus alburnus</trd>:
      <ekz>
        du stratbuboj kaj unu gimnaziano, kiuj sidis sur la pontokestoj 
        kaj fi&scirc;is <tld/>ojn, ekvidis lin 
        <fnt>
        <aut>Selma Lagerl&ouml;f, trad. S. Engholm</aut>,
        <vrk>G&ouml;sta Berling</vrk>,
        <lok>&ccirc;apitro 33a</lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;atentu, pa&ccirc;jo,&rightquot; diris la filo, &leftquot;fi&scirc;o kapti&gcirc;as&rightquot;,
        &leftquot;&gcirc;i estas nur <tld/>o,&rightquot; murmuris la maljuna sinjoro
        <fnt>
          <bib>Salamandroj</bib>,
          <lok>libro 3a, &ccirc;apitro 10a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    <ref tip="hom" cel="alburn.0o">Alburno</ref>
    estas anka&ubreve; speco de ligno.
  </rim>
  <trd lng="be">&c_v;&c_je;&c_r;&c_h;&c_a;&c_v;&c_o;&c_d;&c_k;&c_a;</trd>
  <trd lng="bg">&c_u;&c_k;&c_l;&c_je;&c_j;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>b&ecaron;lice</trd>,
    <trd>ouklej</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ukelei</trd>,
    <trd>Laube <klr>(Fisch)</klr></trd>
  </trdgrp>
  <trd lng="es">alburno</trd>
  <trd lng="fr">ablette</trd>
  <trd lng="he">&#x5E2;&#x5E6;&#x5D4; (&#x5E8;&#x5E7;&#x5DE;&#x5EA; &#x5E2;&#x5E5;)</trd>
  <trdgrp lng="hu">
    <trd>sz&eacute;lhajt&oacute; <ind>k&uuml;sz</ind></trd>,
    <trd>&ouml;kle</trd>,
    <trd>sz&eacute;lkeszeg</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x767D;&#x592A;  <pr>&#x3057;&#x3089;&#x305F;</pr></trd>,
    <trd>&#x8FBA;&#x6750;  <pr>&#x3078;&#x3093;&#x3056;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="mk">&c_p;&c_l;&c_a;&c_sh;&c_i;&c_c;&c_a;</trd>
  <trdgrp lng="nl">
    <trd>witvis</trd>,
    <trd>alver</trd>
  </trdgrp>
  <trd lng="pl">ukleja</trd>
  <trdgrp lng="pt">
    <trd>alburno <klr tip="amb">(zool.)</klr></trd>,
    <trd>alburnete <klr tip="amb">(zool.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_k;&c_l;&c_je;&c_j;&c_k;&c_a;</trd>
  <trd lng="sk">belica</trd>
  <trd lng="sr">&c_k;&c_je;&c_d;&c_je;&c_r;</trd>
  <trd lng="sv">l&ouml;ja</trd>
  <trd lng="uk">&c_v;&c_je;&c_r;&c_h;&c_o;&c_v;&c_o;&c_d;&c_k;&c_a;</trd>
</drv>
</art>

