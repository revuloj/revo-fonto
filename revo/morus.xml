<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>morus</rad>/o
</kap>
<drv mrk="morus.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Man&gcirc;ebla frukto de specio de <tld/>ujo
      (<trd lng="la">Morus nigra</trd>), simila al violenigra frambo.
    </dif>
  </snc>
  <trd lng="de">Maulbeere</trd>
  <trd lng="en">mulberry</trd>
  <trd lng="tr">dut</trd>
</drv>
<drv mrk="morus.0ujo">
  <kap><tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="morus.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Morus</trd>). La folioj de la blanka <tld/>o
      (<trd lng="la">Morus alba</trd>) servas por nutri la silkra&ubreve;pojn.
    </dif>
  </snc>
  <trd lng="de">Maulbeerbaum</trd>
  <trd lng="en">mulberry tree</trd> <trd lng="tr">dut</trd>
</drv>
<drv mrk="morus.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Moraceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Maulbeergew&auml;chse</trd>
</drv>
</art>
