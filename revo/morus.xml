<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: morus.xml,v 1.28 2025/06/11 20:48:29 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>morus</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="morus.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="morus.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;ebla <ref tip="super" cel="frukt.0o.KUI">frukto</ref> de
      specio de <tld/>ujo
      (<trd lng="la">Morus nigra</trd>),
      simila al violenigra frambo:
      <ekz>
         li prenis <tld/>on, man&gcirc;is &gcirc;in kaj grimacis &leftquot;ili estas nematuraj&rightquot;
        <fnt>
          <aut>E. Turner, trad. R. McGrath</aut>
          <vrk><url ref="https://eduinf.waw.pl/esp/lern/librejo/0144/0001.php">Sep Junaj
          A&ubreve;stralianoj</url></vrk>
          <lok>1994</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_t;&c_a;&c_v;&c_a;&c_ja; &c_ja;&c_g;&c_a;&c_d;&c_a;</trd>
  <trd lng="ca">m&oacute;ra</trd>
  <trd lng="cs">moru&scaron;ovn&iacute;k</trd>
  <trd lng="de">Maulbeere</trd>
  <trd lng="en">mulberry</trd>
  <trdgrp lng="fa">
    <trd>&a_t;&a_w;&a_t;</trd>,
    <trd>&a_sh;&a_A;&a_h;&zwnj;&a_t;&a_w;&a_t;</trd>
  </trdgrp>
  <trd lng="fr">mure<klr> (du murier)</klr></trd>
  <trd lng="hu">eper</trd>
  <trd lng="ja">&#26705;&#12398;&#23455;  <pr>&#12367;&#12431;&#12398;&#12415;</pr></trd>
  <trd lng="nl">moerbei</trd>
  <trd lng="pl">morwa</trd>
  <trd lng="ru">&c_t;&c_u;&c_t;&c_o;&c_v;&c_a;&c_ja; &c_ja;&c_g;&c_o;&c_d;&c_a;</trd>
  <trd lng="sk">moru&scaron;a <klr>(strom)</klr></trd>
  <trd lng="tr">dut</trd>
  <trdgrp lng="uk">
    <trd>&c_sh;&c_o;&c_v;&c_k;&c_o;&c_v;&c_i;&c_c;&c_ja;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#26705;&#33882; <pr>s&amacron;ngsh&egrave;n</pr></trd>,
    <trd>&#26705;&#26937; <pr>s&amacron;ngsh&egrave;n</pr></trd>,
    <trd>&#26705;&#33882; <pr>s&amacron;ngren</pr></trd>
  </trdgrp>
</drv>

<drv mrk="morus.0ujo">
  <kap><tld/>ujo, <var><kap><tld/>arbo</kap></var></kap>
  <snc mrk="morus.0ujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="morusa.0oj"><tld/>acoj</ref> (<trd lng="la">Morus</trd>);
      la foliojn de la blanka <tld/>o (<trd lng="la">Morus alba</trd>) man&gcirc;as <ref cel="rauxp.silk0o">silkra&ubreve;po</ref>j:
      <ekz>
        sur la supro de la <tld/>arbo
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 5:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        amiko mia donacis dek silkra&ubreve;pojn, kiujn mi metis en &scirc;uskatolon kaj nutris per
        folioj de <tld/>ujo
        <fnt>
          <bib>Kon11</bib>
          <lok>Blogo de finvenkisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        jarcenta&gcirc;aj <tld/>arboj gardis la &scirc;tonege
        pavimitan vojon
        <fnt>
          <aut>S. Lang, trad. G. Berveling</aut>,
          <vrk>La Talpo&ccirc;asisto</vrk>,
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/3/3b/Illustration_Morus_nigra0.jpg/514px-Illustration_Morus_nigra0.jpg"
      prm="PD"
      >botanika tabulo de Nigra <tld/>ujo
        <fnt>
          <lok><url ref="&WCW;/File:Illustration_Morus_nigra0.jpg"
              >https://commons.wikimedia.org/wiki/File:Illustration_Morus_nigra0.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_u;&c_t;&c_a;&c_v;&c_a;&c_je; &c_d;&c_r;&c_e;&c_v;&c_a;</trd>,
    <trd>&c_sh;&c_a;&c_w;&c_k;&c_o;&c_w;&c_n;&c_ib;&c_c;&c_a;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">morera</trd>
  <trd lng="cs">moru&scaron;ovn&iacute;k</trd>
  <trd lng="de">Maulbeerbaum</trd>
  <trd lng="en">mulberry tree</trd>
  <trd lng="fa">&a_d;&a_r;&a_kh;&a_t; <ind>&a_t;&a_w;&a_t;</ind></trd>
  <trd lng="fr">murier</trd>
  <trd lng="hu">eperfa</trd>
  <trd lng="ja">&#12463;&#12527;</trd>
  <trd lng="nl">moerbeiboom</trd>
  <trd lng="pl">morwa</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_u;&c_t;&c_o;&c_v;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>,
    <trd>&c_t;&c_u;&c_t;&c_a;</trd>,
    <trd>&c_sh;&c_je;&c_l;&c_k;&c_o;&c_v;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">moru&scaron;a <klr>(strom)</klr></trd>
  <trd lng="tr">dut</trd>
  <trdgrp lng="zh">
    <trd>&#26705; <pr>s&amacron;ng</pr></trd>,
    <trd>&#26705;&#26641; <pr>s&amacron;ngsh&ugrave;</pr></trd>,
    <trd>&#26705;&#27193; <pr>s&amacron;ngsh&ugrave;</pr></trd>
  </trdgrp>
</drv>
</art>
