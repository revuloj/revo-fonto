<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>morus</rad>/o
</kap>
<drv mrk="morus.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Man&gcirc;ebla frukto de specio de <tld/>ujo
      (<trd lng="la">Morus nigra</trd>), simila al violenigra frambo.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_t;&c_a;&c_v;&c_a;&c_ja; &c_ja;&c_g;&c_a;&c_d;&c_a;</trd>
  <trd lng="de">Maulbeere</trd>
  <trd lng="en">mulberry</trd>
  <trd lng="hu">eper</trd>
  <trd lng="nl">moerbei</trd>
  <trd lng="pl">morwa</trd>
  <trd lng="ru">&c_t;&c_u;&c_t;&c_o;&c_v;&c_a;&c_ja;
&c_ja;&c_g;&c_o;&c_d;&c_a;</trd>
  <trd lng="tr">dut</trd>
</drv>

<drv mrk="morus.0ujo">
  <kap><tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="morusa.0oj"><tld/>acoj</ref>
      (<trd lng="la">Morus</trd>). La folioj de la blanka <tld/>o
      (<trd lng="la">Morus alba</trd>) servas por nutri la
silkra&ubreve;pojn.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_u;&c_t;&c_a;&c_v;&c_a;&c_je; &c_d;&c_r;&c_e;&c_v;&c_a;</trd>,
    <trd>&c_sh;&c_a;&c_w;&c_k;&c_o;&c_w;&c_n;&c_ib;&c_c;&c_a;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Maulbeerbaum</trd>
  <trd lng="en">mulberry tree</trd>
  <trd lng="hu">eperfa</trd>
  <trd lng="nl">moerbeiboom</trd>
  <trd lng="pl">morwa</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_u;&c_t;&c_o;&c_v;&c_o;&c_je;
&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>,
    <trd>&c_t;&c_u;&c_t;&c_a;</trd>,
    <trd>&c_sh;&c_je;&c_l;&c_k;&c_o;&c_v;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="tr">dut</trd>
</drv>
</art>
