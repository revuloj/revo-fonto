    <trd>&c_s;&c_a;&c_m;&c_o;&c_n;&c_a;&c_d;&c_ib;&c_ja;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="cert.tut0e">
  <kap>
    tut<tld/>e
  </kap>
  <snc>
    <dif>
      Absolute <tld/>e:
      <ekz>
        tutcerte! diris Kakambo, <klr>[&#x2026;]</klr> mi konas la gubernion
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        se ne ekzistus la Centro de Esploro kaj Dokumentado <klr>[&#x2026;]</klr> la nuna
        impona volumo &ndash; plia rezulto de multjara laboro &ndash; tut<tld/>e ne povus aperi
        <fnt>
          <bib>EeP</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        progresado, tut<tld/>e, ne &ccirc;iam estas egale rapida
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li havas onklon en Kievo? tut<tld/>e, nenia gazeto raportis tion
        <fnt>
          <bib>MkM</bib>
          <lok>3</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="nepr.0e">nepre</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_s;&c_a;&c_l;&c_ju;&c_t;&c_n;&c_a; <ind>&c_p;&c_e;&c_w;&c_n;&c_a;</ind></trd>
  <trdgrp lng="de">
    <trd>ganz <ind>gewiss</ind></trd>,
    <trd>todsicher</trd>
  </trdgrp>
<trdgrp lng="en">
  <trd>most certainly</trd>,
  <trd>most definitely</trd>,
  <trd>doubtless</trd>,
  <trd>no doubt</trd>,
  <trd>without a doubt</trd>
</trdgrp>
  <trdgrp lng="ja">
    <trd>&#x307E;&#x3063;&#x305F;&#x304F;&#x3082;&#x3063;&#x3066;</trd>,
    <trd>&#x304D;&#x3063;&#x3068;&#x78BA;&#x304B;&#x306B;  <pr>&#x304D;&#x3063;&#x3068;&#x305F;&#x3057;&#x304B;&#x306B;</pr></trd>
