  <trdgrp lng="sk">
    <trd>ist&yacute;</trd>,
    <trd>ur&ccaron;it&yacute;</trd>
  </trdgrp>
  <trd lng="sq">sigurt</trd>
  <trd lng="st">itseng</trd>
  <trd lng="ta">&#xBA8;&#xBBF;&#xB9A;&#xBCD;&#xB9A;&#xBAF;&#xBAE;&#xBBE;&#xBA9;</trd>
  <trd lng="te">&#xC15;&#xC4A;&#xC28;&#xC4D;&#xC28;&#xC3F;</trd>
  <trd lng="th">&#xE1A;&#xE32;&#xE07;&#xE2D;&#xE22;&#xE48;&#xE32;&#xE07;</trd>
