  </trdgrp>
  <trd lng="it">certezza</trd>
  <trdgrp lng="ja">
    <trd>&#x78BA;&#x304B;&#x3055;  <pr>&#x305F;&#x3057;&#x304B;&#x3055;</pr></trd>,
    <trd>&#x78BA;&#x4FE1;  <pr>&#x304B;&#x304F;&#x3057;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="sk">istota</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&c_p;&c_je;&c_v;&c_n;&c_je;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_p;&c_je;&c_r;&c_je;&c_ch;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_s;&c_u;&c_m;&c_n;&c_ib;&c_v;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x628A;&#x63E1; <pr>b&#x1CE;w&#xF2;</pr></trd>,
    <trd>&#x5B89;&#x5168;&#x611F; <pr>&#x101;nqu&#xE1;ng&#x1CE;n</pr></trd>
  </trdgrp>
