<drv mrk="cert.mal0a">
  <kap>mal<tld/>a, <var><kap>ne<tld/>a</kap></var></kap>
  <snc mrk="cert.mal0a.dubanta">
    <dif>
      Dubanta, nefirma:
      <ekz>
        mal<tld/>a pri la morga&ubreve;o
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LXXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        min kaptis stranga, mal<tld/>a sento
        <fnt>
          <bib>VaK</bib>
          <lok>Dua &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        sendecida, mal<tld/>a pri si
        <fnt>
          <bib>BonaLingvo</bib>
          <lok>4. Emo Difini, Emo Elvoki</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[paroli]</klr> per mal<tld/>a vo&ccirc;o
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ar se la trumpeto donos ne<tld/>an vo&ccirc;on,
        kiu preti&gcirc;os por batalo
        <fnt><bib>MT</bib>, <lok>&Ko1; 14:8</lok></fnt>?
      </ekz>
    </dif>
    <trd lng="be">&c_n;&c_ja;&c_p;&c_e;&c_w;&c_n;&c_y;</trd>
    <trd lng="ca">indec&iacute;s</trd>
    <trd lng="es">indeciso</trd>
    <trdgrp lng="fr">
      <trd>incertain<klr tip="ind"> (irr&eacute;solu)</klr></trd>,
      <trd>ind&eacute;cis<klr tip="ind"> (irr&eacute;solu)</klr></trd>,
      <trd>irr&eacute;solu</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>bizonytalan</trd>,
      <trd>k&eacute;telked&odblac;</trd>
    </trdgrp>
    <trd lng="id">tidak <ind>yakin</ind></trd>
    <trd lng="no">usikker</trd>
    <trd lng="ru">
    &c_n;&c_je;&c_u;&c_v;&c_je;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="cert.mal0a.dubebla">
    <dif>
      Dubebla, <ref cel="dub.0a">duba</ref>:
      <ekz>
        la le&gcirc;eco de lia faro ne estas por mi mal<tld/>a
        <fnt>
          <bib>DL</bib>
          <lok>Dua Libro de l' Lingvo Internacia</lok>
        </fnt>;
      </ekz>
      <ekz>
        li vidis ruinojn de la malnova <nom>Ilion</nom>, sed <klr>[&#x2026;]</klr> povis doni nur tre
        mal<tld/>ajn informojn pri la lokoj
        <fnt>
          <bib>VRA</bib>
          <lok>Eyvind Johnson</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni devas kontroli mal<tld/>ajn personojn
        <fnt>
          <aut>Michel Marko</aut>
            <vrk><url ref="&Monato;/2000/007051.php">La stranga grupo</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 29</lok>
          </fnt>;
      </ekz>
      <ekz>
        &scirc;i denove sidas en mal<tld/>a stato
        <fnt>
          <bib>Monato</bib>
          <aut>Probal Da&scirc;gupto</aut>
          <vrk><url ref="&Monato;/2003/007815.html">Verki unu por la aliaj</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        esperon plibonigi vian sorton per laboro vi povas havi nur <klr>[&#x2026;]</klr> tre
        ne<tld/>an
        <fnt>
          <bib>Marta</bib>
        </fnt>!
      </ekz>
      <ekz>
        la preciza naturo de la objekto estis mal<tld/>a
        <fnt>
          <bib>Monato</bib>
          <aut>Oliver Kim</aut>
          <vrk><url ref="&Monato;/2006/008796.php">Unua fotografa&jcirc;o de ekstersunsistema
          planedo</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili apogu sian esperon ne sur la <ind>mal<tld/>eco</ind>n de la
        ri&ccirc;o, sed sur Dion, kiu donas al ni ri&ccirc;e &ccirc;ion
        por &gcirc;uado
        <fnt><bib>NT</bib>, <lok>&Ti1; 6:17</lok></fnt>;
        <trd lng="ca">incertesa</trd>
        <trd lng="es">incertidumbre</trd>
        <trd lng="fr">incertitude</trd>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_ja;&c_p;&c_e;&c_w;&c_n;&c_y;</trd>,
      <trd>&c_n;&c_je;&c_v;&c_je;&c_r;&c_a;&c_g;&c_o;&c_d;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="ca">incert</trd>
    <trd lng="es">incierto</trd>
    <trdgrp lng="fr">
      <trd>douteux</trd>,
      <trd>incertain<klr tip="ind"> (douteux)</klr></trd>,
      <trd>ind&eacute;cis<klr tip="ind"> (douteux)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>bizonytalan</trd>,
      <trd>k&eacute;ts&eacute;ges</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>ragu</trd>,
      <trd>bimbang</trd>,
      <trd>sangsi</trd>
    </trdgrp>
    <trdgrp lng="no">
      <trd>usikker</trd>,
      <trd>uviss</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_n;&c_je;&c_o;&c_p;&c_r;&c_je;&c_d;&c_je;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_d;&c_o;&c_s;&c_t;&c_o;&c_v;&c_je;&c_r;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">nejist&yacute;</trd>
  <trdgrp lng="de">
    <trd>unsicher</trd>, <trd>ungewiss</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>uncertain</trd>,
    <trd>unsure</trd>,
    <trd>doubtful</trd>
  </trdgrp>
  <trdgrp lng="it">
  <trd>dubbio<klr> (agg.)</klr></trd>,
  <trd>incerto</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4E0D;&#x78BA;&#x304B;&#x306A;  <pr>&#x3075;&#x305F;&#x3057;&#x304B;&#x306A;</pr></trd>,
    <trd>&#x81EA;&#x4FE1;&#x306E;&#x306A;&#x3044;  <pr>&#x3058;&#x3057;&#x3093;&#x306E;&#x306A;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">onzeker</trd>
  <trdgrp lng="pl">
    <trd>w&aogonek;tpliwy</trd>,
    <trd>niepewny</trd>
  </trdgrp>
  <trd lng="pt">incerto</trd>
  <trd lng="sk">neist&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_n;&c_je;&c_p;&c_je;&c_v;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_j;&c_m;&c_o;&c_v;&c_ib;&c_r;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_r;&c_a;&c_v;&c_d;&c_o;&c_p;&c_o;&c_d;&c_ib;&c_b;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x832B;&#x7136; <pr>m&#xE1;ngr&#xE1;n</pr></trd>,
    <trd>&#x60D9; <pr>chu&#xF2;</pr></trd>,
    <trd>&#x672A;&#x5FC5; <pr>w&#xE8;ib&#xEC;</pr></trd>,
