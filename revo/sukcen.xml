<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sukcen</rad>/o
</kap>
<drv mrk="sukcen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Malmola flava diafana fosilia rezino, el kiu oni faras diversajn
      objektojn, precipe enbu&scirc;a&jcirc;ojn de pipoj.
    </dif>
  </snc>
  <trd lng="en">amber</trd>
  <trd lng="be">&c_b;&c_u;&c_r;&c_sh;&c_t;&c_y;&c_n;</trd>
  <trdgrp lng="de">
    <trd>Bernstein</trd>,
    <trd>gelber <ind>Amber</ind></trd>
  </trdgrp>
  <trd lng="fa">&f_k;&a_h;&a_r;&a_b;&a_A;</trd>
  <trd lng="hu">borosty&aacute;nk&odblac;</trd>
  <trd lng="nl">barnsteen</trd>
  <trd lng="ru">&c_ja;&c_n;&c_t;&c_a;&c_r;&c_mol;</trd>
  <trd lng="ca">ambre</trd>
</drv>
</art>
