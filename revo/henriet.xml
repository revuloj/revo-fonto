<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: henriet.xml,v 1.1 2017/12/12 12:10:13 revo Exp $">
<kap>
  <ofc>*</ofc><ofc>7</ofc>
    <rad>Henriet</rad>/o <fnt><bib></bib></fnt>
</kap>
<drv mrk="henriet.0o">
  <kap>
    <ofc>*</ofc>
    <tld />o
  </kap>
  <snc mrk="henriet.0o.SNC">
  <uzo tip="fak">NOM</uzo>
    <dif>
      Virina <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> germandevena:
      <ekz>
        -nj- = diminutive of female names; e. g. Henriet' = Henrietta &dash;
        Henri'nj', He'nj' = Hetty
        <fnt><bib>F</bib>, <lok>Universala Vortaro: "-nj-"</lok></fnt>
      </ekz>
    </dif>
    <ref tip="vid" cel="henrik.0o">Henriko</ref>
  </snc>
  <trd lng="en">Henrietta</trd>
  <trd lng="es">Enriqueta</trd>
  <trd lng="fr">Henriette</trd>
  <trd lng="it">Enrica</trd>
  <trd lng="nl">Hendrika</trd>
  <trd lng="no">Henrikke</trd>
  <trd lng="pt">Henriqueta</trd>
  <trd lng="sv">Henrika</trd>
</drv>

<drv mrk="henriet.0njo">
  <kap>
   <ofc>*</ofc>Henrinjo, <var><kap>Henjo</kap></var>
  </kap>
  <snc mrk="henriet.0njo.SNC">
    <dif>
      Karesformo de <ref tip="lst" cel="henriet.0o"
      lst="voko:personaj_nomoj">Henrieto</ref>:
      <ekz>
        -nj- = diminutive of female names; e. g. Henriet' = Henrietta &dash;
        Henri'nj', He'nj' = Hetty
        <fnt><bib>F</bib>, <lok>Universala Vortaro: "-nj-"</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="en">Hetty</trd>
  <trd lng="fr">Riette</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>