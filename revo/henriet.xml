<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: henriet.xml,v 1.2 2021/02/19 20:37:11 revo Exp $">
<kap>
  <ofc>*</ofc><ofc>7</ofc>
    <rad>Henriet</rad>/o <fnt><bib/></fnt>
</kap>
<drv mrk="henriet.0o">
  <kap>
    <ofc>*</ofc>
    <tld/>o
  </kap>
  <snc mrk="henriet.0o.SNC">
  <uzo tip="fak">NOM</uzo>
    <dif>
      Virina <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> germandevena:
      <ekz>
        <tld/>o de Francio, re&gcirc;ino de Anglio
        <fnt>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Maria_de_Medi%C4%89o">Maria de Medi&ccirc;o - Vikipedio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la bela <tld/>o
        <fnt>
          <vrk><url ref="https://eo.wikipedia.org/wiki/La_Faute-sur-Mer">La Faute-sur-Mer - Vikipedio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="henrik.0o">Henriko</ref>
  </snc>
  <trd lng="de">Henriette</trd>
  <trd lng="en">Henrietta</trd>
  <trd lng="es">Enriqueta</trd>
  <trd lng="fr">Henriette</trd>
  <trd lng="it">Enrica</trd>
  <trd lng="nl">Hendrika</trd>
  <trd lng="no">Henrikke</trd>
  <trd lng="pt">Henriqueta</trd>
  <trd lng="sv">Henrika</trd>
</drv>

<drv mrk="henriet.0njo">
  <kap>
   <ofc>*</ofc> 
   Henjo
  </kap>
  <snc mrk="henriet.0njo.SNC">
    <dif>
      Karesformo de <ref tip="lst" cel="henriet.0o" lst="voko:personaj_nomoj">Henrieto</ref>:
      <ekz>
        Henjo demetis la okulvitrojn kaj rigardis el la fenestro denove, sed &scirc;i vidis
        nur ian nebulecan mallumon
        <fnt>
          <vrk><url ref="http://www.autodidactproject.org/other/boulton_interne.html">Interne kaj Ekstere</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Jette</trd>,
    <trd>Henja</trd>,
    <trd>Henrike</trd>,
    <trd>Heike</trd>
  </trdgrp>
  <trd lng="en">Hetty</trd>
  <trd lng="fr">Riette</trd>
</drv>
</art>

