<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vag.xml,v 1.42 2024/03/30 06:24:56 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vag</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vag.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="vag.0i.KOMUNE">
    <dif>
      Sencele <ref tip="super" cel="ir.0i">iri</ref> de loko al loko:
      <ekz>
        <tld/>adi en la dezerto
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 32:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>as senlabore
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 5:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        fari&gcirc;is teruro en la tendaro, sur la kampo, kaj inter la tuta popolo, la
        garnizonanoj kaj la <tld/>atakistoj <klr>(nomadaj rabistoj)</klr>
        anka&ubreve; ektimis, kaj la tero ektremis
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 14:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        fari&gcirc;as nokto, dum kiu <tld/>as &ccirc;iuj arbaraj bestoj
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 104:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>as pro man&gcirc;o kaj sen sati&gcirc;o pasigas la nokton
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 59:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        la korv<klr>[&#8230;]</klr>idoj <klr>[&#8230;]</klr> <tld/>flugas, ne havante, kion man&gcirc;i
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 38:41</lok>
        </fnt>;
      </ekz>
      <ekz>
        erar<tld/>antaj &scirc;afoj
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 50:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        kial mi similu al <tld/>istino &ccirc;e la brutaroj de viaj kamaradoj
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 1:7</lok>
        </fnt>?
      </ekz>
      <ekz>
        kiel blinduloj ili <tld/>adis sur la stratoj
        <fnt>
          <bib>MT</bib>
          <lok>Plorkanto 4:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;irka&ubreve; sanktuloj diabloj <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[&scirc;iaj]</klr> okuloj <klr>[&#8230;]</klr> <tld/>adis sur la viza&gcirc;oj de la pasantoj
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cirkul.0i.TRA">cirkuli<sncref/></ref>,
      <ref cel="ir.cxirkaux0i.cxien">&ccirc;irka&ubreve;iri<sncref/></ref>,
      <ref cel="marod.0i">marodi</ref>,
      <ref cel="svarm.0i.KOMUNE">svarmi<sncref/></ref>,
      <ref cel="lum.vag0o"><tld/>lumo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_d;&c_z;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_b;&c_l;&c_u;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_l;&c_u;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_a;&c_n;&c_d;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_c;&c_ja;&c_g;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_sh;&c_v;&c_e;&c_n;&c_d;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>c'hwistolat</trd>,
    <trd>kantren</trd>,
    <trd>troidella&ntilde;</trd>
  </trdgrp>
  <trd lng="ca">vagar</trd>
  <trdgrp lng="cs">
    <trd>bloudit</trd>,
    <trd>toulat se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>umherstreifen</trd>,
    <trd>umherirren</trd>,
    <trd>umherziehen</trd>,
    <trd>stromern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>wander</trd>,
    <trd>ramble</trd>,
    <trd>meander</trd>,
    <trd>roam</trd>,
    <trd>drift</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>vagar</trd>,
    <trd>deambular</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>errer</trd>,
    <trd>fl&acirc;ner</trd>,
    <trd>r&ocirc;der</trd>,
    <trd>vaguer</trd>,
    <trd>vadrouiller</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&lamed;&shin;&vav;&tet;&tet; \<ind>&shin;&tet;&tet;</ind>\</trd>,
    <trd>&lamed;&tav;&ayin;&vav;&tav; \<ind>&tav;&ayin;&he;</ind>\</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&oacute;borol</trd>,
    <trd>bolyong</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12373;&#12414;&#12424;&#12358;</trd>,
    <trd>&#25918;&#28010;&#12377;&#12427;  <pr>&#12411;&#12358;&#12429;&#12358;&#12377;&#12427;</pr></trd>,
    <trd>&#27497;&#12365;&#22238;&#12427;  <pr>&#12354;&#12427;&#12365;&#12414;&#12431;&#12427;</pr></trd>,
    <trd>&#12358;&#12429;&#12388;&#12367;</trd>,
    <trd>&#24472;&#24458;&#12377;&#12427;  <pr>&#12399;&#12356;&#12363;&#12356;&#12377;&#12427;</pr></trd>,
    <trd>&#28418;&#12358;  <pr>&#12383;&#12384;&#12424;&#12358;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zwerven</trd>,
    <trd>slenteren</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>vagar</trd>,
    <trd>errar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_l;&c_u;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>poneviera&tcaron; sa</trd>,
    <trd>potulova&tcaron; sa <klr>(bez cie&lcaron;a)</klr></trd>,
    <trd>t&uacute;la&tcaron; sa</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>str&ouml;va</trd>,
    <trd>irra omkring</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_r;&c_o;&c_d;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_l;&c_u;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_m;&c_a;&c_n;&c_d;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="vag.0o">
  <kap><tld/>o,
  <var><kap><tld/>ado</kap></var></kap>
  <snc mrk="vag.0o.KOMUNE">
    <dif>
      Hazarda, sencela irado:
      <ekz>
        kun timego &scirc;i pensis pri tiu soleca <tld/>ado
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Ib</nom> nun povis permesi al si <tld/>adon tra la strata labirinto de Kopenhago
        <fnt>
          <bib>Fab3</bib>
          <lok>Ib kaj malgranda kristino</lok>
        </fnt>;
      </ekz>
      <ekz>
        knabo, vi havas specialan talenton por <tld/>ado
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>!
      </ekz>
      <ekz>
        post kvaronhora erar<tld/>ado sur mal&gcirc;ustaj vojetoj, la faraono atingis pordon
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        en mia <tld/>ado mi trafis la mona&hcirc;inejon pli-malpli &ccirc;e la vesperi&gcirc;o
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_d;&c_z;&c_ja;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="de">Streifzug</trd>
  <trdgrp lng="fr">
    <trd>errance</trd>,
    <trd>fl&acirc;nerie</trd>,
    <trd>vagabondage</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12373;&#12414;&#12424;&#12356;</trd>,
    <trd>&#25918;&#28010;  <pr>&#12411;&#12358;&#12429;&#12358;</pr></trd>,
    <trd>&#24472;&#24458;  <pr>&#12399;&#12356;&#12363;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_m;&c_a;&c_n;&c_d;&c_r;&c_i;</trd>,
    <trd>&c_b;&c_r;&c_o;&c_d;&c_ja;&c_zh;&c_n;&c_i;&c_c;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="vag.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <tld lit="V"/>i en la &ccirc;irka&ubreve;a&jcirc;o:
      <ekz>
        tuj falis sur lin nebulo kaj mallumo, kaj
        &ccirc;irka&ubreve;<tld/>ante, li ser&ccirc;is mankondukantojn
        <fnt><bib>NT</bib><lok>&Ago; 13:11</lok></fnt>;
      </ekz>
      <ekz>
        sen fajro ili tre kredeble fari&gcirc;os viktimoj de la unua
        sova&gcirc;a besto &ccirc;irka&ubreve;<tld/>anta en la nokto
        <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 9</lok></fnt>;
      </ekz>
      <ekz>
        unu el la malagrablaj amindumantoj de <nom>Adaha</nom> (kiu vane
        <ind>&ccirc;irka&ubreve;vaga&ccirc;i</ind>s &scirc;ian tendon en la nokto) ree ofendis &scirc;in per persistaj atenta&jcirc;oj
        <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 9</lok></fnt>.
        <trd lng="fr">r&ocirc;der <klr tip="amb">(autour de)</klr></trd>
        <trd lng="hu">&oacute;l&aacute;lkodik</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_u;&c_zh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_r;&c_u;&c_zh;&c_y;&c_c;&c_c;&c_a; <klr>(&c_v;&c_a;&c_k;&c_o;&c_l; &c_ch;&c_a;&c_g;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  </trdgrp>
  <trd lng="ca">rondar</trd>
  <trdgrp lng="fr">
    <trd>errer</trd>,
    <trd>r&ocirc;der</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&ouml;rbebolyong</trd>,
    <trd>k&ouml;zelben <ind>k&oacute;borog</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>vagar<klr tip="amb"> (nas cercanias)</klr></trd>,
   <trd>errar<klr tip="amb"> (nas cercanias)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="vag.for0i">
  <kap>for<tld/>i</kap>
  <snc mrk="vag.for0i.KOMUNE">
    <dif>
      Malproksimi&gcirc;i, <ref tip="sin" cel="voj.de0igxi">devoji&gcirc;i</ref> <tld/>ante:
      <ekz>
        tiuj, kun kiuj vi <klr>[&#8230;]</klr> interrilatis de via juneco, &ccirc;iu for<tld/>is
        siaflanken
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 47:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        elvoja for<tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>Jakobo 5:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        for<tld/>as flanken &ccirc;iam liaj pensoj al la gepatra
        domo
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="devi.0i">devii</ref>,
      <ref cel="flank.de0igxi">deflanki&gcirc;i</ref>,
      <ref cel="flank.0igxi">forflanki&gcirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_v;&c_a;&c_n;&c_d;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">odej&iacute;t na toulky</trd>
  <trd lng="de">abschweifen</trd>
  <trdgrp lng="es">
    <trd>vagabundear</trd>,
    <trd>divagar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>divaguer</trd>,
    <trd>vagabonder</trd>
    <trd>s'<ind>&eacute;garer</ind></trd>
   </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12373;&#12414;&#12424;&#12356;&#20986;&#12427;  <pr>&#12373;&#12414;&#12424;&#12356;&#12391;&#12427;</pr></trd>,
    <trd>&#12399;&#12368;&#12428;&#12427;</trd>,
    <trd>&#27178;&#36947;&#12408;&#12381;&#12428;&#12427;  <pr>&#12424;&#12371;&#12415;&#12385;&#12408;&#12381;&#12428;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">wegzwerven</trd>
  <trd lng="ru">&c_u;&c_b;&c_r;&c_je;&c_s;&c_t;&c_i;</trd>
  <trd lng="sk">zat&uacute;la&tcaron; sa</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_m;&c_a;&c_n;&c_d;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_p;&c_r;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_i;&c_s;&c_ja; &c_v; &c_m;&c_a;&c_n;&c_d;&c_r;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="vag.gap0i">
  <kap>gap<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="vag.gap0i.KOMUNE">
    <dif>
      Sencele <ref tip="super" cel="promen.0i">promeni</ref> interesi&gcirc;ante pri strataj bagatela&jcirc;oj:
      <ekz>
        la&ubreve; la Re&gcirc;a Strato nun pasumas turistoj kaj butikumantoj gap<tld/>antaj
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2014/011095.php">Tammisaari, urbeto &ccirc;e Balta
          Maro</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        gap<tld/>i tra la kaduka <nom>Hoi An</nom> estas vera plezuro
        <fnt>
          <aut>Verda Pino</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/verdapino/107739">Hoi An,
          reala kaj virtuala</url></vrk>,
          <lok>2008-11-14</lok>
        </fnt>;
      </ekz>
      <ekz>
        turisma man&gcirc;ejo <klr>[&#8230;]</klr> preta montri sian menuon en plasta folio &ccirc;e la
        proksimi&gcirc;o de gap<tld/>ulo
        <fnt>
          <bib>MD14</bib>
          <vrk>Reorienti&gcirc;o de la amas-turismo</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_d;&c_z;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_sh;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_sh;&c_v;&c_e;&c_n;&c_d;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_s;&c_o;&c_w;&c_g;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_sh;&c_l;&c_ja;&c_a;&c_s;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>bloumat</trd>,
    <trd>potulovat se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>flanieren</trd>,
    <trd>bummeln</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&f_p;&a_r;&a_s;&a_h; &a_z;&a_d;&a_n;</trd>,
    <trd>&a_w;&a_l; &f_g;&a_sh;&a_t;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>baguenauder</trd>,
    <trd>fl&acirc;ner</trd>
  </trdgrp>
  <trd lng="ja">&#12418;&#12398;&#29645;&#12375;&#12381;&#12358;&#12395;&#27497;&#12365;&#22238;&#12427;  <pr>&#12418;&#12398;&#12417;&#12378;&#12425;&#12375;&#12381;&#12358;&#12395;&#12354;&#12427;&#12365;&#12414;&#12431;&#12427;</pr></trd>
  <trdgrp lng="nl">
    <trd>flaneren</trd>,
    <trd>rondslenteren</trd>,
    <trd>slenteren</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_o;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_sh;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_f;&c_l;&c_a;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_o;&c_l;&c_t;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">potulova&tcaron; sa</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_je;&c_sh;&c_t;&c_a;&c_t;&c_i;&c_s;&c_ja;/&c_t;&c_i;&c_n;&c_ja;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_d;&c_i;&c_v;&c_l;&c_ja;&c_ju;&c_ch;&c_i;&c_s;&c_mol;</trd>,
    <trd>&c_l;&c_o;&c_v;&c_i;&c_t;&c_i; &c_gu;&c_a;&c_v;&c_i;/&c_gu;&c_a;&c_v;</trd>
  </trdgrp>
</drv>
</art>
