<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vag</rad>/i
</kap>
<drv mrk="vag.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Sencele <ref tip="super" cel="ir.0i">iri</ref> de loko al loko:
      <ekz>
        kiel leonoj, kiuj <tld/>as sen celo, kaj kiujn tiu <tld/>ado
        balda&ubreve; lacigas kaj enuigas<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i en la dezerto<fnt>Z</fnt>,
      </ekz>
      <ekz>
        en nekonita arbaro;
      </ekz>
      <ekz>
        la <tld/>antoj kun pa&scirc;tantaro<fnt>Z</fnt>
        <klr>(nomadoj)</klr>;
      </ekz>
      <ekz>
        &ccirc;irka&ubreve; sanktuloj diabloj <tld/>as<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>adi dum la kursotempo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kelkaj nuboj <tld/>as sur la &ccirc;ielo;
      </ekz>
      <ekz>
        <tld/>atakistoj<fnt>Z</fnt>
        <klr>(nomadaj rabistoj)</klr>;
      </ekz>
      <ekz>
        li malrapide ek<tld/>is &gcirc;is la mezo de la vojo;
      </ekz>
      <ekz>
        mi ne plu el<tld/>igos la piedon de Izrael el la tero, kiun mi
        donis al iliaj patroj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        korvidoj <tld/>flugas ne havante kion man&gcirc;i<fnt>Z</fnt>;
      </ekz>
      <ekz>
        for<tld/>i el la rekta vojo;
      </ekz>
      <ekz>
        erar<tld/>antaj &scirc;afoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la granda celo de lia <tld/>a vivo<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        lia terurita rigardo <tld/>is sur la muro<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la rigardo plezure <tld/>as sur tiu pejza&gcirc;o;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        nur &ccirc;e la bu&scirc;o <tld/>is ia mal&gcirc;ojo<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        amaso da malnovaj rememoroj &ccirc;irka&ubreve;<tld/>is lin;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        for<tld/>as flanken &ccirc;iam liaj pensoj al la gepatra
        domo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">toulat se</trd>
  <trdgrp lng="de">
    <trd>umherstreifen</trd>,
    <trd>umherirren</trd>,
    <trd>umherziehen</trd>
  </trdgrp>
  <trd lng="en">wander</trd>
  <trdgrp lng="es">
    <trd>vagar</trd>,
    <trd>deambular</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>errer</trd>,
    <trd>flaner</trd>,
    <trd>roder</trd>,
    <trd>vadrouiller</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zwerven</trd>,
    <trd>slenteren</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_l;&c_u;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>str&ouml;va</trd>,
    <trd>irra omkring</trd>
  </trdgrp>
</drv>
</art>
