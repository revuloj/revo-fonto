<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>trivial</rad>/a
</kap>
<drv mrk="trivia.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="trivia.0a.KOMUNE">
    <dif>
      Maldelikata, vulgara&ccirc;a:
      <ekz>
        homo <tld/>a kaj malsobra;
      </ekz>
      <ekz>
        <tld/>a sprita&jcirc;o<fnt>B</fnt>;
      </ekz>
      <ekz>
        ili neniam sin altigis super la <tld/>a&jcirc;oj de maldelikata
        kaj
        kruda vivado;
      </ekz>
      <ekz>
        <tld/>aj diskuta&ccirc;oj pri mono;
      </ekz>
      <ekz>
        la simpla, tera, <tld/>a, sed tamen tragedia, minaca,
        trudi&gcirc;ema,
        &scirc;iranta krio: panon<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="proz.0a">proza</ref>,
      <ref cel="poezi">senpoezia</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_v;&c_u;&c_l;&c_mol;&c_g;&c_a;&c_r;&c_n;&c_y;</trd>,
      <trd>&c_p;&c_l;&c_ju;&c_g;&c_k;&c_ib;</trd>
    </trdgrp>
    <trd lng="ca">vulgar</trd>
    <trdgrp lng="de">
      <trd>gew&ouml;hnlich</trd>,
      <trd>abgedroschen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>trivial</trd>,
      <trd>vulgaire</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>k&ouml;z&ouml;ns&eacute;ges</trd>,
      <trd>otromba</trd>,
      <trd>trivi&aacute;lis</trd>
    </trdgrp>
    <trdgrp lng="lat">
      <trd>trivialis</trd>,
      <trd>vulgaris</trd>,
      <trd>plebeius</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>trywialny</trd>,
      <trd>wulgarny</trd>,
      <trd>grubia&nacute;ski</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>vulgar</trd>,
      <trd>obsceno</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_u;&c_l;&c_mol;&c_g;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_p;&c_o;&c_sh;&c_l;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="trivia.0a.EVI">
    <uzo tip="stl">EVI</uzo>
    <dif>
      Banala:
      <ekz>
        <tld/>a pasero;
      </ekz>
      <ekz>
        maloriginala kaj <tld/>a temo;
      </ekz>
      <ekz>
        <tld/>aj ideoj;
      </ekz>
      <ekz>
        <tld/>aj kaj &ccirc;ie renkontataj esprimoj;
      </ekz>
      <ekz>
        kvankam &scirc;ajne <tld/>a, tio estas tre profundsenca observo.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_t;&c_r;&c_y;&c_v;&c_ib;&c_ja;&c_l;&c_mol;&c_n;&c_y;</trd>,
      <trd>&c_b;&c_a;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="ca">banal</trd>
    <trdgrp lng="pl">
      <trd>trywialny</trd>,
      <trd>banalny</trd>,
      <trd>oklepany</trd>,
      <trd>pospolity</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_t;&c_r;&c_i;&c_v;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_b;&c_a;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="ca">trivial</trd>
  <trd lng="de">gew&ouml;hnlich</trd>
  <trd lng="en">vulgar</trd>
  <trdgrp lng="nl">
    <trd>triviaal</trd>,
    <trd>plat<klr tip="ind"> (vulgair)</klr></trd>,
    <trd>vulgair</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hrubý</trd>,
    <trd>otrepaný</trd>,
    <trd>triviálny</trd>,
    <trd>vulgárny</trd>,
    <trd>všedný</trd>
  </trdgrp>
</drv>
</art>
