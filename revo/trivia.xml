<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: trivia.xml,v 1.22 2023/10/21 14:37:42 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>trivial</rad>/a
</kap>
<drv mrk="trivia.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="trivia.0a.KOMUNE">
    <dif>
      <ref tip="dif" cel="delika.mal0a.iu_malbona">Maldelikata</ref>, <ref tip="dif"
      cel="vulgar.0a">vulgara</ref>&ccirc;a:
      <ekz>
        enver&scirc;ante en la orelon de virino-patrino la simplan, teran, <tld/>an, sed tamen
        tragedian, minacan, trudi&gcirc;eman, &scirc;irantan krion: panon
        <fnt>
          <bib>Marta</bib>
        </fnt>!
      </ekz>
      <ekz>
        kompatinda ekzista&jcirc;o, <tld/>e naskita <klr>[kiel]</klr> bovo
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="proz.0a">proza</ref>,
      <ref cel="poezi">senpoezia</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_v;&c_u;&c_l;&c_mol;&c_g;&c_a;&c_r;&c_n;&c_y;</trd>,
      <trd>&c_p;&c_l;&c_ju;&c_g;&c_k;&c_ib;</trd>
    </trdgrp>
    <trd lng="ca">vulgar</trd>
    <trdgrp lng="fr">
      <trd><ofc>*</ofc>trivial</trd>,
      <trd>vulgaire</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>k&ouml;z&ouml;ns&eacute;ges</trd>,
      <trd>otromba</trd>,
      <trd>trivi&aacute;lis</trd>
    </trdgrp>
    <trdgrp lng="lat">
      <trd>trivialis</trd>,
      <trd>vulgaris</trd>,
      <trd>plebeius</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd><ofc>*</ofc>trywialny</trd>,
      <trd>wulgarny</trd>,
      <trd>grubia&nacute;ski</trd>,
      <trd><ofc>*</ofc>gminny</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>vulgar</trd>,
      <trd>obsceno</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_u;&c_l;&c_mol;&c_g;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>,
      <trd><ofc>*</ofc>&c_p;&c_o;&c_sh;&c_l;&c_y;&c_j;</trd>,
      <trd><ofc>*</ofc>&c_i;&c_z;&c_b;&c_i;&c_t;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="trivia.0a.EVI">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="banal.0a">Banala</ref>:
      <ekz>
        tio estas <tld/>a historio, sed &gcirc;i estas belete esprimita
        <fnt>
          <bib>Fab3</bib>
          <lok>Bona humoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        neniu ta&ubreve;gis por kompanio, &ccirc;iu estis sengusta kaj <tld/>a
        <fnt>
          <bib>Fab4</bib>
          <lok>Korta koko kaj ventkoko</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_t;&c_r;&c_y;&c_v;&c_ib;&c_ja;&c_l;&c_mol;&c_n;&c_y;</trd>,
      <trd>&c_b;&c_a;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="ca">banal</trd>
    <trdgrp lng="pl">
      <trd>trywialny</trd>,
      <trd>banalny</trd>,
      <trd>oklepany</trd>,
      <trd>pospolity</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_t;&c_r;&c_i;&c_v;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_b;&c_a;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="ca">trivial</trd>
  <trdgrp lng="cs">
    <trd>ban&aacute;ln&iacute;</trd>,
    <trd>obhroubl&yacute;</trd>,
    <trd>oby&ccaron;ejn&yacute;</trd>,
    <trd>ot&rcaron;el&yacute;</trd>,
    <trd>ot&rcaron;epan&yacute;</trd>,
    <trd>trivi&aacute;ln&iacute;</trd>,
    <trd>v&scaron;edn&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gew&ouml;hnlich</trd>,
    <trd><ofc>*</ofc>abgedroschen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>vulgar</trd>,
    <trd><ofc>*</ofc>trivial</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x91CE;&#21329;&#x306A;  <pr>&#x306E;&#21329;&#x306A;</pr></trd>,
    <trd>&#x4E0B;&#21329;&#x305F;  <pr>&#12375;&#x305F;&#21329;&#x305F;</pr></trd>,
    <trd>&#21329;&#x4FD7;&#x306A;  <pr>&#12402;&#12382;&#12367;&#12394;</pr></trd>,
    <trd>&#x4F4E;&#x4FD7;&#x306A;  <pr>&#12390;&#12356;&#x305E;&#x304F;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>triviaal</trd>,
    <trd>plat<klr tip="ind"> (vulgair)</klr></trd>,
    <trd>vulgair</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hrub&yacute;</trd>,
    <trd>otrepan&yacute;</trd>,
    <trd>trivi&aacute;lny</trd>,
    <trd>vulg&aacute;rny</trd>,
    <trd>v&scaron;edn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_r;&c_i;&c_v;&c_ib;&c_a;&#x43B;&#x44C;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_u;&#x43B;&#x44C;&c_gu;&c_a;&c_r;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_a;&#x44F;&#x43B;&#x43E;&c_zh;&c_je;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
