<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>faden</rad>/o
</kap>
<drv mrk="faden.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="faden.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Maldika, multe pli longa ol dika produkta&jcirc;o el kuntorditaj 
      <ref tip="prt" cel="fibr.0o.ero">fibroj</ref>,
      uzata por kudrado, trikado a&ubreve; teksado:
      <ekz>
        <tld/>o el lino, kotono, lano, silko;
      </ekz>
      <ekz>
        <ind>enkudriligi <tld/>on</ind>;
        <trd lng="en">thread a needle</trd>
      </ekz>
      <ekz>
        &scirc;iri&gcirc;is <tld/>o sur la bobeno
        <klr>(la afero malsukcesis)</klr>
        <fnt><bib>PrV</bib></fnt>
      </ekz>
      <ekz>
        <tld/>o iras, kien kudrilo &gcirc;in tiras <klr>(li devas obei)</klr>
        <fnt><bib>PrV</bib></fnt>
      </ekz>
      <ekz>
        <ind>ne havi sur si e&ccirc; unu sekan <tld/>on</ind>
        <klr>(porti tute malsekajn vestojn)</klr>
        <fnt><bib>Fab1</bib><lok>&Gcirc;ardeno de la paradizo</lok></fnt>;
        <trd lng="fr">&ecirc;tre <ind>mouill&eacute;</ind> jusqu'aux os</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>pendi nur sur <tld/>o malforta</ind><fnt>Z</fnt>
        <klr>(p.p. io: esti en dan&gcirc;era situacio)</klr>;
        <trd lng="fr">ne tenir qu'&agrave; un <ind>fil</ind></trd>
        <trd lng="hu">hajsz&aacute;lon f&uuml;gg</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>teni &ccirc;iujn <tld/>ojn en sia mano</ind>
        <klr>(&ccirc;ion direkti)</klr>.
        <trdgrp lng="fr">
          <trd>tenir les <ind>fils</ind> d'une affaire</trd>,
          <trd>tirer les <ind>ficelles</ind></trd>
        </trdgrp>
        <trd lng="hu">k&eacute;zben tartja a dolgot</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_n;&c_ib;&c_t;&c_k;&c_a;</trd>
    <trd lng="de">Faden</trd>
    <trd lng="en">thread</trd>
    <trdgrp lng="hu">
      <trd>fonal</trd>,
      <trd>fon&aacute;l</trd>
    </trdgrp>
    <trd lng="nl">draad</trd>
    <trd lng="ru">&c_n;&c_i;&c_t;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="faden.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <ref tip="dif" cel="drat.0o.fadeno">drato</ref>
    <ekz>
      malgrandaj araneoj &scirc;pinis &ccirc;irka&ubreve;
      <klr>[la bastono]</klr> orajn <tld/>ojn kaj ornamis
      &gcirc;in per flirtantaj vualoj
      <fnt><bib>Fab3</bib><lok>Kion la unua knabino vidis kaj lernis dum la voja&gcirc;o</lok></fnt>;
    </ekz>
    <ekz>
      oni &jcirc;etis al &scirc;i sur la tamburon volva&jcirc;ojn
      de kupra <tld/>o
      <fnt><bib>Far1</bib><lok>&ccirc;ap. 19</lok></fnt>.
    </ekz>
    <trd lng="be">&c_d;&c_r;&c_o;&c_t;</trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_v;&c_o;&c_l;&c_o;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="faden.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Sinsekva interligiteco:
      <ekz>
        <ind>perdi la <tld/>on</ind> en la parolo<fnt>Z</fnt>
        <klr>(&ccirc;esi kompreni a&ubreve; &ccirc;esi povi sin esprimi pro
        malatento a&ubreve; forgeso pri la celita senco)</klr>;
        <trd lng="en">lose the <ind>train</ind> of thought</trd>
        <trd lng="fr">perdre le <ind>fil</ind><klr tip="ind"> (de ses pens&eacute;es)</klr></trd>
      </ekz>
      <ekz>
        <tld/>o de la ideoj;
      </ekz>
      <ekz>
        <tld/>o de la rakonto;
      </ekz>
      <ekz>
        &ccirc;iuj fakoj de la scienco estas ligitaj inter si per
        ne&scirc;ireblaj <tld/>oj;
      </ekz>
      <ekz>
        <uzo tip="fak">KOMP</uzo>
        <ind>tem<tld/>o</ind> en interreta forumo.
        <trdgrp lng="fr">
          <trd>th&egrave;me de discussion<klr tip="ind"> (Internet)</klr></trd>,
          <trd>thread<klr tip="ind"> (Internet)</klr></trd>
        </trdgrp>
        <trd lng="hu">vita <ind>fonala</ind></trd>
        <trd lng="ru">&c_t;&c_je;&c_m;&c_a;<klr tip="amb"> (&c_v; &c_s;&c_je;&c_t;&c_je;&c_v;&c_o;&c_m; &c_f;&c_o;&c_r;&c_u;&c_m;&c_je;)</klr></trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxen.0o">&ccirc;eno</ref>,
      <ref cel="sekv">sekvo</ref>,
      <ref cel="konsek1.0o">konsekvenco</ref>.
    </refgrp>
    <trd lng="be">&c_n;&c_ib;&c_t;&c_k;&c_a;</trd>
    <trd lng="de">Faden</trd>
    <trd lng="hu">fonal</trd>
    <trd lng="nl">draad</trd>
  </snc>
  <snc mrk="faden.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Parto de programo funkcianta samtempe kaj sendepende de aliaj
      <tld/>oj;
      subproceso.
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_ib;&c_t;&c_k;&c_a;</trd>,
      <trd>&c_p;&c_a;&c_d;&c_p;&c_r;&c_a;&c_c;&c_e;&c_s;</trd>,
      <trd>&c_l;&c_je;&c_g;&c_k;&c_a;&c_v;&c_a;&c_g;&c_a;&c_v;&c_y; <ind>&c_p;&c_r;&c_a;&c_c;&c_e;&c_s;</ind></trd>
    </trdgrp>
    <trd lng="de">Thread</trd>
    <trd lng="hu">sz&aacute;l<klr tip="ind"> (v&eacute;grehajt&aacute;si)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_t;&c_o;&c_k;</trd>,
      <trd>&c_p;&c_o;&c_d;&c_p;&c_r;&c_o;&c_c;&c_je;&c_s;&c_s;</trd>
    </trdgrp>
  </snc>
  <trd lng="en">thread</trd>
  <trd lng="br">neudenn</trd>
  <trdgrp lng="fr">
    <trd>fil<klr tip="ind"> (&agrave; coudre, etc.)</klr></trd>,
    <trd>fil<klr tip="ind"> (d'un r&eacute;cit, de ses pens&eacute;es)</klr></trd>,
    <trd>t&acirc;che<klr tip="ind"> (inf., thread)</klr></trd>,
    <trd>thread<klr tip="ind"> (t&acirc;che)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sv">tr&aring;d</trd>
</drv>

<drv mrk="faden.0aro">
  <kap><tld/>aro</kap>
  <snc>
    <dif>
      Bulo, volva&jcirc;eto da <tld/>oj.
    </dif>
    <refgrp tip="vid">
      <ref cel="bul.faden0o"><tld/>bulo</ref>,
      <ref cel="fask.faden0o"><tld/>fasko</ref>.
    </refgrp>
  </snc>
  <adm>
    Kun la nuna difino, sxajnas, ke temas pri sinonimo de "~bulo".
    PIV1 enkondukis sencnuancon inter "~aro" kaj "~bulo". La unua
    estus sinonima al "~fasko" (unu longa fadeno multfoje refaldita).
    Mi ne scias, cxu tio estas sekvenda. <aut>MB</aut>
  </adm>
  <trd lng="be">&c_k;&c_l;&c_u;&c_b;&c_o;&c_k;</trd>
  <trd lng="br">pellenn<klr tip="ind"> (neud)</klr></trd>
  <trd lng="de">Kn&auml;uel</trd>
  <trd lng="fr">pelote<klr tip="ind"> (de fil, etc.)</klr></trd>
  <trd lng="hu">gombolyag</trd>
  <trd lng="ru">&c_k;&c_l;&c_u;&c_b;&c_o;&c_k;</trd>
</drv>

<drv mrk="faden.0ero">
  <kap><tld/>ero</kap>
  <snc>
    <dif>
      <tld lit="F"/>o tordita kun aliaj por formi pli grandan <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_l;&c_a;&c_k;&c_n;&c_o;</trd>
  <trd lng="br">neudenn</trd>
  <trdgrp lng="en">
    <trd>strand</trd>,
    <trd>ply</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>brin<klr tip="ind"> (d'un fil)</klr></trd>,
    <trd>filament<klr tip="ind"> (d'un fil)</klr></trd>,
    <trd>fibre<klr tip="ind"> (d'un fil)</klr></trd>
  </trdgrp>
  <trd lng="hu">sz&aacute;l<klr tip="ind"> (fonal&eacute;)</klr></trd>
  <trd lng="ru">&c_v;&c_o;&c_l;&c_o;&c_k;&c_n;&c_o;</trd>
</drv>

<drv mrk="faden.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Formi <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ib;</trd>
  <trd lng="br">neza&ntilde;</trd>
  <trd lng="en">draw into a <ind>thread</ind></trd>
  <trd lng="fr">filer<klr tip="ind"> (mettre en fil)</klr></trd>
  <trd lng="hu">fon</trd>
  <trd lng="ru">&c_p;&c_r;&c_ja;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="faden.dis0igi">
  <kap>dis<tld/>igi</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Malkunmeti teksa&jcirc;on, apartigante la <tld/>ojn:
      <ekz>
        la kolumo iom dis<tld/>i&gcirc;is <klr>(malteksi&gcirc;is)</klr> sur la
        rando<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_p;&c_u;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol; <klr tip="amb">(&c_n;&c_a; &c_n;&c_ib;&c_t;&c_k;&c_ib;)</klr></trd>
  <trd lng="br">dineudenni&ntilde;</trd>
  <trd lng="de">aufdr&ouml;seln</trd>
  <trd lng="en">unravel</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;filer<klr tip="ind"> (un tissu)</klr></trd>,
    <trd>effiler<klr tip="ind"> (un tissu)</klr></trd>,
    <trd>effilocher</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&aacute;lakra bont</trd>,
    <trd>sz&eacute;tbont<klr tip="ind"> (fonalat)</klr></trd>,
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol; <klr>(&c_t;&c_k;&c_a;&c_n;&c_mol; &c_n;&c_a; &c_n;&c_i;&c_t;&c_i;)</klr></trd>
</drv>

<drv mrk="faden.sur0igi">
  <kap>sur<tld/>igi</kap>
  <snc>
    <dif>
      Meti sur <tld/>on:
      <ekz>
        sur<tld/>igi perlojn.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ib;&c_z;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_n;&c_ib;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">stroba&ntilde;<klr tip="ind"> (ouzh un neudenn)</klr></trd>
  <trd lng="de">auff&auml;deln</trd>
  <trdgrp lng="en">
    <trd>thread</trd>,
    <trd>string</trd>
  </trdgrp>
  <trd lng="fr">enfiler<klr tip="ind"> (mettre sur un fil)</klr></trd>
  <trdgrp lng="hu">
    <trd>f&udblac;z</trd>,
    <trd>felf&udblac;z</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_n;&c_i;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_n;&c_i;&c_z;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="faden.fi&scirc;0o">
  <kap>fi&scirc;<tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      <tld lit="F"/>o uzata por fi&scirc;kaptado.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_jo;&c_s;&c_k;&c_a;</trd>
  <trd lng="de">Angelsehne</trd>
  <trd lng="en">fishing <ind>line</ind></trd>
  <trd lng="fr">fil de p&ecirc;che</trd>
  <trdgrp lng="hu">
    <trd>damil</trd>,
    <trd>horg&aacute;szzsineg</trd>
  </trdgrp>
  <trd lng="nl">vislijn</trd>
  <trd lng="ru">&c_l;&c_je;&c_s;&c_k;&c_a;</trd>
</drv>

<drv mrk="faden.metal0o">
  <kap><ofc>*</ofc>metal<tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Metala <ref tip="super" cel="drat.0o.fadeno">drato</ref>:
      <ekz>
        la para&scirc;ito prenis kurban metal<tld/>on, metis &gcirc;in
        en la nazon de l' mortinto kaj komencis eltiri la cerbon
        <fnt><bib>Far3</bib><lok>&ccirc;ap. 4</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Draht</trd>
  <trdgrp lng="br">
    <trd>orjalenn</trd>,
    <trd>neudenn-orjal</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>thread</trd>,
    <trd>wire</trd>
  </trdgrp>
  <trd lng="fr">fil m&eacute;tallique</trd>
  <trd lng="pl">drut</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_v;&c_o;&c_l;&c_o;&c_k;&c_a;</trd>
</drv>

<drv mrk="faden.pens0o">
  <kap>pens<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="F"/>o <sncref ref="faden.0o.FIG"/> de oniaj pensoj:
      <ekz>
        malla&ubreve;te, de neniu esplorate nek divenate,
        disvolvi&gcirc;adis la pens<tld/>o en humila, de neniu
        rimarkata virina kapo
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">fil des pens&eacute;es</trd>
</drv>

<drv mrk="faden.plumbo0o">
  <kap>plumbo<tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <ref tip="dif" cel="vertik.0ilo">vertikalilo.</ref>
  </snc>
  <trd lng="br">neudenn-blom</trd>
  <trd lng="de">Lot</trd>
  <trd lng="en">plumb line</trd>
  <trd lng="fr">fil &agrave; plomb</trd>
  <trd lng="hu">f&uuml;gg&odblac;&oacute;n</trd>
  <trd lng="nl">schietlood</trd>
  <trd lng="ru">&c_o;&c_t;&c_v;&c_je;&c_s;</trd>
</drv>

<drv mrk="faden.vel0o">
  <kap>vel<tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Fortika <tld/>o, por velkudrado ks.
    </dif>
    <trd lng="be">&c_sh;&c_p;&c_a;&c_g;&c_a;&c_t;</trd>
    <trd lng="en">seaming twine</trd>
    <trd lng="hu">vitorlavarr&oacute; <ind>fon&aacute;l</ind></trd>
    <trd lng="ru">&c_sh;&c_p;&c_a;&c_g;&c_a;&c_t;</trd>
  </snc>
</drv>
</art>
