    <trd>k&lstroke;&aogonek;b</trd>,
    <trd>wi&aogonek;zka</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_l;&c_u;&c_b;&c_o;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>klbko priadze</trd>,
    <trd>vl&aacute;kien</trd>,
    <trd>zv&auml;zok</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>klubko</trd>,
