<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>faden</rad>/o
</kap>
<drv mrk="faden.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="faden.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Maldika, multe pli longa ol dika produkta&jcirc;o el kuntorditaj fibroj,
      uzata por kudrado, trikado a&ubreve; teksado:
      <ekz>
        <tld/>o el lino, kotono, lano, silko;
      </ekz>
      <ekz>
        enkudriligi <tld/>on;
      </ekz>
      <ekz>&scirc;iri&gcirc;is <tld/>o sur la bobeno
        <klr>(la afero malsukcesis)</klr>
        <fnt><bib>PrV</bib></fnt></ekz>
      <ekz><tld/>o iras, kien kudrilo &gcirc;in tiras <klr>(li devas obei)</klr>
        <fnt><bib>PrV</bib></fnt></ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>pendi nur sur <tld/>o malforta</ind><fnt>Z</fnt>
        <klr>(p.p. io: esti en dan&gcirc;era situacio)</klr>;
        <trd lng="fr">ne tenir qu'&agrave; un <ind>fil</ind></trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>teni &ccirc;iujn <tld/>ojn en sia mano</ind>
        <klr>(&ccirc;ion direkti)</klr>.
        <trdgrp lng="fr">
          <trd>tenir les <ind>fils</ind> d'une affaire</trd>,
          <trd>tirer les <ind>ficelles</ind></trd></trdgrp> 
      </ekz>   
    </dif>
    <trd lng="de">Faden</trd>
    <trd lng="nl">draad</trd>
    <trd lng="ru">&c_n;&c_i;&c_t;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="faden.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="drat.0o">Drato</ref>:
      <ekz>
        ora <tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="faden.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Sinsekva interligiteco:
      <ekz>
        perdi la <tld/>on en la parolo<fnt>Z</fnt>
        <klr>(&ccirc;esi kompreni a&ubreve; &ccirc;esi povi sin esprimi pro
        malatento a&ubreve; forgeso pri la celita senco)</klr>;
      </ekz>
      <ekz>
        <tld/>o de la ideoj;
      </ekz>
      <ekz>
        <tld/>o de la rakonto;
      </ekz>
      <ekz>
        &ccirc;iuj fakoj de la scienco estas ligitaj inter si per
        ne&scirc;ireblaj <tld/>oj;
      </ekz>
      <ekz>
        <uzo tip="fak">KOMP</uzo>
        <ind>tem<tld/>o</ind> en interreta forumo.
        <trdgrp lng="fr">
          <trd>th&egrave;me de discussion<klr tip="ind"> (Internet)</klr></trd>,
          <trd>thread<klr tip="ind"> (Internet)</klr></trd></trdgrp> 
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxen.0o">&ccirc;eno</ref>,
      <ref cel="sekv">sekvo</ref>,
      <ref cel="konsek1.0o">konsekvenco</ref>.
    </refgrp>
    <trd lng="de">Faden</trd>
    <trd lng="nl">draad</trd>
  </snc>
  <snc mrk="faden.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Parto de programo funkcianta samtempe kaj sendepende de aliaj <tld/>oj;
      subproceso.
    </dif>
    <trd lng="de">Thread</trd>
  </snc>
  <trd lng="en">thread</trd>
  <trdgrp lng="fr">
    <trd>fil<klr tip="ind"> (&agrave; coudre, etc.)</klr></trd>,
    <trd>fil<klr tip="ind"> (d'un r&eacute;cit, de ses pens&eacute;es)</klr></trd>,
    <trd>t&acirc;che<klr tip="ind"> (inf., thread)</klr></trd>,
    <trd>thread<klr tip="ind"> (t&acirc;che)</klr></trd>
  </trdgrp> 
  <trd lng="ru">&c_n;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sv">tr&aring;d</trd>
</drv>
<drv mrk="faden.0aro">
  <kap><tld/>aro</kap>
  <snc>
    <dif>
      Bulo, volva&jcirc;eto da <tld/>oj.  
    </dif>
    <refgrp tip="vid">
      <ref cel="bul.faden0o"><tld/>bulo</ref>,
      <ref cel="fask.faden0o"><tld/>fasko</ref>.
    </refgrp>
  </snc>
  <adm>
  Kun la nuna difino, sxajnas, ke temas pri sinonimo de "~bulo".
  PIV1 enkondukis sencnuancon inter "~aro" kaj "~bulo". La unua
  estus sinonima al "~fasko" (unu longa fadeno multfoje refaldita).
  Mi ne scias, cxu tio estas sekvenda. <aut>MB</aut>
  </adm>
  <trd lng="de">Kn&auml;uel</trd>
  <trd lng="fr">pelote<klr tip="ind"> (de fil, etc.)</klr></trd>
  <trd lng="ru">&c_k;&c_l;&c_u;&c_b;&c_o;&c_k;</trd>
</drv>
<drv mrk="faden.0ero">
  <kap><tld/>ero</kap>
  <snc>
    <dif>
      <tld lit="F"/>o tordita kun aliaj por formi pli grandan <tld/>on.  
    </dif>
  </snc>
  <trd lng="en">strand</trd>
  <trdgrp lng="fr">
    <trd>brin<klr tip="ind"> (d'un fil)</klr></trd>,
    <trd>filament<klr tip="ind"> (d'un fil)</klr></trd>,
    <trd>fibre<klr tip="ind"> (d'un fil)</klr></trd></trdgrp> 
</drv>
<drv mrk="faden.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Formi <tld/>on.
    </dif>
  </snc>
  <trd lng="en">draw into a thread</trd>
  <trd lng="fr">filer<klr tip="ind"> (mettre en fil)</klr></trd>
</drv>
<drv mrk="faden.dis0igi">
  <kap>dis<tld/>igi</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Malkunmeti teksa&jcirc;on, apartigante la <tld/>ojn:
      <ekz>
        la kolumo iom dis<tld/>i&gcirc;is <klr>(malteksi&gcirc;is)</klr> sur la
        rando<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">aufdr&ouml;seln</trd>
  <trd lng="en">unravel</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;filer<klr tip="ind"> (un tissu)</klr></trd>,
    <trd>effiler<klr tip="ind"> (un tissu)</klr></trd>,
    <trd>effilocher</trd></trdgrp> 
  <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol; <klr>(&c_t;&c_k;&c_a;&c_n;&c_mol; &c_n;&c_a; &c_n;&c_i;&c_t;&c_i;)</klr></trd>
</drv>
<drv mrk="faden.sur0igi">
  <kap>sur<tld/>igi</kap>
  <snc>
    <dif>
      Meti sur <tld/>on:
      <ekz>
        sur<tld/>igi perlojn.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">auff&auml;deln</trd>
  <trd lng="en">thread</trd>
  <trd lng="fr">enfiler<klr tip="ind"> (mettre sur un fil)</klr></trd>
  <trd lng="ru">&c_n;&c_a;&c_n;&c_i;&c_z;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="faden.fi&scirc;0o">
  <kap>fi&scirc;<tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      <tld lit="F"/>o uzata por fi&scirc;kaptado.  
    </dif>
  </snc>
  <trd lng="de">Angelsehne</trd>
  <trd lng="en">fishing line</trd>
  <trd lng="fr">fil de p&ecirc;che</trd>
  <trd lng="nl">vislijn</trd>
  <trd lng="ru">&c_l;&c_je;&c_s;&c_k;&c_a;</trd>
</drv>
<drv mrk="faden.plumbo0o">
  <kap>plumbo<tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <!-- dif>
      <tld lit="F"/>o kun peza&jcirc;o &ccirc;e unu fino por montri
      <ref cel="vertik.0a">vertikalecon</ref>.
    </dif -->
    <ref tip="dif" cel="vertik.0ilo">vertikalilo.</ref>
  </snc>
  <trd lng="de">Lot</trd>
  <trd lng="en">plumb line</trd>
  <trd lng="fr">fil &agrave; plomb</trd>
  <trd lng="nl">schietlood</trd>
  <trd lng="ru">&c_o;&c_t;&c_v;&c_je;&c_s;</trd>
</drv>
<drv mrk="faden.vel0o">
  <kap>vel<tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Fortika <tld/>o, por velkudrado ks.
    </dif>
  </snc>
  <trd lng="en">seaming twine</trd>
</drv>
</art>
