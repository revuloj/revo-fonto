<drv mrk="faden.0ero">
  <kap><tld/>ero</kap>
  <snc mrk="faden.0ero.TEKS">
    <dif>
      Fibro a&ubreve; <tld/>o tordita kun aliaj por formi pli grandan <tld/>on:
      <ekz>
        7 &my;m &mdash; dikeco de <tld/>ero da aranea&jcirc;o
        <fnt>
          <vrk><url ref="https://eo.wikipedia.org/wiki/1_E-6_m">1 E-6 m &dash; Vikipedio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="filame.0o">filamento</ref>
    <trdgrp lng="de">
      <trd>Faser</trd>,
      <trd>Filament</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_l;&c_a;&c_k;&c_n;&c_o;</trd>
  <trd lng="br">neudenn</trd>
  <trdgrp lng="ca">
    <trd>bri</trd>,
    <trd>filament</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>strand</trd>,
    <trd>ply</trd>
  </trdgrp>
  <trd lng="es">trenza<klr tip="amb"> (de hilo)</klr></trd>
  <trdgrp lng="fr">
    <trd>brin<klr tip="ind"> (d'un fil)</klr></trd>,
    <trd>filament<klr tip="ind"> (d'un fil)</klr></trd>,
    <trd>fibre<klr tip="ind"> (d'un fil)</klr></trd>
  </trdgrp>
  <trd lng="hu">sz&aacute;l<klr tip="ind"> (fonal&eacute;)</klr></trd>
  <trd lng="pl">w&lstroke;&oacute;kno</trd>
  <trd lng="ru">&c_v;&c_o;&c_l;&c_o;&c_k;&c_n;&c_o;</trd>
</drv>

<drv mrk="faden.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="faden.0igi.TEKS">
    <dif>
      Formi <tld/>on:
      <ekz>
        li <tld/>igis la kokonon de insekto nestanta &ccirc;e ka&scirc;tanarbo, oni povas &scirc;pini
        blankan fadenon el la kokono <klr>[&#8230;]</klr> kiel el silkra&ubreve;po
        <fnt>
          <vrk><url ref="https://s22fe80463b65e112.jimcontent.com/download/version/1437946226/module/7046096767/name/NT282majo.pdf">Nova&jcirc;oj Tamtamas n-ro 282/ majo 2013 1/4</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ib;</trd>
  <trd lng="br">neza&ntilde;</trd>
  <trd lng="ca">filar</trd>
  <trd lng="de">spinnen</trd>
  <trd lng="en">draw into a <ind>thread</ind></trd>
  <trd lng="es">hilar</trd>
  <trd lng="fr">filer<klr tip="ind"> (mettre en fil)</klr></trd>
  <trd lng="hu">fon</trd>
  <trd lng="pl">prz&aogonek;&sacute;&cacute;</trd>
  <trd lng="ru">&c_p;&c_r;&c_ja;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="faden.dis0igi">
  <kap>dis<tld/>igi</kap>
  <snc mrk="faden.dis0igi.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Malkunmeti teksa&jcirc;on, apartigante la <tld/>ojn.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_p;&c_u;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;
  <klr tip="amb">(&c_n;&c_a;
  &c_n;&c_ib;&c_t;&c_k;&c_ib;)</klr></trd>
  <trd lng="br">dineudenni&ntilde;</trd>
  <trd lng="ca">desfilar</trd>
  <trd lng="cs">p&aacute;rat textilii na nit&ecaron;</trd>
  <trd lng="de">aufdr&ouml;seln</trd>
  <trd lng="en">unravel</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;filer<klr tip="ind"> (un tissu)</klr></trd>,
    <trd>effiler<klr tip="ind"> (un tissu)</klr></trd>,
    <trd>effilocher</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&aacute;lakra bont</trd>,
    <trd>sz&eacute;tbont<klr tip="ind"> (fonalat)</klr></trd>,
  </trdgrp>
  <trd lng="pl">rozplata&cacute;</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol;<klr tip="amb"> (&c_t;&c_k;&c_a;&c_n;&c_mol; &c_n;&c_a;
  &c_n;&c_i;&c_t;&c_i;)</klr></trd>
  <trdgrp lng="sk">
    <trd>rozstrapka&tcaron;</trd>,
    <trd>rozvl&aacute;kni&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="faden.dis0igxi">
  <kap>dis<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="faden.dis0igxi.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Diseri&gcirc;i elmontrante <tld/>ojn:
      <ekz>
        senforma pajlo&ccirc;apelo, kiu komencis dis<tld/>i&gcirc;i
        <fnt>
          <aut>T. Mann, trad. P. Buijnsters</aut>,
          <vrk>La Morto en Venecio</vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fibr.dis0igxi.KOMUNE">disfibri&gcirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="fr">partir en <ind>lambeaux</ind><klr> (en fils)</klr></trd>
</drv>

<drv mrk="faden.sur0igi">
  <kap>sur<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="faden.sur0igi.KOMUNE">
    <dif>
      Meti sur <tld/>on:
      <ekz>
        sur<tld/>igi perlojn.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ib;&c_z;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_n;&c_ib;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">stroba&ntilde;<klr tip="ind"> (ouzh un
  neudenn)</klr></trd>
  <trd lng="ca">enfilar</trd>
  <trd lng="cs">navl&eacute;knout na nit</trd>
  <trd lng="de">auff&auml;deln</trd>
  <trdgrp lng="en">
    <trd>thread</trd>,
    <trd>string</trd>
  </trdgrp>
  <trd lng="fr">enfiler<klr tip="ind"> (mettre sur un fil)</klr></trd>
  <trdgrp lng="hu">
    <trd>f&udblac;z</trd>,
    <trd>felf&udblac;z</trd>
  </trdgrp>
  <trd lng="pl">niza&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_n;&c_i;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_n;&c_i;&c_z;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">navliec&tcaron; na ni&tcaron;</trd>
  <trd lng="tr">ipe ge&ccedil;irmek</trd>
</drv>

<drv mrk="faden.fisx0o">
  <kap>fi&scirc;<tld/>o</kap>
  <snc mrk="faden.fisx0o.sxnuro">
    <uzo tip="fak">MAR</uzo>
    <dif>
      <tld lit="F"/>o uzata por fi&scirc;kaptado:
      <ekz>
        la malnova ludista kordo tremas en li simile al stre&ccirc;ita fi&scirc;<tld/>o
        <fnt>
          <aut>Aleksandro Grin</aut>
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/kuranta_20.html">Kuranta sur ondoj</url></vrk>
          <lok>XX</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_l;&c_jo;&c_s;&c_k;&c_a;</trd>
    <trd lng="ca">xarxa</trd>
    <trdgrp lng="de">
      <trd>Angelschnur</trd>,
      <trd>Angelsehne</trd>
    </trdgrp>
    <trd lng="en">fishing <ind>line</ind></trd>
    <trd lng="fr">fil de p&ecirc;che</trd>
    <trd lng="es">red de pesca</trd><trdgrp lng="hu">
      <trd>damil</trd>,
      <trd>horg&aacute;szzsineg</trd>
    </trdgrp>
    <trd lng="nl">vislijn</trd>
    <trd lng="pl">&zdot;y&lstroke;ka</trd>
    <trd lng="ru">&c_l;&c_je;&c_s;&c_k;&c_a;</trd>
   </snc>
   <snc mrk="faden.fisx0o.barbo">
     <uzo tip="fak">ZOO</uzo>
     <dif>
       Barbsimila <tld/>forma elkreska&jcirc;o &ccirc;e la makzelo de fi&scirc;oj.
     </dif>
     <trd lng="de">Bartel</trd>
     <trd lng="fr">barbillon</trd>
   </snc>
</drv>

<drv mrk="faden.metal0o">
  <kap><ofc>*</ofc>metal<tld/>o</kap>
  <snc mrk="faden.metal0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Metala <ref tip="super" cel="drat.0o.fadeno">drato</ref>:
      <ekz>
        la para&scirc;ito prenis kurban metal<tld/>on, metis &gcirc;in
        en la nazon de l' mortinto kaj komencis eltiri la cerbon
        <fnt><bib>Far3</bib>, <lok>&ccirc;ap. 4</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="br">
    <trd>orjalenn</trd>,
    <trd>neudenn-orjal</trd>
  </trdgrp>
  <trd lng="ca">fil met&agrave;l&middot;lic</trd>
  <trd lng="cs">dr&aacute;t</trd>
  <trd lng="de">Draht</trd>
  <trdgrp lng="en">
    <trd>thread</trd>,
    <trd>wire</trd>
  </trdgrp>
  <trd lng="fr">fil m&eacute;tallique</trd>
  <trdgrp lng="pl">
    <trd>drut</trd>,
    <trd>drucik</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_v;&c_o;&c_l;&c_o;&c_k;&c_a;</trd>
  <trd lng="sk">kovov&yacute; dr&ocirc;t</trd>
</drv>

<drv mrk="faden.pens0o">
  <kap>pens<tld/>o</kap>
  <snc mrk="faden.pens0o.PSI">
    <dif>
      Sinsekvo,
      <ref tip="vid" cel="faden.0o.FIG"><tld/>o<sncref/></ref>
      de ies pensoj:
      <ekz>
        malla&ubreve;te, de neniu esplorate nek divenate,
        disvolvi&gcirc;adis la pens<tld/>o en humila, de neniu
        rimarkata virina kapo
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">fil <klr>(de pensaments)</klr></trd>
  <trd lng="cs">nit my&scaron;lenek</trd>
  <trd lng="de">Gedankengang</trd>
  <trd lng="fr">fil des pens&eacute;es</trd>
  <trd lng="pl">w&aogonek;tek</trd>
  <trd lng="ru">&c_h;&c_o;&c_d; &c_m;&c_y;&c_s;&c_l;&c_je;&c_j;</trd>
  <trd lng="sk">ni&tcaron; my&scaron;lienok</trd>
</drv>

<drv mrk="faden.plumbo0o">
  <kap>plumbo<tld/>o</kap>
  <snc mrk="faden.plumbo0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <ref tip="dif" cel="vertik.0ilo">vertikalilo</ref>
  </snc>
  <trd lng="br">neudenn-blom</trd>
  <trd lng="ca">plom <klr>(nivell vertical)</klr></trd>
  <trd lng="cs">olovnice</trd>
  <trd lng="de">Lot</trd>
  <trd lng="en">plumb line</trd>
  <trd lng="fr">fil &agrave; plomb</trd>
  <trd lng="hu">f&uuml;gg&odblac;&oacute;n</trd>
  <trd lng="nl">schietlood</trd>
  <trd lng="pl">pion</trd>
  <trd lng="ru">&c_o;&c_t;&c_v;&c_je;&c_s;</trd>
  <trd lng="sk">olovnica</trd>
</drv>

<drv mrk="faden.rugxa0o">
  <kap>ru&gcirc;a <tld/>o</kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc mrk="faden.rugxa0o.KOMUNE">
    <dif>
      Gvida, revenanta elemento en rakonto, en klarigo,
      en serio de aferoj:
      <ekz>
        estas ru&gcirc;a <tld/>o, kiu kunigas la evoluojn
        de la nuntempa tutglobi&gcirc;o        
        <fnt>
          <aut>W. Bruno</aut>
          <vrk>Esperanto en la E&ubreve;ropo de la post-vero</vrk>
          <lok><url ref="http://iej.esperanto.it/ijf/?page_id=3055">
          IJF 2018</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Roter Faden</trd>
  </snc>
  <trd lng="fr">fil rouge</trd>
</drv>

<drv mrk="faden.vel0o">
