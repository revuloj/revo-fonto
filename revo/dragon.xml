<art mrk="$Id: dragon.xml,v 1.22 2023/11/04 18:50:51 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>dragon</rad>/o
</kap>
<drv mrk="dragon.0o">
  <kap><tld/>o</kap>
  <snc mrk="dragon.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="super" cel="soldat.0o">Soldato</ref> de malpeza
      kavalerio, kiu povas batali rajde a&ubreve; piede:
      <ekz>
        li estas le&ubreve;tenanto &ccirc;e la <tld/>a regimento
        &ccirc;evala de Lia
        mo&scirc;to
        <fnt><bib>Voja&gcirc;Imp</bib>, <lok>&ccirc;e A&hcirc;med&#x2019;
        Pa&scirc;a&#x2019;, la
        granda pentristo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_a;&c_g;&c_u;&c_n;</trd>
  <trd lng="bg">&c_d;&c_r;&c_a;&c_g;&c_u;&c_n;</trd>
  <trd lng="ca">drag&oacute; <klr>(mil.)</klr></trd>
  <trdgrp lng="cs">
    <trd>dragoun</trd>,
    <trd>p&#x159;&#xED;slu&#x161;n&#xED;k jezdectva</trd>
  </trdgrp>
  <trd lng="de">Dragoner</trd>
  <trd lng="en">dragoon</trd>
  <trd lng="fr">dragon<klr tip="ind"> (soldat)</klr></trd>
  <trd lng="it">dragone<klr> (mil.)</klr></trd>
  <trd lng="ja">&#x7ADC;&#x9A0E;&#x5175;  <pr>&#x308A;&#x3085;&#x3046;&#x304D;&#x3078;&#x3044;</pr></trd>
  <trd lng="pl">ragon</trd>
  <trd lng="pt">drag&atilde;o</trd>
  <trd lng="ru">&c_d;&c_r;&c_a;&c_g;&c_u;&c_n;</trd>
  <trd lng="sk">drag&#xFA;n</trd>
  <trd lng="sv">dragon</trd>
  <trdgrp lng="uk">
    <trd>&#x432;&#x456;&#x439;&#x441;&#x44C;&#x43A;.</trd>,
    <trd>&#x434;&#x440;&#x430;&#x491;&#x443;&#x43D;</trd>
  </trdgrp>
  <trd lng="zh">&#x9F8D;&#x9A0E;&#x5175; <pr>l&#xF3;ngq&#xED;b&#x12B;ng</pr></trd>
</drv>
