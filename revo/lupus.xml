<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>lupus</rad>/o
</kap>
<drv mrk="lupus.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">MED</uzo>
    <dif>
      Vario de nekonta&gcirc;a, kronika, imune mem-agresa, 
      <ref tip="lst" cel="san.mal0o.MED"
      lst="voko:malsanoj">malsano</ref>, 
konata anka&ubreve; kiel ha&ubreve;ta 
<ref tip="super" cel="tuberk.0o">tuberkulozo</ref>.
    </dif>
  </snc>
  <trd lng="ca">lupus</trd>
  <trdgrp lng="de">
    <trd>Lupus</trd>,
    <trd>Hauttuberkulose</trd>
  </trdgrp>
  <trd lng="es">lupus</trd>
  <trdgrp lng="fr">
    <trd>lupus</trd>,
    <trd><ind>tuberculose</ind> cutan&eacute;e</trd>
  </trdgrp>
  <trd lng="hu">b&odblac;rfarkas</trd>
  <trdgrp lng="nl">
    <trd>lupus</trd>,
    <trd>huidtuberculose</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>l&uacute;pus</trd>,
    <trd>lupo</trd>
    <trd>tuberculose cut&acirc;nea</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_l;&c_ch;&c_a;&c_n;&c_k;&c_a;</trd>,
    <trd>
&c_t;&c_u;&c_b;&c_je;&c_r;&c_k;&c_u;&c_l;&c_jo;&c_z; &c_k;&c_o;&c_zh;&c_i;
</trd>
  </trdgrp>
<trd lng="it">lupus</trd>
</drv>
</art>
