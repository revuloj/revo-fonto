<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lupus.xml,v 1.21 2024/01/15 19:32:09 revo Exp $">
<kap>
  <rad>lupus</rad>/o
</kap>
<drv mrk="lupus.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <uzo tip="fak">MED</uzo>
    <dif>
      Komuna nomo de ha&ubreve;taj 
      <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsanoj</ref>, 
      kiel t.n. ha&ubreve;ta <ref tip="super" cel="tuberk.0o">tuberkulozo</ref>
      (<trd lng="la">Lupus vulgaris</trd>),
      speco de sarkoidozo (<trd lng="la">Lupus pernio</trd>) a&ubreve; papilia
      eritemo (<trd lng="la">Lupus erythematodes</trd>):
      <ekz>
        <nom>Lyme</nom>-malsano en malfrua stadio <klr>[&#x2026;]</klr> povas
        esti misdiagnozata kiel
        multobla sklerozo, re&ubreve;matoida artrito, fibromialgio, kronika
        laceco-sindromo,
        <tld/>o, a&ubreve; aliaj a&ubreve;toimunaj kaj ne&ubreve;rodegeneraj
        <klr>[malsanoj]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=330394">Boreliozo Lyme</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_w;&c_ch;&c_a;&c_n;&c_k;&c_a;</trd>
  <trd lng="ca">lupus</trd>
  <trdgrp lng="de">
    <trd>Lupus</trd>,
    <trd>Hauttuberkulose</trd>
  </trdgrp>
  <trd lng="es">lupus</trd>
  <trdgrp lng="fr">
    <trd>lupus</trd>,
    <trd><ind>tuberculose</ind> cutan&eacute;e</trd>
  </trdgrp>
  <trd lng="hu">b&odblac;rfarkas</trd>
  <trd lng="id">lupus</trd>
  <trd lng="ja">&#x72FC;&#x7621;  <pr>&#x308D;&#x3046;&#x305D;&#x3046;</pr></trd>
  <trdgrp lng="nl">
    <trd>lupus</trd>,
    <trd>huidtuberculose</trd>
  </trdgrp>
  <trd lng="pl">tocze&nacute;</trd>
  <trdgrp lng="pt">
    <trd>l&uacute;pus</trd>,
    <trd>lupo</trd>
    <trd>tuberculose cut&acirc;nea</trd>
  </trdgrp>
  <trd lng="ro">lupus</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_l;&c_ch;&c_a;&c_n;&c_k;&c_a;</trd>,
    <trd>
&c_t;&c_u;&c_b;&c_je;&c_r;&c_k;&c_u;&c_l;&c_jo;&c_z; &c_k;&c_o;&c_zh;&c_i;
</trd>
  </trdgrp>
<trd lng="it">lupus</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x44E;&#x43F;&c_u;&c_s;</trd>,
    <trd>&c_v;&#x43E;&c_v;&c_ch;&c_a;&#x43A;</trd>,
    <trd>&c_v;&#x43E;&c_v;&c_ch;&c_i;&c_j; &#x43B;&c_i;&c_sh;&c_a;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x72FC;&#x75AE; <pr>l&#xE1;ngchu&#x101;ng</pr></trd>,
    <trd>&#x72FC;&#x7621; <pr>l&#xE1;ngchu&#x101;ng</pr></trd>
  </trdgrp>
</drv>
</art>
