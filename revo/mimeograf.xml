  <rad>mimeograf</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="mimeograf.0o">
  <kap><tld/>o</kap>
  <snc mrk="mimeograf.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Presma&scirc;ineto, kiu per rulo trapremas inkon tra vaksita &scirc;ablono
      al
      folioj.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_m;&c_e;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
  <trdgrp lng="en">
    <trd>mimeograph</trd>,
    <trd>stencil duplicator</trd>
  </trdgrp>
  <trd lng="ca">ciclostil</trd>
  <trdgrp lng="cs">
    <trd>cyklostyl</trd>,
    <trd>mimeograf</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Wachsmatrizendruck</trd>,
    <trd>Mimeograph</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>mim&eacute;ographe</trd>,
    <trd>ron&eacute;otype</trd>
  </trdgrp>
  <trd lng="hu">stencilg&eacute;p</trd>
  <trd lng="ja">&#x8B04;&#x5199;&#x7248;  [<ind>&#x3068;&#x3046;&#x3057;&#x3083;&#x3070;&#x3093;</ind>]</trd>
  <trdgrp lng="nl">
    <trd>mimeograaf</trd>,
    <trd>stencilmachine</trd>
  </trdgrp>
  <trd lng="pt">mime&oacute;grafo</trd>
  <trdgrp lng="sk">
    <trd>cyklostyl</trd>,
    <trd>rozmno&zcaron;ovac&iacute; stroj</trd>
  </trdgrp>
  <trd lng="uk">&#x43C;&#x456;&#x43C;&#x435;&#x43E;&#x433;&#x440;&#x430;&#x444;</trd>
</drv>

<drv mrk="mimeograf.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="mimeograf.0i.TIP">
    <dif>
      Produkti ekzemplerojn de dokumento per <tld/>o:
      <ekz>
        la grupanoj <klr>...</klr> tajpas kaj <tld/>as, &ccirc;iusemajne,
        aron
        da notoj
        <fnt>
        <aut>K. Laughery</aut>,
        <vrk>Lingvoproblemoj en la pontifikaj romaj universitatoj</vrk>,
        <lok><url ref="http://www.ikue.org/ek/ek1980_10.htm">Espero
        Katolika,
        1980-10</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_p;&c_ib;&c_ja;&c_v;&c_a;&c_c;&c_mol; &c_n;&c_a; <ind>&c_m;&c_ib;&c_m;&c_e;&c_o;&c_g;&c_r;&c_a;&c_f;&c_y;</ind></trd>
  <trd lng="en">to mimeograph</trd>
  <trd lng="ca">ciclostilar</trd>
  <trd lng="cs">cyklostylovat</trd>
  <trd lng="de">mimeographieren</trd>
  <trdgrp lng="fr">
    <trd>mim&eacute;ographier</trd>,
    <trd>ron&eacute;otyper</trd>
  </trdgrp>
  <trd lng="hu">stencilez</trd>
  <trd lng="ja">&#x8B04;&#x5199;&#x7248;&#x3067;&#x5237;&#x308B;  [<ind>&#x8B04;&#x5199;&#x306F;&#x3093;&#x3067;&#x3059;&#x308B;</ind>]</trd>
  <trd lng="nl">stencilen</trd>
  <trd lng="pt">mimeografar</trd>
  <trd lng="sk">cyklostylova&tcaron;</trd>
  <trd lng="uk">&#x440;&#x43E;&#x437;&#x43C;&#x43D;&#x43E;&#x436;&#x443;&#x432;&#x430;&#x442;&#x438; &#x43D;&#x430; &#x43C;&#x456;&#x43C;&#x435;&#x43E;&#x433;&#x440;&#x430;&#x444;&#x456;</trd>
</drv>
</art>

