<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>placent</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="placen.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">ANA</uzo>
    <dif>
      Spongeca kaj mola organo, kunliganta idon al la patrino anta&ubreve;
la
      nasko.
    </dif>
    <trdgrp lng="de">
      <trd>Plazenta</trd>,
      <trd>Mutterkuchen</trd>
    </trdgrp>
    <trd lng="en">placenta</trd>
    <trd lng="fr">placenta</trd>
    <trd lng="hu">m&eacute;hlep&eacute;ny</trd>
    <trdgrp lng="nl">
      <trd>placenta</trd>,
      <trd>moederkoek</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_a;&c_c;&c_je;&c_n;&c_t;&c_a;</trd>,
      <trd>&c_p;&c_o;&c_s;&c_l;&c_je;&c_d;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Angia parto de la perikarpo, al kiu estas fiksitaj la grajnoj per
      la
      funiklo.
    </dif>
    <trdgrp lng="de">
      <trd>Plazenta</trd>,
      <trd>Samenleiste</trd>
    </trdgrp>
    <trd lng="en">placenta</trd>
    <trd lng="hu">magl&eacute;c</trd>
    <trd lng="pl">&lstroke;o&zdot;ysko <klr>(anatomia)</klr></trd>
    <trd lng="pt">placenta</trd> 
    <trd lng="ru">&c_p;&c_l;&c_a;&c_c;&c_je;&c_n;&c_t;&c_a;</trd>
  </snc>
  <trd lng="it">placenta</trd>
  <trdgrp lng="sk">
    <trd>placenta</trd>,
    <trd>plodové lôžko</trd>
  </trdgrp>
</drv>
</art>
