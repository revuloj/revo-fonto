<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>tog</rad>/o
</kap>
<drv mrk="tog.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Ceremonia vira vesto de la antikvaj Romianoj, konsistanta el vasta
      lana tuko, kiun oni drapire volvis &ccirc;irka&ubreve; la korpo, lasante libera
      la dekstran brakon..
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_o;&c_g;&c_a;</trd>
  <trd lng="de">Toga</trd>
  <trd lng="en">toga</trd>
  <trd lng="fr">toge</trd>
  <trd lng="hu">t&oacute;ga</trd>
  <trd lng="nl">toga</trd>
  <trd lng="ru">&c_t;&c_o;&c_g;&c_a;</trd>
</drv>
</art>
