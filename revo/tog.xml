<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>tog</rad>/o
</kap>
<drv mrk="tog.0o">
  <kap><tld/>o</kap>
  <snc mrk="tog.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Ceremonia vira <ref tip="super" cel="rob.0o.surtuto">robo</ref> de
      la antikvaj Romianoj, konsistanta el vasta lana tuko, kiun oni
      drapire volvis &ccirc;irka&ubreve; la korpo, lasante libera la dekstran brakon.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_o;&c_g;&c_a;</trd>
  <trd lng="ca">toga</trd>
  <trd lng="de">Toga</trd>
  <trd lng="en">toga</trd>
  <trd lng="es">toga</trd>
  <trd lng="fr">toge</trd>
  <trd lng="hu">t&oacute;ga</trd>
  <trd lng="nl">toga</trd>
  <trd lng="ru">&c_t;&c_o;&c_g;&c_a;</trd>
</drv>
</art>
