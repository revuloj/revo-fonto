<vortaro>
<art mrk="$Id: jxujxu.xml,v 1.3 2023/04/06 08:32:09 revo Exp $">
<kap>
  <rad>&jcirc;u&jcirc;u</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="jxujxu.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="jxujxu.0o.KOMUNE">
    <dif>
      <ref tip="dif" cel="bagate.0o.KOMUNE">Bagatelo</ref>,
      <ref cel="valor.sen0a">senvalora</ref>&jcirc;o:
      <ekz>
        estu nur flor&#8217;, <tld/>o senutila,
        sed bela
        <fnt>
          <aut>I. Mad&aacute;ch, trad. K. Kalocsay</aut>, 
          <vrk>La Tragedio de l' Homo</vrk>, 
          <lok>1965</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Strato de la Turkoj, kun sia brilega ekspono de 
        <tld/>oj, turni&gcirc;is en melodian oazon
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>1992</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_o;&c_b;&c_ja;&c_z;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>Pillepalle</trd>,
    <trd>Tand</trd>
  </trdgrp>
  <trd lng="fr">babiole</trd>
</drv>
</art>

