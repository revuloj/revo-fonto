<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: puber.xml,v 1.8 2023/12/29 06:31:43 revo Exp $">
    <kap><rad>puber</rad>/a <fnt><bib>PIV1</bib></fnt></kap>

<drv mrk="puber.0a">
  <kap><tld/>a</kap>
  <snc>
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="dif" cel="pubert.0a">Puberta</ref>:
      <ekz>
        iuj knabaj &hcirc;oroj nur konsistas el pra<tld/>aj knaboj kaj do nur el sopranaj kaj aldaj vo&ccirc;oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=308342">Knaba &hcirc;oro</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la pubhararo <klr>[&#8230;]</klr> aperas samtempe kun la seksmaturi&gcirc;o,
        pro tio &gcirc;i memorigas anka&ubreve; la etimologian parencecon de la du vortoj
        <tld/>a (= seksmatura) kaj pubo (= seksorgana korporegiono, kun sia hararo)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=227356">Pubvilo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_l;&c_je;&c_t;&c_k;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_p;&c_u;&c_b;&c_e;&c_r;&c_t;&c_a;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="pl">dojrza&lstroke;y p&lstroke;ciowo</trd>
  <trd lng="pt">p&uacute;bere</trd>
  <trd lng="ro">matur&abreve; sexual&abreve;</trd>
  <trd lng="uk">&#x43F;&c_u;&c_b;&c_je;&c_r;&c_t;&c_a;&c_t;&#x43D;&c_i;&c_j;</trd>
</drv>

<drv mrk="puber.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="dif" cel="pubert.0o">Puberto</ref>:
      <ekz>
        <tld/>eco a&ubreve; puberto estas stato de knabo a&ubreve; knabino atinginta periodon de la vivo inter
        infana&gcirc;o kaj adolesko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=108142">Pubereco</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_l;&c_je;&c_t;&c_k;&c_a;&c_v;&c_y; &c_w;&c_z;&c_r;&c_o;&c_s;&c_t;</trd>,
    <trd>&c_p;&c_u;&c_b;&c_e;&c_r;&c_t;&c_a;&c_t;&c_n;&c_y; &c_w;&c_z;&c_r;&c_o;&c_s;&c_t;</trd>
  </trdgrp>
  <trd lng="cs">puberta</trd>
  <trdgrp lng="pl">
    <trd>pokwitanie</trd>,
    <trd>dojrza&lstroke;o&sacute;&cacute; p&lstroke;ciowa</trd>
  </trdgrp>
  <trd lng="ro">pubertate</trd>
  <trd lng="sk">puberta</trd>
  <trd lng="uk">&#x43F;&c_u;&c_b;&c_je;&c_r;&c_t;&c_a;&c_t;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>
</drv>

  </art>

