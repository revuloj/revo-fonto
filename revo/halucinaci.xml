<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: halucinaci.xml,v 1.8 2023/11/15 16:45:17 revo Exp $">
<kap>
  <rad>halucinaci</rad>/o <fnt><bib>Far1</bib></fnt>
</kap>
<drv mrk="halucinaci.0o">
  <kap><tld/>o</kap>
  <snc mrk="halucinaci.0o.halucino">
    <dif>
      <ref tip="dif" cel="haluci.0o">Halucino</ref>:
      <ekz>
        li eksentis komence kapdolorojn, kiuj ofte faris ian ajn laboron
        neebla, poste dum vesperoj aperis al li <tld/>oj
        <fnt>
          <aut>Ivan G. &Scirc;irjaev</aut>,
          <vrk>Sen titolo</vrk>,
          <lok>&ccirc;apitro 8a</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum unuj pastroj per longeda&ubreve;raj fastoj kaj sobreco sin preparas al elvokado de
        l' spiritoj, aliaj nomas la spiritojn <tld/>o a&ubreve; trompo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">
  &c_g;&c_a;&c_l;&c_ju;&c_c;&c_y;&c_n;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="de">
     <trd>Halluzination</trd>
  </trdgrp>
  <trd lng="fr">hallucination</trd>
  <trdgrp lng="hu">
     <trd>hallucin&aacute;ci&oacute;</trd>,
     <trd>&eacute;rz&eacute;kcsal&oacute;d&aacute;s</trd>,
     <trd>r&eacute;ml&aacute;t&aacute;s</trd>,
     <trd>beteges <ind>k&eacute;pzel&odblac;d&eacute;s</ind></trd>
  </trdgrp>
  <trd lng="it">allucinazione</trd>
  <trd lng="nl">hallucinatie</trd>
  <trd lng="pl">halucynacje</trd>
  <trd lng="pt">alucina&ccedil;&atilde;o</trd>
  <trd lng="ru">
  &c_g;&c_a;&c_l;&c_l;&c_ju;&c_c;&c_i;&c_n;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>halucin&aacute;cia</trd>,
    <trd>prelud</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>halucinace</trd>,
    <trd>p&rcaron;elud</trd>
  </trdgrp>
  <trd lng="uk">&#x433;&#x430;&#x43B;&#x44E;&#x446;&#x438;&#x43D;&#x430;&#x446;&#x456;&#x44F;</trd>
  <trd lng="zh">&#x5E7B;&#x89BA; <pr>hu&#xE0;nju&#xE9;</pr></trd>
</drv>
</art>

