<art mrk="$Id: harpal.xml,v 1.15 2023/08/31 15:46:14 revo Exp $">
<kap>
<rad>harpal</rad>/o
</kap>
<drv mrk="harpal.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="karab.0oj">karabedoj</ref>
      (<trd lng="la">Harpalus</trd>):
      <ekz>
        la genro <tld/>o estis unuafoje priskribita 1802 de
        <nom>Pierre A. Latreille</nom>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/9/91/Harpalus_sp._%2814244218353%29.jpg/319px-Harpalus_sp._%2814244218353%29.jpg" prm="CC BY-SA 2.0"><tld/>o
        <fnt>
          <aut>Pavel Kirillov el St.Petersburg, Rusujo</aut>
          <lok><url ref="&WCW;/File:Harpalus_sp._(14244218353).jpg">&WCW;/File:Harpalus_sp._(14244218353).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_zh;&c_u;&c_zh;&c_a;&c_l;&c_mol;</trd>
  <trd lng="de">Schnelll&auml;ufer</trd>
  <trd lng="fr">harpale</trd>
  <trdgrp lng="hu">
    <trd>f&eacute;mfut&oacute;</trd>,
    <trd>futrinka</trd>
  </trdgrp>
  <trd lng="ja">&#x30B4;&#x30E2;&#x30AF;&#x30E0;&#x30B7;</trd>
  <trd lng="pt">carab&iacute;deo</trd>
  <trd lng="uk">&c_g;&c_a;&c_r;&#x43F;&c_a;&#x43B;</trd>
</drv>
</art>
