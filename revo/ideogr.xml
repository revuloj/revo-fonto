<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>ideografi</rad>/o
</kap>
<drv mrk="ideogr.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Sistemo de skribado, en kiu oni reprezentas ne la sonojn de la vortoj,
      sed la ideojn mem helpe de &ccirc;iuspecaj signoj, desegnoj ktp:
      <ekz>la &hcirc;inoj uzas &gcirc;isnun <tld/>an skribadon.</ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Ideographie</trd>,
     <trd>Begriffsschrift</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_d;&c_je;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
</drv>
<drv mrk="ideogr.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Desegno a&ubreve; signo reprezentanta ideon:
      <ekz>la ciferoj estas veraj <tld/>a&jcirc;oj.</ekz>
    </dif>
  </snc>
  <trd lng="de">Ideogramm</trd>
  <trd lng="ru">&c_i;&c_d;&c_je;&c_o;&c_g;&c_r;&c_a;&c_m;&c_m;&c_a;</trd>
</drv>
</art>
