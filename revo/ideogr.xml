<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ideogr.xml,v 1.17 2022/03/21 15:27:48 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>ideografi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ideogr.0o">
  <kap><tld/>o</kap>
  <snc mrk="ideogr.0o.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="super" cel="sistem.skrib0o.KOMUNE">Skribsistemo</ref>
      kiu prezentas ne la sonojn de la
      vortoj, sed la ideojn mem helpe de &ccirc;iuspecaj signoj, desegnoj
      ktp:
      <ekz>
        la &ccirc;inoj uzas &gcirc;is nun <ind><tld/>a</ind>n skribadon.
        <trd lng="fr">id&eacute;ographique</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_d;&c_e;&c_a;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>ideografie</trd>,
    <trd>obr&#xE1;zkov&#xE9; p&#xED;smo</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Ideographie</trd>,
     <trd>Begriffsschrift</trd>
  </trdgrp>
  <trd lng="fa">&a_A;&a_n;&a_d;&f_y;&a_sh;&a_h;&zwnj;&a_n;&f_g;&a_A;&a_r;&f_y;</trd>
  <trd lng="fr">id&eacute;ographie</trd>
  <trdgrp lng="hu">
     <trd>fogalom&iacute;r&aacute;s</trd>,
     <trd>ideogr&aacute;fia</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ideografie</trd>,
    <trd>beeldschrift</trd>
  </trdgrp>
  <trd lng="pt">ideografia</trd>
  <trd lng="ru">&c_i;&c_d;&c_je;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
  <trd lng="sk">obr&#xE1;zkov&#xE9; p&#xED;smo</trd>
  <trd lng="uk">&#x456;&#x434;&#x435;&#x43E;&#x433;&#x440;&#x430;&#x444;&#x456;&#x44F;</trd>
</drv>

<drv mrk="ideogr.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="ideogr.0ajxo.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      Desegno a&ubreve; signo reprezentanta ideon;
      <ref tip="sin" cel="ideogram.0o.LIN">ideogramo</ref>:
      <ekz>
        la ciferoj estas veraj <tld/>a&jcirc;oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_d;&c_e;&c_a;&c_g;&c_r;&c_a;&c_m;&c_a;</trd>
  <trd lng="de">Ideogramm</trd>
  <trd lng="es">ideograma</trd>
  <trd lng="fa">&a_A;&a_n;&a_d;&f_y;&a_sh;&a_h;&zwnj;&a_n;&f_g;&a_A;&a_sh;&a_t;</trd>
  <trd lng="fr">id&eacute;ogramme</trd>
  <trd lng="hu">ideogramma</trd>
  <trd lng="nl">ideogram</trd>
  <trd lng="pt">ideograma</trd>
  <trd lng="ru">&c_i;&c_d;&c_je;&c_o;&c_g;&c_r;&c_a;&c_m;&c_m;&c_a;</trd>
</drv>
</art>
