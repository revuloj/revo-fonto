<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>asert</rad>/i
</kap>
<drv mrk="asert.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Firme certigi, pretendi, ke io estas vera.
    </dif>
  </snc>
  <trd lng="en">assert</trd>
  <trd lng="fr">affirmer</trd>
  <trd lng="hu">&aacute;ll&iacute;t</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_v;&c_je;&c_r;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_t;&c_v;&c_je;&c_r;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>p&aring;st&aring;</trd>,
    <trd>f&ouml;rs&auml;kra</trd>
  </trdgrp>
</drv>

<drv mrk="asert.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Firma certigo pri io:
      <ekz>
        tia <tld/>o estas iom senprudenta.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>assertion</trd>,
    <trd>affirmation</trd>
  </trdgrp>
  <trd lng="hu">&aacute;ll&iacute;t&aacute;s</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_v;&c_je;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_u;&c_t;&c_v;&c_je;&c_r;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>p&aring;st&aring;ende</trd>,
    <trd>f&ouml;rs&auml;kran</trd>
  </trdgrp>
</drv>
</art>
