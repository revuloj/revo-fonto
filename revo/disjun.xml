<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <rad>disjunkci</rad>/o
  </kap>
<drv mrk="disjun.0o">
  <kap><tld/>o</kap>
    <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc mrk="disjun.0o.logika">
    <dif>
      <ref tip="super" cel="operac.logika0o">Logika operacio</ref>,
      kiu al du <ref cel="propoz.0o.aserto">propozicioj</ref>
      asocias la propozicion, kiu estas vera, se kaj nur se
      almena&ubreve; unu el ili estas vera; rezulto de tiu operacio:
      <ekz>
        la <tld/>on de P kaj Q oni foje signas per P&#8744;Q
        <klr>(legu: P a&ubreve; Q)</klr>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Disjunktion</trd>,
      <trd>logische <ind>Summe</ind></trd></trdgrp> 
    <trd lng="en">disjunction</trd>
    <trdgrp lng="es">
       <trd>disyunci&oacute;n</trd>
       <trd>disjunci&oacute;n</trd></trdgrp>
    <trdgrp lng="fr">
      <trd>disjonction<klr tip="ind"> (produit logique)</klr></trd>,
      <trd>somme logique</trd></trdgrp> 
    <trdgrp lng="ru">
      <trd>&c_d;&c_i;&c_z;&c_malmol;&c_ju;&c_n;&c_k;&c_c;&c_i;&c_ja;</trd>,
      <trd>&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja; <ind>&c_s;&c_u;&c_m;&c_m;&c_a;</ind></trd>
    </trdgrp> 
  </snc>
</drv>
</art>

