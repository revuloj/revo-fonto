<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: svag.xml,v 1.11 2022/05/05 14:43:49 revo Exp $">
<kap>
    <rad>svag</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="svag.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">POE</uzo>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="svag.0a.KOMUNE">
    <dif>
      <ref tip="dif" cel="preciz.mal0a.KOMUNE">Malpreciza</ref>,
      <ref tip="dif" cel="nebul.0a.FIG">nebula <sncref/></ref>:
      <ekz>
        la tolo <klr>[&#x2026;]</klr> &scirc;ajnis vasti&gcirc;i al la <tld/>aj kaj nebulaj
        
        muroj 
        <fnt>
          <aut>J. Camacho</aut>,
          <vrk>La Majstro kaj Martinelli</vrk>,
          <lok>Iltis, 1993</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni vidis steleon el bronzo kun Greklingva surskribo, kvankam 
        <tld/>e kaj apena&ubreve; plu legeble
        <fnt>
          <aut>Lukiano, trad. G. Berveling</aut>, 
          <vrk><url ref="http://www.esperanto.gr/lit/els/rakontoj.pdf">veraj 
            rakontoj</url> &#x2010; 2005</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> celas precizigi &ccirc;ion, kio &ccirc;e <nom>Zamenhof</nom>
        restis ambigua, <tld/>a a&ubreve; e&ccirc; rekte erara
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk><url ref="&Steloj;monato2/006820.html">Plia
          libro biblia</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj limoj inter realo kaj son&gcirc;o, inter efektivo kaj imago,
        inter frenezo kaj racio
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk><url ref="&Steloj;monato2/006836.html">Pri realo
          kaj la malo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj memoroj
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="&Monato;/2003/007921.html">Ho,
          mistera pi!</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la nocio pri dio mem estas tiom <tld/>a, tiom abstrakta, ke &ccirc;iu
        el 
        ni devus la&ubreve;vole, libere ne atence retrovi sian propran bildon
        <fnt>
          <bib>Monato</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>unscharf</trd>,
      <trd>vage</trd>,
      <trd>ungenau</trd>,
      <trd>verschwommen</trd>,
      <trd>konturlos</trd>,
      <trd>schemenhaft</trd>,
      <trd>unklar</trd>,
      <trd>nebul&ouml;s</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_n;&c_je;&c_ja;&c_s;&c_je;&c_n;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_m;&c_i;&c_t;&c_a;<klr tip="amb">
    (&c_l;&c_o;&c_g;&c_i;&c_k;&c_a;)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mlhav&yacute;</trd>,
    <trd>nejasn&yacute;</trd>,
    <trd>neur&ccaron;it&yacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>vague</trd>,
    <trd>fuzzy<klr tip="amb"> (logic)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>impreciso</trd>,
    <trd>vago</trd>
  </trdgrp>
  <trd lng="fr">vague<klr> (adj.)</klr></trd>
  <trdgrp lng="it">
    <trd>vago<klr> (agg.)</klr></trd>,
    <trd>fuzzy<klr> (log. e inf.)</klr></trd>
  </trdgrp>
  <trd lng="nl">vaag</trd>
  <trdgrp lng="sk">
    <trd>hmlist&yacute;</trd>,
    <trd>nejasn&yacute;</trd>,
    <trd>neur&ccaron;it&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x440;&#x43E;&#x437;&#x43F;&#x43B;&#x438;&#x432;&#x447;&#x430;&#x441;&#x442;&#x438;&#x439;</trd>,
    <trd>&#x43D;&#x435;&#x447;&#x456;&#x442;&#x43A;&#x438;&#x439;</trd>,
    <trd>&#x43D;&#x435;&#x44F;&#x441;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x43D;&#x435;&#x432;&#x438;&#x440;&#x430;&#x437;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>
</art>

