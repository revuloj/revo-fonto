<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: svag.xml,v 1.17 2024/03/25 07:28:38 revo Exp $">
<kap>
    <rad>svag</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="svag.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">POE</uzo>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="svag.0a.KOMUNE">
    <dif>
      <ref tip="dif" cel="preciz.mal0a.KOMUNE">Malpreciza</ref>,
      <ref tip="dif" cel="nebul.0a.FIG">nebula <sncref/></ref>:
      <ekz>
        la tolo <klr>[&#x2026;]</klr> &scirc;ajnis vasti&gcirc;i al la <tld/>aj kaj nebulaj muroj
        <fnt>
          <aut>J. Camacho</aut>,
          <vrk>La Majstro kaj Martinelli</vrk>,
          <lok>Iltis, 1993</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni vidis steleon el bronzo kun Greklingva surskribo, kvankam
        <tld/>e kaj apena&ubreve; plu legeble
        <fnt>
          <aut>Lukiano, trad. G. Berveling</aut>,
          <vrk><url ref="http://www.esperanto.gr/lit/els/rakontoj.pdf">veraj
            rakontoj</url> &#x2010; 2005</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> celas precizigi &ccirc;ion, kio &ccirc;e <nom>Zamenhof</nom>
        restis ambigua, <tld/>a a&ubreve; e&ccirc; rekte erara
        <fnt>
	<aut>Jorge Camacho</aut>
	<vrk><url ref="&Monato;/2000/006820.php">Plia libro biblia</url></vrk>
	<bib>Monato</bib>
	<lok>2000/08, p. 24</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj limoj inter realo kaj son&gcirc;o, inter efektivo kaj imago,
        inter frenezo kaj racio
        <fnt>
       <aut>Jorge Camacho</aut>
          <vrk><url ref="&Monato;/2000/006836.php">Pri realo kaj la malo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/09, p. 23</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj memoroj
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2003/007921.html">Ho,
          mistera pi!</url></vrk>
          <bib>Monato</bib>
          <lok>2003/06, p. 22</lok>
        </fnt>;
      </ekz>
      <ekz>
        la nocio pri dio mem estas tiom <tld/>a, tiom abstrakta, ke &ccirc;iu
        el
        ni devus la&ubreve;vole, libere ne atence retrovi sian propran bildon
        <fnt>
        <aut>Gilbert Ledon</aut>
         <vrk><url ref="&Monato;/2000/007018.php">Kulturo de milito</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 5</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>unscharf</trd>,
      <trd>vage</trd>,
      <trd>ungenau</trd>,
      <trd>verschwommen</trd>,
      <trd>konturlos</trd>,
      <trd>schemenhaft</trd>,
      <trd>unklar</trd>,
      <trd>nebul&ouml;s</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_v;&c_y;&c_r;&c_a;&c_z;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_ja;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_l;&c_y;&c_v;&c_ib;&c_s;&c_t;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_n;&c_je;&c_ja;&c_s;&c_je;&c_n;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_m;&c_i;&c_t;&c_a;<klr tip="amb">
    (&c_l;&c_o;&c_g;&c_i;&c_k;&c_a;)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mlhav&yacute;</trd>,
    <trd>nejasn&yacute;</trd>,
    <trd>neur&ccaron;it&yacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>vague</trd>,
    <trd>fuzzy<klr tip="amb"> (logic)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>impreciso</trd>,
    <trd>vago</trd>
  </trdgrp>
  <trd lng="fr">vague<klr> (adj.)</klr></trd>
  <trdgrp lng="it">
    <trd>vago<klr> (agg.)</klr></trd>,
    <trd>fuzzy<klr> (log. e inf.)</klr></trd>
  </trdgrp>
  <trd lng="nl">vaag</trd>
  <trdgrp lng="pl">
    <trd>nieprecyzyjny</trd>,
    <trd>rozmyty</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hmlist&yacute;</trd>,
    <trd>nejasn&yacute;</trd>,
    <trd>neur&ccaron;it&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&#x43E;&c_z;&#x43F;&#x43B;&c_i;&c_v;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_j;</trd>,
    <trd>&#x43D;&c_je;&c_ch;&c_ib;&c_t;&#x43A;&c_i;&c_j;</trd>,
    <trd>&#x43D;&c_je;&#x44F;&c_s;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43D;&c_je;&c_v;&c_i;&c_r;&c_a;&c_z;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6A21;&#x7CCA; <pr>m&#xF3;hu</pr></trd>,
    <trd>&#x542B;&#x7CCA; <pr>h&#xE1;nhu</pr></trd>,
    <trd>&#x6E3A;&#x832B; <pr>mi&#x1CE;om&#xE1;ng</pr></trd>,
    <trd>&#x7B3C;&#x7EDF; <pr>l&#x1D2;ngt&#x1D2;ng</pr></trd>,
    <trd>&#x7C60;&#x7D71; <pr>l&#x1D2;ngt&#x1D2;ng</pr></trd>,
    <trd>&#x9690;&#x7EA6; <pr>y&#x1D0;nyu&#x113;</pr></trd>,
    <trd>&#x96B1;&#x7D04; <pr>y&#x1D0;nyu&#x113;</pr></trd>
  </trdgrp>
</drv>
</art>

