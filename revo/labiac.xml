<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>labiac</rad>/oj
</kap>
<drv mrk="labiac.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Tradicia nomo de <ref tip="dif" cel="lamiac.0oj">lamiacoj</ref>:
      <ekz>
        la granda ponarda vespo &scirc;atas viziti florojn, precipe la kompozitacojn kaj la
        odorajn <tld/>ojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=289128"
              >Megascolia flavifrons</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_b;&c_a;&c_t;&c_k;&c_ib;</trd>
  <trdgrp lng="ca">
    <trd>labiades</trd>,
    <trd>lami&agrave;cies</trd>
  </trdgrp>
  <trd lng="de">Lippenbl&uuml;tler<klr> (Familie)</klr></trd>
  <trd lng="la">Labiatae</trd>
  <trdgrp lng="hu">
    <trd>ajakosak</trd>,
    <trd>&aacute;rvacsal&aacute;nf&eacute;l&eacute;k</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>labiadas <klr tip="amb">(bot.)</klr></trd>,
    <trd>lami&aacute;ceas <klr tip="amb">(bot.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_u;&c_b;&c_o;&c_c;&c_v;&c_je;&c_t;&c_n;&c_y;&c_je;</trd>
</drv>
</art>
