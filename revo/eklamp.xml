<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: eklamp.xml,v 1.21 2023/10/14 15:34:42 revo Exp $">
<kap>
  <rad>eklampsi</rad>/o
        <fnt>
          <bib>PV</bib>
        </fnt>
</kap>
<drv mrk="eklamp.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">Malsano</ref> de virino ofta dum la lasta
      tempo de &scirc;ia gravedeco a&ubreve; tuj poste kaj karakterizata per
      konvulsiaj, epilepsiaj spasmoj kaj per <ref tip="prt" cel="albumi1.0o">albuminurio</ref>:
      <ekz>
        ka&ubreve;zo por la nasko per la sekco estas anka&ubreve; diversaj gravaj malsanoj de la patrino kiel grava diabeto a&ubreve; <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=101541">Cezara sekco</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_l;&c_ja;&c_m;&c_p;&c_s;&c_ib;&c_ja;</trd>
  <trd lng="ca">ecl&agrave;mpsia</trd>
  <trdgrp lng="cs">
    <trd>d&rcaron;. bo&zcaron;ec</trd>,
    <trd>eklampsie</trd>,
    <trd>k&rcaron;e&ccaron;e t&ecaron;hotn&eacute;</trd>,
    <trd>psotn&iacute;k</trd>
  </trdgrp>
  <trd lng="de">Eklampsie</trd>
  <trd lng="en">eclampsia</trd>
  <trd lng="es">eclampsia</trd>
  <trd lng="fr">&eacute;clampsie</trd>
  <trd lng="hu">eklampszia</trd>
  <trd lng="id">eklampsia</trd>
  <trd lng="ja">&#x5B50;&#x7647;  <pr>&#x3057;&#x304B;&#x3093;</pr></trd>
  <trdgrp lng="pl">
    <trd>rzucawka<klr> ci&aogonek;&zdot;owa</klr></trd>,
    <trd>eklampsja</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>eclampsia</trd>,
    <trd>eclampse</trd>,
    <trd>ecl&acirc;mpsia</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_k;&c_l;&c_a;&c_m;&c_p;&c_s;&c_i;&c_ja;</trd>
  <trd lng="sk">eklampsia</trd>
  <trd lng="uk">&#x435;&#x43A;&#x43B;&#x430;&#x43C;&#x43F;&#x441;&#x456;&#x44F;</trd>
</drv>
</art>
