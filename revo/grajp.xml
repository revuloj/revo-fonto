<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>grajp</rad>/o</kap>
<drv mrk="grajp.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Kurba stango alligita al kilo.
    </dif>
  </snc>
  <trd lng="hu">t&odblac;kepipa</trd>
  <trd lng="nl">slemphout</trd>
  <trd lng="pt">trave curta ligada ao leme</trd>
</drv>

<drv mrk="grajp.antauxa0o">
  <kap>anta&ubreve;a <tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      <tld lit="G"/>o, kiu kunligas la kilon al la anta&ubreve;kilo.
    </dif>
  </snc>
  <trd lng="hu">orrt&odblac;kepipa</trd>
</drv>

<drv mrk="grajp.malantauxa0o">
  <kap>malanta&ubreve;a <tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      <tld lit="G"/>o, kiu kunligas la kilon al la malanta&ubreve;a kilo.
    </dif>
  </snc>
  <trd lng="hu">fart&odblac;kepipa</trd>
</drv>
</art>
