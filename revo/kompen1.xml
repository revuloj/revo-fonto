<drv mrk="kompen1.0e">
  <kap><tld/>e</kap>
  <snc mrk="kompen1.0e.KOMUNE">
    <dif>
      Kiel <tld/>o:
      <ekz>
        <klr>[la lo&gcirc;eja ilaro de kavaliro]</klr> konsistis nur el
        bototirilo kaj kombilo, sed <tld/>e li havis la plej belan
        kolumon en la mondo
        <fnt><bib>Fab3</bib>, <lok>Kolumo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u; &c_ja;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_ib; <ind>&c_k;&c_a;&c_m;&c_p;&c_e;&c_n;&c_s;&c_a;&c_c;&c_y;&c_ib;</ind></trd>
  <trdgrp lng="fr">
    <trd>en <ind>compensation</ind></trd>,
    <trd>en <ind>contrepartie</ind></trd>,
    <trd>en <ind>&eacute;change</ind></trd>,
    <trd>en <ind>retour</ind></trd>,
    <trd>en <ind>revanche</ind></trd>,
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&aacute;rp&oacute;tl&aacute;sul</trd>,
    <trd>ellent&eacute;telez&eacute;s&uuml;l</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4EE3;&#x511F;&#x3068;&#x3057;&#x3066;  <pr>&#x3060;&#x3044;&#x3057;&#x3087;&#x3046;&#x3068;&#x3057;&#x3066;</pr></trd>,
    <trd>&#x305D;&#x308C;&#x306B;&#x53CD;&#x3057;&#x3066;  <pr>&#x305D;&#x308C;&#x306B;&#x306F;&#x3093;&#x3057;&#x3066;</pr></trd>,
    <trd>&#x305D;&#x306E;&#x4EE3;&#x308F;&#x308A;  <pr>&#x305D;&#x306E;&#x304B;&#x308F;&#x308A;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>jako odszkodowanie</trd>,
    <trd>w zamian</trd>
  </trdgrp> 
  <trd lng="ro">compensare</trd>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_a; &c_v;&c_ib;&c_d;&c_sh;&#x43A;&#x43E;&c_d;&c_u;&c_v;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="kompen1.0epro">
  <kap><tld/>e pro</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="kompen1.0epro.KOMUNE">
    <dif>
      Kiel <tld/>o, repago, kontra&ubreve;dono al...:
      <ekz>
        <tld/>e pro &ccirc;io, kion &scirc;i suferis kaj travivis,
        &scirc;i tenis en la brakoj &jcirc;usnaskitan infaneton
        <fnt>
          <bib>Fab4</bib>,
          <lok>Historio el la dunoj</lok>
        </fnt>
      </ekz>
      <ekz>
        se iu batos okulon de sia sklavo a&ubreve; de sia sklavino
        kaj difektos &gcirc;in, tiam li forliberigu tiun
        <tld/>e pro la okulo
        <fnt>
          <bib>MT</bib>, <lok>&Eli; 21:26</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u; &c_ja;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_ib; <ind>&c_k;&c_a;&c_m;&c_p;&c_e;&c_n;&c_s;&c_a;&c_c;&c_y;&c_ib;</ind> &c_z;&c_a;&#x2026;</trd>
  <trdgrp lng="fr">
    <trd>en <ind>&eacute;change</ind> de</trd>,
    <trd>en <ind>compensation</ind> de</trd>,
    <trd>en <ind>contrepartie</ind> de</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&aacute;rp&oacute;tl&aacute;sul &eacute;rte</trd>,
    <trd>cser&eacute;be &eacute;rte</trd>
  </trdgrp>
  <trd lng="pl">w zamian za</trd>     
  <trd lng="ro">pro compensare</trd>
</drv>

<drv mrk="kompen1.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="kompen1.0ajxo.KOMUNE">
    <dif>
      <ref tip="dif" cel="kompen1.0o"><tld lit="K"/>o</ref>:
      <ekz>
        <klr>[fini&gcirc;is la milito]</klr>, sed amiko jam malnova
        ek&jcirc;aluzas, postulas <ctl><tld/>a&jcirc;on</ctl>
        <fnt><bib>VivZam</bib><lok>3</lok></fnt>;
      </ekz>
      <ekz>
        ka&scirc;ite sub anonima nomo, mi sendis monsumon sufi&ccirc;an,
        por liveri al &ccirc;iu el ili lar&gcirc;an <tld/>a&jcirc;on
        <fnt><aut>H. Vallienne</aut>,
        <vrk>&Ccirc;u li?</vrk>, <lok>1908</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_p;&c_e;&c_n;&c_s;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Ausgleich</trd>,
    <trd>Entsch&auml;digung</trd>,
    <trd>Kompensation</trd>,
    <trd>Ersatz</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>compensation</trd>,
    <trd>amends</trd>,
    <trd>reparation</trd>,
    <trd>satisfaction</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>compensation</trd>,
    <trd>contrepartie</trd>,
    <trd>correction</trd>,
