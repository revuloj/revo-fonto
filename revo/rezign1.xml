<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rezign1.xml,v 1.21 2019/10/20 07:10:15 revo Exp $">
<kap>
  <ofc>3</ofc><rad>rezignaci</rad>/i <fnt><bib>FK</bib></fnt>
</kap>

<drv mrk="rezign1.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="rezign1.0i.akcepti">
    <dif>
      Manke de propra forto <ref tip="super" cel="akcept.0i.ion">akcepti</ref>,
      <ref tip="super" cel="toler.0i.KOMUNE">toleri<sncref/></ref> ion, kion oni ne volas:
      <ekz>
        miloj da homoj finis la aferojn terajn kaj atendis <tld/>e la morton
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        laikoj surprizi&gcirc;as, sed <tld/>as, ne sciante, kion fari
        <fnt>
          <bib>Monato</bib>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url ref="https://www.monato.be/2004/008386.html">Sociolingvistoj &ccirc;u glacikoraj?</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        la studa&jcirc;o <klr>[&#8230;]</klr> <tld/>e devas konfirmi, ke nenio pliboni&gcirc;is
        <fnt>
          <bib>Monato</bib>
          <lok><url ref="http://steloj.de/esperanto/monato2/007070.html">http://steloj.de/esperanto/monato2/007070.html</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kapitu.0i.FIG">kapitulaci<sncref/></ref>,
      <ref cel="rezign.0i">rezigni</ref>,
      <ref cel="desper.0a">despera</ref>,
      <ref cel="las.al0i">allasi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>aufgeben</trd>,
    <trd>resignieren</trd>
  </trdgrp>
  <trd lng="en">give up</trd>
</drv>

<drv mrk="rezign1.0o">
  <kap><tld/>o</kap>
  <snc mrk="rezign1.0o.akcepto">
    <dif>
      Stato de tiu, kiu <tld/>as; kontra&ubreve;vola <ref tip="super" cel="akcept.0o.ago">akcepto</ref>:
      <ekz>
        en lingvo natura &ccirc;iu e&ccirc; plej granda efektiva malbona&jcirc;o <klr>[&#8230;]</klr> estas
        akceptata kun plena kontenteco a&ubreve; <tld/>o
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Oka Kongreso Esperantista en Krakow en la 11a de a&ubreve;gusto 1912</vrk>
        </fnt>;
      </ekz>
      <ekz>
        sur la viza&gcirc;o de Sfinkso estis nenia krueleco, sed <tld/>o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="rezign.0emo">rezignemo</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_k;&c_o;&c_r;&c_a;</trd>
  <trdgrp lng="bg">
    <trd>&c_r;&c_je;&c_z;&c_i;&c_g;&c_n;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_m;&c_i;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="ca">resignaci&oacute;</trd>
  <trdgrp lng="cs">
    <trd>malomyslnost</trd>,
    <trd>odevzdanost</trd>,
    <trd>rezignace</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Aufgabe</trd>,
    <trd>Resignation</trd>
  </trdgrp>
  <trd lng="en">resignation</trd>
  <trd lng="es">resignaci&oacute;n</trd>
  <trd lng="fr">r&eacute;signation</trd>
  <trdgrp lng="hu">
    <trd>lemond&aacute;s</trd>,
    <trd>belenyugv&aacute;s</trd>,
    <trd>rezign&aacute;ci&oacute;</trd>
  </trdgrp>
  <trd lng="nl">berusting</trd>
  <trdgrp lng="pl">
    <trd>rezygnacja</trd>,
    <trd>zniech&eogonek;cenie</trd>,
    <trd>brak <ind>woli</ind></trd>
  </trdgrp>
  <trd lng="pt">resigna&ccedil;&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_z;&c_i;&c_n;&c_mol;&c_ja;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_s;&c_m;&c_i;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odst&uacute;penie z miesta</trd>,
    <trd>rezign&aacute;cia</trd>,
    <trd>vzdanie sa</trd>,
    <trd>zrieknutie sa</trd>
  </trdgrp>
</drv>
</art>
