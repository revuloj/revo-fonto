<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rezign1.xml,v 1.30 2024/04/13 20:15:53 revo Exp $">
<kap>
  <ofc>3</ofc><rad>rezignaci</rad>/i
  <fnt><bib>JED</bib></fnt>
</kap>

<drv mrk="rezign1.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="rezign1.0i.akcepti">
    <dif>
      Manke de propra forto <ref tip="super" cel="akcept.0i.ion">akcepti</ref>,
      <ref tip="super" cel="toler.0i.KOMUNE">toleri<sncref/></ref> ion, kion oni ne volas:
      <ekz>
        miloj da homoj finis la aferojn terajn kaj atendis <tld/>e la morton
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        laikoj surprizi&gcirc;as, sed <tld/>as, ne sciante, kion fari
        <fnt>
          <bib>Monato</bib>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url ref="&Monato;/2004/008386.html">Sociolingvistoj &ccirc;u glacikoraj?</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        la studa&jcirc;o <klr>[&#x2026;]</klr> <tld/>e devas konfirmi, ke nenio pliboni&gcirc;is
        <fnt>
          <aut>Stefan Maul</aut>
            <vrk><url ref="&Monato;/2000/007070.php">Estimataj gelegantoj,</url></vrk>
            <bib>Monato</bib>
            <lok>2000/08, p. 3</lok>
          </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kapitu.0i.FIG">kapitulaci<sncref/></ref>,
      <ref cel="rezign.0i">rezigni</ref>,
      <ref cel="desper.0a">despera</ref>,
      <ref cel="las.al0i">allasi</ref>,
      <ref cel="ced.0i">cedi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_r;&c_y;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_r;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_g;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_c;&c_a; <klr>(&c_z;&c_mol; &c_l;&c_jo;&c_s;&c_a;&c_m;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>aufgeben</trd>,
    <trd>resignieren</trd>
  </trdgrp>
  <trd lng="en">give up</trd>
  <trdgrp lng="ja">
    <trd>&#x7518;&#x53D7;&#x3059;&#x308B;  <pr>&#x304B;&#x3093;&#x3058;&#x3085;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5FCD;&#x5F93;&#x3059;&#x308B;  <pr>&#x306B;&#x3093;&#x3058;&#x3085;&#x3046;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="pl">pogodzi&cacute; si&eogonek; z losem</trd>
  <trd lng="ro">accept&abreve;-l a&#x219;a cum este</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&c_i;&#x43C;&c_i;&c_r;&#x44F;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&c_z;&#x43C;&c_i;&c_r;&#x44F;&c_t;&c_i;&c_s;&#x44F; (&c_z; &c_ch;&c_i;&#x43C;-&#x43D;.)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4F4E;&#x5934;&#x4E27;&#x6C14; <pr>d&#x12B;t&#xF3;us&#xE0;ngq&#xEC;</pr></trd>,
    <trd>&#x4F4E;&#x982D;&#x55AA;&#x6C23; <pr>d&#x12B;t&#xF3;us&#xE0;ngq&#xEC;</pr></trd>,
    <trd>&#x8F9E;&#x53BB; <pr>c&#xED;q&#xF9;</pr></trd>,
    <trd>&#x8FAD;&#x53BB; <pr>c&#xED;q&#xF9;</pr></trd>,
    <trd>&#x6B7B;&#x5FC3; <pr>s&#x1D0;x&#x12B;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rezign1.0o">
  <kap><ofc>3</ofc><tld/>o</kap>
  <snc mrk="rezign1.0o.akcepto">
    <dif>
      Stato de tiu, kiu <tld/>as; kontra&ubreve;vola <ref tip="super" cel="akcept.0o.ago">akcepto</ref>:
      <ekz>
        en lingvo natura &ccirc;iu e&ccirc; plej granda efektiva malbona&jcirc;o <klr>[&#x2026;]</klr> estas
        akceptata kun plena kontenteco a&ubreve; <tld/>o
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Oka Kongreso Esperantista en Krak&oacute;w en la 11a de a&ubreve;gusto 1912</vrk>
        </fnt>;
      </ekz>
      <ekz>
        sur la viza&gcirc;o de Sfinkso estis nenia krueleco, sed <tld/>o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="rezign.0emo">rezignemo</ref>
    <refgrp tip="vid">
      <ref cel="apati.0o">apatio</ref>,
      <ref cel="indife.0eco">indiferenteco</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_k;&c_o;&c_r;&c_a;</trd>
  <trdgrp lng="bg">
    <trd>&c_r;&c_je;&c_z;&c_i;&c_g;&c_n;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_m;&c_i;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="ca">resignaci&oacute;</trd>
  <trdgrp lng="cs">
    <trd>malomyslnost</trd>,
    <trd>odevzdanost</trd>,
    <trd>rezignace</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Aufgabe <klr>(Resignation)</klr></trd>,
    <trd>Resignation</trd>,
    <trd><ofc>3</ofc>Entsagung</trd>,
    <trd><ofc>3</ofc>Ergebung</trd>
  </trdgrp>
  <trd lng="en"><ofc>3</ofc>resignation</trd>
  <trd lng="es"><ofc>3</ofc>resignaci&oacute;n</trd>
  <trd lng="fr"><ofc>3</ofc>r&eacute;signation</trd>
  <trdgrp lng="hu">
    <trd>lemond&aacute;s</trd>,
    <trd>belenyugv&aacute;s</trd>,
    <trd>rezign&aacute;ci&oacute;</trd>
  </trdgrp>
  <trd lng="it"><ofc>3</ofc>rassegnazione</trd>
  <trdgrp lng="ja">
    <trd>&#x7518;&#x53D7;  <pr>&#x304B;&#x3093;&#x3058;&#x3085;</pr></trd>,
    <trd>&#x5FCD;&#x5F93;  <pr>&#x306B;&#x3093;&#x3058;&#x3085;&#x3046;</pr></trd>,
    <trd>&#x8AE6;&#x5FF5;  <pr>&#x3066;&#x3044;&#x306D;&#x3093;</pr></trd>,
    <trd>&#x3042;&#x304D;&#x3089;&#x3081;</trd>
  </trdgrp>
  <trd lng="nl">berusting</trd>
  <trdgrp lng="pl">
    <trd><ofc>3</ofc>rezygnacja</trd>,
    <trd>zniech&eogonek;cenie</trd>,
    <trd>brak <ind>woli</ind></trd>
  </trdgrp>
  <trd lng="pt"><ofc>3</ofc>resigna&ccedil;&atilde;o</trd>
  <trdgrp lng="ro">
    <trd>retragere</trd>,
    <trd>descurajare</trd>,
    <trd>lipsa de vointa</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_z;&c_i;&c_n;&c_mol;&c_ja;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_s;&c_m;&c_i;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odst&uacute;penie z miesta</trd>,
    <trd>rezign&aacute;cia</trd>,
    <trd>vzdanie sa</trd>,
    <trd>zrieknutie sa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43C;&c_i;&c_r;&c_je;&#x43D;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&c_v;&c_ib;&c_d;&#x43C;&#x43E;&c_v;&c_a; &c_v;&c_ib;&c_d; &c_b;&#x43E;&c_r;&#x43E;&c_t;&#x44C;&c_b;&c_i; (&c_z; &c_ch;&c_i;&#x43C;-&#x43D;.)</trd>,
    <trd>&#x43F;&c_r;&c_i;&#x43C;&c_i;&c_r;&c_je;&#x43D;&#x43D;&#x44F; &c_z; &c_d;&#x43E;&#x43B;&c_je;&#x44E;</trd>
  </trdgrp>
</drv>
</art>
