<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc> <rad>rezignaci</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="rezign1.0o">
  <kap><tld/>o</kap>
  <snc mrk="rezign1.0o.KOMUNE">
    <dif>
      Cedema submeti&gcirc;o; senrezista, senprotesta kaj senplenda 
      akcepto a&ubreve; forlaso pri io:
      <ekz>
        en lingvo natura &ccirc;iu e&ccirc; plej granda efektiva 
        malbona&jcirc;o estas akceptata kun plena kontenteco a&ubreve; 
        <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i pri sia sorto.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="rezign.0emo">rezignemo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_k;&c_o;&c_r;&c_a;</trd>
  <trdgrp lng="bg">
    <trd>&c_r;&c_je;&c_z;&c_i;&c_g;&c_n;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_m;&c_i;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="ca">resignaci&oacute;</trd>
  <trd lng="de">Resignation</trd>
  <trd lng="en">resignation</trd>
  <trd lng="es">resignaci&oacute;n</trd>
  <trd lng="fr">r&eacute;signation</trd>
  <trdgrp lng="hu">
    <trd>lemond&aacute;s</trd>,
    <trd>belenyugv&aacute;s</trd>,
    <trd>rezign&aacute;ci&oacute;</trd>
  </trdgrp>
  <trd lng="nl">berusting</trd>
  <trd lng="pt">resigna&ccedil;&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_z;&c_i;&c_n;&c_mol;&c_ja;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_s;&c_m;&c_i;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
