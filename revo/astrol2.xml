    <trd>&a_A;&a_kh;&a_t;&a_r;&a_b;&f_y;&a_n;</trd>,
    <trd>&a_s;&a_t;&a_A;&a_r;&a_h;&#8204;&a_b;&f_y;&a_n;</trd>,
