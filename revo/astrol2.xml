<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>astrologi</rad>/o
</kap>
<drv mrk="astrol2.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      &Hcirc;imera arto diveni la estonton la&ubreve; la astroj.
    </dif>
  </snc>
  <trd lng="en">astrology</trd>
  <trd lng="es">astrolog&iacute;a</trd>
  <trd lng="fr">astrologie</trd>
  <trdgrp lng="hu">
    <trd>asztrol&oacute;gia</trd>,
    <trd>csillagj&oacute;sl&aacute;s</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_s;&c_t;&c_r;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="sk">astrol&oacute;gia</trd>
</drv>

<drv mrk="astrol2.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Specialisto pri <tld/>o.
    </dif>
    <refgrp tip="sin">
      <ref cel="astrol1.0o">astrologo</ref>.
    </refgrp>
  </snc>
  <trd lng="en">astrologist</trd>
  <trd lng="es">astr&oacute;logo</trd>
  <trd lng="fr">astrologue</trd>
  <trdgrp lng="hu">
    <trd>asztrol&oacute;gus</trd>,
    <trd>csillagj&oacute;s</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_s;&c_t;&c_r;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="sk">astrol&oacute;g</trd>
</drv>
</art>
