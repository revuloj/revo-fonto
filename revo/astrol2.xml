<drv mrk="astrol2.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      &Hcirc;imera arto diveni la estontecon la&ubreve; la astroj:
      <ekz>
        <tld/>o estas nura supersti&ccirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2009/009682.php">La Pendantaj &Gcirc;ardenoj estis
          son&gcirc;o</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        du litovaj fizikistoj <klr>[&#8230;]</klr> penis pruvi la kontra&ubreve;sciencecon de la fundamento de
        <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2007/008953.php">Astrologoj progresas</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Anka&#365; eblas diri <ke>steldiveno</ke>, <ke>stela&#365;gurado</ke>, <ke>astrodivenado</ke>.
  </rim>

  <trd lng="be">&c_a;&c_s;&c_t;&c_r;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="cs">astrologie</trd>
  <trdgrp lng="de">
    <trd>Astrologie</trd>,
    <trd>Sterndeutung</trd>,
    <trd>Sterndeuterei</trd>
  </trdgrp>
  <trd lng="en">astrology</trd>
  <trd lng="es">astrolog&iacute;a</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_kh;&a_t;&a_r;&a_b;&f_y;&a_n;&f_y;</trd>,
    <trd>&a_s;&a_t;&a_A;&a_r;&a_h;&#8204;&a_b;&f_y;&a_n;&f_y;</trd>,
    <trd>&a_T;&a_A;&a_l;&a_ayn;&#8204;&a_b;&f_y;&a_n;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">astrologie</trd>
  <trd lng="he">&#1488;&#1463;&#1505;&#1496;&#1512;&#1493;&#1465;&#1500;&#1493;&#1465;&#1490;&#1460;&#1497;&#1464;&#1492;</trd>
  <trdgrp lng="hu">
    <trd>asztrol&oacute;gia</trd>,
    <trd>csillagj&oacute;sl&aacute;s</trd>
  </trdgrp>
  <trd lng="it">astrologia</trd>
  <trd lng="ja">&#21344;&#26143;&#34899;  <pr>&#12379;&#12435;&#12379;&#12356;&#12376;&#12421;&#12388;</pr></trd>
  <trd lng="pl">astrologia</trd>
  <trd lng="ru">&c_a;&c_s;&c_t;&c_r;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>astrol&oacute;gia</trd>,
    <trd>astrol&oacute;gia</trd>,
    <trd>ve&scaron;tenie z hviezd</trd>
  </trdgrp>
  <trd lng="uk">&c_a;&c_s;&c_t;&c_r;&#1086;&#1083;&#1086;&c_g;&c_ib;&#1103;</trd>
</drv>

