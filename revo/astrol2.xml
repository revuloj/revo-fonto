<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: astrol2.xml,v 1.21 2021/03/04 17:56:54 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>astrologi</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="astrol2.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      &Hcirc;imera arto diveni la estontecon la&ubreve; la astroj:
      <ekz>
        <tld/>o estas nura supersti&ccirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="https://www.monato.be/2009/009682.php">La Pendantaj &Gcirc;ardenoj estis son&gcirc;o</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        du litovaj fizikistoj <klr>[&#x2026;]</klr> penis pruvi la kontra&ubreve;sciencecon de la fundamento de <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="https://www.monato.be/2007/008953.php">Astrologoj progresas</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_t;&c_r;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="cs">astrologie</trd>
  <trd lng="de">Astrologie</trd>
  <trd lng="en">astrology</trd>
  <trd lng="es">astrolog&iacute;a</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_kh;&a_t;&a_r;&a_b;&f_y;&a_n;&f_y;</trd>,
    <trd>&a_s;&a_t;&a_A;&a_r;&a_h;&zwnj;&a_b;&f_y;&a_n;&f_y;</trd>,
    <trd>&a_T;&a_A;&a_l;&a_ayn;&zwnj;&a_b;&f_y;&a_n;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">astrologie</trd>
  <trd lng="he">&#x5D0;&#x5B7;&#x5E1;&#x5D8;&#x5E8;&#x5D5;&#x5B9;&#x5DC;&#x5D5;&#x5B9;&#x5D2;&#x5B4;&#x5D9;&#x5B8;&#x5D4;</trd>
  <trdgrp lng="hu">
    <trd>asztrol&oacute;gia</trd>,
    <trd>csillagj&oacute;sl&aacute;s</trd>
  </trdgrp>
  <trd lng="it">astrologia</trd>
  <trd lng="pl">astrologia</trd>
  <trd lng="ru">&c_a;&c_s;&c_t;&c_r;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>astrol&oacute;gia</trd>,
    <trd>astrol&oacute;gia</trd>,
    <trd>ve&scaron;tenie z hviezd</trd>
  </trdgrp>
</drv>

<drv mrk="astrol2.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Specialisto pri <tld/>o.
    </dif>
    <refgrp tip="sin">
      <ref cel="astrol1.0o">astrologo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_t;&c_r;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trdgrp lng="cs">
    <trd>astrolog</trd>,
    <trd>hv&ecaron;zdopravec</trd>
  </trdgrp>
  <trd lng="de">Astrologe</trd>
  <trd lng="en">astrologist</trd>
  <trd lng="es">astr&oacute;logo</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_kh;&a_t;&a_r;&a_b;&f_y;&a_n;</trd>,
    <trd>&a_s;&a_t;&a_A;&a_r;&a_h;&zwnj;&a_b;&f_y;&a_n;</trd>,
    <trd>&a_T;&a_A;&a_l;&a_ayn;&zwnj;&a_b;&f_y;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">astrologue</trd>
  <trdgrp lng="hu">
    <trd>asztrol&oacute;gus</trd>,
    <trd>csillagj&oacute;s</trd>
  </trdgrp>
  <trd lng="it">astrologo</trd>
  <trd lng="pl">astrolog</trd>
  <trd lng="ru">&c_a;&c_s;&c_t;&c_r;&c_o;&c_l;&c_o;&c_g;</trd>
  <trdgrp lng="sk">
    <trd>astrol&oacute;g</trd>,
    <trd>astrol&oacute;g</trd>
  </trdgrp>
</drv>
</art>
