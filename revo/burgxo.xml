<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: burgxo.xml,v 1.19 2021/03/05 18:27:14 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bur&gcirc;on</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="burgxo.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="burgxo.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      &Scirc;vela volva&jcirc;o de vegeta&jcirc;o, el kiu elvolvi&gcirc;as la
      folioj kaj la floroj. 
    </dif>
    <refgrp tip="vid">
      <ref cel="buton.0o.BOT">butono<sncref/></ref>,
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_p;&c_u;&c_p;&c_y;&c_sh;&c_k;&c_a;</trd>,
      <trd>&c_p;&c_o;&c_k;&c_a;&c_w;&c_k;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>bimb&oacute;</trd>,
      <trd>r&uuml;gy</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>p&aogonek;k</trd>,
      <trd>p&aogonek;czek</trd>
    </trdgrp>
    <trd lng="pt">bot&atilde;o<klr> (de flor)</klr></trd>
    <trd lng="ru">&c_p;&c_o;&c_ch;&c_k;&c_a; <klr>(&c_n;&c_a;
    &c_d;&c_je;&c_r;&c_je;&c_v;&c_mol;&c_ja;&c_h;)</klr></trd>
  </snc>
  <snc mrk="burgxo.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Io ekaperanta, komenci&gcirc;anta, ne plene matura:
      <ekz>
        ofte la senco de iu vorto diferencas de alia nur per la bildo,
        kiun &gcirc;i entenas <tld/>e.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_r;&c_o;&c_d;&c_a;&c_k;</trd>,
      <trd>&c_p;&c_a;&c_ch;&c_a;&c_t;&c_a;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>cs&iacute;ra</trd>,
      <trd>mag<klr tip="ind"> (kezdem&eacute;ny)</klr></trd>
    </trdgrp>
    <trd lng="pl">zarodek</trd>
  </snc>
  <refgrp tip="vid">
    <ref cel="gxerm.0o">&gcirc;ermo</ref>
  </refgrp>
  <trd lng="bg">&c_p;&c_malmol;&c_p;&c_k;&c_a;</trd>
  <trd lng="de">Knospe</trd>
  <trd lng="en">bud</trd>
  <trd lng="fa">&a_j;&a_w;&a_A;&a_n;&a_h;</trd>
  <trd lng="fr">bourgeon</trd>
  <trd lng="he">&#x5E0;&#x5D9;&#x5E6;&#x5DF;</trd>
</drv>

<drv mrk="burgxo.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="burgxo.0i.BOT">
    <dif>
      Naski <tld/>ojn, ekkomenci montri foliojn a&ubreve; florojn. 
    </dif>
    <trdgrp lng="de">
       <trd>knospen</trd>,
       <trd>sprie&szlig;en</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kics&iacute;r&aacute;zik</trd>,
      <trd>kisarjad</trd>
    </trdgrp>
    <trd lng="pl">wypuszcza&cacute; <ind>p&aogonek;ki</ind></trd>
  </snc>
  <snc mrk="burgxo.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ekkomenci prosperi:
      <ekz>
        vigla esperantista movado <tld/>as nun en la urbo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bril">ekbrili</ref>,
      <ref cel="flor.ek0i">ekflori</ref>,
      <ref cel="gxerm.0i">&gcirc;ermi</ref>
    </refgrp>
    <trd lng="de">aufbl&uuml;hen</trd>
  </snc>
  <trd lng="en">bud</trd>
  <trd lng="fa">&a_j;&a_w;&a_A;&a_n;&a_h; &a_z;&a_d;&a_n;</trd>
  <trd lng="fr">bourgeonner</trd>
  <trd lng="he">&#x5DC;&#x5D4;&#x5E0;&#x5E5;</trd>
  <trdgrp lng="hu">
    <trd>kibontakozik</trd>,
    <trd>kivir&aacute;gzik</trd>
  </trdgrp>
</drv>
</art>
