<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nauxt.xml,v 1.9 2022/03/24 10:41:39 revo Exp $">
<kap>
  <rad>na&ubreve;t</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="nauxt.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="nauxt.0o.intervalo">
    <dif>
      <ref tip="super" cel="interv.0o.MUZ">Intervalo</ref>
      inter la du ekstremaj el na&ubreve; sinsekvaj tonoj de diatona gamo.
      <ekz>
        Goldberg-variacioj &dash; Varia&jcirc;o 27 &dash; Kanono je la <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=32931">Goldberg-variacioj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        Art Tatum <klr>[&#x2026;]</klr> rafinis la harmoniajn eblecojn, 
        enigante la grandajn intervalojn <tld/>o, dekunuto kaj dektrito en siajn harmoniojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=367373">Art Tatum</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">n&oacute;na<klr tip="amb"> (hangk&ouml;z)</klr></trd>    
    <trd lng="pl">nona</trd>
  </snc>
  <snc mrk="nauxt.0o.de_toniko">
    <dif>
      Na&ubreve;a tono de gamo.
    </dif>
    <trd lng="hu">n&oacute;na<klr tip="amb"> (kilencedik hang)</klr></trd>
    <trd lng="pl">nona</trd>
  </snc>
  <refgrp tip="sin">
    <ref cel="nauxn.0o">na&ubreve;no</ref>
  </refgrp>
  <trd lng="be">&c_n;&c_o;&c_n;&c_a;<klr>
(&c_m;&c_u;&c_z;&c_y;&c_ch;&c_n;&c_y;
&c_ib;&c_n;&c_t;&c_e;&c_r;&c_v;&c_a;&c_l;)</klr></trd>
  <trd lng="de">None</trd>
  <trd lng="en">ninth</trd>
  <trdgrp lng="fr">
    <trd>neuvi&egrave;me<klr> (intervalle musical)</klr></trd>
  </trdgrp>
  <trd lng="nl">none</trd>
  <trd lng="pt">nona</trd>
  <trd lng="ru">&c_n;&c_o;&c_n;&c_a;<klr>
(&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;
&c_i;&c_n;&c_t;&c_je;&c_r;&c_v;&c_a;&c_l;)</klr></trd>
  <trd lng="sv">nona</trd>
  <trd lng="uk">&#x43E;&#x434;&#x43D;&#x430; &#x434;&#x435;&#x432;&#x2019;&#x44F;&#x442;&#x430;</trd>
</drv>
</art>

