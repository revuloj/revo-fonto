<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: sor1.xml,v 1.5 2023/04/24 05:28:20 afido Exp $">
    <kap>
      <rad>sor</rad>/i
    </kap>

<drv mrk="sor1.0i">
  <kap><tld/>i</kap>
  <snc mrk="sor1.0i.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      Eluzante termikan fluon, <ref tip="super" cel="glis.0i">glisi</ref> supren:
      <ekz>
        alten <tld/>as, klifojn boras
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2004/008066.html">Retrorigardo al intensa vivo</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        el la profundo <tld/>as tristeco
        <fnt>
          <bib>Monato</bib>
          <aut>Raita Pyh&auml;l&auml;</aut>
          <vrk><url ref="&Monato;/2005/008685.php">Kesto de viveroj</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        liaj tropoj foje forlasas la limojn de la prudento kaj <tld/>as propraimpete
        <fnt>
          <bib>Monato</bib>
          <aut>Geraldo Mattos</aut>
          <vrk><url ref="&Monato;/2008/009422.php">Rezulto de preseraro</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la ekonomio en Usono <tld/>is <klr>(vd <ref cel="konjun3.0o.EKON">konjunkturo</ref>)</klr>
        en 1999 sen malfavoraj sekvoj por la inflacio
        <fnt>
	<aut>Marc Vanden Bempt</aut>
	<vrk><url ref="&Monato;/2000/006821.php">E&ubreve;ro unujara</url></vrk>
	<bib>Monato</bib>
	<lok>2000/03, p. 14</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="gir.0i">giri</ref>,
       <ref cel="spiral.0i">spirali</ref>
    </refgrp>
    <trd lng="de">aufsteigen <klr>(Segelflug)</klr></trd>
    <trd lng="en">soar</trd>
  </snc>
  <trd lng="uk">&c_z;&c_d;&c_ib;&c_j;&#x43C;&c_a;&c_t;&c_i;&c_s;&#x44F; &#x43D;&c_a; &c_v;&c_i;&c_s;&c_h;&c_ib;&c_d;&#x43D;&#x43E;&#x43C;&c_u;
  &#x43F;&#x43E;&c_t;&#x43E;&c_c;&c_ib;</trd>
</drv>

  </art>

