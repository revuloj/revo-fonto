<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pont</rad>/o
</kap>
<drv mrk="pont.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Konstruo ebliganta trairon de rivero, fosa&jcirc;o, kavo ks:
      <ekz>
        &scirc;tona, fera, ligna <tld/>o;
      </ekz>
      <ekz>
        la arka&jcirc;oj de <tld/>o;
      </ekz>
      <ekz>
        turni&gcirc;anta <tld/>o;
      </ekz>
      <ekz>
        pend<tld/>o
        <klr>(kies planko pendas de feraj &scirc;nuregoj)</klr>;
      </ekz>
      <ekz>
        lev<tld/>o
        <klr>(levebla <tld/>o)</klr>;
      </ekz>
      <ekz>
        bruligi post si la <tld/>ojn<fnt>Z</fnt>
        <klr>(malebligi al si &ccirc;iun &scirc;an&gcirc;on de decido.
        <refgrp tip="vid">
          <ref cel="kart.0o">karto</ref>,
          <ref cel="kub.0o">kubo</ref>,
          <ref cel="pas.trans0i">transpasi</ref>
        </refgrp>)
        </klr>;
      </ekz>
      <ekz>la afero ne staras sur pinto de <tld/>o
        <klr>(ne ur&gcirc;as)</klr>
<fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
    <trd lng="tr">k&ouml;pr&uuml;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Interkomunikilo:
      <ekz>
        ni deziras krei ligantan <tld/>on inter la gentoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        Esperanto estas <tld/>o trans &ccirc;iuj lingvaj limoj;
      </ekz>
      <ekz>
        Hungarlando estas <tld/>o inter oriento kaj okcidento;
      </ekz>
      <ekz>
        tiu procedo prezentos <tld/>on de transiro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &scirc;i ser&ccirc;is <tld/>on (transiron, pretekston)
        al pli intima konversacio.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_o;&c_s;&c_t;</trd>
  <trd lng="de">Br&uuml;cke</trd>
  <trd lng="en">bridge</trd>
  <trd lng="nl">brug</trd>
  <trd lng="fr">pont</trd>
  <trd lng="hu">h&iacute;d</trd>
  <trd lng="pl">most</trd>
  <trd lng="ro">pod</trd>
  <trd lng="ru">&c_m;&c_o;&c_s;&c_t;</trd>
  <trd lng="sv">bro</trd>
</drv>

<drv mrk="pont.super0igitrans0igi">
  <kap>super<tld/>igi, trans<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Meti <tld/>on super io:
      <ekz>
        super<tld/>igi kanalon;
      </ekz>
      <ekz>
        trans<tld/>igi fendegon.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_b;&c_u;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; <ind>&c_m;&c_o;&c_s;&c_t;</ind></trd>
    <trd lng="de">&uuml;berbr&uuml;cken</trd>
    <trdgrp lng="fr">
      <trd>ponter</trd>
    </trdgrp>
    <trd lng="ru">&c_n;&c_a;&c_v;&c_je;&c_s;&c_t;&c_i; <ind>&c_m;&c_o;&c_s;&c_t;</ind></trd>
  </snc>
  <snc mrk="pont.trans0igi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Transpasi malhelpa&jcirc;on, baron:
      <ekz>
        trans<tld/>igi malfacila&jcirc;on.
      </ekz>
    </dif>
    <trd lng="de">&uuml;berwinden</trd>
    <trdgrp lng="fr">
      <trd>surmonter</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_r;&c_je;&c_o;&c_d;&c_o;&c_l;&c_je;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="hu">&aacute;thidal</trd>
  <refgrp tip="sin">
    <ref cel="venk.0i.FIG">venki</ref>
  </refgrp>
</drv>
</art>
