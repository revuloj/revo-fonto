<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mikronezi</rad>/o
</kap>

<drv mrk="mikronez.0o">
  <kap>
    <tld lit="M"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="mikronez.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Meza parto de la <ref tip="malprt" cel="oceani.O0o">Oceanio</ref>
      al kiu apartenas &ccirc;. 1500 insuloj.
    </dif>
    <trdgrp lng="pl">
      <trd>Mikronezja</trd>
    </trdgrp>
  </snc>
  <snc mrk="mikronez.0o.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      &Scirc;tato ampleksanta plejparton de la <ref tip="malprt" 
      cel="mikronez.0o.GEOG">Mikronezio</ref> kun &ccirc;efurbo
      Palikiro.
      ISO-3166 kodo: FM.
    </dif>
    <trdgrp lng="pl">
      <trd>Mikronezja</trd>,
      <trd>Sfederowane Stany <ind>Mikronezja</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

