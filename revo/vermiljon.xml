<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vermiljon.xml,v 1.17 2024/09/27 09:31:08 revo Exp $">
<kap>
  <rad>vermiljon</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="vermiljon.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Dense <ref tip="super" cel="rugx.0a">ru&gcirc;a</ref> kun oran&gcirc;eta
      nuanco, kiel <ref tip="vid" cel="cinabr.0o">cinabro</ref>:
      <ekz>
        jen marmora &scirc;tuparo borderita per <tld/>aj balustradoj
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 5a, volumo 1a, p.
        65a</lok></fnt>;
      </ekz>
      <ekz>
        oni foruzis &ccirc;iun farbon, kiu jam sur&scirc;ipis, &ccirc;u bruna, verda, viola, <tld/>a,
        flava a&ubreve; oran&gcirc;a
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 19</lok>
        </fnt>;
      </ekz>
      <ekz>
        hiponimoj <klr>[de]</klr> ru&gcirc;a estas <klr>[&#8230;]</klr> karmezina, skarlata, punca,
        karmina, <tld/>a, eble anka&ubreve; al fuksina, rufa
        <fnt>
          <bib>LAE</bib>
          <lok>5. Leksiko Kaj Semantiko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la horizonto estis vermiljona, fajre-ru&gcirc;a, incendie-rufa
        <fnt>
          <bib>Monato</bib>
          <aut>Geraldo Mattos</aut>
          <vrk>Kota Akvo: Konstanten Krysakov</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_r;&c_n;&c_y;</trd>,
    <trd>&c_c;&c_y;&c_n;&c_o;&c_b;&c_r;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_ja;&c_r;&c_k;&c_a;-&c_ch;&c_y;&c_r;&c_v;&c_o;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">vermell&oacute;<klr> (adj.)</klr></trd>
  <trd lng="en">vermilion</trd>
  <trdgrp lng="fr">
    <trd>vermillon<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>cin&oacute;berpiros</trd>,
    <trd>cin&oacute;berv&ouml;r&ouml;s</trd>
    <trd>cin&oacute;bersz&iacute;n&udblac;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#26417;&#x306E;  <pr>&#12375;&#12421;&#x306E;</pr></trd>,
    <trd>&#26417;&#33394;&#x306E;  <pr>&#12375;&#12421;&#12356;&#x308D;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="pl">cynobrowy</trd>
  <trdgrp lng="ru">
    <trd>&c_ja;&c_r;&c_k;&c_o;-&c_k;&c_r;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_k;&c_i;&c_n;&c_o;&c_v;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="uk">&c_ja;&c_s;&c_k;&c_r;&c_a;&c_v;&c_o;-&c_ch;&c_je;&c_r;&c_v;&c_o;&c_n;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x4E2D;&#x56FD;&#x7EA2; <pr>Zh&#x14D;nggu&#xF3;h&#xF3;ng</pr></trd>,
    <trd>&#x4E2D;&#x570B;&#x7D05; <pr>Zh&#x14D;nggu&#xF3;h&#xF3;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="vermiljon.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="V"/>a <ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">koloro</ref>:
      <ekz>
        en la grundo aperas senfina gamo de kolornuancoj: de helega flavsafrano &gcirc;is
        vinfe&ccirc;kolora purpurviolo, kaj de karmino &gcirc;is <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Robert Roze</aut>
          <vrk>Francio: Flagrantaj koloroj</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_r;</trd>,
    <trd>&c_c;&c_y;&c_n;&c_o;&c_b;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">vermell&oacute;<klr> (subst.)</klr></trd>
  <trdgrp lng="cs">
    <trd>rum&ecaron;lka <klr>(ohniv&ecaron; &ccaron;erven&eacute; barvivo)</klr></trd>,
    <trd>rum&ecaron;lkov&aacute; &ccaron;erve&ncaron;</trd>
  </trdgrp>
  <trd lng="de">Zinnoberrot</trd>
  <trd lng="en">vermilion</trd>
  <trdgrp lng="fr">
    <trd>vermillon<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="hu">cin&oacute;bersz&iacute;n</trd>
  <trdgrp lng="ja">
    <trd>&#26417;  <pr>&#12375;&#12421;</pr></trd>,
    <trd>&#x30D0;&#x30FC;&#x30DF;&#x30EA;&#x30AA;&#x30F3;</trd>
  </trdgrp>
  <trd lng="pl">cynober<klr> (kolor)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_ja;&c_r;&c_k;&c_o;-&c_k;&c_r;&c_a;&c_s;&c_n;&c_y;&c_j;
    &c_c;&c_v;&c_je;&c_t;</trd>,
    <trd>&c_c;&c_v;&c_je;&c_t;
    <ind>&c_k;&c_i;&c_n;&c_o;&c_v;&c_a;&c_r;&c_i;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>2.rumelkov&aacute; &ccaron;erve&ncaron; <klr>(farba)</klr></trd>,
    <trd>rumelka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_je;&c_r;&c_m;&c_ib;&c_l;&c_mol;&c_j;&c_o;&c_n;</trd>,
    <trd>&c_k;&c_ib;&c_n;&c_o;&c_v;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="zh">&#x4E39; <pr>d&amacron;n</pr></trd>
</drv>
</art>
