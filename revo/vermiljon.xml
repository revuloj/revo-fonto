<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>vermiljon</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="vermiljon.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Dense ru&gcirc;a kun oran&gcirc;eta nuanco, kiel <ref tip="vid"
      cel="cinabr.0o">cinabro</ref>:
      <ekz>
        jen marmora &scirc;tuparo borderita per <tld/>aj balustradoj
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 5a, p. 65a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>vermillon<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="vermiljon.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="V"/>a <ref tip="super" cel="kolor.0o.propre">koloro</ref>.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>vermillon<klr> (subst.)</klr></trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
-->
</vortaro>