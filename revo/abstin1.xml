<kap>
  <rad>abstinenc</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="abstin1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="abstin.0ado">Abstinado</ref>:
      <ekz>
        la novplatonismano Porfirio de Tiro (3-a jarcento), en &leftquot;Pri <tld/>o&rightquot;, 
        ligas vegetarismon al misterokulto de Kreto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=220842">Nutrado en antikva Grekujo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_s;&c_t;&c_r;&c_y;&c_m;&c_a;&c_n;&c_mol;&c_n;&c_je;
    </trd>,
    <trd>&c_a;&c_b;&c_s;&c_t;&c_y;&c_n;&c_e;&c_n;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
  <trd lng="bg">&c_v;&c_malmol;&c_z;&c_d;&c_malmol;&c_r;&c_zh;&c_a;&c_n;&c_je;</trd>
  <trd lng="ca">abstin&egrave;ncia</trd>
  <trdgrp lng="cs">
    <trd>abstinence</trd>,
    <trd>zdr&zcaron;enlivost</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Abstinenz</trd>,
      <trd>Enthaltsamkeit</trd>
  </trdgrp>
  <trd lng="en">abstinence</trd>
  <trd lng="es">abstinencia</trd>
  <trdgrp lng="fr">
    <trd>abstinence</trd>,
    <trd>privation<klr tip="ind"> (abstinence)</klr></trd>,
    <trd>asc&egrave;se</trd>
  </trdgrp>
  <trd lng="he">&#x5E4;&#x5BC;&#x5B0;&#x5E8;&#x5B4;&#x5D9;&#x5E9;&#x5C1;&#x5D5;&#x5BC;&#x5EA;</trd>
  <trdgrp lng="hu">
    <trd>absztinencia</trd>,
    <trd>&ouml;nmegtart&oacute;ztat&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>onthouding</trd>,
    <trd>abstinentie</trd>
  </trdgrp>
  <trd lng="it">astinenza</trd>
  <trdgrp lng="pl">
    <trd>abstynencja</trd>,
    <trd>pow&sacute;ci&aogonek;gliwo&sacute;&cacute;</trd>
  </trdgrp>
  <trd lng="pt">abstin&ecirc;ncia</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_z;&c_d;&c_je;&c_r;&c_zh;&c_a;&c_n;&c_i;&c_je;
    </trd>,
    <trd>&c_a;&c_b;&c_s;&c_t;&c_i;&c_n;&c_je;&c_n;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>abstinencia</trd>,
    <trd>zdr&zcaron;anie sa</trd>
  </trdgrp>
</drv>
</art>
