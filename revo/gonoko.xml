<art mrk="$Id: gonoko.xml,v 1.18 2023/10/15 18:38:19 revo Exp $">
<kap>
  <rad>gonokok</rad>/o
</kap>
<drv mrk="gonoko.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref cel="gram1.0negativa">Gramnegativa</ref>, movi&gcirc;kapabala 
      <ref tip="super" cel="bakter.0o">bakterio</ref>
      ka&ubreve;zanta <ref cel="blenor1.0o">blenoreon</ref>
      (<trd lng="la">Neisseria gonorrhoeae</trd>,
      sin. <trd lng="la">Gonococcus neisseri</trd>):
      <ekz>
        <tld/>a konjunktivito
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=53931">Konjunktivito</url></lok>
        </fnt>.
      </ekz> 
    </dif>
    <bld lok="&WCU;/thumb/e/eb/Neisseria_gonorrhoeae_Bacteria_(16221300454).jpg/320px-Neisseria_gonorrhoeae_Bacteria_(16221300454).jpg" prm="CC BY 2.0">kolorigita elektronmikroskopa bildo de <tld/>oj (Neisseria gonorrhoeae bacteria)
        <fnt>
          <aut>NIAID</aut>
          <lok><url ref="&WCW;/File:Neisseria_gonorrhoeae_Bacteria_(16221300454).jpg">&WCW;/File:Neisseria_gonorrhoeae_Bacteria_(16221300454).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_n;&c_a;&c_k;&c_o;&c_k;</trd>
  <trd lng="ca">gonococ</trd>
  <trdgrp lng="cs">
    <trd>gonokok</trd>,
    <trd>gonokokus</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Gonokokkus</trd>,
     <trd>Trippererreger</trd></trdgrp>
  <trd lng="es">gonococo</trd>
  <trd lng="fr">gonocoque</trd>
  <trd lng="hu">Gonococcus</trd>
  <trd lng="ja">&#x6DCB;&#x83CC;  <pr>&#x308A;&#x3093;&#x304D;&#x3093;</pr></trd>
  <trd lng="nl">gonokok</trd>
  <trd lng="pt">gonococo</trd>
  <trd lng="ru">&c_g;&c_o;&c_n;&c_o;&c_k;&c_o;&c_k;&c_k;</trd>
  <trd lng="sk">gonokok</trd>
  <trd lng="uk">&#x433;&#x43E;&#x43D;&#x43E;&#x43A;&#x43E;&#x43A;</trd>
</drv>
