<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: sapind.xml,v 1.7 2024/03/24 19:59:59 revo Exp $">
    <kap>
      <rad>sapind</rad>/o
    </kap>

<drv mrk="sapind.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el la familio <ref tip="malprt" cel="sapindac.0oj">sapindacoj</ref>
      (<trd lng="la">Sapindus</trd>), al kiu i.a. apartenas la saparbo
      <klr>(Sapindus saponaria)</klr>, kies nuksojn oni uzas kiel
      lavilo:
      <ekz>
        saponino estas glukozido, <klr>[&#x2026;]</klr> solvebla en la akvo,
        &scirc;a&ubreve;migante tion,
        enhavas &gcirc;in <tld/>o, saponario, frukto de hipokastano
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=26284">Saponino</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/6/66/Sapindus_trifoliatus_05.jpg/376px-Sapindus_trifoliatus_05.jpg" prm="CC BY-SA 3.0">bran&ccirc;o de <tld/>o (Sapindus emarginatus, sin. trifoliatus)
        <fnt>
          <aut>Vinayaraj, propra verko</aut>
          <lok><url ref="&WCW;/File:Sapindus_trifoliatus_05.jpg">&WCW;/File:Sapindus_trifoliatus_05.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Seifenbaum</trd>
    <trd lng="fr">Savonnier</trd>
    <trd lng="pl">mydleniec</trd>
  </snc>
  <trdgrp lng="uk">
    <trd>&c_s;&c_a;&#x43F;&c_ib;&#x43D;&c_d;&c_u;&c_s;</trd>,
    <trd>&#x43C;&c_i;&#x43B;&#x44C;&#x43D;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&#x43E;</trd>
  </trdgrp>
  <trd lng="zh">&#x69F5; <pr>hu&#xE0;n</pr></trd>
</drv>

  </art>

