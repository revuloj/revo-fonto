<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&scirc;amisen</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="sxamisen.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxamisen.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Japana klasika
      <ref tip="super" cel="instru2.kord0o">kordinstrumento</ref>
      trikorda, el kvadrata sonkesto tegita per katha&ubreve;to,
      longa kolo kaj kapo kun tri kejloj, plektre ludata:
      <ekz>
        tiu malnova formo estis ludita, ne de <tld/>o, sed tamburo kaj
        fluto
        <fnt>
        <aut>M. Sasaki</aut>,
        <vrk><url
        ref="http://morioka.cool.ne.jp/ihatov-esperanto/yoshare.html">Jo&scirc;are</url></vrk>,
        <lok>2004-08-15</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_ja;&c_m;&c_ib;&c_s;&c_e;&c_n;</trd>
  <trd lng="fr">shamisen</trd>
  <trd lng="hu">samiszen</trd>
  <trd lng="ja">&#19977;&#21619;&#32218; [<ind>&#12375;&#12419;&#12415;&#12379;&#12435;</ind>]</trd>
  <trd lng="ru">&c_s;&c_ja;&c_m;&c_i;&c_s;&c_e;&c_n;</trd>
  <trd lng="pt">shamisen</trd>
</drv>
</art>
