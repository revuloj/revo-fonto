<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&scirc;amisen</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="sxamisen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Japana klasika
      <ref tip="super" cel="instru2.kord0o">kordinstrumento</ref>
      trikorda konsista el kvadrata sonkesto tegita per kat-ha&ubreve;to,
      61,5 cm longa kolo kaj kapo kun tri kejloj, plektre ludata akompane
      al kantoj kaj dancoj.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_ja;&c_m;&c_ib;&c_s;&c_e;&c_n;</trd>
  <trdgrp lng="fr">
    <trd>shamisen</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_ja;&c_m;&c_i;&c_s;&c_e;&c_n;</trd>
</drv>
</art>
