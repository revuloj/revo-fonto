<vortaro>
<art mrk="$Id: veksilol.xml,v 1.3 2023/05/03 14:27:03 afido Exp $">
<kap>
    <rad>veksilolog</rad>/o 
</kap>
<drv mrk="veksilol.0o">
  <kap><tld/>o</kap>
  <snc mrk="veksilol.0o.HER">
    <uzo tip="fak">HER</uzo>
    <dif>
      Spertulo pri <ref cel="flag.0o">flago</ref>j:
      <ekz>
        kia rikolto de standardoj por la <tld/>oj
        <fnt>
          <aut>Ji&rcaron;&iacute; Patera</aut>
          <vrk><url ref="&Monato;/2002/007933.html">Tridek jaroj de la Veksilologia Klubo en
          Prago</url></vrk>
          <bib>Monato</bib>
          <lok>2002/10, p. 23</lok>
        </fnt>!
      </ekz>
      <ekz>
        amiko mia, amatora <tld/>o (studanto de flagoj), komencis utiligi Interreton
        <fnt>
          <aut>Thomas Eccardt</aut>
          <vrk><url ref="&Monato;/2000/006859.php">Urbeto a&ubreve; sendependa lando?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/05, p. 8</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Flaggenkundler</trd>,
    <trd>Vexillologe</trd>
  </trdgrp>
  <trd lng="en">vexillologist</trd>
  <trd lng="nl">vexilloloog</trd>
</drv>

</art>

