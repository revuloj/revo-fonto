<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>defi</rad>/i
<fnt>Z</fnt>
</kap>
<drv mrk="defi.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Provoki al batalo a&ubreve; konkurso:
      <ekz>
        <tld/>i iun pri kuro, bilardo, duelo;
      </ekz>
      <ekz>
        akcepti <ind><tld/>o</ind>n<fnt>PIV1</fnt>;
        <trd lng="fr">d&eacute;fi</trd>
      </ekz>
      <klr>(f)</klr>
      <ekz>
        <tld/>i la dan&gcirc;eron.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tim.mal0i">maltimi</ref>,
      <ref cel="evit.mal0i">maleviti</ref>,
      <ref cel="provok.0i">provoki</ref>,
      <ref cel="spit.0i">spiti</ref>.
    </refgrp>
  </snc>
  <snc>
    <uzo tip="klr">(io)</uzo>
    <dif>
      Prezenti nesupereblan obstaklon:
      <ekz>
        skriba&jcirc;o, <tld/>anta la de&ccirc;ifron<fnt>PIV1</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">d&eacute;fier</trd>
  <trd lng="ru">&c_b;&c_r;&c_o;&c_s;&c_i;&c_t;&c_mol; &c_v;&c_y;&c_z;&c_o;&c_v;</trd>
</drv>
</art>
