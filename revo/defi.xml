<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: defi.xml,v 1.26 2024/01/08 07:23:03 revo Exp $">
<kap>
  <ofc>9</ofc> <rad>defi</rad>/i
  <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="defi.0i">
  <kap><ofc>9</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="defi.0i.iu">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Prezenti konkursproponon, minacon, obstaklon, enigmon ktp instigante al batalo por superi tiun:
      <ekz>
        <tld/>i iun batali, amikon &scirc;akludi
        <fnt><bib>OA9</bib></fnt>,
      </ekz>
      <ekz>
        la komenca timo jam transformi&gcirc;as en memfidan <tld/>emon <klr>[&#8230;]</klr>: &leftquot;pri kio vi parolas?&rightquot;
        <fnt>
          <bib>ChV</bib>
          <lok>8</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a sinteno
        <fnt>
          <bib>ChC</bib>
          <lok>5</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> alrigardis <tld/>e la &ccirc;irka&ubreve;ajn starantojn
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Destino de Atrejo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi estas preta <tld/>i vin tri samtempe
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Ar&gcirc;enta Urbo Amarganto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind><tld/>i la dan&gcirc;eron</ind> <klr>(<ref cel="risk.0i">riski</ref>)</klr>.
        <trd lng="nl">het gevaar <ind>tarten</ind></trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tim.mal0i">maltimi</ref>,
      <ref cel="evit.mal0i">maleviti</ref>,
      <ref cel="provok.0i">provoki</ref>,
      <ref cel="spit.0i">spiti</ref>
    </refgrp>
    <trd lng="pl">wyzywa&cacute;<klr> (np. na pojedynek)</klr></trd>
    <trdgrp lng="pt">
      <trd>desafiar</trd>,
      <trd>provocar</trd>,
      <trd>arrostar</trd>
    </trdgrp>
  </snc>
  <snc mrk="defi.0i.io">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Prezenti malfacile supereblan minacon, obstaklon, enigmon ktp:
      <ekz>
        &scirc;akludo <tld/>as la intelekton
        <fnt><bib>OA9</bib></fnt>;
      </ekz>
      <ekz>
        skriba&jcirc;o, <tld/>anta la de&ccirc;ifron
        <fnt><bib>PIV1</bib></fnt>;
      </ekz>
      <ekz>
        mistero <tld/>as
        <fnt>
          <bib>ChC</bib>
          <lok>1</lok>
        </fnt>;
      </ekz>
      <ekz>
        la groteska, rigida pozo de l&#8217; nigra figuro &ndash; <tld/>is &ccirc;ian provon priskribi
        <fnt>
          <bib>ChR</bib>
          <lok>Ne nur tamtamado</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj kurblinioj de alloga inkorpo
        <fnt>
          <bib>SkandalJozef</bib>
          <lok>Plendoj de Jozefo arestita</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tiom lar&gcirc;signifa]</klr> &#8218;vorto&#8216; <tld/>as &ccirc;iun provon esti inkluzivata en
        kiun ajn aktualan gramatikan (logikan) kategorion
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>aufreizen</trd>,
      <trd>trotzen</trd>,
      <trd>die <ind>Stirn</ind> bieten</trd>
    </trdgrp>
    <trd lng="pt">desafiar</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_ib;&c_n;&c_u;&c_c;&c_mol;
    <ind>&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;</ind></trd>
  </trdgrp>
  <trd lng="de">herausfordern</trd>
  <trdgrp lng="en">
    <trd>defy</trd>,
    <trd>challenge</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;fier</trd>
    <trd>mettre au <ind>d&eacute;fi</ind> (de)</trd>
    <trd>braver</trd>
    <trd>narguer</trd>
    <trd>provoquer</trd>
  </trdgrp>
  <trd lng="he">&lamed;&he;&ayin;&zayin;</trd>
  <trd lng="hu">kih&iacute;v<klr tip="ind"> (&aacute;tv. is)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#25361;&#25126;&#12377;&#12427;  <pr>&#12385;&#12423;&#12358;&#12379;&#12435;&#12377;&#12427;</pr></trd>,
    <trd>&#12356;&#12393;&#12416;</trd>,
    <trd>&#20844;&#28982;&#12392;&#21453;&#25239;&#12377;&#12427;  <pr>&#12371;&#12358;&#12380;&#12435;&#12392;&#12399;&#12435;&#12371;&#12358;&#12377;&#12427;</pr></trd>,
    <trd>&#24179;&#28982;&#12392;&#28961;&#35222;&#12377;&#12427;  <pr>&#12408;&#12356;&#12380;&#12435;&#12392;&#12416;&#12375;&#12377;&#12427;</pr></trd>,
    <trd>&#12418;&#12398;&#12392;&#12418;&#12375;&#12394;&#12356;</trd>
  </trdgrp>
  <trd lng="nl">uitdagen</trd>
  <trd lng="es">desafiar</trd>
  <trd lng="ru">&c_b;&c_r;&c_o;&c_s;&c_i;&c_t;&c_mol;
  &c_v;&c_y;&c_z;&c_o;&c_v;</trd>
  <trd lng="sk">vyzva&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_k;&c_l;&c_i;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_b;&c_i;&c_t;&c_i; &c_v;&c_i;&c_k;&c_l;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20882;&#30528; <pr>m&agrave;ozhe</pr></trd>,
    <trd>&#23547;&#34885; <pr>x&uacute;nx&igrave;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="defi.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Io, kio <tld/>as:
      <ekz>
        &scirc;i akceptis la <tld/>on fari el Esperanto profesion
        <fnt>
          <bib>OIP</bib>
        </fnt>;
      </ekz>
      <ekz>
        kontra&ubreve; tia rekta <tld/>o al ilia a&ubreve;toritato, ili reagas nur per malplenaj blufoj
        <fnt>
          <bib>StU</bib>
          <lok>Britujo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia patrino arigis &ccirc;iujn siajn infanojn inter siaj brakoj per lasta senespera
        <tld/>o, kvaza&ubreve; &scirc;i ankora&ubreve; esperis iel protekti ilin
        <fnt>
          <bib>StU</bib>
          <lok>La Nazaretanoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        900 kilometra longeco, 26&#8239;000 altecmetroj, 14 etapoj <klr>[&#8230;]</klr> estas
        <tld/>o por &ccirc;iu montbiciklisto
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni GEORGIEV</aut>
          <vrk>900 km tra montbicikla paradizo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        majstri la <tld/>ojn de nuna ekonomio
        <fnt>
          <bib>Monato</bib>
          <aut>Maul Stefan</aut>
          <vrk>Majstroverko, sed nur taktika</vrk>
        </fnt>;
      </ekz>
      <ekz>
        formi &leftquot;grupon&rightquot; <klr>[tian]</klr> el 25 membroj de EU restas <tld/>o enorma
        <fnt>
          <bib>Monato</bib>
          <aut>Zlatko Ti&scaron;ljar</aut>
          <vrk><url ref="&Monato;/2004/008214.html"
              >Nepras emocie interrilati</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        renkonti &ccirc;iajn <tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="&Monato;/2004/008325.html"
              >Kvin defioj por &ccirc;inaj urboj</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        gravaj estas tiaj <tld/>oj <klr>[&#8230;]</klr>, &ccirc;ar se lingvo ne sufi&ccirc;as por traduki
        maturajn altnivelajn nacilingvajn literatura&jcirc;ojn, &gcirc;i ja ne sufi&ccirc;as por esti
        plene funkcianta literatura lingvo
        <fnt>
          <bib>Plumamikoj</bib>
          <lok>Tradukoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;fi</trd>
    <trd>bravade</trd>
    <trd>challenge<klr tip="ind"> (d&eacute;fi)</klr></trd>
  </trdgrp>
  <trd lng="de">Herausforderung</trd>
  <trd lng="nl">uitdaging</trd>
  <trd lng="pl">wyzwanie<klr> (np. na pojedynek)</klr></trd>
  <trd lng="ru">&c_v;&c_y;&c_z;&c_o;&c_v;</trd>
</drv>

</art>
