<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: defi.xml,v 1.21 2023/08/31 10:43:58 revo Exp $">
<kap>
  <ofc>9</ofc> <rad>defi</rad>/i
  <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="defi.0i">
  <kap><ofc>9</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Inviti a&ubreve; provoki al batalo, konkurso ks, prezentante
      sin mem kiel kontra&ubreve;ulon:
      <ekz>
        <tld/>i iun batali
        <fnt><bib>9OA</bib></fnt>,
      </ekz>
      <ekz>
        je/al batalo
        <fnt><bib>9OA</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>i iun pri kuro, bilardo, duelo;
      </ekz>
      <ekz>
        <tld/>i boksan &ccirc;ampionon
        <fnt><bib>9OA</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>i amikon &scirc;akludi
        <fnt><bib>9OA</bib></fnt>;
      </ekz>
      <ekz>
        akcepti <ind><tld/>o</ind>n
        <fnt><bib>PIV1</bib></fnt>;
        <trd lng="be">&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;</trd>
        <trdgrp lng="fr">
          <trd>d&eacute;fi</trd>
          <trd>bravade</trd>
          <trd>challenge<klr tip="ind"> (d&eacute;fi)</klr></trd>
        </trdgrp>
        <trd lng="nl">uitdaging</trd>
        <trd lng="pl">wyzwanie<klr> (np. na pojedynek)</klr></trd>
        <trd lng="ru">&c_v;&c_y;&c_z;&c_o;&c_v;</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind><tld/>i la dan&gcirc;eron</ind>.
        <trd lng="nl">het gevaar <ind>tarten</ind></trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tim.mal0i">maltimi</ref>,
      <ref cel="evit.mal0i">maleviti</ref>,
      <ref cel="provok.0i">provoki</ref>,
      <ref cel="spit.0i">spiti</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>herausfordern</trd>
    </trdgrp>
    <trd lng="pl">wyzywa&cacute;<klr> (np. na pojedynek)</klr></trd>
    <trdgrp lng="pt">
      <trd>desafiar</trd>,
      <trd>provocar</trd>,
      <trd>arrostar</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="klr">(io)</uzo>
    <dif>
      Prezenti malfacile nesupereblan obstaklon:
      <ekz>
        &scirc;akludo <tld/>as la intelekton
        <fnt><bib>9OA</bib></fnt>;
      </ekz>
      <ekz>
        skriba&jcirc;o, <tld/>anta la de&ccirc;ifron
        <fnt><bib>PIV1</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>trotzen</trd>,
      <trd>die <ind>Stirn</ind> bieten</trd>
    </trdgrp>
    <trd lng="pt">desafiar</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_ib;&c_n;&c_u;&c_c;&c_mol;
    <ind>&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>defy</trd>,
    <trd>challenge</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;fier</trd>
    <trd>mettre au <ind>d&eacute;fi</ind> (de)</trd>
    <trd>braver</trd>
    <trd>narguer</trd>
    <trd>provoquer</trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5D4;&#x5E2;&#x5D6;</trd>
  <trd lng="hu">kih&iacute;v<klr tip="ind"> (&aacute;tv. is)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x6311;&#x6226;&#x3059;&#x308B;  [<ind>&#x3061;&#x3087;&#x3046;&#x305B;&#x3093;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x3044;&#x3069;&#x3080;</trd>,
    <trd>&#x516C;&#x7136;&#x3068;&#x53CD;&#x6297;&#x3059;&#x308B;  [<ind>&#x3053;&#x3046;&#x305C;&#x3093;&#x3068;&#x306F;&#x3093;&#x3053;&#x3046;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x5E73;&#x7136;&#x3068;&#x7121;&#x8996;&#x3059;&#x308B;  [<ind>&#x3078;&#x3044;&#x305C;&#x3093;&#x3068;&#x3080;&#x3057;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x3082;&#x306E;&#x3068;&#x3082;&#x3057;&#x306A;&#x3044;</trd>
  </trdgrp>
  <trd lng="nl">uitdagen</trd>
  <trd lng="es">desafiar</trd>
  <trd lng="ru">&c_b;&c_r;&c_o;&c_s;&c_i;&c_t;&c_mol;
  &c_v;&c_y;&c_z;&c_o;&c_v;</trd>
  <trd lng="sk">vyzva&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x432;&#x438;&#x43A;&#x43B;&#x438;&#x43A;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x440;&#x43E;&#x431;&#x438;&#x442;&#x438; &#x432;&#x438;&#x43A;&#x43B;&#x438;&#x43A;</trd>
  </trdgrp>
</drv>
</art>
