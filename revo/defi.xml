<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: defi.xml,v 1.25 2024/01/08 05:29:57 revo Exp $">
<kap>
  <ofc>9</ofc> <rad>defi</rad>/i
  <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="defi.0i">
  <kap><ofc>9</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Inviti a&ubreve; provoki al batalo, konkurso ks, prezentante
      sin mem kiel kontra&ubreve;ulon:
      <ekz>
        <tld/>i iun batali
        <fnt><bib>OA9</bib></fnt>,
      </ekz>
      <ekz>
        je/al batalo
        <fnt><bib>OA9</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>i iun pri kuro, bilardo, duelo;
      </ekz>
      <ekz>
        <tld/>i boksan &ccirc;ampionon
        <fnt><bib>OA9</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>i amikon &scirc;akludi
        <fnt><bib>OA9</bib></fnt>;
      </ekz>
      <ekz>
        akcepti <ind><tld/>o</ind>n
        <fnt><bib>PIV1</bib></fnt>;
        <trd lng="be">&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;</trd>
        <trdgrp lng="fr">
          <trd>d&eacute;fi</trd>
          <trd>bravade</trd>
          <trd>challenge<klr tip="ind"> (d&eacute;fi)</klr></trd>
        </trdgrp>
        <trd lng="nl">uitdaging</trd>
        <trd lng="pl">wyzwanie<klr> (np. na pojedynek)</klr></trd>
        <trd lng="ru">&c_v;&c_y;&c_z;&c_o;&c_v;</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind><tld/>i la dan&gcirc;eron</ind>.
        <trd lng="nl">het gevaar <ind>tarten</ind></trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tim.mal0i">maltimi</ref>,
      <ref cel="evit.mal0i">maleviti</ref>,
      <ref cel="provok.0i">provoki</ref>,
      <ref cel="spit.0i">spiti</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>herausfordern</trd>
    </trdgrp>
    <trd lng="pl">wyzywa&cacute;<klr> (np. na pojedynek)</klr></trd>
    <trdgrp lng="pt">
      <trd>desafiar</trd>,
      <trd>provocar</trd>,
      <trd>arrostar</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="klr">(io)</uzo>
    <dif>
      Prezenti malfacile supereblan obstaklon:
      <ekz>
        &scirc;akludo <tld/>as la intelekton
        <fnt><bib>OA9</bib></fnt>;
      </ekz>
      <ekz>
        skriba&jcirc;o, <tld/>anta la de&ccirc;ifron
        <fnt><bib>PIV1</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>trotzen</trd>,
      <trd>die <ind>Stirn</ind> bieten</trd>
    </trdgrp>
    <trd lng="pt">desafiar</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_ib;&c_n;&c_u;&c_c;&c_mol;
    <ind>&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>defy</trd>,
    <trd>challenge</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;fier</trd>
    <trd>mettre au <ind>d&eacute;fi</ind> (de)</trd>
    <trd>braver</trd>
    <trd>narguer</trd>
    <trd>provoquer</trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5D4;&#x5E2;&#x5D6;</trd>
  <trd lng="hu">kih&iacute;v<klr tip="ind"> (&aacute;tv. is)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#25361;&#25126;&#12377;&#x308B;  <pr>&#12385;&#12423;&#12358;&#x305B;&#12435;&#12377;&#x308B;</pr></trd>,
    <trd>&#12356;&#12393;&#12416;</trd>,
    <trd>&#x516C;&#28982;&#12392;&#x53CD;&#25239;&#12377;&#x308B;  <pr>&#12371;&#12358;&#x305C;&#12435;&#12392;&#x306F;&#12435;&#12371;&#12358;&#12377;&#x308B;</pr></trd>,
    <trd>&#x5E73;&#28982;&#12392;&#28961;&#35222;&#12377;&#x308B;  <pr>&#12408;&#12356;&#x305C;&#12435;&#12392;&#12416;&#12375;&#12377;&#x308B;</pr></trd>,
    <trd>&#12418;&#x306E;&#12392;&#12418;&#12375;&#x306A;&#12356;</trd>
  </trdgrp>
  <trd lng="nl">uitdagen</trd>
  <trd lng="es">desafiar</trd>
  <trd lng="ru">&c_b;&c_r;&c_o;&c_s;&c_i;&c_t;&c_mol;
  &c_v;&c_y;&c_z;&c_o;&c_v;</trd>
  <trd lng="sk">vyzva&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&#x43A;&#x43B;&c_i;&#x43A;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&#x43E;&c_b;&c_i;&c_t;&c_i; &c_v;&c_i;&#x43A;&#x43B;&c_i;&#x43A;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20882;&#30528; <pr>m&#xE0;ozhe</pr></trd>,
    <trd>&#x5BFB;&#34885; <pr>x&#xFA;nx&#xEC;n</pr></trd>
  </trdgrp>
</drv>
</art>
