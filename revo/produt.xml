<drv mrk="produt.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="produt.0o.rezulto">
    <fnt><bib>MatTerm</bib><lok>p. 9</lok></fnt>
    <dif>
      Rezulto de <ref cel="multip.0o">multipliko</ref>:
      <ekz>
        <frm>15</frm> estas la <tld/>o de <frm>3</frm> 
        kaj <frm>5</frm>;
      </ekz>
      <ekz>
        la <tld/>on de <frm am="a"><k>a</k></frm> kaj <frm am="b"><k>b</k></frm> 
        oni signas per <frm am="a xx b"><k>a</k>&#xD7;<k>b</k></frm>,
        <frm am="a*b"><k>a</k>&middot;<k>b</k></frm>, a&ubreve; simple 
        <frm am="ab"><k>a</k><k>b</k></frm>,
      </ekz>
      <ekz>
        kaj la <tld/>on de &ccirc;iuj termoj en 
        <ref cel="famili.0o.bildigo">familio</ref>
        <frm am="(a_i)_(i in bb I)">(<k>a</k><sub><k>i</k></sub>)<sub><k>i</k>&#x2208;<g>I</g></sub></frm>
        per <frm am="prod_(i in bb I)a_i">&Pi;<sub><k>i</k>&#x2208;<g>I</g></sub> 
        <k>a</k><sub><k>i</k></sub></frm>;
      </ekz>
      <ekz>
        <tld/>o de matricoj, polinomoj.
      </ekz>
    </dif>
    <ref tip="super" cel="obl.0o">oblo.</ref>
    <trd lng="ca">producto</trd>
    <trd lng="de">Produkt</trd>
    <trd lng="en">product<klr tip="ind"> (math.)</klr></trd>
    <trd lng="es">producto</trd>
    <trd lng="fr">produit</trd>
    <trd lng="hu">szorzat</trd>    
    <trd lng="nl">product<klr tip="ind">
    (v.vermenigvuldiging)</klr></trd>
    <trd lng="pl">iloczyn<klr tip="ind"> (wynik mno&zdot;enia)</klr></trd>
    <trd lng="pt">produto</trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je;<klr tip="ind"> (&c_m;&c_a;&c_t;.)</klr></trd>
  </snc>
  <snc mrk="produt.0o.operacio">
    <dif>
      Nomo de pluraj multipliksimilaj operacioj kaj de iliaj rezultoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="produt.kartezia0o">kartezia <tld/>o</ref>;
      <ref cel="produt.hermita_skalara0o">hermita (skalara)
      <tld/>o</ref>,
      <ref cel="produt.skalara0o">skalara <tld/>o</ref>,
      <ref cel="produt.vektora0o">vektora <tld/>o</ref>;
      <ref cel="produt.ekstera0o">ekstera <tld/>o</ref>,
      <ref cel="produt.interna0o">interna <tld/>o</ref>.
    </refgrp>
    <trd lng="de">Produkt</trd>
    <trd lng="pt">produto</trd>
  </snc>
    <trd lng="be">&c_z;&c_d;&c_a;&c_b;&c_y;&c_t;&c_a;&c_k; <klr>(&c_m;&c_a;&c_t;&c_e;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;)</klr></trd>
  <trd lng="cs">sou&ccaron;in</trd>
  <trd lng="de">Produkt</trd>
  <trd lng="ja">&#x7A4D;  <pr>&#x305B;&#x304D;</pr></trd>
  <trdgrp lng="sk">
    <trd>n&aacute;sobok</trd>,
    <trd>s&uacute;&ccaron;in</trd>
  </trdgrp>
  <trd lng="uk">&c_d;&#x43E;&c_b;&c_u;&c_t;&#x43E;&#x43A;</trd>
  <trdgrp lng="zh">
    <trd>&#x79EF;&#x6570; <pr>j&#x12B;sh&#xF9;</pr></trd>,
    <trd>&#x7A4D;&#x6578; <pr>j&#x12B;sh&#xF9;</pr></trd>
  </trdgrp>
</drv>

