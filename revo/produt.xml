<drv mrk="produt.skalara0o">
  <kap>skalara <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="produt.skalara0o.operacio">
    <fnt><bib>EKV</bib><lok>&para;402</lok></fnt>
    <dif>
      <klr>(super reela <ref cel="spac.vektora0o">vektora spaco</ref>
      <frm am="bb E"><g>E</g></frm>)</klr>
      Tia <ref cel="linear.n0a">dulineara</ref>
      <ref cel="simetr.0a.bildigo">simetria</ref>
      <ref cel="form.0o.bildigo">formo</ref> <frm am="phi">&phi;</frm>, ke
      <frm am="phi(x,x) ge 0">&phi;(<k>x</k>,<k>x</k>) &#x2265; 0</frm> por &ccirc;iuj
      <frm am="x in bb E"><k>x</k>&#x2208;<g>E</g></frm>, kaj
      egalas al nulo, se kaj nur se <frm am="x=0"><k>x</k> = 0</frm>.
    </dif>
    <refgrp tip="vid">
      <ref cel="spac.euxklida0o">e&ubreve;klida spaco</ref>,
      <ref cel="norm.0o.MAT">normo</ref>.
    </refgrp>
  <trdgrp lng="pt">
    <trd>produto escalar</trd>,
    <trd>produto interno</trd>
  </trdgrp>
  </snc>
  <snc mrk="produt.skalara0o.rezulto">
    <dif>
      <klr>(de du vektoroj <frm am="x"><k>x</k></frm>,
      <frm am="y"><k>y</k></frm>)</klr>
      La bildo de <frm am="(x,y)">(<k>x</k>,<k>y</k>)</frm> per
      skalara <tld/>o <sncref ref="produt.skalara0o.operacio"/>:
      <ekz>
        la skalaran <tld/>on de <frm am="x"><k>x</k></frm> kaj
        <frm am="y"><k>y</k></frm>
        oni signas foje per <frm am="x*y"><k>x</k>&middot;<k>y</k></frm>,
        foje per <frm am="(:x|y:)">&lt;<k>x</k>|<k>y</k>&gt;</frm>.
      </ekz>
    </dif>
   <trdgrp lng="pt">
    <trd>produto escalar</trd>,
    <trd>produto interno</trd>
  </trdgrp>
  </snc>
   <trd lng="be">&c_z;&c_d;&c_a;&c_b;&c_y;&c_t;&c_a;&c_k; &c_s;&c_k;&c_a;&c_l;&c_ja;&c_r;&c_n;&c_y;</trd>
  <trd lng="en">scalar <ind>product</ind></trd>
  <trd lng="de">Skalarprodukt</trd>
  <trd lng="fr">produit scalaire</trd>
  <trd lng="hu">skal&aacute;ris <ind>szorzat</ind></trd>  
  <trd lng="pl">iloczyn skalarny</trd>
  <trd lng="ru">&c_s;&c_k;&c_a;&c_l;&c_ja;&c_r;&c_n;&c_o;&c_je; <ind>&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je;</ind></trd>
  <trd lng="uk">&c_s;&#x43A;&c_a;&#x43B;&#x44F;&c_r;&#x43D;&c_i;&c_j; &c_d;&#x43E;&c_b;&c_u;&c_t;&#x43E;&#x43A;</trd>
  <trdgrp lng="zh">
    <trd>&#x6570;&#x91CF;&#x79EF; <pr>sh&#xF9;li&#xE0;ngj&#x12B;</pr></trd>,
    <trd>&#x6578;&#x91CF;&#x7A4D; <pr>sh&#xF9;li&#xE0;ngj&#x12B;</pr></trd>,
    <trd>&#x6807;&#x79EF; <pr>bi&#x101;oj&#x12B;</pr></trd>,
    <trd>&#x6A19;&#x7A4D; <pr>bi&#x101;oj&#x12B;</pr></trd>,
    <trd>&#x70B9;&#x79EF; <pr>di&#x1CE;nj&#x12B;</pr></trd>,
    <trd>&#x9EDE;&#x7A4D; <pr>di&#x1CE;nj&#x12B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="produt.vektora0o">
  <kap>vektora <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="produt.vektora0o.operacio">
    <dif>
      <klr>(super <frm am="n"><k>n</k></frm>-dimensia
      <ref cel="spac.euxklida0o">e&ubreve;klida spaco</ref>
      <frm am="bb E"><g>E</g></frm>) </klr>
      <ref tip="super" cel="bild.0igo.MAT">Bildigo</ref> de
      <frm am="bb E^(n-1)"><g>E</g><sup><k>n</k>-1</sup></frm>
      al <frm am="bb E"><g>E</g></frm>,
      kiu &jcirc;etas &ccirc;iun <frm am="(n-1)">(<k>n</k>-1)</frm>-opon
      <frm am="(x_1,...,x_(n-1))">(<k>x</k><sub>1</sub>,..., <k>x</k><sub><k>n</k>-1</sub>)</frm>
      al tia ununura vektoro <frm am="a"><k>a</k></frm>, ke la skalara
      <tld/>o
      de <frm am="a"><k>a</k></frm> per <frm am="x_n"><k>x</k><sub><k>n</k></sub></frm>
      egalas al la
      <ref cel="determ.0anto.vektoroj">determinanto</ref> de
      <frm am="(x_1,...,x_(n-1),x_n)">(<k>x</k><sub>1</sub>,..., <k>x</k><sub><k>n</k>-1</sub>,
      <k>x</k><sub><k>n</k></sub>)</frm>, kiu ajn estas
      <frm am="x_n"><k>x</k><sub><k>n</k></sub></frm>.
    </dif>
    <trd lng="es">producto vectorial</trd>
    <trd lng="pt">produto vetorial</trd>
  </snc>
  <snc mrk="produt.vektora0o.rezulto">
    <dif>
      <klr>(de <frm am="n-1"><k>n</k>-1</frm> vektoroj de
      <frm am="n"><k>n</k></frm>-dimensia
      <ref cel="spac.euxklida0o">e&ubreve;klida spaco</ref>)</klr>
      Ilia bildo per la vektora
      <tld/>o <sncref ref="produt.vektora0o.operacio"/>:
      <ekz>
        la vektoran <tld/>on de <frm am="x"><k>x</k></frm> kaj
        <frm am="y"><k>y</k></frm>
        en tridimensia spaco oni signas foje per
        <frm am="x^^y"><k>x</k>&#x2227;<k>y</k></frm>;
