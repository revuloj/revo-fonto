<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: klimat.xml,v 1.24 2021/05/28 12:15:05 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>klimat</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="klimat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="klimat.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Tuto de la atmosferaj kaj veteraj kondi&ccirc;oj de iu regiono:
      <ekz>
        konstrua&jcirc;o <klr>[&#x2026;]</klr> konforma al la <tld/>o
        kaj materialo de la lando
        <fnt><bib>Fab3</bib>, <lok>Io</lok></fnt>;
      </ekz>
      <ekz>
        <ind>modera <tld/>o</ind>;
        <trd lng="fr">climat temp&eacute;r&eacute;</trd>
        <trd lng="pl">klimat umiarkowany</trd>
        <trd lng="ru">&c_u;&c_m;&c_je;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j; &c_k;&c_l;&c_i;&c_m;&c_a;&c_t;</trd>
      </ekz>
      <ekz>
        <ind>tropika <tld/>o</ind>;
        <trd lng="fr">climat tropical</trd>
        <trd lng="pl">klimat tropikalny</trd>
        <trd lng="ru">&c_t;&c_r;&c_o;&c_p;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; &c_k;&c_l;&c_i;&c_m;&c_a;&c_t;</trd>  
      </ekz>
      <ekz>
        <ind>kontinenta <tld/>o</ind>;
        <trd lng="fr">climat continental</trd>
        <trd lng="pl">klimat kontynentalny</trd>
        <trd lng="ru">&c_k;&c_o;&c_n;&c_t;&c_i;&c_n;&c_je;&c_n;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_k;&c_l;&c_i;&c_m;&c_a;&c_t;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_ib;&c_m;&c_a;&c_t;</trd>
  <trd lng="bg">&c_k;&c_l;&c_i;&c_m;&c_a;&c_t;</trd>
  <trd lng="bo">&#xF42;&#xF53;&#xF58;&#xF0B;&#xF42;&#xF64;&#xF72;&#xF66;&#xF0B;</trd>
  <trd lng="ca">clima</trd>
  <trdgrp lng="cs">
    <trd>klima</trd>,
    <trd>podneb&iacute;</trd>
  </trdgrp>
  <trd lng="de">Klima</trd>
  <trd lng="en">climate</trd>
  <trd lng="es">clima</trd>
  <trd lng="fr">climat</trd>
  <trd lng="nl">klimaat</trd>
  <trd lng="pl">klimat</trd>     
  <trd lng="ro">clim&abreve;</trd>
  <trd lng="ru">&c_k;&c_l;&c_i;&c_m;&c_a;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>kl&iacute;ma</trd>,
    <trd>podnebie</trd>
  </trdgrp>
  <trd lng="tr">iklim</trd>
</drv>

<drv mrk="klimat.al0igi">
  <kap>
    al<tld/>igi
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="klimat.al0igi.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      Akomodi al nova <tld/>o,
      adapti al la naturaj vivkondi&ccirc;oj de alia mondregiono:
      <ekz>
        &gcirc;ardenkulturo <klr>[&#x2026;]</klr> alklimatigas la belegajn florojn de fremdaj
        terzonoj, transportante ilin en niajn &gcirc;ardenojn
        <fnt>
          <aut>A. Giesswein</aut>
          <vrk><url ref="https://eo.wikisource.org/wiki/Esperanto_kaj_mondliteraturo">Esperanto kaj mondliteraturo</url></vrk>
          <lok>Literatura Mondo, 1922-11 (2)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">acclimater</trd>
</drv>

<drv mrk="klimat.al0igxi">
  <kap>al<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="klimat.al0igxi.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      Akomodi&gcirc;i al iu <tld/>o:
      <ekz>
        en niaj &gcirc;ardenoj <klr>[aneto]</klr> bone alklimati&gcirc;as
        <fnt>
          <aut>E.de Mestral</aut>,
          <vrk><url ref="http://lingvo.unblog.fr/2008/04/03/fumigita-fia-amao-aromigita-per-frea-aneto/">
          Fumigita fi&scirc;a &scirc;a&ubreve;ma&jcirc;o...</url></vrk>,
          <lok>Romanda Laborgrupo, 2008-04-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_l;&c_ib;&c_m;&c_a;&c_t;&c_y;&c_z;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trd lng="bg">&c_a;&c_k;&c_l;&c_i;&c_m;&c_a;&c_t;&c_i;&c_z;&c_i;&c_r;&c_a;&c_m;</trd>
  <trd lng="de">akklimatisieren</trd>
  <trd lng="ca">acostumar-se al clima</trd>
  <trdgrp lng="en">
    <trd>acclimate</trd>,
    <trd>acclimatize</trd>
  </trdgrp>
  <trd lng="fr">s'<ind>acclimater</ind></trd>
  <trd lng="es">aclimatarse</trd>
  <trd lng="nl">acclimatiseren</trd>
  <trdgrp lng="pl">
    <trd>przyzwyczai&cacute; si&eogonek;</trd>,
    <trd>zaaklimatyzowa&cacute; si&eogonek;</trd>
  </trdgrp>   
<trdgrp lng="ro">
    <trd>s&abreve; se obi&#x219;nuiasc&abreve;</trd>,
    <trd>deprinde</trd>,
    <trd>ob&#x21B;ine aclimatizate</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_k;&c_k;&c_l;&c_i;&c_m;&c_a;&c_t;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="klimat.pra0o">
  <kap>pra<tld/>o</kap>
  <snc mrk="klimat.pra0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      <tld lit="K"/>o de anta&ubreve;longa epoko;
      <ref tip="sin" cel="paleoklimat.0o.MET">paleoklimato</ref>:
      <ekz>
        en &ccirc;apitro X, ni klopodis rekonstrui la pra<tld/>ojn
        kaj la reliefon en la montaro, malgra&ubreve; multaj necerta&jcirc;oj
        <fnt>
          <aut>J. M. Mabesoone</aut>,
          <vrk><url ref="http://www.repository.naturalis.nl/document/549662">
          Tertiary and Quaternary Sedimentation in a Part of the 
          Duero Basin Palencia (Spain)</url></vrk>,
          <lok>[Leidse Geologische Mededelingen, 1959 (24), p. 
          31a-180a]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>palaeoclimate</trd>,
    <trd>paleoclimate</trd>
  </trdgrp>
  <trd lng="fr">pal&eacute;oclimat</trd>
  <trd lng="pl">paleoklimat</trd>     
  <trd lng="ro">palaeoclimate</trd>
</drv>
</art>
