<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>blokad</rad>/o
</kap>
<drv mrk="blokad.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;irka&ubreve;sie&gcirc;ado, &ccirc;irka&ubreve;baro de fortika&jcirc;o, urbo, haveno per
      seninterrompa gardo, por malhelpi la eniron de necesa&jcirc;oj:
      <ekz>
        mi trovis vian adreson en jarlibro de 1991, &ccirc;ar pro la <tld/>o de
        Sarajevo, mi ne povis ricevi novan<fnt><aut>Spomenka &Scirc;timec</aut>:
        <vrk>Saluton el Absurdistano</vrk>, <bib>Monato</bib><lok>jaro
        1993a, numero 12a, p. 9a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Blockierung</trd>,
     <trd>Blockade</trd>,
     <trd>Absperrung</trd>
  </trdgrp>
  <trd lng="es">bloqu&eacute;o</trd>
  <trd lng="fr">blocus</trd>
  <trd lng="ru">&c_b;&c_l;&c_o;&c_k;&c_a;&c_d;&c_a;</trd>
</drv>

<drv mrk="blokad.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Ccirc;irka&ubreve;sie&gcirc;i.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>belagern</trd>,
     <trd>umzingeln</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>assi&eacute;ger</trd>,
    <trd>faire le <ind>blocus</ind> (de)</trd>,
    <trd>faire le <ind>si&egrave;ge</ind> (de)</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_l;&c_o;&c_k;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
