<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: fiancx.xml,v 1.27 2021/05/28 12:14:57 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>fian&ccirc;</rad>/o <fnt><bib>PV</bib></fnt>
    </kap>

    <drv mrk="fiancx.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="fiancx.0o.promesinta">
        <uzo tip="fak">GEN</uzo>
        <dif>
          Edzi&gcirc;onto; viro havanta <tld/>on<sncref ref="fiancx.0o.promesinta"/> a&ubreve; <tld/>inon<sncref ref="fiancx.0ino.promesinta"/>:
          <ekz>
            <tld/>o kaj <tld/>ino estas ge<tld/>oj.
          </ekz>
        </dif>
        <trdgrp lng="ca">
          <trd>prom&eacute;s</trd>,
          <trd>xicot</trd>
        </trdgrp>
        <trdgrp lng="en">
          <trd>fiance</trd>,
          <trd>fianc&eacute;</trd>
        </trdgrp>
        <trdgrp lng="es">
          <trd>prometido</trd>,
          <trd>novio</trd>
        </trdgrp>
        <trd lng="fr">fianc&eacute;</trd>
        <trd lng="it">fidanzato</trd>
        <trd lng="hu">v&odblac;leg&eacute;ny</trd>        
        <trd lng="pl">narzeczony</trd>
      </snc>
      <snc mrk="fiancx.0o.eventuala">
        <dif>
          Edzigebla viro; viro ser&ccirc;anta <tld/>on a&ubreve; <tld/>inon:
          <ekz>
            multe da <tld/>oj, sed la &gcirc;usta ne venas
            <fnt><bib>PrV</bib></fnt>.
          </ekz>
        </dif>
        <trd lng="ca">casador</trd>
        <trdgrp lng="es">
          <trd>hombre casadero</trd>,
          <trd>pretendiente</trd>
        </trdgrp>
        <trd lng="hu">h&aacute;zasuland&oacute; <ind>leg&eacute;ny</ind></trd>
        <trd lng="pl">kawaler do wzi&eogonek;cia</trd>
      </snc>
      <trd lng="bg">&c_g;&c_o;&c_d;&c_je;&c_n;&c_i;&c_k;</trd>
      <trd lng="de">Verlobter</trd>
      <trd lng="io">fiancitulo</trd>
      <trd lng="nl">verloofde<klr tip="ind"> (ma.)</klr></trd>
      <trdgrp lng="pt">
        <trd>noivo</trd>,
        <trd>nubente</trd>
      </trdgrp>
      <trd lng="ru">&c_zh;&c_je;&c_n;&c_i;&c_h;</trd>
    </drv>

    <drv mrk="fiancx.0ino">
      <kap><ofc>*</ofc><tld/>ino
      <fnt><bib>F</bib>, <lok>Universala Vortaro, -in'</lok></fnt></kap>
      <snc mrk="fiancx.0ino.promesinta">
        <uzo tip="fak">GEN</uzo>
        <dif>
          Edzini&gcirc;onto; virino havanta <tld/>on<sncref ref="fiancx.0o.promesinta"/> a&ubreve; <tld/>inon<sncref ref="fiancx.0ino.promesinta"/>:
          <ekz>
            en la salono estis neniu krom li kaj lia <tld/>ino
            <fnt><bib>F</bib>, <lok>&FE; 26</lok></fnt>;
          </ekz><ekz>
            kiel bela estas via amo, mia fratino, mia <tld/>ino!
            <fnt><bib>MT</bib>, <lok>&Alt; 4:10</lok></fnt>.
          </ekz>
        </dif>
        <trdgrp lng="ca">
          <trd>promesa</trd>,
          <trd>xicota</trd>
        </trdgrp>
        <trdgrp lng="en">
          <trd>fiancee</trd>,
          <trd>fianc&eacute;e</trd>
        </trdgrp>
        <trdgrp lng="es">
          <trd>prometida</trd>,
          <trd>novia</trd>
        </trdgrp>
        <trd lng="fr">fianc&eacute;e</trd>
        <trd lng="hu">menyasszony</trd>
        <trd lng="it">fidanzata</trd>
        <trd lng="pl">narzeczona</trd>
      </snc>
      <snc mrk="fiancx.0ino.eventuala">
        <dif>
          Edzinigebla virino; virino ser&ccirc;anta <tld/>on a&ubreve; <tld/>inon:
          <ekz>
            sian edzinon Malanja'n, kiu fra&ubreve;line estis la plej ri&ccirc;a
            kaj plej bela <tld/>ino en la tula vila&gcirc;o, li amis pasie
            <fnt>
              <aut>A. Edelmann, trad. I. Kaminski</aut>,
              <vrk>La forgesita pipo</vrk>,
              <lok>Fundamenta Krestomatio, 1904</lok>
            </fnt>.
          </ekz>
        </dif>
        <trd lng="ca">(noia) n&uacute;bil</trd>
        <trd lng="es">mujer casadera</trd>
        <trd lng="pl">panna na wydaniu</trd>
      </snc>
      <trd lng="bg">&c_g;&c_o;&c_d;&c_je;&c_n;&c_i;&c_c;&c_a;</trd>
      <trd lng="bo">&#xF46;&#xF44;&#xF0B;&#xF66;&#xF0B;&#xF62;&#xF92;&#xFB1;&#xF42;&#xF0B;&#xF61;&#xF42;&#xF0B;&#xF42;&#xF72;&#xF0B;&#xF58;&#xF5B;&#xF60;&#xF0B;&#xF42;&#xFB2;&#xF7C;&#xF42;&#xF66;&#xF0B;</trd>
  <trd lng="de">Verlobte</trd>
      <trd lng="hu">elad&oacute; <ind>l&aacute;ny</ind></trd>
      <trd lng="io">fiancitino</trd>
      <trd lng="nl">verloofde<klr tip="ind"> (vr.)</klr></trd>
      <trd lng="ru">&c_n;&c_je;&c_v;&c_je;&c_s;&c_t;&c_a;</trd>
    </drv>

    <drv mrk="fiancx.0igi">
      <kap><tld/>igi, <var><kap><tld/>inigi</kap></var></kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="fiancx.0igi.GEN">
        <uzo tip="fak">GEN</uzo>
        <dif>
          Fari iun <tld/>o a&ubreve; <tld/>ino: 
          <ekz>
            la patro <tld/>igis sian filon.
          </ekz>
        </dif>
      </snc>
      <trd lng="bg">&c_s;&c_g;&c_o;&c_d;&c_ja;&c_v;&c_a;&c_m;</trd>
      <trd lng="ca">prometre <klr>(la m&agrave;)</klr></trd>
      <trd lng="de">verloben</trd>
      <trdgrp lng="en">
        <trd>affiance</trd>,
        <trd>betroth</trd>
      </trdgrp>
      <trd lng="es"><ind>prometer</ind> en matrimonio</trd>
      <trd lng="fr">fiancer</trd>
      <trd lng="hu">eljegyez</trd>
      <trd lng="io">fiancar</trd>
      <trdgrp lng="it">
        <trd>fidanzare</trd>,
        <trd>promettere in sposo/a</trd>
      </trdgrp>
      <trd lng="nl">verloven</trd>
      <trd lng="ru">&c_o;&c_b;&c_r;&c_u;&c_ch;&c_i;&c_t;&c_mol;</trd>
    </drv>

    <drv mrk="fiancx.0igxi">
      <kap><tld/>i&gcirc;i, <var><kap><tld/>ini&gcirc;i</kap></var></kap>
      <gra><vspec>ntr</vspec></gra>
      <snc mrk="fiancx.0igxi.GEN">
        <uzo tip="fak">GEN</uzo>
        <dif>
          Promesi edzi&gcirc;i a&ubreve; edzini&gcirc;i al iu:
          <ekz>
            <tld/>i&gcirc;i kun iu.
          </ekz>
        </dif>
      </snc>
      <trd lng="bg">&c_s;&c_je; <ind>&c_a;&c_n;&c_g;&c_a;&c_zh;&c_i;&c_r;&c_a;&c_m;</ind></trd>
      <trd lng="ca">prometre's</trd>
      <trd lng="de">sich <ind>verloben</ind></trd>
      <trdgrp lng="en">
        <trd>get engaged</trd>,
        <trd>become engaged</trd>
      </trdgrp>
      <trd lng="es"><ind>prometerse</ind> en matrimonio</trd>
      <trd lng="fr">se <ind>fiancer</ind></trd>
      <trd lng="hu">eljegyzi mag&aacute;t</trd>
      <trd lng="io">fianceskar</trd>
      <trd lng="it">fidanzarsi</trd>
      <trd lng="nl">zich <ind>verloven</ind></trd>
      <trd lng="pl">zar&eogonek;cza&cacute; si&eogonek;</trd>
      <trd lng="ru">&c_o;&c_b;&c_r;&c_u;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </drv>

    <drv mrk="fiancx.0igxo">
      <kap><tld/>i&gcirc;o, <var><kap><tld/>ini&gcirc;o</kap></var>,
      <var><kap>ge<tld/>i&gcirc;o</kap></var></kap>
      <snc mrk="fiancx.0igxo.GEN">
        <uzo tip="fak">GEN</uzo>
        <dif>
          Solena promeso por edzi&gcirc;i a&ubreve; edzini&gcirc;i al iu, ordinare farita
          dum festo:
          <ekz>
            oni asertis, ke li tuj edzi&gcirc;os, kaj ke oni tiam festas lian
            fian&ccirc;i&gcirc;on
            <fnt>
              <aut>A. Fournier, trad. R. Bernard</aut>,
              <vrk>La Granda Meaulnes</vrk>,
              <lok>p. 146</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="bg">&c_a;&c_n;&c_g;&c_a;&c_zh;&c_i;&c_m;&c_je;&c_n;&c_t;</trd>
      <trdgrp lng="en">
        <trd>engagement</trd>,
        <trd>betrothal</trd>
      </trdgrp>
      <trd lng="de">Verlobung</trd>
      <trd lng="es">compromiso</trd>
      <trd lng="fr">fian&ccedil;ailles</trd>
      <trd lng="io">fianco</trd>
      <trd lng="nl">verloving</trd>
      <trd lng="pl">zar&eogonek;czyny</trd>
    </drv>
  </art>
