<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: apacx.xml,v 1.23 2023/08/30 18:23:35 revo Exp $">
<kap>
  <rad>apa&ccirc;</rad>/o
</kap>

<drv mrk="apacx.0o">
  <kap><tld/>o</kap>
  <snc mrk="apacx.0o.ETN">
    <dif>
      <ref tip="super" cel="indian.0o.ETN">Indiano</ref> el multgenta 
      popolo vivinta en sudokcidenta Nordameriko per bizon&ccirc;aso, 
      rikoltado, kultivo, rabado, nun malabunde resta en Novmeksiko, 
      Arizono kaj dise en Usono:
      <ekz>
        la <tld/>oj reprezentas la orientan bran&ccirc;on de la
        nava&hcirc;oj
        <fnt>
          <aut>T. Carlevaro</aut>,
          <vrk>Skize pri indianaj kulturoj</vrk>,
          <lok>Literatura foiro, 1998-10, numero 115a, p. 32-37a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_p;&c_a;&c_ch;</trd>
    <trd lng="ca">apatxe</trd>
    <trdgrp lng="fr">
      <trd>apache<klr> (subst.)</klr></trd>
    </trdgrp>
    <trd lng="de">Apache</trd>
    <trd lng="es">apache</trd>
    <trd lng="pl">Apacz</trd>
    <trd lng="ru">&c_a;&c_p;&c_a;&c_ch;</trd>
    <trd lng="sk">Apa&ccaron;</trd>
  </snc>
  <snc mrk="apacx.0o.huligano">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="huligan.0o.POL">Huligano</ref>, precipe ano
      de tiel kromnomataj parizaj bandoj agintaj inter 1900 kaj 1920:
      <ekz>
        iu <tld/>o mortigis vin per klabo de malanta&ubreve;e, kaj &ccirc;ion
        forrabis
        <fnt><bib>Lusin</bib>, <lok>El Malnovaj legendoj nove
        redaktitaj, Revivigo</lok></fnt>.
      </ekz>
      <ekz>
        Belleville! &Gcirc;i estis iam la misfama <tld/>a kvartalo de Parizo
        <fnt>
        <aut>J. Rejt&odblac;, trad. L. Bal&aacute;zs</aut>,
        <vrk><url ref="http://www.freeweb.hu/verkoj/21b.htm">Sinjoro
        Vanek en Parizo, 2a parto</url></vrk>,
        <lok>2002-07</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_h;&c_u;&c_l;&c_ib;&c_g;&c_a;&c_n;</trd>
    <trdgrp lng="ru">
      <trd>&c_h;&c_u;&c_l;&c_i;&c_g;&c_a;&c_n;</trd>,
      <trd>&c_a;&c_p;&c_a;&c_sh;</trd>
    </trdgrp>
  </snc>
  <trd lng="de">Apache</trd>
      <trd lng="en">Apache</trd>
  <trd lng="he">&#x5D0;&#x5E4;&#x5D0;&#x5E6;'&#x5D9;</trd>
  <trd lng="hu">apacs</trd>
  <trd lng="ja">&#x30A2;&#x30D1;&#x30C3;&#x30C1;</trd>
  <trdgrp lng="uk">
    <trd>&#x445;&#x443;&#x43B;&#x456;&#x433;&#x430;&#x43D;</trd>,
    <trd>&#x430;&#x43F;&#x430;&#x448;</trd>
  </trdgrp>
</drv>

<drv mrk="apacx.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al la <tld/>oj.
    </dif>
  </snc>
  <trd lng="de">Apachen-</trd>
  <trd lng="en">Apache</trd>
  <trd lng="es">apache</trd>
  <trdgrp lng="fr">
    <trd>apache<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="hu">apacs</trd>
  <trd lng="sk">apa&ccaron;sk&yacute;</trd>
</drv>
</art>
