<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>apa&ccirc;</rad>/o
</kap>
<drv mrk="apacx.0o">
  <kap><tld/>o</kap>
  <snc mrk="apacx.0o.indiano">
    <dif>
      <ref tip="super" cel="indian.0o">Indiano</ref> el multgenta popolo
      vivinta en sudokcidenta Nordameriko per bizon&ccirc;aso, rikoltado,
      kultivo, rabado, nun malabunde resta en Novmeksiko, Arizono kaj dise
      en Usono:
      <ekz>
        la <tld/>oj reprezentas la orientan bran&ccirc;on de la
        nava&hcirc;oj<fnt><aut>Tazio Carlevaro</aut>: <vrk>Skize pri indianaj
        kulturoj</vrk>, <lok>Literatura foiro, numero 115a, 1998-10, p.
        32-37a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_p;&c_a;&c_ch;</trd>
    <trd lng="ca">apatxe</trd>
    <trdgrp lng="fr">
      <trd>apache<klr> (subst.)</klr></trd>
    </trdgrp>
    <trd lng="de">Apatsche</trd>
    <trd lng="es">apache</trd>
    <trd lng="ru">&c_a;&c_p;&c_a;&c_ch;</trd>
    <trd lng="sk">Apa&ccaron;</trd>
  </snc>
  <snc mrk="apacx.0o.huligano">
    <uzo tip="stl">ARK</uzo>
    <uzo tip="stl">EVI</uzo>
    <ekz>Iu <tld/>o mortigis vin per klabo de malanta&ubreve;e, kaj &ccirc;ion forrabis<fnt><bib>Lusin</bib>, <lok>El Malnovaj legendoj nove redaktitaj, Revivigo</lok></fnt>.</ekz>
    <ref tip="dif" cel="huligan.0o">huligano</ref>
    <trd lng="be">&c_h;&c_u;&c_l;&c_ib;&c_g;&c_a;&c_n;</trd>
    <trdgrp lng="ru">
      <trd>&c_h;&c_u;&c_l;&c_i;&c_g;&c_a;&c_n;</trd>,
      <trd>&c_a;&c_p;&c_a;&c_sh;</trd>
    </trdgrp>
  </snc>
  <trd lng="hu">apacs</trd>
</drv>

<drv mrk="apacx.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al la <tld/>oj.
    </dif>
  </snc>
  <trd lng="de">Apatschen-</trd>
  <trd lng="es">apache</trd>
  <trdgrp lng="fr">
    <trd>apache<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="hu">apacs</trd>
  <trd lng="sk">apa&ccaron;sk&yacute;</trd>
</drv>
</art>
