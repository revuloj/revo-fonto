<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fab.xml,v 1.35 2023/11/09 19:13:15 revo Exp $">
<kap><ofc>*</ofc>
<rad>fab</rad>/o</kap>
<drv mrk="fab.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fab.0o.semo">
    <uzo tip="fak">KUI</uzo>
    <dif>
      La <ref tip="super" cel="grajn.sem0o">semgrajno</ref>
      de pluraj kulturataj legomoj, ekz. <ref tip="malprt" cel="fab.0o.BOT">fabo<sncref/></ref>,
      <ref tip="malprt" cel="fazeol.0o.BOT">fazeolo</ref>, sojo, kafo, kakao ks:
      <ekz>
        <nom>&Scirc;obi</nom> alportis <klr>[&#x2026;]</klr> farunon kaj rostitajn grajnojn, kaj <tld/>ojn kaj
        lentojn <klr>[&#x2026;]</klr>
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 17:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen estas <tld/>oj, kesto da rizo, tapioko kaj makaronoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi estas en la varmaj landoj, kie kreskas la <tld/>oj de kafo
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi donis <klr>[&#x2026;]</klr> al maljuna virino kelke da kafaj <tld/>oj
        <fnt>
          <bib>Fab3</bib>
          <lok>Koboldo &ccirc;e la butikisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni atentigis pri &gcirc;ermoj de sojo kaj aliaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2011/010451.php">E&ubreve;ropo en la fajro de la epidemio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Bohne <klr>(Samen)</klr></trd>
  </snc>
  <snc mrk="fab.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Specio de <ref tip="super" cel="vici.0o">vicio</ref>
      (<trd lng="la">Vicia faba var. major</trd>), kun grandaj plataj
      grajnoj en gu&scirc;o, kulturata kiel legomo:
      <ekz>
        sur la kampoj oni semis la linon, hordeon, <tld/>on, fazeolon, kukumojn kaj
        aliajn legomojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        balkono kun kvar kolonoj, kiujn &ccirc;irka&ubreve;volvis puncofloraj <tld/>oj kaj trikoloroj
        kun belaj violkoloraj kalikoj
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      Ne konfuzu kun <ref tip="vid" cel="fazeol.0o">fazeolo</ref>.
    </rim>
    <bld lok="&WCU;/thumb/b/bb/Vicia_faba_02.jpg/360px-Vicia_faba_02.jpg" prm="CC BY-SA     3.0"><tld/>oplanto (Vicia faba), Oslo, Norvegujo
        <fnt>
          <aut>Bjoertvedt, propra verko</aut>
          <lok><url ref="&WCW;/File:Vicia_faba_02.jpg">&WCW;/File:Vicia_faba_02.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="de">
      <trd>Pferdebohne</trd>,
      <trd>Saubohne</trd>
    </trdgrp>
    <trd lng="zh">&#x8695;&#x8C46; <pr>c&#xE1;nd&#xF2;u</pr></trd>
</snc>
  <trd lng="bo">&t_sa;&t_rata;&t_na;&t_tsheg;&t_ma;&t_tsheg;</trd>
  <trdgrp lng="ja">
    <trd>&#x30BD;&#x30E9;&#x30DE;&#x30E1;</trd>,
    <trd>&#x8C46;  <pr>&#x307E;&#x3081;</pr></trd>
  </trdgrp>
  <trd lng="kek">aaws</trd>
  <trd lng="be">&c_b;&c_o;&c_b;</trd>
  <trd lng="br">favenn</trd>
  <trdgrp lng="ca">
    <trd>fava</trd>,
    <trd>favera</trd>
  </trdgrp>
  <trd lng="cs">bob</trd>
  <trdgrp lng="en">
    <trd>broad bean</trd>,
    <trd>fava bean</trd>
  </trdgrp>
  <trd lng="fa">&a_b;&a_A;&a_q;&a_l;&a_A;</trd>
  <trd lng="fr">f&egrave;ve<klr tip="ind"> (l&eacute;gume)</klr></trd>
  <trdgrp lng="hu">
    <trd>l&oacute;bab</trd>,
    <trd>laposbab</trd>,
    <trd>diszn&oacute;bab</trd>,
    <trd>l&oacute;b&uuml;kk&ouml;ny</trd>
  </trdgrp>
  <trd lng="nl">tuinboon</trd>
  <trd lng="pl">b&oacute;b</trd>
  <trd lng="pt">fava</trd>
  <trd lng="ru">&c_b;&c_o;&c_b;</trd>
  <trd lng="sk">b&ocirc;b</trd>
  <trd lng="uk">&c_b;&c_ib;&c_b;</trd>
</drv>

<drv mrk="fab.0eto">
  <kap><tld/>eto</kap>
  <snc>
    <ref tip="dif" cel="fazeol.0o">fazeolo</ref>
    <trd lng="pl">fasola</trd>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_s;&c_o;&c_l;&c_ja;</trd>
  <trdgrp lng="ca">
    <trd>fesol</trd>,
    <trd>mongeta</trd>
  </trdgrp>
<trdgrp lng="de">
  <trd>Gartenbohne</trd>,
  <trd>Gr&uuml;ne Bohne</trd>,
  <trd>Schnittbohne</trd>,
  <trd>Fisole</trd>
</trdgrp>
<trd lng="en">common bean</trd>
</drv>
</art>
