<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fab</rad>/o
</kap>
<drv mrk="fab.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="vici.0o">vicio</ref>
      (<trd lng="la">Vicia faba</trd>),
      kun grandaj renformaj grajnoj en gu&scirc;o, kulturata
      kiel legomo.
    </dif>
    <rim>
      Ne konfuzu kun <ref tip="vid" cel="fazeol.0o">fazeolo</ref>.
    </rim>
  </snc>
  <trdgrp lng="de">
    <trd>Saubohne</trd>,
    <trd>Pferdebohne</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_o;&c_b;</trd>
</drv>

<drv mrk="fab.0eto">
  <kap><tld/>eto</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="fazeol.0o">fazeolo</ref>.
    </dif>
  </snc>
</drv>
</art>
