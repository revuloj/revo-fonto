<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fab</rad>/o
</kap>
<drv mrk="fab.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="vici.0o">vicio</ref>
      (<trd lng="la">Vicia faba</trd>),
      kun grandaj renformaj grajnoj en gu&scirc;o, kulturata
      kiel legomo.
    </dif>
    <rim>
      Ne konfuzu kun <ref tip="vid" cel="fazeol.0o">fazeolo</ref>.
    </rim>
  </snc>
  <trdgrp lng="de">
    <trd>Saubohne</trd>,
    <trd>Pferdebohne</trd>
  </trdgrp>
</drv>
<drv mrk="fab.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Fabaceae</trd>),
      kun nesimetriaj papilisimilaj floroj kaj gu&scirc;oj
    </dif>
    <ref tip="sin" cel="legumen1.0acoj">legumenacoj</ref>
    <refgrp tip="sub">
      <ref cel="vici.0o">vicio</ref>,
      <ref cel="fazeol.0o">fazeolo</ref>,
      <ref cel="piz.0o">pizo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Schmetterlingsbl&uuml;tengew&auml;chse</trd>
</drv>
<drv mrk="fab.0eto">
  <kap><tld/>eto</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="fazeol.0o">fazeolo</ref>.
    </dif>
  </snc>
</drv>
</art>
