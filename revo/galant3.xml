<art mrk="$Id$">
<kap><ofc>2</ofc>
<rad>galantin</rad>/o</kap>
<drv mrk="galant3.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Malvarma man&gcirc;a&jcirc;o, farita el pecetigita kuirita porka
      a&ubreve; kokida viando, garnita per la propra gelateneca suko.
    </dif>
  </snc>
  <trd lng="de">Galantine</trd>
  <trd lng="es">galantina</trd>
  <trd lng="fr">galantine</trd>
  <trd lng="nl">galantine</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_u;&c_d;&c_je;&c_n;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_l;&c_i;&c_v;&c_n;&c_o;&c_je;</trd>,
    <trd>&c_h;&c_o;&c_l;&c_o;&c_d;&c_je;&c_c;</trd>
  </trdgrp>
</drv>
</art>
