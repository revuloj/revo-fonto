<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: horde.xml,v 1.42 2023/11/15 17:08:01 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>horde</rad>/o
</kap>

<drv mrk="horde.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">AGR</uzo>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o" lst="voko:botanikaj_genroj">Genro</ref> 
      el familio <ref tip="malprt" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Hordeum</trd>), unujara 
      <ref tip="lst" cel="gren.0o.planto" lst="voko:pooparencoj">greno</ref>
      kun longaj aristoj, kaj kies grajnoj estas uzataj por fari krudan panon, 
      griosupon, por nutri bestojn a&ubreve; por fabriki 
      <ref cel="bier.0o">bieron</ref>:
      <ekz>
        grenoj: tritiko, sekalo, <tld/>o, aveno, rizo, maizo, poligono, spelto, milio 
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la lino kaj la <tld/>o estis batdifektitaj, &ccirc;ar la <tld/>o havis spikojn kaj la lino havis
        bur&gcirc;onojn
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 9:31</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i kolektadis sur la kampo &gcirc;is la vespero, kaj &scirc;i dra&scirc;is tion, kion &scirc;i kolektis, kaj tio
        estis &ccirc;irka&ubreve; efo da <tld/>o  
        <fnt>
          <bib>MT</bib>
          <lok>Rut 2:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ebenigis &gcirc;ian supra&jcirc;on, li dis&scirc;utas ja nigelon kaj dis&jcirc;etas kuminon kaj 
        metas tritikon la&ubreve; vicoj kaj <tld/>on en la difinitan lokon kaj spelton &ccirc;e la limoj
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 28:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur &ccirc;iuj kampoj &ccirc;irka&ubreve;e kreskis greno, sekalo, <tld/>o kaj aveno
        <fnt>
          <bib>Fab2</bib>
          <lok>Poligono</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fenicia proverbo diras: pli bona estas <tld/>o en la grenejo, ol oro en la dezerto
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        belgaj alt-fermentadaj bierspecoj kun &ccirc;irka&ubreve; 5 alkoholgradoj, le&gcirc;ere lupolizita kaj farita
        el helkoloraj maltoj de apudmaraj <tld/>oj
        <fnt>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="&Monato;/2001/007144.php">Bierlando Belgio</url></vrk>
          <bib>Monato</bib>
          <lok>2001/01, p. 18</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bavara duko Vilhelmo la 4a, <klr>[&#x2026;]</klr> dekretis, ke por biero oni uzu nur <tld/>on,
        lupolon kaj akvon
        <fnt>
          <bib>Monato</bib>
          <aut>Marc Vanden Bempt</aut>
          <vrk><url ref="&Monato;/2008/009232.php">Simpatia vulgariga fakliteraturo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/d/d7/Hordeum_vulgare_L.jpg/360px-Hordeum_vulgare_L.jpg" prm="CC BY-SA     3.0">hordeo (Hordeum vulgare)
         <fnt>
           <aut>Henrik Sendelbach</aut>
           <lok><url ref="&WCW;/File:Hordeum_vulgare_L.jpg">&WCW;/File:Hordeum_vulgare_L.jpg</url></lok>
         </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_ja;&c_ch;&c_m;&c_je;&c_n;&c_mol;</trd>
  <trd lng="bg">&c_je;&c_ch;&c_je;&c_m;&c_i;&c_k;</trd>
  <trd lng="bo">&#xF60;&#xF56;&#xFB2;&#xF74;&#xF0B;</trd>
  <trd lng="br">heiz</trd>
  <trd lng="ca">ordi</trd>
  <trd lng="cs">je&ccaron;men</trd>
  <trd lng="de">Gerste</trd>
  <trd lng="el">&kappa;&rho;&jota;&theta;&alfa_ton;&rho;&jota;</trd>
  <trd lng="en">barley</trd>
  <trd lng="es">cebada</trd>
  <trd lng="fa">&a_j;&a_w;</trd>
  <trd lng="fr">orge</trd>
  <trd lng="hu">&aacute;rpa</trd>
  <trd lng="it">orzo</trd>
  <trd lng="ja">&#x30AA;&#x30AA;&#x30E0;&#x30AE;</trd>
  <trd lng="nl">gerst</trd>
  <trd lng="pl">j&eogonek;czmie&nacute;</trd>
  <trd lng="pt">cevada</trd>
  <trd lng="ru">&c_ja;&c_ch;&c_m;&c_je;&c_n;&c_mol;</trd>
  <trd lng="sk">ja&ccaron;me&ncaron;</trd>
  <trd lng="sv">korn<klr tip="amb"> (s&auml;desslag)</klr></trd>
  <trd lng="uk">&#x44F;&c_ch;&#x43C;&c_ib;&#x43D;&#x44C;</trd>
  <trdgrp lng="zh">
    <trd>&#x5927;&#x9EA5; <pr>d&#xE0;m&#xE0;i</pr></trd>,
    <trd>&#x725F; <pr>m&#xF3;u</pr></trd>,
    <trd>&#x9EB0; <pr>m&#xF3;u</pr></trd>
  </trdgrp>
</drv>
<drv mrk="horde.sablo0o">
  <kap>sablo<tld/>o</kap>
  <snc>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:botanikaj_specioj">Specio</ref>
      el familio <ref tip="malprt" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Leymus arenarius</trd>), uzata por protekti dunojn
      kontra&ubreve; erozio.
    </dif>
    <bld lok="&WCU;/thumb/a/ad/Leymus-arenarius.JPG/640px-Leymus-arenarius.JPG" prm="CC-BY-SA-3.0">sablohordeo (Leymus arenarius)
         <fnt>
           <aut>Sten Porse</aut>
           <lok><url ref="&WCW;/File:Leymus-arenarius.JPG">&WCW;/File:Leymus-arenarius.JPG</url></lok>
         </fnt>
    </bld>
    <trd lng="be">&c_k;&c_a;&c_l;&c_a;&c_s;&c_mol;&c_n;&c_ja;&c_k; &c_p;&c_ja;&c_sh;&c_ch;&c_a;&c_n;&c_y;</trd>
    <trdgrp lng="da">
      <trd>Almindelig Marehalm</trd>,
      <trd>marehalm</trd>
    </trdgrp>
    <trd lng="de">Strandroggen</trd>
    <trd lng="en">sea lyme grass</trd>
  </snc>
  <trd lng="cs">je&ccaron;menice p&iacute;se&ccaron;n&aacute;</trd>
  <trdgrp lng="pl">
    <trd>wydmuchrzyca piaskowa</trd>,
    <trd>wydmuchrzyca nadmorska <klr>(pot.)</klr></trd>
  </trdgrp>
  <trd lng="sk">ja&ccaron;menica pieso&ccaron;n&aacute;</trd>
</drv>
</art>
