<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>horde</rad>/o
</kap>

<drv mrk="horde.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">AGR</uzo>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Hordeum</trd>), unujara 
      <ref tip="lst" cel="gren.0o.planto"
      lst="voko:pooparencoj">greno</ref>
      kun longaj aristoj, kaj
      kies grajnoj estas uzataj por fari krudan panon, griosupon, por
      nutri
      bestojn a&ubreve; por fabriki 
      <ref cel="bier.0o">bieron</ref>.
    </dif>
  </snc>                     
  <trd lng="be">&c_ja;&c_ch;&c_m;&c_je;&c_n;&c_mol;</trd>
  <trd lng="bg">&c_je;&c_ch;&c_je;&c_m;&c_i;&c_k;</trd>
  <trd lng="br">heiz</trd>
  <trd lng="ca">ordi</trd>
  <trd lng="de">Gerste</trd>
  <trd lng="el">&kappa;&rho;&jota;&theta;&alfa_ton;&rho;&jota;</trd>
  <trd lng="en">barley</trd>
  <trd lng="es">cebada</trd>
  <trd lng="fa">&a_j;&a_w;</trd>
  <trd lng="fr">orge</trd>
  <trd lng="hu">&aacute;rpa</trd>
  <trd lng="it">orzo</trd>
  <trd lng="nl">gerst</trd>
  <trd lng="pl">j&eogonek;czmie&nacute;</trd>
  <trd lng="pt">cevada</trd>
  <trd lng="ru">&c_ja;&c_ch;&c_m;&c_je;&c_n;&c_mol;</trd>
  <trd lng="sv">korn<klr tip="amb"> (s&auml;desslag)</klr></trd>
</drv>
<drv mrk="horde.sablo0o">
  <kap>sablo<tld/>o</kap>
  <snc>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO"
      lst="voko:botanikaj_specioj">Specio</ref>
      el familio <ref tip="malprt" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Leymus arenarius</trd>), uzata por protekti dunojn
      kontra&ubreve; erozio.
    </dif>
    <trdgrp lng="da">
      <trd>Almindelig Marehalm</trd>,
      <trd>marehalm</trd>
    </trdgrp>
    <trd lng="de">Strandroggen</trd>
    <trd lng="en">sea lyme grass</trd>
  </snc>
</drv>
</art>
