<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>horde</rad>/o
</kap>
<drv mrk="horde.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">AGR</uzo>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Hordeum</trd>), unujara grena vegeta&jcirc;o,
      kies grajnoj estas uzataj por fari krudan panon, por nutri
      bestojn a&ubreve; por fabriki bieron.
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_ch;&c_m;&c_je;&c_n;&c_mol;</trd>
  <trd lng="ca">ordi</trd>
  <trd lng="de">Gerste</trd>
  <trd lng="el">&kappa;&rho;&jota;&theta;&alfa_ton;&rho;&jota;</trd>
  <trd lng="en">barley</trd>
  <trd lng="es">cebada</trd>
  <trd lng="fr">orge</trd>
  <trd lng="hu">&aacute;rpa</trd>
  <trd lng="nl">gerst</trd>
  <trd lng="pt">cevada</trd>
  <trd lng="ru">&c_ja;&c_ch;&c_m;&c_je;&c_n;&c_mol;</trd>
</drv>
</art>
