<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>horde</rad>/o
</kap>
<drv mrk="horde.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="po1.0acoj">poacoj</ref>
      (<trd lng="la">Hordeum</trd>), unujara grena vegeta&jcirc;o,
      kies grajnoj estas uzataj por fari krudan panon, por nutri
      bestojn a&ubreve; por fabriki bieron.
    </dif>
  </snc>
  <trd lng="de">Gerste</trd>
</drv>
</art>
