<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>frand</rad>/i
</kap>
<drv mrk="frand.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Pro plezuro man&gcirc;i ion kiel delikatan bongusta&jcirc;on:
      <ekz>
        <tld/>i bombonojn, likvorojn, paste&ccirc;on;
      </ekz>
      <ekz>
        la infanoj <tld/>is la kukon;
      </ekz>
      <ekz>
        la vermoj <tld/>u lin
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Ijob 24, 20</lok>)</fnt>;
      </ekz>
      <ekz>
        <klr>(f)</klr>
        la tiutempaj Grekoj tre <tld/>is la pirata&jcirc;ojn.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="gust.0umi">gustumi</ref>, 
      <ref cel="regal.0i">sin regali</ref>, 
      <ref cel="mangx.0egi">man&gcirc;egi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>naschen</trd>,
    <trd>schlecken</trd>
  </trdgrp>
  <trd lng="en">relish</trd>
</drv>
<drv mrk="frand.0a">
  <kap><tld/>a</kap> 
  <uzo tip="klr">(io)</uzo> 
  <snc>
    <dif>
      Delikate loganta la guston. 
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tre alloga:
      <ekz>
        tiel <tld/>an titolon<fnt>Z</fnt> ambicias multaj homoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">all&eacute;chant</trd>
</drv>
<drv mrk="frand.0ema">
  <kap><tld/>ema</kap> 
  <uzo tip="klr">(iu)</uzo>
  <snc>
    <dif>
      Tre &scirc;atanta bongusta&jcirc;ojn. 
    </dif>
  </snc>
  <trd lng="fr">gourmet</trd>
</drv>
<drv mrk="frand.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Delikata&jcirc;o, kiu logas la guston.  
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>N&auml;scherei</trd>,
    <trd>Leckerbissen</trd>
  </trdgrp>
  <trd lng="fr">gourmandise</trd>
</drv>
</art>
