<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: frand.xml,v 1.27 2015/10/26 12:12:05 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>frand</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="frand.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="frand.0i.KUI">
    <dif>
      Pro plezuro man&gcirc;i a&ubreve; trinki ion kiel delikatan
      bongusta&jcirc;on:
      <ekz>
        <tld/>i bombonojn, likvorojn, paste&ccirc;on;
      </ekz>
      <ekz>
        la infanoj <tld/>is la kukon;
      </ekz>
      <ekz>
        la vermoj <tld/>u lin
        <fnt><bib>MT</bib>, <lok>&Ijo; 24:20</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la tiutempaj Grekoj tre <tld/>is la pirata&jcirc;ojn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gust.0umi">gustumi</ref>,
      <ref cel="mangx.0egi">man&gcirc;egi</ref>,
      <ref cel="regal.0i">sin regali</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_s;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trd lng="cs">mlsat</trd>
  <trdgrp lng="de">
    <trd>naschen</trd>,
    <trd>schlecken</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>relish</trd>,
    <trd>enjoy</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>degustar</trd>,
    <trd>saborear</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>manger avec gourmandise</trd>,
    <trd>savourer</trd>,
    <trd>se <ind>r&eacute;galer</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nyalakodik</trd>,
    <trd>nassol</trd>,
    <trd>&iacute;nyenckedik</trd>
  </trdgrp>
  <trd lng="nl">snoepen</trd>
  <trdgrp lng="pl">
    <trd>&lstroke;asowa&cacute;</trd>,
    <trd>pa&lstroke;aszowa&cacute;</trd>,
    <trd>raczy&cacute; si&eogonek;</trd>,
    <trd>delektowa&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>saborear</trd>,
    <trd>degustar</trd>,
    <trd>saborear-se com</trd>,
    <trd>deleitar-se com</trd>
  </trdgrp>
  <trd
  lng="ru">&c_l;&c_a;&c_k;&c_o;&c_m;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="sk">ma&scaron;krti&tcaron;</trd>
</drv>

<drv mrk="frand.0a">
  <kap><tld/>a</kap>
  <uzo tip="klr">(io)</uzo>
  <snc mrk="frand.0a.KUI">
    <dif>
      Delikate loganta la guston.
    </dif>
    <trd lng="be">&c_l;&c_a;&c_s;&c_y;
    <klr>(&c_s;&c_m;&c_a;&c_ch;&c_n;&c_y;)</klr></trd>
    <trdgrp lng="en">
      <trd>appetizing</trd>,
      <trd>delicious</trd>,
      <trd>tasty</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>apetyczny</trd>,
      <trd>smakowity</trd>,
      <trd>pyszny</trd>,
      <trd>wy&sacute;mienity</trd>,
      <trd>pycha<klr> (pot.)</klr></trd>,
      <trd>palce liza&cacute;<klr> (pot.)</klr></trd>
    </trdgrp>
    <trd lng="pt">apetitoso</trd>
  </snc>
  <snc mrk="frand.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tre alloga:
      <ekz>
        tiel <tld/>a titolo logis nun multajn
        <fnt><bib>Fab2</bib>, <lok>sonorilo</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_l;&c_a;&c_s;&c_y;
      <klr>(&c_p;&c_je;&c_r;&c_a;&c_n;.)</klr></trd>,
      <trd>&c_s;&c_p;&c_a;&c_k;&c_u;&c_s;&c_mol;&c_l;&c_ib;&c_v;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>enticing</trd>,
      <trd>appetizing</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>apetyczny</trd>,
      <trd>smakowity</trd>
    </trdgrp>
    <trd lng="pt">apetitoso</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>dobr&yacute;</trd>,
    <trd>labu&zcaron;nicky p&rcaron;ita&zcaron;liv&yacute;</trd>
  </trdgrp>
  <trd lng="fr">all&eacute;chant</trd>
  <trdgrp lng="hu">
    <trd>&iacute;nycsikland&oacute;</trd>,
    <trd>k&iacute;v&aacute;natos</trd>,
    <trd>&eacute;tv&aacute;gygerjeszt&odblac;</trd>
  </trdgrp>
  <trd lng="nl">lekker</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_b;&c_l;&c_a;&c_z;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_a;&c_p;&c_p;&c_je;&c_t;&c_i;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_l;&c_a;&c_k;&c_o;&c_m;&c_y;&c_j;</trd>,
  </trdgrp>
  <trd lng="sk">labu&zcaron;n&iacute;cky</trd>
</drv>

<drv mrk="frand.0ajxo">
  <kap><ofc>*</ofc><tld/>a&jcirc;o</kap>
  <snc mrk="frand.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Delikata&jcirc;o, kiu logas la guston:
      <ekz>
        glacia&jcirc;o estas dol&ccirc;a glaciigita <tld/>a&jcirc;o
        <fnt><bib>F</bib>, <lok>&FE; 35</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_s;&c_u;&c_n;&c_a;&c_k;</trd>
  <trdgrp lng="de">
    <trd>N&auml;scherei</trd>,
    <trd>Leckerbissen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>delicacy</trd>,
    <trd>tasty morsel</trd>
  </trdgrp>
  <trd lng="fr">gourmandise<klr tip="ind"> (qc qui
  r&eacute;gale)</klr></trd>
  <trdgrp lng="hu">
    <trd>&iacute;nyencs&eacute;g</trd>,
    <trd>&iacute;nyenc falat</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>lekkernij</trd>,
    <trd>delicatesse</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przysmak</trd>,
    <trd>smako&lstroke;yk</trd>,
    <trd>delikates</trd>,
    <trd>frykas</trd>,
    <trd>&lstroke;akocie<klr> {pl.}</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>guloseima</trd>,
    <trd>gulodice</trd>,
    <trd>acepipe</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_a;&c_k;&c_o;&c_m;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>

<drv mrk="frand.0ema">
  <kap><tld/>ema</kap>
  <uzo tip="klr">(iu)</uzo>
  <snc mrk="frand.0ema.KUI">
    <dif>
      Tre &scirc;atanta bongusta&jcirc;ojn.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_s;&c_y; <klr>(&c_n;&c_a;
  &c_sh;&c_t;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  <trdgrp lng="cs">
    <trd>labu&zcaron;nick&yacute;</trd>,
    <trd>mlsn&yacute;</trd>
  </trdgrp>
  <trd lng="en">sweet-toothed</trd>
  <trdgrp lng="fr">
    <trd>gourmet<klr> (adj.)</klr></trd>,
    <trd>gourmand<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&iacute;nyenc</trd>,
    <trd>nyal&aacute;nk</trd>,
    <trd>p&aacute;kosztos</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>snoeperig</trd>,
    <trd>snoepachtig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&lstroke;asy</trd>,
    <trd>&lstroke;akomy</trd>
  </trdgrp>
  <trd lng="pt">guloso</trd>
  <trd lng="ru">&c_l;&c_ju;&c_b;&c_ja;&c_shch;&c_i;&c_j;
  &c_p;&c_o;&c_l;&c_a;&c_k;&c_o;&c_m;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>labu&zcaron;n&iacute;cky</trd>,
    <trd>ma&scaron;krtn&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="frand.0emulo">
  <kap><tld/>emulo</kap>
  <snc mrk="frand.0emulo.KOMUNE">
    <dif>
      Homo tre &scirc;atanta bongusta&jcirc;ojn.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>labu&zcaron;n&iacute;k</trd>,
    <trd>mlsoun</trd>,
    <trd>mlsout</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>gastronome</trd>,
    <trd>person with a sweet tooth</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gourmet<klr> (subst.)</klr></trd>,
    <trd>gourmand<klr> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>lucullus</trd>,
    <trd>smulpaap</trd>,
    <trd>lekkerbek</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&lstroke;asuch</trd>,
    <trd>&lstroke;akomczuch</trd>,
    <trd>smakosz</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>labu&zcaron;n&iacute;k</trd>,
    <trd>ma&scaron;krtn&iacute;k</trd>
  </trdgrp>
</drv>

<drv mrk="frand.0inda">
  <kap><tld/>inda</kap>
  <snc mrk="frand.0inda.KOMUNE">
    <dif>
      Kiun oni deziras <tld/>i;
      <ref tip="vid" cel="gust.bon0a">bongust</ref>ega:
      <ekz>
        Elly, la tortetoj vere estas <tld/>indaj! 
        <fnt>
          <aut>L. Couperus, trad. G. Berveling</aut>,
          <vrk>Pri maljunuloj, la a&jcirc;oj kiuj pasas...</vrk>,
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>appetizing</trd>,
    <trd>mouthwatering</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;licieux</trd>
</drv>
</art>
