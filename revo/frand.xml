<kap>
  <ofc>*</ofc>
  <rad>frand</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="frand.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="frand.0i.KUI">
    <dif>
      Pro plezuro <ref tip="super" cel="mangx.0i.macxi">man&gcirc;i</ref> a&ubreve;
      <ref tip="super" cel="trink.0i.gluti">trinki</ref> ion kiel delikatan
      bongusta&jcirc;on:
      <ekz>
        <tld/>i mielon
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2006/009020.php">Bavario funebras pro Beppo
          Bruno</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        du maljunaj virinoj frandis siajn teon kaj kukojn
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        familiaruloj tie &ccirc;i regule <tld/>is hejmecajn kuira&jcirc;ojn
        <fnt>
          <bib>ChB</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[mi]</klr> eliros por teman&gcirc;eto, kaj post tio frandos iom da opio
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        la vermoj <tld/>u lin
        <fnt><bib>MT</bib>, <lok>&Ijo; 24:20</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;iu fi&scirc;o <klr>[&#8230;]</klr> jam <tld/>is peceton de droninta maristo
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        perokule mi &scirc;tel<tld/>is la promesdonajn bongusta&jcirc;ojn de &Scirc;ia plenforma korpo
        <fnt>
          <bib>SkandalJozef</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dibocx.0i">dibo&ccirc;i</ref>,
      <ref cel="gust.0umi">gustumi</ref>,
      <ref cel="gxu.0i.senti">&gcirc;ui</ref>,
      <ref cel="mangx.0egi">man&gcirc;egi</ref>,
      <ref cel="regal.0i">sin regali</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_s;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trd lng="cs">mlsat</trd>
  <trdgrp lng="de">
    <trd>naschen</trd>,
    <trd>schlecken</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>relish</trd>,
    <trd>enjoy</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>degustar</trd>,
    <trd>saborear</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>manger avec gourmandise</trd>,
    <trd>savourer</trd>,
    <trd>se <ind>r&eacute;galer</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nyalakodik</trd>,
    <trd>nassol</trd>,
    <trd>&iacute;nyenckedik</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x597D;&#12435;&#12391;&#x98DF;&#12409;&#x308B;  <pr>&#12371;&#x306E;&#12435;&#12391;&#x305F;&#12409;&#x308B;</pr></trd>,
    <trd>&#x597D;&#x304D;&#12391;&#12354;&#x308B;  <pr>&#12377;&#x304D;&#12391;&#12354;&#x308B;</pr></trd>,
    <trd>&#x8CDE;&#21619;&#12377;&#x308B;  <pr>&#12375;&#12423;&#12358;&#x307F;&#12377;&#x308B;</pr></trd>,
    <trd>&#21619;&#x308F;&#12358;  <pr>&#12354;&#12376;&#x308F;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">snoepen</trd>
  <trdgrp lng="pl">
    <trd>&lstroke;asowa&cacute;</trd>,
    <trd>pa&lstroke;aszowa&cacute;</trd>,
    <trd>raczy&cacute; si&eogonek;</trd>,
    <trd>delektowa&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>saborear</trd>,
    <trd>degustar</trd>,
    <trd>saborear-se com</trd>,
    <trd>deleitar-se com</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_a;&c_k;&c_o;&c_m;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="sk">ma&scaron;krti&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&c_a;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43D;&c_a;&c_s;&#x43E;&#x43B;&#x43E;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>
  </trdgrp>
  <trd lng="zh">&#20599;&#20599;&#22320;&#21507; <pr>t&#x14D;ut&#x14D;ud&#xEC;ch&#x12B;</pr></trd>
</drv>

<drv mrk="frand.0a">
  <kap><tld/>a</kap>
  <uzo tip="klr">(io)</uzo>
  <snc mrk="frand.0a.KUI">
    <dif>
      <ref tip="super" cel="delika.0a.io_bona">Delikate</ref> loganta la gusto- a&ubreve; flarosenton:
      <ekz>
        kuko estas &ccirc;ia speco de <tld/>a pano
        <fnt>
          <bib>LR</bib>
          <lok>Unua Parto &ndash; Leksikologio</lok>
        </fnt>;
      </ekz>
      <ekz>
        butikoj plenaj de <tld/>aj man&gcirc;a&jcirc;oj
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        peza odoro, en kiu cigaredfumo miksi&gcirc;is
        kun <tld/>aj naztikla&jcirc;oj
        <fnt>
          <bib>ChB</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="gust.bon0a">bongusta</ref>,
       <ref cel="dolcx.0a.gusto">dol&ccirc;a</ref>,
       <ref cel="miel.0a.de_mielo">miela</ref>
    </refgrp>
    <trd lng="be">&c_l;&c_a;&c_s;&c_y;
    <klr>(&c_s;&c_m;&c_a;&c_ch;&c_n;&c_y;)</klr></trd>
    <trdgrp lng="de">
      <trd>lecker</trd>,
      <trd>delizi&ouml;s</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>appetizing</trd>,
      <trd>delicious</trd>,
      <trd>tasty</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>apetyczny</trd>,
      <trd>smakowity</trd>,
      <trd>pyszny</trd>,
      <trd>wy&sacute;mienity</trd>,
      <trd>pycha<klr> (pot.)</klr></trd>,
      <trd>palce liza&cacute;<klr> (pot.)</klr></trd>
    </trdgrp>
    <trd lng="pt">apetitoso</trd>
    <trdgrp lng="zh">
    <trd>&#x7F8E;&#x7F8E; <pr>m&#x11B;im&#x11B;i</pr></trd>,
    <trd>&#26368;&#x53EF;&#x53E3; <pr>zu&#xEC;k&#x11B;k&#x1D2;u</pr></trd>,
    <trd>&#26368;&#x7F8E;&#21619; <pr>zu&#xEC;m&#x11B;iw&#xE8;i</pr></trd>,
    <trd>&#x79C0;&#x4E3D; <pr>xi&#xF9;l&#xEC;</pr></trd>,
    <trd>&#x53EF;&#x53E3; <pr>k&#x11B;k&#x1D2;u</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="frand.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tre alloga:
      <ekz>
        tiel <tld/>a titolo logis nun multajn
        <fnt><bib>Fab2</bib>, <lok>sonorilo</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="cxarm.0a">&ccirc;arma</ref>,
       <ref cel="placx.0a">pla&ccirc;a</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_l;&c_a;&c_s;&c_y;
      <klr>(&c_p;&c_je;&c_r;&c_a;&c_n;.)</klr></trd>,
      <trd>&c_s;&c_p;&c_a;&c_k;&c_u;&c_s;&c_mol;&c_l;&c_ib;&c_v;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>enticing</trd>,
      <trd>appetizing</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>apetyczny</trd>,
      <trd>smakowity</trd>
    </trdgrp>
    <trd lng="pt">apetitoso</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>dobr&yacute;</trd>,
    <trd>labu&zcaron;nicky p&rcaron;ita&zcaron;liv&yacute;</trd>
  </trdgrp>
  <trd lng="fr">all&eacute;chant</trd>
  <trdgrp lng="hu">
    <trd>&iacute;nycsikland&oacute;</trd>,
    <trd>k&iacute;v&aacute;natos</trd>,
    <trd>&eacute;tv&aacute;gygerjeszt&odblac;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7F8E;&#21619;&#x306E;  <pr>&#12403;&#x307F;&#x306E;</pr></trd>,
    <trd>&#x304A;&#12356;&#12375;&#12356;&#x305D;&#12358;&#x306A;</trd>
  </trdgrp>
  <trd lng="nl">lekker</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_b;&c_l;&c_a;&c_z;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_a;&c_p;&c_p;&c_je;&c_t;&c_i;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_l;&c_a;&c_k;&c_o;&c_m;&c_y;&c_j;</trd>,
  </trdgrp>
  <trd lng="sk">labu&zcaron;n&iacute;cky</trd>
  <trd lng="uk">&#x43E;&c_h;&#x43E;&c_ch;&c_i;&c_j; &#x43F;&#x43E;&#x43B;&c_a;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="frand.0ajxo">
  <kap><ofc>*</ofc><tld/>a&jcirc;o</kap>
  <snc mrk="frand.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Delikata&jcirc;o, kiu logas la guston:
      <ekz>
        glacia&jcirc;o estas dol&ccirc;a glaciigita <tld/>a&jcirc;o
        <fnt><bib>F</bib>, <lok>&FE; 35</lok></fnt>;
      </ekz>
      <ekz>
        preska&ubreve; &ccirc;iuj ili amas <tld/>a&jcirc;ojn, ekzemple sukeron
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i alportis kun si kafon kaj cikorion, <klr>[kaj]</klr> por la malri&ccirc;a
        taglaboristino, kiu longe ne havis tiajn <tld/>a&jcirc;ojn, tia donaco estis bonfaro
        <fnt>
          <bib>Fab4</bib>
          <lok>Anneto</lok>
        </fnt>;
      </ekz>
      <ekz>
        bongustaj ranoj, malgrandaj serpentoj kaj &ccirc;iaj troveblaj <tld/>a&jcirc;oj por cikonioj
        <fnt>
          <bib>Fab1</bib>
          <lok>Cikonioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la vortoj de kalumnianto estas kiel <tld/>a&jcirc;oj, kaj ili penetras en la profundon
        de la ventro
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 18:8</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="desert.0o">deserto</ref>,
      <ref cel="dolcx.0ajxo">dol&ccirc;a&jcirc;o</ref>,
      <ref cel="konfit.0ajxo">konfita&jcirc;o</ref>,
      <ref cel="suker.0ajxo">sukera&jcirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_s;&c_u;&c_n;&c_a;&c_k;</trd>
  <trdgrp lng="de">
    <trd>N&auml;scherei</trd>,
    <trd>Leckerbissen</trd>,
    <trd>Delikatesse</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>delicacy</trd>,
    <trd>tasty morsel</trd>
  </trdgrp>
  <trd lng="fr">gourmandise<klr tip="ind"> (qc qui
  r&eacute;gale)</klr></trd>
  <trdgrp lng="hu">
    <trd>&iacute;nyencs&eacute;g</trd>,
    <trd>&iacute;nyenc falat</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x304A;&#12356;&#12375;&#12356;&#12418;&#x306E;</trd>,
    <trd>&#x73CD;&#21619;  <pr>&#12385;&#12435;&#x307F;</pr></trd>,
    <trd>&#12372;&#x99B3;&#x8D70;  <pr>&#12372;&#12385;&#x305D;&#12358;</pr></trd>,
    <trd>&#x83D3;&#x5B50;  <pr>&#x304B;&#12375;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>lekkernij</trd>,
    <trd>delicatesse</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przysmak</trd>,
    <trd>smako&lstroke;yk</trd>,
    <trd>delikates</trd>,
    <trd>frykas</trd>,
    <trd>&lstroke;akocie<klr> {pl.}</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>guloseima</trd>,
    <trd>gulodice</trd>,
    <trd>acepipe</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_a;&c_k;&c_o;&c_m;&c_s;&c_t;&c_v;&c_o;</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&c_a;&c_s;&#x43E;&c_shch;&c_ib;</trd>,
    <trd>&#x43B;&c_a;&#x43A;&c_ib;&c_t;&#x43A;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7F8E;&#21619; <pr>m&#x11B;iw&#xE8;i</pr></trd>,
    <trd>&#x7CBE;&#x81F4; <pr>j&#x12B;ngzh&#xEC;</pr></trd>,
    <trd>&#x73CD;&#39240; <pr>zh&emacron;nxi&#x16B;</pr></trd>
  </trdgrp>
