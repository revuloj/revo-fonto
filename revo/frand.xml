<drv mrk="frand.0ema">
  <kap><tld/>ema</kap>
  <uzo tip="klr">(iu)</uzo>
  <snc mrk="frand.0ema.KUI">
    <dif>
      Tre &scirc;atanta bongusta&jcirc;ojn:
      <ekz>
        &ccirc;u <klr>[&#8230;]</klr> la homoj &ccirc;iam <klr>[estis]</klr> enviemaj, <tld/>emaj,
        drinkemaj, avaraj <klr>[&#8230;]</klr>?
        <fnt>
          <bib>Kandid</bib>
        </fnt>;
      </ekz>
      <ekz>
        trovi&gcirc;as en la kastelo aliaj fruktoj ol tiuj de la &gcirc;ardeno <klr>[&#8230;]</klr> &ndash; en
        &gcirc;i lo&gcirc;as du belulinoj kaj kredeble la kapitano estas <tld/>ema de tiaj
        bongusta&jcirc;oj
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        mieno de <tld/>ema knabineto &gcirc;uanta bombonon
        <fnt>
          <bib>ChB</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li formetis la gazeton kaj rigardis min per <tld/>emaj okuloj
        <fnt>
          <bib>KrB</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="gxu.0ema">&gcirc;uema</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_s;&c_y; <klr>(&c_n;&c_a;
  &c_sh;&c_t;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  <trdgrp lng="cs">
    <trd>labu&zcaron;nick&yacute;</trd>,
    <trd>mlsn&yacute;</trd>
  </trdgrp>
  <trd lng="de">naschhaft</trd>
  <trd lng="en">sweet-toothed</trd>
  <trdgrp lng="fr">
    <trd>gourmet<klr> (adj.)</klr></trd>,
    <trd>gourmand<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&iacute;nyenc</trd>,
    <trd>nyal&aacute;nk</trd>,
    <trd>p&aacute;kosztos</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7F8E;&#x98DF;&#x5BB6;&#x306E;  <pr>&#12403;&#12375;&#12423;&#x304F;&#x304B;&#x306E;</pr></trd>,
    <trd>&#x53E3;&#x306E;&#x80A5;&#12360;&#x305F;  <pr>&#12371;&#12358;&#x306E;&#12371;&#12360;&#x305F;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>snoeperig</trd>,
    <trd>snoepachtig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&lstroke;asy</trd>,
    <trd>&lstroke;akomy</trd>
  </trdgrp>
  <trd lng="pt">guloso</trd>
  <trd lng="ru">&c_l;&c_ju;&c_b;&c_ja;&c_shch;&c_i;&c_j;
  &c_p;&c_o;&c_l;&c_a;&c_k;&c_o;&c_m;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>labu&zcaron;n&iacute;cky</trd>,
    <trd>ma&scaron;krtn&yacute;</trd>
  </trdgrp>
  <trd lng="uk">&#x43E;&c_h;&#x43E;&c_ch;&c_i;&c_j; &#x43F;&#x43E;&#x43B;&c_a;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x995E;&#22068; <pr>ch&#xE1;nzu&#x1D0;</pr></trd>,
    <trd>&#x995E; <pr>ch&#xE1;n</pr></trd>
  </trdgrp>
</drv>

