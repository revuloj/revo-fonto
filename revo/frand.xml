<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>frand</rad>/i
</kap>
<drv mrk="frand.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="frand.0i.KUI">
    <dif>
      Pro plezuro man&gcirc;i ion kiel delikatan bongusta&jcirc;on:
      <ekz>
        <tld/>i bombonojn, likvorojn, paste&ccirc;on;
      </ekz>
      <ekz>
        la infanoj <tld/>is la kukon;
      </ekz>
      <ekz>
        la vermoj <tld/>u lin
        <fnt><bib>MT</bib>, <lok>&Ijo; 24:20</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la tiutempaj Grekoj tre <tld/>is la pirata&jcirc;ojn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gust.0umi">gustumi</ref>,
      <ref cel="mangx.0egi">man&gcirc;egi</ref>,
      <ref cel="regal.0i">sin regali</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_s;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="de">
    <trd>naschen</trd>,
    <trd>schlecken</trd>
  </trdgrp>
  <trd lng="en">relish</trd>
  <trdgrp lng="fr">
    <trd>manger avec gourmandise</trd>,
    <trd>savourer</trd>,
    <trd>se <ind>r&eacute;galer</ind></trd>
  </trdgrp>
  <trd lng="nl">snoepen</trd>
  <trd lng="ru">&c_l;&c_a;&c_k;&c_o;&c_m;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="frand.0a">
  <kap><tld/>a</kap>
  <uzo tip="klr">(io)</uzo>
  <snc mrk="frand.0a.KUI">
    <dif>
      Delikate loganta la guston.
    </dif>
    <trd lng="be">&c_l;&c_a;&c_s;&c_y; <klr>(&c_s;&c_m;&c_a;&c_ch;&c_n;&c_y;)</klr></trd>
  </snc>
  <snc mrk="frand.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tre alloga:
      <ekz>
        tiel <tld/>a titolo logis nun multajn
        <fnt><bib>Fab2</bib>, <lok>sonorilo</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_l;&c_a;&c_s;&c_y; <klr>(&c_p;&c_je;&c_r;&c_a;&c_n;.)</klr></trd>,
      <trd>&c_s;&c_p;&c_a;&c_k;&c_u;&c_s;&c_mol;&c_l;&c_ib;&c_v;&c_y;</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">all&eacute;chant</trd>
</drv>

<drv mrk="frand.0ema">
  <kap><tld/>ema</kap>
  <uzo tip="klr">(iu)</uzo>
  <snc mrk="frand.0ema.KUI">
    <dif>
      Tre &scirc;atanta bongusta&jcirc;ojn.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_s;&c_y; <klr>(&c_n;&c_a; &c_sh;&c_t;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  <trdgrp lng="fr">
    <trd>gourmet</trd>,
    <trd>gourmand<klr tip="ind"> (gourmet)</klr></trd>
  </trdgrp>
  <trd lng="nl">snoepziek</trd>
  <trd lng="ru">&c_l;&c_ju;&c_b;&c_ja;&c_shch;&c_i;&c_j; &c_p;&c_o;&c_l;&c_a;&c_k;&c_o;&c_m;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="frand.0ajxo">
  <kap><ofc>*</ofc><tld/>a&jcirc;o</kap>
  <snc mrk="frand.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Delikata&jcirc;o, kiu logas la guston:
      <ekz>
        glacia&jcirc;o estas dol&ccirc;a glaciigita <tld/>a&jcirc;o
        <fnt><bib>F</bib>, <lok>&FE; 35</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_s;&c_u;&c_n;&c_a;&c_k;</trd>
  <trdgrp lng="de">
    <trd>N&auml;scherei</trd>,
    <trd>Leckerbissen</trd>
  </trdgrp>
  <trd lng="fr">gourmandise<klr tip="ind"> (qc qui r&eacute;gale)</klr></trd>
  <trd lng="nl">snoep</trd>
  <trd lng="ru">&c_l;&c_a;&c_k;&c_o;&c_m;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>
</art>
