<art mrk="$Id: mikrob.xml,v 1.24 2024/01/06 12:42:28 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>mikrob</rad>/o
</kap>
<drv mrk="mikrob.0o">
  <kap><tld/>o</kap>
  <snc mrk="mikrob.0o.BAK">
    <tezrad fak="BAK"/>
    <uzo tip="fak">BAK</uzo>
    <dif>
      Mikroskopa, &gcirc;enerale unu&ccirc;ela organismo, ekzemple
      <ref tip="sub" cel="bakter.0o.BAK">bakterio</ref>,
      <ref tip="sub" cel="gist.0o.BAK">gisto</ref>,
      <ref tip="sub" cel="protoz.0o.BAK">protozoo</ref> a&ubreve;
      <ref tip="sub" cel="virus.0o.BAK">viruso</ref>:
      <ekz>
        la&ubreve; oficialaj informoj de Monda Organiza&jcirc;o pri
        Sano, nuntempe
        en
        la mondo vivaj <tld/>oj ka&ubreve;zas &ccirc;irka&ubreve; 15 %
        de &ccirc;iuj malignaj
        tumoroj
        <fnt>
        <aut>Vladimir Lemelev</aut>,
        <vrk>Mikroboj provokas neinfektajn malsanojn
        &scirc;an&gcirc;i&gcirc;i al
        infektaj</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2002a, numero 1a, p. 16-17a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_ib;&c_k;&c_r;&c_o;&c_b;</trd>
    <trd lng="ca">microbi</trd>
    <trd lng="de">Mikrobe</trd>
    <trdgrp lng="en">
    <trd>microbe</trd>
    <trd>microorganism</trd>
    <trd>germ (cause of disease)</trd>
    </trdgrp>
    <trd lng="es">microbio</trd>
    <trd lng="fa">&a_m;&f_y;&f_k;&a_r;&a_w;&a_b;</trd>
    <trdgrp lng="fr">
      <trd>microbe</trd>,
      <trd>micro-organisme</trd>
    </trdgrp>
    <trd lng="hu">mikr&oacute;ba</trd>
    <trd lng="id">mikroba</trd>
    <trd lng="nl">microbe</trd>
    <trd lng="pl">mikrob</trd>
    <trd lng="ru">&c_m;&c_i;&c_k;&c_r;&c_o;&c_b;</trd>
  </snc>
  <trd lng="cs">mikrob</trd>
  <trd lng="ja">&#x5FAE;&#x751F;&#29289;  <pr>&#12403;&#x305B;&#12356;&#12406;&#12388;</pr></trd>
  <trd lng="sk">mikr&#xF3;b</trd>
  <trd lng="uk">&#x43C;&c_ib;&#x43A;&c_r;&#x43E;&c_b;</trd>
</drv>
</art>
