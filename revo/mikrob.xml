<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>mikrob</rad>/o
</kap>
<drv mrk="mikrob.0o">
  <kap><tld/>o</kap>
  <snc mrk="mikrob.0o.BAK">
    <tezrad fak="BAK"/>
    <uzo tip="fak">BAK</uzo>
    <dif>
      Mikroskopa unu&ccirc;ela organismo, nome
      <ref tip="sub" cel="bakter.0o.BAK">bakterio</ref>,
      <ref tip="sub" cel="gist.0o.BAK">gisto</ref>,
      <ref tip="sub" cel="protoz.0o.BAK">protozoo</ref> a&ubreve;
      <ref tip="sub" cel="virus.0o.BAK">viruso</ref>:
      <ekz>
        la&ubreve; oficialaj informoj de Monda Organiza&jcirc;o pri Sano, nuntempe
        en
        la mondo vivaj <tld/>oj ka&ubreve;zas &ccirc;irka&ubreve; 15 % de &ccirc;iuj malignaj
        tumoroj
        <fnt>
        <aut>Vladimir Lemelev</aut>,
        <vrk>Mikroboj provokas neinfektajn malsanojn &scirc;an&gcirc;i&gcirc;i al
        infektaj</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2002a, numero 1a, p. 16-17a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_k;&c_r;&c_o;&c_b;</trd>
  <trd lng="de">Mikrobe</trd>
  <trd lng="en">microbe</trd>
  <trd lng="es">microbio</trd>
  <trd lng="fa">&a_m;&f_y;&f_k;&a_r;&a_w;&a_b;</trd>
  <trdgrp lng="fr">
    <trd>microbe</trd>,
    <trd>micro-organisme</trd>
  </trdgrp>
  <trd lng="hu">mikr&oacute;ba</trd>
  <trd lng="nl">microbe</trd>
  <trd lng="ru">&c_m;&c_i;&c_k;&c_r;&c_o;&c_b;</trd>
</drv>
</art>
