<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mikrob.xml,v 1.29 2024/01/26 17:40:06 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>mikrob</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="mikrob.0o">
  <kap><tld/>o</kap>
  <snc mrk="mikrob.0o.BAK">
    <tezrad fak="BAK"/>
    <uzo tip="fak">BAK</uzo>
    <dif>
      Mikroskopa, &gcirc;enerale unu&ccirc;ela organismo, ekzemple
      <ref tip="sub" cel="bakter.0o.BAK">bakterio</ref>,
      <ref tip="sub" cel="gist.0o.BAK">gisto</ref>,
      <ref tip="sub" cel="protoz.0o.BAK">protozoo</ref> a&ubreve;
      <ref tip="sub" cel="virus.0o.BAK">viruso</ref>:
      <ekz>
        la&ubreve; oficialaj informoj de Monda Organiza&jcirc;o pri
        Sano, nuntempe en la mondo vivaj <tld/>oj ka&ubreve;zas &ccirc;irka&ubreve; 15&#x202F;%
        de &ccirc;iuj malignaj tumoroj
        <fnt>
          <aut>Vladimir Lemelev</aut>,
          <vrk>Mikroboj provokas neinfektajn malsanojn
          &scirc;an&gcirc;i&gcirc;i al
          infektaj</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2002a, numero 1a, p. 16-17a</lok>
        </fnt>;
      </ekz>
      <ekz>
        sciencistoj en multaj landoj intense penegis trovi la <tld/>on, kiu ka&ubreve;zas tiun
        anta&ubreve;e nekonatan infektan malsanon
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2005/008721.php">De kie venis kaj kial &ccirc;esis netipa pne&ubreve;monio?</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li estas nura <tld/>o, sensignifa bacilo, pri kies vivo kaj morto neniu plu
        interesi&gcirc;as
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
          <lok>1. Kakume.</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="mikrorganism.0o">mikroorganismo</ref>
    <trd lng="be">&c_m;&c_ib;&c_k;&c_r;&c_o;&c_b;</trd>
    <trd lng="ca">microbi</trd>
    <trd lng="de">Mikrobe</trd>
    <trdgrp lng="en">
    <trd>microbe</trd>
    <trd>microorganism</trd>
    <trd>germ (cause of disease)</trd>
    </trdgrp>
    <trd lng="es">microbio</trd>
    <trd lng="fa">&a_m;&f_y;&f_k;&a_r;&a_w;&a_b;</trd>
    <trdgrp lng="fr">
      <trd>microbe</trd>,
      <trd>micro-organisme</trd>
    </trdgrp>
    <trd lng="hu">mikr&oacute;ba</trd>
    <trd lng="id">mikroba</trd>
    <trd lng="nl">microbe</trd>
    <trd lng="pl">mikrob</trd>
    <trd lng="ru">&c_m;&c_i;&c_k;&c_r;&c_o;&c_b;</trd>
    <trd lng="zh">&#x5FAE;&#x751F;&#x7269; <pr>w&#x113;ish&#x113;ngw&#xF9;</pr></trd>
</snc>
  <trd lng="cs">mikrob</trd>
  <trd lng="ja">&#x5FAE;&#x751F;&#x7269;  <pr>&#x3073;&#x305B;&#x3044;&#x3076;&#x3064;</pr></trd>
  <trd lng="sk">mikr&oacute;b</trd>
  <trd lng="uk">&c_m;&c_ib;&c_k;&c_r;&c_o;&c_b;</trd>
</drv>
</art>
