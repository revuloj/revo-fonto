<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: eon.xml,v 1.9 2024/03/25 13:25:15 revo Exp $">
<kap>
  <rad>eon</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="eon.0o">
  <kap><tld/>o</kap>
  <snc mrk="eon.0o.GEOL">
   <uzo tip="fak">GEOL</uzo>
    <dif>
      Plej granda geokronologia tempunuo,
      entenanta plurajn
      <ref tip="prt" cel="era.0o.GEOL">eraojn</ref>:
      <ekz>
        la geologiaj <tld/>oj estas de pratempoj &gcirc;is nun:
        hadeano, anta&ubreve;kambrio kaj fanerozoiko.
      </ekz>
    </dif>
    <trd lng="bg">&c_je;&c_o;&c_n;</trd>
    <trd lng="de">&Auml;on</trd>
    <trd lng="en">eon</trd>
    <trd lng="fr">&eacute;on</trd>
    <trd lng="pl">eon</trd>
    <trd lng="pt">&eacute;on</trd>
  </snc>
 <trd lng="be">&c_e;&c_o;&c_n; <klr>(&c_g;&c_je;&c_a;&c_l;.)</klr></trd>
  <trd lng="cs">eon</trd>
  <trd lng="sk">eon</trd>
  <trd lng="uk">&c_je;&#x43E;&#x43D;</trd>
</drv>
</art>

