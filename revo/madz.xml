<art mrk="$Id: madz.xml,v 1.4 2024/01/15 21:20:53 revo Exp $">
<kap>
  <rad>madz</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="madz.0o">
  <kap><tld/>o</kap>
  <snc mrk="madz.0o.ARM">
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref cel="klab.0o.ARM">Klabo<sncref/></ref>
      fasonita el metalo, kun peza kapo:
      <ekz>
        unu el la sturmantoj frapis lin per <tld/>o
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Monato;/2003/007991.html"
              >Espero</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Eva</nom> kaptas <tld/>on de sur la tero kaj suprenkuras la &scirc;tuparon
        <fnt>
          <aut>G. G&aacute;rdonyi, trad. J. Horvath</aut>
          <vrk><url ref="http://vortaro.hu/stelspec.pdf">
          Steloj de Eger</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/6/6c/Various_Indo-Persian_maces.jpg/378px-Various_Indo-Persian_maces.jpg"
      prm="CC BY-SA 3.0"
      >diversaj hindaj persaj <tld/>oj
        <fnt>
          <aut>Samuraiantiqueworld, propra foto</aut>
          <lok><url ref="&WCW;/File:Various_Indo-Persian_maces.jpg"
              >https://commons.wikimedia.org/wiki/File:Various_Indo-Persian_maces.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_l;&c_a;&c_v;&c_a; <klr>(&c_z; &c_m;&c_e;&c_t;&c_a;&c_l;&c_u;)</klr></trd>
  <trdgrp lng="de">
    <trd>Streitkolben</trd>,
    <trd>Schl&auml;gel</trd>
  </trdgrp>
  <trd lng="fr">masse d'armes</trd>
  <trd lng="it">mazza</trd>
  <trd lng="uk">&c_b;&c_u;&c_l;&c_a;&c_v;&c_a;</trd>
</drv>
</art>

