<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc> <rad>akt</rad>/o
</kap>
<drv mrk="akt.0o">
  <kap><tld/>o</kap>
  <snc mrk="akt.0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Dramparto enhavanta kelkajn scenojn.
    </dif>
    <trd lng="hu">felvon&aacute;s</trd>
    <trd lng="nl">bedrijf</trd>
    <trd lng="pt">ato</trd>
    <trd lng="sk">dejstvo</trd>
  </snc>
  <snc mrk="akt.0o.ago">
    <dif>
      Oficiala grave faro a&ubreve; decido.
    </dif>
    <ref cel="ag.0o.montro">ago.</ref>
    <trd lng="hu">aktus</trd>
    <trd lng="nl">daad</trd>
    <trd lng="pt">ato</trd>
  </snc>
  <snc mrk="akt.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Oficiala dokumento atestanta ion:
      <ekz>
        testamento estas le&gcirc;a <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="protok.0o.raporto">protokolo</ref>
    </refgrp>
  <trdgrp lng="hu">
    <trd>akta</trd>,
    <trd>okm&aacute;ny</trd>,
    <trd>irat</trd>
  </trdgrp>
    <trd lng="nl">akte</trd>
  <trdgrp lng="pt">
    <trd>auto</trd>,
    <trd>instrumento</trd>,
    <trd>escritura</trd>
  </trdgrp>
  </snc>
  <trd lng="en">act</trd>
  <trd lng="fr">acte</trd>
  <trd lng="ru">&c_a;&c_k;&c_t;</trd>
  <trd lng="sk">akt</trd>
</drv>

<drv mrk="akt.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Oficisto, kiu konservas a&ubreve; skribas la oficialajn <tld/>ojn.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>clerk</trd>,
    <trd>recorder</trd>
  </trdgrp>
  <trd lng="fr">notaire</trd>
  <trdgrp lng="hu">
    <trd>t&ouml;rv&eacute;nysz&eacute;ki <ind>&iacute;rnok</ind></trd>,
    <trd>b&iacute;r&oacute;s&aacute;gi <ind>jegyz&odblac;</ind></trd>
  </trdgrp>
  <trd lng="nl">actuaris</trd>
  <trdgrp lng="pt">
    <trd>escriv&atilde;o</trd>,
    <trd>not&aacute;rio</trd>
  </trdgrp>
</drv>

<drv mrk="akt.inter0o">
  <kap>inter<tld/>o</kap>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <dif>
      Tempo inter du <tld/>oj, &ccirc;e la teatro.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>intermission</trd>,
    <trd>entr'acte</trd>,
    <trd>interlude</trd>,
    <trd>intermezzo</trd>
  </trdgrp>
  <trd lng="fr">entracte</trd>
  <trdgrp lng="hu">
    <trd>felvon&aacute;sk&ouml;z</trd>,
    <trd>sz&uuml;net<klr tip="ind"> (sz&iacute;nh&aacute;zban)</klr></trd>
  </trdgrp>
  <trd lng="nl">entr'acte</trd>
  <trd lng="pt">intervalo</trd>
  <trd lng="ru">&c_a;&c_n;&c_t;&c_r;&c_a;&c_k;&c_t;</trd>
</drv>
</art>
