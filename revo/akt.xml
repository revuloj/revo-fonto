<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc> <rad>akt</rad>/o
</kap>
<drv mrk="akt.0o">
  <kap><tld/>o</kap>
  <snc mrk="akt.0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Dramparto enhavanta kelkajn scenojn.
    </dif>
    <trd lng="nl">bedrijf</trd>
    <trd lng="pt">ato</trd>
  </snc>
  <snc mrk="akt.0o.ago">
    <dif>
      Oficiala grave faro a&ubreve; decido.
    </dif>
    <ref cel="ag.0o.montro">ago.</ref>
    <trd lng="nl">daad</trd>
  </snc>
  <snc mrk="akt.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Oficiala dokumento atestanta ion:
      <ekz>
        testamento estas le&gcirc;a <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="protok.0o.raporto">protokolo</ref>
    </refgrp>
    <trd lng="nl">akte</trd>
  </snc>
  <trd lng="fr">acte</trd>
  <trdgrp lng="pt">
    <trd>auto</trd>,
    <trd>instrumento</trd>,
    <trd>escritura</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_k;&c_t;</trd>
</drv>

<drv mrk="akt.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Oficisto, kiu konservas a&ubreve; skribas la oficialajn <tld/>ojn.
    </dif>
  </snc>
  <trd lng="fr">notaire</trd>
  <trd lng="nl">actuaris</trd>
  <trdgrp lng="pt">
    <trd>escriv&atilde;o</trd>,
    <trd>not&aacute;rio</trd>
  </trdgrp>
</drv>

<drv mrk="akt.inter0o">
  <kap>inter<tld/>o</kap>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <dif>
      Tempo inter du <tld/>oj, &ccirc;e la teatro.
    </dif>
  </snc>
  <trd lng="fr">entracte</trd>
  <trd lng="nl">entr'acte</trd>
  <trd lng="pt">intervalo</trd>
  <trd lng="ru">&c_a;&c_n;&c_t;&c_r;&c_a;&c_k;&c_t;</trd>
</drv>
</art>
