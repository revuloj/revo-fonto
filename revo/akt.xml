<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: akt.xml,v 1.28 2021/02/28 15:17:51 revo Exp $">
<kap>
   <ofc>1</ofc> <rad>akt</rad>/o
</kap>
<drv mrk="akt.0o">
   <kap><tld/>o</kap>
   <snc mrk="akt.0o.TEA">
     <uzo tip="fak">TEA</uzo>
     <dif>
       Parto de <ref tip="malprt" cel="teatr.0ajxo">teatra&jcirc;o</ref>
       enhavanta kelkajn scenojn.
     </dif>
     <trd lng="ca">acte</trd>
     <trd lng="de">Akt</trd>
     <trd lng="es">acto</trd>
     <trd lng="hu">felvon&aacute;s</trd>
     <trd lng="nl">bedrijf</trd>
     <trdgrp lng="pl">
       <trd>akt</trd>,
       <trd>ods&lstroke;ona</trd>
     </trdgrp>
     <trd lng="pt">ato</trd>
     <trd lng="sk">dejstvo</trd>
     <trdgrp lng="sl">
       <trd>dejanje</trd>,
       <trd>spis</trd>
       <trd>akt</trd>
     </trdgrp>
   </snc>
   <snc mrk="akt.0o.JUR">
     <uzo tip="fak">JUR</uzo>
     <dif>
       Oficiala dokumento atestanta ion:
       <ekz>
         testamento estas le&gcirc;a <tld/>o.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="protok.0o.raporto">protokolo</ref>
     </refgrp>
     <trd lng="ca">acta</trd>
     <trdgrp lng="de">
       <trd>Akte</trd>,
       <trd>Dokument</trd>
     </trdgrp>
     <trd lng="es">acta</trd>
     <trdgrp lng="hu">
       <trd>akta</trd>,
       <trd>okm&aacute;ny</trd>,
       <trd>irat</trd>
     </trdgrp>
     <trd lng="nl">akte</trd>
     <trdgrp lng="pl">
       <trd>akt <klr>prawny</klr></trd>,
       <trd>akt <klr>normatywny</klr></trd>,
       <trd>dokument</trd>
     </trdgrp>
     <trdgrp lng="pt">
       <trd>auto</trd>,
       <trd>instrumento</trd>,
       <trd>escritura</trd>
     </trdgrp>
   </snc>
   <snc mrk="akt.0o.ago">
     <dif>
       Oficiala grava faro a&ubreve; decido.
     </dif>
     <ref tip="vid" cel="ag.0o.montro">ago <sncref/></ref>
     <trdgrp lng="ca">
       <trd>acte</trd>,
       <trd>actuaci&oacute;</trd>
     </trdgrp>
     <trdgrp lng="de">
       <trd>offizieller Akt</trd>,
       <trd>offizieller Beschluss</trd>
     </trdgrp>
     <trd lng="hu">aktus</trd>
     <trd lng="nl">daad</trd>
     <trd lng="pl">czyn</trd>
     <trd lng="pt">ato</trd>
   </snc>
   <trd lng="be">&c_a;&c_k;&c_t;</trd>
   <trd lng="en">act</trd>
   <trd lng="fr">acte</trd>
   <trd lng="he">&#x5E4;&#x5E2;&#x5D5;&#x5DC;&#x5D4;</trd>
  <trd lng="ru">&c_a;&c_k;&c_t;</trd>
   <trd lng="sk">akt</trd>
</drv>

<drv mrk="akt.0isto">
   <kap><tld/>isto</kap>
   <snc mrk="akt.0isto.POL">
     <uzo tip="fak">POL</uzo>
     <dif>
       Oficisto, kiu konservas a&ubreve; skribas la oficialajn <tld/>ojn.
     </dif>
   </snc>
   <ref tip="vid" cel="notari.0o">notario</ref>
   <trdgrp lng="ca">
     <trd>notari</trd>,
     <trd>protonotari</trd>
   </trdgrp>
   <trd lng="de">Aktenverwalter</trd>
   <trdgrp lng="en">
     <trd>clerk</trd>,
     <trd>recorder</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>clerc de notaire</trd>,
     <trd>greffier</trd>
   </trdgrp>
   <trd lng="he">&#x5E8;&#x5E9;&#x5DD; &#x5DE;&#x5E1;&#x5DE;&#x5DB;&#x5D9;&#x5DD;</trd>
  <trdgrp lng="hu">
     <trd>t&ouml;rv&eacute;nysz&eacute;ki <ind>&iacute;rnok</ind></trd>,
     <trd>b&iacute;r&oacute;s&aacute;gi <ind>jegyz&odblac;</ind></trd>
   </trdgrp>
   <trd lng="nl">actuaris</trd>
   <trdgrp lng="pl">
     <trd>aktuariusz</trd>,
     <trd>kancelista</trd>,
     <trd>registrator</trd>
   </trdgrp>
   <trdgrp lng="pt">
     <trd>escriv&atilde;o</trd>,
     <trd>not&aacute;rio</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>kancel&aacute;rsky &uacute;radn&iacute;k</trd>,
    <trd>konceptn&yacute; &uacute;radn&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>aktu&aacute;r</trd>,
    <trd>archiv&aacute;&rcaron; &uacute;&rcaron;edn&iacute;ch spis&uring;</trd>
  </trdgrp>
</drv>

<drv mrk="akt.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="akt.0ujo.OFI">
    <dif>
       Plata, rigida entenujo por <tld/>oj:
       <ekz>
         pastro Franko eltiris el leda teko,
         kiel unuan, <tld/>on plenplenan je
         paperoj sur kiu  tekstis:
         <ctl>Projektoj</ctl>
         <fnt>
          <aut>I. Silone, trad. D. Mistretta</aut>
          <vrk><url ref="http://ekladata.com/XiKWRW1ETT4zBOHrQZm16PxBQmc/silone-la-sekreto-de-luka.pdf">La
          sekreto de Luka</url></vrk>
          <lok>[vidita en 2018]</lok>
         </fnt>.
       </ekz>
    </dif>
  </snc>
  <trd lng="fr">dossier</trd>
  <trd lng="he">&#x5EA;&#x5D9;&#x5E7; &#x5DE;&#x5E1;&#x5DE;&#x5DB;&#x5D9;&#x5DD;</trd>
</drv>

<drv mrk="akt.inter0o">
   <kap>inter<tld/>o</kap>
   <snc mrk="akt.inter0o.TEA">
     <uzo tip="fak">TEA</uzo>
     <dif>
       Pa&ubreve;zo inter du <tld/>oj de <ref tip="malprt" cel="teatr.0ajxo">teatra&jcirc;o</ref>.
     </dif>
   </snc>
  <trd lng="be">&c_a;&c_n;&c_t;&c_r;&c_a;&c_k;&c_t;</trd>
  <trdgrp lng="ca">
     <trd>entreacte</trd>,
     <trd>interludi</trd>,
     <trd>intermezzo</trd>
   </trdgrp>
  <trd lng="cs">p&rcaron;est&aacute;vka mezi dv&ecaron;ma jedn&aacute;n&iacute;mi</trd>
  <trdgrp lng="en">
     <trd>intermission</trd>,
     <trd>entr'acte</trd>,
     <trd>interlude</trd>,
     <trd>intermezzo</trd>
   </trdgrp>
  <trd lng="es">entreacto</trd>
  <trd lng="fr">entracte</trd>
  <trdgrp lng="hu">
     <trd>felvon&aacute;sk&ouml;z</trd>,
     <trd>sz&uuml;net<klr tip="ind"> (sz&iacute;nh&aacute;zban)</klr></trd>
   </trdgrp>
  <trd lng="nl">entr'acte</trd>
  <trd lng="pl">antrakt</trd>
  <trd lng="pt">intervalo</trd>
  <trd lng="ru">&c_a;&c_n;&c_t;&c_r;&c_a;&c_k;&c_t;</trd>
  <trd lng="sk">prest&aacute;vka medzi dvoma jednaniami</trd>
</drv>
</art>
