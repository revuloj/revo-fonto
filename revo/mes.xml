<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>mes</rad>/o
    </kap>
    <drv mrk="mes.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <uzo tip="fak">KAT</uzo>
      <snc>
        <dif>
          <ref tip="super" cel="liturg.0o.KRI">Liturgio</ref>
          en Roma katolikismo kaj kelkaj aliaj okcident-kristanaj
          konfesioj.
        </dif>
      </snc>
     <trdgrp lng="de">
       <trd>Messe</trd>,
       <trd>Gottesdienst</trd></trdgrp>
      <trdgrp lng="ru">
        <trd>&c_m;&c_je;&c_s;&c_s;&c_a;</trd>,
        <trd>&c_o;&c_b;&c_je;&c_d;&c_n;&c_ja;</trd></trdgrp>
    </drv>
  </art>
