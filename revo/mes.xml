<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mes.xml,v 1.25 2024/01/25 20:10:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>mes</rad>/o
</kap>
<drv mrk="mes.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="liturg.0o.KRI">Liturgio</ref>
      en Roma katolikismo kaj kelkaj aliaj okcident-kristanaj
      konfesioj:
      <ekz>tradicie &ccirc;i tie katolikaj <tld/>oj okazadis en la
germana lingvo &gcirc;is la jaro 1940<fnt><aut>Last</aut>:
<vrk>Pre&gcirc;eja jubileo</vrk>, <bib>Monato</bib><lok>jaro 2002a,
numero 2a, p. 17a</lok></fnt>.</ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_m;&c_sh;&c_a;</trd>,
    <trd>&c_b;&c_a;&c_g;&c_a;&c_s;&c_l;&c_u;&c_zh;&c_b;&c_a;</trd>
  </trdgrp>
  <trd lng="br">oferenn</trd>
  <trdgrp lng="ca">
    <trd>missa</trd>,
    <trd>ofici</trd>
  </trdgrp>
  <trd lng="cs">okru&zcaron;&iacute;  <klr>(kone&ccaron;n&iacute;ku| vejcovodu)</klr></trd>
  <trdgrp lng="de">
    <trd>Messe</trd>,
    <trd>Gottesdienst</trd>
  </trdgrp>
  <trd lng="en">mass<klr tip="ind"> (religious)</klr></trd>
  <trd lng="es">misa</trd>
  <trdgrp lng="fr">
    <trd>messe</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>mise</trd>,
    <trd>istentisztelet</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>messa<klr> (celebrazione eucaristica)</klr></trd>,
    <trd><ind>celebrazione</ind> eucaristica</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30DF;&#x30B5;</trd>,
    <trd>&#x8056;&#x796D;  <pr>&#x305B;&#x3044;&#x3055;&#x3044;</pr></trd>,
    <trd>&#x30DF;&#x30B5;&#x66F2;  <pr>&#x30DF;&#x30B5;&#x304D;&#x3087;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">mis<klr tip="ind"> (kath.)</klr></trd>
  <trd lng="pl">msza</trd>
  <trd lng="pt">missa</trd>
  <trdgrp lng="ro">
    <trd>liturghie</trd>
    <trd>slujb&abreve;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_s;&c_s;&c_a;</trd>,
    <trd>&c_o;&c_b;&c_je;&c_d;&c_n;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>okru&zcaron;ie</trd>,
    <trd>om&scaron;a</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x435;&#x441;&#x430;</trd>,
    <trd>&#x421;&#x43B;&#x443;&#x436;&#x431;&#x430; &#x411;&#x43E;&#x436;&#x430;</trd>,
    <trd>&#x43B;&#x456;&#x442;&#x443;&#x440;&#x433;&#x456;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x793C;&#x62DC; <pr>l&#x1D0;b&#xE0;i</pr></trd>,
    <trd>&#x79AE;&#x62DC; <pr>l&#x1D0;b&#xE0;i</pr></trd>
  </trdgrp>
</drv>
</art>
