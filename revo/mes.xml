<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mes</rad>/o
</kap>
<drv mrk="mes.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="liturg.0o.KRI">Liturgio</ref>
      en Roma katolikismo kaj kelkaj aliaj okcident-kristanaj
      konfesioj:
      <ekz>tradicie &ccirc;i tie katolikaj <tld/>oj okazadis en la
germana lingvo &gcirc;is la jaro 1940<fnt><aut>Last</aut>:
<vrk>Pre&gcirc;eja jubileo</vrk>, <bib>Monato</bib><lok>jaro 2002a,
numero 2a, p. 17a</lok></fnt>.</ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_m;&c_sh;&c_a;</trd>,
    <trd>&c_b;&c_a;&c_g;&c_a;&c_s;&c_l;&c_u;&c_zh;&c_b;&c_a;</trd>
  </trdgrp>
  <trd lng="br">oferenn</trd>
  <trdgrp lng="de">
    <trd>Messe</trd>,
    <trd>Gottesdienst</trd>
  </trdgrp>
  <trd lng="en">mass<klr tip="ind"> (religious)</klr></trd>
  <trdgrp lng="fr">
    <trd>messe</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>mise</trd>,
    <trd>istentisztelet</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>messa<klr> (celebrazione eucaristica)</klr></trd>,
    <trd><ind>celebrazione</ind> eucaristica</trd>
  </trdgrp>
  <trd lng="nl">mis<klr tip="ind"> (kath.)</klr></trd>
  <trd lng="pt">missa</trd>
  <trd lng="es">misa</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_s;&c_s;&c_a;</trd>,
    <trd>&c_o;&c_b;&c_je;&c_d;&c_n;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">om&scaron;a</trd>
</drv>
</art>
