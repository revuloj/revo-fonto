<art mrk="$Id: sxalup.xml,v 1.23 2023/11/29 18:31:30 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>&scirc;alup</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sxalup.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxalup.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Granda boato, boatego:
      <ekz>
        balda&ubreve; en urbaj vendejoj aperis liaj ludiloj &mdash; arte
        faritaj  etaj modeloj de boatoj, <tld/>oj, unuferdekaj kaj
        duferdekaj vel&scirc;ipoj
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/skarlataj.html">
          Skarlataj veloj</url></vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="lst" cel="vetur.0ilo" lst="voko:veturiloj">veturilo</ref>
  </snc>
  <trd lng="be">&c_sh;&c_l;&c_ju;&c_p;</trd>
  <trdgrp lng="ca">
    <trd>xalupa</trd>,
    <trd>llanxa</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>longboat</trd>,
    <trd>launch<klr tip="amb"> (large utility boat)</klr></trd>,
    <trd>sloop</trd>
  </trdgrp>
  <trd lng="de">Schaluppe</trd>
  <trdgrp lng="es">
    <trd>chalupa</trd>,
    <trd>lancha<klr> (auxiliar)</klr></trd>
  </trdgrp>
  <trd lng="fr">chaloupe</trd>
  <trdgrp lng="hu">
    <trd>dereglye</trd>,
    <trd>b&aacute;rka</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>scialuppa</trd>,
    <trd>lancia<klr> (barca)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30E9;&#x30F3;&#x30C1;  <pr>&#12425;&#12435;&#12385;</pr></trd>,
    <trd>&#22823;&#x578B;&#x30DC;&#x30FC;&#x30C8; 
    <pr>&#x304A;&#x304A;&#x304C;&#x305F;&#x307C;&#x30FC;&#12392;</pr></trd>
  </trdgrp>
  <trd lng="nl">sloep</trd>
  <trd lng="pl">szalupa</trd>
  <trd lng="pt">chalupa</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_l;&c_ju;&c_p;</trd>,
    <trd>&c_k;&c_a;&c_t;&c_je;&c_r;</trd>,
    <trd>&c_b;&c_a;&c_r;&c_k;&c_a;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_sh;&#x43B;&#x44E;&#x43F;</trd>,
    <trd>&#x43A;&c_a;&c_t;&c_je;&c_r;</trd>,
    <trd>&c_b;&c_a;&c_r;&#x43A;&c_a;&c_s;</trd>
  </trdgrp>
</drv>
</art>
