<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>&scirc;alup</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sxalup.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxalup.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Granda boato, boatego:
      <ekz>
        balda&ubreve; en urbaj vendejoj aperis liaj ludiloj &#8212; arte 
        faritaj  etaj modeloj de boatoj, <tld/>oj, unuferdekaj kaj duferdekaj 
        vel&scirc;ipoj
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/skarlataj.html">
          Skarlataj veloj</url></vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_l;&c_ju;&c_p;</trd>
  <trd lng="en">sloop</trd>
  <trd lng="de">Schaluppe</trd>
  <trd lng="fr">chaloupe</trd>
  <trdgrp lng="hu">
    <trd>dereglye</trd>,
    <trd>b&aacute;rka</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>scialuppa</trd>,
    <trd>lancia<klr> (barca)</klr></trd>
  </trdgrp>
  <trd lng="nl">sloep</trd>
  <trd lng="pt">chalupa</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_l;&c_ju;&c_p;</trd>,
    <trd>&c_k;&c_a;&c_t;&c_je;&c_r;</trd>,
    <trd>&c_b;&c_a;&c_r;&c_k;&c_a;&c_s;</trd>
  </trdgrp>
</drv>
</art>
