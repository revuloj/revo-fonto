<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>spad</rad>/o
</kap>
<drv mrk="spad.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="super" cel="glav.0o.MIL">Glavo</ref> konsistanta el longa,
      rekta, mallar&gcirc;a, pinta kaj unutran&ccirc;a a&ubreve; dutran&ccirc;a klingo kun tenilo.
    </dif>
    <refgrp tip="vid">
      <ref cel="skerm.0i">skermi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_p;&c_a;&c_g;&c_a;</trd>
  <trd lng="de">Degen</trd>
  <trd lng="fr">&eacute;p&eacute;e</trd>
  <trdgrp lng="hu">
    <trd>sp&aacute;d&eacute;</trd>,
    <trd>p&aacute;rbajt&odblac;r</trd>
  </trdgrp>
  <trd lng="nl">degen</trd>
  <trd lng="ru">&c_sh;&c_p;&c_a;&c_g;&c_a;</trd>
</drv>

<drv mrk="spad.0fisxo">
  <kap><tld/>fi&scirc;o</kap>
  <uzo tip="fak">ZOO</uzo>
  <uzo tip="stl">KOMUNE</uzo>
  <snc>
    <ref tip="dif" cel="ksifi.0o">ksifio</ref>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_ch;-&c_r;&c_y;&c_b;&c_a;</trd>
  <trd lng="de">Schwertfisch</trd>
  <trd lng="fr">espadon<klr> (poisson)</klr></trd>
  <trd lng="hu">kardhal</trd>  
  <trd lng="nl">zwaardvis</trd>
  <trd lng="ru">&c_m;&c_je;&c_ch;-&c_r;&c_y;&c_b;&c_a;</trd>
</drv>
</art>
