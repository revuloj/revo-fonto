<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>spad</rad>/o
</kap>
<drv mrk="spad.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Glavo konsistanta el longa, rekta, mallar&gcirc;a, pinta kaj unutran&ccirc;a a&ubreve;
      dutran&ccirc;a klingo kun tenilo.
    </dif>
    <refgrp tip="vid">
      <ref cel="skerm.0i">skermi</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Degen</trd>
  <trd lng="nl">degen</trd>
  <trd lng="ru">&c_sh;&c_p;&c_a;&c_g;&c_a;</trd>
</drv>

<drv mrk="spad.0fisxo">
  <kap><tld/>fi&scirc;o</kap>
  <snc>
    <ref tip="dif" cel="ksifi.0o">Ksifio</ref>
  </snc>
  <trd lng="de">Schwertfisch</trd>
  <trd lng="nl">zwaardvis</trd>
  <trd lng="ru">&c_m;&c_je;&c_ch;-&c_r;&c_y;&c_b;&c_a;</trd>
</drv>
</art>
