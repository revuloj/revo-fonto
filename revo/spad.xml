<art mrk="$Id: spad.xml,v 1.24 2024/03/25 07:28:34 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>spad</rad>/o
</kap>

<drv mrk="spad.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="lst" lst="voko:bataliloj" cel="glav.0o.MIL">Glavo</ref> konsistanta el
      longa, rekta, mallar&gcirc;a, pinta kaj unutran&ccirc;a a&ubreve; dutran&ccirc;a klingo kun
      tenilo:
      <ekz>
        la skermisto eltiris la <tld/>on kaj eksvingis &gcirc;in super la kapo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tri fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        kuru anta&ubreve;e en la &ccirc;ambron <klr>[&#x2026;]</klr> kaj alportu el tie la <tld/>on kaj la
        novan &ccirc;apelon
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto unua</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="skerm.0i">skermi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_p;&c_a;&c_g;&c_a;</trd>
  <trd lng="bg">&c_sh;&c_p;&c_a;&c_g;&c_a;</trd>
  <trd lng="cs">kord</trd>
  <trd lng="de">Degen</trd>
  <trdgrp lng="en">
    <trd>rapier</trd>,
    <trd>dress sword</trd>,
    <trd>dueling sword</trd>,
    <trd>court sword</trd>,
    <trd>small sword</trd>,
    <trd>&eacute;p&eacute;e</trd>,
    <trd>epee</trd>
  </trdgrp>
  <trd lng="fr">&eacute;p&eacute;e</trd>
  <trdgrp lng="hu">
    <trd>sp&aacute;d&eacute;</trd>,
    <trd>p&aacute;rbajt&odblac;r</trd>
  </trdgrp>
  <trd lng="ja">&#x5263;  <pr>&#x3051;&#x3093;</pr></trd>
  <trd lng="nl">degen</trd>
  <trd lng="pl">szpada</trd>
  <trd lng="ru">&c_sh;&c_p;&c_a;&c_g;&c_a;</trd>
  <trd lng="sk">kord <klr>(zbra&ncaron;)</klr></trd>
  <trd lng="uk">&c_sh;&#x43F;&c_a;&c_g;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x92CF; <pr>ji&#xE1;</pr></trd>,
    <trd>&#x94D7; <pr>ji&#xE1;</pr></trd>,
    <trd>&#x5251; <pr>ji&#xE0;n</pr></trd>,
    <trd>&#x5292; <pr>ji&#xE0;n</pr></trd>,
    <trd>&#x8ED2;&#x8F45;&#x528D; <pr>xu&#x101;nyu&#xE1;nji&#xE0;n</pr></trd>,
    <trd>&#x8F69;&#x8F95;&#x5251; <pr>xu&#x101;nyu&#xE1;nji&#xE0;n</pr></trd>,
    <trd>&#x91CD;&#x5251; <pr>zh&#xF2;ngji&#xE0;n</pr></trd>,
    <trd>&#x91CD;&#x528D; <pr>zh&#xF2;ngji&#xE0;n</pr></trd>,
    <trd>&#x5F2F;&#x5200; <pr>w&#x101;nd&#x101;o</pr></trd>,
    <trd>&#x5F4E;&#x5200; <pr>w&#x101;nd&#x101;o</pr></trd>,
    <trd>&#x9577;&#x67C4;&#x6B66;&#x5668; <pr>ch&#xE1;ngb&#x1D0;ngw&#x1D4;q&#xEC;</pr></trd>,
    <trd>&#x957F;&#x67C4;&#x6B66;&#x5668; <pr>ch&#xE1;ngb&#x1D0;ngw&#x1D4;q&#xEC;</pr></trd>
  </trdgrp>
</drv>

</art>
