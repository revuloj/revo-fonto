<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: spad.xml,v 1.23 2023/10/21 07:46:47 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>spad</rad>/o
</kap>

<drv mrk="spad.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="lst" lst="voko:bataliloj" cel="glav.0o.MIL">Glavo</ref> konsistanta el
      longa, rekta, mallar&gcirc;a, pinta kaj unutran&ccirc;a a&ubreve; dutran&ccirc;a klingo kun
      tenilo:
      <ekz>
        la skermisto eltiris la <tld/>on kaj eksvingis &gcirc;in super la kapo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tri fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        kuru anta&ubreve;e en la &ccirc;ambron <klr>[&#8230;]</klr> kaj alportu el tie la <tld/>on kaj la
        novan &ccirc;apelon
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto unua</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="skerm.0i">skermi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_p;&c_a;&c_g;&c_a;</trd>
  <trd lng="bg">&c_sh;&c_p;&c_a;&c_g;&c_a;</trd>
  <trd lng="cs">kord</trd>
  <trd lng="de">Degen</trd>
  <trdgrp lng="en">
    <trd>rapier</trd>,
    <trd>dress sword</trd>,
    <trd>dueling sword</trd>,
    <trd>court sword</trd>,
    <trd>small sword</trd>,
    <trd>&eacute;p&eacute;e</trd>,
    <trd>epee</trd>
  </trdgrp>
  <trd lng="fr">&eacute;p&eacute;e</trd>
  <trdgrp lng="hu">
    <trd>sp&aacute;d&eacute;</trd>,
    <trd>p&aacute;rbajt&odblac;r</trd>
  </trdgrp>
  <trd lng="ja">&#21091;  <pr>&#12369;&#12435;</pr></trd>
  <trd lng="nl">degen</trd>
  <trd lng="pl">szpada</trd>
  <trd lng="ru">&c_sh;&c_p;&c_a;&c_g;&c_a;</trd>
  <trd lng="sk">kord <klr>(zbra&ncaron;)</klr></trd>
  <trd lng="uk">&c_sh;&#x43F;&c_a;&c_g;&c_a;</trd>
</drv>

</art>
