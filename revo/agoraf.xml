<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>agorafobi</rad>/o
</kap>
<drv mrk="agoraf.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Mensa malsano karakterizata per timo al malplena spaco.
    </dif>
    <refgrp tip="vid">
      <ref cel="klauxs.0ofobio">kla&ubreve;strofobio</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Platzangst</trd>,
     <trd>Agoraphobie<klr> [fak]</klr></trd>,
     <trd>Angst vor gro&szlig;en Pl&auml;tzen</trd>
  </trdgrp>
  <trd lng="en">agoraphobia</trd>
  <trd lng="es">agorafobia</trd>
  <trd lng="fr">agoraphobie</trd>
  <trd lng="hu">t&eacute;riszony</trd>
  <trdgrp lng="nl">
    <trd>pleinvrees</trd>,
    <trd>agorafobie</trd>
  </trdgrp>
  <trd lng="pt">agorafobia</trd>
  <trd lng="ru">&c_a;&c_g;&c_o;&c_r;&c_a;&c_f;&c_o;&c_b;&c_i;&c_ja;</trd>
    <trd lng="sk">agoraf&oacute;bia</trd>
</drv>
</art>
