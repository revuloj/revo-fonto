<drv mrk="ordon.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="ordon.0ema.kutimanta">
    <dif>
      Kutimanta, &scirc;atanta <tld/>i:
      <ekz>
        <tld/>ema kaporalo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ven&gcirc;ema, regulema, punema, <tld/>ema dio de la malnova testamento
        <fnt>
          <bib>Monato</bib>
          <aut>Petro Desmet'</aut>
          <vrk><url ref="&Monato;/2006/008993.php">La unua Zamenhof-monumento de la 21a
          jarcento: dika Pomego!</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>emaj ideoj rapide disvasti&gcirc;as sub la registaro
        <fnt>
          <bib>Monato</bib>
          <aut>Jo&atilde;o Jos&eacute; Santos</aut>
          <vrk><url ref="&Monato;/2007/009360.php">La sa&gcirc;eco de S&oacute;crates</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
