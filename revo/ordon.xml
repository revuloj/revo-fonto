    <trd>imperativ</trd>
  </trdgrp>
  <trdgrp lng="ru"> 
    <trd>&c_p;&c_o;&c_v;&c_je;&c_l;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_i;&c_m;&c_p;&c_je;&c_r;&c_a;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">obsahuj&uacute;ci zlat&uacute; rudu</trd>
  <trd lng="uk">&#x43D;&c_a;&#x43A;&c_a;&c_z;&#x43E;&c_v;&c_i;&c_j;</trd>
