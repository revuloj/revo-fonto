<drv mrk="ordon.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ordon.0i.estre">
    <dif>
       Estre esprimi al iu sian volon, ke li faru ion:
      <ekz>
         al la popol' <tld/>u trankvili&gcirc;i
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz><ekz>
        mi insiste <tld/>as al vi ne &scirc;teli plu
        <fnt>
          <vrk><url ref="https://sites.google.com/site/deklecionoj/lektury">Dek lecionoj</url></vrk>
          <lok>Konfeso</lok>
        </fnt>;
      </ekz><ekz>
         <tld/>u nur la malbenitan koboldon reveni en la 
         sakon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tablo , la orazeno kaj bastono el sako</lok>
        </fnt>;
      </ekz><ekz>
         li <klr>[&#x2026;]</klr> <tld/>is siajn akompanantojn atendi ekstere
        <fnt>
          <bib>ElektFab</bib>
          <lok>La kura&gcirc;a tajloreto</lok>
        </fnt>;
      </ekz><ekz><tld/>u al li, ke li ne babilu
         <fnt><bib>F</bib>, 
         <lok>&FE; 20</lok></fnt>;
      </ekz><ekz>
         li <tld/>is al mi ripozon;
      </ekz>
      <ekz>
        kie mi disponas, tie mi <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>kiu man&gcirc;on al mi donas, tiu al mi <tld/>as</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
        <trd lng="de">Wes' Brot ich ess', des' Lied ich sing</trd>
      </ekz>
      <ekz>
        ekzemplo proponas, sed ne <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz><ekz>
         <tld/>ita kiso havas guston maldol&ccirc;an
         <fnt><bib>PrV</bib></fnt>;
      </ekz><ekz>
         kiam forto <tld/>as, le&gcirc;o pardonas
         <klr>(al potenculoj &ccirc;io estas permesata)</klr>
         <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        al la bu&scirc;o de oni neniu povas <tld/>i
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="reg.0i">regi</ref>,
       <ref cel="dikt.0i">dikti</ref>,
       <ref cel="dekret.0i">dekreti</ref>,
       <ref cel="instru1">instrukcii</ref>,
       <ref cel="komand.0i">komandi</ref>,
       <ref cel="obe.0igi">obeigi</ref>.
     </refgrp>
   </snc>
  <trd lng="be">&c_z;&c_a;&c_g;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bo">&#xF56;&#xF40;&#xF7C;&#xF51;&#xF0B;&#xF54;&#xF0B;&#xF42;&#xF4F;&#xF7C;&#xF44;&#xF0B;</trd>
