 <drv mrk="ordon.0o">
 <kap><tld/>o</kap>
 <snc mrk="ordon.0o.volo">
   <dif>
     <ref tip="lst" lst="voko:komunikoj" cel="esprim.0o.ado">Esprimo</ref>
     de ies estra <ref cel="vol.0o">volo</ref>:
     <ekz>
       bezono estas plej forta <tld/>o
       <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        de la volo la <tld/>o pli efikas ol bastono
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz><ekz>
       deziro kaj inklino <tld/>on ne obeas
       <fnt><bib>PrV</bib></fnt>;
      </ekz><ekz>
       mi restas tie &ccirc;i la&ubreve; la <tld/>o de mia 
       estro
        <fnt>
          <bib>F</bib>
          <lok>&FE; 26</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tiu]</klr> respektanta <tld/>on estos rekompencita
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 13:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        observi kaj plenumi &ccirc;iujn <klr>[&#x2026;]</klr> <tld/>ojn
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 15:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne for&jcirc;etu la <tld/>on de via patrino
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 1:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> jam estis for por disdoni novajn <tld/>ojn
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Batalo je la Ebura Turo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
       <ref cel="edikt.0o">edikto</ref>,
       <ref cel="ukaz.0o">ukazo</ref>,
       <ref cel="direktiv.0o">direktivo</ref>,
       <ref cel="komand.0o">komando</ref>
    </refgrp>
    <trd lng="be">&c_z;&c_a;&c_g;&c_a;&c_d;</trd>    
    <trd lng="cs">rozkaz</trd>
    <trdgrp lng="de">
      <trd>Anordnung</trd>,
    </trdgrp> 
    <trdgrp lng="es">
      <trd>orden</trd>,
      <trd>mandato</trd>
    </trdgrp>     
    <trdgrp lng="fr">
      <trd>ordre</trd>,
      <trd>injonction</trd>
    </trdgrp>
    <trd lng="nl">bevel</trd>
    <trdgrp lng="pl">
      <trd>nakaz</trd>,
      <trd>polecenie</trd>,
      <trd>rozkaz</trd>,
      <trd>zarz&aogonek;dzenie</trd>,
      <trd>zlecenie</trd>
    </trdgrp>
    <trdgrp lng="pt">
     <trd>ordem</trd>,
     <trd>comando</trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>mandat</trd>,
      <trd>porunc&abreve;</trd>,
      <trd>ordin</trd>,
      <trd>comand&abreve;</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_r;&c_i;&c_k;&c_a;&c_z;</trd>
    <trd lng="sv">order</trd>
  </snc>
  <snc mrk="ordon.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      En <ref tip="malprt" cel="progra.0o.komp">programo</ref>,
      <ref tip="super" cel="fraz.0o.KOMP">frazo</ref> indikanta
      agon plenumendan:
      <ekz>
         simpla <tld/>o <klr>(baza,
         kiu ne entenas aliajn <tld/>ojn)</klr>;
      </ekz><ekz>
          malsimpla <tld/>o <klr>(konstruita el pli simplaj 
          <tld/>oj)</klr>;
      </ekz><ekz>
         kondi&ccirc;a, iteracia <tld/>o;
      </ekz>
      <ekz>
        estas du metodoj agordi la amplekson de indicoj por tabeloj deklaritaj per la
        <tld/>o <frm>Dim</frm>
        <fnt>
          <vrk><url ref="https://helponline.libreoffice.org/5.4/eo/text/sbasic/shared/03102101.html">Ordono
          ReDim [Rultempa] - helponline.libreoffice.org</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la komputilo simple plenumas tiujn <tld/>ojn en la donita sinsekvo
        <fnt>
          <vrk><url ref="&Viki;/Programlingvo">Programlingvo -
          Vikipedio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la parto de la proces-unuo, kie &ccirc;iu <tld/>o el la programo estas &leftquot;legata&rightquot;,
        interpretata kaj direktata
        <fnt>
          <vrk><url ref="http://www.ilei.info/dok/INVERSA%20VORTARO%20HILAND.pdf">Inversa
          Vortaro</url></vrk>
          <lok>ilei.info</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="deklar.0o.KOMP">deklaro</ref>
    <ref tip="sub" cel="komand.0o.KOMP">komando</ref>
    <trd lng="be">&c_a;&c_p;&c_e;&c_r;&c_a;&c_t;&c_a;&c_r; <klr>(&c_u; &c_k;&c_a;&c_m;&c_p;&c_u;&c_t;&c_a;&c_r;&c_n;&c_a;&c_j; &c_p;&c_r;&c_a;&c_g;&c_r;&c_a;&c_m;&c_je;)</klr></trd>
    <trd lng="de">Instruktion</trd>
    <trd lng="en"><klr>(executable)</klr>
     <ind>statement</ind></trd>
    <trdgrp lng="fr"><trd>commande</trd><trd>instruction</trd></trdgrp>
    <trd lng="nl">instructie</trd>
    <trdgrp lng="es">
     <trd>comando</trd>,
     <trd>instrucci&oacute;n</trd>
    </trdgrp>
    <trdgrp lng="pt">
     <trd>comando</trd>,
     <trd>instru&ccedil;&atilde;o</trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_p;&c_je;&c_r;&c_a;&c_t;&c_o;&c_r;</trd>
    <trd lng="zh">&#x547D;&#x4EE4; <pr>m&#xEC;ngl&#xEC;ng</pr></trd>
</snc>
  <trd lng="bo">&#xF56;&#xF40;&#xF60;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>na&rcaron;&iacute;zen&iacute;</trd>,
    <trd>p&rcaron;&iacute;kaz</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Befehl</trd>,
     <trd>Anweisung</trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>parancs</trd>,
     <trd>utas&iacute;t&aacute;s</trd>
   </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x547D;&#x4EE4;  <pr>&#x3081;&#x3044;&#x308C;&#x3044;</pr></trd>,
    <trd>&#x6307;&#x56F3;  <pr>&#x3055;&#x3057;&#x305A;</pr></trd>,
    <trd>&#x53F7;&#x4EE4;  <pr>&#x3054;&#x3046;&#x308C;&#x3044;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nariadenie</trd>,
    <trd>pr&iacute;kaz</trd>,
    <trd>rozkaz</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_a;&#x43A;&c_a;&c_z;</trd>,
    <trd>&c_v;&c_je;&#x43B;&c_ib;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_r;&#x43E;&c_z;&#x43F;&#x43E;&c_r;&#x44F;&c_d;&c_zh;&c_je;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x547D;&#x4EE4; <pr>m&#xEC;ngl&#xEC;ng</pr></trd>,
    <trd>&#x6307;&#x4EE4; <pr>zh&#x1D0;l&#xEC;ng</pr></trd>,
    <trd>&#x6307;&#x6325; <pr>zh&#x1D0;hu&#x12B;</pr></trd>,
    <trd>&#x6307;&#x63EE; <pr>zh&#x1D0;hu&#x12B;</pr></trd>,
    <trd>&#x6307;&#x6325;&#x6743; <pr>zh&#x1D0;hu&#x12B;qu&#xE1;n</pr></trd>,
    <trd>&#x6307;&#x63EE;&#x6B0A; <pr>zh&#x1D0;hu&#x12B;qu&#xE1;n</pr></trd>,
    <trd>&#x53F7;&#x4EE4; <pr>h&#xE0;ol&#xEC;ng</pr></trd>,
    <trd>&#x865F;&#x4EE4; <pr>h&#xE0;ol&#xEC;ng</pr></trd>,
    <trd>&#x8A13;&#x4EE4; <pr>x&#xF9;nl&#xEC;ng</pr></trd>,
    <trd>&#x8BAD;&#x4EE4; <pr>x&#xF9;nl&#xEC;ng</pr></trd>,
    <trd>&#x6307;&#x793A; <pr>zh&#x1D0;sh&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ordon.0a">
  <kap><tld/>a</kap>
  <snc mrk="ordon.0a.esprimanta">
    <dif>
      Esprimanta <tld/>on:
      <ekz>
        <tld/>a tono, rigardo, vorto;
      </ekz>
      <ekz>
        <klr>[li]</klr> ion diris al li kun <tld/>a gesto
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiuj vortoj frapus &scirc;in per ia subita, <tld/>a rememoro
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve; <tld/>a trumpetado, jen envenas grupo da ta&ubreve;roprenantoj
        <fnt>
          <bib>Monato</bib>
          <aut>Eduardo Novembro</aut>
          <vrk><url ref="&Monato;/2014/011318.php">Ta&ubreve;ro??? Festo!!!</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        la modo <tld/>a &leftquot;-u&rightquot;
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_g;&c_a;&c_d;&c_n;&c_y;</trd>
  <trd lng="cs">obsahuj&iacute;c&iacute; zlatou rudu</trd>
  <trd lng="de">befehlend</trd>
  <trdgrp lng="es">
   <trd>ordenado, -a</trd>,
   <trd>mandado, -a</trd>,
   <trd>mandatorio, -a</trd>
  </trdgrp>
  <trdgrp lng="fr">
   <trd>imp&eacute;ratif</trd>,
   <trd>de <ind>commandement</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
   <trd>parancsol&oacute;</trd>,
   <trd>utas&iacute;t&oacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x547D;&#x4EE4;&#x306E;  <pr>&#x3081;&#x3044;&#x308C;&#x3044;&#x306E;</pr></trd>,
    <trd>&#x547D;&#x4EE4;&#x7684;&#x306A;  <pr>&#x3081;&#x3044;&#x308C;&#x3044;&#x3066;&#x304D;&#x306A;</pr></trd>
  </trdgrp>
  <trd lng="nl">gebiedend</trd>
  <trdgrp lng="pl">
    <trd>nakazowy</trd>,
    <trd>rozkazuj&aogonek;cy</trd>
  </trdgrp>
  <trd lng="pt">imperativo <klr tip="amb">(adj.)</klr></trd>
