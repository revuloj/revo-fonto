<art mrk="$Id$">
<kap>
   <rad>terbi</rad>/o
</kap>
<drv mrk="terbi.0o">
   <kap><tld/>o</kap>
   <mlg kod="IUPAC">Tb</mlg>
   <uzo tip="fak">KEM</uzo>
   <snc>
     <dif>
       &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
       lst="voko:&hcirc;emiaj_elementoj" val="65">elemento</ref> kun
       atomnumero 65, atompezo 158,924; malofta, 
       mola, ar&gcirc;entgriza <ref tip="super" cel="metal.0o">metalo</ref>,
       lantanoido.
     </dif>
   </snc>
  <trd lng="be">&c_t;&c_e;&c_r;&c_b;&c_ib;&c_j;</trd>
  <trd lng="bg">&c_t;&c_je;&c_r;&c_b;&c_i;&c_j;</trd>
  <trd lng="br">terbiom</trd>
  <trd lng="ca">terbi</trd>
  <trd lng="de">Terbium</trd>
  <trd lng="en">terbium</trd>
  <trd lng="es">terbio</trd>
  <trd lng="fr">terbium</trd>
  <trd lng="hu">terbium</trd>
  <trd lng="id">terbium</trd>
  <trd lng="it">terbio</trd>
  <trd lng="la">Terbium</trd>
  <trd lng="nl">terbium</trd>
  <trd lng="pl">terb</trd>
  <trd lng="pt">t&eacute;rbio</trd>
  <trd lng="ru">&c_t;&c_je;&c_r;&c_b;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>Tb</trd>,
    <trd>terbium</trd>
  </trdgrp>
  <trd lng="sv">terbium</trd>
</drv>
</art>
