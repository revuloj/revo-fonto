<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>prefiks</rad>/o
    </kap>
    <drv mrk="prefik.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc>
        <dif>
          Afikso, kiu metate anta&ubreve; vorto modifas ties sencon,
          formante alian novan vorton; ekz. <ekz>pra, ge, mal.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="sufiks.0o">sufikso</ref>.
        </refgrp>
      </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_s;&c_t;&c_a;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_e;&c_f;&c_ib;&c_k;&c_s;</trd>
  </trdgrp>
      <trd lng="ca">prefix</trd>
      <trdgrp lng="de">
         <trd>Pr&auml;fix</trd>,
         <trd>Vorsilbe</trd>
      </trdgrp>
      <trd lng="en">prefix</trd>
      <trd lng="es">prefijo</trd>
      <trdgrp lng="hu">
        <trd>el&odblac;k&eacute;pz&odblac;</trd>,
        <trd>prefixum</trd>
      </trdgrp>
      <trdgrp lng="nl">
    <trd>voorvoegsel</trd>,
    <trd>prefix</trd>
  </trdgrp>
  <trd lng="pt">prefixo</trd>
<trd lng="ru">&c_p;&c_r;&c_i;&c_s;&c_t;&c_a;&c_v;&c_k;&c_a;</trd>
    </drv>
  </art>
