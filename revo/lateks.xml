<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: lateks.xml,v 1.1 2025/08/25 20:47:14 afido Exp $">
    <kap>
      <rad>lateks</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="lateks.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo><uzo tip="fak">KEM</uzo>
  <snc mrk="lateks.0o.laktosuko">
    <dif>
      <ref tip="dif" cel="suk.lakto0o">Laktosuko</ref>:
      <ekz>
        <klr>[&ccirc;e leontodo]</klr> la <tld/>o kiu eliras se oni fendas parton de la
        planto, estas amara
        <fnt>
          <aut>Filomena Canzano</aut>
          <vrk><url ref="https://apetito.ikso.net/artikolo/leontodo-taraksako"
              >Leontodo (taraksako)</url></vrk>
          <lok>Esperantista Vegetarano, n-ro 2, 2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelidonio <klr>[&#x2026;]</klr> enhavas akran oran&gcirc;flavan <tld/>on (laktosukon) kiu
        helpus forigi verukojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=22018"
              >Kelidonio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kauxcx.0o">ka&ubreve;&ccirc;uko</ref>,
      <ref cel="gum.0o.KEM">gumo<sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Latex</trd>,
      <trd>Milchsaft</trd>
    </trdgrp>
    <trd lng="en">latex</trd>
    <trd lng="fr">latex</trd>
  </snc>
</drv>

  </art>

