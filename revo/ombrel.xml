<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ombrel.xml,v 1.31 2022/03/29 14:19:36 revo Exp $">
<kap><ofc>*</ofc>
<rad>ombrel</rad>/o</kap>

<drv mrk="ombrel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="ombrel.0o.TEK">
    <dif>
      Kontra&ubreve;pluva a&ubreve; kontra&ubreve;suna portebla
      &scirc;irmilo, konsistanta el
      tenilo
      kaj &scirc;tofo a&ubreve; papero ronde stre&ccirc;ita sur
      vergetoj:
      <ekz>
        la homoj kuras kun <tld/>oj, tamen sen galo&scirc;oj, &ccirc;ar
        kiu do en
        Germanio uzas la galo&scirc;ojn
        <fnt><bib>Metrop</bib></fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_s;&c_o;&c_n;</trd>
  <trd lng="bg">&c_ch;&c_a;&c_d;&c_malmol;&c_r;</trd>
  <trd lng="bo">&#xF49;&#xF72;&#xF0B;&#xF42;&#xF51;&#xF74;&#xF42;&#xF66;&#xF0B;</trd>
  <trdgrp lng="br">
    <trd>disglavier</trd>,
    <trd>disheolier</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>paraigua</trd>,
    <trd>para-sol</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>chr&aacute;n&iacute;tko</trd>,
    <trd>de&scaron;tn&iacute;k</trd>,
    <trd>slune&ccaron;n&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schirm</trd>,
    <trd>Regenschirm</trd>,
    <trd>Sonnenschirm</trd>
  </trdgrp>
  <trd lng="el">&omikron;&my;&pi;&rho;&epsilon_ton;&lambda;&lambda;&alfa;</trd>
  <trdgrp lng="en">
    <trd>parasol</trd>,
    <trd>umbrella</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>paraguas</trd>,
    <trd>sombrilla</trd>,
    <trd>quitasol</trd>
  </trdgrp>
  <trd lng="fa">&f_ch;&a_t;&a_r;</trd>
  <trdgrp lng="fr">
    <trd>ombrelle</trd>,
    <trd>parapluie</trd>,
    <trd>parasol</trd>
  </trdgrp>
  <trd lng="hu">erny&odblac;</trd>
  <trd lng="id">payung</trd>
  <trdgrp lng="nl">
    <trd>scherm</trd>,
    <trd>regenscherm</trd>,
    <trd>zonnescherm</trd>,
    <trd>paraplu</trd>,
    <trd>parasol</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>parasol</trd>,
    <trd>parasolka</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>guarda-chuva</trd>,
    <trd>guarda-sol</trd>,
    <trd>sombrinha</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_o;&c_n;&c_t;</trd>,
    <trd>&c_z;&c_o;&c_n;&c_t;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">d&aacute;&zcaron;dnik</trd>
  <trdgrp lng="sv">
    <trd>paraply</trd>,
    <trd>parasoll</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x430;&#x440;&#x430;&#x441;&#x43E;&#x43B;&#x44C;&#x43A;&#x430;</trd>,
    <trd>&#x43F;&#x430;&#x440;&#x430;&#x441;&#x43E;&#x43B;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="ombrel.pluv0o">
  <kap>pluv<tld/>o</kap>
  <snc mrk="ombrel.pluv0o.TEK">
    <dif>
      Akvoimuna <ref tip="super" cel="ombrel.0o.TEK"><tld/>o</ref>
      ta&ubreve;ga
      por &scirc;irmi de pluvo:
      <ekz>
        homoj &scirc;irmis sin per pluv<tld/>oj kontra&ubreve; la pluvo,
        per
        manteloj
        kontra&ubreve; la malvarmo
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">disglavier</trd>
  <trd lng="ca">paraigua</trd>
  <trd lng="cs">de&scaron;tn&iacute;k</trd>
  <trd lng="de">Regenschirm</trd>
  <trd lng="en">umbrella</trd>
  <trd lng="fa">&f_ch;&a_t;&a_r;</trd>
  <trd lng="fr">parapluie</trd>
  <trd lng="hu">eserny&odblac;</trd>
  <trd lng="id">payung</trd>
  <trdgrp lng="nl">
    <trd>paraplu</trd>,
    <trd>regenscherm</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>parasol</trd>,
    <trd>parasolka</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_o;&c_n;&c_t;<klr tip="ind"> (&c_o;&c_t;
    &c_d;&c_o;&c_zh;&c_d;&c_ja;)</klr></trd>,
    <trd>&c_z;&c_o;&c_n;&c_t;&c_i;&c_k;<klr tip="amb"> (&c_o;&c_t;
    &c_d;&c_o;&c_zh;&c_d;&c_ja;)</klr></trd>
  </trdgrp>
  <trd lng="sk">d&aacute;&zcaron;dnik</trd>
  <trd lng="uk">&#x43F;&#x430;&#x440;&#x430;&#x441;&#x43E;&#x43B;&#x44C;&#x43A;&#x430; &#x432;&#x456;&#x434; &#x434;&#x43E;&#x449;&#x443;</trd>
</drv>

<drv mrk="ombrel.sun0o">
  <kap>sun<tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="ombrel.0o.TEK"><tld lit="O"/>o</ref>
      ta&ubreve;ga
      por
      &scirc;irmi de sunlumo:
      <ekz>
        la daktilpalmo etendis sian &scirc;irman foliaron tre alte,
        kvaza&ubreve;
        &gcirc;i
        volus ludi la rolon de sun<tld/>o
        <fnt><bib>Fab4</bib>, <lok>filino de la mar&ccirc;a
        re&gcirc;o</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">disheolier</trd>
  <trd lng="ca">para-sol</trd>
  <trd lng="cs">slune&ccaron;n&iacute;k</trd>
  <trd lng="de">Sonnenschirm</trd>
  <trd lng="en">parasol</trd>
  <trd lng="fa">&f_ch;&a_t;&a_r;
  &a_A_madda;&a_f;&a_t;&a_A;&a_b;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>ombrelle</trd>,
    <trd>parasol</trd>
  </trdgrp>
  <trd lng="hu">naperny&odblac;</trd>
  <trd lng="id">payung</trd>
  <trdgrp lng="nl">
    <trd>parasol</trd>,
    <trd>zonnescherm</trd>
  </trdgrp>
  <trd lng="pl">parasol przeciws&lstroke;oneczny</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_o;&c_n;&c_t; &c_o;&c_t;
    &c_s;&c_o;&c_l;&c_n;&c_c;&c_a;</trd>
    <trd>&c_z;&c_o;&c_n;&c_t;&c_i;&c_k; &c_o;&c_t;
    &c_s;&c_o;&c_l;&c_n;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">slne&ccaron;n&iacute;k</trd>
  <trd lng="uk">&#x43F;&#x430;&#x440;&#x430;&#x441;&#x43E;&#x43B;&#x44C;&#x43A;&#x430; &#x432;&#x456;&#x434; &#x441;&#x43E;&#x43D;&#x446;&#x44F;</trd>
</drv>
</art>
