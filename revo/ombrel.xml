<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ombrel</rad>/o
</kap>
<drv mrk="ombrel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Kontra&ubreve;pluva a&ubreve; kontra&ubreve;suna portebla &scirc;irmilo, konsistanta el tenilo
      kaj &scirc;tofo a&ubreve; papero ronde stre&ccirc;ita sur vergetoj:
      <ekz>
        pluv<tld/>o, sun<tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_s;&c_o;&c_n;</trd>
  <trdgrp lng="de">
     <trd>Schirm</trd>,
     <trd>Regenschirm</trd>,
     <trd>Sonnenschirm</trd>
  </trdgrp>
  <trd lng="el">&omikron;&my;&pi;&rho;&epsilon_ton;&lambda;&lambda;&alfa;</trd>
  <trd lng="hu">erny&odblac;</trd>
  <trdgrp lng="nl">
    <trd>scherm</trd>,
    <trd>regenscherm</trd>,
    <trd>zonnescherm</trd>,
    <trd>paraplu</trd>,
    <trd>parasol</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>guarda-chuva</trd>,
    <trd>guarda-sol</trd>,
    <trd>sombrinha</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>paraply</trd>,
    <trd>parasoll</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_o;&c_n;&c_t;</trd>,
    <trd>&c_z;&c_o;&c_n;&c_t;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
