<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ombrel</rad>/o
</kap>
<drv mrk="ombrel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Kontra&ubreve;pluva a&ubreve; kontra&ubreve;suna portebla &scirc;irmilo, konsistanta el tenilo
      kaj &scirc;tofo a&ubreve; papero ronde stre&ccirc;ita sur vergetoj:
      <ekz>
        pluv<tld/>o, sun<tld/>o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="nl">
    <trd>scherm</trd>,
    <trd>regenscherm</trd>,
    <trd>zonnescherm</trd>,
    <trd>paraplu</trd>,
    <trd>parasol</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>paraply</trd>,
    <trd>parasoll</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_o;&c_n;&c_t;</trd>,
    <trd>&c_z;&c_o;&c_n;&c_t;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
