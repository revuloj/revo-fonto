<vortaro>
<art mrk="$Id$">
<kap>
  <rad>potencial</rad>/o
</kap>
<drv mrk="potenc1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      Mezuro de la energio entenata en kondukilo:
      <ekz>
        <ind><tld/>a diferenco</ind>
        <klr>(= tensio)</klr>.
        <trd lng="ru">&c_r;&c_a;&c_z;&c_n;&c_o;&c_s;&c_t;&c_mol; &c_p;&c_o;&c_t;&c_je;&c_n;&c_c;&c_i;&c_a;&c_l;&c_o;&c_v;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Potential</trd>
  <trd lng="nl">potentiaal</trd>
  <trd lng="ru">&c_p;&c_o;&c_t;&c_je;&c_n;&c_c;&c_i;&c_a;&c_l;</trd>
</drv>
</art>
