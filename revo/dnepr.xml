<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: dnepr.xml,v 1.5 2023/11/04 18:50:51 afido Exp $">
    <kap>
      <rad>Dnepr</rad>/o
    </kap>

<drv mrk="dnepr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:riveroj" cel="river.0o.GEOG">Rivero</ref>
      fontanta 200&#x202F;km okcidente de Moskvo kaj post trafluo de
      Belarusujo kaj Ukrainujo (i.a. tra Kievo) enfluanta Nigran Maron:
      <ekz>
        A&ubreve;skultu: regimento nia &#x2AFD; &Ccirc;e <tld/>o lar&gcirc;a enloki&gcirc;is
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekde Uralo, en orienta E&ubreve;ropo, la pli grandaj
        riveroj fluas meridiane-suden
        (Volgo, Dono, <tld/>o)
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, n-ro 2 (100)</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_D;&c_n;&c_ja;&c_p;&c_r;&c_o;</trd>
    <trd lng="de">Dnepr</trd>
    <trd lng="en">Dnieper</trd>
    <trd lng="fr">Dniepr</trd>
    <trd lng="nl">Dnjepr</trd>
    <trd lng="ru">&c_D;&c_n;&c_je;&c_p;&c_r;</trd>
    <trd lng="uk">&c_D;&c_n;&c_ib;&c_p;&c_r;&c_o;</trd>
  </snc>
  <trd lng="ja">&#x30C9;&#x30CB;&#x30A8;&#x30D7;&#x30EB;&#x5DDD;  <pr>&#x30C9;&#x30CB;&#x30A8;&#x30D7;&#x30EB;&#x304B;&#x308F;</pr></trd>
  <trd lng="zh">&#x7B2C;&#x8042;&#x4F2F;&#x6CB3; <pr>d&#xEC;ni&#xE8;b&#xF3;h&#xE9;</pr></trd>
</drv>

  </art>

