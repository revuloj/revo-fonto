<vortaro>
  <art mrk="$Id: pert.xml,v 1.5 2024/02/17 06:43:06 afido Exp $">
    <kap>
      <rad>Pert</rad>/o
    </kap>

<drv mrk="pert.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Granda <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      en la okcidento de <ref tip="malprt" cel="auxstr1.A0o.sxtato">A&ubreve;stralio</ref>,
      fondita 1829 &ccirc;e la elfluejo de Cignorivero:
      <ekz>
        ekspedicio al <tld/>o, naski&gcirc;anta &ccirc;efurbo de la frua dekna&ubreve;a-jarcenta
        okcident-a&ubreve;stralia setlado
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan MacGill</aut>
          <vrk><url ref="&Monato;/2012/010164.php">Panorama soro super kontinento</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        la observatorio de Perto<klr>[&#x2026;]</klr> estis konstruita en 1896
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Observatorio_de_Perto">Observatorio de Perto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_P;&c_e;&c_r;&c_t;</trd>
    <trd lng="de">Perth</trd>
    <trd lng="en">Perth</trd>
    <trd lng="fr">Perth</trd>
    <trd lng="pl">Perth</trd>
    <trd lng="ro">Perth</trd>
  </snc>
  <trdgrp lng="zh">
    <trd>&#x5E15;&#x65AF; <pr>P&#xE0;s&#x12B;</pr></trd>,
    <trd>&#x73C0;&#x65AF; <pr>P&#xF2;s&#x12B;</pr></trd>
  </trdgrp>
</drv>

  </art>

