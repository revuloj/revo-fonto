<art mrk="$Id: frisbe.xml,v 1.2 2025/10/21 06:43:30 afido Exp $">
<kap>
    <rad>frisbe</rad>/o,
    <var><kap><rad var="i">frisbi</rad>/o</kap></var>
</kap>
<drv mrk="frisbe.0o">
  <kap><tld/>o,
  <var><kap><tld var="i"/>o</kap></var></kap>
  <snc mrk="frisbe.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="dif" cel="disk.flug0o.SPO">Flugdisko</ref>:
      <ekz>
        ludu <tld/>on kun ni! facilas lerni kaj tre amuzas
        <fnt>
          <aut>ilei</aut>
          <vrk><url ref="https://www.ilei.info/konferenco/2023/kongresoLinjano2023.pdf">
          Kongresa Libro Komuna</url></vrk>
          <lok>2023</lok>
        </fnt>;
      </ekz>
      <ekz>
        voja&gcirc;emuloj povos paroli pri anka&ubreve; kajako, motorbicikloj, Danlando,
        mineraloj, telefonlibroj, Universal' Kalendar' (tiel!) <tld var="i"/>o, skermado, kaj
        radioestezo
        <fnt>
          <aut>N. Saletti</aut>
          <vrk>Pasporta Servo</vrk>
          <lok><url ref="http://www.eventoj.hu/arkivo/eve-005.doc">Eventoj, 1992-05 (5)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
<trd lng="be">&c_l;&c_ja;&c_t;&c_u;&c_ch;&c_y; <ind>&c_d;&c_y;&c_s;&c_k;</ind></trd>
</drv>
</art>

