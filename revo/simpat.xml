<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: simpat.xml,v 1.25 2022/05/10 06:35:57 revo Exp $">
<kap>
   <ofc>1</ofc>
   <rad>simpati</rad>/o
</kap>
<drv mrk="simpat.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Reciproka favora instinkta inklino de du personoj, pro similaj 
      <ref tip="super" cel="sent.0o.inklino">sento</ref>j kaj sintenoj:
      <ekz>
        en Kristo ekzistas ia konsolo, ia <tld/>o de amo, ia kunuleco de la Spirito, ia
        korfavoro kaj kompato
        <fnt>
          <bib>NT</bib>
          <lok>Filipianoj 2:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Hiram</nom> rigardis lin kun kompato, a&ubreve; <tld/>o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        diru al fra&ubreve;lino Klaro, ke miaj agoj estis nek &scirc;erco, nek kaprico, en la komenco
        tio estis <tld/>o, kaj poste amo kaj respekto
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro V</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akord.0o.samopinieco">akordo<sncref/></ref>,
      <ref cel="harmon1.0o.paco">harmonio<sncref/></ref>,
      <ref cel="konkor.0o">konkordo</ref>,
      <ref cel="sent.kun0o">kunsento</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Gleichklang <klr>(der Empfindungen)</klr></trd>,
      <trd>Einklang</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Favora <ref tip="super" cel="inklin.0o.KOMUNE">inklino</ref> de iu a&ubreve; io, 
      kiu pla&ccirc;as al li:
      <ekz>
        &scirc;i akiris &ccirc;e li <tld/>on kaj favoron
        <fnt>
          <bib>MT</bib>
          <lok>Ester 2:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili veturu tra Svedujo, sed la maljunulo &gcirc;is nun ankora&ubreve; ne havas <tld/>on por
        tiu lando
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        multaj homoj <klr>[&#8230;]</klr> ne kutimis donadi al si precizan kalkulon pri siaj
        <tld/>oj kaj <ref tip="ant" cel="antipa.0o">antipatio</ref>j <klr>[al]</klr> ia ideo
        <fnt>
          <bib>EE</bib>
          <lok>II</lok>
        </fnt>;
      </ekz>
      <ekz>
        verdire ni sentas pli da <tld/>o kaj admiro por homo, kiu pretas oferi sian
        vivon pro sia idealo
        <fnt>
          <bib>Kandid</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="favor.0o.KOMUNE">favoro</ref>,
      <ref cel="placx.0o">pla&ccirc;o</ref>,
      <ref cel="prefer.0o">prefero</ref>,
      <ref cel="sxat.0o">&scirc;ato</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_m;&c_p;&c_a;&c_t;&c_y;&c_ja;</trd>
  <trd lng="ca">simpatia</trd>
  <trdgrp lng="cs">
    <trd>n&aacute;chylnost</trd>,
    <trd>n&aacute;klonnost</trd>,
    <trd>p&rcaron;&iacute;chylnost</trd>,
    <trd>sympatie</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Sympathie</trd>,
    <trd>Zuneigung</trd>
  </trdgrp>
  <trd lng="en">sympathy</trd>
  <trd lng="es">simpat&iacute;a</trd>
  <trd lng="fr">sympathie</trd>
  <trdgrp lng="hu">
     <trd>rokonszenv</trd>,
     <trd>szimp&aacute;tia</trd>
   </trdgrp>
  <trd lng="it">simpatia<klr> (benevolenza)</klr></trd>
  <trd lng="nl">sympathie</trd>
  <trd lng="ru">&c_s;&c_i;&c_m;&c_p;&c_a;&c_t;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>n&aacute;klonnos&tcaron;</trd>,
    <trd>sympatia</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&c_m;&c_p;&c_a;&c_t;&c_ib;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="simpat.0i">
   <kap><tld/>i</kap>
   <gra><vspec>x</vspec></gra>
   <snc>
     <dif>
       Senti <tld/>on al iu, havi <tld/>on por:
       <ekz>
         mi ne <tld/>as kun li;
       </ekz>
       <ekz>
         ili <tld/>as nian aferon<fnt>Z</fnt>;
       </ekz>
       <ekz>
         tre <tld/>i al iu ideo.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_s;&c_y;&c_m;&c_p;&c_a;&c_t;&c_y;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
   <trd lng="ca">simpatitzar (amb)</trd>
   <trd lng="de">sympathisieren</trd>
   <trd lng="en">sympathize</trd>
   <trd lng="es">simpatizar</trd>
   <trd lng="fr">sympathiser</trd>
   <trdgrp lng="hu">
     <trd>rokonszenvezik</trd>,
     <trd>szimpatiz&aacute;l</trd>
   </trdgrp>
   <trdgrp lng="it">
     <trd>avere <ind>simpatia</ind></trd>, 
     <trd>provare <ind>simpatia</ind></trd>,
     <trd>mostrare <ind>simpatia</ind></trd>
   </trdgrp>      
   <trd lng="nl">sympathiseren</trd>
   <trd lng="ru">&c_s;&c_i;&c_m;&c_p;&c_a;&c_t;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&c_m;&c_p;&c_a;&c_t;&c_i;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i; <klr>(&c_k;&c_o;&c_m;&c_u;-&c_n;.)</klr></trd>,
    <trd>&c_v;&c_ib;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i; &c_v;&c_z;&c_a;&c_jeu;&c_m;&c_n;&c_u; &c_s;&c_i;&c_m;&c_p;&c_a;&c_t;&c_ib;&c_ju;</trd>
  </trdgrp>
</drv>

<drv mrk="simpat.0a">
   <kap><tld/>a</kap>
   <snc mrk="simpat.0a.simpatianta">
     <dif>
       <tld lit="S"/>a, sentanta a&ubreve; montranta <tld/>on por iu;
       <tld/>plena:
       <ekz>
         Jaspa <tld/>e suspiris
         <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 57a, p. 274a</lok></fnt>;
       </ekz>
       <ekz>
         la <tld/>a etoso, kiu regis dum la simpozio
         <fnt>
         <aut>Carlo Minnaja</aut>,
         <vrk>Du akademioj kune (Geraldo Mattos kaj Christer Kiselman,
         Lingva planado kaj leksikologio)</vrk>,
         <bib>Monato</bib>,
         <lok>jaro 2002a, numero 1a, p. 26a</lok>
         </fnt>..
       </ekz>
     </dif>
     <adm>
       &Ccirc;i tiu aktiva senco ne aperas en PIV1 a&ubreve; PIV2.
       <aut>&jcirc;v, mb</aut>
     </adm>
     <trd lng="be">&c_s;&c_y;&c_m;&c_p;&c_a;&c_t;&c_y;&c_z;&c_u;&c_ju;&c_ch;&c_y;</trd>
   </snc>
   <snc mrk="simpat.0a.placxa">
     <dif>
       Estiganta <tld/>on de iu; <tld/>inda, <tld/>veka, pla&ccirc;a:
       <ekz>
         personoj, al kiuj mi ne estis <tld/>a<fnt>Z</fnt>.
       </ekz>
     </dif>
     <trd lng="be">&c_s;&c_y;&c_m;&c_p;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
     <trd lng="de">sympathisch</trd>
     <trd lng="fr">sympathique</trd>
   </snc>
  <trd lng="ca">simp&agrave;tic</trd>
  <trd lng="cs">sympatick&yacute;</trd>
  <trd lng="en">sympathetic</trd>
  <trd lng="es">simp&aacute;tico<klr>/a</klr></trd>
  <trdgrp lng="hu">
     <trd>rokonszenves</trd>,
     <trd>szimpatikus</trd>
   </trdgrp>
  <trd lng="it">simpatico<klr> (benevolenza)</klr></trd>
  <trd lng="nl">sympathiek</trd>
  <trd lng="ru">&c_s;&c_i;&c_m;&c_p;&c_a;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">sympatick&yacute;</trd>
  <trd lng="uk">&c_s;&c_i;&c_m;&c_p;&c_a;&c_t;&c_i;&c_ch;&c_n;&c_i;&c_j;</trd>
</drv>

<drv mrk="simpat.mal0o">
   <kap>mal<tld/>o</kap>
   <snc>
     <dif>
       Spontanea malfavora sento al iu, ofte reciproka:
       <ekz>
         la germana registaro <klr>[&#8230;]</klr> falis en truegon de
         mal<tld/>o
         <fnt>
         <aut>Stefan Maul</aut>,
         <vrk>En giganta truo</vrk>,
         <bib>Monato</bib>,
         <lok>jaro 1999a, numero 9a, p. 6a</lok></fnt>.
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="antipa.0o">antipatio</ref>
     </refgrp>
   </snc>
  <trdgrp lng="be">
     <trd>&c_a;&c_n;&c_t;&c_y;&c_p;&c_a;&c_t;&c_y;&c_ja;</trd>,
     <trd>&c_n;&c_je;&c_p;&c_r;&c_y;&c_ja;&c_z;&c_mol;</trd>
   </trdgrp>
  <trdgrp lng="ca">
     <trd>antipatia</trd>,
     <trd>malvolen&ccedil;a</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>antipatie</trd>,
    <trd>averze</trd>
  </trdgrp>
  <trd lng="de">Antipathie</trd>
  <trd lng="en">antipathy</trd>
  <trd lng="es">antipat&iacute;a</trd>
  <trd lng="fr">antipathie</trd>
  <trdgrp lng="hu">
     <trd>ellenszenv</trd>,
     <trd>antip&aacute;tia</trd>,
     <trd>unszimp&aacute;tia</trd>
   </trdgrp>
  <trd lng="it">antipatia</trd>
  <trdgrp lng="ru">
     <trd>&c_a;&c_n;&c_t;&c_i;&c_p;&c_a;&c_t;&c_i;&c_ja;</trd>,
     <trd>&c_n;&c_je;&c_p;&c_r;&c_i;&c_ja;&c_z;&c_n;&c_mol;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>antipatia</trd>,
    <trd>averzia</trd>
  </trdgrp>
  <trd lng="uk">&c_a;&c_n;&c_t;&c_i;&c_p;&c_a;&c_t;&c_ib;&c_ja;</trd>
</drv>

<drv mrk="simpat.mal0i">
   <kap>mal<tld/>i</kap>
   <gra><vspec>x</vspec></gra>
   <snc>
     <dif>
       Senti, montri malfavoran inklinon, ofte reciprokan:
       <ekz>
         ili mal<tld/>as kun la landoj, kiuj <klr>...</klr> devis
         ekmiliti
         <fnt>
         <aut>Aleksandro Gofen</aut>,
         <vrk>Brave, prezidanto Putin!</vrk>,
         <bib>Monato</bib>,
         <lok>jaro 2003a, numero 2a, p. 12a</lok></fnt>.
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="antipa.0i">antipatii</ref>
     </refgrp>
   </snc>
   <trdgrp lng="be">
     <trd>&c_a;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_c;&c_mol; <ind>&c_a;&c_n;&c_t;&c_y;&c_p;&c_a;&c_t;&c_y;&c_ju;</ind></trd>,
     <trd>&c_a;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_c;&c_mol; <ind>&c_n;&c_je;&c_p;&c_r;&c_y;&c_ja;&c_z;&c_mol;</ind></trd>
   </trdgrp>
   <trdgrp lng="ca">
     <trd>avorrir</trd>,
     <trd>detestar</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>Abneigung haben</trd>,
     <trd>unangenehm <ind>finden</ind></trd>,
     <trd>unsympathisch <ind>finden</ind></trd>
   </trdgrp>
   <trd lng="en">antipathize</trd>
<trdgrp lng="es">
  <trd>detestar</trd>,
  <trd>rechazar</trd>
</trdgrp>
   <trd lng="fr">ressentir de l'<ind>antipathie</ind> (pour)</trd>
   <trdgrp lng="hu">
     <trd>ellenszenvet &eacute;rez</trd>,
     <trd>nem rokonszenvez</trd>
   </trdgrp>
   <trdgrp lng="it">
     <trd>avere <ind>antipatia</ind></trd>, 
     <trd>provare <ind>antipatia</ind></trd>,
     <trd>mostrare <ind>antipatia</ind></trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;
     &c_a;&c_n;&c_t;&c_i;&c_p;&c_a;&c_t;&c_i;&c_ju;</trd>,
     <trd>&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;
     &c_n;&c_je;&c_p;&c_r;&c_i;&c_ja;&c_z;&c_n;&c_mol;</trd>
   </trdgrp>
   <trd lng="sk">nezn&aacute;&scaron;a&tcaron;</trd>
</drv>
</art>
