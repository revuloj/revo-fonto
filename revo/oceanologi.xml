<vortaro>
<art mrk="$Id: oceanologi.xml,v 1.12 2024/01/30 16:09:13 revo Exp $">
<kap>
  <rad>oceanologi</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>

<drv mrk="oceanologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="oceanologi.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Marscienco studanta la maran medion, &gcirc;iajn fizikajn, &hcirc;emiajn kaj
      vivajn aspektojn:
      <ekz>
        la <tld/>a instituto en San Diego
        <fnt>
        <aut>D. Bindmann</aut>,
        <vrk>Re: biologia klasifiko</vrk>,
        <lok>soc.culture.esperanto, 1999-11-20</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_ib;&c_ja;&c_n;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="cs">oce&aacute;nologie</trd>
  <trdgrp lng="de">
    <trd>Ozeanografie</trd>,
    <trd>Meereskunde</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>oceanography</trd>,
    <trd>oceanology</trd>,
    <trd>sea science</trd>,
    <trd>ocean science</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_A;&a_q;&f_y;&a_A;&a_n;&a_w;&a_s;&zwnj;&a_sh;&a_n;&a_A;&a_s;&f_y;</trd>,
    <trd>&a_A;&a_q;&f_y;&a_A;&a_n;&a_w;&a_s;&zwnj;&a_n;&f_g;&a_A;&a_r;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">oc&eacute;anologie</trd>
  <trd lng="nl">oceanologie</trd>
  <trd lng="pl">oceanografia</trd>
  <trd lng="ro">oceanografie</trd>
  <trd lng="ru">&c_o;&c_k;&c_je;&c_a;&c_n;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="sk">oce&aacute;nol&oacute;gia</trd>
  <trdgrp lng="zh">
    <trd>&#x6D77;&#x6D0B;&#x5B66; <pr>h&#x1CE;iy&#xE1;ngxu&#xE9;</pr></trd>,
    <trd>&#x6D77;&#x6D0B;&#x5B78; <pr>h&#x1CE;iy&#xE1;ngxu&#xE9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="oceanologi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="oceanologi.0isto.SCI">
    <dif>
      Marsciencisto, fakulo pri <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_ib;&c_ja;&c_n;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trdgrp lng="de">
    <trd>Ozeanograf</trd>,
    <trd>Meereskundler</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>oceanographer</trd>,
    <trd>oceanologist</trd>
  </trdgrp>
  <trd lng="fa">&a_A;&a_q;&f_y;&a_A;&a_n;&a_w;&a_s;&zwnj;&a_sh;&a_n;&a_A;&a_s;</trd>
  <trd lng="fr">oc&eacute;anologiste</trd>
  <trd lng="nl">oceanoloog</trd>
  <trd lng="pl">oceanograf</trd>
  <trd lng="pt">oceanologia</trd>
  <trd lng="ro">oceanograf</trd>
  <trd lng="ru">&c_o;&c_k;&c_je;&c_a;&c_n;&c_o;&c_l;&c_o;&c_g;</trd>
  <trdgrp lng="zh">
    <trd>&#x6D77;&#x6D0B;&#x5B66;&#x8005; <pr>h&#x1CE;iy&#xE1;ngxu&#xE9;zh&#x11B;</pr></trd>,
    <trd>&#x6D77;&#x6D0B;&#x5B78;&#x8005; <pr>h&#x1CE;iy&#xE1;ngxu&#xE9;zh&#x11B;</pr></trd>,
    <trd>&#x6D77;&#x6D0B;&#x7814;&#x7A76;&#x8005; <pr>h&#x1CE;iy&#xE1;ngy&#xE1;nji&#x16B;zh&#x113;</pr></trd>
  </trdgrp>
</drv>
</art>

