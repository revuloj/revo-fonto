<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>oceanologi</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="oceanologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="oceanologi.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Marscienco studanta la maran medion, &gcirc;iajn fizikajn, &hcirc;emiajn kaj
      vivajn aspektojn:
      <ekz>
        la <tld/>a instituto en San Diego
        <fnt>
        <aut>D. Bindmann</aut>,
        <vrk>Re: biologia klasifiko</vrk>,
        <lok>soc.culture.esperanto, 1999-11-20</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">oc&eacute;anologie</trd>
  <trd lng="ru">&c_o;&c_k;&c_je;&c_a;&c_n;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
</drv>

<drv mrk="oceanologi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="oceanologi.0isto.SCI">
    <dif>
      Marsciencisto, fakulo pri <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">oc&eacute;anologiste</trd>
  <trd lng="pt">oceanologia</trd>
  <trd lng="ru">&c_o;&c_k;&c_je;&c_a;&c_n;&c_o;&c_l;&c_o;&c_g;</trd>
</drv>
</art>

