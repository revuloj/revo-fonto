<drv mrk="direkt.0anto">
  <kap><tld/>anto</kap>
  <snc mrk="direkt.0anto.KOMUNE">
    <dif>
      Iu, kiu <tld/>as
    </dif>
    <subsnc mrk="direkt.0anto.de_veturilo">
      <dif>
        veturilon:    
        <ekz>
          kun rideto <klr>[li]</klr> a&ubreve;skultis la plendojn de la juna <tld/>anto 
          <klr>(vd <ref tip="sub" cel="direkt.0ilisto">direktilisto</ref>)</klr> de la
          regna &scirc;ipo
          <fnt>
            <bib>Far1</bib>
            <lok>&Ccirc;apitro XVIII</lok>
          </fnt>;
        </ekz>
        <ekz>
          la junulo mem kondukis la veturilon, <klr>[&#8230;]</klr> kaj lia kunulino
          <klr>[&#8230;]</klr> flanke de la <tld/>anto, &gcirc;uis tiun akran senton de rapideco
          <fnt>
            <bib>ChL</bib>
            <lok>&Ccirc;apitro Dekkvina</lok>
          </fnt>;
        </ekz>
      </dif>
    <trdgrp lng="de">
      <trd>Steuermann</trd>,
      <trd>Fahrer</trd>
    </trdgrp>
    </subsnc>
    <subsnc mrk="direkt.0anto.de_organizo">
      <dif>
        organiza&jcirc;on:  
        <ekz>
          la kapelestro rapidas al la <tld/>anto de la &hcirc;oroj
          <fnt>
            <bib>FK</bib>
            <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
          </fnt>;
        </ekz>
        <ekz>
          Mare&ccirc;alo, la &ccirc;efa &ccirc;as<tld/>anto <klr>[&#8230;]</klr> aliris al la markizo por ricevi
          liajn lastajn ordonojn
          <fnt>
            <bib>KPr</bib>
            <lok>&Ccirc;apitro Unua</lok>
          </fnt>;
        </ekz>
        <ekz>
          ellaboris la planon <tld/>anto <klr>[&#8230;]</klr> de la a&ubreve;tomobil-firmao
          <fnt>
            <bib>Monato</bib>
            <aut>Stefan Maul</aut>
            <vrk><url ref="https://www.monato.be/2008/009448.php"
                >Krimaj ri&ccirc;uloj</url></vrk>
            <lok>2008</lok>
          </fnt>.
        </ekz>
      </dif>
    <trdgrp lng="de">
      <trd>Leiter</trd>,
      <trd>Direktor</trd>
    </trdgrp>
    </subsnc>
    <trd lng="be">&c_k;&c_ib;&c_r;&c_a;&c_w;&c_n;&c_ib;&c_k;</trd>
    <trdgrp lng="pl">
      <trd>kieruj&aogonek;cy</trd>,
      <trd>kierownik</trd>
    </trdgrp>
  </snc>
  <snc mrk="direkt.0anto.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="direkt.0anto.linio">
      <fnt><bib>PIV1</bib></fnt>
      <dif>
        <klr>(de rektara <ref cel="surfac.0o.MAT">surfaco</ref>)
        </klr>
        Iu linio, sekcata de &ccirc;iuj
        <ref cel="nask.0anto.rekto">naskantoj</ref> de la koncerna
        surfaco:
        <ekz>
          <tld/>antoj de
          <ref cel="konus.0o.surfaco">konuso</ref>,
          <ref cel="cilind.0o.surfaco">cilindro</ref>.
        </ekz>
      </dif>
      <ref tip="sin" cel="direkt3.0o.linio">direktrico</ref>
      <trdgrp lng="be">
        <trd>
        &c_n;&c_a;&c_k;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja;
        </trd>,
        <trd>
        &c_n;&c_a;&c_k;&c_ib;&c_r;&c_o;&c_w;&c_v;&c_a;&c_ju;&c_ch;&c_a;&c_ja;
        </trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Direktrix</trd>,
        <trd>Leitkurve</trd>
      </trdgrp>
      <trd lng="pl">kieruj&aogonek;ca</trd>
      <trd lng="ru">&c_n;&c_a;&c_p;&c_r;&c_a;&c_v;&c_l;&c_ja;&c_ju;&c_shch;&c_a;&c_ja;</trd>
    </subsnc>
    <subsnc mrk="direkt.0anto.koniko">
      <fnt><bib>PIV1</bib>, <lok>alude</lok></fnt>
      <dif>
        <klr>(de <ref cel="konik.0o">koniko</ref>)</klr>
        Tia <ref tip="super" cel="rekt.0o.MAT">rekto</ref>,
        ke la rilato inter la distanco de &ccirc;iu punkto de la
        koniko al &gcirc;ia fokuso
        kaj la distanco de la sama punkto al la rekto egalas al
        la discentreco.
      </dif>
      <ref tip="sin" cel="direkt3.0o.koniko">direktrico</ref>
      <trd lng="be">&c_d;&c_y;&c_r;&c_e;&c_k;&c_t;&c_r;&c_y;&c_s;&c_a;</trd>
      <trdgrp lng="de">
        <trd>Direktrix</trd>,
        <trd>Leitlinie</trd>
      </trdgrp>
      <trd lng="pl">kierownica<klr tip="ind"> (sto&zdot;kowej)</klr></trd>
      <trd lng="ru">&c_d;&c_i;&c_r;&c_je;&c_k;&c_t;&c_r;&c_i;&c_s;&c_a;</trd>
    </subsnc>
    <trd lng="en">directrix</trd>
    <trd lng="fr">directrice</trd>
  </snc>
  <trd lng="cs">sm&ecaron;rnice <klr>(p&rcaron;&iacute;mky)</klr></trd>
  <trd lng="he">&mem;&#1456;&nun;&#1463;&he;&#1461;&lamed;</trd>
  <trd lng="sk">smernica <klr>(priamky)</klr></trd>
</drv>

