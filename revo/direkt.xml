    <trd lng="ru">&c_n;&c_a;&c_p;&c_r;&c_a;&c_v;&c_l;&c_ja;&c_ju;&c_shch;&c_a;&c_ja;<klr tip="ind"> (&c_a;&c_f;&c_f;&c_i;&c_n;&c_n;&c_o;&c_g;&c_o;
    &c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_a;)</klr></trd>
  </snc>
  <trd lng="bg">&c_p;&c_o;&c_s;&c_o;&c_k;&c_a;</trd>
  <trd lng="bo">&t_pha;&t_yata;&t_naro;&t_ga;&t_sa;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>sm&ecaron;r</trd>,
    <trd>sm&ecaron;r</trd>
  </trdgrp>
