  <trd lng="he">&#x5DC;&#x5D4;&#x5E1;&#x5EA;&#x5D5;&#x5D1;&#x5D1;</trd>
  <trd lng="ru">&c_o;&c_b;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>
</drv>
