<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tortur</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="tortur.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Kruela turmentego, al kiu oni iam submetis kondamniton por lin puni
      a&ubreve; akuziton por devigi lin al konfeso.
    </dif>
    <trdgrp lng="de">
      <trd>Folter</trd>,
      <trd>Marter</trd>,
      <trd>Tortur</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Fizika a&ubreve; morala turmentego.
    </dif>
    <trdgrp lng="de">
      <trd>Qual</trd>,
      <trd>Pein</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">torture</trd>
  <trdgrp lng="lat">
    <trd>tormentum, i</trd>,
    <trd>cruciatus, us</trd>
    <trd>supplicium, ii</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>marteling</trd>,
    <trd>foltering</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_y;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_i;&c_s;&c_t;&c_ja;&c_z;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="tortur.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kruele turmenti.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>foltern</trd>,
    <trd>martern</trd>,
    <trd>peinigen</trd>
  </trdgrp>
  <trd lng="fr">torturer</trd>
  <trd lng="lat">excrucio, are</trd>
  <trdgrp lng="nl">
    <trd>martelen</trd>,
    <trd>folteren</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_y;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_s;&c_t;&c_ja;&c_z;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
