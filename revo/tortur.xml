      &c_p;&c_y;&c_t;&c_k;&c_i;</trd>
  <trd lng="sk">mu&ccaron;idlo</trd>
  <trd lng="uk">&c_z;&c_n;&c_a;&c_r;&c_ja;&c_d;&c_d;&c_ja; &c_d;&c_l;&c_ja; &c_t;&c_o;&c_r;&c_t;&c_u;&c_r;</trd>
  <trd lng="zh">&#21009;&#20855; <pr>x&iacute;ngj&ugrave;</pr></trd>
</drv>

<drv mrk="tortur.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="tortur.0ejo.JUR">
    <dif>
      Loko destinita por <tld/>oj:
      <ekz>
        la elizabetaj <tld/>ejoj en la Turo de Londono
        <fnt>
          <bib>Monato</bib>
          <aut>Juan J. Schmitter-Soto</aut>
          <vrk><url ref="&Monato;/2004/008272.html">&Ccirc;u esperantistoj kapablus altnivele
          diskuti?</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        &Scirc;tatdefenda Oficejo, fakte <tld/>ejo por alipensantoj
        <fnt>
          <bib>Monato</bib>
          <aut>Jozefo Horv&aacute;th</aut>
          <vrk><url ref="&Monato;/2006/009060.php">Tra infanaj okuloj: memoroj pri la eventoj de
          '56</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_t;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_mol;&c_c;&c_je;&c_n;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">sala de <ind>tortura</ind></trd>
  <trd lng="cs">mu&ccaron;&iacute;rna</trd>
  <trdgrp lng="de">
    <trd>Folterkammer</trd>,
    <trd>Marterpfahl</trd>
  </trdgrp>
  <trd lng="en">torture chamber</trd>
  <trd lng="fr">salle de <ind>torture</ind></trd>
  <trd lng="hu">k&iacute;nz&oacute;kamra</trd>
  <trd lng="ja">&#25335;&#21839;&#37096;&#23627;  <pr>&#12372;&#12358;&#12418;&#12435;&#12408;&#12420;</pr></trd>
  <trd lng="pl">sala tortur</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_s;&c_t;&c_je;&c_n;&c_o;&c_k;</trd>,
    <trd>&c_k;&c_a;&c_m;&c_je;&c_r;&c_a;
    &c_p;&c_y;&c_t;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">mu&ccaron;iare&ncaron;</trd>
  <trd lng="uk">&c_k;&c_a;&c_t;&c_ib;&c_v;&c_n;&c_ja;</trd>
  <trd lng="zh">&#21009;&#22530; <pr>x&iacute;ngt&aacute;ng</pr></trd>
</drv>
