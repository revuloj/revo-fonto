<art mrk="$Id: atletik.xml,v 1.21 2023/10/28 08:33:10 revo Exp $">
<kap>
  <rad>atletik</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="atletik.0o">
  <kap><tld/>o</kap>
  <snc mrk="atletik.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      La grupo de sportoj, kiuj bezonas rimarkindan fortecon;
      <ref tip="sin" cel="atlet.0ismo">atletismo</ref>:
      <ekz>
        a&ubreve;stralia heroino de <tld/>o fari&gcirc;is monde konata: estis &scirc;i, kiu metis fajron en
        la olimpian kalikon
        <fnt>
          <aut>IGARASI Takeo</aut>
            <vrk><url ref="&Monato;/2000/007204.html">Tor&ccirc;o de hipokriteco</url></vrk>
            <bib>Monato</bib>
            <lok>2000/12, p. 15</lok>
          </fnt>;
      </ekz>
      <ekz>
        kiuj interesi&gcirc;as pri <tld/>o a&ubreve; korpa prizorgado, tiuj ne nur 
        pensas pri sanstato kaj trejnado, sed siatempe anka&ubreve; pri 
        malstre&ccirc;o
        <fnt><aut>Lukiano, trad. G. Berveling</aut>, <vrk><url ref="http://www.esperanto.gr/lit/els/rakontoj.pdf">veraj 
        rakontoj</url>. - 2005</vrk></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x5D07;&#x5C1A;&#x8FD0;&#x52A8; <pr>ch&#xF3;ngsh&#xE0;ngy&#xF9;nd&#xF2;ng</pr></trd>,
    <trd>&#x5D07;&#x5C1A;&#x904B;&#x52D5; <pr>ch&#xF3;ngsh&#xE0;ngy&#xF9;nd&#xF2;ng</pr></trd>,
    <trd>&#x7ADE;&#x6280;&#x70ED; <pr>j&#xEC;ngj&#xEC;r&#xE8;</pr></trd>,
    <trd>&#x7AF6;&#x6280;&#x71B1; <pr>j&#xEC;ngj&#xEC;r&#xE8;</pr></trd>,
    <trd>&#x8FD0;&#x52A8;&#x7ADE;&#x8D5B; <pr>y&#xF9;nd&#xF2;ngj&#xEC;ngs&#xE0;i</pr></trd>,
    <trd>&#x904B;&#x52D5;&#x7AF6;&#x8CFD; <pr>y&#xF9;nd&#xF2;ngj&#xEC;ngs&#xE0;i</pr></trd>,
    <trd>&#x8FD0;&#x52A8;&#x672F; <pr>y&#xF9;nd&#xF2;ngsh&#xF9;</pr></trd>,
    <trd>&#x904B;&#x52D5;&#x8853; <pr>y&#xF9;nd&#xF2;ngsh&#xF9;</pr></trd>,
    <trd>&#x4F53;&#x80B2;&#x6D3B;&#x52A8; <pr>t&#x1D0;y&#xF9;hu&#xF3;d&#xF2;ng</pr></trd>,
    <trd>&#x9AD4;&#x80B2;&#x6D3B;&#x52D5; <pr>t&#x1D0;y&#xF9;hu&#xF3;d&#xF2;ng</pr></trd>,
    <trd>&#x7ADE;&#x6280; <pr>j&#xEC;ngj&#xEC;</pr></trd>,
    <trd>&#x7AF6;&#x6280; <pr>j&#xEC;ngj&#xEC;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_t;&c_l;&c_je;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="bg">&c_a;&c_t;&c_l;&c_je;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="cs">atletika</trd>
  <trd lng="de">Athletik</trd>
  <trd lng="en">athletics</trd>
  <trd lng="es">atletismo</trd>
  <trd lng="fr">athl&eacute;tisme</trd>
  <trd lng="he">&alef;&#x5B7;&tav;&lamed;&#x5B5;&tet;&#x5B4;&yod;&qof;&#x5B8;&he;</trd>
  <trd lng="hu">atl&eacute;tika</trd>
  <trd lng="it">atletica</trd>
  <trdgrp lng="ja">
    <trd>&#x904B;&#x52D5;&#x7AF6;&#x6280;  <pr>&#x3046;&#x3093;&#x3069;&#x3046;&#x304D;&#x3087;&#x3046;&#x304E;</pr></trd>,
    <trd>&#x9678;&#x4E0A;&#x7AF6;&#x6280;  <pr>&#x308A;&#x304F;&#x3058;&#x3087;&#x3046;&#x304D;&#x3087;&#x3046;&#x304E;</pr></trd>
  </trdgrp>
  <trd lng="nl">atletiek</trd>
  <trd lng="pl">atletyka</trd>
  <trd lng="ro">atletism</trd>
  <trd lng="ru">&c_a;&c_t;&c_l;&c_je;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">atletika</trd>
  <trd lng="sv">friidrott</trd>
  <trd lng="uk">&c_a;&c_t;&#x43B;&c_je;&c_t;&c_i;&#x43A;&c_a;</trd>
</drv>

<drv mrk="atletik.malpeza_0o">
  <kap>
    malpeza <tld/>o
  </kap>
  <snc>
    <dif>
      La sportoj de kurado, saltado, &jcirc;etado
      (kontraste al peza <tld/>o, t.e. pezlevado, luktado ktp):
      <ekz>
        jen komenci&gcirc;as konkuro de la malpeza <tld/>o,
        homoj simile al katoj saltas trans la barieroj
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_jo;&c_g;&c_k;&c_a;&c_ja; <ind>&c_a;&c_t;&c_l;&c_je;&c_t;&c_y;&c_k;&c_a;</ind></trd>
  <trd lng="de">Leichtathletik</trd>
</drv>


</art>

