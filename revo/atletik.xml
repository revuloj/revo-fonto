<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>atletik</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="atletik.0o">
  <kap><tld/>o</kap>
  <snc mrk="atletik.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <ref tip="dif" cel="atlet.0ismo">atletismo</ref>
    <ekz>
      jen komenci&gcirc;as konkuro de la malpeza <tld/>o
      <fnt><bib>Metrop</bib></fnt>;
    </ekz>
    <ekz>
      kiuj interesi&gcirc;as pri <tld/>o a&ubreve; korpa prizorgado, tiuj ne nur 
      pensas pri sanstato kaj trejnado, sed siatempe anka&ubreve; pri 
      malstre&ccirc;o
      <fnt><aut>Lukiano, trad. G. Berveling</aut>, <vrk><url 
      ref="http://www.esperanto.gr/lit/els/rakontoj.pdf">veraj 
      rakontoj</url>. - 2005</vrk></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_a;&c_t;&c_l;&c_je;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="de">Athletik</trd>
  <trd lng="fr">athl&eacute;tisme</trd>
  <trd lng="ru">&c_a;&c_t;&c_l;&c_je;&c_t;&c_i;&c_k;&c_a;</trd>
</drv>
</art>

