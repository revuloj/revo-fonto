<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kofr</rad>/o
</kap>
<drv mrk="kofr.0o">
  <kap><tld/>o</kap>
  <snc mrk="kofr.0o.KOMUNE">
    <dif>
      Granda, &scirc;losebla <ref tip="super"
      cel="kest.0o.KOMUNE">kesto</ref> tre solida,
      ekzemple el ligno, por enteni vestojn kaj aliajn a&jcirc;ojn, ta&ubreve;ga
      por
      voja&gcirc;a transporto:
      <ekz>
        la trezoro trovi&gcirc;is en <tld/>o metita en kavo
        <fnt><bib>Gerda</bib>, <lok>&ccirc;apitro 23a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="valiz.0o">valizo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_f;&c_a;&c_r;</trd>
  <trd lng="cs">kufr</trd>
  <trdgrp lng="de">
    <trd>Reisekoffer</trd>,
    <trd>gro&szlig;er Reisekoffer</trd>,
    <trd>&Uuml;berseekoffer</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>coffer</trd>,
    <trd>chest</trd>,
    <trd>trunk</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cantine<klr tip="ind"> (malle)</klr></trd>,
    <trd>coffre</trd>,
    <trd>malle</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>koffer</trd>,
    <trd>utaz&oacute;l&aacute;da</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>koffer</trd>,
    <trd>reiskoffer</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_f;&c_r;</trd>,
    <trd>&c_s;&c_u;&c_n;&c_d;&c_u;&c_k;</trd>
  </trdgrp>
</drv>
</art>
