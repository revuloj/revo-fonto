<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kofr</rad>/o
</kap>
<drv mrk="kofr.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Granda, &scirc;losebla kesto, el ligno kaj ledo, por enteni vestojn
      kaj aliajn a&jcirc;ojn, kiujn oni transportas voja&gcirc;e. 
    </dif>
    <refgrp tip="vid">
      <ref cel="valiz.0o">valizo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_f;&c_a;&c_r;</trd>
  <trd lng="cs">kufr</trd>
  <trdgrp lng="de">
    <trd>Reisekoffer</trd>,
    <trd>gro&szlig;er Reisekoffer</trd>,
    <trd>&Uuml;berseekoffer</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>chest</trd>,
    <trd>trunk</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>coffre</trd>,
    <trd>malle</trd>,
    <trd>cantine<klr tip="ind"> (malle)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>koffer</trd>,
    <trd>reiskoffer</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_f;&c_r;</trd>,
    <trd>&c_s;&c_u;&c_n;&c_d;&c_u;&c_k;</trd>
  </trdgrp>
</drv>
</art>
