<art mrk="$Id: trigle.xml,v 1.11 2022/03/28 14:44:12 revo Exp $">
<kap>
  <rad>trigled</rad>/oj
</kap>
<drv mrk="trigle.0oj">
  <kap><tld/>oj</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="skorpen.0oformaj">skorpenoformaj
      fi&scirc;oj</ref> (<trd lng="la">Triglidae</trd>):
      <ekz>
        <ref tip="prt" cel="trigl.0o.genro">triglo</ref>
       <klr>[&#x2026;]</klr> apartenas al la <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=619978">Triglo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">tr&iacute;glids</trd>
  <trd lng="de">Knurrh&auml;hne<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="fr">grondins</trd>
  <trd lng="hu">morg&oacute;half&eacute;l&eacute;k</trd>
  <trd lng="nl">ponen<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pl">kurkowate</trd>
  <trd lng="pt">trigl&iacute;deos</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_i;&c_g;&c_l;&c_y;<klr tip="ind"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_m;&c_o;&c_r;&c_s;&c_k;&c_i;&c_je; &c_p;&c_je;&c_t;&c_u;&c_h;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="uk">&#x442;&#x440;&#x438;&#x433;&#x43B;&#x43E;&#x432;&#x456;</trd>
</drv>
</art>
