<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kibernetik</rad>/o <fnt><bib>PIV2</bib></fnt>,
  <var><kap><rad>cibernetik</rad>/o <fnt><bib>SPV</bib></fnt></kap></var>
</kap>
<drv mrk="kibernetik.0o">
  <kap><tld/>o, <var><kap>cibernetiko</kap></var></kap>
  <snc mrk="kibernetik.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Scienco pri agokapablaj a&ubreve; memregaj sistemoj kaj arto krei tiajn 
      sistemojn:
      <ekz>
        la sanmarina Instituto pri <tld lit="K"/>o
        <fnt>
          <aut>H. G. Frank</aut>,
          <vrk><url ref="http://www.ais-sanmarino.org/pr/eo/histor1.html">Historieto
          de la Akademio Internacia de la Sciencoj San
          Marino</url></vrk>,
          <lok>1985</lok>
        </fnt>;
      </ekz>    
      <ekz>
        la nova komputik-<tld/>a revolucio &scirc;an&gcirc;as filozofiojn, celojn, politikon kaj la
        ekonomian sistemon
        <fnt>
          <bib>Monato</bib>
          <aut>Zlatko Ti&scaron;ljar</aut>
          <vrk><url ref="https://www.monato.be/2006/009073.php">Survoje al alia mondo</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        povas esti, ke ni staras sur la sojlo de nova socia
        revolucio jam heroldita de la konkeroj
        de la nuklea scienco kaj de la cibernetiko  
        <fnt><aut>W. F. Rolt</aut>, <vrk>El la Polvo de la Tero</vrk>, 
        <lok>1967</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="auxtom.0o">a&ubreve;tomato</ref>,
      <ref cel="komput.0ilo">komputilo</ref>,
      <ref cel="robot.0o">roboto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_b;&c_e;&c_r;&c_n;&c_e;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="cs">kybernetika</trd>
  <trd lng="de">Kybernetik</trd>
  <trdgrp lng="fa">
    <trd>&a_f;&a_r;&a_m;&a_A;&a_n;&zwnj;&a_sh;&a_n;&a_A;&a_s;&f_y;</trd>,
    <trd>&a_s;&f_y;&a_b;&a_r;&a_n;&a_t;&f_y;&f_k;</trd>
  </trdgrp>
  <trd lng="fr">cybern&eacute;tique</trd>
  <trdgrp lng="nl">
    <trd>cybernetica</trd>,
    <trd>stuurkunde</trd>
  </trdgrp>
  <trd lng="pl">cybernetyka</trd>     
  <trd lng="ro">cibernetic&abreve;</trd>
  <trd lng="sk">kybernetika</trd>
</drv>

<drv mrk="kibernetik.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="kibernetik.0isto.SCI">
    <dif>
      Fakulo pri <ref tip="vid" cel="kibernetik.0o.SCI"><tld/>o</ref>:
      <ekz>
        diskutata estas anka&ubreve; propono de itala <tld/>isto <nom>Silvio 
        Ceccato</nom> konstrui internacian helplingvon per <tld/>aj metodoj
        <fnt>
          <aut>V. Barandovsk&aacute;-Frank</aut>,
          <vrk>Enkonduka lernolibro de interlingvistiko</vrk>,
          <lok><url ref="http://wwwcs.uni-paderborn.de/extern/fb/2/Kyb.Paed/INT/02-int.htm">&ccirc;apitro
          2a</url></lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_b;&c_e;&c_r;&c_n;&c_e;&c_t;&c_y;&c_k;</trd>
  <trd lng="de">Kybernetiker</trd>
  <trdgrp lng="fa">
    <trd>&a_f;&a_r;&a_m;&a_A;&a_n;&zwnj;&a_sh;&a_n;&a_A;&a_s;</trd>,
    <trd>&a_m;&a_t;&a_kh;&a_S;&a_S; <ind>&a_s;&f_y;&a_b;&a_r;&a_n;&a_t;&f_y;&f_k;</ind></trd>
  </trdgrp>
  <trd lng="fr">cybern&eacute;ticien</trd>
  <trd lng="nl">cyberneticus</trd>
  <trd lng="pl">cybernetyk</trd>
</drv>
</art>

