<kap>
  <rad>kairn</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="kairn.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="kairn.0o.ARKE">
    <uzo tip="fak">ARKE</uzo>
    <dif>
      <ref tip="dif" cel="amas.sxton0o.ARKE">&Scirc;tonamaso<sncref/></ref>:
      <ekz>
        vojmontrila <tld/>o konstruita |
        anta&ubreve; longe de &scirc;afoj-kolektantoj |
        por sekura gvido sur la montovojo
        <fnt>
          <aut>B. Ragnarsson</aut>
          <vrk>la&ubreve; neplanitaj padoj</vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>cairn</trd>,
    <trd>galgal</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x442;&#x443;&#x440;</trd>,
    <trd>&#x43F;&#x456;&#x440;&#x430;&#x43C;&#x456;&#x434;&#x43A;&#x430;</trd>,
    <trd>&#x43A;&#x443;&#x43F;&#x430; &#x43A;&#x430;&#x43C;&#x456;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43A;&#x443;&#x440;&#x433;&#x430;&#x43D;</trd>
  </trdgrp>
</drv>

