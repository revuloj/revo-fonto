<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kairn.xml,v 1.3 2023/07/19 08:31:12 revo Exp $">
<kap>
  <rad>kairn</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="kairn.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="kairn.0o.ARKE">
    <uzo tip="fak">ARKE</uzo>
    <dif>
      <ref tip="dif" cel="amas.sxton0o.ARKE">&Scirc;tonamaso<sncref/></ref>:
      <ekz>
        vojmontrila <tld/>o konstruita |
        anta&ubreve; longe de &scirc;afoj-kolektantoj |
        por sekura gvido sur la montovojo
        <fnt>
          <aut>B. Ragnarsson</aut>
          <vrk>la&ubreve; neplanitaj padoj</vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_g;&c_o;&c_r; &c_z; &c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;&c_n;&c_ja;</trd>
  <trdgrp lng="fr">
    <trd>cairn</trd>,
    <trd>galgal</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_u;&c_r;</trd>,
    <trd>&#x43F;&c_ib;&c_r;&c_a;&#x43C;&c_ib;&c_d;&#x43A;&c_a;</trd>,
    <trd>&#x43A;&c_u;&#x43F;&c_a; &#x43A;&c_a;&#x43C;&c_ib;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43A;&c_u;&c_r;&c_g;&c_a;&#x43D;</trd>
  </trdgrp>
</drv>

  </art>

