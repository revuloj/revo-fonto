<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kairn.xml,v 1.4 2023/11/20 21:22:57 revo Exp $">
<kap>
  <rad>kairn</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="kairn.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="kairn.0o.ARKE">
    <uzo tip="fak">ARKE</uzo>
    <dif>
      <ref tip="dif" cel="amas.sxton0o.ARKE">&Scirc;tonamaso<sncref/></ref>:
      <ekz>
        vojmontrila <tld/>o konstruita &parsl;
        anta&ubreve; longe de &scirc;afoj-kolektantoj &parsl;
        por sekura gvido sur la montovojo
        <fnt>
          <aut>B. Ragnarsson</aut>
          <vrk>la&ubreve; neplanitaj padoj</vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_g;&c_o;&c_r; &c_z; &c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;&c_n;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Steinm&auml;nnchen</trd>,
    <trd>Steinhaufen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cairn</trd>,
    <trd>galgal</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_u;&c_r;</trd>,
    <trd>&c_p;&c_ib;&c_r;&c_a;&c_m;&c_ib;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_u;&c_p;&c_a; &c_k;&c_a;&c_m;&c_ib;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_k;&c_u;&c_r;&c_g;&c_a;&c_n;</trd>
  </trdgrp>
</drv>

  </art>

