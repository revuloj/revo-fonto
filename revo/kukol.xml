<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kukol</rad>/o
</kap>

<drv mrk="kukol.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="kukole.0oj"><tld/>edoj</ref>
      (<trd lng="la">Cuculus</trd>), rimarkinda per tio,
      ke la ino metas siajn ovojn en nestojn de aliaj birdoj.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_ja;&c_z;&c_ju;&c_l;&c_ja;</trd>       
  <trd lng="bg">&c_k;&c_u;&c_k;&c_u;&c_v;&c_i;&c_c;&c_a;</trd>
  <trd lng="br">koukoug</trd>
  <trd lng="cs">kuka&ccaron;ka</trd>
  <trd lng="de">Kuckuck</trd>
  <trd lng="en">cuckoo</trd>
  <trd lng="fr">coucou</trd>
  <trd lng="hu">kakukk</trd>
  <trd lng="nl">koekoek</trd>
  <trd lng="pl">kuku&lstroke;ka</trd>
  <trd lng="pt">cuco</trd>
  <trd lng="ro">cuc</trd>
  <trd lng="ru">&c_k;&c_u;&c_k;&c_u;&c_sh;&c_k;&c_a;</trd>
</drv>

<drv mrk="kukol.0oformaj">
  <kap>
    <tld/>oformaj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Cuculiformes</trd>).
    </dif>
  </snc>
  <trd lng="de">Kuckucksv&ouml;gel</trd>
  <trd lng="hu">kakukkalak&uacute;ak</trd>
  <trd 
lng="ru">&c_k;&c_u;&c_k;&c_u;&c_sh;&c_k;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;</trd>
</drv>

<drv mrk="kukol.0horlogxo">
  <kap>
    <tld/>horlo&gcirc;o
  </kap>
  <snc>
    <dif>
      Horlo&gcirc;o, entenanta artefaritan <tld/>on, kiu sin montras
      por anonci la horojn per sia krio.
    </dif>
  </snc>
  <trd lng="de">Kuckucksuhr</trd>
  <trd lng="en">cuckoo <ind>clock</ind></trd>
  <trd lng="fr">horloge &agrave; coucou</trd>
  <trd lng="hu">kakukkos <ind>&oacute;ra</ind></trd>
  <trd lng="nl">koekoeksklok</trd>
  <trd lng="pl">zegar z kuku&lstroke;k&aogonek;</trd>
  <trd lng="ro">ceas cu cuc</trd>
  <trd lng="ru">&c_ch;&c_a;&c_s;&c_y; &c_s; 
&c_k;&c_u;&c_k;&c_u;&c_sh;&c_k;&c_o;&c_j;</trd>
</drv>
</art>
