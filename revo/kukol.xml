<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>kukol</rad>/o
</kap>
<drv mrk="kukol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="kukol.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Cuculus</trd>), rimarkinda per tio, 
      ke la ino metas siajn ovojn en nestojn de aliaj birdoj.
    </dif>
  </snc>
  <trd lng="cs">kuka&ccaron;ka</trd>
  <trd lng="de">Kuckuck</trd>
  <trd lng="en">cuckoo</trd>
  <trd lng="fr">coucou</trd>
  <trd lng="hu">kakukk</trd>
  <trd lng="nl">koekoek</trd>
  <trd lng="pl">kuku&lstroke;ka</trd>
  <trd lng="ru">&c_k;&c_u;&c_k;&c_u;&c_sh;&c_k;&c_a;</trd>
</drv>
<drv mrk="kukol.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="kukol.0oformaj"><tld/>oformaj
      birdoj</ref> (<trd lng="la">Cuculidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Kuckucke <klr tip="ind">(Familie)</klr></trd>
</drv>
<drv mrk="kukol.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Cuculiformes</trd>).
    </dif>
  </snc>
  <trd lng="de">Kuckucksv&ouml;gel</trd>
</drv>
<drv mrk="kukol.0horlogxo">
  <kap><tld/>horlo&gcirc;o</kap>
  <snc>
    <dif>
      Horlo&gcirc;o,  entenanta artefaritan <tld/>on, kiu sin montras
      por anonci la horojn per sia krio.
    </dif>
  </snc>
  <trd lng="de">Kuckucksuhr</trd>
  <trd lng="nl">koekoeksklok</trd>
  <trd lng="fr">horloge &agrave; coucou</trd>
</drv>
</art>
