<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sekvestr.xml,v 1.14 2024/03/24 20:00:00 revo Exp $">
<kap>
  <rad>sekvestr</rad>/i <fnt><bib>PIV1</bib></fnt>,
  <var><kap><rad var="ac">sekvestraci</rad>/i</kap></var>,
  <var><kap><rad var="k">sekestr</rad>/i <fnt><bib>PV</bib></fnt></kap></var>
</kap>
<drv mrk="sekvestr.0i">
  <kap><tld/>i, <var><kap><tld var="k"/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sekvestr.0i.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Konfidi al administra <ref tip="vid" cel="gard.0i">gardo</ref> 
      atendante donon a&ubreve; redonon al la la&ubreve;le&gcirc;a posedanto:
      <ekz>
        oni devas vin kuratori kaj <tld/>i la kapitalon &gcirc;is via
        plena&gcirc;i&gcirc;o
        <fnt><aut>A. Grin, trad. J. Finkel</aut>, <vrk><url ref="http://www.esperanto.mv.ru/Grin/ora.html">La ora 
        &ccirc;eno</url></vrk>, <lok>2009</lok></fnt>;
      </ekz>
      <ekz>
        polico <tld var="k"/>is kaj fermis la studion
        <klr>[&#x2026;]</klr> pro iu nebula afero
        <fnt>
          <aut>F. Martorell, M. Eaton</aut>,
          <vrk>Esperanto Desperado sekestrita</vrk>,
          <lok><url ref="http://artista.ikso.net/eurokka/novajx/junio04_3.php">
          Informletero de Vinilkosmo, 2004-06 (7)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        jen, vi timas nenion &mdash; vian cerbon neniu povas <tld var="k"/>i
        <fnt>
          <aut>Georgi Stamatov, trad. Ivan H. Krestanoff</aut>
          <vrk><url ref="https://eo.wikisource.org/wiki/Nuntempaj_rakontoj/Du_talentoj">Du
          talentoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la komunista re&gcirc;imo estis <tld/>inta &gcirc;is la jaro 1948 entute 43&#x202F;816&#x202F;682 orajn
        monerojn
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2007/009237.php">Kien la oro?</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        nur ju&gcirc;istoj, kun la subteno de la &scirc;tata prokurorejo, povas kontra&ubreve;konstitucie
        <tld/>i &jcirc;urnalojn kaj vendi &ccirc;iujn &gcirc;iajn proprietojn dum la enketado, sen
        atendo de verdikto
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2010/010065.php">Maljusta justico</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni tuttage en&scirc;losis la na&ubreve; &ccirc;eestantajn episkopojn, <tld/>is iliajn
        po&scirc;telefonojn, komputilojn kaj agendojn
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="&Monato;/2010/010119.php">&Ccirc;u viktimoj de justico?</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="konfis.0i">konfiski</ref>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_k;&c_v;&c_e;&c_s;&c_t;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">segrest<klr> (dret)</klr></trd>
  <trdgrp lng="cs">
    <trd>d&aacute;t do nucen&eacute; spr&aacute;vy</trd>,
    <trd>obstavit</trd>,
    <trd>sekvestrovat</trd>,
    <trd>zabavit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>unter <ind>Zwangsverwaltung</ind> stellen</trd>,
    <trd>einziehen <klr>(entziehen)</klr></trd>,
    <trd>entziehen</trd>
  </trdgrp>
  <trd lng="en">sequester</trd>
  <trd lng="fr">mettre sous <ind>s&eacute;questre</ind></trd>
  <trdgrp lng="hu">
    <trd>lefoglal<klr tip="ind"> (z&aacute;rol)</klr></trd>,
    <trd>z&aacute;r al&aacute; vesz</trd>
  </trdgrp>
  <trd lng="it">sequestrare<klr> (lex)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x4F9B;&#x8A17;&#x3059;&#x308B;  <pr>&#x304D;&#x3087;&#x3046;&#x305F;&#x304F;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5F37;&#x5236;&#x7BA1;&#x7406;&#x3059;&#x308B;  <pr>&#x304D;&#x3087;&#x3046;&#x305B;&#x3044;&#x304B;&#x3093;&#x308A;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x4EEE;&#x51E6;&#x5206;&#x3059;&#x308B;  <pr>&#x304B;&#x308A;&#x3057;&#x3087;&#x3076;&#x3093;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">sekwesteren</trd>
  <trd lng="pl">sekwestrowa&cacute;</trd>
  <trd lng="pt">seq&uuml;estrar</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_k;&c_v;&c_je;&c_s;&c_t;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_mol; <ind>&c_s;&c_je;&c_k;&c_v;&c_je;&c_s;&c_t;&c_r;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>sekvestrova&tcaron;</trd>,
    <trd>s&uacute;dne zhaba&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_je;&#x43A;&c_v;&c_je;&c_s;&c_t;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43D;&c_a;&#x43A;&#x43B;&c_a;&c_d;&c_a;&c_t;&c_i; &c_s;&c_je;&#x43A;&c_v;&c_je;&c_s;&c_t;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x67E5;&#x5C01; <pr>ch&#xE1;f&#x113;ng</pr></trd>,
    <trd>&#x5C01;&#x5B58; <pr>f&#x113;ngc&#xFA;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sekvestr.0o">
  <kap><tld/>o, <var><kap>sekvestracio <fnt><bib>BdV</bib></fnt>
  <fnt><bib>PV</bib></fnt></kap></var></kap>
  <snc mrk="sekvestr.0o.JUR">
    <dif>
      La&ubreve;le&gcirc;a konfido de redonota hava&jcirc;o:
      <ekz>
        levi la ju&gcirc;istan <tld/>on pezantan sur liaj 
        proprieta&jcirc;oj
        <fnt>
          <aut>F. X. Puig Rovira</aut>,
          <vrk>Francesc Galceran Ferrer...</vrk>,
          <lok><url ref="http://www.esperanto.cat/ke/ke09-0304w.pdf">
          Kataluna Esperantisto, 2009-07 &gcirc;is 12</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        tera propra&jcirc;o staranta sub <tld var="ac"/>o
        <fnt>
          <aut>C. Dickens, trad. L. L. Zamenhof</aut>,
          <vrk><url ref="&Gutenberg;files/24501/24501-h/24501-h.htm">
          La Batalo de l' Vivo</url></vrk>,
          <lok>1891</lok>
        </fnt>.
      </ekz>  
    </dif>
    <refgrp tip="vid">
       <ref cel="regi.0o">regio</ref>,
       <ref cel="konfis.0o">konfisko</ref>,
       <ref cel="kurato.0o">kuratoro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_k;&c_v;&c_e;&c_s;&c_t;&c_a;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>nucen&aacute; spr&aacute;va</trd>,
    <trd>obst&aacute;vka</trd>,
    <trd>sekvestr</trd>,
    <trd>sekvestrace</trd>,
    <trd>sekvestura</trd>,
    <trd>zabaven&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Zwangsverwaltung</trd>,
    <trd>Entzug</trd>
  </trdgrp>
  <trd lng="en">sequester</trd>
  <trd lng="fr">s&eacute;questre</trd>
  <trd lng="ja">&#x4EEE;&#x51E6;&#x5206;&#x547D;&#x4EE4;  <pr>&#x304B;&#x308A;&#x3057;&#x3087;&#x3076;&#x3093;&#x3081;&#x3044;&#x308C;&#x3044;</pr></trd>
  <trdgrp lng="sk">
    <trd>n&uacute;ten&aacute; spr&aacute;va</trd>,
    <trd>sekvestr&aacute;cia</trd>,
    <trd>zhabanie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_je;&#x43A;&c_v;&c_je;&c_s;&c_t;&c_r;</trd>,
    <trd>&c_a;&c_r;&c_je;&c_sh;&c_t; &#x43C;&c_a;&c_j;&#x43D;&c_a;</trd>
  </trdgrp>
</drv>
</art>

