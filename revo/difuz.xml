<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>difuz</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="difuz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Ccirc;iudirekten disvasti&gcirc;i.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>diffundieren <klr>(W&auml;rme)</klr></trd>,
    <trd>zerstreuen <klr>(Teilchen)</klr></trd>,
    <trd>streuen <klr>(Licht)</klr></trd>
  </trdgrp>
  <trd lng="fr">diffuser<klr tip="ind"> (se r&eacute;pandre)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_s;&c_je;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_d;&c_i;&c_f;&c_f;&c_u;&c_n;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="difuz.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Fenomeno en kiu gaso, lumo, varmo disi&gcirc;as al &ccirc;iuj direktoj.
    </dif>
  </snc>
  <trd lng="fr">diffusion<klr tip="ind"> (ph&eacute;nom&egrave;ne physique)</klr></trd>
  <trdgrp lng="de">
    <trd>Diffusion</trd>,
    <trd>Zerstreuung</trd>,
    <trd>Streuung</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_s;&c_je;&c_ja;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_d;&c_i;&c_f;&c_f;&c_u;&c_z;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="difuz.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Aparato (ajuto a&ubreve; lenso) por difuzi.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Diffusor</trd>,
    <trd>Zerstreuungsvorrichtung</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_i;&c_f;&c_f;&c_u;&c_z;&c_o;&c_r;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_s;&c_je;&c_i;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
