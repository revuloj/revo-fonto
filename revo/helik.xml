<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>helik</rad>/o
</kap>
<drv mrk="helik.0o">
  <kap><tld/>o</kap>
  <snc mrk="helik.0o.konkhava">
    <dif>
      Gastropodo havanta firman konkon.
    </dif>
    <ref tip="vid" cel="limak.0o.gastropodo">limako</ref>
    <trdgrp lng="de">
      <trd>Schnecke</trd>,
      <trd>Geh&auml;useschnecke</trd>
    </trdgrp>
    <trd lng="fr">escargot</trd>
    <trd lng="it">lumaca</trd>
    <trdgrp lng="nl">
      <trd>slak<klr tip="ind"> (huisjes-)</klr></trd>,
      <trd>huisjesslak</trd>
    </trdgrp>
    <trd lng="pl">&sacute;limak</trd>
   <trd lng="ru">&#x443;&#x43b;&#x438;&#x442;&#x43a;&#x430;</trd>
  </snc>
  <snc mrk="helik.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el la familio <ref tip="super" cel="helike.0oj"><tld/>edoj</ref>
      (<trd lng="la">Helix</trd>).
    </dif>
    <bld lok="../bld/helik.jpg">
      <trd lng="la">Helix pomatia</trd>
    </bld>
    <trd lng="de">Weinbergschnecke</trd>
  </snc>
  <snc mrk="helik.0o.ascii">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Neformala nomo de la signo @ uzata en programlingvoj kaj retadresoj.
    </dif>
    <ref tip="malprt" cel="aski.0o">Askio</ref>
    <ref tip="sin" cel="po.0-signo">po-signo</ref>
    <trd lng="de">Klammeraffe</trd>
    <trdgrp lng="en">
      <trd>at sign</trd>,
      <trd>snail</trd>
    </trdgrp>
    <trd lng="fr">arobace</trd>
    <trdgrp lng="it">
      <trd>chiocciola,</trd>
      <trd>chiocciolina</trd>
    </trdgrp>
    <trd lng="nl">apenstaart</trd>
    <trdgrp lng="ru">
      <trd>&#x443;&#x43b;&#x438;&#x442;&#x43a;&#x430;</trd>,
      <trd>&#x441;&#x43e;&#x431;&#x430;&#x43a;&#x430;</trd>,
      <trd>&#x43a;&#x43e;&#x440;&#x44e;&#x447;&#x43a;&#x430;</trd>
    </trdgrp>
  </snc>
</drv>

</art>
