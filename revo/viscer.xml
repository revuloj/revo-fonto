<art mrk="$Id: viscer.xml,v 1.16 2024/05/19 11:18:32 revo Exp $">
<kap>
  <rad>viscer</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="viscer.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">ANA</uzo>
    <dif>
      <ref tip="vid" cel="intern.0ajxo">Interna&jcirc;o</ref>, ena grava
      organo de <ref tip="malprt" cel="korp.0o.ANA">korpo</ref>:
      <ekz>
        <ind><tld/>a</ind> lej&scirc;maniozo
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2007/009243.php">Epidemiaj sekvoj en E&ubreve;ropo</url></vrk>
          <lok>2007</lok>
        </fnt>;
        <trd lng="de">viszeral</trd>
      </ekz>
      <ekz>
        Jado sentis malkomforton en la membroj kaj la <tld/>oj
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 73a, volumo 2a, p.
        532a</lok></fnt>;
      </ekz>
      <ekz>
        indi&gcirc;enoj de la tribo &leftquot;<nom>Fore</nom>&rightquot;, <klr>[&#x2026;]</klr> man&gcirc;adis pro ritaj celoj la cerbon
        kaj <tld/>ojn de mortintaj parencoj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2006/008861.php">Nova etapo en biologio kaj medicino</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_u;&c_t;&c_r;&c_a;&c_n;&c_y; <ind>&c_v;&c_o;&c_r;&c_g;&c_a;&c_n;</ind></trd>
  <trd lng="ca">v&iacute;scera</trd>
  <trdgrp lng="cs">
    <trd>vnit&rcaron;nosti</trd>,
    <trd>&uacute;troby</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>entrailles</trd>,
    <trd>visc&egrave;re</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>zsiger</trd>,
    <trd>bels&odblac;s&eacute;g</trd>,
    <trd>bels&odblac; szerv</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>interiora</trd>,
    <trd>viscere</trd>,
    <trd>budello<klr> (per estens.)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x5185;&#x81D3;  <pr>&#x306A;&#x3044;&#x305E;&#x3046;</pr></trd>
  <trdgrp lng="pl">
    <trd>trzewia</trd>,
    <trd>wn&eogonek;trzno&sacute;ci</trd>,
  </trdgrp>
  <trd lng="pt">v&iacute;scera</trd>
  <trd lng="ru">&c_v;&c_n;&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;<klr> (&c_v;&c_n;&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_i;&c_je;
  &c_o;&c_r;&c_g;&c_a;&c_n;&c_y;)</klr></trd>
  <trdgrp lng="sk">
    <trd>vn&uacute;tornosti</trd>,
    <trd>vn&uacute;torn&eacute; org&aacute;ny</trd>,
    <trd>&uacute;troby</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_n;&c_u;&c_t;&c_r;&c_ib;&c_sh;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_n;&c_u;&c_t;&c_r;&c_ib;&c_sh;&c_n;&c_ib;&c_j; &c_o;&c_r;&c_g;&c_a;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x810F; <pr>z&#xE0;ng</pr></trd>,
    <trd>&#x81DF; <pr>z&#xE0;ng</pr></trd>,
    <trd>&#x8151; <pr>f&#x1D4;</pr></trd>,
    <trd>&#x5167;&#x81DF; <pr>n&#xE8;iz&#xE0;ng</pr></trd>,
    <trd>&#x5185;&#x810F; <pr>n&#xE8;iz&#xE0;ng</pr></trd>
  </trdgrp>
</drv>
</art>
