  <rad>viscer</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="viscer.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">ANA</uzo>
    <dif>
      <ref tip="vid" cel="intern.0ajxo">Interna&jcirc;o</ref>, ena grava
      organo
      de <ref tip="malprt" cel="korp.0o.ANA">korpo</ref>:
      <ekz>
        Jado sentis malkomforton en la membroj kaj la <tld/>oj
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 73a, volumo 2a, p.
        532a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">v&iacute;scera</trd>
  <trdgrp lng="cs">
    <trd>vnit&#x159;nosti</trd>,
    <trd>&#xFA;troby</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>entrailles</trd>,
    <trd>visc&egrave;re</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>zsiger</trd>,
    <trd>bels&odblac;s&eacute;g</trd>,
    <trd>bels&odblac; szerv</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>interiora</trd>,
    <trd>viscere</trd>,
    <trd>budello<klr> (per estens.)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x5185;&#x81D3;  <pr>&#x306A;&#x3044;&#x305E;&#x3046;</pr></trd>
  <trdgrp lng="pl">                                        
    <trd>trzewia</trd>,                       
    <trd>wn&eogonek;trzno&sacute;ci</trd>,                                   
  </trdgrp>
  <trd lng="pt">v&iacute;scera</trd>
  <trd lng="ru">&c_v;&c_n;&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;<klr> (&c_v;&c_n;&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_i;&c_je;
  &c_o;&c_r;&c_g;&c_a;&c_n;&c_y;)</klr></trd>
  <trdgrp lng="sk">
    <trd>vn&#xFA;tornosti</trd>,
    <trd>vn&#xFA;torn&#xE9; org&#xE1;ny</trd>,
    <trd>&#xFA;troby</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x43D;&#x443;&#x442;&#x440;&#x456;&#x448;&#x43D;&#x456;&#x441;&#x442;&#x44C;</trd>,
    <trd>&#x432;&#x43D;&#x443;&#x442;&#x440;&#x456;&#x448;&#x43D;&#x456;&#x439; &#x43E;&#x440;&#x433;&#x430;&#x43D;</trd>
  </trdgrp>
