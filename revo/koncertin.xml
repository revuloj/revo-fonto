<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: koncertin.xml,v 1.9 2023/12/03 09:58:54 revo Exp $">
<kap>
  <rad>koncertin</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="koncertin.0o">
  <kap><tld/>o</kap>
  <snc mrk="koncertin.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="lst" lst="voko:blovinstrumentoj" cel="harmon2.tir0o">Tirharmoniko</ref>, sen akordoj,
      kun kvar, ses a&ubreve; okangulaj ekstremoj; la angla <tld/>o estas
      patentita en
      1844 de <nom>Charles Wheatstone</nom>: 
      <ekz>
        mi amas ludi akordionojn kaj <tld/>ojn <klr>[&#x2026;]</klr> kiel
        diletanto
        <fnt>
          <aut>Katou T.</aut>,
          <vrk><url ref="http://www.geocities.jp/cato1963/esperanto.html">
          [hejmpa&gcirc;o]</url></vrk>,
          <lok>2001-12</lok>
        </fnt>;
      </ekz>
      <ekz>
        li scipovas ludi anka&ubreve; germanan kaj rusan tirharmonikojn,
        bandonion, <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2003/008095.html">La
          segilo sonas!</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <klr>[&#x2026;]</klr> melodion mi e&ccirc; lernis ludi je la <tld/>o
        <fnt>
          <aut>M. K. Gandhi, trad. A Giridhar Rao</aut>,
          <vrk>Eksperimentoj kun la Vero...</vrk>,
          <lok>2001-2006,
          <url ref="http://www.geocities.com/agrao99/gandhi/eksperimentoj/unua/infanagxo.htm">
          1. 2. Infana&gcirc;o</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Concertina</trd>,
      <trd>Konzertina</trd>
    </trdgrp>
    <trd lng="zh">&#x5C0F;&#x516D;&#x89D2;&#x624B;&#x98A8;&#x7434; <pr>xi&#x1CE;oli&#xF9;ji&#x1CE;osh&#x1D2;uf&#x113;ngq&#xED;n</pr></trd>
</snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_c;&c_e;&c_r;&c_t;&c_y;&c_n;&c_a;</trd>
  <trd lng="en">concertina<klr> (accordion)</klr></trd>
  <trd lng="fr">concertina</trd>
  <trd lng="nl">concertina</trd>
  <trd lng="pl">koncertyna</trd>
  <trd lng="pt">concertina</trd>     
  <trd lng="ro">concertin&abreve;</trd>
</drv>
</art>

