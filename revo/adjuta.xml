<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>adjutant</rad>/o
</kap>
<drv mrk="adjuta.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="oficir.0o.MIL">Oficiro</ref>, la&ubreve;ofice 
      akompananta sian &ccirc;efon kaj helpanta lin en la 
      komandado:
      <ekz>
        <tld/>oj kun senceremoniaj, sed servutaj manieroj
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 1a, p. 6a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      Ne konfuzu kun:
      <ref cel="adjuda.0o">adjudanto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_d;'&c_ju;&c_t;&c_a;&c_n;&c_t;</trd>
  <trd lng="de">Adjutant</trd>
  <trdgrp lng="en">
    <trd>adjutant</trd>,
    <trd>aide-de-camp</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>aide-de-camp</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>adjut&aacute;ns</trd>,
    <trd>seg&eacute;dtiszt</trd>
  </trdgrp>
  <trd lng="nl">adjudant</trd>
  <trd lng="pt">ajudante-de-ordens</trd>
  <trd lng="ru">&c_a;&c_d;&c_malmol;&c_ju;&c_t;&c_a;&c_n;&c_t;</trd>
  <trd lng="sk">pobo&ccaron;n&iacute;k</trd>
</drv>
</art>
