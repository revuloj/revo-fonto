<vortaro>
  <art mrk="$Id: otav.xml,v 1.7 2024/01/30 16:09:26 afido Exp $">
    <kap>
      <rad>Otav</rad>/o
    </kap>

<drv mrk="otav.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc mrk="otav.0o.cxefurbo">
    <dif>
      <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&Ccirc;efurbo</ref> de 
      <ref tip="malprt" cel="kanad.K0o">Kanado</ref> &ccirc;e la samnoma 
      rivero en la orienta angulo de la provinco <ref tip="malprt" cel="ontari.O0o">Ontario</ref>:
      <ekz>
        la Universitato de <tld/>o estas la plej grava dulingva universitato (angla kaj
        franca) de Nordameriko
        <fnt>
          <vrk><url ref="&Viki;/Universitato_de_Otavo">Universitato de Otavo - Vikipedio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        videa ponto kun kvin kanadaj urboj: Halifakso, Londono, <tld/>o, Toronto kaj
        Montrealo
        <fnt>
          <vrk><url ref="https://www.youtube.com/watch?v=j27dQBc5xM4">Zamenhof-festo en Montrealo 2014</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_A;&c_t;&c_a;&c_v;&c_a;</trd>
    <trd lng="de">Ottawa</trd>
    <trd lng="en">Ottawa</trd>
    <trd lng="fr">Ottawa</trd>
    <trd lng="pl">Ottawa</trd>
    <trd lng="ro">Ottawa</trd>
    <trdgrp lng="zh">
    <trd>&#x6E25;&#x592A;&#x534E; <pr>W&#xF2;t&#xE0;ihu&#xE1;</pr></trd>,
    <trd>&#x6E25;&#x592A;&#x83EF; <pr>W&#xF2;t&#xE0;ihu&#xE1;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="ja">&#x30AA;&#x30BF;&#x30EF;</trd>
  <trd lng="uk">&#x41E;&#x442;&#x442;&#x430;&#x432;&#x430; <klr>(&c_s;&#x442;&#x43E;&#x43B;&#x438;&#x446;&#x44F;)</klr></trd>
</drv>

