<drv mrk="blu.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="blu.0a.KOL">
    <dif>
      &Ccirc;ielkolora, lazura:
      <ekz>
        la &ccirc;ielo estas <tld/>a
        <fnt><bib>F</bib>, <lok>ekzercaro, &para; 6</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a kiel cejano, kobalto, safiro, turkiso;
      </ekz>
      <ekz>
        <tld/>a, <ref cel="purpur.0o">purpura</ref>, kaj ru&gcirc;a teksa&jcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 28:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a somer&ccirc;ielo
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="https://www.monato.be/2005/008666.php">Folklora festivalo en Kaustinen</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a marakvo
        <klr>(spegulanta la &ccirc;ielon)</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="https://www.monato.be/2004/008097.html">Adriatika marbordo</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen estas mia anta&ubreve;tuko kun la <tld/>aj &ccirc;eloj
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ku&scirc;is sur io malmola, kaj mia korpo pro tio fari&gcirc;is <tld/>a
        kaj <ref cel="brun.0a">bruna</ref>
        <fnt>
          <bib>Fab1</bib>
          <lok>Re&gcirc;idino sur pizo</lok>
        </fnt>!
      </ekz>
      <ekz>
        disputo pri la vivdona <tld/>a &leftquot;oro&rightquot; minacas &ccirc;efe pro tio, ke duono de la homaro
        <klr>[&#x2026;]</klr> devas dividi akvejojn kun siaj najbaroj
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006866.html">La blua &leftquot;oro&rightquot; &ccirc;iam malsufi&ccirc;os</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        tipaj litovinoj estas <tld/>okulaj blondulinoj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007142.html">In-forme</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <ref cel="alg.blu0o">blualgo</ref>;
      </ekz>
      <ekz>
        en iliaj okuloj ankora&ubreve; brilis la <tld/>eco de la maro
        <fnt>
          <bib>Monato</bib>
          <lok><url ref="http://steloj.de/esperanto/monato2/007050.html">http://steloj.de/esperanto/monato2/007050.html</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        UN retiris siajn <tld/>kaskulojn
        <fnt>
          <bib>Monato</bib>
          <lok><url ref="http://steloj.de/esperanto/monato2/006837.html">http://steloj.de/esperanto/monato2/006837.html</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        estas problemo pri kaj pro la nuna <tld/>-nigra registaro
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007216.html">Universitatoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="af">blou</trd>
  <trd lng="am">&#x1230;&#x121B;&#x12EB;&#x12CA;</trd>
  <trd lng="ar">&#x627;&#x644;&#x623;&#x632;&#x631;&#x642;</trd>
