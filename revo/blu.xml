<drv mrk="blu.0o">
  <kap><tld/>o</kap>
  <snc mrk="blu.0o.KOL">
    <dif>
      <tld lit="B"/>a <ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">koloro</ref>
      a&ubreve; kolorilo:
      <ekz>
        prusa <tld/>o, marista <tld/>o;
      </ekz>
      <ekz>
        mi ekvidis malhelan <tld/>on de &ccirc;ielo
        <fnt>
          <bib>FdO</bib>
        </fnt>;
      </ekz>
      <ekz>
        maro da <tld/>o, da helo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        foriras lante mi sur remboato kiel nubet' senzorga trans la <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2007/009010.php">Resume de l' mio</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_ib;&c_n;&c_mol;</trd>,
      <trd>&c_b;&c_l;&c_a;&c_k;&c_ib;&c_t;</trd>
    </trdgrp>
    <trd lng="br">glaz<klr tip="ind"> (liv)</klr></trd>
    <trd lng="ca">blau <klr>(subst.)</klr></trd>
    <trd lng="de">Blau</trd>
    <trd lng="fr">bleu<klr tip="ind"> (couleur)</klr></trd>
    <trd lng="ku">&scedil;&icirc;n</trd>
    <trd lng="nl">blauw<klr tip="ind"> (bn.)</klr></trd>
    <trdgrp lng="pl">
      <trd>b&lstroke;&eogonek;kit<klr> (jasny niebieski)</klr></trd>,
      <trd>granat<klr> (ciemny niebieski)</klr></trd>,
      <trd>turkus<klr> (zielono-niebieski)</klr></trd>
    </trdgrp>
    <trd lng="ro">albastru</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_i;&c_n;&c_je;&c_v;&c_a;</trd>,
      <trd>&c_s;&c_i;&c_n;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="en">blue</trd>
  <trd lng="he">&kaf;&#1468;&#1464;&het;&vav;&#1465;&lamed;</trd>
  <trdgrp lng="ja">
    <trd>&#38738;  <pr>&#12354;&#12362;</pr></trd>,
    <trd>&#38738;&#33394;  <pr>&#12354;&#12362;&#12356;&#12429;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&c_n;&c_mol;</trd>,
    <trd>&c_b;&c_l;&c_a;&c_k;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_g;&c_o;&c_l;&c_u;&c_b;&c_ib;&c_n;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="blu.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="blu.0ajxo.KOMUNE">
    <dif>
      Io <tld/>a, <tld/>a makulo, <tld/>a parto:
      <ekz>
        <tld/>a&jcirc;o &ccirc;iela enua nubi&gcirc;as
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        en brilanta <tld/>a&jcirc;o de spaco
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_ib;&c_n;&c_mol;</trd>
    <trd lng="ca">quelcom blau</trd>
    <trd lng="en">blue</trd>
    <trd lng="fr">chose <ind>bleue</ind></trd>
    <trd lng="nl">iets <ind>blauw</ind>s</trd>
    <trdgrp lng="pl">
      <trd>siniak</trd>,
      <trd>sinienie</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_i;&c_n;&c_je;&c_v;&c_a;</trd>,
      <trd>&c_s;&c_i;&c_n;&c_ja;&c_k;</trd>
    </trdgrp>
  </snc>
  <snc mrk="blu.0ajxo.MED">
    <uzo tip="stl">KOMUNE</uzo>
    <uzo tip="fak">MED</uzo>
    <dif>
      <tld lit="B"/>i&gcirc;inta <ref tip="vid" cel="makul.bat0o.MED">batmakulo</ref>,
      <ref tip="dif" cel="livid.0o.MED">livido</ref>:
      <ekz>
        li, pro ordo, al &ccirc;iu batas <tld/>a&jcirc;ojn sub la okuloj
        <fnt><bib>Revizoro</bib>, <lok>akto 1a, sceno 5a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="super">
       <ref cel="sang.0umo">sangumo</ref>,
       <ref cel="ehximoz.0o">e&hcirc;imozo</ref>
    </refgrp>
    <trd lng="be">&c_s;&c_ib;&c_n;&c_ja;&c_k;</trd>
    <trdgrp lng="ca">
      <trd>blau</trd>,
      <trd>morat</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Bluterguss</trd>,
      <trd>Blaues Auge <klr>(u.&auml;.)</klr></trd>
    </trdgrp>
    <trd lng="en">bruise</trd>
    <trd lng="fr">bleu<klr tip="ind"> (h&eacute;matome)</klr></trd>
    <trd lng="id">lebam</trd>
    <trdgrp lng="nl">
      <trd><ind>blauw</ind>e vlek</trd>,
      <trd>hematoom</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="ja">
    <trd>&#38738;&#33394;&#12398;&#29289;  <pr>&#12354;&#12362;&#12356;&#12429;&#12398;&#12406;&#12388;</pr></trd>,
    <trd>&#38738;&#12354;&#12374;  <pr>&#12354;&#12362;&#12354;&#12374;</pr></trd>,
    <trd>&#25171;&#12385;&#36523;  <pr>&#12358;&#12385;&#12415;</pr></trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_i;&c_n;&c_je;&c_c;&c_mol;</trd>
</drv>

<drv mrk="blu.cxiel0a">
  <kap>
    &ccirc;iel<tld/>a
  </kap>
  <snc>
    <dif>
      <tld/>a kiel la sennuba &ccirc;ielo, precipe en la tago:
      <ekz>
        du &ccirc;iel<tld/>aj birdoj kantis
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> parolis pri &scirc;iaj &ccirc;iel<tld/>aj okuloj
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 1</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="lazur.0a">lazura</ref>
  </snc>
  <trd
  lng="be">&c_n;&c_ja;&c_b;&c_je;&c_s;&c_n;&c_a;-&c_b;&c_l;&c_a;&c_k;&c_ib;&c_t;&c_n;&c_y;</trd>
  <trd lng="ca">blau cel</trd>
  <trd lng="de">himmelblau</trd>
  <trd lng="en">sky <ind>blue</ind></trd>
  <trd lng="fr">bleu ciel</trd>
  <trd lng="ja">&#31354;&#33394;&#12398;  <pr>&#12381;&#12425;&#12356;&#12429;&#12398;</pr></trd>
  <trdgrp lng="pl">
    <trd>b&lstroke;&eogonek;kitny</trd>,
    <trd>lazurowy</trd>
  </trdgrp>
</drv>

<drv mrk="blu.hel0a">
  <kap>
    hel<tld/>a
  </kap>
  <snc>
    <dif>
      <tld/>a kun konsiderinda parto de blanka koloro:
      <ekz>
        li havis malgrandajn hel<tld/>ajn okulojn
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        malproksime levi&gcirc;is montoj <klr>[&#8230;]</klr>,
        &ccirc;iu kun sia kolornuanco, de la
        malhele verda en la proksimo &gcirc;is la
        vualece hel<tld/>a, kiu en fora
        malproksimo kunfandi&gcirc;is kun la koloro de la &ccirc;ielo
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_l;&c_a;&c_k;&c_ib;&c_t;&c_n;&c_y;</trd>
  <trd lng="ca">blau clar</trd>
  <trd lng="de">hellblau</trd>
  <trd lng="en">light <ind>blue</ind></trd>
  <trd lng="fr">bleu clair</trd>
  <trdgrp lng="ja">
    <trd>&#28129;&#38738;&#33394;&#12398;  <pr>&#12354;&#12431;&#12354;&#12362;&#12356;&#12429;&#12398;</pr></trd>,
    <trd>&#27700;&#33394;&#12398;  <pr>&#12415;&#12378;&#12356;&#12429;&#12398;</pr></trd>
  </trdgrp>
  <trd lng="pl">jasnoniebieski</trd>
</drv>
