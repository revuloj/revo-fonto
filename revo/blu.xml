<drv mrk="blu.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="blu.0a.KOL">
    <dif>
      &Ccirc;ielkolora, lazura:
      <ekz>
        la &ccirc;ielo estas <tld/>a
        <fnt><bib>F</bib>, <lok>ekzercaro, &para; 6</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a kiel cejano, kobalto, safiro, turkiso;
      </ekz>
      <ekz>
        <tld/>a, <ref cel="purpur.0o">purpura</ref>, kaj ru&gcirc;a teksa&jcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 28:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a somer&ccirc;ielo
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2005/008666.php">Folklora festivalo en Kaustinen</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a marakvo
        <klr>(spegulanta la &ccirc;ielon)</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2004/008097.html">Adriatika marbordo</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen estas mia anta&ubreve;tuko kun la <tld/>aj &ccirc;eloj
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ku&scirc;is sur io malmola, kaj mia korpo pro tio fari&gcirc;is <tld/>a
        kaj <ref cel="brun.0a">bruna</ref>
        <fnt>
          <bib>Fab1</bib>
          <lok>Re&gcirc;idino sur pizo</lok>
        </fnt>!
      </ekz>
      <ekz>
        disputo pri la vivdona <tld/>a &leftquot;oro&rightquot; minacas &ccirc;efe pro tio, ke duono de la homaro
        <klr>[&#8230;]</klr> devas dividi akvejojn kun siaj najbaroj
        <fnt>
       <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2000/006866.php">La blua &leftquot;oro&#x201D; &ccirc;iam malsufi&ccirc;os</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 20</lok>
        </fnt>;
      </ekz>
      <ekz>
        tipaj litovinoj estas <tld/>okulaj blondulinoj
        <fnt>
          <aut>LAST</aut>
            <vrk><url ref="&Monato;/2001/007142.php">In-forme</url></vrk>
            <bib>Monato</bib>
            <lok>2001/01, p. 9</lok>
          </fnt>;
      </ekz>
      <ekz>
        <ref cel="alg.blu0o">blualgo</ref>;
      </ekz>
      <ekz>
        en iliaj okuloj ankora&ubreve; brilis la <tld/>eco de la maro
        <fnt>
          <aut>Julian Modest</aut>
            <vrk><url ref="&Monato;/2000/007050.php">La kaptilo</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 21</lok>
          </fnt>;
      </ekz>
      <ekz>
        UN retiris siajn <tld/>kaskulojn
        <fnt><aut>Stefan Maul</aut>:
          <vrk><url ref="&Monato;/2000/006837.php">Neglektata kontinento</url></vrk>,
          <bib>Monato</bib>
          <lok>2000/02, p. 5</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        estas problemo pri kaj pro la nuna <tld/>-nigra registaro
        <fnt>
          <aut>Walter Klag</aut>
            <vrk><url ref="&Monato;/2000/007216.php">Kolerigas kotizoj, tentas tendo</url></vrk>
            <bib>Monato</bib>
            <lok>2000/12, p. 8</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
