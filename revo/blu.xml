<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>blu</rad>/a
    </kap>

    <drv mrk="blu.0a">
      <kap><ofc>*</ofc><tld/>a</kap>
      <snc>
        <dif>
          &Ccirc;ielkolora, lazura:
          <ekz>
            <tld/>a kiel cejano, kobalto, safiro, turkiso.
          </ekz>
        </dif>
      </snc>
      <trd lng="cs">modr&yacute;</trd>
      <trd lng="en">blue</trd>
      <trdgrp lng="es">
        <trd>azul</trd>,
        <trd>celeste</trd>
      </trdgrp>
      <trd lng="de">blau</trd>
      <trd lng="fr">bleu <klr tip="ind">(adj.)</klr></trd>
      <trd lng="nl">blauw</trd>
      <trdgrp lng="pl">
        <trd>niebieski</trd>,
        <trd>b&lstroke;&eogonek;kitny</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_s;&c_i;&c_n;&c_i;&c_j;</trd>,
        <trd>&c_g;&c_o;&c_l;&c_u;&c_b;&c_o;&c_j;</trd>
      </trdgrp>
      <trd lng="sv">bl&aring;</trd>
      <trd lng="tr">mavi</trd>
    </drv>

    <drv mrk="blu.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          <tld lit="B"/>a koloro a&ubreve; kolorilo:
          <ekz>
            Prusa <tld/>o, marista <tld/>o.
          </ekz>
        </dif>
        <trd lng="fr">bleu <klr tip="ind">(couleur)</klr></trd>
        <trdgrp lng="ru">
          <trd>&c_s;&c_i;&c_n;&c_je;&c_v;&c_a;</trd>,
          <trd>&c_s;&c_i;&c_n;&c_mol;</trd>
        </trdgrp>
      </snc>
    </drv>

    <drv mrk="blu.0ajxo">
      <kap><tld/>(a&jcirc;)o</kap>
      <snc>
        <dif>
          Io <tld/>a, <tld/>a makulo, <tld/>a parto:
          <ekz>
            bati <tld/>a&jcirc;on al iu<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>a&jcirc;o sub la okulo<fnt>Z</fnt>.
          </ekz>  
        </dif>
        <trdgrp lng="fr">
          <trd>trace bleue</trd>,
          <trd>bleu <klr tip="ind">(h&eacute;matome)</klr></trd></trdgrp> 
        <trdgrp lng="ru">
          <trd>&c_s;&c_i;&c_n;&c_je;&c_v;&c_a;</trd>,
          <trd>&c_s;&c_i;&c_n;&c_ja;&c_k;</trd>
        </trdgrp>
      </snc>
    </drv>

    <drv mrk="flav.B0aRivero">
      <kap>
        <tld lit="B"/>a Rivero<fnt>PIV</fnt>
      </kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
        <dif>
          Granda rivero en la
          <ref tip="malprt" cel="azi.A0o.orienta">Orienta Azio</ref>,
          en <ref tip="malprt" cel="cxin.Cx0ujo">&Ccirc;inujo</ref>,
          la plej longa en <ref tip="malprt" cel="euxrazi.0o">E&ubreve;razio</ref>
          (6300km), fluas orienten, de Tibeto en la
          <ref cel="cxin.OrientaCx0aMaro">Orientan &Ccirc;inan Maron</ref>.
        </dif>
      </snc>
      <trdgrp lng="en">
        <trd>Yangtze Kiang</trd>,
        <trd>Chang Jiang</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_Ja;&c_n;&c_c;&c_z;&c_y;</trd>,
        <trd>&c_G;&c_o;&c_l;&c_u;&c_b;&c_a;&c_ja; &c_r;&c_je;&c_k;&c_a;</trd>
      </trdgrp>
    </drv>

  </art>
