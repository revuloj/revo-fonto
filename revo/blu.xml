<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>blu</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="blu.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="blu.0a.KOL">
    <dif>
      &Ccirc;ielkolora, lazura:
      <ekz>
        la &ccirc;ielo estas <tld/>a
        <fnt><bib>F</bib>, <lok>ekzercaro, &para; 6</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a kiel cejano, kobalto, safiro, turkiso.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_ib;&c_n;&c_ib;</trd>,
    <trd>&c_b;&c_l;&c_a;&c_k;&c_ib;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_i;&c_n;</trd>
  <trd lng="br">glas</trd>
  <trd lng="cs">modr&yacute;</trd>
  <trd lng="en">blue</trd>
  <trdgrp lng="es">
    <trd>azul</trd>,
    <trd>celeste</trd>
  </trdgrp>
  <trd lng="de">blau</trd>
  <trd lng="fr">bleu<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="nl">blauw<klr tip="ind"> (bn.)</klr></trd>
  <trdgrp lng="pl">
    <trd>niebieski</trd>,
    <trd>modry</trd>,
    <trd>b&lstroke;&eogonek;kitny<klr> (jasny niebieski)</klr></trd>,
    <trd>granatowy<klr> (ciemny niebieski)</klr></trd>,
    <trd>turkusowy<klr> (zielono-niebieski)</klr></trd>,
    <trd>siny<klr> (szaro-niebieski)</klr></trd>
  </trdgrp>
  <trd lng="pt">azul</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_i;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_g;&c_o;&c_l;&c_u;&c_b;&c_o;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">modr&yacute;</trd>
  <trd lng="sv">bl&aring;</trd>
  <trd lng="tp">laso</trd>
  <trd lng="tr">mavi</trd>
  <trd lng="vo">yulibik</trd>
</drv>

<drv mrk="blu.0o">
  <kap><tld/>o</kap>
  <snc mrk="blu.0o.KOL">
    <dif>
      <tld lit="B"/>a <ref tip="super" cel="kolor.0o.propre">koloro
      </ref>
      a&ubreve; kolorilo:
      <ekz>
        Prusa <tld/>o, marista <tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_ib;&c_n;&c_mol;</trd>,
      <trd>&c_b;&c_l;&c_a;&c_k;&c_ib;&c_t;</trd>
    </trdgrp>
    <trd lng="br">glaz<klr tip="ind"> (liv)</klr></trd>
    <trd lng="fr">bleu<klr tip="ind"> (couleur)</klr></trd>
    <trd lng="ku">&scedil;&icirc;n</trd>
    <trd lng="nl">blauw<klr tip="ind"> (bn.)</klr></trd>
    <trdgrp lng="pl">
      <trd>b&lstroke;&eogonek;kit<klr> (jasny
      niebieski)</klr></trd>,
      <trd>granat<klr> (ciemny niebieski)</klr></trd>,
      <trd>turkus<klr> (zielono-niebieski)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_i;&c_n;&c_je;&c_v;&c_a;</trd>,
      <trd>&c_s;&c_i;&c_n;&c_mol;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="blu.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="blu.0ajxo.KOMUNE">
    <dif>
      Io <tld/>a, <tld/>a makulo, <tld/>a parto.
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_ib;&c_n;&c_mol;</trd>,
      <trd>&c_s;&c_ib;&c_n;&c_ja;&c_k;</trd>
    </trdgrp>
    <trd lng="en">blue</trd>
    <trd lng="fr">chose <ind>bleue</ind></trd>
    <trd lng="nl">iets <ind>blauw</ind>s</trd>
    <trdgrp lng="pl">
      <trd>siniak</trd>,
      <trd>sinienie</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_i;&c_n;&c_je;&c_v;&c_a;</trd>,
      <trd>&c_s;&c_i;&c_n;&c_ja;&c_k;</trd>
    </trdgrp>
  </snc>
  <snc mrk="blu.0ajxo.MED">
    <uzo tip="stl">KOMUNE</uzo>
    <uzo tip="fak">MED</uzo>
    <dif>
      <tld lit="B"/>i&gcirc;inta <ref tip="vid" 
      cel="makul.bat0o.MED">batmakulo</ref>,
      <ref tip="dif" cel="livid.0o.MED">livido</ref>:
      <ekz>
        li, pro ordo, al &ccirc;iu batas <tld/>a&jcirc;ojn sub la okuloj
        <fnt><bib>Revizoro</bib>, <lok>akto 1a, sceno 5a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="fr">bleu<klr tip="ind"> (h&eacute;matome)</klr></trd><trdgrp
    lng="nl">
      <trd><ind>blauw</ind>e vlek</trd>,
      <trd>hematoom</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="blu.B0aRivero">
  <kap><tld lit="B"/>a Rivero<fnt>PIV</fnt></kap>
  <snc mrk="blu.B0aRivero.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Granda rivero en la
      <ref tip="malprt" cel="azi.OrientaA0o">Orienta Azio</ref>,
      en <ref tip="malprt" cel="cxin.Cx0ujo">&Ccirc;inujo</ref>,
      la plej longa en <ref tip="malprt" cel="euxraz.0o">E&ubreve;razio
      </ref> (6300 km), fluas orienten, de Tibeto en la
      <ref cel="mar.OrientaCxinaM0o">Orientan &Ccirc;inan Maron</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_Ja;&c_n;&c_d;&c_z;&c_y;</trd>,
    <trd>&c_B;&c_l;&c_a;&c_k;&c_ib;&c_t;&c_n;&c_a;&c_ja; &c_r;&c_a;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Jangtsekiang</trd>,
    <trd>Jangtse-Fluss</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Yangtze Kiang</trd>,
    <trd>Chang Jiang</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Chang jiang</trd>,
    <trd>Yang-tseu-kiang</trd>,
    <trd>Yangzi jiang</trd>
  </trdgrp>
  <trd lng="nl">Jangtsekiang</trd>
  <trdgrp lng="ru">
    <trd>&c_Ja;&c_n;&c_c;&c_z;&c_y;</trd>,
    <trd>&c_G;&c_o;&c_l;&c_u;&c_b;&c_a;&c_ja; &c_r;&c_je;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
