<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap><rad>Kameruni</rad>/o</kap>

    <drv mrk="kameruni.0o">
      <kap><tld/>o<fnt><bib>PIV2</bib></fnt></kap>
      <uzo tip="fak">GEOG</uzo>
      <uzo tip="fak">POL</uzo>
      <snc>
        <dif>
          Regno en la ekvatora regiono de la Okcidenta Afriko, kie
          situas la monto
          <ref tip="prt" cel="kamerun.o.monto">Kameruno<sncref/></ref>:
          <ekz>
            Asocio pri Esperanto en <tld/>o
            <fnt><vrk>Jarlibro de UEA</vrk>, <lok>1996, p. 41</lok></fnt>;
          </ekz><ekz>
            reprezentanto de <tld/>o ne povis veni, &ccirc;ar dumvoje li
            estis forkaptita de fremdaj soldatoj
            <fnt>
              <vrk>Solena ina&ubreve;guro</vrk>.
              <lok>Kongresa kuriero,
                <url ref="http://www.uea.org/kongresoj/1996/sonorilo03.html"
                  >n-ro 3</url>, UK-1996, Prago</lok>
            </fnt>;
          </ekz><ekz>
            la projekto <ctl>Lumo por <tld/>o</ctl>, la&ubreve;vice la
            kvara iniciatita de la tutmonda esperantistaro por
            helpi la blindulojn
            <fnt>
              <aut>Jacques Tuinder</aut>: <vrk>Lumo por Kamerunio</vrk>.
              <lok><url ref="http://www.ikue.org/ek/ek1980_10.htm#3"
                  >Espero katolika, 1980:10</url></lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <ref tip="sin" cel="kamerun.o.GEOG">Kameruno<sncref/></ref>
      <trd lng="bg">&c_K;&c_a;&c_m;&c_je;&c_r;&c_u;&c_n;</trd>
      <trd lng="de">Kamerun</trd>
      <trd lng="en">Cameroon</trd>
      <trdgrp lng="pl">
        <trd>Kamerun</trd>,
        <trd>Republika <ind>Kamerunu</ind></trd>
      </trdgrp>
      <trd lng="ru">&c_K;&c_a;&c_m;&c_je;&c_r;&c_u;&c_n;<klr> (&c_s;&c_t;&c_r;&c_a;&c_n;&c_a;)</klr></trd>
    </drv>

    <drv mrk="kameruni.0a">
      <kap><tld lit="k"/>a</kap>
      <snc>
        <dif>
          Rilata al <tld/>o:
          <ekz>
            la 31-an de januaro en <tld lit="k"/>a &ccirc;efurbo Jaundo,
            &ccirc;ina &scirc;tata prezidanto Hu Jintao, kiu faras &scirc;tatan
            viziton en <tld/>o, kondukis serion da aktivadoj
            <fnt>
              <vrk><url
                  ref="http://esperanto.cri.cn/1/2007/02/02/1@53422.htm"
                  >Hu Jintao faras viziton en Jaundo</url></vrk>
            </fnt>;
          </ekz><ekz>
            temas pri <tld lit="k"/>a civitanino,
            Chamberline Nguefack Zedong, elektita al la Senato
            okaze de la lasta Zamenhofa Tago, unua afrika senatano
            <fnt>
              <vrk><url ref="http://www.esperantio.net/index.php?id=313"
                  >Senatanino Chamberline Nguefack en E&ubreve;ropo</url></vrk>
              <lok>HeKo 327 6-C, 7 apr 2007</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_r;&c_u;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
    </drv>

    <drv mrk="kameruni.0ano">
      <kap><tld lit="k"/>ano</kap>
      <snc>
        <dif>
          Civitano a&ubreve; enlo&gcirc;anto de <tld/>o:
          <ekz>
            13 afrikanoj estis invititaj per mono de diversaj
            fonda&jcirc;oj kaj donacantoj: <klr>...</klr> du <tld lit="k"/>anoj,
            unu burundiano  <klr>...</klr>
            <fnt>
              <vrk><url
 ref="http://pagesperso-orange.fr/eric.coffinet/Afrika_Agado.html"
                  >Kiel Naskigis la Afrika Agado de U.E.A.</url></vrk>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_r;&c_u;&c_n;&c_je;&c_c;</trd>
    </drv>

    <drv mrk="kameruni.0anino">
      <kap><tld lit="k"/>anino</kap>
      <snc>
        <dif>
          Civitanino a&ubreve; enlo&gcirc;antino de <tld/>o:
          <ekz>
            <klr>Esperantologian Semajnon</klr> partoprenis 6
            togolandanoj <klr>...</klr> kaj unu <tld lit="k"/>anino
            <fnt>
              <aut>GBEGLO Koffi</aut>:
              <vrk><url
                  ref="http://groups.yahoo.com/group/landa-agado/message/9617"
                  >Unuaj Esperantologoj en Afriko</url></vrk>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_r;&c_u;&c_n;&c_k;&c_a;</trd>
    </drv>

  </art>

