<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: kameruni.xml,v 1.19 2023/12/02 21:20:05 revo Exp $">
    <kap>
      <rad>Kameruni</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="kameruni.K0o">
  <kap><tld/>o</kap>
  <snc mrk="kameruni.K0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="kamerun.K0o.GEOG">Kameruno<sncref/></ref>:
      <ekz>
        Asocio pri Esperanto en <tld/>o
        <fnt><vrk>Jarlibro de UEA</vrk>, <lok>1996, p.
        41</lok></fnt>;
      </ekz><ekz>
        reprezentanto de <tld/>o ne povis veni, &ccirc;ar dumvoje li
        estis forkaptita de fremdaj soldatoj
        <fnt>
          <vrk>Solena ina&ubreve;guro</vrk>.
          <lok>Kongresa kuriero,
            <url ref="&UEA;kongresoj/1996/sonorilo03.html">n-ro 3</url>, UK-1996, Prago</lok>
        </fnt>;
      </ekz><ekz>
        la projekto <ctl>Lumo por <tld/>o</ctl>, la&ubreve;vice la
        kvara iniciatita de la tutmonda esperantistaro por
        helpi la blindulojn
        <fnt>
          <aut>J. Tuinder</aut>: <vrk>Lumo por Kamerunio</vrk>.
          <lok><url ref="http://www.ikue.org/ek/ek1980_10.htm#3">Espero katolika, 1980:10</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_K;&c_a;&c_m;&c_e;&c_r;&c_u;&c_n;</trd>
  <trd lng="bg">&c_K;&c_a;&c_m;&c_je;&c_r;&c_u;&c_n;</trd>
  <trdgrp lng="ca">
    <trd>el <ind>Camerun</ind></trd>,
    <trd>Rep&uacute;blica del <ind>Camerun</ind></trd>
  </trdgrp>
  <trd lng="de">Kamerun</trd>
  <trdgrp lng="en"><trd>Cameroon</trd>, <trd>Republic of
  Cameroon</trd></trdgrp>
  <trd lng="es">Camer&uacute;n</trd>
  <trd lng="fr">Cameroun</trd>
  <trd lng="it">Camerun</trd>
  <trd lng="nl">Kameroen</trd>
  <trdgrp lng="pl">
    <trd>Kamerun</trd>,
    <trd>Republika <ind>Kamerunu</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_K;&c_a;&c_m;&c_je;&c_r;&c_u;&c_n;<klr>
  (&c_s;&c_t;&c_r;&c_a;&c_n;&c_a;)</klr></trd>
  <trd lng="sk">Kamerun</trd>
  <trd lng="cs">Kamerun</trd>
  <trd lng="uk">&#x41A;&#x430;&#x43C;&#x435;&#x440;&#x443;&#x43D;</trd>
  <trd lng="zh">&#x5580;&#x9EA5;&#x9686; <pr>K&#x101;m&#xE0;il&#xF3;ng</pr></trd>
</drv>

<drv mrk="kameruni.0a">
  <kap><tld lit="k"/>a</kap>
  <snc mrk="kameruni.0a.GEOG">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        la 31-an de januaro en <tld lit="k"/>a &ccirc;efurbo Jaundo,
        &ccirc;ina &scirc;tata prezidanto <nom>Hu Jintao</nom>, kiu faras
        &scirc;tatan viziton en <tld/>o, kondukis serion da aktivadoj
        <fnt>
          <vrk><url ref="&CRI;1/2007/02/02/1@53422.htm">Hu Jintao faras viziton en Jaundo</url></vrk>
        </fnt>;
      </ekz><ekz>
        temas pri <tld lit="k"/>a civitanino,
        <nom>Chamberline Nguefack Zedong</nom>, elektita al la Senato
        okaze de la lasta Zamenhofa Tago, unua afrika senatano
        <fnt>
          <vrk><url ref="http://www.esperantio.net/index.php?id=313">Senatanino Chamberline Nguefack en
              E&ubreve;ropo</url></vrk>
          <lok>HeKo 327 6-C, 7 apr 2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_e;&c_r;&c_u;&c_n;&c_s;&c_k;&c_ib;</trd>
  <trd lng="ca">camerun&eacute;s<klr> (adj.)</klr></trd>
  <trd lng="en">Cameroonian</trd>
  <trd lng="fr">camerounais<klr> (adj.)</klr></trd>
  <trdgrp lng="it">
  <trd>camerunese<klr> (agg.)</klr></trd>,
  <trd>camerunense<klr> (agg.)</klr></trd>
  </trdgrp>
  <trd lng="nl">Kameroens</trd>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_r;&c_u;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="kameruni.0ano">
  <kap><tld lit="k"/>ano</kap>
  <snc mrk="kameruni.0ano.GEOG">
    <dif>
      Civitano a&ubreve; lo&gcirc;anto de <tld/>o:
      <ekz>
        13 afrikanoj estis invititaj per mono de diversaj
        fonda&jcirc;oj kaj donacantoj: <klr>[&#x2026;]</klr> du <tld lit="k"/>anoj,
        unu burundiano  <klr>[&#x2026;]</klr>
        <fnt>
          <vrk><url ref="http://pagesperso-orange.fr/eric.coffinet/Afrika_Agado.html">Kiel Naskigis la Afrika Agado de U.E.A.</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_e;&c_r;&c_u;&c_n;&c_je;&c_c;</trd>
  <trd lng="ca">camerun&eacute;s<klr> (subst.)</klr></trd>
  <trd lng="en">Cameroonian</trd>
  <trd lng="fr">camerounais<klr> (subst.)</klr></trd>
  <trdgrp lng="it">
  <trd>camerunese<klr> (sost. m.)</klr></trd>,
  <trd>camerunense<klr> (sost. m.)</klr></trd>
  </trdgrp>
  <trd lng="nl">Kameroener</trd>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_r;&c_u;&c_n;&c_je;&c_c;</trd>
</drv>

<drv mrk="kameruni.0anino">
  <kap><tld lit="k"/>anino</kap>
  <snc mrk="kameruni.0anino.GEOG">
    <dif>
      Civitanino a&ubreve; lo&gcirc;antino de <tld/>o:
      <ekz>
        <klr>Esperantologian Semajnon</klr> partoprenis 6
        togolandanoj <klr>[&#x2026;]</klr> kaj unu <tld lit="k"/>anino
        <fnt>
          <aut>Gbeglo Koffi</aut>:
          <vrk><url ref="http://groups.yahoo.com/group/landa-agado/message/9617">Unuaj Esperantologoj en Afriko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_e;&c_r;&c_u;&c_n;&c_k;&c_a;</trd>
  <trd lng="ca">camerunesa<klr> (subst.)</klr></trd>
  <trd lng="fr">camerounaise<klr> (subst.)</klr></trd>
  <trdgrp lng="it">
  <trd>camerunese<klr> (sost. f.)</klr></trd>,
  <trd>camerunense<klr> (sost. f.)</klr></trd>
  </trdgrp>
  <trd lng="nl">Kameroense</trd>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_r;&c_u;&c_n;&c_k;&c_a;</trd>
</drv>

</art>

