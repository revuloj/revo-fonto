<vortaro>
  <art mrk="$Id: brn.xml,v 1.5 2023/10/29 19:11:03 afido Exp $">
    <kap>
      <rad>Brn</rad>/o
    </kap>

<drv mrk="brn.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="brn.0o.urbo">
    <dif>
      Granda &ccirc;e&hcirc;a <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>, historia centro de
      <ref tip="malprt" cel="moravi.0o">Moravio</ref>, &ccirc;. 190&#x202F;km sudoriente de Prago:
      <ekz>
        vienaj infanoj gastis en Prago, <tld/>o, Bratislavo kaj
        Krakovo, por prezenti al
        tieaj lo&gcirc;antoj kristnaskajn kantojn
        <fnt>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2000/006867.php">Mez-E&ubreve;ropo: Onto-ponto</url></vrk>
              <bib>Monato</bib>
              <lok>2000/05, p. 11</lok>
        </fnt>;
      </ekz>
      <ekz>
        la trajnoj veturos en &ccirc;iuj direktoj, ligante diversajn urbojn: Parizo,
        Stutgarto, Dresdeno, Prago, Gdansko, <tld/>o, Bratislavo
        <fnt>
          <aut>Walter Klag</aut>
          <vrk><url ref="https://www.monato.be/2011/010483.php">Trajnojn trak-tante</url></vrk>
              <bib>Monato</bib>
          <lok>2011/11, p. 17</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Br&uuml;nn</trd>
    <trd lng="en">Brno</trd>
    <trd lng="fr">Brno</trd>
    <trd lng="nl">Brno</trd>
    <trd lng="zh">&#x5E03;&#x5C14;&#x8BFA; <pr>b&#xF9;&#x11B;rnu&#xF2;</pr></trd>
</snc>
  <trd lng="ja">&#x30D6;&#x30EB;&#x30CE;</trd>
</drv>

  </art>

