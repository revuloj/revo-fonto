<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>iridi</rad>/o
</kap>
<drv mrk="iridi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun 
      atomnumero 77, atompezo 192,2, simbolo Ir. Grizeblanka, tre malmola
kaj 
      fortika <ref tip="super" cel="metal.0o">metalo</ref>,
      trovata kun plateno en kelkaj mineraloj, tre malfacile prilaborebla
      kaj fandebla, uzata &ccirc;e produktado de diversaj mezuriloj, por
pintoj
      de &scirc;talaj instrumentoj, ktp.  
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_r;&c_y;&c_d;&c_y;&c_j;</trd>
  <trd lng="de">Iridium</trd>
  <trd lng="es">iridio</trd>
  <trd lng="fr">iridium</trd>
  <trd lng="hu">ir&iacute;dium</trd>
  <trd lng="nl">iridium</trd>
  <trd lng="ru">&c_i;&c_r;&c_i;&c_d;&c_i;&c_j;</trd>
</drv>
</art>
