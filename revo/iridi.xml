<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: iridi.xml,v 1.24 2022/03/21 15:27:49 revo Exp $">
<kap>
  <rad>iridi</rad>/o
</kap>
<drv mrk="iridi.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Ir</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="77">elemento</ref> kun
      atomnumero 77, atompezo 192,2; grize blanka,
      tre malmola kaj fortika <ref tip="super" cel="metal.0o">metalo</ref>,
      trovata kun plateno en kelkaj mineraloj, tre malfacile
      prilaborebla kaj fandebla, uzata &ccirc;e produktado de diversaj 
      mezuriloj, por pintoj de &scirc;talaj instrumentoj, ktp. 
      <ekz>
        <tld/>o estas pli rara ol oro a&ubreve; plateno, en la naturo &gcirc;i trovi&gcirc;as
        <klr>[&#x2026;]</klr> kiel etaj grajnoj sur a&ubreve; <klr>[kun]</klr> plateno, kune kun
        osmio &gcirc;i formas du natura&jcirc;ajn mineralojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=27273">Iridio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_r;&c_y;&c_d;&c_y;&c_j;</trd>
  <trd lng="bg">&c_i;&c_r;&c_i;&c_d;&c_i;&c_j;</trd>
  <trd lng="br">iridiom</trd>
  <trd lng="cs">iridium</trd>
  <trd lng="de">Iridium</trd>
  <trd lng="en">iridium</trd>
  <trd lng="es">iridio</trd>
  <trd lng="fr">iridium</trd>
  <trd lng="hu">ir&iacute;dium</trd>
  <trd lng="id">iridium</trd>
  <trd lng="it">iridio</trd>
  <trd lng="la">Iridium</trd>
  <trd lng="nl">iridium</trd>
  <trd lng="pl">iryd</trd>
  <trd lng="pt">ir&iacute;dio<klr tip="ind"> (qu&iacute;mica)</klr></trd>
  <trd lng="ru">&c_i;&c_r;&c_i;&c_d;&c_i;&c_j;</trd>
  <trd lng="sk">ir&iacute;dium</trd>
  <trd lng="sv">iridium</trd>
  <trd lng="uk">&#x456;&#x440;&#x438;&#x434;&#x456;&#x439;</trd>
</drv>
</art>
