<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>latitud</rad>/o
</kap>
<drv mrk="latitu.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">SCI</uzo>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Distanco de iu loko al la ekvatoro, mezurata per la meridiano
      trapasanta tiun lokon.
    </dif>
  </snc>
  <trd lng="el">&gamma;&epsilon;&omega;&gamma;&rho;&alfa;&phi;&jota;&kappa;&omikron_ton; &pi;&lambda;&alfa_ton;&tau;&omikron;&sigma_fina;</trd>
  <trd lng="en">latitude</trd>
  <trd lng="es">latitud</trd>
  <trd lng="nl">breedte</trd>
  <trd lng="pt">latitude</trd>
  <trd lng="ru">&c_sh;&c_i;&c_r;&c_o;&c_t;&c_a;</trd>
</drv>

<drv mrk="latitu.astronomiaauxgeografia0o">
  <kap>
    astronomia a&ubreve; geografia <tld/>o
  </kap>
  <snc>
    <dif>
      Angulo inter la ekvatora ebeno kaj la normalo je la surfaco &ccirc;e iu
      punkto.
    </dif>
  </snc>
</drv>

<drv mrk="latitu.geodezia0o">
  <kap>
    geodezia <tld/>o
  </kap>
  <snc>
    <dif>
      Astronomia <tld/>o precize difinita per korekto pro loka ekarto de
      la perpendiklilo.
    </dif>
  </snc>
</drv>

<drv mrk="latitu.tercentra0o">
  <kap>
    tercentra <tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="L"/>o inter la ekvatora ebeno kaj la tera radio, kiu tran&ccirc;as la
      lokon.
    </dif>
  </snc>
</drv>

<drv mrk="latitu.cirklode0o">
  <kap>
    cirklo de <tld/>o
  </kap>
  <snc>
    <dif>
      Fikcia geografia cirklo sur la terglobo paralela al la ekvatoro.
    </dif>
    <refgrp tip="vid">
      <ref cel="ekvato.0o">ekvatoro</ref>,
      <ref cel="longit.0o">longitudo</ref>.
    </refgrp>
  </snc>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_a;&c_l;&c_l;&c_je;&c_l;&c_mol;</trd>
</drv>
</art>
