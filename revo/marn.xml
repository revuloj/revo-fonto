<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: marn.xml,v 1.19 2024/01/25 20:10:34 revo Exp $">
<kap>
  <rad>marn</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="marn.0o">
  <kap><tld/>o</kap>
  <snc mrk="marn.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Kalkeca tera&jcirc;o miksita kun <ref tip="prt" cel="argil.0o">argilo</ref>, uzebla
      kiel <ref tip="super" cel="sterk.0o.AGR">sterko</ref>
      por plibonigi grundon:
      <ekz>
        blankaj kaj grizaj <tld/>oj
        <fnt>
          <aut>J. M. Mabesoone</aut>,
          <vrk><url ref="http://www.repository.naturalis.nl/document/549662">
          Tertiary and Quaternary Sedimentation in a Part of the 
          Duero Basin Palencia (Spain)</url></vrk>,
          <lok>[Leidse Geologische Mededelingen, 1959 (24), p. 
          31a-180a]</lok>
        </fnt>;
      </ekz>
      <ekz>
        kalkori&ccirc;a <tld/>o estas uzata por plibonigi grundojn tro acidajn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=33589">Marno</url></lok>
        </fnt>.
      </ekz>      
    </dif>
    <ref tip="vid" cel="leuxs.0o">le&ubreve;so</ref>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_r;&c_g;&c_je;&c_l;&c_mol;</trd>
  <trd lng="ca">marga</trd>
  <trd lng="da">mergel</trd>
  <trd lng="de">Mergel</trd>
  <trd lng="en">marl</trd>
  <trd lng="es">marga</trd>
  <trd lng="et">mergel</trd>
  <trd lng="fa">&a_A_madda;&a_h;&f_k;&a_r;&a_damma;&a_s;</trd>
  <trd lng="fr">marne</trd>
  <trd lng="he">&het;&vav;&vav;&alef;&resh;</trd>
  <trd lng="hr">lapor</trd>
  <trd lng="hu">m&aacute;rga</trd>
  <trd lng="io">marno</trd>
  <trd lng="it">marna</trd>
  <trdgrp lng="ja">
    <trd>&#x6CE5;&#x7070;&#x5CA9;  <pr>&#x3067;&#x3044;&#x304B;&#x3044;&#x304C;&#x3093;</pr></trd>,
    <trd>&#x6CE5;&#x7070;&#x571F;  <pr>&#x3069;&#x308D;&#x306F;&#x3044;&#x3064;&#x3061;</pr></trd>,
    <trd>&#x30DE;&#x30FC;&#x30EB;</trd>
  </trdgrp>
  <trd lng="ko">&#xC774;&#xD68C;&#xD1A0;</trd>
  <trd lng="li">mergel</trd>
  <trd lng="nl">mergel</trd>
  <trd lng="no">mergel</trd>
  <trd lng="pl">margiel</trd>
  <trd lng="pt">marga</trd>
  <trd lng="ro">marn&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_r;&c_g;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_r;&c_u;&c_h;&c_l;&c_ja;&c_k;</trd>
  </trdgrp>
  <trd lng="sv">m&auml;rgel</trd>
  <trd lng="uk">&c_m;&c_je;&c_r;&c_g;&c_je;&c_l;&c_mol;</trd>
  <trdgrp lng="zh">
    <trd>&#x6CE5;&#x7070;&#x571F; <pr>n&#xED;hu&#x12B;t&#x1D4;</pr></trd>,
    <trd>&#x6CE5;&#x7070;&#x5CA9; <pr>n&#xED;hu&#x12B;y&#xE1;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="marn.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="marn.0i.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Plibonigi kampon per la <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_g;&c_n;&c_o;&c_j;&c_v;&c_a;&c_c;&c_mol; <ind>&c_m;&c_je;&c_r;&c_g;&c_je;&c_l;&c_je;&c_m;</ind></trd>
  <trd lng="ca">margar</trd>
  <trd lng="de">mergeln</trd>
  <trd lng="en">marl</trd>
  <trd lng="fr">marner</trd>
  <trd lng="hu">m&aacute;rg&aacute;z</trd>
  <trd lng="ja">&#x6CE5;&#x7070;&#x571F;&#x3092;&#x65BD;&#x3059;  <pr>&#x3069;&#x308D;&#x306F;&#x3044;&#x3064;&#x3061;&#x3092;&#x307B;&#x3069;&#x3053;&#x3059;</pr></trd>
  <trd lng="nl">mergelen</trd>
  <trd lng="pl">marglowa&cacute;</trd>
  <trd lng="ru">&c_u;&c_d;&c_o;&c_b;&c_r;&c_ja;&c_t;&c_mol; &c_m;&c_je;&c_r;&c_g;&c_je;&c_l;&c_je;&c_m;</trd>
  <trd lng="uk">&#x443;&#x434;&#x43E;&#x431;&#x440;&#x44E;&#x432;&#x430;&#x442;&#x438; &#x43C;&#x435;&#x440;&#x491;&#x435;&#x43B;&#x435;&#x43C;</trd>
</drv>
</art>
