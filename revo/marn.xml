<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>marn</rad>/o
</kap>
<drv mrk="marn.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Kalkeca tera&jcirc;o miksita kun argilo, uzata por plibonigi la grundon.
    </dif>
  </snc>
  <trd lng="de">Mergel</trd>
  <trd lng="en">marl</trd>
  <trd lng="nl">mergel</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_r;&c_g;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_r;&c_u;&c_h;&c_l;&c_ja;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="marn.0i">
  <kap>
    <tld/>i
  </kap>
  <uzo tip="fak">AGR</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Plibonigi kampon per la <tld/>o.
    </dif>
  </snc>
  <trd lng="de">mergeln</trd>
  <trd lng="en">marl</trd>
  <trd lng="nl">mergelen</trd>
  <trd lng="ru">&c_u;&c_d;&c_o;&c_b;&c_r;&c_ja;&c_t;&c_mol; &c_m;&c_je;&c_r;&c_g;&c_je;&c_l;&c_je;&c_m;</trd>
</drv>
</art>
