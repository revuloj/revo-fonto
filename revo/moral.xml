<drv mrk="moral.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="moral.0isto.KOMUNE">
    <dif>
      Homo studanta, esploranta, instruanta <tld/>on:
      <ekz>
        estis la unua fojo, ke mi vidis tiun <tld/>iston
        ru&gcirc;i&gcirc;i
        <fnt>
          <aut>Ba Jin, trad. Li Shijun</aut>,
          <vrk>A&ubreve;tuno en la Printempo</vrk>,
          <lok>1980</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_a;&c_l;&c_ib;&c_s;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>moralista</trd>,
    <trd>mravok&aacute;rce</trd>,
    <trd>mravouk</trd>
  </trdgrp>
  <trd lng="fr">moraliste</trd>
  <trdgrp lng="ja">
    <trd>&#x30E2;&#x30E9;&#x30EA;&#x30B9;&#x30C8;</trd>,
    <trd>&#x9053;&#x5FB3;&#x5BB6;  <pr>&#x3069;&#x3046;&#x3068;&#x304F;&#x304B;</pr></trd>
  </trdgrp>
  <trd lng="pl">moralista</trd>
  <trd lng="ro">moralist</trd>
  <trd lng="sk">moralista</trd>
  <trd lng="uk">&#x43C;&#x43E;&c_r;&c_a;&#x43B;&c_ib;&c_s;&c_t;</trd>
</drv>

<drv mrk="moral.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="moral.mal0a.FIL">
    <dif>
      Kontra&ubreve;a al la <tld/>o, devo, virto:
      <ekz>
        mal<tld/>a homo;
      </ekz>
      <ekz>
        mal<tld/>a ago.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_a;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>nemor&aacute;ln&iacute;</trd>,
    <trd>nemravn&yacute;</trd>
  </trdgrp>
  <trd lng="fr">immoral</trd>
  <trdgrp lng="ja">
    <trd>&#x80CC;&#x5FB3;&#x306E;  <pr>&#x305B;&#x3068;&#x304F;&#x306E;</pr></trd>,
    <trd>&#x4E0D;&#x9053;&#x5FB3;&#x306E;  <pr>&#x3075;&#x3069;&#x3046;&#x3068;&#x304F;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>amoralny</trd>,
    <trd>niemoralny</trd>,
    <trd>wyst&eogonek;pny</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>amoral</trd>,
    <trd>imoral</trd>,
    <trd>r&abreve;u</trd>,
    <trd>r&abreve;ut&abreve;cios</trd>
  </trdgrp>   
  <trdgrp lng="ru">
    <trd>&c_a;&c_m;&c_o;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_n;&c_r;&c_a;&c_v;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nemor&aacute;lny</trd>,
    <trd>nemravn&yacute;</trd>
  </trdgrp>
