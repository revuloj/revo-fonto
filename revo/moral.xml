<drv mrk="moral.0o">
  <kap><tld/>o</kap>
  <snc mrk="moral.0o.FIL">
    <dif>
      Parto de la filozofio esploranta la principojn de homa konduto kaj
      instruanta pri la celinda bono kaj pri la devoj;
      <ref tip="sin" cel="etik.0o">etiko</ref>.
    </dif>
    <trd lng="nl">zedenleer</trd>
  </snc>
  <snc mrk="moral.0o.MOR">
    <dif>
      Tuto de la reguloj montrantaj al la homo liajn devojn en socia a&ubreve;
      privata konduto.
    </dif>
    <trd lng="nl">moraal</trd>
  </snc>
  <snc>
    <dif>
      Tuto de la kondutreguloj kaj moroj de iu aparta socio.
    </dif>
    <trd lng="nl">moraal</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_r;&c_a;&c_l;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mor&aacute;lka</trd>,
    <trd>mravnost</trd>,
    <trd>mravouka</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;thique<klr> (subst.)</klr></trd>,
    <trd>morale<klr> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x9053;&#x5FB3;  <pr>&#x3069;&#x3046;&#x3068;&#x304F;</pr></trd>,
    <trd>&#x9053;&#x7FA9;  <pr>&#x3069;&#x3046;&#x304E;</pr></trd>,
    <trd>&#x502B;&#x7406;  <pr>&#x308A;&#x3093;&#x308A;</pr></trd>,
    <trd>&#x30E2;&#x30E9;&#x30EB;  <pr>&#x3082;&#x3089;&#x308B;</pr></trd>,
