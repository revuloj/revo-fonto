  <trdgrp lng="uk">
    <trd>&#x43C;&#x43E;&c_r;&c_a;&#x43B;&#x44C;</trd>,
    <trd>&#x43C;&#x43E;&c_r;&c_a;&#x43B;&#x44C;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4F26;&#x7406; <pr>l&#xFA;nl&#x1D0;</pr></trd>,
    <trd>&#x502B;&#x7406; <pr>l&#xFA;nl&#x1D0;</pr></trd>,
