<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">

<kap>
  <rad>greb</rad>/o
  <fnt>
    <vrk>
      <url ref="http://eo.wikipedia.org/wiki/Greboj">Grebo</url>
    </vrk>,
    <bib>Viki</bib>
 </fnt>
</kap>

<drv mrk="greb.0o">
  <kap><tld/>o</kap>
  <snc mrk="greb.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Birdoj de la ordo Podicipoformaj de diversaj genroj.
    </dif>
    <trd lng="pl">perkoz</trd>
  </snc>
</drv>

<drv mrk="greb.tuf0o">
  <kap>tuf<tld/>o</kap>
  <snc mrk="greb.tuf0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Plej granda specio de <tld/>oj karakterizanta de duobla tufo de la
      kapo.
    </dif>
    <trd lng="la">Podiceps cristatus</trd>
    <trd lng="pl">perkoz dwuczuby</trd>
  </snc>
</drv>

<drv mrk="greb.malgranda0o">
  <kap>malgranda <tld/>o</kap>
  <snc mrk="greb.malgranda0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Plej malgranda specio de <tld/>oj.
    </dif>
    <trd lng="la">Podiceps ruficollis</trd>
    <trd lng="pl">perkozek</trd>
  </snc>
</drv>

</art>

<!--
$Log$
-->
</vortaro>