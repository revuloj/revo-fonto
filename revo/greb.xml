<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">

<kap>
  <rad>greb</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="greb.0o">
  <kap><tld/>o</kap>
  <snc mrk="greb.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="bird.nagxo0o"
      lst="voko:birdoj">Na&gcirc;birdo</ref>
      kun lobecaj piedoj; iu el la ordo podicipoformaj birdoj
      (<trd lng="la">Podicipediformes</trd>):
      <ekz>
        <tld/>oj havas mallar&gcirc;ajn flugilojn kaj kelkaj specioj apena&ubreve;
        flugas, inter ili
        du sudamerikaj specioj e&ccirc; tute ne flugas, se okazas dan&gcirc;ero, ili
        mergi&gcirc;as
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=53453"
              >Greboj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">cabuss&oacute;</trd>
    <trd lng="de">Lappentaucher</trd>
    <trd lng="en">grebe</trd>
    <trd lng="fr">gr&egrave;be</trd>
    <trd lng="nl">fuut<klr tip="ind"> (lid van de orde
    Podicipediformes)</klr></trd>
    <trd lng="pl">perkoz</trd>
  </snc>
  <trd lng="cs">pot&aacute;pka</trd>
  <trd lng="sk">pot&aacute;pka</trd>
</drv>

<drv mrk="greb.tuf0o">
  <kap>tuf<tld/>o</kap>
  <snc mrk="greb.tuf0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Plej granda <ref tip="lst" cel="speci.0o.BIO"
      lst="voko:zoologiaj_specioj">specio</ref> de <tld/>oj
      karakterizata de duobla tufo sur la
      kapo (<trd lng="la">Podiceps cristatus</trd>):
      <ekz>
        la tuf<tld/>o lo&gcirc;as en somero &ccirc;e lagoj, lagetoj, mar&ccirc;oj,
        akvorezervejoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16612"
              >Tufgrebo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/3/34/Great_crested_grebe_(Podiceps_cristatus)_on_nest.jpg/640px-Great_crested_grebe_(Podiceps_cristatus)_on_nest.jpg"
      prm="CC BY-SA 4.0"
      >Tuf<tld/>o (Podiceps cristatus), Oxfordshire
        <fnt>
          <aut>Charles J Sharp, propra verko, sharpphotography</aut>
          <lok><url
          ref="&WCW;/File:Great_crested_grebe_(Podiceps_cristatus)_on_nest.jpg"
              >https://commons.wikimedia.org/wiki/File:Great_crested_grebe_(Podiceps_cristatus)_on_nest.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="ca"><ind>cabuss&oacute;</ind> emplomallat</trd>
  <trdgrp lng="cs">
    <trd>pot&aacute;pka roh&aacute;&ccaron;</trd>,
    <trd>roh&aacute;&ccaron; velk&yacute;</trd>
  </trdgrp>
  <trd lng="de">Haubentaucher</trd>
  <trd lng="en">great crested grebe</trd>
  <trd lng="fr">gr&egrave;be hupp&eacute;</trd>
  <trd lng="ja">&#12459;&#12531;&#12512;&#12522;&#12459;&#12452;&#12484;&#12502;&#12522; [<ind>&#12363;&#12435;&#12416;&#12426;&#12363;&#12356;&#12388;&#12406;&#12426;</ind>]</trd>
  <trd lng="nl">fuut<klr tip="ind"> (diersoort Podiceps
  cristatus)</klr></trd>
  <trd lng="pl">perkoz dwuczuby</trd>
  <trd lng="sk">pot&aacute;pka chochlat&aacute;</trd>
</drv>

<drv mrk="greb.malgranda0o">
  <kap>malgranda <tld/>o</kap>
  <snc mrk="greb.malgranda0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Plej malgranda <ref tip="lst" cel="speci.0o.BIO"
      lst="voko:zoologiaj_specioj">specio</ref> de <tld/>oj
      (<trd lng="la">Tachybaptus ruficollis</trd> sin. <trd
      lng="la">Podiceps ruficollis</trd>):
      <ekz>
        la Malgranda <tld/>o estas timema birdo, &gcirc;i ofte na&gcirc;as inter la
        densa vegeta&jcirc;aro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=56247"
              >Malgranda grebo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/8/84/Little_grebe_Zwergtaucher.jpg/640px-Little_grebe_Zwergtaucher.jpg"
      prm="CC BY-SA 2.5"
      >Malgranda <tld/>o
        <fnt>
          <aut>Andreas Trepte, propra verko</aut>
          <lok><url ref="&WCW;/File:Little_grebe_Zwergtaucher.jpg"
              >https://commons.wikimedia.org/wiki/File:Little_grebe_Zwergtaucher.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="ca">cabusset</trd>
  <trdgrp lng="en">
    <trd>little grebe</trd>,
    <trd>dabchick<klr tip="ind"> (little grebe)</klr></trd>
  </trdgrp>
  <trd lng="de">Zwergtaucher</trd>
  <trd lng="fr">gr&egrave;be castagneux</trd>
  <trd lng="ja">&#12459;&#12452;&#12484;&#12502;&#12522; [<ind>&#12363;&#12356;&#12388;&#12406;&#12426;</ind>]</trd>
  <trd lng="nl">dodaar</trd>
  <trd lng="pl">perkozek</trd>
</drv>
</art>

