<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">

<kap>
  <rad>greb</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="greb.0o">
  <kap><tld/>o</kap>
  <snc mrk="greb.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Birdo de la ordo Podicipoformaj de diversaj genroj.
    </dif>
    <trd lng="fr">gr&egrave;be</trd>
    <trd lng="pl">perkoz</trd>
  </snc>
</drv>

<drv mrk="greb.tuf0o">
  <kap>tuf<tld/>o</kap>
  <snc mrk="greb.tuf0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Plej granda specio de <tld/>oj karakterizata de duobla tufo sur la
      kapo (<trd lng="la">Podiceps cristatus</trd>).
    </dif>
  </snc>
  <trd lng="fr">gr&egrave;be hupp&eacute;</trd>
  <trd lng="pl">perkoz dwuczuby</trd>
</drv>

<drv mrk="greb.malgranda0o">
  <kap>malgranda <tld/>o</kap>
  <snc mrk="greb.malgranda0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Plej malgranda specio de <tld/>oj
      (<trd lng="la">Podiceps ruficollis</trd>).
    </dif>
  </snc>
  <trd lng="fr">gr&egrave;be castagneux</trd>
  <trd lng="pl">perkozek</trd>
</drv>
</art>

