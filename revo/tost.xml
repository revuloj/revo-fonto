<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tost.xml,v 1.25 2024/05/10 10:43:46 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>tost</rad>/o
</kap>
<drv mrk="tost.0o">
  <kap><tld/>o</kap>
  <snc mrk="tost.0o.MOR">
    <dif>
      Dumfestena propono pri trinko por honoro a&ubreve; sukceso de iu
      a&ubreve; io:
      <ekz>
        sen <tld/>oj, sen paroladoj pasis la tagman&gcirc;o, per kiu
        fini&gcirc;is la soleno
        <fnt><aut>Ivan G. &Scirc;irjaev</aut> <vrk>Sen titolo</vrk> 
        <lok>&ccirc;apitro 7a</lok></fnt>. 
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_o;&c_s;&c_t; <klr>(&c_z;&c_a;
  &c_s;&c_t;&c_a;&c_l;&c_o;&c_m;)</klr></trd>
  <trd lng="bg">&c_t;&c_o;&c_s;&c_t;</trd>
  <trd lng="ca">brindis</trd>
  <trd lng="cs">p&rcaron;&iacute;pitek</trd>
  <trdgrp lng="de">
    <trd>Trinkspruch</trd>,
    <trd>Tost</trd>
  </trdgrp>
  <trd lng="en">toast<klr tip="amb"> (speech during drink)</klr></trd>
  <trd lng="es">brindis</trd>
  <trd lng="fr">toast</trd>
  <trdgrp lng="hu">
    <trd>poh&aacute;rk&ouml;sz&ouml;nt&odblac;</trd>,
    <trd>&aacute;ldom&aacute;s</trd>,
    <trd>t&oacute;szt</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4E7E;&#x676F;  <pr>&#x304B;&#x3093;&#x3071;&#x3044;</pr></trd>,
    <trd>&#x795D;&#x676F;  <pr>&#x3057;&#x3085;&#x304F;&#x306F;&#x3044;</pr></trd>,
    <trd>&#x4E7E;&#x676F;&#x306E;&#x97F3;&#x982D;  <pr>&#x304B;&#x3093;&#x3071;&#x3044;&#x306E;&#x304A;&#x3093;&#x3069;</pr></trd>,
    <trd>&#x4E7E;&#x676F;&#x306E;&#x6328;&#x62F6;  <pr>&#x304B;&#x3093;&#x3071;&#x3044;&#x306E;&#x3042;&#x3044;&#x3055;&#x3064;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>toast</trd>,
    <trd>dronk</trd>,
    <trd>heildronk</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>toast</trd>,
    <trd>strzemienny</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>toste</trd>,
    <trd>brinde</trd>,
    <trd>sa&uacute;de</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_o;&c_s;&c_t; <klr tip="amb">(&c_z;&c_a;
  &c_s;&c_t;&c_o;&c_l;&c_o;&c_m;)</klr></trd>
  <trdgrp lng="sk">
    <trd>pr&iacute;pitok</trd>,
    <trd>zdravica</trd>
  </trdgrp>
  <trd lng="uk">&#x442;&#x43E;&#x441;&#x442;</trd>
  <trdgrp lng="zh">
    <trd>&#x795D;&#x9152; <pr>zh&#xF9;ji&#x1D4;</pr></trd>,
    <trd>&#x656C;&#x9152; <pr>j&#xEC;ngji&#x1D4;</pr></trd>,
    <trd>&#x4E7E;&#x676F; <pr>g&#x101;nb&#x113;i</pr></trd>,
    <trd>&#x5E72;&#x676F; <pr>g&#x101;nb&#x113;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="tost.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="tost.0i.MOR">
    <dif>
      Eldiri <tld/>on:
      <ekz>
        ili <tld/>is unu la aliajn
        <fnt>
          <aut>J. Merchant</aut>
          <vrk>Kompatinda Klem</vrk>
          <lok>1931</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi &scirc;atas vin kaj <tld/>as je via sano!
        <fnt>
          <aut>A. Kor&jcirc;enkov</aut>
          <vrk>&rightquot;Literatura Foiro&#x201D; jubileas</vrk>
          <lok>La Ondo de Esperanto, 2000:6 (68)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_z;&c_a;&c_c;&c_mol; <ind>&c_t;&c_o;&c_s;&c_t;</ind></trd>
  <trd lng="bg">&c_v;&c_d;&c_i;&c_g;&c_n;&c_a; <ind>&c_t;&c_o;&c_s;&c_t;</ind></trd>
  <trd lng="ca">brindar</trd>
  <trd lng="cs">&rcaron;&iacute;kat p&rcaron;&iacute;pitek</trd>
  <trd lng="de">einen <ind>Tost</ind> ausbringen</trd>
  <trd lng="en">toast<klr tip="amb"> (speech during drink)</klr></trd>
  <trd lng="es">brindar</trd>
  <trdgrp lng="fr">
     <trd>trinquer</trd>,
     <trd>porter un toast</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>poh&aacute;rk&ouml;sz&ouml;nt&odblac;t mond</trd>,
    <trd>&aacute;ldom&aacute;st mond</trd>,
    <trd>t&oacute;sztot mond</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4E7E;&#x676F;&#x3059;&#x308B;  <pr>&#x304B;&#x3093;&#x3071;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x795D;&#x676F;&#x3092;&#x3042;&#x3052;&#x308B;  <pr>&#x3057;&#x3085;&#x304F;&#x306F;&#x3044;&#x3092;&#x3042;&#x3052;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">een <ind>toast</ind> uitbrengen</trd>
  <trdgrp lng="pl">
    <trd>wznosi&cacute; <ind>toast</ind></trd>,
    <trd>przypija&cacute;<klr> do kogo&sacute;</klr></trd>
  </trdgrp>
  <trd lng="pt">brindar</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_v;&c_o;&c_z;&c_g;&c_l;&c_a;&c_s;&c_i;&c_t;&c_mol;
  <ind>&c_t;&c_o;&c_s;&c_t;</ind></trd>
  <trdgrp lng="sk">
    <trd>poveda&tcaron; pr&iacute;pitok</trd>,
    <trd>zdravicu</trd>
  </trdgrp>
  <trd lng="uk">&#x432;&#x438;&#x433;&#x43E;&#x43B;&#x43E;&#x448;&#x443;&#x432;&#x430;&#x442;&#x438; &#x442;&#x43E;&#x441;&#x442;</trd>
  <trdgrp lng="zh">
    <trd>&#x4E7E;&#x676F; <pr>g&#x101;nb&#x113;i</pr></trd>,
    <trd>&#x5E72;&#x676F; <pr>g&#x101;nb&#x113;i</pr></trd>,
    <trd>&#x656C;&#x9152; <pr>j&#xEC;ngji&#x1D4;</pr></trd>,
    <trd>&#x4E3E;&#x676F; <pr>j&#x1D4;b&#x113;i</pr></trd>,
    <trd>&#x8209;&#x676F; <pr>j&#x1D4;b&#x113;i</pr></trd>
  </trdgrp>
</drv>
</art>
