<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>firm</rad>/a
</kap>
<drv mrk="firm.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="firm.0a.densa">
    <dif>
      Ne mola, ne cedanta sub la premo; konsistanta el forte kunigitaj
      eroj:
      <ekz>
        <tld/>a viando;
      </ekz>
      <ekz>
        <tld/>a tero.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dens.0a">densa</ref>,
      <ref cel="rezist">rezista</ref>.
    </refgrp>
    <trdgrp lng="fr">
      <trd>consistant<klr> (ferme)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="firm.0a.solida">
    <dif>
      Forte fiksita, ne&scirc;ancelebla:
      <ekz>
        <tld/>a pa&scirc;o<fnt>Z</fnt>,
      </ekz>
      <ekz>
        konstrua&jcirc;o, muro, apogilo;
      </ekz>
      <ekz>
        sidi <tld/>e sur la selo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fortik.0a">fortika</ref>,
      <ref cel="kompak.0a">kompakta</ref>,
      <ref cel="solid.0a">solida</ref>,
      <ref cel="stabil.0a">stabila</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="mason.0i">masoni</ref>.
    </refgrp>
  </snc>
  <snc mrk="firm.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Certa, konstanta, malfacile &scirc;an&gcirc;ebla:
      <ekz>
        <tld/>a situacio, ofico<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a decido<fnt>Z</fnt>,
      </ekz>
      <ekz>
        <tld/>a volo<fnt>B</fnt>,
      </ekz>
      <ekz>
        <tld/>a karaktero;
      </ekz>
      <ekz>
        <tld/>a vo&ccirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e elteni atakon;
      </ekz>
      <ekz>
        <tld/>e starigita paco;
      </ekz>
      <ekz>
        tio <tld/>igas la pacon, la kura&gcirc;on, mian opinion;
      </ekz>
      <ekz>
        <ind><tld/>igi</ind> &ccirc;e iu la opinion, ke...<fnt>Z</fnt>.
        <trdgrp lng="fr">
          <trd>affermir</trd>,
          <trd>conforter</trd>,
          <trd>&eacute;tayer</trd>,
          <trd>renforcer</trd>
        </trdgrp>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dauxr.0a">da&ubreve;ra</ref>,
      <ref cel="konsta.0a">konstanta</ref>,
      <ref cel="persis">persista</ref>,
      <ref cel="romp">nerompebla</ref>,
      <ref cel="sku">neskuebla</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="fer.0a">fera</ref>,
      <ref cel="grund.0o">grundo</ref>,
      <ref cel="konfir.0i">konfirmi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>fest</trd>,
    <trd>stabil</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ferme<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="nl">stevig</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_k;&c_r;&c_je;&c_p;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
