    <trd>&#x5805;&#x56FA;&#x306A;  <pr>&#x3051;&#x3093;&#x3054;&#x306A;</pr></trd>,
    <trd>&#x78BA;&#x56FA;&#x3068;&#x3057;&#x305F;  <pr>&#x304B;&#x3063;&#x3053;&#x3068;&#x3057;&#x305F;</pr></trd>
  </trdgrp>
  <trd lng="nl">stevig</trd>
  <trdgrp lng="pt">
    <trd>firme</trd>,
    <trd>constante</trd>,
    <trd>teso</trd>,
    <trd>consistente</trd>,
