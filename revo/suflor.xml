<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: suflor.xml,v 1.27 2023/10/21 07:46:48 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>suflor</rad>/o
</kap>
<drv mrk="suflor.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEA</uzo>
  <snc mrk="suflor.0o.homo">
    <dif>
      Homo, kies <ref tip="lst" lst="voko:metioj" cel="okup.0o">okupo</ref>
      estas malla&ubreve;te flustri al aktoroj la parolojn, kiujn ili ne memoras:
      <ekz>
        kia <tld/>o, tia aktoro
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        lia patro estis drama aktoro kaj lia patrino estis teatra <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1978">Julio Baghy</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_f;&c_l;&c_jo;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>n&aacute;pov&ecaron;da <klr>(osoba)</klr></trd>,
    <trd>sufl&eacute;r</trd>
  </trdgrp>
  <trd lng="de">Souffleur</trd>
  <trd lng="en">prompter</trd>
  <trd lng="es">apuntador<klr> (teatro)</klr></trd>
  <trdgrp lng="fr">
    <trd>souffleur</trd>
  </trdgrp>
  <trd lng="hu">s&uacute;g&oacute;<klr tip="ind">(sz&iacute;nh&aacute;zi)</klr></trd>
  <trd lng="it">suggeritore</trd>
  <trdgrp lng="ja">
    <trd>&#x30D7;&#x30ED;&#x30F3;&#x30D7;&#x30BF;&#x30FC;</trd>,
    <trd>&#x9ED2;&#x8863;  <pr>&#x3053;&#x304F;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">souffleur</trd>
  <trd lng="pl">sufler</trd>
  <trd lng="ru">&c_s;&c_u;&c_f;&c_l;&c_jo;&c_r;</trd>
  <trd lng="sk">&scaron;epk&aacute;r</trd>
  <trd lng="uk">&c_s;&c_u;&c_f;&#x43B;&c_je;&c_r;</trd>
</drv>

<drv mrk="suflor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="suflor.0i.flustri">
    <dif>
      <ref tip="super" cel="flustr.0i">Flustri</ref> kiel <tld/>o:
      <ekz>
        la princo volis <tld/>i al <nom>Mentezufis</nom> la celon de 
        la voja&gcirc;o de <nom>Istubar</nom>, sed li
        mordis la lipojn kaj silentis
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="slip.kasx0eto"><tld/>slipeto</ref>
    <trd lng="be">&c_s;&c_u;&c_f;&c_l;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>vorsagen</trd>,
    <trd>souffleur</trd>
  </trdgrp>
    <trd lng="ru">&c_s;&c_u;&c_f;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="suflor.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="sugest.0i">Sugesti</ref>:
      <ekz>
        la&ubreve;du la stultulon kaj lasu lin labori <tld/>as la sa&gcirc;ulo
        <fnt>
          <aut>Alejandro Cossavella</aut>
          <vrk><url ref="&Monato;/2006/008766.php">Poemoj de Alejandro
          Cossavella</url></vrk>
           <bib>Monato</bib>
          <lok>2006/04, p. 22</lok>
        </fnt>;
      </ekz>
      <ekz>
        la dira&jcirc;oj kaj la konduto de multaj atendantoj en la longega vico anta&ubreve; la
        gi&ccirc;etoj dokumentigaj suspektigas tiujn motivojn <tld/>atajn en la latinlingva
        dokumento
        <fnt>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2010/010136.php">Strato de la e&ubreve;ropa
          kulturo</url></vrk>
          <bib>Monato</bib>
          <lok>2010/12, p. 24</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi devas nur le&gcirc;ere premi du klavojn &ndash; la
        vortaro sufloras la bezonatan vorton
        <fnt>
        <aut>Sergeo
        Dereviannykh</aut>: 
        <vrk><url ref="&Monato;/pdf/2002/monato200204.pdf#page=18">&Ccirc;u vi tradukas
        france?</url></vrk>,
        <bib>Monato</bib>
        <lok>2002/04, p. 18</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_d;&c_k;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_n;&c_a;&c_sh;&c_e;&c_p;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">einfl&uuml;stern</trd>
    <trd lng="ru">&c_p;&c_o;&c_d;&c_s;&c_k;&c_a;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>napov&iacute;dat</trd>,
    <trd>suflovat</trd>
  </trdgrp>
  <trd lng="en">prompt</trd>
  <trdgrp lng="es">
    <trd>apuntar</trd>
    <trd>sugerir</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>souffler<klr> (chuchoter, sugg&eacute;rer)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>s&uacute;g</trd>,
    <trd>megs&uacute;g</trd>
  </trdgrp>
  <trd lng="it">suggerire</trd>
  <trdgrp lng="ja">
    <trd>&#x305B;&#x308A;&#x3075;&#x3092;&#x4ED8;&#x3051;&#x308B;  <pr>&#x305B;&#x308A;&#x3075;&#x3092;&#x3064;&#x3051;&#x308B;</pr></trd>,
    <trd>&#x305D;&#x3063;&#x3068;&#x6559;&#x3048;&#x308B;  <pr>&#x305D;&#x3063;&#x3068;&#x304A;&#x3057;&#x3048;&#x308B;</pr></trd>,
    <trd>&#x8033;&#x6253;&#x3061;&#x3059;&#x308B;  <pr>&#x307F;&#x307F;&#x3046;&#x3061;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">souffleren</trd>
  <trdgrp lng="pl">
    <trd>podpowiada&cacute;</trd>,
    <trd>suflerowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>poradi&tcaron;</trd>,
    <trd>vnukn&uacute;&tcaron; (pren.)</trd>,
    <trd>&scaron;epka&tcaron; <klr>(div.)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_u;&c_f;&#x43B;&c_ib;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_ib;&c_d;&#x43A;&c_a;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
</art>
