<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>suflor</rad>/o
</kap>
<drv mrk="suflor.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <dif>
      Homo, kies okupo estas malla&ubreve;te flustri al aktoro la parolojn, kiujn
      li ne memoras.
    </dif>
  </snc>
  <trd lng="de">Souffleur</trd>
  <trd lng="nl">souffleur</trd>
  <trd lng="ru">&c_s;&c_u;&c_f;&c_l;&c_jo;&c_r;</trd>
</drv>

<drv mrk="suflor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Flustri kiel <tld/>o.
    </dif>
    <ref tip="vid" cel="slip.suflor0eto"><tld/>slipeto</ref>
    <trd lng="ru">&c_s;&c_u;&c_f;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Subinspiri, sugestii.
    </dif>
    <trd lng="ru">&c_p;&c_o;&c_d;&c_s;&c_k;&c_a;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="de">soufflieren</trd>
  <trd lng="nl">souffleren</trd>
</drv>
</art>
