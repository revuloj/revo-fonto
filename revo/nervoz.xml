<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nervoz.xml,v 1.24 2021/05/29 14:32:58 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>nervoz</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="nervoz.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
     <ref tip="dif" cel="nerv.0a.nervoza">Nerva<sncref/></ref>:
      <ekz>
        li jam estis tre <tld/>a
        <fnt><aut>Spomenka &Scirc;timec</aut>:
          <vrk>Morto en Melburna tramo</vrk>, 
          <bib>Monato</bib><lok>jaro 1997a, numero 8a, p. 24a</lok>
        </fnt>;
      </ekz>
      <ekz>
        post pa&ubreve;zo, sentante min iom <tld/>a post tiu longa rigardado,
        mi klopodas rekomenci la konversacion
        <fnt>
          <bib>Monato</bib>
          <aut>Daniel L. Mason</aut>
          <vrk><url ref="&Monato;/2011/010276.php">Matena
          kafo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        sen vir&ccirc;impanzo Regina sentas sin ege <tld/>a, malgaja, kaprica
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="https://www.monato.be/2004/008297.html">Havi
          &ccirc;impanzon hejme</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        birdo, kiu estas iom <tld/>a, kaj flirtas de bran&ccirc;o al bran&ccirc;o,
        neniam kontenta resti
        <fnt>
          <bib>Monato</bib>
          <aut>Angela Tellier</aut>
          <vrk><url ref="https://www.monato.be/2004/008306.html">La&ubreve;proverbaj
          fabeloj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e alrigardis policanoj, tro malmultnombraj por &ccirc;iam
        interveni
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2011/010478.php">Malstulta
          sezono</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_e;&c_r;&c_v;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_n;&c_e;&c_r;&c_v;&c_o;&c_z;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bo">&#xF5A;&#xF0B;&#xF42;&#xF72;&#xF0B;&#xF5A;&#xF72;&#xF0B;&#xF42;&#xF72;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>nerv&oacute;zn&iacute;</trd>,
    <trd>podr&aacute;&zcaron;d&ecaron;n&yacute;</trd>,
    <trd>trp&iacute;c&iacute; nervozitou</trd>,
    <trd>vzn&ecaron;tliv&yacute;</trd>
  </trdgrp>
  <trd lng="de">nerv&ouml;s</trd>
  <trdgrp lng="en">
    <trd>anxious</trd>,
    <trd>nervous</trd>
  </trdgrp>
  <trd lng="es">nervioso</trd>
  <trdgrp lng="fr">
    <trd>&eacute;nerv&eacute;</trd>,
    <trd>impatient</trd>,
    <trd>nerveux<klr> (&eacute;nerv&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="hu">ideges</trd>
  <trdgrp lng="nl">
    <trd>zenuwachtig</trd>,
    <trd>nerveus</trd>
  </trdgrp>
  <trd lng="pt">nervoso</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_r;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_r;&c_v;&c_o;&c_z;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nerv&oacute;zny</trd>,
    <trd>popudliv&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="nervoz.0eco">
  <kap>
    <tld/>eco
  </kap>
  <snc>
    <dif>
      Eco esti <tld/>a:
      <ekz>
        la eniro de dekstreguloj en la registaron en A&ubreve;strio ka&ubreve;zis en
        Belgio malver&scirc;ajnan <tld/>econ
        <fnt>
          <bib>Monato</bib>
          <aut>Guido Van Damme</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006921.html">Belgio:
          Malvuali nigran fantomon</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        preska&ubreve; bolis mia kapo pro <tld/>eco
        <fnt>
          <bib>Monato</bib>
          <aut>Julia Sigmond</aut>
          <vrk><url ref="&Monato;/2011/010147.php">Georgo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Nervosit&auml;t</trd>
  <trd lng="en">anxiety</trd>
  <trdgrp lng="fr">
    <trd>angoisse</trd>,
    <trd>nervosit&eacute;</trd>,
    <trd>stress</trd>
  </trdgrp>
</drv>



<drv mrk="nervoz.0iga">
  <kap><tld/>iga</kap>
  <snc>
    <dif>
      Ka&ubreve;zanta inciton, anta&ubreve;timon, animstre&ccirc;on:
      <ekz>
        <tld/>iga serena rideto<fnt><aut>Higinio Garc&iacute;a</aut>: <vrk>El
        la atesto de Mustafao Vanesavir', submastro</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 3a, p. 25a</lok></fnt>;
      </ekz>
      <ekz>
        post kelkminuta <tld/>iga revici&gcirc;o &scirc;i fine havigis al si
        forketon kaj reatingis sian man&gcirc;olokon
        <fnt>
          <bib>Monato</bib>
          <aut>Sen Rodin</aut>
          <vrk><url ref="&Monato;/2006/008905.php">Negro
          i&gcirc;is nigrulo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_n;&c_ja;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trdgrp lng="de">
     <trd>nervend</trd>,
     <trd>nervig</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;nervant</trd>,
    <trd>irritant<klr> (&eacute;nervant)</klr></trd>
  </trdgrp>
  <trd lng="hu">ideges&iacute;t&odblac;</trd>
  <trdgrp lng="nl">
    <trd>irritant</trd>,
    <trd>op de <ind>zenuw</ind>en werkend</trd>
  </trdgrp>  
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_r;&c_v;&c_i;&c_r;&c_u;&c_ju;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="nervoz.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ka&ubreve;zi da&ubreve;ran maltrankvilon, detrui ies egalanimecon:
      <ekz>
        ili <tld/>igis lin per siaj bruoj, odoroj,
        mallerta&jcirc;oj<fnt><aut>Renervo Da&scirc;o</aut>: <vrk>Ne-atenditaj
        sekvoj</vrk>, <bib>Monato</bib><lok>jaro 1999a, numero 8a, p.
        17a</lok></fnt>;
      </ekz>
      <ekz>
        la rezultoj de TIMSS iel maltrankviligis kaj <tld/>igis la
        germanajn politikistojn respondecajn pri instruado<fnt><aut>Jomo
        Ipfelkoffer</aut>: <vrk>Komparo de instrumetodoj</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 6a, p. 9a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="incit.0i">inciti</ref>,
    <ref cel="trankv.mal0igi">maltrankviligi</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_n;&c_e;&c_r;&c_v;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_n;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>nerven</trd>,
     <trd>nerv&ouml;s machen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;stabiliser<klr> (&eacute;nerver)</klr></trd>,
    <trd>&eacute;nerver</trd>,
    <trd>impatienter</trd>,
    <trd>irriter<klr> (&eacute;nerver)</klr></trd>
  </trdgrp>
  <trd lng="hu">ideges&iacute;t</trd> 
  <trdgrp lng="nl">
    <trd>irriteren</trd>,
    <trd><ind>nerveus</ind> maken</trd>,
    <trd><ind>zenuwachtig</ind> maken</trd>
  </trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_r;&c_v;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">znerv&oacute;z&ncaron;ova&tcaron;</trd>
  <trd lng="cs">znerv&oacute;z&ncaron;ovat</trd>
</drv>
</art>
