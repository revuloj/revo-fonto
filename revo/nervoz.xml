<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>nervoz</rad>/a
</kap>
<drv mrk="nervoz.0a">
  <kap><tld/>a</kap>
  <snc>
    <ref tip="dif" cel="nerv.0a.nervoza">nerva <sncref/></ref>
    <ekz>
      li jam estis tre <tld/>a<fnt><aut>Spomenka &Scirc;timec</aut>: <vrk>Morto
      en Melburna tramo</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero
      8a, p. 24a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="de">nerv&ouml;s</trd>
  <trd lng="en">nervous</trd>
  <trd lng="es">nervioso</trd>
  <trdgrp lng="fr">
    <trd>&eacute;nerv&eacute;</trd>,
    <trd>impatient</trd>,
    <trd>nerveux<klr> (&eacute;nerv&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="nl">zenuwachtig</trd>
  <trd lng="pt">nervoso</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_r;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_r;&c_v;&c_o;&c_z;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="nervoz.0iga">
  <kap><tld/>iga</kap>
  <snc>
    <dif>
      Ka&ubreve;zanta inciton, anta&ubreve;timon, animstre&ccirc;on:
      <ekz>
        <tld/>iga serena rideto<fnt><aut>Higinio Garc&iacute;a</aut>: <vrk>El la
        atesto de Mustafao Vanesavir', submastro</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 3a, p. 25a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>&eacute;nervant</trd>,
    <trd>irritant<klr> (&eacute;nervant)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="nervoz.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ka&ubreve;zi da&ubreve;ran maltrankvilon, detrui ies egalanimecon:
      <ekz>
        ili <tld/>igis lin per siaj bruoj, odoroj,
        mallerta&jcirc;oj<fnt><aut>Renervo Da&scirc;o</aut>: <vrk>Ne-atenditaj
        sekvoj</vrk>, <bib>Monato</bib><lok>jaro 1999a, numero 8a, p.
        17a</lok></fnt>;
      </ekz>
      <ekz>
        la rezultoj de TIMSS iel maltrankviligis kaj <tld/>igis la
        germanajn politikistojn respondecajn pri instruado<fnt><aut>Jomo
        Ipfelkoffer</aut>: <vrk>Komparo de instrumetodoj</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 6a, p. 9a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="incit.0i">inciti</ref>,
    <ref cel="trankv.mal0igi">maltrankviligi</ref>
  </refgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;stabiliser<klr> (&eacute;nerver)</klr></trd>,
    <trd>&eacute;nerver</trd>,
    <trd>impatienter</trd>,
    <trd>irriter<klr> (&eacute;nerver)</klr></trd>
  </trdgrp>
</drv>
</art>
