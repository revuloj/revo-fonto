<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>strik</rad>/o
</kap>
<drv mrk="strik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Intencita &ccirc;eso de laboro celanta devigi la mastron kontentigi la
      postulojn de la salajratoj.
    </dif>
  </snc>
  <trd lng="de">Streik</trd>
  <trd lng="en">strike</trd>
  <trd lng="es">huelga</trd>
  <trd lng="nl">staking</trd>
  <trd lng="ru">&c_z;&c_a;&c_b;&c_a;&c_s;&c_t;&c_o;&c_v;&c_k;&c_a;</trd>
</drv>

<drv mrk="strik.mastro0o">
  <kap>mastro<tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;eso de laboro ordonata de mastro por cedigi la salajratojn;
      <ref tip="sin" cel="lokaux.0o">loka&ubreve;to</ref>.
    </dif>
  </snc>
  <trd lng="de">Aussperrung</trd>
  <trd lng="nl">lockout</trd>
  <trd lng="ru">&c_l;&c_o;&c_k;&c_a;&c_u;&c_t;</trd>
</drv>

<drv mrk="strik.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Intence &ccirc;esigi la laboron:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mia memoro ofte <tld/>as
        <klr>(haltas, ne funkcias)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">streiken</trd>
  <trd lng="nl">staken</trd>
  <trd lng="ru">&c_b;&c_a;&c_s;&c_t;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
