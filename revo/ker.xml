<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ker.xml,v 1.32 2025/04/01 05:06:39 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ker</rad>/o
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="ker.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">SPO</uzo>
  <mlg>&#9825;</mlg>
  <snc>
    <dif>
      Ludkarta
      <ref tip="super" cel="emblem.0o.karta">emblemo</ref>,
      signata per ru&gcirc;a <ref tip="super" cel="kor.0o.FIG">koro<sncref/></ref>:
      <ekz>
        tiaj kartoj, kiuj prezentas <tld/>ajn damojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Konkurso de saltado</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj damoj, la karoa, <tld/>a, trefa kaj pika
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tistino kaj kamentubisto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="karo.0o">karoo</ref>,
      <ref cel="pik.0o.emblemo">piko</ref>,
      <ref cel="tref.0o">trefo</ref>
    </refgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: (en kartoludo) anka&#365; eblas diri <ke>koro</ke>.
  </rim>

  <trd lng="be">&c_ch;&c_y;&c_r;&c_v;&c_a; <klr tip="amb">
  (&c_u; &c_k;&c_a;&c_r;&c_t;&c_a;&c_h;)</klr></trd>
  <trd lng="br">keur</trd>
  <trdgrp lng="cs">
    <trd>srdce v kart&aacute;ch</trd>,
    <trd>&ccaron;erven&eacute; v kart&aacute;ch</trd>
  </trdgrp>
  <trd lng="de">Herz <klr>(Spielkarte)</klr></trd>
  <trd lng="en">heart</trd>
  <trd lng="es">corazones <klr>(palo de la baraja de naipes
  franceses)</klr></trd>
  <trd lng="fa">&a_d;&a_l;<klr tip="ind"> (&a_n;&a_q;&a_sh;
  &a_w;&a_r;&a_q; &a_b;&a_A;&a_z;&f_y;)</klr></trd>
  <trd lng="fr">c&oelig;ur<klr tip="ind"> (jeu de cartes)</klr></trd>
  <trdgrp lng="gl">
    <trd>coraz&oacute;ns (naipe/carta)</trd>,
    <trd>coraz&oacute;n (naipe/carta)</trd>
  </trdgrp>
  <trd lng="hu">k&odblac;r</trd>
  <trd lng="it">cuori (carta da gioco)</trd>
  <trd lng="ja">&#12495;&#12540;&#12488;</trd>
  <trd lng="nl">harten<klr tip="ind"> (in kaartspel)</klr></trd>
  <trdgrp lng="pl">
    <trd>kier</trd>,
    <trd>czerwone <ind>serca</ind><klr> (pot.)</klr></trd>,
    <trd>czerwie&nacute; <klr>(pot.)</klr></trd>
  </trdgrp>
  <trd lng="pt">copas (naipe/carta)</trd>
  <trd lng="ro">inim&abreve;</trd>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_v;&c_y;</trd>
  <trdgrp lng="sk">
    <trd>srdce v kart&aacute;ch</trd>,
    <trd>&ccaron;erven&eacute; v kart&aacute;ch</trd>
  </trdgrp>
  <trd lng="sv">hj&auml;rter</trd>
  <trdgrp lng="uk">
    <trd>&#1089;&#1077;&#1088;&#1076;&#1077;&#1095;&#1082;&#1086;</trd>,
    <trd>&#1086;&#1089;&#1077;&#1088;&#1076;&#1103;</trd>,
    <trd>&#1095;&#1080;&#1088;&#1074;&#1072;</trd>,
    <trd>&#1095;&#1077;&#1088;&#1074;&#1072;</trd>
  </trdgrp>
  <trd lng="zh">&#32418;&#24515; <pr>h&oacute;ng x&#299;n</pr></trd>
</drv>
</art>
