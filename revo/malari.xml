<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>malari</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="malari.0o">
  <kap><tld/>o</kap>
  <snc mrk="malari.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Febra <ref tip="super" cel="san.mal0o">malsano</ref>, propra al
      mar&ccirc;aj regionoj, transinokulita per piko de kuloj.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_ja;&c_r;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_m;&c_a;&c_l;&c_a;&c_r;&c_i;&c_ja;</trd>
  <trd lng="br">malaria</trd>
  <trd lng="ca">mal&agrave;ria</trd>
  <trd lng="de">Malaria</trd>
  <trd lng="en">malaria</trd>
  <trd lng="es">malaria</trd>
  <trdgrp lng="fr">
    <trd>malaria</trd>,
    <trd>paludisme</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>mal&aacute;ria</trd>,
    <trd>forr&oacute;l&aacute;z</trd>
  </trdgrp>
  <trd lng="nl">malaria</trd>
  <trd lng="pt">mal&aacute;ria</trd>
  <trd lng="ru">&c_m;&c_a;&c_l;&c_ja;&c_r;&c_i;&c_ja;</trd>
</drv>
</art>
