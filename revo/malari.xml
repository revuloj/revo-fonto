<art mrk="$Id: malari.xml,v 1.30 2024/01/25 20:10:33 revo Exp $">
  <kap>
    <ofc>2</ofc>
    <rad>malari</rad>/o <fnt><bib>PV</bib></fnt>
  </kap>

<drv mrk="malari.0o">
  <kap><tld/>o</kap>
  <snc mrk="malari.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Febra <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>, propra al
      mar&ccirc;aj regionoj, transinokulata per piko de kuloj:
      <ekz>
        ekspedicianoj mortis ka&ubreve;ze de tropikaj malsanoj &ndash; <tld/>o,
        disenterio, flava febro
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2004/008160.html">Afriko kaj aidoso</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        restas tamen kiel &ccirc;efaj malsanoj <tld/>o, diareo, pulmaj
        infektoj, nesufi&ccirc;a
        nutrado kaj morbilo
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="&Monato;/2005/008741.php">Norduloj ebligas sudan sanon</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; &ccirc;irka&ubreve; 60&#x202F;-&#x202F;70 jaroj <tld/>o en E&ubreve;ropo ankora&ubreve; estis
        disvasti&gcirc;inta malsano
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2007/009243.php">Epidemiaj sekvoj en E&ubreve;ropo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_ja;&c_r;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_m;&c_a;&c_l;&c_a;&c_r;&c_i;&c_ja;</trd>
  <trd lng="br">malaria</trd>
  <trd lng="ca">mal&agrave;ria</trd>
  <trd lng="cs">mal&aacute;rie</trd>
  <trd lng="de">Malaria</trd>
  <trd lng="en">malaria</trd>
  <trd lng="es">malaria</trd>
  <trd lng="fa">&a_m;&a_A;&a_l;&a_A;&a_r;&f_y;&a_A;</trd>
  <trdgrp lng="fr">
    <trd>malaria</trd>,
    <trd>paludisme</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>mal&aacute;ria</trd>,
    <trd>forr&oacute;l&aacute;z</trd>
  </trdgrp>
  <trd lng="id">malaria</trd>
  <trd lng="ja">&#x30DE;&#x30E9;&#x30EA;&#x30A2;</trd>
  <trd lng="nl">malaria</trd>
