    <trd>&#x51B7;&#x71B1;&#x75C5; <pr>l&#x11B;ngr&#xE8;b&#xEC;ng</pr></trd>,
    <trd>&#x759F;&#x75BE; <pr>nu:&#xE8;j&#xED;</pr></trd>,
