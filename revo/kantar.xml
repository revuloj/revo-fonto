<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kantarel</rad>/o
</kap>
<drv mrk="kantar.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
     Man&gcirc;ebla, ofta, oran&gcirc;kolora 
     <ref tip="lst" cel="fung.0o.fruktokorpo"
     lst="voko:fungoj">fungo</ref>;
     <ref tip="lst" cel="genr.0o.BIO"
     lst="voko:botanikaj_genroj">genro</ref>
      el familio kantarelacoj (<trd lng="la">Cantharellus</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_s;&c_ib;&c_ch;&c_k;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>rossinyol</trd>,
    <trd>fals rossinyol</trd>,
    <trd>camagroc</trd>,
    <trd>fals camagroc</trd>
  </trdgrp>
  <trd lng="de">Pfifferling</trd>
  <trd lng="en">chanterelle</trd>
  <trd lng="fr">chanterelle</trd>
  <trd lng="hu">r&oacute;kagomba</trd>
  <trdgrp lng="nl">
    <trd>hanenkam</trd>,
    <trd>cantharel</trd>
  </trdgrp>
  <trd lng="pl">pieprznik</trd>
  <trd lng="ru">&c_l;&c_i;&c_s;&c_i;&c_ch;&c_k;&c_a;</trd>
</drv>
</art>
