    <kap>
      <rad>Bofort</rad>/o
    </kap>

<drv mrk="bofort.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">PERS</uzo>
    <dif>
      <trd lng="en">Francis Beaufort</trd> (1774&#x202F;&#8208;&#x202F;1857), brita mara
      oficiro kaj
      <ref tip="lst" lst="voko:historiaj_personoj" cel="hidrolog.0o">hidrologo</ref>:
      <ekz>
        la <tld/>a Maro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=272004">Beaufort-maro</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="b"/>a skalo <klr>[&#8230;]</klr> por taksi la fortecon de
        vento
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=49869">Boforta skalo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Francis <ind>Beaufort</ind></trd>
    <trd lng="fr">Francis <ind>Beaufort</ind></trd>
    <trd lng="pl">Francis <ind>Beaufort</ind></trd>
  </snc>
  <trd lng="be">&c_B;&c_a;&c_f;&c_o;&c_r;&c_t;</trd>
  <trd lng="ja">&#x30D3;&#x30E5;&#x30FC;&#x30D5;&#x30A9;&#x30FC;&#x30C8;</trd>
  <trd lng="uk">&c_B;&#x43E;&c_f;&#x43E;&c_r;&c_t;</trd>
</drv>
