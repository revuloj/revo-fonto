<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kolon1.xml,v 1.15 2021/05/30 19:16:14 revo Exp $">
<kap>
  <rad>kolon</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="kolon1.0o">
  <kap><tld/>o</kap>
  <snc mrk="kolon1.0o.EKON">
    <dif>
      <ref tip="lst" cel="unu.mon0o.EKON" lst="voko:monunuoj">Monunuo</ref> de <ref tip="malprt" cel="kostarik.0o">Kostariko</ref> 
      kaj, kune kun la usona dolaro, de <ref tip="malprt" cel="salvador.S0o">Salvadoro</ref>:
      <ekz>
        1 <tld/>o = 100 centavoj
        <fnt>
          <vrk><url ref="https://www.esperanto.be/fel/jar/landoj.php">La landoj de l' tero</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="kolon1.0o.RIM_HOM">
    Fundamente <ref tip="hom" cel="kolon.0o">kolono</ref> estas 
    dika fosto subtenanta konstrua&jcirc;on.
    </rim>
  <trd lng="af">kolom</trd>
  <trd lng="am">&#x12A0;&#x121D;&#x12F5;</trd>
  <trd lng="ar">&#x639;&#x645;&#x648;&#x62F;</trd>
  <trd lng="de">Col&oacute;n</trd>
  <trd lng="en">col&oacute;n</trd>
  <trd lng="es">col&oacute;n</trd>
  <trd lng="fr">col&oacute;n<klr> (monnaie)</klr></trd>
  <trd lng="id">colon</trd>
  <trd lng="ne">&#x938;&#x94D;&#x924;&#x92E;&#x94D;&#x92D;</trd>
  <trd lng="nl">col&oacute;n<klr tip="ind"> (munt)</klr></trd>
  <trd lng="pl">col&oacute;n<klr> (1 col&oacute;n kostaryka&nacute;ski = 100 
  centavos)</klr></trd>
  <trdgrp lng="pt">
    <trd>colom</trd>,
    <trd>col&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kol&oacute;nka</trd>,
    <trd>st&lacute;pec</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>colon</trd>,
    <trd>kolon</trd>,
    <trd>tra&ccaron;n&iacute;k</trd>
  </trdgrp>
  <trd lng="sq">kolona</trd>
</drv>
</art>
