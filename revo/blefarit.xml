<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: blefarit.xml,v 1.6 2024/01/08 07:24:57 revo Exp $">
<kap>
  <rad>blefarit</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="blefarit.0o">
  <kap><tld/>o</kap>
  <snc mrk="blefarit.0o.MED">
    <dif>
      Palpebr-<ref tip="lst" cel="inflam.0o" lst="voko:malsanoj">inflamo</ref>:
      <ekz>
        al la plej oftaj komplika&jcirc;oj apartenas la brulumo de la palpebra
        rando &ndash; <tld/>o
        <fnt><bib>FK</bib>, <lok>5, kronika katara
        konjunktivito</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_l;&c_je;&c_f;&c_a;&c_r;&c_y;&c_t;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je; &c_p;&c_a;&c_v;&c_je;&c_k;&c_a;&c_w;</trd>
  </trdgrp>
  <trd lng="en">blepharitis</trd>
  <trd lng="fr">bl&eacute;pharite</trd>
  <trd lng="he">&#x5D3;&#x5DC;&#x5E7;&#x5EA; &#x5E2;&#x5E4;&#x5E2;&#x5E3;</trd>
  <trdgrp lng="id">
    <trd>blefaritis</trd>,
    <trd><ind>radang</ind> kelopak mata</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&#x43B;&c_je;&c_f;&c_a;&c_r;&c_i;&c_t;</trd>,
    <trd>&c_z;&c_a;&#x43F;&c_a;&#x43B;&c_je;&#x43D;&#x43D;&#x44F; &#x43F;&#x43E;&c_v;&c_ib;&#x43A;</trd>
  </trdgrp>
</drv>
</art>

