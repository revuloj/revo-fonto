<art mrk="$Id: sciure.xml,v 1.14 2024/03/24 20:00:00 revo Exp $">
<kap>
   <rad>sciured</rad>/oj <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sciure.0oj">
   <kap><tld/>oj</kap>
   <snc mrk="sciure.0oj.ZOO">
     <uzo tip="fak">ZOO</uzo>
     <dif>
       Mezgrandaj mambestoj vivantaj en arboj
       a&ubreve; en herbejoj, kun grandaj okuloj;
       <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">familio</ref> el la ordo
       <ref tip="malprt" cel="rongx.0uloj">ron&gcirc;uloj</ref>
       (<trd lng="la">Sciuridae</trd>):
      <ekz>
        marmoto estas holarktisa genro de mamuloj el la <klr>[&#x2026;]</klr> familio <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=33594">Marmoto</url></lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_v;&c_a;&c_v;&c_jo;&c_r;&c_k;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
   <trd lng="ca">esci&uacute;rids</trd>
   <trd lng="de">H&ouml;rnchen</trd>
   <trd lng="fr">sciurid&eacute;s</trd>
   <trd lng="hu">m&oacute;kusf&eacute;l&eacute;k</trd>
   <trd lng="it">sciuridi</trd>
   <trd lng="nl">eekhoorns<klr tip="ind"> (fam.)</klr></trd>
   <trd lng="pl">wiewi&oacute;rkowate</trd>
   <trd lng="ru">&c_b;&c_je;&c_l;&c_i;&c_ch;&c_mol;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&c_b;&c_ib;&#x43B;&#x44F;&c_ch;&c_ib;</trd>
  <trd lng="zh">&#x677E;&#x9F20;&#x79D1; <pr>s&#x14D;ngsh&#x1D4;k&#x113;</pr></trd>
</drv>
</art>
