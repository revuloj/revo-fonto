<art mrk="$Id$">
<kap>
   <rad>sciured</rad>/oj
</kap>
<drv mrk="sciure.0oj">
   <kap><tld/>oj</kap>
   <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
       Familio el la ordo <ref tip="super"
       cel="rongx.0uloj">ron&gcirc;uloj</ref>
       (<trd lng="la">Sciuridae</trd>), kun grandaj okuloj kaj longa
       longhara vosto.
     </dif>
   </snc>
   <trd lng="be">&c_v;&c_a;&c_v;&c_jo;&c_r;&c_k;&c_a;&c_v;&c_y;&c_ja; <klr
   tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
