<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: dorote.xml,v 1.1 2020/05/30 20:10:35 revo Exp $">
    <kap>
      <rad>Dorote</rad>/o 
      <fnt><bib>FK</bib></fnt> <fnt><bib>Fab4</bib></fnt>
    </kap>

<drv mrk="dorote.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">NOM</uzo>
  <snc mrk="dorote.0o.nomo">
    <dif>
      Grekdevena <ref tip="lst" lst="voko:personaj_nomoj" cel="nom.persona0o.GEN">persona nomo</ref>,
      uzata kaj por virinoj kaj por viroj, signifanta &leftquot;donaco de Diino&rightquot; resp. &leftquot;donaco de Dio&rightquot;:
    </dif>
    <subsnc>
      <ekz>
        tri elegantaj knabinoj, Ido, Johanino kaj Anno-<tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o kaj la Sor&ccirc;isto en <nom>Oz</nom>
        <fnt>
          <aut>Baum, L. Frank</aut>
          <vrk><url ref="https://www.remush.be/teksto/Baum/doroteo-kaj-la-sor%C4%89isto-en-oz.html"
              >Doroteo kaj la Sor&ccirc;isto en Oz</url></vrk>
        </fnt>.
      </ekz>
      <trd lng="de">Dorothea</trd>
      <trd lng="en">Dorothy</trd>
      <trd lng="el">&Delta;&omega;&rho;&omikron_ton;&theta;&epsilon;&alfa;</trd>
    </subsnc>
    <subsnc>
      <ekz>
        <tld/>o <klr>[&#8230;]</klr> batadis &scirc;in preska&ubreve; &ccirc;iutage, revenante hejmen malsobre
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
      <rim>
        Anka&ubreve; la inversigita formo ekzistas: <ref tip="sin" cel="teodor.0o">Teodoro</ref>.
      </rim>
      <trd lng="de">Dorotheos</trd>
      <trd lng="en">Dorotheus</trd>
      <trd lng="el">&Delta;&omega;&rho;&omikron_ton;&theta;&epsilon;&omikron;&sigma_fina;</trd>
    </subsnc>
  </snc>
</drv>

  </art>

