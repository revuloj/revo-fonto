<drv mrk="kat.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o, propra al <tld/>o:
      <ekz>
        Helka <ind>per <tld/>aj pa&scirc;oj</ind>, kun oblikva
        rigardo &scirc;teliris al la fortepiano
        <fnt>
        <aut>E. Orzeszkowa trad. K. Bein</aut>
        <vrk>La bona sinjorino</vrk><lok>
        <url ref="http://www.esperanto.pl/page.php?tid=1196">
        http://www.esperanto.pl/page.php?tid=1196</url></lok>
        </fnt>;
        <trd lng="fr">&agrave; pas de <ind>loup</ind></trd>
      </ekz>
      <ekz>
        <ind><tld/>aj manieroj</ind> <klr>(flataj)</klr>.
        <trdgrp lng="fr">
          <trd><ind>chatterie</ind>s<klr tip="ind">
          (&ccirc;alineries)</klr></trd>,
          <trd><ind>c&acirc;linerie</ind>s</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_c;&c_ib;&c_n;&c_y;</trd>
  <trd lng="br">-<ind>kazh</ind></trd>
  <trd lng="ca">fel&iacute;</trd>
  <trdgrp lng="cs">
    <trd>ko&ccaron;i&ccaron;&iacute;</trd>,
    <trd>ko&ccaron;i&ccaron;&iacute;</trd>
  </trdgrp>
  <trd lng="de">Katzen...</trd>
  <trd lng="en">feline</trd>
  <trd lng="es">felino</trd>
  <trdgrp lng="fr">
    <trd>de <ind>chat</ind></trd>,
    <trd>f&eacute;lin<klr tip="ind"> (relatif au chat)</klr></trd>
  </trdgrp>
  <trd lng="hu">macska-</trd>
  <trd lng="io">katala</trd>
  <trd lng="ja">&#x732B;&#x306E; <pr>&#x306D;&#x3053;&#x306E;</pr></trd>
  <trd lng="nl">katten-</trd>
  <trd lng="pl">koci</trd>
  <trd lng="pt">felino</trd>
  <trd lng="ru">&c_k;&c_o;&c_sh;&c_a;&c_ch;&c_i;&c_j;</trd>
  <trd lng="sk">ma&ccaron;ac&iacute;</trd>
  <trd lng="sv">katt-</trd>
  <trd lng="uk">&c_k;&c_o;&c_t;&c_ja;&c_ch;&c_i;&c_j;</trd>
  <trd lng="vo">katik</trd>
</drv>

<drv mrk="kat.0ido">
  <kap><tld/>ido, <var><kap><tld/>idino</kap></var></kap>
  <snc>
    <dif>
      Ido de <tld/>o:
      <ekz>
        <ind>plej juna <tld/>ido musojn jam pelas</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="fr">bon <ind>chien</ind> chasse de race</trd>
        <trd lng="nl">de <ind>appel</ind> valt niet ver van de
        boom</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_c;&c_ja;&c_n;&c_ja;</trd>
  </snc>
  <trd lng="br">kazhig</trd>
  <trdgrp lng="de">
    <trd>Katzenjunges</trd>
  </trdgrp>
  <trd lng="en">kitten</trd>
  <trd lng="es">cachorro de <ind>gato</ind></trd>
  <trd lng="fr">chaton<klr tip="ind"> (petit du chat)</klr></trd>
