<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kat</rad>/o
</kap>
<drv mrk="kat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="felis.0o">feliso</ref>
      (<trd lng="la">Felis silvestris</trd>), kies hejma formo
      ekzistas en multaj varia&jcirc;oj:
      <ekz>
        <ind>kiam <tld/>o promenas, la musoj festenas</ind>
        <klr>(kiam mastro foriras, ne sin &gcirc;enas la
        servantoj)
        </klr><fnt><bib>PrV</bib></fnt>;
        <trd lng="fr">quand le <ind>chat</ind> n'est pas l&agrave;, les souris dansent</trd>
        <trd lng="hu">ha nincs otthon a macska, cincognak az egerek</trd>
      </ekz>
      <ekz>
        <ind>danci kiel <tld/>o &ccirc;irka&ubreve; poto</ind>
        <klr>(peni por pla&ccirc;i)</klr>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="fr">faire la danse du <ind>ventre</ind><klr tip="ind"> (fig., pour parvenir &agrave; ses fins)</klr></trd>
        <trd lng="hu">ker&uuml;lgeti, mint macska a forr&oacute; k&aacute;s&aacute;t</trd>
      </ekz>
      <ekz>
        <ind>vivi kun iu, kiel hundo kun <tld/>o</ind>
        <klr>(malamike)</klr>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="fr">s'entendre avec qn comme <ind>chien</ind> et chat</trd>
        <trd lng="hu">&uacute;gy &eacute;lnek, mint a kutya meg a macska</trd>
      </ekz>
      <ekz>
        <ind>kiam <tld/>o jam forman&gcirc;is, forpelado ne helpos</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="hu">es&odblac; ut&aacute;n k&ouml;p&ouml;nyeg</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_t;</trd>
  <trd lng="br">kazh</trd>
  <trd lng="ca">gat</trd>
  <trd lng="cs">ko&ccaron;ka (kocour)</trd>
  <trd lng="de">Katze</trd>
  <trd lng="en">cat</trd>
  <trd lng="fr">chat</trd>
  <trd lng="hu">macska</trd>
  <trd lng="nl">kat</trd>
  <trd lng="pl">kot</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">ma&ccaron;ka</trd>
  <trd lng="sv">katt</trd>
</drv>

<drv mrk="kat.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o, propra al <tld/>o:
      <ekz>
        Helka <ind>per <tld/>aj pa&scirc;oj</ind>, kun oblikva
        rigardo &scirc;teliris al la fortepiano
        <fnt>
        <aut>E. Orzeszkowa trad. K. Bein</aut>
        <vrk>La bona sinjorino</vrk><lok>
        <url ref="http://www.esperanto.pl/page.php?tid=1196">http://www.esperanto.pl/page.php?tid=1196</url></lok>
        </fnt>;
        <trd lng="fr">&agrave; pas de <ind>loup</ind></trd>
      </ekz>
      <ekz>
        <ind><tld/>aj manieroj</ind> <klr>(flataj)</klr>.
        <trdgrp lng="fr">
          <trd><ind>chatterie</ind>s<klr tip="ind"> (&ccirc;alineries)</klr></trd>,
          <trd><ind>c&acirc;linerie</ind>s</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_c;&c_ib;&c_n;&c_y;</trd>
  <trd lng="br">-<ind>kazh</ind></trd>
  <trd lng="ca">fel&iacute;</trd>
  <trd lng="cs">ko&ccaron;i&ccaron;&iacute;</trd>
  <trd lng="de">Katzen...</trd>
  <trd lng="en">feline</trd>
  <trdgrp lng="fr">
    <trd>de <ind>chat</ind></trd>,
    <trd>f&eacute;lin<klr tip="ind"> (relatif au chat)</klr></trd>
  </trdgrp>
  <trd lng="hu">macska-</trd>
  <trd lng="nl">katten-</trd>
  <trd lng="ru">&c_k;&c_o;&c_sh;&c_a;&c_ch;&c_i;&c_j;</trd>
  <trd lng="sk">ma&ccaron;ac&iacute;</trd>
</drv>

<drv mrk="kat.0ido">
  <kap><tld/>id(in)o</kap>
  <snc>
    <dif>
      Ido de <tld/>o:
      <ekz>
        <ind>plej juna <tld/>ido musojn jam pelas</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="fr">bon <ind>chien</ind> chasse de race</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="br">kazhig</trd>
  <trd lng="fr">chaton<klr tip="ind"> (petit du chat)</klr></trd>
  <trdgrp lng="hu">
    <trd>kismacska</trd>,
    <trd>cica</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_t;&c_jo;&c_n;&c_o;&c_k;</trd>
</drv>

<drv mrk="kat.0ino">
  <kap><tld/>ino</kap>
  <snc>
    <dif>
      Inseksa <tld/>o.
    </dif>
  </snc>
  <trd lng="br">kazhez</trd>
  <trd lng="fr"><ind>chat</ind>te</trd>
  <trd lng="hu">n&odblac;st&eacute;ny <ind>macska</ind></trd>
  <trd lng="ru">&c_k;&c_o;&c_sh;&c_k;&c_a;</trd>
</drv>

<drv mrk="kat.vir0o">
  <kap>vir<tld/>o</kap>
  <snc>
    <dif>
      Virseksa <tld/>o.
    </dif>
  </snc>
  <trd lng="br">targazh</trd>
  <trd lng="de">Kater</trd>
  <trd lng="en">tomcat</trd>
  <trdgrp lng="fr">
    <trd><ind>chat</ind> m&acirc;le</trd>,
    <trd>matou</trd>
  </trdgrp>
  <trd lng="hu">kandur</trd>
  <trd lng="ru">&c_k;&c_o;&c_t;</trd>
</drv>
</art>
