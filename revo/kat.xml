<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kat</rad>/o
</kap>
<drv mrk="kat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="felis.0o">feliso</ref>
      (<trd lng="la">Felis silvestris</trd>), kies hejma formo
      ekzistas en multaj varia&jcirc;oj:
      <ekz>
        kiam <tld/>o promenas, la musoj festenas
        <klr>(kiam mastro foriras, ne sin &gcirc;enas la
        servantoj)</klr><fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        danci kiel <tld/>o &ccirc;irka&ubreve; poto
        <klr>(peni por pla&ccirc;i)</klr><fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        vivi kun iu, kiel hundo kun <tld/>o
        <klr>(malamike)</klr><fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        kiam <tld/>o jam forman&gcirc;is, forpelado ne
        helpos<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>                 
  <trd lng="be">&c_k;&c_o;&c_t;</trd>
  <trd lng="br">kazh</trd>
  <trd lng="ca">gat</trd>
  <trd lng="cs">ko&ccaron;ka (kocour)</trd>
  <trd lng="de">Katze</trd>
  <trd lng="en">cat</trd>
  <trd lng="fr">chat</trd>
  <trd lng="hu">macska</trd>
  <trd lng="nl">kat</trd>
  <trd lng="pl">kot</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">ma&ccaron;ka</trd>
  <trd lng="sv">katt</trd>
</drv>

<drv mrk="kat.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o, propra al <tld/>o:
      <ekz>
        <tld/>aj pa&scirc;oj;
      </ekz>
      <ekz>
        <tld/>aj (flataj) manieroj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_c;&c_ib;&c_n;&c_y;</trd>      
  <trd lng="br">-<ind>kazh</ind></trd>
  <trd lng="ca">fel&iacute;</trd>
  <trd lng="cs">ko&ccaron;i&ccaron;&iacute;</trd>
  <trd lng="de">Katzen...</trd>
  <trd lng="en">feline</trd>
  <trdgrp lng="fr">
    <trd>de <ind>chat</ind></trd>,
    <trd>f&eacute;lin<klr tip="ind"> (relatif au chat)</klr></trd>
  </trdgrp>
  <trd lng="hu">macska-</trd>
  <trd lng="nl">katten-</trd>
  <trd lng="ru">&c_k;&c_o;&c_sh;&c_a;&c_ch;&c_i;&c_j;</trd>
  <trd lng="sk">ma&ccaron;ac&iacute;</trd>
</drv>

<drv mrk="kat.0ido">
  <kap><tld/>id(in)o</kap>
  <snc>
    <dif>
      Ido de <tld/>o:
      <ekz>
        plej juna <tld/>ido musojn jam pelas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc> 
  <trd lng="br">kazhig</trd>
  <trd lng="fr">chaton<klr tip="ind"> (petit du chat)</klr></trd>
  <trd lng="ru">&c_k;&c_o;&c_t;&c_jo;&c_n;&c_o;&c_k;</trd>
</drv>

<drv mrk="kat.0ino">
  <kap><tld/>ino</kap>
  <snc>
    <dif>
      Inseksa <tld/>o.
    </dif>
  </snc>                
  <trd lng="br">kazhez</trd>
  <trd lng="fr"><ind>chat</ind>te</trd>
  <trd lng="ru">&c_k;&c_o;&c_sh;&c_k;&c_a;</trd>
</drv>

<drv mrk="kat.vir0o">
  <kap>vir<tld/>o</kap>
  <snc>
    <dif>
      Virseksa <tld/>o.
    </dif>
  </snc>   
  <trd lng="br">targazh</trd>
  <trd lng="de">Kater</trd>
  <trd lng="en">tomcat</trd>
  <trdgrp lng="fr">
    <trd>chat m&acirc;le</trd>,
    <trd>matou</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_t;</trd>
</drv>
</art>
