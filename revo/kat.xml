  <trd lng="ro">pisoi</trd>
  <trd lng="ru">&c_k;&c_o;&c_t;&c_jo;&c_n;&c_o;&c_k;</trd>
  <trd lng="sv">kattunge</trd>
  <trd lng="uk">&c_k;&c_o;&c_sh;&c_je;&c_n;&c_ja;</trd>
