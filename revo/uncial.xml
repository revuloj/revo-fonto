<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: uncial.xml,v 1.7 2023/12/28 13:31:45 revo Exp $">
    <kap>
      <rad>uncial</rad>/o <fnt><bib>PIV2</bib></fnt>
    </kap>
    <drv mrk="uncial.0o">
      <kap><tld/>o</kap>
      <snc>
        <uzo tip="fak">HIS</uzo>
        <uzo tip="fak">TIP</uzo>
        <dif>
          Speco de <ref tip="super" cel="majuskl.0a">majuskla</ref>
          skribo, kiu estis uzata en la manuskriptoj grekaj kaj
          latinaj de la 4a &gcirc;is la 8a jc:
          <ekz>
            oni povas facile vidi, ke kirilico entenas la 24 literojn
            de la bizancia <tld/>o, plus la literojn por la slavonaj
            sonoj kiuj malestas en la mezgreka
            <fnt>
              <aut>Sergio Pokrovskij</aut>:
              <vrk>Komputada leksikono</vrk>,
              <lok><url
              ref="http://bertilow.com/div/komputada_leksikono/AZBUKOJ.html#Pri-kirilico">Pri
              kirilico</url></lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_u;&c_n;&c_c;&c_y;&c_ja;&c_l;</trd>,
        <trd>&c_u;&c_n;&c_c;&c_y;&c_ja;&c_l;&c_mol;&c_n;&c_a;&c_je; &c_p;&c_ib;&c_s;&c_mol;&c_m;&c_o;</trd>
      </trdgrp>
  <trd lng="cs">unci&#xE1;la</trd>
  <trd lng="de">Unziale</trd>
  <trd lng="en">uncial script</trd>
  <trd lng="fr">onciale</trd>
  <trd lng="pl">uncja&lstroke;a</trd>
  <trd lng="ru">&c_u;&c_n;&c_c;&c_i;&c_a;&c_l;</trd>
  <trd lng="sk">unci&#xE1;la</trd>
  <trd lng="uk">&c_u;&#x43D;&c_ib;&c_c;&c_ib;&c_a;&#x43B;&#x44C;&#x43D;&c_a; &#x43B;&c_ib;&c_t;&c_je;&c_r;&c_a;</trd>
</drv>
  </art>

