<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aristotel.xml,v 1.18 2021/02/28 18:35:45 revo Exp $">
<kap>
  <rad>Aristotel</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="aristotel.0o">
  <kap><tld/>o</kap>
  <snc mrk="aristotel.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> grekdevena.
    </dif>
  </snc>
  <snc mrk="aristotel.0o.PERS">
    <uzo tip="fak">PERS</uzo>
    <dif>
      Greklingve <trd lng="grc">&Alfa_acute;&rho;&jota;&sigma;&tau;&omikron;&tau;&epsilon_acute;&lambda;&eta;&sigma_fina;</trd>,
      helena <ref tip="lst" cel="filozo.0o" lst="voko:historiaj_personoj">filozofo</ref> kaj sciencisto (384 &ndash;
      322 a. K.),
      dis&ccirc;iplo de <ref tip="vid" cel="platon.0o.PERS">Platono</ref>:
      <ekz>
        la&ubreve; <tld/>o estas &leftquot;nature&rightquot;
        inter&scirc;an&gcirc;i unu konsumvaron kontra&ubreve; alia
        konsumvaro
        <fnt>
        <aut>Bo Sandelin</aut>,
        <vrk>Anta&ubreve;merkantilisma ekonomia pensado</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2001a, numero 2a, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_A;&c_r;&c_y;&c_s;&c_t;&c_o;&c_t;&c_e;&c_l;&c_mol;</trd>
  <trd lng="bg">&c_A;&c_r;&c_i;&c_s;&c_t;&c_o;&c_t;&c_je;&c_l;</trd>
  <trd lng="br">Aristoteles</trd>
  <trd lng="cs">Aristoteles</trd>
  <trd lng="de">Aristoteles</trd>
  <trd lng="en">Aristotle</trd>
  <trd lng="es">Arist&oacute;teles</trd>
  <trd lng="fr">Aristote</trd>
  <trd lng="he">&#x5D0;&#x5E8;&#x5D9;&#x5E1;&#x5D8;&#x5D5;</trd>
  <trd lng="hu">Arisztotel&eacute;sz</trd>
  <trd lng="it">Aristotele</trd>
  <trd lng="nl">Aristoteles</trd>
  <trd lng="pl">Arystoteles</trd>
  <trd lng="ru">&c_A;&c_r;&c_i;&c_s;&c_t;&c_o;&c_t;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">Aristoteles</trd>
</drv>
</art>
