<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aristotel.xml,v 1.22 2023/10/28 08:33:08 revo Exp $">
<kap>
  <rad>Aristotel</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="aristotel.0o">
  <kap><tld/>o</kap>
  <snc mrk="aristotel.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> grekdevena.
    </dif>
  </snc>
  <snc mrk="aristotel.0o.PERS">
    <uzo tip="fak">PERS</uzo>
    <dif>
      Greklingve <trd lng="grc">&Alfa_acute;&rho;&jota;&sigma;&tau;&omikron;&tau;&epsilon_acute;&lambda;&eta;&sigma_fina;</trd>,
      helena <ref tip="lst" cel="filozo.0o" lst="voko:historiaj_personoj">filozofo</ref> kaj sciencisto (384 &ndash;
      322 a. K.),
      dis&ccirc;iplo de <ref tip="vid" cel="platon.0o.PERS">Platono</ref>:
      <ekz>
        siajn supozojn, naskitajn per la legado de Aristotelo, Plinio, Strabono, li
        fortigis per multo da faktoj, kiujn li kolektis
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o instruas nin, sinjoro, ke oni devas oferi aferojn malpli grandajn por la
        pli grandaj
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        se bona oratoro konvinkas &ndash; diras <tld/>o &ndash; signifas ke eblas eltrovi la
        ka&ubreve;zojn, pro kiuj li atingas tiun sukceson
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve; <tld/>o estas &leftquot;nature&rightquot; inter&scirc;an&gcirc;i unu konsumvaron kontra&ubreve; alia
        konsumvaro
        <fnt>
          <aut>Bo Sandelin</aut>,
          <vrk>Anta&ubreve;merkantilisma ekonomia pensado</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2001a, numero 2a, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x4E9A;&#x91CC;&#x58EB;&#x591A;&#x5FB7; <pr>y&#xE0;l&#x1D0;sh&#xEC;du&#x14D;d&#xE9;</pr></trd>,
    <trd>&#x4E9E;&#x91CC;&#x58EB;&#x591A;&#x5FB7; <pr>y&#xE0;l&#x1D0;sh&#xEC;du&#x14D;d&#xE9;</pr></trd>,
    <trd>&#x4E9A;&#x91CC;&#x65AF;&#x591A;&#x5FB7; <pr>y&#xE0;l&#x1D0;s&#x12B;du&#x14D;d&#xE9;</pr></trd>,
    <trd>&#x4E9E;&#x91CC;&#x65AF;&#x591A;&#x5FB7; <pr>y&#xE0;l&#x1D0;s&#x12B;du&#x14D;d&#xE9;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_A;&c_r;&c_y;&c_s;&c_t;&c_o;&c_t;&c_e;&c_l;&c_mol;</trd>
  <trd lng="bg">&c_A;&c_r;&c_i;&c_s;&c_t;&c_o;&c_t;&c_je;&c_l;</trd>
  <trd lng="br">Aristoteles</trd>
  <trd lng="cs">Aristoteles</trd>
  <trd lng="de">Aristoteles</trd>
  <trd lng="en">Aristotle</trd>
  <trd lng="es">Arist&oacute;teles</trd>
  <trd lng="fr">Aristote</trd>
  <trd lng="he">&alef;&resh;&yod;&samekh;&tet;&vav;</trd>
  <trd lng="hu">Arisztotel&eacute;sz</trd>
  <trd lng="it">Aristotele</trd>
  <trd lng="ja">&#x30A2;&#x30EA;&#x30B9;&#x30C8;&#x30C6;&#x30EC;&#x30B9;</trd>
  <trd lng="nl">Aristoteles</trd>
  <trd lng="pl">Arystoteles</trd>
  <trd lng="ru">&c_A;&c_r;&c_i;&c_s;&c_t;&c_o;&c_t;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">Aristoteles</trd>
  <trd lng="uk">&#x410;&#x440;&#x456;&#x441;&#x442;&#x43E;&#x442;&#x435;&#x43B;&#x44C;</trd>
</drv>
</art>
