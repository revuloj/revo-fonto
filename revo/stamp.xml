<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>stamp</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="stamp.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="stamp.0i.reliefe">
    <dif>
      Fari reliefan markon sur io per frapo:
      <ekz>
        <tld/>i medalon, monerojn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="matric.0o">matrico</ref>,
      <ref cel="patric.0o">patrico</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_sh;&c_t;&c_a;&c_m;&c_p;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_ch;&c_a;&c_k;&c_a;&c_n;&c_ib;&c_c;&c_mol; &c_m;&c_a;&c_n;&c_e;&c_t;&c_u;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>ver<klr tip="amb"> (p&eacute;nzt)</klr></trd>,
      <trd>dombor&iacute;t</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_sh;&c_t;&c_a;&c_m;&c_p;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_b;&c_i;&c_t;&c_mol;<klr tip="amb"> (&c_m;&c_o;&c_n;&c_je;&c_t;&c_u;)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="stamp.0i.presi">
    <dif>
      Fari <tld/>on<sncref ref="stamp.0o.presajxo"/>:
      <ekz>
        <tld/>i po&scirc;tkarton, rabatilon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gravur.0i">gravuri</ref>,
      <ref cel="pres.0i">presi</ref>,
      <ref cel="sign.0i">signi</ref>,
      <ref cel="sigel.0i">sigeli</ref>.
    </refgrp>
    <trd lng="be">&c_sh;&c_t;&c_a;&c_m;&c_p;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="hu">
      <trd>b&eacute;lyegez</trd>,
      <trd>leb&eacute;lyegez</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_sh;&c_t;&c_a;&c_m;&c_p;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_sh;&c_t;&c_je;&c_m;&c_p;&c_je;&c_l;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="stamp.0i.menso">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Estigi en la spirito, koro a&ubreve; konduto
      de iu da&ubreve;ran kutimi&gcirc;on:
      <ekz>
        <tld/>i ion en sia memoro.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_h;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_k;&c_ib;&c_n;&c_u;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="hu">bev&eacute;s<klr tip="amb"> (&aacute;tv.)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_l;&c_je;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="stamp.0i.karaktero">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Karakterizi ion:
      <ekz>
        &scirc;ia delikateco kaj &scirc;ia kostumo <tld/>is &scirc;in
        per profunda disonanco
        kun la fono<fnt>B</fnt>
        <klr>(medio)</klr>;
      </ekz>
      <ekz>
        ilia propagando estas <tld/>ita per bonega organizado kaj
        metodo.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_h;&c_a;&c_r;&c_a;&c_k;&c_t;&c_a;&c_r;&c_y;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_d;&c_z;&c_n;&c_a;&c_ch;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="hu">jellemez</trd>
    <trdgrp lng="ru">
      <trd>&c_h;&c_a;&c_r;&c_a;&c_k;&c_t;&c_je;&c_r;&c_i;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_m;&c_je;&c_ch;&c_a;&c_t;&c_mol;<klr> (&c_ch;&c_je;&c_m;-&c_l;.)</klr></trd>,
      <trd>&c_z;&c_a;&c_r;&c_je;&c_k;&c_o;&c_m;&c_je;&c_n;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>razit mince</trd>,
    <trd>razítkovat</trd>
  </trdgrp>
  <trd lng="de">pr&auml;gen</trd>
  <trd lng="en">stamp</trd>
  <trd lng="es">acu&ntilde;ar</trd>
  <trdgrp lng="fr">
    <trd>affranchir</trd>,
    <trd>estampiller</trd>
  </trdgrp>
  <trd lng="nl">stempelen</trd>
  <trdgrp lng="sk">
    <trd>dať pečať</trd>,
    <trd>opečiatkovať</trd>,
    <trd>raziť <klr>(peniaze)</klr></trd>
  </trdgrp>
  <trd lng="sv">st&auml;mpla</trd>
</drv>

<drv mrk="stamp.0o">
  <kap><tld/>o</kap>
  <snc mrk="stamp.0o.presajxo">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Speco de oficiala identiga marko, farita sur dokumento per
      speciala inko kaj <tld/>ilo.
    </dif>
    <ref tip="vid" cel="sigel.0o">sigelo</ref>
    <trdgrp lng="be">
      <trd>&c_p;&c_ja;&c_ch;&c_a;&c_t;&c_k;&c_a;</trd>,
      <trd>&c_sh;&c_t;&c_a;&c_m;&c_p;</trd>,
      <trd>&c_sh;&c_t;&c_e;&c_m;&c_p;&c_e;&c_l;&c_mol;</trd>,
    </trdgrp>
    <trd lng="es">acu&ntilde;aci&oacute;n</trd>
    <trd lng="nl">stempel<klr tip="ind"> (afdruk)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_je;&c_ch;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_sh;&c_t;&c_je;&c_m;&c_p;&c_je;&c_l;&c_mol;</trd>,
      <trd>&c_sh;&c_t;&c_a;&c_m;&c_p;</trd>,
      <trd>&c_k;&c_l;&c_je;&c_j;&c_m;&c_o;</trd>
    </trdgrp>
  </snc>
  <snc mrk="stamp.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Karakterizilo:
      <ekz>
        lia tuta politiko portas la <tld/>on de perfido kaj mensogo;
      </ekz>
      <ekz>
        homo kun tiu <tld/>o, kiun donas la longaj rilatoj kun la altaj
        klasoj de la societo<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_d;&c_b;&c_ib;&c_t;&c_a;&c_k;</trd>,
      <trd>&c_v;&c_o;&c_d;&c_c;&c_ib;&c_s;&c_k;</trd>
    </trdgrp>
    <trd lng="de">Stempel</trd>
    <trdgrp lng="en">
      <trd>stamp</trd>,
      <trd>mark</trd>
    </trdgrp>
    <trd lng="nl">merkteken</trd>
    <trd lng="ru">&c_o;&c_t;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_o;&c_k;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>otisk razítka</trd>,
    <trd>ražba</trd>,
    <trd>známka</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cachet<klr> (estampage)</klr></trd>,
    <trd>flamme postale</trd>,
    <trd>tampon<klr> (trace)</klr></trd>
  </trdgrp>
  <trd lng="hu">b&eacute;lyeg<klr tip="amb"> (&aacute;tv. is)</klr></trd>
  <trdgrp lng="sk">
    <trd>biľag</trd>,
    <trd>pečať <klr>(vosková)</klr></trd>,
    <trd>pečiatka (na známke)</trd>
  </trdgrp>
</drv>

<drv mrk="stamp.brul0i">
  <kap>brul<tld/>i</kap>
  <snc mrk="stamp.brul0i.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Marki la ha&ubreve;ton per brula&jcirc;o, ordinare tiacela fera&jcirc;o:
      <ekz>
        viroj estis kastritaj, blindigitaj je unu okulo per arda fero, 
        kaj en &ccirc;enoj, kun brul<tld/>o sur la frunto
        <fnt>
          <aut>I. Efremov, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Efremov/horo.html">
          Horo de Bovo</url></vrk>,
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">vypálit značku</trd>
  <trd lng="fr">marquer au fer</trd>
  <trd lng="sk">vypáliť značku</trd>
</drv>
</art>
