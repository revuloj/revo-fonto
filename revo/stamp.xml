<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: stamp.xml,v 1.24 2020/10/26 10:10:56 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>stamp</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="stamp.0o">
  <kap><tld/>o</kap>
  <snc mrk="stamp.0o.presajxo">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Speco de oficiala identiga marko, farita sur dokumento per
      speciala inko kaj <tld/>ilo.
    </dif>
    <ref tip="vid" cel="sigel.0o.TIP">sigelo<sncref/></ref>
    <trdgrp lng="be">
      <trd>&c_p;&c_ja;&c_ch;&c_a;&c_t;&c_k;&c_a;</trd>,
      <trd>&c_sh;&c_t;&c_a;&c_m;&c_p;</trd>,
      <trd>&c_sh;&c_t;&c_e;&c_m;&c_p;&c_e;&c_l;&c_mol;</trd>,
    </trdgrp>
    <trd lng="es">acu&ntilde;aci&oacute;n</trd>
    <trd lng="nl">stempel<klr tip="ind"> (afdruk)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_je;&c_ch;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_sh;&c_t;&c_je;&c_m;&c_p;&c_je;&c_l;&c_mol;</trd>,
      <trd>&c_sh;&c_t;&c_a;&c_m;&c_p;</trd>,
      <trd>&c_k;&c_l;&c_je;&c_j;&c_m;&c_o;</trd>
    </trdgrp>
  </snc>
  <snc mrk="stamp.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Karakterizilo:
      <ekz>
        lia tuta politiko portas la <tld/>on de perfido kaj mensogo;
      </ekz>
      <ekz>
        homo kun tiu <tld/>o, kiun donas la longaj rilatoj kun la altaj
        klasoj de la societo<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_d;&c_b;&c_ib;&c_t;&c_a;&c_k;</trd>,
      <trd>&c_v;&c_o;&c_d;&c_c;&c_ib;&c_s;&c_k;</trd>
    </trdgrp>
    <trd lng="de">Stempel</trd>
    <trdgrp lng="en">
      <trd>stamp</trd>,
      <trd>mark</trd>
    </trdgrp>
    <trd lng="nl">merkteken</trd>
    <trd lng="ru">&c_o;&c_t;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_o;&c_k;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>otisk raz&iacute;tka</trd>,
    <trd>ra&zcaron;ba</trd>,
    <trd>zn&aacute;mka</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cachet<klr> (estampage)</klr></trd>,
    <trd>flamme postale</trd>,
    <trd>tampon<klr> (trace)</klr></trd>
  </trdgrp>
  <trd lng="hu">b&eacute;lyeg<klr tip="amb"> (&aacute;tv. is)</klr></trd>
  <trdgrp lng="sk">
    <trd>bi&lcaron;ag</trd>,
    <trd>pe&ccaron;a&tcaron; <klr>(voskov&aacute;)</klr></trd>,
    <trd>pe&ccaron;iatka (na zn&aacute;mke)</trd>
  </trdgrp>
</drv>

<drv mrk="stamp.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="stamp.0i.materie">
    <dif>
      <ref tip="super" cel="mark.0i.iu">Marki</ref>
      per alfrapita desegno a&ubreve; emblemo rekoniga,
      a&ubreve;tentiga, validiga...
    </dif>
    <subsnc mrk="stamp.0i.reliefe">
      <dif>
        Almeti reliefan markon per frapo:
        <ekz>
          <tld/>i medalon, monerojn.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="gravur.0i">gravuri</ref>,
        <ref cel="sigel.0i">sigeli</ref>.
      </refgrp>
      <trdgrp lng="be">
        <trd>&c_sh;&c_t;&c_a;&c_m;&c_p;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_ch;&c_a;&c_k;&c_a;&c_n;&c_ib;&c_c;&c_mol; &c_m;&c_a;&c_n;&c_e;&c_t;&c_u;</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>ver<klr tip="amb"> (p&eacute;nzt)</klr></trd>,
        <trd>dombor&iacute;t</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_sh;&c_t;&c_a;&c_m;&c_p;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_b;&c_i;&c_t;&c_mol;<klr tip="amb"> (&c_m;&c_o;&c_n;&c_je;&c_t;&c_u;)</klr></trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="stamp.0i.presi">
      <dif>
        Almeti inkan markon per frapo:
        <ekz>
          <tld/>i po&scirc;tkarton, rabatilon.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="pres.0i">presi</ref>.
      </refgrp>
      <trd lng="be">&c_sh;&c_t;&c_a;&c_m;&c_p;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
      <trdgrp lng="hu">
        <trd>b&eacute;lyegez</trd>,
        <trd>leb&eacute;lyegez</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_sh;&c_t;&c_a;&c_m;&c_p;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_sh;&c_t;&c_je;&c_m;&c_p;&c_je;&c_l;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
    </subsnc>
  </snc>
  <snc mrk="stamp.0i.menso">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Estigi en la spirito, koro a&ubreve; konduto
      de iu da&ubreve;ran kutimi&gcirc;on:
      <ekz>
        <tld/>i ion en sia memoro.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_h;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_k;&c_ib;&c_n;&c_u;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="hu">bev&eacute;s<klr tip="amb"> (&aacute;tv.)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_l;&c_je;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="stamp.0i.karaktero">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Karakterizi, distingi:
      <ekz>
        &scirc;ia kostumo, la delikateco de &scirc;ia viza&gcirc;o, movoj kaj manoj,
        <tld/>is &scirc;in per profunda disonanco kun la
        fono <klr>(medio)</klr>
        <fnt>
          <aut>E. Orzeszkowa, trad. Kabe</aut>,
          <vrk><url
          ref="http://eduinf.waw.pl/esp/lern/librejo/0009/0003.php">
          Bona sinjorino</url></vrk>,
          <lok>1909</lok>
        </fnt>;
      </ekz>
      <ekz>
        ilia propagando estas <tld/>ita per bonega organizado kaj
        metodo.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_h;&c_a;&c_r;&c_a;&c_k;&c_t;&c_a;&c_r;&c_y;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_d;&c_z;&c_n;&c_a;&c_ch;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="hu">jellemez</trd>
    <trdgrp lng="ru">
      <trd>&c_h;&c_a;&c_r;&c_a;&c_k;&c_t;&c_je;&c_r;&c_i;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_m;&c_je;&c_ch;&c_a;&c_t;&c_mol;<klr> (&c_ch;&c_je;&c_m;-&c_l;.)</klr></trd>,
      <trd>&c_z;&c_a;&c_r;&c_je;&c_k;&c_o;&c_m;&c_je;&c_n;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>razit mince</trd>,
    <trd>raz&iacute;tkovat</trd>
  </trdgrp>
  <trd lng="de">pr&auml;gen</trd>
  <trd lng="en">stamp</trd>
  <trd lng="es">acu&ntilde;ar</trd>
  <trdgrp lng="fr">
    <trd>affranchir</trd>,
    <trd>estampiller</trd>
  </trdgrp>
  <trd lng="nl">stempelen</trd>
  <trdgrp lng="sk">
    <trd>da&tcaron; pe&ccaron;a&tcaron;</trd>,
    <trd>ope&ccaron;iatkova&tcaron;</trd>,
    <trd>razi&tcaron; <klr>(peniaze)</klr></trd>
  </trdgrp>
  <trd lng="sv">st&auml;mpla</trd>
</drv>

<drv mrk="stamp.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="stamp.0ilo.TEK">
    <dif>
      Ilo portanta sur sia ekstremo reliefan
      desegnon alfrapeblan: 
      <ekz>
        en eta po&scirc;toficejo de la urbeto po&scirc;tistino
	&ccirc;iun matenon enpikas <tld/>ilon por &scirc;an&gcirc;i
	ties daton
        <fnt>
          <aut>S. &Scaron;timec</aut>,
          <vrk>Zagrebaj solistoj en Bonaero</vrk>,
          <bib>Monato</bib>,
          <lok>2001:7</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="matric.0o">matrico</ref>,
      <ref cel="patric.0o">patrico</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>affranchisseuse</trd>,
    <trd>tampon encreur</trd>
  </trdgrp>
</drv>

<drv mrk="stamp.brul0i">
  <kap>brul<tld/>i</kap>
  <snc mrk="stamp.brul0i.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      <ref tip="dif" cel="mark.brul0i">Brulmarki</ref>:
      <ekz>
        viroj estis kastritaj, blindigitaj je unu okulo per arda fero, 
        kaj en &ccirc;enoj, kun brul<tld/>o sur la frunto
        <fnt>
          <aut>I. Efremov, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Efremov/horo.html">
          Horo de Bovo</url></vrk>,
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_l;&c_je;&c_j;&c_m;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_l;&c_ja;&c_j;&c_m;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_t;&c_a;&c_w;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_zh;&c_i;&c_g;&c_o;&c_s;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_k;&c_l;&c_je;&c_j;&c_m;&c_o;&c_s;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="cs">vyp&aacute;lit zna&ccaron;ku</trd>
  <trd lng="de">brandmarken</trd>
  <trd lng="en">brand</trd>
  <trd lng="es">marcar a fuego</trd>
  <trd lng="fr">marquer au fer</trd>
  <trd lng="hu">megb&eacute;lyegez<klr tip="ind">
  (billoggal)</klr></trd>
  <trd lng="nl">brandmerken</trd>
  <trd lng="pt">marcar a fogo</trd>
  <trd lng="ru">&c_k;&c_l;&c_je;&c_j;&c_m;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">vyp&aacute;li&tcaron; zna&ccaron;ku</trd>
</drv>

<drv mrk="stamp.brul0ilo">
  <kap>brul<tld/>ilo</kap>
  <snc mrk="stamp.brul0ilo.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      <ref tip="dif" cel="mark.brul0i">Brulmarkilo</ref>:
      <ekz>
        kiam li, kunpremante siajn lipojn, rigardis iun, tiu eksentis 
        korpremon kvaza&ubreve; bruligite de brul<tld/>ilo
        <fnt>
          <aut>Mo Yan, trad. Vejdo</aut>
          <vrk>Travidebla Ru&gcirc;a Rafano</vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Brandeisen</trd>
  <trd lng="en">branding iron</trd>
  <trd lng="es">hierro de marcar a fuego<klr> (el ganado)</klr></trd>
  <trd lng="fr">fer<klr> (&agrave; marquer le b&eacute;tail)</klr></trd>
  <trd lng="hu">b&eacute;lyegz&odblac;vas</trd>
  <trd lng="nl">schroei-ijzer</trd>
  <trd lng="pt">marcador</trd>
  <trd lng="ru">&c_k;&c_l;&c_je;&c_j;&c_m;&c_o;</trd>
</drv>
</art>
