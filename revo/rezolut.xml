<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>rezolut</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="rezolut.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <ref tip="super" cel="firm.0a.morale">Firma</ref> pri siaj intencoj,
      e&ccirc; koste de penoj por ilia realigo:
      <ekz>
        kun digna kaj <tld/>a mieno li sin firmigis sur la
        piedingoj<fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 4a, p.
        47a</lok></fnt>
      </ekz>
    </dif>
    <ref tip="ant" cel="hezit.0ema">hezitema</ref>
  </snc>
  <trdgrp lng="fr">
    <trd>d&eacute;cid&eacute;<klr> (r&eacute;solu)</klr></trd>,
    <trd>r&eacute;solu<klr> (ferme)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="rezolut.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      <ref tip="super" cel="hezit.sen0e">Senhezite</ref>, ja volante
      plenumi decidita&jcirc;on.
    </dif>
  </snc>
  <trd lng="fr">r&eacute;solument</trd>
</drv>
</art>

<!--
$Log$
