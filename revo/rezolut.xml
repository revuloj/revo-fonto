<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rezolut.xml,v 1.17 2024/04/13 20:15:53 revo Exp $">
<kap>
  <rad>rezolut</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="rezolut.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <ref tip="super" cel="firm.0a.morale">Firma</ref> pri siaj
      intencoj,
      e&ccirc; koste de penoj por ilia realigo:
      <ekz>
        kun digna kaj <tld/>a mieno li sin firmigis sur la
        piedingoj<fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 4a, p.
        47a</lok></fnt>
      </ekz>
    </dif>
    <ref tip="ant" cel="hezit.0ema">hezitema</ref>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_sh;&c_u;&c_ch;&c_y;</trd>
  <trdgrp lng="ca">
    <trd>decidit</trd>,
    <trd>resolut</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>rezolutn&iacute;</trd>,
    <trd>rozhodn&yacute;</trd>,
    <trd>r&aacute;zn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>resolut</trd>,
    <trd>entschlossen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>determined</trd>,
    <trd>resolute</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_m;&a_S;&a_m;&a_m;</trd>,
    <trd>&a_th;&a_A;&a_b;&a_t;&zwnj;&a_q;&a_d;&a_m;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;cid&eacute;<klr> (r&eacute;solu)</klr></trd>,
    <trd>r&eacute;solu<klr> (ferme)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hat&aacute;rozott</trd>,
    <trd>elsz&aacute;nt</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>resoluut</trd>,
    <trd>vastberaden</trd>,
    <trd>zelfzeker</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rezolutny</trd>,
    <trd>zdecydowany</trd>
  </trdgrp>
  <trd lng="pt">resoluto</trd>
  <trd lng="ru">&c_r;&c_je;&c_sh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>rozhodn&yacute;</trd>,
    <trd>r&aacute;zny</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x442;&#x432;&#x435;&#x440;&#x434;&#x438;&#x439;</trd>,
    <trd>&#x440;&#x456;&#x448;&#x443;&#x447;&#x438;&#x439;</trd>,
    <trd>&#x43D;&#x435;&#x43F;&#x43E;&#x445;&#x438;&#x442;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x43D;&#x430;&#x43F;&#x43E;&#x43B;&#x435;&#x433;&#x43B;&#x438;&#x432;&#x438;&#x439;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x575A;&#x5B9A; <pr>ji&#x101;nd&#xEC;ng</pr></trd>,
    <trd>&#x5805;&#x5B9A; <pr>ji&#x101;nd&#xEC;ng</pr></trd>,
    <trd>&#x6BC5; <pr>y&#xEC;</pr></trd>,
    <trd>&#x51B3;&#x5FC3; <pr>ju&#xE9;x&#x12B;n</pr></trd>,
    <trd>&#x6C7A;&#x5FC3; <pr>ju&#xE9;x&#x12B;n</pr></trd>,
    <trd>&#x575A;&#x51B3; <pr>ji&#x101;nju&#xE9;</pr></trd>,
    <trd>&#x5805;&#x6C7A; <pr>ji&#x101;nju&#xE9;</pr></trd>,
    <trd>&#x4EB9; <pr>w&#x11B;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rezolut.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      <ref tip="super" cel="hezit.sen0e">Senhezite</ref>, ja volante
      plenumi decidita&jcirc;on.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_sh;&c_u;&c_ch;&c_a;</trd>,
    <trd>&c_c;&c_mol;&c_v;&c_jo;&c_r;&c_d;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">decididament</trd>
  <trdgrp lng="de">
    <trd>resolut</trd>,
    <trd>entschlossen</trd>
  </trdgrp>
  <trd lng="en">resolutely</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&a_S;&a_m;&a_m;</trd>,
    <trd>&a_q;&a_A;&a_T;&a_ayn;&a_A;&a_n;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">r&eacute;solument</trd>
  <trdgrp lng="hu">
    <trd>hat&aacute;rozottan</trd>,
    <trd>elsz&aacute;ntan</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>resoluut</trd>,
    <trd>vastberaden</trd>,
    <trd>zelfzeker</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rezolutnie</trd>,
    <trd>zdecydowanie</trd>
  </trdgrp>
  <trd lng="pt">resolutamente</trd>
  <trd lng="ru">&c_r;&c_je;&c_sh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;</trd>
</drv>
</art>

