<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: krev.xml,v 1.21 2022/04/11 16:40:26 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>krev</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="krev.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="krev.0i.rompigxi">
    <dif>
      Subite rompi&gcirc;i pro interna tro granda premforto:
      <ekz>
        la diablo ridadis tiel, ke al li preska&ubreve; <tld/>is la
        ventro
        <fnt>
          <bib>Fab2</bib>,
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>as bombo, veziko, pafilo, pne&ubreve;matikoj de bicikleto;
      </ekz>
      <ekz>
        feli&ccirc;o vezike sin levas, sed balda&ubreve; falas kaj
        <tld/>as<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        maro &ccirc;iujn riverojn ricevas kaj tamen ne
        <tld/>as<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        de tro multa scio <tld/>as la kranio
        <fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la &gcirc;is ekstremeco stre&ccirc;ita kordo de sentoj <tld/>is<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        malgra&ubreve; la naciismo <tld/>as la landlimoj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="eksplo.0i">eksplodi</ref>,
      <ref cel="krak.0i">kraki</ref>,
      <ref cel="split.0igxi.KOMUNE">spliti&gcirc;i</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>bersten</trd>,
      <trd>zerspringen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>crever<klr tip="ind"> (intransitif)</klr></trd>,
      <trd>&eacute;clater</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>barsten</trd>,
      <trd>springen<klr tip="ind"> (barsten)</klr></trd>,
      <trd>creperen</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>p&eogonek;ka&cacute;</trd>,
      <trd>rozsadza&cacute;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>estourar</trd>,
      <trd>explodir</trd>,
      <trd>pipocar</trd>
    </trdgrp>
  </snc>
  <snc mrk="krev.0i.fendigxi">
    <dif>
      Fendi&gcirc;i pro malvarmo, sekeco a&ubreve; alia efiko.
      <ekz>
        bulketoj, kiuj <tld/>is de abundego de sekvinberoj
        <fnt>
          <bib>Fab2</bib>,
          <lok>Dio de dormo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>platzen</trd>,
      <trd>aufplatzen</trd>,
      <trd>Risse bekommen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>se <ind>crevasser</ind></trd>,
      <trd>se <ind>fendre</ind></trd>,
      <trd>gercer</trd>
    </trdgrp>
    <trd lng="nl">barsten</trd>
    <trdgrp lng="pl">
      <trd>p&eogonek;ka&cacute;</trd>,
      <trd>trzaska&cacute;<klr> (np. o mrozie)</klr></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>rachar</trd>,
      <trd>fender-se</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_o;&c_p;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_t;&c_r;&c_e;&c_s;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_a;&c_r;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_a;&c_l;&c_o;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>praskat</trd>,
    <trd>pukat</trd>,
    <trd>rozpraskat</trd>,
    <trd>rozpuknout</trd>
  </trdgrp>
  <trd lng="en">burst</trd>
  <trdgrp lng="es">
    <trd>quebrarse</trd>,
    <trd>romperse</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_p;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_l;&c_o;&c_p;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_s;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_s;&c_k;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_o;&c_r;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>praska&tcaron;</trd>,
    <trd>puka&tcaron;</trd>,
    <trd>rozpuka&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_l;&c_o;&c_p;&c_n;&c_u;&c_t;&c_i;</trd>,
    <trd>&c_l;&c_u;&c_s;&c_n;&c_u;&c_t;&c_i;</trd>,
    <trd>&c_t;&c_r;&c_ib;&c_s;&c_n;&c_u;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_ib;&c_r;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja; (&c_p;&c_r;&c_o; &c_s;&c_n;&c_a;&c_r;&c_ja;&c_d;)</trd>
  </trdgrp>
</drv>

<drv mrk="krev.0o">
  <kap><tld/>o,
  <var><kap><tld/>a&jcirc;o</kap></var></kap>
  <snc mrk="krev.0o.fendo">
    <dif>
      Fendo plimalpli profunda en io, kio <tld/>is:
      <ekz>
        <tld/>oj de la muro
        <fnt>
          <bib>Fab4</bib>,
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj
          filinoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a&jcirc;oj en la manoj
        <klr>(ka&ubreve;zitaj de malvarmo)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_k;&c_o;&c_l;&c_ib;&c_n;&c_a;</trd>
  <trd lng="bo">&#3942;&#3962;&#3938;&#3851;&#3905;&#3851;</trd>
  <trdgrp lng="de">
      <trd>Spalt</trd>,
      <trd>Riss</trd>,
      <trd>Furche</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>burst</trd>,
    <trd>crack</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>quebradura</trd>,
    <trd>ruptura</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>crevasse</trd>,
    <trd>ger&ccedil;ure</trd>,
    <trd>l&eacute;zarde</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>p&eogonek;kni&eogonek;cie</trd>,
    <trd>rysa</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>barst</trd>,
    <trd>spleet</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_shch;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_z;&c_r;&c_i;&c_v;</trd>,
    <trd>&c_t;&c_r;&c_ib;&c_shch;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="krev.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="krev.0igi.rompi">
    <dif>
      Fari, ke io <tld/>u:
      <ekz>
        <tld/>igi nukson, &scirc;tonon per pulvo, absceson, pordon;
      </ekz>
      <ekz>
        <ind><tld/>iga malvarmo</ind>
        <klr>(tiel kruda, ke &scirc;tonoj <tld/>as)</klr>;
        <trd lng="ru">&c_t;&c_r;&c_je;&c_s;&c_k;&c_u;&c_ch;&c_i;&c_j;
        &c_m;&c_o;&c_r;&c_o;&c_z;</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tio <tld/>igas la koron
        <klr>(pro dolora kompato)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxir">kor&scirc;iri</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_l;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>drtit</trd>,
    <trd>rozb&iacute;jet</trd>,
    <trd>rozdrtit</trd>,
    <trd>t&rcaron;&iacute;&scaron;tit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>knacken</trd>,
    <trd>aufbrechen</trd>,
    <trd>sprengen</trd>
  </trdgrp>
  <trd lng="en">burst</trd>
  <trdgrp lng="es">
    <trd>quebrar</trd>,
    <trd>romper</trd>
  </trdgrp>
  <trdgrp lng="fr">
      <trd>crever<klr tip="ind"> (transitif)</klr></trd>,
      <trd>faire &eacute;clater</trd>
  </trdgrp>
  <trd lng="nl">doen <ind>barsten</ind></trd>
  <trdgrp lng="pl">
    <trd>powodowa&cacute; <ind>p&eogonek;kanie</ind></trd>,
    <trd>rozsadzi&cacute;</trd>,
    <trd>roztrzaska&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_o;&c_r;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rozdrvi&tcaron;</trd>,
    <trd>rozpu&ccaron;i&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_z;&c_b;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_r;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_t;&c_r;&c_o;&c_shch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_t;&c_r;&c_o;&c_shch;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_o;&c_t;&c_r;&c_o;&c_shch;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
</art>
