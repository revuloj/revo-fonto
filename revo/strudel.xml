    <trd lng="de">Strudel <klr>(Geb&auml;ck)</klr></trd>
    <trd lng="en">strudel</trd>
    <trd lng="fr">strudel</trd>
    <trd lng="ru">&c_sh;&c_t;&c_r;&c_u;&c_d;&c_je;&c_l;&c_mol;</trd>
  </snc>
  <rim>
