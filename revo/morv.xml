<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: morv.xml,v 1.17 2023/10/19 18:36:53 revo Exp $">
<kap>
  <rad>morv</rad>/o
</kap>
<drv mrk="morv.0o">
  <kap><tld/>o</kap>
    <snc>
    <uzo tip="fak">MED</uzo>
    <dif>
      Speco de &ccirc;eval<ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>
      infekta al homo:
      <ekz>
        dum la unua mondmilito, la germanoj infektis per <tld/>o &ccirc;evalojn de la rumana
        kavalerio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=161184">Biologia armilo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_p;</trd>
  <trd lng="ca">borm</trd>
  <trd lng="cs">ozh&rcaron;ivka</trd>
  <trdgrp lng="de">
    <trd>Rotz<klr> (Krankheit)</klr></trd>,
    <trd>Hautwurm</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>glanders</trd>,
    <trd>equinia</trd>,
    <trd>farcy</trd>,    
    <trd>malleus</trd>
  </trdgrp>
  <trd lng="fr">morve<klr> (maladie)</klr></trd>
  <trd lng="ja">&#x99AC;&#x9F3B;&#x75BD;  <pr>&#x3070;&#x3073;&#x305D;</pr></trd>
  <trd lng="nl">droes</trd>
  <trd lng="pl">nosacizna</trd>
  <trd lng="ru">&c_s;&c_a;&c_p;</trd>
  <trd lng="sk">sop&lcaron;avka <klr>(u ko&ncaron;a)</klr></trd>
</drv>
</art>
