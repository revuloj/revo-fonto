<vortaro>
<art mrk="$Id$">
<kap>
  <rad>morv</rad>/o
</kap>
<drv mrk="morv.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Speco de &ccirc;eval<ref tip="lst" cel="san.mal0o.MED"
      lst="voko:malsanoj">malsano</ref>
      infekta al homo.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_p;</trd>
  <trd lng="ca">borm</trd>
  <trdgrp lng="de">
    <trd>Rotz<klr> (Krankheit)</klr></trd>,
    <trd>Hautwurm</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>glanders</trd>,
    <trd>equinia</trd>,
    <trd>farcy</trd>,    
    <trd>malleus</trd>
  </trdgrp>
  <trd lng="nl">droes</trd>
  <trd lng="pl">nosacizna</trd>
  <trd lng="ru">&c_s;&c_a;&c_p;</trd>
</drv>
</art>
