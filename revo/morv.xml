<art mrk="$Id$">
<kap>
  <rad>morv</rad>/o
</kap>
<drv mrk="morv.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Speco de &ccirc;eval<ref tip="super" cel="san.mal0o">malsano</ref>
      infekta al homo.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_p;</trd>
  <trdgrp lng="de">
    <trd>Rotz<klr> (Krankheit)</klr></trd>,
    <trd>Hautwurm</trd>
  </trdgrp>
  <trd lng="nl">droes</trd>
  <trd lng="ru">&c_s;&c_a;&c_p;</trd>
</drv>
</art>
