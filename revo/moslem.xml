<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: moslem.xml,v 1.11 2024/01/26 19:05:53 revo Exp $">
<kap>
   <rad>moslem</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>

<drv mrk="moslem.0o">
   <kap><tld/>o</kap>
   <uzo tip="stl">RAR</uzo>
   <snc mrk="moslem.0o.islamano">
     <ref tip="dif" cel="islam.0ano">islamano</ref>
   </snc>
   <trd lng="be">&c_m;&c_u;&c_s;&c_u;&c_l;&c_mol;&c_m;&c_a;&c_n;&c_ib;&c_n;</trd>
   <trdgrp lng="bg">
     <trd>&c_m;&c_ju;&c_s;&c_ju;&c_l;&c_m;&c_a;&c_n;&c_i;&c_n;</trd>,
     <trd>&c_m;&c_o;&c_h;&c_a;&c_m;&c_je;&c_d;&c_a;&c_n;&c_i;&c_n;</trd>
   </trdgrp>
   <trd lng="ca">musulm&agrave; <klr>(subst.)</klr></trd>
   <trdgrp lng="de">
    <trd>Moslem</trd>,
    <trd>Muslim</trd>
  </trdgrp>
   <trd lng="hu">mozlim</trd>
   <trdgrp lng="pl">
     <trd>muzu&lstroke;manin</trd>,
     <trd>mahometanin <klr>(przest.)</klr></trd>
   </trdgrp>
   <trd lng="ro">musulman</trd>
   <trd lng="ru">&c_m;&c_u;&c_s;&c_u;&c_l;&c_mol;&c_m;&c_a;&c_n;&c_i;&c_n;</trd>
  <trd lng="zh">&#x7A46;&#x65AF;&#x6797; <pr>m&#xF9;s&#x12B;l&#xED;n</pr></trd>
</drv>

<drv mrk="moslem.0a">
   <kap><tld/>a</kap>
   <uzo tip="stl">RAR</uzo>
   <snc mrk="moslem.0a.islama">
     <dif>
       <ref tip="dif" cel="islam.0a">Islama</ref>:
       <ekz>
         <tld/>a nigra fez&ccirc;apo
         <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro 36a, p. 86a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_ib;&c_s;&c_l;&c_a;&c_m;&c_s;&c_k;&c_ib;</trd>,
     <trd>&c_m;&c_u;&c_s;&c_u;&c_l;&c_mol;&c_m;&c_a;&c_n;&c_s;&c_k;&c_ib;</trd>
   </trdgrp>
   <trd lng="bg">&c_m;&c_ju;&c_s;&c_ju;&c_l;&c_m;&c_a;&c_n;&c_s;&c_k;&c_i;</trd>
   <trd lng="ca">musulm&agrave; <klr>(adj.)</klr></trd>
   <trdgrp lng="de">
    <trd>muslimisch</trd>,
  </trdgrp>
   <trd lng="hu">mozlim</trd>
   <trd lng="pl">muzu&lstroke;ma&nacute;ski</trd>
   <trdgrp lng="pt">
     <trd>islamita</trd>,
     <trd>mu&ccedil;ulmano</trd>,
     <trd>maometano</trd>    
   </trdgrp>
   <trd lng="ro">musulman</trd>
   <trd lng="ru">&c_m;&c_u;&c_s;&c_u;&c_l;&c_mol;&c_m;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>

