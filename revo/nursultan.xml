<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: nursultan.xml,v 1.9 2025/02/06 06:35:40 afido Exp $">
    <kap>
      <rad>Astan</rad>/o, <var><kap><rad var="N">Nursultan</rad>/o</kap></var>
    </kap>

<drv mrk="nursultan.0o">
  <kap><tld/>o, <var><kap><tld var="N"/>o</kap></var></kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc mrk="nursultan.0o.cxefurbo">
    <dif>
      La <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&ccirc;efurbo</ref>
      de <ref tip="malprt" cel="kazahx.K0ujo">Kaza&hcirc;ujo</ref>, nomita post 1998 <tld/>o, sed inter
      2019&#8239;kaj&#8239;2022, honore al la prezidanto, <tld var="N"/>o:
      <ekz>
        &Ccirc;ina prezidanto <nom>Xi Jinping</nom> &jcirc;a&ubreve;de vizitis la &ccirc;inan nacian pavilonon de la Monda
        Ekspozicio de 2017 en <tld/>o
        <fnt>
          <vrk><url ref="&CRI;2521/2017/06/09/179s193567.htm">Prezidanto Xi vizitis &ccirc;inan pavilonon de Monda Ekspozicio</url></vrk>
          <lok>china radio international</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld var="N"/>o, anta&ubreve;e konata sub la nomo <tld/>o, Akmolo, Celinogrado kaj Akmolinsko
        <fnt>
          <vrk><url ref="&Viki;/Nursultano">Nursultano &ndash;
          Vikipedio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la 21-an de septembro 2019 en la &ccirc;efurbo de Kaza&hcirc;io <tld var="N"/>o kaj la urbo Almato
        okazis manifestacioj
        <fnt>
          <vrk><url ref="https://eo.wikinews.org/wiki/Protestoj_en_Kaza%C4%A5io_%E2%80%93_kontra%C5%AD_la_%C4%89inoj_kaj_por_demokratio"
              >Protestoj en Kaza&hcirc;io - kontra&ubreve; la &ccirc;inoj kaj por demokratio ...</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kiel lernantino en Celinogrado (nun <tld var="N"/>o) mi vizitis klubon de internacia
        amikeco en la urba pionirpalaco
        <fnt>
          <vrk><url ref="&Sezonoj;2019/11/gorecka-9/">Hobio, kiu i&gcirc;is profesio ...</url></vrk>
          <lok>La Ondo de Esperanto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_N;&c_u;&c_r;-&c_S;&c_u;&c_l;&c_t;&c_a;&c_n;</trd>
    <trdgrp lng="de">
      <trd>Nur-Sultan</trd>,
      <trd>Astana</trd>
    </trdgrp>
    <trd lng="en">Nur-Sultan</trd>
    <trd lng="fr">Noursoultan</trd>
    <trdgrp lng="kk">
      <trd>&c_N;&#1201;&c_r;-&c_S;&#1201;&c_l;&c_t;&c_a;&c_n;</trd>,
      <trd>&c_A;&c_s;&c_t;&c_a;&c_n;&c_a;</trd>
    </trdgrp>
    <trd lng="ky">&c_N;&c_u;&c_r;-&c_S;&c_u;&c_l;&c_t;&c_a;&c_n;</trd>
    <trd lng="pl">Nur-Su&lstroke;tan</trd>
    <trd lng="ro">Nur-Sultan</trd>
    <trd lng="ru">&c_N;&c_u;&c_r;-&c_S;&c_u;&c_l;&c_t;&c_a;&c_n;</trd>
    <trdgrp lng="zh">
    <trd>&#21162;&#23572;&#33487;&#20025; <pr>n&#468;&ecaron;rs&umacron;d&amacron;n</pr></trd>,
    <trd>&#21162;&#29246;&#34311;&#20025; <pr>n&#468;&ecaron;rs&umacron;d&amacron;n</pr></trd>,
    <trd>&#38463;&#26031;&#22612;&#32013; <pr>&amacron;s&imacron;t&#462;n&agrave;</pr></trd>,
    <trd>&#38463;&#26031;&#22612;&#32435; <pr>&amacron;s&imacron;t&#462;n&agrave;</pr></trd>,
    <trd>&#21162;&#23572;&#33487;&#20025; <pr>n&#468;&ecaron;rs&umacron;d&amacron;n</pr></trd>,
    <trd>&#21162;&#29246;&#34311;&#20025; <pr>n&#468;&ecaron;rs&umacron;d&amacron;n</pr></trd>
  </trdgrp>
</snc>
  <trd lng="ja">&#12450;&#12473;&#12479;&#12490;</trd>
  <trd lng="uk">&c_A;&c_s;&c_t;&c_a;&c_n;&c_a;</trd>
</drv>

