<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: grum.xml,v 1.14 2018/12/28 14:10:16 revo Exp $">
<kap><rad>grum</rad>/o
<fnt>Z</fnt></kap>
<drv mrk="grum.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Juna <ref tip="super" cel="lake.0o.servisto">lakeo</ref>, precipe en hoteloj kaj kafejoj:
      <ekz>
        la taskoj de la <tld/>o estas i.a. porti la baga&gcirc;on kaj akompani la gastojn al
        ilia &ccirc;ambro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=455654">Grumo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">grum</trd>
  <trdgrp lng="cs">
    <trd>pikol&iacute;k</trd>,
    <trd>podkon&iacute;</trd>,
    <trd>&ccaron;eled&iacute;n</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Page</trd>,
    <trd>Laufbursche</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>groom</trd>,
    <trd>stable boy</trd>,
    <trd>stable lad</trd>,
    <trd>servant</trd>,
    <trd>page boy</trd>,
    <trd>bellhop</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>groom</trd>,
    <trd>chasseur<klr tip="ind"> (groom)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&aacute;llodainas</trd>,
    <trd>liftes fi&uacute;</trd>,
    <trd>boy</trd>,
    <trd>kifut&oacute;fi&uacute;</trd>,
    <trd>pikol&oacute;<klr tip="ind"> (pinc&eacute;rfi&uacute;)</klr></trd>,
    <trd>pinc&eacute;rfi&uacute;</trd>,
    <trd>pinc&eacute;rtanul&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>picollo</trd>,
    <trd>hoteljongen</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>trintan&aacute;rio</trd>,
    <trd>groom</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_m;</trd>
  <trd lng="sk">mlad&yacute; hotelov&yacute; sluha</trd>
</drv>
</art>
