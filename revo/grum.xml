<kap><rad>grum</rad>/o<fnt><bib>JED</bib></fnt></kap>

<drv mrk="grum.0o">
  <kap>
    <tld/>o
    <fnt><bib>PV</bib></fnt>
  </kap>
  <snc>
    <dif>
      Juna <ref tip="super" cel="lake.0o.servisto">lakeo</ref>, precipe en hoteloj kaj kafejoj:
      <ekz>
        en la koridoro aperas <tld/>o
        <fnt>
          <bib>KrB</bib>
        </fnt>;
      </ekz>
      <ekz>
        la taskoj de la <tld/>o estas i.a. porti la baga&gcirc;on kaj akompani la gastojn al
        ilia &ccirc;ambro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=455654">Grumo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>servist(et)o</ke>.
  </rim>

  <trd lng="be">&c_g;&c_r;&c_u;&c_m;</trd>
  <trd lng="ca">grum</trd>
  <trdgrp lng="cs">
    <trd>pikol&iacute;k</trd>,
    <trd>podkon&iacute;</trd>,
    <trd>&ccaron;eled&iacute;n</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Page</trd>,
    <trd>Laufbursche</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>groom</trd>,
    <trd>stable boy</trd>,
    <trd>stable lad</trd>,
    <trd>servant</trd>,
    <trd>page boy</trd>,
    <trd>bellhop</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>groom</trd>,
    <trd>chasseur<klr tip="ind"> (groom)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&aacute;llodainas</trd>,
    <trd>liftes fi&uacute;</trd>,
    <trd>boy</trd>,
    <trd>kifut&oacute;fi&uacute;</trd>,
    <trd>pikol&oacute;<klr tip="ind"> (pinc&eacute;rfi&uacute;)</klr></trd>,
    <trd>pinc&eacute;rfi&uacute;</trd>,
    <trd>pinc&eacute;rtanul&oacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#39340;&#19969;  <pr>&#12400;&#12390;&#12356;</pr></trd>,
    <trd>&#32102;&#20181;  <pr>&#12365;&#12421;&#12358;&#12376;</pr></trd>,
    <trd>&#12508;&#12540;&#12452;  <pr>&#12412;&#12540;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>picollo</trd>,
    <trd>hoteljongen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ch&lstroke;opiec do pos&lstroke;ug</trd>,
    <trd>boy</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>trintan&aacute;rio</trd>,
    <trd>groom</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_m;</trd>
  <trd lng="sk">mlad&yacute; hotelov&yacute; sluha</trd>
  <trd lng="uk">&c_gu;&c_r;&c_u;&c_m;</trd>
</drv>
</art>
