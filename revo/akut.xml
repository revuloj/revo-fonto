<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>akut</rad>/a
</kap>
<drv mrk="akut.0a">
  <kap><tld/>a</kap>
  <snc mrk="akut.0a.angulo">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 26</lok></fnt>
    <dif>
      <klr>(p.p. <ref cel="angul.0o.figuro">angulo</ref>)</klr>
      Malpli granda ol <ref cel="ort.0o">orto</ref>:
      <ekz>
        la trianguloj havas almena&ubreve; du <tld/>ajn angulojn.
      </ekz>
    </dif>
    <ref tip="ant" cel="obtuz.0a.angulo">obtuza.</ref>
    <trd lng="de">spitz</trd>
    <trd lng="en">acute</trd>
    <trd lng="es">agudo</trd>
    <trd lng="fr"><klr tip="ind">(angle) </klr><ind>aigu</ind></trd>
    <trd lng="nl">scherp</trd>
    <trd lng="pt">agudo</trd>
    <trd lng="ru">&c_o;&c_s;&c_t;&c_r;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="akut.0a.malsano">
    <uzo tip="fak">MED</uzo>
    <uzo tip="klr">(p.p. malsano)</uzo>
    <dif>
      prezentanta fortajn simptomojn, rapide gravi&gcirc;anta:
      <ekz>
        li suferas
        <tld/>an ftizon.
      </ekz>
    </dif>
    <trd lng="en">acute</trd>
    <trd lng="es">agudo</trd>
    <trd lng="fr">aigu</trd>
    <trd lng="nl">acuut</trd>
    <trd lng="pt">agudo</trd>
    <trd lng="ru">&c_o;&c_s;&c_t;&c_r;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="akut.0a.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <uzo tip="fak">FON</uzo>
    <dif>
      Klara kaj penetranta kiel la altaj notoj de la gamo:
      <ekz>
        <tld/>a vo&ccirc;o, <tld/>aj vokaloj.
      </ekz>
    </dif>
    <trd lng="en">penetrating</trd>
    <trd lng="es">agudo</trd>
    <trd lng="fr">aigu</trd>
    <trd lng="nl">scherp</trd>
    <trd lng="pt">agudo</trd>
    <trd lng="ru">&c_r;&c_je;&c_z;&c_k;&c_i;&c_j;</trd>
  </snc>
  <snc>
    <dif>
      = akra 5:
      <ekz>
        <tld/>a observado.
      </ekz>
    </dif>
    <trd lng="es">agudo</trd>
    <trd lng="fr">per&ccedil;ant</trd>
    <trd lng="nl">scherp</trd>
    <trd lng="pt">agu&ccedil;ado</trd>
  </snc>
</drv>

<drv mrk="akut.mal0a">
  <kap>mal<tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <uzo tip="stl">ARK</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 26</lok></fnt>
  <snc mrk="akut.mal0a.angulo">
    <ref tip="dif" cel="obtuz.0a.angulo">obtuza.</ref>
  </snc>
</drv>
</art>
