<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>akut</rad>/a
</kap>
<drv mrk="akut.0a">
  <kap><tld/>a</kap>
  <snc mrk="akut.0a.angulo">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <klr>(p.p. <ref cel="angul.0o.elementa">angulo</ref>)</klr>
      Malpli granda ol <ref cel="ort.0o">orto</ref>:
      <ekz>
        la trianguloj havas almena&ubreve; du <tld/>aj angulojn.
      </ekz>
    </dif>
    <ref tip="ant" cel="obtuz.0a.angulo">obtuza.</ref>
    <trd lng="en">acute</trd>
    <trd lng="es">agudo</trd>
    <trd lng="fr"><klr tip="ind">(angle) </klr><ind>aigu</ind></trd>
    <trd lng="ru">&#1086;&#1089;&#1090;&#1088;&#1099;&#1081;</trd>
  </snc>
  <snc>
    <uzo tip="klr">(Pri malsano)</uzo>
    <dif>
      prezentanta  fortajn simptomojn, rapide gravi&gcirc;anta:
      <ekz>
        li suferas
        <tld/>an ftizon.
      </ekz>
    </dif>
    <trd lng="en">acute</trd>
    <trd lng="es">agudo</trd>
    <trd lng="fr">aigu</trd>
    <trd lng="ru">&#1086;&#1089;&#1090;&#1088;&#1099;&#1081;</trd>
  </snc>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <uzo tip="fak">GRA</uzo>
    <dif>
      Klara kaj penetranta kiel la altaj notoj de la gamo:
      <ekz>
        <tld/>a  vo&ccirc;o, <tld/>aj vokaloj.
      </ekz>
    </dif>
    <trd lng="en">penetrating</trd>
    <trd lng="es">agudo</trd>
    <trd lng="fr">aigu</trd>
    <trd lng="ru">&#1088;&#1077;&#1079;&#1082;&#1080;&#1081;</trd>
  </snc>
  <snc>
    <dif>
      = akra 5:
      <ekz>
        <tld/>a observado.
      </ekz>
    </dif>
    <trd lng="es">agudo</trd>
    <trd lng="fr">per&ccedil;ant</trd>
  </snc>
</drv>
</art>
