<vortaro>
<art mrk="$Id$">
<kap>
<rad>cerefoli</rad>/o
</kap>
<drv mrk="cerefo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">Anthriscus</trd>). La folioj de unu specio
      (<trd lng="la">Anthriscus cerefolium</trd>) estas uzataj en
      supoj kaj salatoj.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_je;&c_r;&c_v;&c_e;&c_l;&c_mol;</trd>
  <trd lng="de">Kerbel</trd>
  <trd lng="el">&chi;&alfa;&jota;&rho;&epsilon_ton;&phi;&ypsilon;&lambda;&lambda;&omikron;</trd>
  <trd lng="fr">cerfeuil</trd>
  <trd lng="hu">turbolya</trd>
  <trd lng="nl">kervel</trd>
  <trd lng="pt">ceref&oacute;lio</trd>
  <trd lng="ru">&c_k;&c_je;&c_r;&c_v;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sv">sm&aring;k&ouml;rvlar</trd>
</drv>
</art>

