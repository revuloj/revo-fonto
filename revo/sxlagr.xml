<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&scirc;lagr</rad>/o
</kap>
<drv mrk="sxlagr.0o">
  <kap><tld/>o <fnt><bib>HejmVort</bib></fnt></kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <ref tip="dif" cel="kant.furor0o">furorkanto</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_l;&c_ja;&c_g;&c_je;&c_r;</trd>, 
    <trd>&c_g;&c_ib;&c_t;</trd>
  </trdgrp>
  <trd lng="de">Schlager</trd>
  <trdgrp lng="fr">
    <trd>chanson &agrave; succ&egrave;s</trd>,
    <trd>hit<klr tip="ind"> (chanson)</klr></trd>,
    <trd>succ&egrave;s<klr tip="ind"> (chanson)</klr></trd>,
    <trd>tube<klr tip="ind"> (chanson)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schlager</trd>,
    <trd>hit</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_l;&c_ja;&c_g;&c_je;&c_r;</trd>,
    <trd>&c_h;&c_i;&c_t;</trd>
  </trdgrp>
</drv>
</art>
