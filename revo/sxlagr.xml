<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&scirc;lagr</rad>/o
</kap>
<drv mrk="sxlagr.0o">
  <kap><tld/>o <fnt><bib>HejmVort</bib></fnt>
  <fnt><bib>PIV3</bib></fnt></kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
    <ref tip="dif" cel="kant.furor0o">furorkanto</ref>:
    <ekz>
      predikoj, <tld/>oj, kartoj rutinaj anoncos benojn de
      la sezono
      <fnt>
        <aut>Krys Ungar</aut>,
        <vrk>Meznokto metropola</vrk>,
        <lok>p. 42</lok>
      </fnt>.
    </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_l;&c_ja;&c_g;&c_je;&c_r;</trd>, 
    <trd>&c_g;&c_ib;&c_t;</trd>
  </trdgrp>
  <trd lng="de">Schlager</trd>
  <trd lng="en">hit <ind>song</ind></trd>
  <trdgrp lng="fr">
    <trd>chanson &agrave; succ&egrave;s</trd>,
    <trd>hit<klr tip="ind"> (chanson)</klr></trd>,
    <trd>succ&egrave;s<klr tip="ind"> (chanson)</klr></trd>,
    <trd>tube<klr tip="ind"> (chanson)</klr></trd>
  </trdgrp>
  <trd lng="hu">sl&aacute;ger</trd>
  <trdgrp lng="it">
    <trd>tormentone<klr> (canzone)</klr></trd>,
    <trd>hit</trd>,
    <trd>successo<klr> (canzone)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schlager</trd>,
    <trd>hit</trd>,
    <trd>tophit</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>&eacute;xito</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_l;&c_ja;&c_g;&c_je;&c_r;</trd>,
    <trd>&c_h;&c_i;&c_t;</trd>
  </trdgrp>
</drv>
</art>
