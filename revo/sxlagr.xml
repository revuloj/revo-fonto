<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxlagr.xml,v 1.16 2023/12/01 01:43:12 revo Exp $">
<kap>
  <rad>&scirc;lagr</rad>/o <fnt><bib>HejmVort</bib></fnt>
  <fnt><bib>PIV3</bib></fnt>
</kap>
<drv mrk="sxlagr.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxlagr.0o.MUZ">
  <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="dif" cel="kant.mod0o">Furorkanto</ref>:
      <ekz>
        &leftquot;marmor&scirc;ton' kaj fer' rompi&gcirc;os &ndash; sed ne nia am' perdi&gcirc;os&#x201D; &ndash; tiel kantis iu <nom>Drafi
        Deutscher</nom> en la jaro 1965, atingante per tiu &ccirc;i <tld/>o unuajn lokojn en la
        furorlistoj
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2010/010074.php">&Scirc;tupoj al malplenaj po&scirc;oj</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        predikoj, <tld/>oj, kartoj rutinaj anoncos benojn de
        la sezono
        <fnt>
          <aut>Krys Ungar</aut>,
          <vrk>Meznokto metropola</vrk>,
          <lok>p. 42</lok>
        </fnt>;
      </ekz>
      <ekz>
        sentimentala interpretado fare de diversaj <tld/>aj kantistoj
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2013/010944.php">Kanto de la montoj</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_l;&c_ja;&c_g;&c_je;&c_r;</trd>,
    <trd>&c_g;&c_ib;&c_t;</trd>
  </trdgrp>
  <trd lng="de">Schlager</trd>
  <trd lng="en">hit <ind>song</ind></trd>
  <trd lng="es">&eacute;xito</trd>
  <trdgrp lng="fr">
    <trd>chanson &agrave; succ&egrave;s</trd>,
    <trd>hit<klr tip="ind"> (chanson)</klr></trd>,
    <trd>succ&egrave;s<klr tip="ind"> (chanson)</klr></trd>,
    <trd>tube<klr tip="ind"> (chanson)</klr></trd>
  </trdgrp>
  <trd lng="hu">sl&aacute;ger</trd>
  <trdgrp lng="it">
    <trd>tormentone<klr> (canzone)</klr></trd>,
    <trd>hit</trd>,
    <trd>successo<klr> (canzone)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schlager</trd>,
    <trd>hit</trd>,
    <trd>tophit</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>szlagier</trd>,
    <trd>hit</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_l;&c_ja;&c_g;&c_je;&c_r;</trd>,
    <trd>&c_h;&c_i;&c_t;</trd>
  </trdgrp>
  <trd lng="uk">&c_sh;&#x43B;&#x44F;&c_gu;&c_je;&c_r; (&#x43C;&#x43E;&c_d;&#x43D;&c_a; &#x43F;&c_ib;&c_s;&#x43D;&#x44F;)</trd>
</drv>
</art>
