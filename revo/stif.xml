<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: stif.xml,v 1.4 2022/05/05 14:43:48 revo Exp $">
<kap>
  <rad>stif</rad>- <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="stif.0">
  <kap><tld/>-</kap>
  <gra>neoficiala <vspec>prefikso</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="stif.0.GEN">
    <dif>
      <klr>(p.p. parenco)</klr> <ref tip="dif" cel="vic.0.GEN">Vic-<sncref/></ref>:
      <ekz>
        en retaj tekstoj <tld/>- aperas preska&ubreve; nur en vortlistoj a&ubreve; diskutoj pri
        parencvortoj, en ordinara lingvouzo oni ne &ccirc;iam diferencigas inter duonparenco kaj vicparenco
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=122616">Parenceco</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        Ho suferoj multaj &scirc;ian vivon ombris, /
        Sed precipe &scirc;ia <tld/>patrin' &scirc;in rompis
        <fnt>
          <aut>S. Pet&odblac;fi, trad. K. Kalocsay</aut>,
          <vrk><url ref="http://xa.yimg.com/kq/groups/2719935/1347203493/name/Johano+la+brava+L.pdf">
          Johano la Brava</url></vrk>,
          <lok>1923</lok>
        </fnt>;
      </ekz>
      <ekz>
        de 1585 &gcirc;is 1586 li soldatis sub sia
        <tld/>-patro en Nederlando
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Robert_Devereux,_2-a_Grafo_de_Essex">
          Robert Devereux...</url>, 2011-02-12</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Stief-</trd>
    <trd lng="en">step-</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>beau-<klr> (parent suite &agrave; un 2e mariage)</klr></trd>,
    <trd>belle-<klr> (parent suite &agrave; un 2e mariage)</klr></trd>
  </trdgrp>
</drv>
</art>

