<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <rad>Paskal</rad>/o
</kap>
<drv mrk="paskal.0o">
   <kap><tld/>o</kap>
   <snc mrk="paskal.0o.PERS">
     <uzo tip="fak">PERS</uzo>
     <fnt><bib>PIV1</bib></fnt>
     <dif>
       Franclingve: Blaise <trd lng="fr">Pascal</trd>, 1623-1662. Franca
       sciencisto kaj filozofo.
     </dif>
     <trd lng="be">&c_P;&c_a;&c_s;&c_k;&c_a;&c_l;&c_mol; 
<klr>(&c_f;&c_r;&c_a;&c_n;&c_c;&c_u;&c_s;&c_k;&c_ib; 
&c_n;&c_a;&c_v;&c_u;&c_k;&c_o;&c_w;&c_c;&c_a;)</klr></trd>
     <trd lng="de">Pascal</trd>
     <trd lng="en">Pascal</trd>
     <trd lng="nl">Blaise <ind>Pascal</ind></trd>
     <trd lng="hu">Pascal</trd>     
     <trd lng="pl">Pascal</trd>
     <trd lng="pt">Pascal</trd>
     <trd lng="ru">&c_P;&c_a;&c_s;&c_k;&c_a;&c_l;&c_mol;</trd>
   </snc>
   <snc mrk="paskal.0o.KOMP">
     <uzo tip="fak">KOMP</uzo>
     <fnt><bib>KompLeks</bib></fnt>
     <dif>
       Populara <ref cel="lingv.0o.program">programlingvo</ref>
       kreita en 1969 kaj nomita oma&gcirc;e al
       <tld/>o, <sncref ref="paskal.0o.PERS"/>.
     </dif>
     <trd lng="de">Pascal <klr tip="ind">(Programmiersprache)</klr></trd>
     <trd lng="be">&c_p;&c_a;&c_s;&c_k;&c_a;&c_l;&c_mol; 
<klr>(&c_m;&c_o;&c_v;&c_a; 
&c_p;&c_r;&c_a;&c_g;&c_r;&c_a;&c_m;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ja;)
</klr></trd>
     <trd lng="nl">Pascal<klr tip="ind"> (taal)</klr></trd>
     <trd lng="fr">Pascal <klr tip="ind">(informatique)</klr></trd>
     <trd lng="hu">Pascal <klr tip="ind">(programnyelv)</klr></trd>      
     <trd lng="ru">&c_p;&c_a;&c_s;&c_k;&c_a;&c_l;&c_mol;
     <klr>(&c_ja;&c_z;&c_y;&c_k; 
&c_p;&c_r;&c_o;&c_g;&c_r;&c_a;&c_m;&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;
&c_ja;)</klr></trd>
   </snc>
   <trd lng="en">Pascal</trd>
   <trd lng="es">Pascal</trd>
</drv>

<drv mrk="paskal.p0o">
   <kap><tld lit="p"/>o</kap>
   <uzo tip="fak">FIZ</uzo>
   <snc>
     <dif>
       <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de
       <ref cel="prem.0o.FIZ">premo</ref> en la Internacia Sistemo de
       unuoj; simb. Pa:
       <ekz>
         unu <tld lit="p"/>o egalas al la premo, estigata de forto je
         unu <ref cel="neuxto.n0o">ne&ubreve;tono</ref>, orte kaj unuforme
         aplikata al surfaco je unu <ref cel="metr.kvadrata0o">kvadrata
         metro</ref>.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_p;&c_a;&c_s;&c_k;&c_a;&c_l;&c_mol; 
<klr>(&c_a;&c_d;&c_z;&c_ib;&c_n;&c_k;&c_a; 
&c_v;&c_y;&c_m;&c_ja;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>
   <trd lng="de">Pascal</trd>
   <trd lng="es">pascal<klr tip="ind"> (unidad de medida)</klr></trd>
   <trd lng="hu">pascal<klr tip="ind"> (m&eacute;rt&eacute;kegys&eacute;g)
</klr></trd>
   <trd lng="nl">pascal<klr tip="ind"> (eenheid)</klr></trd>   
   <trd lng="fr">pascal<klr tip="ind"> (unit&eacute; de mesure)</klr></trd>
   <trd lng="ru">&c_p;&c_a;&c_s;&c_k;&c_a;&c_l;&c_mol; 
<klr>(&c_je;&c_d;&c_i;&c_n;&c_i;&c_c;&c_a; 
&c_d;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>
</drv>

<drv mrk="paskal.p0a">
   <kap><tld lit="p"/>a</kap>
   <snc>
     <refgrp tip="vid">
       <ref cel="limak.paskala0o"><tld lit="p"/>a limako</ref>,
       <ref cel="angul.paskalatri0o"><tld lit="p"/>a triangulo</ref>.
     </refgrp>
   </snc>
   <trd lng="de">Pascalsch</trd>
   <trd lng="en">Pascal['s]</trd>
   <trd lng="es">de <ind>Pascal</ind></trd>
   <trd lng="fr">de <ind>Pascal</ind></trd>
   <trdgrp lng="hu">
     <trd>Pascal-</trd>,
     <trd>Pascal-f&eacute;le</trd>
   </trdgrp>
   <trd lng="nl">van <ind>Pascal</ind></trd>
   <trd lng="pl">Pascala</trd>
   <trd lng="ru">&c_P;&c_a;&c_s;&c_k;&c_a;&c_l;&c_ja;</trd>
</drv>
</art>
