<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: paskal.xml,v 1.29 2024/02/16 19:24:11 revo Exp $">
<kap>
   <rad>Paskal</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="paskal.0o">
   <kap><tld/>o</kap>
   <snc mrk="paskal.0o.NOM">
     <uzo tip="fak">NOM</uzo>
     <dif>
       Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> hebredevena.
     </dif>
     <trd lng="be">&c_P;&c_a;&c_s;&c_k;&c_a;&c_l;&c_mol; <klr>(&c_ib;&c_m;&c_ja;)</klr></trd>
     <trd lng="fr">Pascal</trd>
     <trdgrp lng="zh">
    <trd>&#x5E15;&#x65AF;&#x5361;&#x5C14; <pr>P&#xE0;s&#x12B;k&#x1CE;&#x11B;r</pr></trd>,
    <trd>&#x5E15;&#x65AF;&#x5361;&#x723E; <pr>P&#xE0;s&#x12B;k&#x1CE;&#x11B;r</pr></trd>
  </trdgrp>
</snc>
   <snc mrk="paskal.0o.PERS">
     <uzo tip="fak">PERS</uzo>
     <fnt><bib>PIV1</bib></fnt>
     <dif>
       Franca <ref tip="lst" cel="scienc.0isto" lst="voko:historiaj_personoj">sciencisto</ref> kaj
       filozofo,
       1623-1662;
       franclingve: Blaise <trd lng="fr">Pascal</trd>:
       <ekz>
         la plej bona aran&gcirc;o de la monda trafiko estus,
         se oni la&ubreve;eble trankvile restus en sia &ccirc;ambro,
         kiel konsilas <tld/>o
         <fnt>
           <aut>V. O&scaron;lak</aut>,
           <vrk>Foje e&ccirc; pensi estas &gcirc;uo</vrk>,
           <lok>2003</lok>
         </fnt>.
       </ekz>
     </dif>
     <trd lng="be">&c_P;&c_a;&c_s;&c_k;&c_a;&c_l;&c_mol; <klr>(&c_B;&c_l;&c_je;&c_z;, &c_f;&c_r;&c_a;&c_n;&c_c;&c_u;&c_s;&c_k;&c_ib;
     &c_n;&c_a;&c_v;&c_u;&c_k;&c_o;&c_w;&c_c;&c_a;)</klr></trd>
     <trd lng="bg">&c_B;&c_l;&c_je;&c_z; <ind>&c_P;&c_a;&c_s;&c_k;&c_a;&c_l;</ind></trd>
     <trd lng="de">Pascal</trd>
     <trd lng="en">Pascal</trd>
     <trd lng="fa">&a_b;&a_l;&a_z; <ind>&f_p;&a_A;&a_s;&f_k;&a_A;&a_l;</ind></trd>
     <trd lng="nl">Blaise <ind>Pascal</ind></trd>
     <trd lng="hu">Pascal</trd>
     <trd lng="id">Pascal</trd>
     <trd lng="it">Pascal<klr> (fil.)</klr></trd>     
     <trd lng="pl">Pascal</trd>
     <trd lng="pt">Pascal</trd>
     <trd lng="ru">&c_P;&c_a;&c_s;&c_k;&c_a;&c_l;&c_mol;</trd>
     <trd lng="zh">&#x5E15;&#x65AF;&#x5361; <pr>P&#xE0;s&#x12B;k&#x1CE;</pr></trd>
</snc>
   <snc mrk="paskal.0o.KOMP">
     <uzo tip="fak">KOMP</uzo>
     <fnt><bib>KompLeks</bib></fnt>
     <dif>
       Populara <ref tip="lst" cel="lingv.program0o" lst="voko:lingvoj">programlingvo</ref>
       kreita en 1969 kaj nomita oma&gcirc;e al
       <tld/>o, <sncref ref="paskal.0o.PERS"/>.
     </dif>
     <trd lng="be">&c_p;&c_a;&c_s;&c_k;&c_a;&c_l;&c_mol; <klr>(&c_m;&c_o;&c_v;&c_a;
     &c_p;&c_r;&c_a;&c_g;&c_r;&c_a;&c_m;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>
     <trd lng="bg">&c_P;&c_a;&c_s;&c_k;&c_a;&c_l;</trd>
     <trd lng="de">Pascal <klr tip="ind">(Programmiersprache)</klr></trd>
     <trd lng="fa">&f_p;&a_A;&a_s;&f_k;&a_A;&a_l;<klr tip="ind"> (&a_z;&a_b;&a_A;&a_n;
     &a_b;&a_r;&a_n;&a_A;&a_m;&a_h;&zwnj;&a_n;&a_w;&f_y;&a_s;&f_y;)</klr></trd>
     <trd lng="fr">Pascal <klr tip="ind">(informatique)</klr></trd>
     <trd lng="hu">Pascal <klr tip="ind">(programnyelv)</klr></trd>     
     <trd lng="id">Pascal <klr>(bahasa pemrograman)</klr></trd>
     <trd lng="it">Pascal<klr> (inf.)</klr></trd> 
     <trd lng="nl">Pascal<klr tip="ind"> (taal)</klr></trd>
     <trd lng="ru">&c_p;&c_a;&c_s;&c_k;&c_a;&c_l;&c_mol; <klr>(&c_ja;&c_z;&c_y;&c_k;
     &c_p;&c_r;&c_o;&c_g;&c_r;&c_a;&c_m;&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;&c_ja;)</klr></trd>
     <trdgrp lng="zh">
    <trd>&#x5E15;&#x65AF;&#x5361;&#x5C14; <pr>P&#xE0;s&#x12B;k&#x1CE;&#x11B;r</pr></trd>,
    <trd>&#x5E15;&#x65AF;&#x5361;&#x723E; <pr>P&#xE0;s&#x12B;k&#x1CE;&#x11B;r</pr></trd>
  </trdgrp>
</snc>
  <trd lng="cs">Pascal</trd>
  <trd lng="en">Pascal</trd>
  <trd lng="es">Pascal</trd>
  <trd lng="sk">PASCAL</trd>
</drv>

<drv mrk="paskal.p0o">
   <kap><tld lit="p"/>o</kap>
   <snc mrk="paskal.p0o.FIZ">
     <uzo tip="fak">FIZ</uzo>
     <dif>
       <ref tip="lst" cel="unu.mezur0o" lst="voko:mezurunuoj">Mezurunuo</ref> 
       de <ref cel="prem.0o.FIZ">premo</ref> en la Internacia Sistemo de
       unuoj; simb. Pa:
       <ekz>
         unu <tld lit="p"/>o egalas al la premo, estigata de forto je
         unu <ref cel="neuxto.n0o">ne&ubreve;tono</ref>, orte kaj unuforme
         aplikata al surfaco je unu <ref cel="metr.kvadrata0o">kvadrata
         metro</ref>.
       </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_k;&c_a;&c_l;&c_mol; <klr>(&c_a;&c_d;&c_z;&c_ib;&c_n;&c_k;&c_a;
   &c_v;&c_y;&c_m;&c_ja;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>
  <trd lng="bg">&c_p;&c_a;&c_s;&c_k;&c_a;&c_l; <klr>(&c_je;&c_d;&c_i;&c_n;&c_i;&c_c;&c_a;)</klr></trd>
  <trd lng="cs">Pascal</trd>
  <trd lng="de">Pascal</trd>
  <trd lng="es">pascal<klr tip="ind"> (unidad de medida)</klr></trd>
  <trd lng="fa">&f_p;&a_A;&a_s;&f_k;&a_A;&a_l;<klr tip="ind"> (&f_y;&f_k;&a_A;&f_y; &a_s;&a_n;&a_j;&a_sh;
   &a_f;&a_sh;&a_A;&a_r;)</klr></trd>
  <trd lng="fr">pascal<klr tip="ind"> (unit&eacute; de mesure)</klr></trd>
  <trd lng="hu">pascal<klr tip="ind"> (m&eacute;rt&eacute;kegys&eacute;g)</klr></trd>
  <trd lng="id">pascal</trd>
  <trd lng="it">pascal<klr> (unit&agrave; di misura)</klr></trd>
  <trd lng="ja">&#x30D1;&#x30B9;&#x30AB;&#x30EB;</trd>
  <trd lng="nl">pascal<klr tip="ind"> (eenheid)</klr></trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_k;&c_a;&c_l;&c_mol; <klr>(&c_je;&c_d;&c_i;&c_n;&c_i;&c_c;&c_a;
   &c_d;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>
  <trd lng="sk">PASCAL</trd>
  <trd lng="uk">&#x41F;&c_a;&c_s;&#x43A;&c_a;&#x43B;&#x44C;</trd>
  <trd lng="zh">&#x5E15;&#x65AF;&#x5361; <pr>p&#xE0;s&#x12B;k&#x1CE;</pr></trd>
</drv>

<drv mrk="paskal.p0a">
   <kap><tld lit="p"/>a</kap>
   <snc mrk="paskal.p0a.MAT">
     <refgrp tip="vid">
       <ref cel="limak.paskala0o"><tld lit="p"/>a limako</ref>,
       <ref cel="angul.paskalatri0o"><tld lit="p"/>a triangulo</ref>.
     </refgrp>
   </snc>
   <trdgrp lng="be">
     <trd>&c_P;&c_a;&c_s;&c_k;&c_a;&c_l;&c_ja;</trd>,
     <trd>&c_p;&c_a;&c_s;&c_k;&c_a;&c_l;&c_je;&c_w;</trd>
   </trdgrp>
   <trd lng="de">Pascalsch</trd>
   <trd lng="en">Pascal['s]</trd>
   <trd lng="es">de <ind>Pascal</ind></trd>
   <trd lng="fr">de <ind>Pascal</ind></trd>
   <trdgrp lng="hu">
     <trd>Pascal-</trd>,
     <trd>Pascal-f&eacute;le</trd>
   </trdgrp>
   <trd lng="nl">van <ind>Pascal</ind></trd>
   <trd lng="pl">Pascala</trd>
   <trd lng="ru">&c_P;&c_a;&c_s;&c_k;&c_a;&c_l;&c_ja;</trd>
</drv>
</art>
