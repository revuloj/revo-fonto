<vortaro>
<art mrk="$Id: angi.xml,v 1.27 2025/05/28 10:50:55 revo Exp $">
<kap>
  <rad>angi</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="angi.0o">
  <kap><tld/>o</kap>
  <snc mrk="angi.0o.BIO">
    <uzo tip="fak">ANA</uzo>
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="dif" cel="vaskul.0o">Vaskulo</ref>:
      <ekz>
        &scirc;ia sango fluis printempe varme en &scirc;iaj <tld/>oj
        <fnt>
          <bib>HsT</bib>
          <lok>Sia propra mastro</lok>
        </fnt>;
      </ekz>
      <ekz>
        etaj vejnaj <tld/>oj en la cerbo
        <fnt>
        <aut>Evgeni Georgiev</aut>:
         <vrk>multobla sklerozo, novaj esperoj pri efikaj terapioj</vrk>,
         <bib>Monato</bib>,
         <lok>jaro 1998a, numero 4a, p. 19a</lok>
        </fnt>;
      </ekz>
      <ekz>
        homoj, kiuj dormas tro malmulte, vivas pli mallonge kaj havas pli da kor- kaj
        <tld/>o-problemoj
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="&Monato;/2012/010683.php"
              >Dormu bone, vivu bone</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Anka&ubreve; eblas diri <ke>korpa tubeto</ke>.
  </rim>
  <trd lng="be">&c_s;&c_u;&c_d;&c_z;&c_ib;&c_n;&c_a;</trd>
  <trd lng="ca">vas</trd>
  <trd lng="cs">c&eacute;va</trd>
  <trd lng="de">Gef&auml;&szlig; <klr>(Biologie)</klr></trd>
  <trd lng="en">blood vessel</trd>
  <trd lng="es">vaso<klr> (&oacute;rgano)</klr></trd>
  <trd lng="fr">vaisseau sanguin</trd>
  <trd lng="he">&kaf;&lamed;&yod; &dalet;&fmem;</trd>
  <trdgrp lng="hu">
    <trd>&eacute;r</trd>,
    <trd>ed&eacute;ny <klr tip="amb">(&aacute;llati, n&ouml;v&eacute;nyi)</klr></trd>
  </trdgrp>
  <trd lng="it">vaso<klr> (anatomia)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#33032;&#31649;  <pr>&#12415;&#12419;&#12387;&#12363;&#12435;</pr></trd>,
    <trd/>,
    <trd>&#23566;&#31649;  <pr>&#12393;&#12358;&#12363;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">ader</trd>
  <trd lng="pl">naczynie <klr>(anatomiczne)</klr></trd>
  <trd lng="ru">&c_s;&c_o;&c_s;&c_u;&c_d; <klr tip="amb">(&c_a;&c_n;&c_a;&c_t;., &c_b;&c_o;&c_t;.)</klr></trd>
  <trd lng="sk">cieva</trd>
  <trd lng="uk">&c_s;&c_u;&c_d;&c_i;&c_n;&c_a;</trd>
</drv>
</art>
