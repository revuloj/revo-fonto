<drv mrk="angi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Organa tubo tra kiu fluas sango a&ubreve; alia korpa fluida&jcirc;o:
      <ekz>
        etaj vejnaj angioj en la cerbo<fnt><aut>Evgeni Georgiev</aut>:
        <vrk>multobla sklerozo, novaj esperoj pri efikaj terapioj</vrk>,
        <bib>Monato</bib><lok>jaro 1998a, numero 4a, p. 19a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">vas</trd>
  <trd lng="de">Gef&auml;&szlig;</trd>
  <trd lng="en">blood vessel</trd>
  <trd lng="es">vaso</trd>
  <trd lng="fr">vaisseau sanguin</trd>
  <trd lng="he">&kaf;&lamed;&yod; &dalet;&fmem;</trd>
  <trdgrp lng="hu">
    <trd>&eacute;r</trd>,
    <trd>ed&eacute;ny <klr tip="amb">(&aacute;llati,
    n&ouml;v&eacute;nyi)</klr></trd>
  </trdgrp>
  <trd lng="nl">ader</trd>
  <trd lng="ru">&c_s;&c_o;&c_s;&c_u;&c_d; <klr
  tip="amb">(&c_a;&c_n;&c_a;&c_t;., &c_b;&c_o;&c_t;.)</klr></trd>
  <trd lng="sk">cieva</trd>
</drv>
</art>
