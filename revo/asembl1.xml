<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: asembl1.xml,v 1.13 2021/01/28 18:10:32 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>asemble</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="asembl1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Interkonsili&gcirc;a a&ubreve; le&gcirc;ofaranta organizo, kontraste
      al la plenuma instanco:
      <ekz>
        la <tld/>o de Unesko.
      </ekz>
    </dif>
    <ref tip="vid" cel="kolegi.0o.oficistaro">kolegio</ref>
    <refgrp tip="sub">
      <ref cel="konvent.0o">konvento</ref>,
      <ref cel="parlam.0o">parlamento</ref>,
      <ref cel="sejm.0o">sejmo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_a;&c_m;&c_b;&c_l;&c_je;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>valn&eacute; shrom&aacute;&zcaron;d&ecaron;n&iacute;</trd>,
    <trd>z&aacute;konod&aacute;rn&eacute; shrom&aacute;&zcaron;d&ecaron;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>beratende <ind>K&ouml;rperschaft</ind></trd>,
     <trd>gesetzgebende <ind>K&ouml;rperschaft</ind></trd>,
     <trd>Versammlung</trd>
  </trdgrp>
  <trd lng="en">assembly</trd>
  <trd lng="es">asamblea</trd>
  <trd lng="fr">assembl&eacute;e</trd>
  <trdgrp lng="hu">
     <trd>tan&aacute;cskoz&oacute; <ind>test&uuml;let</ind></trd>,
     <trd>t&ouml;rv&eacute;nyhoz&oacute; <ind>test&uuml;let</ind></trd>,
     <trd>k&ouml;zgy&udblac;l&eacute;s</trd>
  </trdgrp>
  <trd lng="it">assemblea</trd>
  <trd lng="ru">&c_a;&c_s;&c_s;&c_a;&c_m;&c_b;&c_l;&c_je;&c_ja;</trd>
  <trd lng="sk">zhroma&zcaron;denie</trd>
</drv>
</art>
