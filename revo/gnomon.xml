<art mrk="$Id: gnomon.xml,v 1.17 2024/08/30 06:36:57 revo Exp $">
<kap>
  <rad>gnomon</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="gnomon.0o">
  <kap><tld/>o</kap>
  <snc mrk="gnomon.0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Astronomia <ref tip="super" cel="instru2.0o">instrumento</ref>,
      konsistanta el vertikala ombro&jcirc;etilo,
      kaj uzata krom kiel <ref tip="super" cel="horlog.sun0o.KAL">sunhorlo&gcirc;o</ref>
      anka&ubreve; por difini la latitudon, nordon, ekvinokson, solsticon kaj ekliptikon:
      <ekz>
        <klr>[&ccirc;e la]</klr> sunhorlo&gcirc;o de <nom>Whitehurst</nom> <klr>[&#8230;]</klr> unu rando de <tld/>o
        estas la ombro&jcirc;etilo kiu &jcirc;etas la ombron anta&ubreve; tagmezo, kaj la alia rando
        estas la ombro&jcirc;etilo kiu &jcirc;etas la ombron post tagmezo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=364556">Sunhorlo&gcirc;o de Whitehurst &amp;
          Son</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>nap&oacute;ra</trd>,
      <trd>gn&oacute;mon</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_g;&c_n;&c_o;&c_m;&c_a;&c_n;</trd>
  <trd lng="ca">gn&ograve;mon</trd>
  <trd lng="cs">gn&oacute;mon</trd>
  <trd lng="de">Gnomon</trd>
  <trd lng="en">gnomon</trd>
  <trd lng="es">gnomon</trd>
  <trd lng="fr">gnomon</trd>
  <trd lng="it">gnomone</trd>
  <trdgrp lng="ja">
    <trd>&#x30B0;&#x30CE;&#x30E2;&#x30F3;</trd>,
    <trd>&#x304D;&#x91DD;  <pr>&#x304D;&#x306F;&#x308A;</pr></trd>,
    <trd>&#x65E5;&#26178;&#x8A08;  <pr>&#12402;&#12393;&#12369;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">gnomon</trd>
  <trd lng="pl">gnomon</trd>
  <trd lng="pt">gn&ocirc;mon</trd>
  <trd lng="ru">&c_g;&c_n;&c_o;&c_m;&c_o;&c_n;</trd>
  <trd lng="sk">gn&oacute;mon</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&#x43D;&#x43E;&#x43C;&#x43E;&#x43D;</trd>,
    <trd>&c_s;&#x43E;&#x43D;&#x44F;&c_ch;&#x43D;&c_i;&c_j; &c_g;&#x43E;&c_d;&c_i;&#x43D;&#x43D;&c_i;&#x43A;</trd>
  </trdgrp>
</drv>
</art>
