<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gnomon</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="gnomon.0o">
  <kap><tld/>o</kap>
  <snc mrk="gnomon.0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Malnova <ref tip="super" cel="horlog.sun0o.KAL">sunhorlo&gcirc;o</ref>
      kun vertikala ombro&jcirc;etilo. 
    </dif>
    <trdgrp lng="hu">
      <trd>nap&oacute;ra</trd>,
      <trd>gn&oacute;mon</trd>
    </trdgrp>
  </snc>
  <snc mrk="gnomon.0o.MAS">
    <dif>
      La ombro&jcirc;etilo mem.  
    </dif>
    <trdgrp lng="hu">
      <trd>nap&oacute;ramutat&oacute;</trd>,
      <trd>&aacute;rny&eacute;kvet&odblac; oszlop<klr tip="ind"> (nap&oacute;r&aacute;n)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="ca">gn&ograve;mon</trd>
  <trd lng="cs">gnómon</trd>
  <trd lng="de">Gnomon</trd>
  <trd lng="en">gnomon</trd>
  <trd lng="es">gnomon</trd>
  <trd lng="fr">gnomon</trd>
  <trd lng="it">gnomone</trd>
  <trd lng="nl">gnomon</trd>
  <trd lng="pl">gnomon</trd>
  <trd lng="pt">gn&ocirc;mon</trd>
  <trd lng="ru">&c_g;&c_n;&c_o;&c_m;&c_o;&c_n;</trd>
  <trd lng="sk">gnómon</trd>
</drv>
</art>
