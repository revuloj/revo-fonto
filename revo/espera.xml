<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>Esperant</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="espera.0o">
  <kap><tld/>o</kap>
  <snc mrk="espera.0o.LIN">
    <uzo tip="fak">ESP</uzo>
    <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="lst" cel="lingv.0o.homa" lst="voko:lingvoj">Lingvo</ref>
      internacia kreita de <ref cel="zamenhof.0o">Zamenhofo</ref>
      en 1887:
      <ekz>
        en la lingvo &leftquot;<tld/>o&rightquot; ni vidas la
        estontan lingvon de la tuta mondo
        <fnt><bib>F</bib>, <lok>&FE; 22</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_e;&c_s;&c_p;&c_e;&c_r;&c_a;&c_n;&c_t;&c_a;</trd>
    <trd lng="hu">eszperant&oacute;</trd>
    <trd lng="ru">&c_e;&c_s;&c_p;&c_je;&c_r;&c_a;&c_n;&c_t;&c_o;</trd>
  </snc>
  <snc mrk="espera.0o.dro">
    <uzo tip="fak">PERS</uzo>
    <dif>
      <klr>(Propre &leftquot;doktoro <tld/>o&rightquot;.)</klr>
      Plumnomo sub kiu <ref tip="sin" cel="zamenhof.0o">Zamenhofo</ref>
      publikigis sian lingvoprojekton kaj kiu i&gcirc;is nomo de la
      lingvo mem.
    </dif>
    <trd lng="be">&c_E;&c_s;&c_p;&c_e;&c_r;&c_a;&c_n;&c_t;&c_a;</trd>
    <trd lng="hu">Esperanto</trd>
    <trd lng="ru">&c_E;&c_s;&c_p;&c_je;&c_r;&c_a;&c_n;&c_t;&c_o;</trd>
  </snc>
  <trd lng="ca">esperanto</trd>
  <trd lng="de">Esperanto</trd>
  <trd lng="en">Esperanto</trd>
  <trd lng="es">Esperanto</trd>
  <trd lng="fr">Esp&eacute;ranto</trd>
  <trd lng="id">Esperanto</trd>
  <trd lng="it">Esperanto</trd>
  <trd lng="pl">Esperanto</trd>
  <trd lng="pt">Esperanto</trd>
  <trd lng="sv">Esperanto</trd>
</drv>

<drv mrk="espera.0ido">
  <kap><tld lit="e"/>ido</kap>
  <snc mrk="espera.0ido.ESP">
    <uzo tip="fak">ESP</uzo>
    <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="super" cel="lingv.plan0o.LIN">Planlingvo</ref>
      kreita post <tld/>o kaj grandparte &gcirc;in imitanta:
      <ekz>
        anta&ubreve;tagmeze okazis mia prelego pri <tld lit="e"/>idoj
        <fnt>
          <aut>C. J. G&uuml;nkel</aut>,
          <vrk><url
          ref="http://tech.groups.yahoo.com/group/esp-novo/message/722">
          GIL 2000</url></vrk>,
          <lok>esp-novo, 2000-11-19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">esp&eacute;rantide</trd>
</drv>

<drv mrk="espera.0ismo">
  <kap><tld lit="e"/>ismo<fnt>Z</fnt></kap>
  <snc mrk="espera.0ismo.ESP">
    <uzo tip="fak">ESP</uzo>
    <dif>
      Penado disvastigi en la tuta mondo la uzadon de lingvo
      ne&ubreve;trale homa<sup>Z</sup>.
    </dif>
   <trdgrp lng="it">
    <trd>esperantismo</trd>,
    <trd>movimento <ind>esperantista</ind></trd>
  </trdgrp>
    <trd lng="pl">esperantyzm</trd>
    <trd lng="pt">esperantismo</trd>
    <trd
    lng="ru">&c_e;&c_s;&c_p;&c_je;&c_r;&c_a;&c_n;&c_t;&c_i;&c_z;&c_m;</trd>
  </snc>
  <snc mrk="espera.0ismo.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      Dirmaniero speciala al <tld/>o:
      <ekz>
        tiu formo jam de tre longe uzata
        fari&gcirc;is <tld lit="e"/>ismo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="it">esperantismo</trd>
    <trd lng="pl">esperantyzm</trd>
    <trd lng="pt">esperantismo</trd>
  </snc>
  <trd lng="ca">esperantisme</trd>
  <trd lng="de">Esperantismus</trd>
  <trd lng="fr">esp&eacute;rantisme</trd>
  <trd lng="hu">eszperantizmus</trd>
</drv>

<drv mrk="espera.0isto">
  <kap><tld lit="e"/>isto</kap>
  <snc mrk="espera.0isto.ESP">
    <uzo tip="fak">ESP</uzo>
    <dif>
      Persono, kiu scias kaj uzas <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_s;&c_p;&c_e;&c_r;&c_a;&c_n;&c_t;&c_y;&c_s;&c_t;</trd>
  <trd lng="ca">esperantista</trd>
  <trd lng="de">Esperantist</trd>
  <trd lng="es">esperantista</trd>
  <trd lng="fr">esp&eacute;rantiste</trd>
  <trd lng="hu">eszperantista</trd>
  <trd lng="id">esperantis</trd>
  <trd lng="it">esperantista<klr> (sost.)</klr></trd>
  <trd lng="pl">esperantysta</trd>
  <trd lng="pt">esperantista</trd>
  <trd
  lng="ru">&c_e;&c_s;&c_p;&c_je;&c_r;&c_a;&c_n;&c_t;&c_i;&c_s;&c_t;</trd>
  <trd lng="sv">esperantist</trd>
</drv>

<drv mrk="espera.0isteco">
  <kap><tld lit="e"/>isteco<fnt>Z</fnt></kap>
  <snc mrk="espera.0isteco.ESP">
    <dif>
      Scio kaj uzado de <tld/>o.
    </dif>
  </snc>
  <trd lng="hu">eszperantistas&aacute;g</trd>
  <trd lng="it">esperantismo</trd>
  <trd lng="pt">esperantismo</trd>
</drv>

<drv mrk="espera.0ujo">
  <kap><tld/>ujo<fnt>Z</fnt></kap>
  <snc mrk="espera.0ujo.ESP">
    <uzo tip="fak">ESP</uzo>
    <dif>
      La ideala lando de <tld lit="e"/>istoj,
      la <tld lit="e"/>ista mondo:
      <ekz>
        ni &ccirc;iujare kunvenas en la &ccirc;efurbo de <tld/>ujo
        <klr>(la urbo de la vica Universala Kongreso)</klr>,
        por vigligi en ni la amon al la ideo esperantisma
        <fnt>
        <bib>Paroloj</bib>,
        <vrk>anta&ubreve; la Tria Kongreso Esperantista</vrk>.
        <lok>Cambridge, 1907</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvankam la&ubreve;nombre <tld/>ujo ne superas urbon kun
        cent mil lo&gcirc;antoj, &ccirc;iujare en &gcirc;i okazas centoj
        da diversspecaj renkonti&gcirc;oj, funkcias kelkaj
        miloj da klub(et)oj kaj asocioj, aperas multaj
        gazetoj kaj libroj
        <fnt>
          <bib>LOdE</bib>,
          <vrk>Esperanto en 2000</vrk>,
          <lok>2001:1</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Esperantowelt</trd>
  <trdgrp lng="fr">
    <trd>Esp&eacute;rantie</trd>,
    <trd>pays de l'esp&eacute;ranto</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>Eszperancia</trd>,
    <trd>Eszperant&oacute;orsz&aacute;g</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>Esperantia</trd>,
    <trd>comunit&agrave; <ind>esperantista</ind></trd>
  </trdgrp>
  <trd lng="pl">Esperancja</trd>
  <trdgrp lng="pt">
    <trd>Esper&acirc;ntia</trd>,
    <trd>pa&iacute;s do <ind>Esperanto</ind></trd>
  </trdgrp>
  <trd
  lng="ru">&c_E;&c_s;&c_p;&c_je;&c_r;&c_a;&c_n;&c_t;&c_i;&c_d;&c_a;</trd>
</drv>
</art>
