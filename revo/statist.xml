<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: statist.xml,v 1.10 2024/03/25 07:28:36 revo Exp $">
<kap>
  <rad>statist</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="statist.0o">
  <kap><tld/>o</kap>
  <snc mrk="statist.0o.KIN">
    <uzo tip="fak">KIN</uzo>
    <uzo tip="fak">TEA</uzo>
    <dif>
      Akcesora neparolanta <ref tip="super" cel="aktor.0o">aktoro</ref>;
      <ref tip="sin" cel="figur.0anto.KIN">figuranto</ref>.
      <ekz>
        sur scenejo amasi&gcirc;is kelkaj centoj da <tld/>oj
        <fnt>
          <aut>I. Ilf, E. Petrov, trad. V. Okc</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Usono/Enhavo.html">
          Unueta&gcirc;a Usono</url></vrk>,
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_ib;&c_g;&c_u;&c_r;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_t;&c_y;&c_s;&c_t;</trd>,
    <trd>&c_a;&c_k;&c_t;&c_o;&c_r; &c_k;&c_a;&c_r;&c_d;&c_e;&c_b;&c_a;&c_l;&c_je;&c_t;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>figurant</trd>,
    <trd>statista</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Komparse</trd>,
    <trd>Statist</trd>
  </trdgrp>
  <trd lng="fr">figurant</trd>
  <trdgrp lng="hu">
    <trd>statiszta</trd>,
    <trd>mell&eacute;kszerepl&odblac;</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>figurante<klr> (cin. e spettac.)</klr></trd>,
    <trd>comparsa<klr> (cin. e teat.)</klr></trd>
  </trdgrp>
  <trd lng="nl">figurant</trd>
  <trdgrp lng="pl">
    <trd>figurant</trd>,
    <trd>statysta</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_t;&c_i;&c_s;&c_t;(&c_k;&c_a;)
    <ind>&c_k;&c_o;&c_r;&c_d;&c_je;&c_b;&c_a;&c_l;&c_je;&c_t;&c_a;</ind></trd>;
    <trd>&c_s;&c_t;&c_a;&c_t;&c_i;&c_s;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>figurant</trd>,
    <trd>&scaron;tatista</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_t;&c_a;&c_t;&c_i;&c_s;&c_t;</trd>
  <trd lng="zh">&#x914D;&#x89D2; <pr>p&#xE8;iju&#xE9;</pr></trd>
</drv>

<drv mrk="statist.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="statist.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Aperi por nura impresado, sen reala uzo a&ubreve; utilo:
      <ekz>
        la kanonoj de la In&gcirc;eniera Regimento nur mute
    	<tld/>is kiel impona dekora&jcirc;o &ccirc;e la ekskursoj
    	<fnt>
          <aut>J. Baghy</aut>,
          <vrk>Sur Sanga Tero</vrk>,
          <lok>1933</lok>
        </fnt>.
      </ekz>
    </dif>
    </snc>
    <trd lng="be">&c_b;&c_y;&c_c;&c_mol; <ind>&c_s;&c_t;&c_a;&c_t;&c_y;&c_s;&c_t;&c_a;&c_m;</ind></trd>
    <trd lng="de">eine <ind>Nebenrolle</ind> spielen</trd>
    <trdgrp lng="fr">
      <trd>faire de la figuration</trd>,
      <trd>faire tapisserie</trd>
    </trdgrp>
    <trd lng="hu">statiszt&aacute;l</trd>
    <trd lng="pl">statystowa&cacute;</trd>
</drv>
</art>

