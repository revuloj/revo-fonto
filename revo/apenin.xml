<vortaro>
  <art mrk="$Id: apenin.xml,v 1.1 2025/02/22 20:25:27 afido Exp $">
    <kap>
      <rad>Apenin</rad>/oj
      <fnt><bib>JED</bib></fnt>
    </kap>

<drv mrk="apenin.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:montaroj" cel="mont.0aro">Monto&ccirc;eno</ref> etendi&gcirc;anta
      la&ubreve;longe de <ref tip="malprt" cel="ital.I0ujo">Italujo</ref>:
      <ekz>
        la gentoj, kiujn oni trovas sur la <tld/>a Duoninsulo <klr>[&#8230;]</klr> parolis tre
        diversajn lingvojn italajn-keltajn
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        post milionoj da jaroj <klr>[&#8230;]</klr> la fekunda Pada Ebena&jcirc;o malaperos,
        kunpremi&gcirc;inte inter <tld/>oj kaj Alpoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2013/010777.php"
              >&Gcirc;is la tero tremas</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="insul.Itala_Duon0o">Apenina Duoninsulo</ref>
    <trd lng="de">Apeninnen</trd>
    <trd lng="en">Apennine Mountains </trd>
    <trd lng="fr">Apennins</trd>
    <trd lng="ru">&c_A;&c_p;&c_je;&c_n;&c_n;&c_i;&c_n;&c_y;</trd>
    <trd lng="uk">&c_A;&c_p;&c_je;&c_n;&c_n;&c_ib;&c_n;&c_i;</trd>
  </snc>
</drv>

  </art>

