<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>gelat</rad>/o</kap>
<drv mrk="gelat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Duonsolida, tremanta maso, kiu konsistas el malvarmigitaj solva&jcirc;oj
      de gelateno a&ubreve; el iuj plantaj substancoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="gelate.0o">gelateno</ref>,
      <ref cel="jxele.0o">&jcirc;eleo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_zh;&c_e;&c_l;&c_je;</trd>
  </trdgrp>
  <trd lng="ca">gel<klr> (qu&iacute;m.)</klr></trd>
  <trdgrp lng="de">
    <trd>Gel</trd>,
    <trd>Gelee</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gel<klr tip="ind"> (chim.)</klr></trd>,
    <trd>gel&eacute;e<klr tip="ind"> (chim.)</klr></trd>
  </trdgrp>
  <trd lng="hu">g&eacute;l</trd>
  <trdgrp lng="nl">
    <trd>gel</trd>,
    <trd>gelei</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>gel</trd>,
    <trd>gel&eacute;ia</trd>
  </trdgrp>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_je;</trd>
</drv>

<drv mrk="gelat.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Fari&gcirc;i <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_je;&c_r;&c_a;&c_w;&c_t;&c_v;&c_a;&c_r;&c_y;&c_c;&c_c;&c_a; &c_w; <ind>&c_g;&c_je;&c_l;&c_mol;</ind></trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_w;&c_t;&c_v;&c_a;&c_r;&c_y;&c_c;&c_c;&c_a; &c_w; <ind>&c_zh;&c_e;&c_l;&c_je;</ind></trd>
  </trdgrp>
  <trd lng="ca">gelificar-se</trd>
  <trdgrp lng="de">
    <trd>gelieren</trd>,
    <trd>eindicken</trd>
  </trdgrp>
  <trd lng="fr">se <ind>g&eacute;lifier</ind></trd>
  <trd lng="hu">g&eacute;lesedik</trd>
  <trd lng="nl">indikken</trd>
</drv>
</art>
