<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gelat</rad>/o
</kap>
<drv mrk="gelat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Duonsolida, tremanta maso, kiu konsistas el malvarmigitaj solva&jcirc;oj
      de gelateno a&ubreve; el iuj plantaj substancoj. 
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Gel</trd>,
     <trd>Gelee</trd></trdgrp>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_je;</trd>
</drv>
<drv mrk="gelat.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Fari&gcirc;i <tld/>o.  
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>gelieren</trd>,
     <trd>eindicken</trd></trdgrp>
</drv>
</art>

