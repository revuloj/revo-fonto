<drv mrk="eviden.0a">
  <kap><tld/>a</kap>
  <snc mrk="eviden.0a.LOG">
    <dif>
      Tiel <ref cel="klar.0a.komprenebla">klara<sncref/></ref>
      kaj <ref cel="dub.sen0a">senduba</ref>, ke &gcirc;i ne bezonas pruvon:
      <ekz>
        <tld/>a fakto
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        nur <tld/>aj preseraroj estas korektitaj
        <fnt>
          <bib>KPr</bib>
          <lok>Anta&ubreve;vorto</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio estas <tld/>a signo de la justa ju&gcirc;o de Dio
        <fnt>
          <bib>NT</bib>
          <lok>II. Tesalonikanoj 1:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> antikva &scirc;tupa piramido <klr>[estas]</klr> la plej <tld/>a bildo
        de nia regno
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ne esperu atingi pardonon pri sia <tld/>a kulpo
        <fnt>
          <bib>DKM</bib>
          <lok>&Ccirc;apitro LXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a sincereco kaj la varmeco de lia simpatio estas sufi&ccirc;a senkulpigo
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cert.0a">certa</ref>,
      <ref cel="konklu.0iga">konkludiga</ref>,
      <ref cel="frap.okul0a">okulfrapa</ref>,
      <ref cel="palp.0ebla">palpebla</ref>,
      <ref cel="vid.0ebla">videbla</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ib;&c_d;&c_a;&c_v;&c_o;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_u;&c_m;&c_n;&c_je;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_ja;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trd
  lng="bo">&t_ma;&t_nga;&t_naro;&t_na;&t_tsheg;&t_ga;&t_sa;&t_la;&t_tsheg;&t_da;&t_naro;&t_da;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>evidentn&iacute;</trd>,
    <trd>jasn&yacute;</trd>,
    <trd>nepochybn&yacute;</trd>,
    <trd>o&ccaron;ividn&yacute;</trd>,
    <trd>zjevn&yacute;</trd>,
    <trd>z&rcaron;ejm&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>offensichtlich</trd>,
    <trd>offenkundig</trd>,
    <trd>offenbar</trd>,
    <trd>augenscheinlich</trd>,
    <trd>einleuchtend</trd>,
    <trd>naheliegend</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>evident</trd>,
     <trd>clear</trd>,
     <trd>obvious</trd>,
     <trd>unmistakable</trd>
  </trdgrp>
  <trd lng="es">evidente</trd>
  <trdgrp lng="fr">
    <trd>&eacute;vident</trd>,
    <trd>manifeste<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nyilv&aacute;nval&oacute;</trd>,
    <trd>evidens</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>jelas</trd>,
    <trd>terang</trd>,
    <trd>nyata</trd>,
    <trd>kentara</trd>,
    <trd>curai</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x660E;&#x767D;&#x306A;  <pr>&#x3081;&#x3044;&#x306F;&#x304F;&#x306A;</pr></trd>,
    <trd>&#x81EA;&#x660E;&#x306E;  <pr>&#x3058;&#x3081;&#x3044;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ewidentny</trd>,
    <trd>oczywisty</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>evidente</trd>,
    <trd>&oacute;bvio</trd>,
    <trd>inequ&iacute;voco</trd>,
    <trd>patente</trd>,
    <trd>manifesto</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_ch;&c_je;&c_v;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_ja;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_o;&c_m;&c_n;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>zjavn&yacute;</trd>,
    <trd>zrejm&yacute;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>tydlig</trd>,
    <trd>uppenbar</trd>,
    <trd>sj&auml;lvklar</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_o;&c_ch;&c_je;&c_v;&c_i;&c_d;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_ja;&c_v;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_s;&c_u;&c_m;&c_n;&c_ib;&c_v;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x770B;&#x628A; <pr>k&agrave;nb&#x1CE;</pr></trd>,
    <trd>&#x663E;&#x800C;&#x6613;&#x89C1; <pr>xi&#x1CE;n&eacute;ry&igrave;ji&agrave;n</pr></trd>,
    <trd>&#x5F70;&#x5F70; <pr>zh&amacron;ngzh&amacron;ng</pr></trd>,
    <trd>&#x663E;&#x89C1; <pr>xi&#x1CE;nji&agrave;n</pr></trd>,
    <trd>&#x663E;&#x7136; <pr>xi&#x1CE;nr&aacute;n</pr></trd>,
    <trd>&#x660E;&#x663E;&#x5730; <pr>m&iacute;ngxi&#x1CE;nde</pr></trd>,
    <trd>&#x660E;&#x663E; <pr>m&iacute;ngxi&#x1CE;n</pr></trd>,
    <trd>&#x663E;&#x660E; <pr>xi&#x1CE;nm&iacute;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="eviden.0e">
  <kap><tld/>e</kap>
  <snc mrk="eviden.0e.LOG">
    <dif>
      Tute <ref tip="sin" cel="dub.sen0e">sendube</ref>:
      <ekz>
      la dika metala bastonpinto estas tute trivita, &ccirc;ar <tld/>e li
        multe mar&scirc;is kun &gcirc;i
        <fnt>
          <aut>A. Conan Doyle, trad. W. Auld</aut>
          <vrk>La &ccirc;ashundo de la Baskerviloj</vrk>
          <lok>p. 8a</lok>
        </fnt>;
      </ekz>
      <ekz>
        el la labora&jcirc;o, kiun vi pretigis montri&gcirc;as <tld/>e, ke vi <klr>[&#x2026;]</klr> havas
        efektive kapablon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e tiu viro, kiun li konas apena&ubreve;, estas sincera en siaj certigoj
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvara</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ib;&c_d;&c_a;&c_v;&c_o;&c_ch;&c_n;&c_a;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_u;&c_m;&c_n;&c_je;&c_n;&c_n;&c_a;</trd>,
    <trd>&c_ja;&c_w;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>evidentn&ecaron;</trd>,
    <trd>jasn&ecaron;</trd>,
    <trd>nasnad&ecaron;</trd>,
    <trd>nasnad&ecaron;</trd>,
    <trd>nepochybn&ecaron;</trd>,
    <trd>o&ccaron;ividn&ecaron;</trd>,
    <trd>z&rcaron;ejm&ecaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>offensichtlich</trd>,
    <trd>offenkundig</trd>,
    <trd>offenbar</trd>,
    <trd>augenscheinlich</trd>,
    <trd>einleuchtend</trd>,
    <trd>unzweifelhaft</trd>
  </trdgrp>
  <trd lng="en">obviously</trd>
  <trd lng="es">evidentemente</trd>
  <trdgrp lng="fr">
    <trd>&eacute;videmment</trd>,
    <trd>&agrave; l'&eacute;vidence</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nyilv&aacute;nval&oacute;an</trd>,
    <trd>k&eacute;ts&eacute;gtelen&uuml;l</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>cara</baz>secara jelas</trd>,
    <trd><baz>cara</baz>secara terang</trd>,
    <trd><baz>cara</baz>secara nyata</trd>,
    <trd><baz>cara</baz>secara kentara</trd>,
    <trd><baz>cara</baz>secara curai</trd>,
    <trd>dengan jelas</trd>,
    <trd>dengan terang</trd>,
    <trd>dengan nyata</trd>,
    <trd>dengan kentara</trd>,
    <trd>dengan curai</trd>
  </trdgrp>
  <trd lng="ja">&#x660E;&#x3089;&#x304B;&#x306B;  <pr>&#x3042;&#x304D;&#x3089;&#x304B;&#x306B;</pr></trd>
  <trdgrp lng="pl">
    <trd>ewidentnie</trd>,
    <trd>oczywi&sacute;cie</trd>,
    <trd>pewnie</trd>
  </trdgrp>
  <trd lng="ro">desigur</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_ch;&c_je;&c_v;&c_i;&c_d;&c_n;&c_o;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_o;&c_m;&c_n;&c_je;&c_n;&c_n;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>navidomo&ccaron;i</trd>,
    <trd>nepochybne</trd>,
    <trd>zrejme</trd>
  </trdgrp>
  <trd lng="sv">tydligen</trd>
  <trdgrp lng="uk">
    <trd>&c_o;&c_ch;&c_je;&c_v;&c_i;&c_d;&c_n;&c_o;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_s;&c_u;&c_m;&c_n;&c_ib;&c_v;&c_n;&c_o;</trd>
  </trdgrp>
  <trd lng="zh">&#x660E;&#x663E;&#x5730; <pr>m&iacute;ngxi&#x1CE;nde</pr></trd>
</drv>

<drv mrk="eviden.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="eviden.0ajxo.LOG">
    <dif>
      Afero evidenta, klare videbla a&ubreve; memkomprenebla, kiu ne bezonas pruvon:
      <ekz>
        li ekkredis, kiel oni kredas <tld/>a&jcirc;on
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        tie &ccirc;i mi devas substreki <tld/>a&jcirc;on: elektra nukleenergio ja havas riskojn kaj
        malavanta&gcirc;ojn
        <fnt>
          <bib>Kon11</bib>
          <lok>Inter riskoj kaj timoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Offensichtliches</trd>,
      <trd>Offenbarung</trd>
    </trdgrp>
  </snc>
  <snc mrk="eviden.0ajxo.atesto">
    <dif>
      Evidentiga&jcirc;o, <ref cel="manife.0ajxo">manifesta&jcirc;o</ref>:
      <ekz>
        <klr>[li]</klr> rigardis <klr>[&#x2026;]</klr> unu el la &ccirc;ieaj <tld/>a&jcirc;oj de idolkulto
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="atest.0o">atesto</ref>,
      <ref cel="pruv.0o">pruvo</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Daseinsbeweis</trd>,
      <trd>Anzeichen</trd>,
      <trd>Manifestation</trd>
    </trdgrp>
  </snc>
  <trd lng="be"> &c_v;&c_ib;&c_d;&c_a;&c_v;&c_o;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol; <klr>(&c_n;&c_je;&c_sh;&c_t;&c_a;)</klr></trd>
  <trd lng="fr">&eacute;vidence</trd>
  <trdgrp lng="id">
    <trd>bukti</trd>,
    <trd><baz>nyata</baz>pernyataan</trd>,
    <trd><baz>nyata</baz>kenyataan</trd>,
    <trd><baz>saksi</baz>kesaksian</trd>,
    <trd><baz>terang</baz>keterangan</trd>
  </trdgrp>
  <trd lng="pl">rzecz oczywista</trd>
</drv>

<drv mrk="eviden.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="eviden.0eco.LOG">
    <dif>
      Klareco, kiun prezentas la vero al la spirito,
      estigante certecon:
      <ekz>
        la <tld/>eco de la geometriaj demonstroj;
      </ekz>
      <ekz>
        ni devas akcepti la <tld/>econ, tial ke anta&ubreve; ni staras fakto
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Deka</lok>
        </fnt>;
      </ekz>
      <ekz>
        nei pli longatempe estus nei la <tld/>econ
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekdua</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ib;&c_d;&c_a;&c_v;&c_o;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_u;&c_m;&c_n;&c_je;&c_n;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>evidentnost</trd>,
    <trd>nepochybnost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Offensichtlichkeit</trd>,
    <trd>Augenscheinlichkeit</trd>,
    <trd>Evidenz</trd>
  </trdgrp>
  <trd lng="en">obviousness</trd>
  <trd lng="fr">&eacute;vidence</trd>
  <trdgrp lng="hu">
    <trd>nyilv&aacute;nval&oacute;s&aacute;g</trd>,
    <trd>evidencia</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>jelas</baz>kejelasan</trd>,
    <trd><baz>nyata</baz>kenyataan</trd>
  </trdgrp>
  <trd lng="ja">&#x660E;&#x767D;&#x3055;  <pr>&#x3081;&#x3044;&#x306F;&#x304F;&#x3055;</pr></trd>
  <trdgrp lng="pl">
    <trd>ewidentno&sacute;&cacute;</trd>,
    <trd>oczywisto&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_ch;&c_je;&c_v;&c_i;&c_d;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_o;&c_m;&c_n;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nepochybnos&tcaron;</trd>,
    <trd>zrejmos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_o;&c_ch;&c_je;&c_v;&c_i;&c_d;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_s;&c_u;&c_m;&c_n;&c_ib;&c_v;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="eviden.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="eviden.0igi.LOG">
    <dif>
      Igi <tld/>a, klare elmontri:
      <ekz>
        mi timas, ke la malka&scirc;i&gcirc;o de mia kulpeco samtempe <tld/>igos vian kunkulpecon
        <fnt>
          <bib>MrA</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> <tld/>igis sian plenan malkapablon kaj malta&ubreve;gecon
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        frapanta malsameco <tld/>igis grandan diferencon en iliaj karakteroj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_ja;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_v;&c_ib;&c_d;&c_a;&c_v;&c_o;&c_ch;&c_n;&c_y;&c_m;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>offenbaren</trd>,
    <trd>aufzeigen</trd>,
    <trd>zeigen <klr>(offenbaren)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>montrer avec &eacute;vidence</trd>,
    <trd>rendre <ind>&eacute;vident</ind></trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>bukti</baz>membuktikan</trd>,
    <trd><baz>jelas</baz>menjelaskan</trd>,
    <trd><baz>lihat</baz>memperlihatkan bukti</trd>,
    <trd><baz>terang</baz>menerangkan</trd>,
    <trd><baz>tunjuk</baz>menunjukkan bukti</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wyjawia&cacute;</trd>,
    <trd>unaocznia&cacute;</trd>,
    <trd>wykazywa&cacute;</trd>
  </trdgrp>
</drv>

<drv mrk="eviden.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="eviden.0igxi.LOG">
    <dif>
      I&gcirc;i klare vera, nedubebla:
      <ekz>
        via progreso <tld/>i&gcirc;u al &ccirc;iuj
        <fnt>
          <bib>NT</bib>
          <lok>I. Timoteo 4:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;is 33 malsanoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        jam <tld/>i&gcirc;is ke alia tutmonda milito,
        pli detrua ol la unua, estis neevitebla
        <fnt>
          <aut>W. Heller, trad. B. Westerhoff</aut>
          <vrk>Lidia, la Vivo de Lidia Zamenhof, Filino de Esperanto</vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_ja;&c_v;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_a;&c_k;&c_a;&c_z;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_ja;&c_w;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_ja;&c_w;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><ind>offensichtlich</ind> werden</trd>,
    <trd>zu <ind>Tage</ind> treten</trd>,
    <trd>sich <ind>offenbaren</ind></trd>
  </trdgrp>
  <trd lng="fr">s'<ind>av&eacute;rer</ind></trd>
  <trdgrp lng="id">
