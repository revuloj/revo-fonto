<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: poziti.xml,v 1.35 2021/05/29 14:33:07 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>pozitiv</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="poziti.0a">
  <kap><tld/>a</kap>
  <snc mrk="poziti.0a.FIL">
    <dif>
      Bazita nur sur reala&jcirc;oj, faktoj:
      <ekz>
        <tld/>a filozofio, scienco.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>positiv</trd>,
      <trd>positivistisch</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_o;&c_z;&c_i;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="poziti.0a.praktike">
    <dif>
      Reala, praktike efektivi&gcirc;inta:
      <ekz>
        <tld/>aj rezultatoj.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>f&uuml;hlbar</trd>,
      <trd>greifbar</trd>,
      <trd>tats&auml;chlich</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_z;&c_i;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_p;&c_o;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="poziti.0a.malnegativa">
    <dif>
      Donanta, asertanta ion certan:
      <ekz>
        ligi la homojn povas nur sameco <tld/>a, sed ne
        negativa<fnt>Z</fnt>;
      </ekz>
      <ekz>
        senreligieco donas al la homo neniun <tld/>an
        apogon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        feli&ccirc;e ni povas respondi tiun demandon tute <tld/>e<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_z;&c_i;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_u;&c_t;&c_v;&c_je;&c_r;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="poziti.0a.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      Asertanta simple la kvaliton, sen ia komparo:
      <ekz>
        <tld/>a adjektivo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kompar2.0o">komparativo</ref>,
      <ref cel="superl.0o">superlativo</ref>.
    </refgrp>
    <trd lng="es">positivo</trd>
    <trd lng="de">positiv</trd>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; <klr>(&c_o; &c_s;&c_t;&c_je;&c_p;&c_je;&c_n;&c_i; &c_s;&c_r;&c_a;&c_v;&c_n;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>
  </snc>
  <snc mrk="poziti.0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="poziti.0a.nombro">
      <fnt><bib>MatTerm</bib><lok>p. 7</lok></fnt>
      <dif>
        <klr>(p.p. <ref cel="nombr.0o">nombro</ref> a&ubreve; p.p. elemento en
        <ref cel="ord.0a.aro">orda</ref>, adicie signata
        <ref cel="monoid.0o">monoido</ref>)</klr>
        Pli granda ol <ref cel="nul0.0o.neuxtra">nulo</ref>:
        <ekz>
          <tld/>a kvanto povas esti signata per la signo <frm>+</frm>
          <klr>(ekz-e <frm>+3</frm> a&ubreve; simple <frm>3</frm>)</klr>;
        </ekz>
        <ekz>
          kvadrato de reelo estas &ccirc;iam <tld/>a.
        </ekz>
      </dif>
      <ref tip="ant" cel="negati.0a.nombro">negativa.</ref>
      <rim>
        Kiel por &ccirc;iu <ref cel="rilat.ordo0o">ordo-rilato</ref>
        ekzistas ambigueco, &ccirc;u oni konsideras &gcirc;in strikte
        a&ubreve; malstrikte.
        La&ubreve; la kunteksto eblas konsideri, ke <frm>0</frm> estas
        <tld/>a a&ubreve; ne.
      </rim>
      <trd lng="de">positiv</trd>
      <trd lng="en">positive</trd>
      <trd lng="pl">dodatni</trd>
      <trd lng="ru">&c_p;&c_o;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </subsnc>
    <subsnc mrk="poziti.0a.izometrio">
      <dif>
        <klr>(p.p. <ref cel="afin.0a.bildigo">afina</ref>
        <ref tip="super" cel="izomet.0o">izometrio</ref>)</klr>
        Tia, ke la <ref cel="determ.0anto.endomorfio">determinanto</ref>
        de &gcirc;ia asociita endomorfio estas <tld/>a
        <sncref ref="poziti.0a.nombro"/>.
      </dif>
      <ref tip="ant" cel="negati.0a.izometrio">negativa;</ref>
      <ref tip="vid" cel="lok.de0igo.MAT">delokigo.</ref>
      <rim>
        Ni ne trovis a&ubreve;toritatan fonton por tiu &ccirc;i termino,
        sed aspektas, ke inter &ccirc;iuj nacilingvaj metafora&jcirc;oj
        <ekz>
          pozitiva,
        </ekz>
        <ekz>
          rekta,
        </ekz>
        <ekz>
          propra...
        </ekz>
        tiu &ccirc;i estas la plej facile pravigebla kaj oportuna por krei
        malasencan terminon.
      </rim>
      <trd lng="de">eigentlich<klr tip="ind"> (-e Bewegung)</klr></trd>
      <trd lng="en">proper<klr tip="ind"> (movement, motion)</klr></trd>
      <trdgrp lng="fr">
        <trd><klr tip="amb">(isom&eacute;trie) </klr>directe</trd>,
        <trd><klr tip="amb">(isom&eacute;trie) </klr>positive</trd></trdgrp>
      <trd lng="pl"><klr tip="amb">(izometria) </klr>parzysta</trd>
      <trd lng="ru">&c_s;&c_o;&c_b;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;&c_je;<klr tip="amb"> (&c_d;&c_v;&c_i;&c_zh;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
    </subsnc>
  </snc>
  <snc mrk="poziti.0a.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      Karakterizas bildon, kies nigra&jcirc;oj kaj blanka&jcirc;oj respektive
      respondas al tiuj de la reproduktita bildo.
    </dif>
    <trd lng="de">positiv</trd>
    <trd lng="ru">&c_p;&c_o;&c_z;&c_i;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="poziti.0a.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Karakterizas tiun el la du elektroj, kiu respondas al la karbo &ccirc;e
      pilo.
    </dif>
    <refgrp tip="vid">
      <ref cel="negati.0a">negativa</ref>.
    </refgrp>
    <trd lng="de">positiv</trd>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_z;&c_ib;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_n;&c_o;&c_w;&c_ch;&c_y;</trd>
    <trd>&c_d;&c_a;&c_d;&c_a;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bo">&#xF42;&#xF4F;&#xF53;&#xF0B;&#xF41;&#xF7A;&#xF63;&#xF0B;&#xF54;&#xF7C;&#xF0B;</trd>
  <trd lng="ca">positiu</trd>
  <trdgrp lng="cs">
    <trd>kladn&#xFD;</trd>,
    <trd>pozitivn&#xED;</trd>
  </trdgrp>
  <trd lng="fr">positif</trd>
  <trd lng="hu">pozit&iacute;v</trd>
  <trd lng="nl">positief</trd>
  <trd lng="pt">positivo</trd>
  <trdgrp lng="sk">
    <trd>kladn&#xFD;</trd>,
    <trd>pozit&#xED;vny</trd>
  </trdgrp>
  <trd lng="tp">pona</trd>
</drv>

<drv mrk="poziti.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="poziti.0ismo.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Doktrino, kiu forpu&scirc;as metafizikon kaj allasas nur eksperimentajn
      faktojn, kontraste al
      <ref tip="ant" cel="spiritual.0ismo.FIL">spiritualismo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_z;&c_ib;&c_t;&c_y;&c_v;&c_ib;&c_z;&c_m;</trd>
  <trd lng="ca">positivisme</trd>
  <trd lng="cs">pozitivizmus</trd>
  <trd lng="de">Positivismus</trd>
  <trd lng="fr">positivisme</trd>
  <trd lng="hu">pozitivizmus</trd>
  <trd lng="nl">positivisme</trd>
  <trd lng="ru">&c_p;&c_o;&c_z;&c_i;&c_t;&c_i;&c_v;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">pozitivizmus</trd>
</drv>

<drv mrk="poziti.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="poziti.0isto.FIL">
    <dif>
      <tld lit="P"/>ismano.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_z;&c_ib;&c_t;&c_y;&c_v;&c_ib;&c_s;&c_t;</trd>
  <trd lng="ca">positivista</trd>
  <trd lng="de">Positivist</trd>
  <trd lng="fr">positiviste</trd>
  <trd lng="hu">pozitivista</trd>
  <trd lng="nl">positivist</trd>
  <trd lng="ru">&c_p;&c_o;&c_z;&c_i;&c_t;&c_i;&c_v;&c_i;&c_s;&c_t;</trd>
</drv>

<drv mrk="poziti.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="poziti.mal0a.ELE">
  <uzo tip="fak">ELE</uzo>
    <dif>
      <klr>(Malo de <ref cel="poziti.0a.ELE"><tld/>a
      <sncref/></ref>)</klr>.
    </dif>
    <ref tip="sin" cel="negati.0a.ELE">negativa <sncref/>.</ref>
    <trd lng="ca">negatiu <klr>(elect.)</klr></trd>
  </snc>
  <snc mrk="poziti.mal0a.FOT">
  <uzo tip="fak">FOT</uzo>
    <dif>
      <klr>(Malo de <ref cel="poziti.0a.FOT"><tld/>a
      <sncref/></ref>)</klr>.
    </dif>
    <ref tip="sin" cel="negati.0a.FOT">negativa <sncref/>.</ref>
    <trd lng="ca">negatiu <klr>(fotog.)</klr></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_e;&c_g;&c_a;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_d;&c_m;&c_o;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">negativ</trd>
  <trd lng="hu">negat&iacute;v</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_r;&c_i;&c_c;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_g;&c_a;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="poziti.ne0a">
  <kap>ne<tld/>a</kap>
  <snc>
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <dif>
      <klr>(p.p. <ref cel="nombr.0o">nombro</ref>)</klr>
      Malpli granda ol <frm>0</frm> a&ubreve; egala al &gcirc;i.
    </dif>
    <rim>
      Vd rimarkon sub <ref cel="negati.ne0a">nenegativa</ref>.
    </rim>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_d;&c_a;&c_d;&c_a;&c_t;&c_n;&c_y;</trd>
  <trd lng="ca">no positiu</trd>
  <trd lng="de">nichtpositiv</trd>
  <trd lng="en">non-positive</trd>
  <trd lng="fr">non positif</trd>
  <trd lng="hu">nem pozit&iacute;v</trd>
  <trd lng="nl">niet-positief</trd>
  <trd lng="pl">niedodatni</trd>
  <trd lng="ru">&c_n;&c_je;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
