<kap>
  <rad>limfangit</rad>/o
</kap>
<drv mrk="limfan.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="inflam.0o" lst="voko:malsanoj">Inflamo</ref>
      de limfaj vaskuloj:
      <ekz>
        1350 Karolo malsani&gcirc;is je <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2264">Karolo la 4-a (Sankta Romia Imperio)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_m;&c_f;&c_a;&c_n;&c_g;&c_ib;&c_t;</trd>
  <trd lng="ca">limfoangitis</trd>
  <trdgrp lng="de">
     <trd>Lymphgef&auml;&szlig;entz&uuml;ndung</trd>,
     <trd>Lymphangitis</trd>
  </trdgrp>
  <trd lng="hu">nyirok&eacute;rgyullad&aacute;s</trd>
  <trd lng="ja">&#x30EA;&#x30F3;&#x30D1;&#x7BA1;&#x708E;  <pr>&#x30EA;&#x30F3;&#x30D1;&#x304B;&#x3093;&#x3048;&#x3093;</pr></trd>
  <trd lng="nl">lymfangitis</trd>
  <trdgrp lng="pl">
  <trd>zapalenie naczy&nacute; <ind>limfatycznych</ind></trd>,
  <trd>zapalenie uk&lstroke;adu <ind>limfatycznego</ind></trd>,
  <trd>zapalenie naczy&nacute; <ind>ch&lstroke;onnych</ind></trd>,
  <trd>zapalenie uk&lstroke;adu <ind>ch&lstroke;onnego</ind></trd>
  </trdgrp>
  <trd lng="pt">linfangite</trd>
  <trd lng="ru">&c_l;&c_i;&c_m;&c_f;&c_a;&c_n;&c_g;&c_i;&c_t;</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x456;&#x43C;&#x444;&#x430;&#x43D;&#x491;&#x456;&#x442;</trd>,
    <trd>&#x43B;&#x456;&#x43C;&#x444;&#x430;&#x43D;&#x491;&#x43E;&#x457;&#x442;</trd>,
    <trd>&#x437;&#x430;&#x43F;&#x430;&#x43B;&#x435;&#x43D;&#x43D;&#x44F; &#x43B;&#x456;&#x43C;&#x444;&#x430;&#x442;&#x438;&#x447;&#x43D;&#x438;&#x445; &#x441;&#x443;&#x434;&#x438;&#x43D;</trd>
  </trdgrp>
</drv>
