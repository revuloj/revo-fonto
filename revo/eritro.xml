<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: eritro.xml,v 1.13 2023/08/31 11:49:30 revo Exp $">
<kap>
  <rad>eritrozin</rad>/o
</kap>

<drv mrk="eritro.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">FOT</uzo>
  <mlg>E&#x202F;127</mlg>
  <snc>
    <dif>
      Ru&gcirc;a substanco tre koloriga, bi-natria salo de 2,4,5,7-tetrajodfluoreskeino,
      uzata en nutroindustrio, histologio kaj fotografio:
      <ekz>
        <tld/>o estas uzata kiel aldono al man&gcirc;a&jcirc;oj, sed pro sia enhavri&ccirc;o de jodo kaj do
        ebla misefiko al la tiroida funkcio permesata nur tre limigite, ekz-e por
        koktelaj &ccirc;erizoj, kolorigiloj de lipoj kaj ovoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_r;&c_y;&c_t;&c_r;&c_a;&c_z;&c_ib;&c_n;</trd>
  <trd lng="bg">&c_je;&c_r;&c_i;&c_t;&c_r;&c_o;&c_z;&c_i;&c_n;</trd>
  <trd lng="de">Erythrosin</trd>
  <trd lng="en">erythrosine</trd>
  <trd lng="fr">&eacute;rythrosine</trd>
  <trd lng="hu">eritrozin</trd>
  <trd lng="ja">&#x30A8;&#x30EA;&#x30B9;&#x30ED;&#x30B7;&#x30F3;</trd>
  <trd lng="nl">erythrosine</trd>
  <trd lng="pl">erytrozyna</trd>
  <trdgrp lng="pt">
    <trd>eritrosina</trd>,
    <trd>eritrina</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_r;&c_i;&c_t;&c_r;&c_o;&c_z;&c_i;&c_n;</trd>
</drv>
</art>
