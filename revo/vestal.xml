<kap>
  <rad>vestal</rad>/o
</kap>
<drv mrk="vestal.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;e antikvaj Romanoj, pastrino de <nom>Vesta</nom>, devigata al virgeco kaj
      al konstanta subtenado de la sankta fajro:
      <ekz>
        la&ubreve; la roma mitologio Romo fondi&gcirc;is la 21-an de aprilo 753 a.K. fare de Romulo
        <klr>[kiu]</klr> poste mortigis sian &gcirc;emelan fraton Remo, <klr>[&#x2026;]</klr> 
        la &gcirc;emeloj estis la&ubreve;legende la gefiloj de la dio Marso kaj
        de la <tld/>o <nom>Rhea Silvia</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3681">Romo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>vestal virgin</trd>,
      <trd>vestal <klr>(seldom)</klr></trd>
    </trdgrp>
    <trd lng="es">vestal</trd>
    <trd lng="de">Vestalin</trd>
    <trd lng="hu">Vesta-sz&udblac;z</trd>    
    <trd lng="nl">Vestaalse <ind>maagd</ind></trd>
    <trd lng="pl">westalka</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tre &ccirc;asta virino.
    </dif>
    <trd lng="de">keusche Frau</trd>
    <trdgrp lng="hu">
      <trd>tiszta n&odblac;</trd>,
      <trd>sz&udblac;zies n&odblac;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_v;&c_e;&c_s;&c_t;&c_a;&c_l;&c_k;&c_a;</trd>
  <trd lng="ca">vestal</trd>
  <trd lng="fr">vestale</trd>
  <trdgrp lng="ja">
    <trd>&#x30A6;&#x30A7;&#x30B9;&#x30BF;&#x306E;&#x5DEB;&#x5973;  <pr>&#x30A6;&#x30A7;&#x30B9;&#x30BF;&#x306E;&#x307F;&#x3053;</pr></trd>,
    <trd>&#x7D14;&#x6F54;&#x306A;&#x4E59;&#x5973;  <pr>&#x3058;&#x3085;&#x3093;&#x3051;&#x3064;&#x306A;&#x304A;&#x3068;&#x3081;</pr></trd>
  </trdgrp>
  <trd lng="pt">vestal</trd>
  <trd lng="ru">&c_v;&c_je;&c_s;&c_t;&c_a;&c_l;&c_k;&c_a;</trd>
  <trd lng="uk">&#x432;&#x435;&#x441;&#x442;&#x430;&#x43B;&#x43A;&#x430; (&#x436;&#x440;&#x438;&#x446;&#x44F; &#x412;&#x435;&#x441;&#x442;&#x438;)</trd>
</drv>
</art>
