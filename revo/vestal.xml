<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>vestal</rad>/o
</kap>
<drv mrk="vestal.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;e antikvaj Romanoj, pastrino de Vesta, devigata al virgeco kaj al
      konstanta subtenado de la sankta fajro.
    </dif>
    <trd lng="de">Vestalin</trd>
    <trd lng="nl">Vestaalse <ind>maagd</ind></trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tre &ccirc;asta virino.
    </dif>
    <trd lng="de">keusche Frau</trd>
  </snc>
  <trd lng="ru">&c_v;&c_je;&c_s;&c_t;&c_a;&c_l;&c_k;&c_a;</trd>
</drv>
</art>
