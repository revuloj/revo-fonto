<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>genu</rad>/o
</kap>
<drv mrk="genu.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Anta&ubreve;a parto de la artiko kuniganta la femuron kun la kruro:
      <ekz>
        preni infanon sur la <tld/>ojn.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Knie</trd>
  <trd lng="fr">genou</trd>
  <trd lng="nl">knie</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_je;&c_n;&c_o;</trd>
</drv>

<drv mrk="genu.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Teni&gcirc;i sur la genuoj:
      <ekz>
        belaj piedoj, vi diras... Nu, li <tld/>os &ccirc;e ili
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 25a, p. 177a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd>s'<ind>agenouiller</ind></trd>
</drv>

<drv mrk="genu.sur0i">
  <kap>(sur)<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Fleksi la <tld/>ojn.
    </dif>
    <trd lng="de">Knie beugen</trd>
  </snc>
  <snc>
    <dif>
      Sur<tld/>i&gcirc;i.
    </dif>
    <trdgrp lng="de">
      <trd>knien</trd>,
      <trd>niederknien</trd>
    </trdgrp>
    <trd lng="nl">knielen</trd>
  </snc>
  <ref tip="vid" cel="fleks.genu0i">genufleksi.</ref>
  <adm>
    Dubindaj sencoj. Vd adm sur fleks (genu~i). La franca
    traduko estas provizora. <aut>MB</aut>
  </adm>
  <trdgrp lng="fr">
    <trd>plier le <ind>genou</ind><klr tip="amb"> (??)</klr></trd>,
    <trd>&ecirc;tre &agrave; <ind>genou</ind><klr tip="amb"> (??)</klr></trd>,
    <trd>s'<ind>agenouiller</ind><klr tip="amb"> (??)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_k;&c_l;&c_o;&c_n;&c_i;&c_t;&c_mol; &c_k;&c_o;&c_l;&c_je;&c_n;&c_i;</trd>,
    <trd>&c_v;&c_s;&c_t;&c_a;&c_t;&c_mol; &c_n;&c_a; &c_k;&c_o;&c_l;&c_je;&c_n;&c_i;</trd>
  </trdgrp>
</drv>
</art>
