<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>genu</rad>/o
</kap>
<drv mrk="genu.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Anta&ubreve;a parto de la artiko kuniganta la femuron kun la kruro:
      <ekz>preni infanon sur la <tld/>ojn.</ekz>
    </dif>
  </snc>
  <trd lng="de">Knie</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_je;&c_n;&c_o;</trd>
</drv>
<drv mrk="genu.sur0i">
  <kap>(sur)<tld/>i</kap> 
  <gra><vspec>ntr</vspec></gra> 
  <snc num="1">
    <dif>
      Fleksi la <tld/>ojn. 
    </dif>
  <trd lng="de">Knie beugen</trd>
  </snc>
  <snc num="2">
    <dif>
      Sur<tld/>i&gcirc;i.  
    </dif>
    <trdgrp lng="de">
     <trd>knien</trd>,
     <trd>niederknien</trd></trdgrp>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_k;&c_l;&c_o;&c_n;&c_i;&c_t;&c_mol; &c_k;&c_o;&c_l;&c_je;&c_n;&c_i;</trd>,
    <trd>&c_v;&c_s;&c_t;&c_a;&c_t;&c_mol; &c_n;&c_a; &c_k;&c_o;&c_l;&c_je;&c_n;&c_i;</trd>
  </trdgrp>
</drv>
</art>
