<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>kinin</rad>/o
</kap>
<drv mrk="kinin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Senkolora kristala substanco, ekstraktita el la &scirc;elo de kinarbo,
      kaj uzata kontra&ubreve; febro.  
    </dif>
    <ref tip="sin" cel="kinkon1.0o">kinkonino</ref>
  </snc>
  <trd lng="de">Chinin</trd>
  <trd lng="en">quinine</trd>
  <trd lng="nl">kinine</trd>
  <trd lng="ru">&c_h;&c_i;&c_n;&c_i;&c_n;</trd>
</drv>
</art>
