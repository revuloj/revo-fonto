<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gubern1.xml,v 1.21 2023/08/31 14:29:55 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>guberni</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="gubern1.0o">
  <kap><tld/>o</kap>
  <snc mrk="gubern1.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Administra <ref tip="super" cel="terito.0o.POL">teritorio</ref>
      de regno, malpli vasta ol
      <ref tip="malprt" cel="provin.0o.POL">provinco</ref>
      kaj pli vasta ol
      <ref tip="prt" cel="kanton.0o.POL">kantono</ref>:
      <ekz>
        en Huangling-<tld/>o de Shaanxi-provinco [...] estas la tombo de
        Huangdi, prapatro de la &ccirc;ina nacio
        <fnt>
          <bib>EncCxi</bib>,
          <lok>&ccirc;apitro &leftquot;La flava rivero&rightquot;, p. 66a</lok>
        </fnt>;
      </ekz>
      <ekz>
        grava temo en la venontaj kvar jaroj estos la rajto al &rightquot;tero
        akvo&leftquot;
        en la <tld/>o Finnmark
        <fnt>
          <aut>B. Hekland</aut>,
          <vrk>Tero kaj akvo</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2001a, numero 12a, p. 7a</lok>
        </fnt>;
      </ekz>
      <ekz>
        en a&ubreve;gusto malsani&gcirc;is bovo en la <tld/>o Tiba
        <fnt>
          <aut>Hori Y.</aut>,
          <vrk>Freneza vivmaniero</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2002a, numero 2a, p. 14a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_u;&c_b;&c_e;&c_r;&c_n;&c_ib;&c_ja;</trd>,
    <trd>&c_sh;&c_t;&c_a;&c_t; <klr>(&c_n;&c_a;&c_p;&c_r;. &c_u; &c_Ib;&c_n;&c_d;&c_y;&c_ib;)</klr></trd>
  </trdgrp>
  <trd lng="ca">governaci&oacute;</trd>
  <trd lng="cs">gubernie</trd>
  <trdgrp lng="de">
    <trd>Gouvernement</trd>,
    <trd>Regierungsbezirk</trd>,
    <trd>Verwaltungsbezirk</trd>
  </trdgrp>
  <trd lng="en">administrative province</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;partement<klr> (territoire)</klr></trd>,
    <trd>gouvernement<klr tip="ind"> (division territoriale)</klr></trd>
  </trdgrp>
  <trd lng="hu">tartom&aacute;ny<klr tip="ind"> (k&ouml;zigazgat&aacute;si
  egys&eacute;g)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x770C;  [<ind>&#x3042;&#x304C;&#x305F;</ind>]</trd>,
    <trd>&#x90FD;&#x9053;&#x5E9C;&#x770C;  [<ind>&#x3068;&#x3069;&#x3046;&#x3075;&#x3051;&#x3093;</ind>]</trd>,
    <trd>&#x5DDE;  [<ind>&#x3059;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gouvernement</trd>,
    <trd>provincie</trd>
  </trdgrp>
  <trd lng="pt">prov&iacute;ncia</trd>
  <trd lng="ru">&c_g;&c_u;&c_b;&c_je;&c_r;&c_n;&c_i;&c_ja;</trd>
  <trd lng="sk">gubernia</trd>
  <trd lng="uk">&c_g;&c_u;&c_b;&c_je;&c_r;&#x43D;&c_ib;&#x44F;</trd>
</drv>

<drv mrk="gubern1.0estro">
  <kap><tld/>estro</kap>
  <snc mrk="gubern1.0estro.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Administranto de <tld/>o:
      <ekz>
        estis <tld/>estro, kiu &scirc;atis &scirc;ajnigi sin klera
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.espero.com.cn/02l/02l-2.htm">
          Legu kaj Ridu, 2</url></vrk>,
          <lok>espero.com.cn, 2003-04</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_b;&c_e;&c_r;&c_n;&c_a;&c_t;&c_a;&c_r;</trd>
  <trd lng="ca">governador</trd>
  <trdgrp lng="cs">
    <trd>gubern&#xE1;tor</trd>,
    <trd>guvern&#xE9;r</trd>,
    <trd>m&#xED;stodr&#x17E;itel</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gouverneur</trd>,
    <trd>Statthalter</trd>
  </trdgrp>
  <trd lng="en">governor</trd>
  <trdgrp lng="fr">
    <trd>gouverneur</trd>,
    <trd>pr&eacute;fet</trd>
  </trdgrp>
  <trd lng="hu">korm&aacute;nyz&oacute;</trd>
  <trd lng="ja">&#x77E5;&#x4E8B;  [<ind>&#x3061;&#x3058;</ind>]</trd>
  <trd lng="nl">gouverneur</trd>
  <trd lng="pt">governador</trd>
  <trd lng="ru">&c_g;&c_u;&c_b;&c_je;&c_r;&c_n;&c_a;&c_t;&c_o;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>gubern&#xE1;tor</trd>,
    <trd>miestodr&#x17E;ite&#x13E;</trd>
  </trdgrp>
  <trd lng="uk">&c_g;&c_u;&c_b;&c_je;&c_r;&#x43D;&c_a;&c_t;&#x43E;&c_r;</trd>
</drv>
</art>
