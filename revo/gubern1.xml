<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>guberni</rad>/o
</kap>
<drv mrk="gubern1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Administracia parto de lando a&ubreve; kolonio. 
    </dif>
    <refgrp tip="vid">
      <ref cel="depart.0o">departemento</ref>,
      <ref cel="provin.0o">provinco</ref>,
      <ref cel="distri1.0o">distrikto</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Gouvernement</trd>,
     <trd>Regierungsbezirk</trd>,
     <trd>Verwaltungsbezirk</trd></trdgrp>
  <trd lng="fr">gouvernement<klr tip="ind"> (division territoriale)</klr></trd>
  <trd lng="ru">&c_g;&c_u;&c_b;&c_je;&c_r;&c_n;&c_i;&c_ja;</trd>
</drv>
<drv mrk="gubern1.0estro">
  <kap><tld/>estro</kap>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Administranto de <tld/>o. 
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Gouverneur</trd>,
     <trd>Statthalter</trd></trdgrp>
  <trd lng="fr">gouverneur</trd>
  <trd lng="ru">&c_g;&c_u;&c_b;&c_je;&c_r;&c_n;&c_a;&c_t;&c_o;&c_r;</trd>
</drv>
</art>
