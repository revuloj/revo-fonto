<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>paser</rad>/o
</kap>
<drv mrk="paser.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <tld/>edoj (<trd lng="la">Passer</trd>),
      vivanta precipe en urboj:
      <ekz>
        &gcirc;i estas ankora&ubreve; <tld/>o en la aero<fnt>Z</fnt>
        <klr>(nur projektita, ne efektivigita.
        <refgrp tip="vid">
          <ref cel="bird.0o">birdo</ref>,
          <ref cel="nebul.0o">nebulo</ref>)
        </refgrp>
        </klr>;
      </ekz>
      <ekz>
        pri tio jam &ccirc;iuj <tld/>oj babilas
        <klr>(tio estas jam diskonata)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/paser.jpg">
    <ind>dom<tld/>o</ind>            
    <trd lng="br">golvan boutin</trd>
    <trd lng="cs">vrabec dom&aacute;ci</trd>
    <trd lng="de">Haussperling</trd>
    <trd lng="en">house-sparrow</trd>
    <trd lng="fr">moineau</trd>
    <trd lng="hu">h&aacute;zi <ind>ver&eacute;b</ind></trd>
    <trd lng="la">Passer domesticus</trd>
    <trd lng="pl">wr&oacute;bel domowy</trd>
    <trd lng="ru">&c_d;&c_o;&c_m;&c_o;&c_v;&c_y;&c_j; &c_v;&c_o;&c_r;&c_o;&c_b;&c_e;&c_j;</trd>
  </bld>        
  <trdgrp lng="br">
    <trd>filip<klr tip="ind"> (evn)</klr></trd>,
    <trd>golvan</trd>
  </trdgrp>
  <trd lng="cs">vrabec</trd>
  <trdgrp lng="de">
    <trd>Sperling</trd>,
    <trd>Spatz</trd>
  </trdgrp>
  <trd lng="be">&c_v;&c_je;&c_r;&c_a;&c_b;&c_je;&c_j;</trd>
  <trd lng="en">sparrow</trd>
  <trd lng="fr">passereaux</trd>
  <trd lng="hu">ver&eacute;b</trd>
  <trd lng="nl">mus</trd>
  <trd lng="pl">wr&oacute;bel</trd>
  <trd lng="ru">&c_v;&c_o;&c_r;&c_o;&c_b;&c_je;&c_j;</trd>
  <trd lng="sv">sparv</trd>
</drv>

<drv mrk="paser.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>,
      (<trd lng="la">Passeres</trd>, <trd lng="la">Passeriformes</trd>)
      kies idoj malfermas la bekon &ccirc;e
      tu&scirc;o de la beko a&ubreve; &ccirc;e tremeto de la nesto
      ka&ubreve;zita de alveno de gepatra birdo.
    </dif>
  </snc>            
  <trd lng="br">golvaneged</trd>
  <trd lng="be">&c_v;&c_je;&c_r;&c_a;&c_b;'&c_ib;&c_n;&c_y;&c_ja; <klr tip="amb">(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="de">Sperlingsv&ouml;gel</trd>
  <trd lng="fr">pass&eacute;riformes</trd>
  <trd lng="hu">ver&eacute;balak&uacute;ak</trd>
  <trd lng="ru">&c_v;&c_o;&c_r;&c_o;&c_b;&c_mol;&c_i;&c_n;&c_y;&c_je; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trd lng="sv">t&auml;ttingar</trd>
</drv>
</art>
