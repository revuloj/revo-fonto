<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: lant.xml,v 1.2 2019/10/16 22:10:37 revo Exp $">
    <kap>
      <rad>lant</rad>/e
    </kap>

<drv mrk="lant.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="hast.mal0a">Malhaste</ref>, <ref tip="super"
      cel="rapid.mal0a">malrapide</ref>:
      <ekz>
        Rafo <tld/>e etendis la manojn kaj le&gcirc;ere tenlevis dumane tiun
        lotusfloron
        <fnt>
          <bib>Monato</bib>
          <vrk><url
          ref="http://steloj.de/esperanto/monato1/monato-9506-kiso.txt"
              >de Shi Chengtai</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li prenis sian tekon, kaj <tld/>e elvagoni&gcirc;is
        <fnt>
          <bib>Monato</bib>
          <aut>Geraldo Mattos</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006805.html"
              >Kota Akvo: Konstanten Krysakov</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Karlo <tld/>e levis sian plenan glason
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007140.html"
              >de Julian Modest</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        foriras <tld/>e mi sur remboato kiel nubet' senzorga trans la
        bluo
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="https://www.monato.net/2007/009010.php"
              >Resume de l' mio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="urgx.0a">ur&gcirc;a</ref>,
       <ref cel="trankv.0a.anime">trankvila<sncref/></ref>
    </refgrp>
    <trd lng="de">gem&auml;chlich</trd>
    <trdgrp lng="pl">
      <trd>powoli</trd>,
      <trd>poma&lstroke;u</trd>,
      <trd>wolno</trd>
    </trdgrp>   
    <trdgrp lng="ro">
      <trd>&icirc;ncet</trd>,
      <trd>cu liber&abreve;</trd>,
      <trd>agale</trd>
    </trdgrp>
  </snc>
</drv>
  </art>

