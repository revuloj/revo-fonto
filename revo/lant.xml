    <kap>
      <rad>lant</rad>/e
    </kap>

<drv mrk="lant.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="hast.mal0a">Malhaste</ref>, <ref tip="super"
      cel="rapid.mal0a">malrapide</ref>:
      <ekz>
        Rafo <tld/>e etendis la manojn kaj le&gcirc;ere tenlevis dumane tiun
        lotusfloron
        <fnt>
          <aut>Shi Chengtai</aut>
          <vrk><url ref="&Monato;/1995/A950601.php">La kompensa kiso</url></vrk>
          <bib>Monato</bib>
          <lok>1995/06, p. 27</lok>
        </fnt>;
      </ekz>
      <ekz>
        li prenis sian tekon, kaj <tld/>e elvagoni&gcirc;is
        <fnt>
          <aut>Konstanten Krysakov</aut>
          <vrk><url ref="&Monato;/2000/006805.php">Kota akvo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 28</lok>
          </fnt>;
      </ekz>
      <ekz>
        Karlo <tld/>e levis sian plenan glason
        <fnt>
          <aut>Julian Modest</aut>
            <vrk><url ref="&Monato;/2001/007140.html">Vetiha</url></vrk>
            <bib>Monato</bib>
            <lok>2001/02, p. 26</lok>
          </fnt>;
      </ekz>
      <ekz>
        foriras <tld/>e mi sur remboato kiel nubet' senzorga trans la
        bluo
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2007/009010.php">Resume de l' mio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="urgx.0a">ur&gcirc;a</ref>,
       <ref cel="trankv.0a.anime">trankvila<sncref/></ref>
    </refgrp>
    <trd lng="de">gem&auml;chlich</trd>
    <trdgrp lng="pl">
      <trd>powoli</trd>,
      <trd>poma&lstroke;u</trd>,
      <trd>wolno</trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>&icirc;ncet</trd>,
      <trd>cu liber&abreve;</trd>,
      <trd>agale</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#24930;&#x60A0;&#x60A0; <pr>m&#xE0;ny&#x14D;uy&#x14D;u</pr></trd>,
    <trd>&#x5B89;&#38290; <pr>&amacron;nxi&#xE1;n</pr></trd>,
    <trd>&#x60A0;&#28982; <pr>y&#x14D;ur&#xE1;n</pr></trd>,
    <trd>&#24930;&#26465;&#x65AF;&#29702; <pr>m&#xE0;nti&#xE1;os&#x12B;l&#x1D0;</pr></trd>,
    <trd>&#24930;&#x689D;&#x65AF;&#29702; <pr>m&#xE0;nti&#xE1;os&#x12B;l&#x1D0;</pr></trd>,
    <trd>&#x900D; <pr>xi&amacron;o</pr></trd>
  </trdgrp>
</drv>
