<kap><ofc>10</ofc>
  <rad>Beliz</rad>/o
  <fnt><bib>RLN</bib></fnt>
</kap>

<drv mrk="beliz.B0o">
  <kap>
    <tld/>o  
  </kap>
  <mlg kod="ISO-3166">BZ</mlg>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
  <snc mrk="beliz.B0o.GEOG">
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&Scirc;tato</ref>
      en la duoninsulo Jukatano en la Centra Ameriko kun
      &ccirc;efurbo Belmopano;
      iam nomata Brita Honduraso.
      &Gcirc;i limas kun
      <ref tip="vid" cel="gvatemal.G0o.GEOG">Gvatemalo</ref>,
      <ref tip="vid" cel="meksik.M0o.lando">Meksiko</ref>
      kaj Kariba Maro:
      <ekz>
        kek&ccirc;oj <klr>[&#x2026;]</klr>, indiana popolo en Gvatemalo, lo&gcirc;as anka&ubreve; en Salvadoro kaj <tld/>o 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3369">Penuta Familio</url></lok>
        </fnt>.
      </ekz>
    </dif>
      <trdgrp lng="zh">
    <trd>&#x8C9D;&#x723E;&#x83AB;&#x6F58; <pr>b&#xE8;i&#x11B;rm&#xF2;p&#x101;n</pr></trd>,
    <trd>&#x4F2F;&#x91CC;&#x5179; <pr>b&#xF3;l&#x1D0;z&#x12B;</pr></trd>
  </trdgrp>
</snc>
    <trd lng="be">&c_B;&c_e;&c_l;&c_ib;&c_z;</trd>
    <trd lng="bg">&c_B;&c_je;&c_l;&c_i;&c_z;</trd>
    <trd lng="de">Belize</trd>
    <trd lng="en">Belize</trd>
    <trd lng="es">Belice</trd>
  <trdgrp lng="fr">
    <trd>Belize</trd>,
    <trd>B&eacute;lize</trd>
    </trdgrp>    
    <trd lng="id">Belize</trd>
    <trd lng="ja">&#x30D9;&#x30EA;&#x30FC;&#x30BA;</trd>
  <trdgrp lng="pl">
      <trd>Belize</trd>
    </trdgrp>
    <trd lng="pt">Belize</trd>
</drv>

</art>

