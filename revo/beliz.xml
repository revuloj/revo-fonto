<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>beliz</rad>/o
</kap>

<drv mrk="beliz.B0o">
  <kap>
    <tld lit="B"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="beliz.B0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la duoninsulo Jukatano en la Centra Ameriko kun
      &ccirc;efurbo Belmopano.
      Iam nomata Brita Honduraso.
      ISO-3166 kodo: BZ.
      &Gcirc;i limas kun
      <ref tip="vid" cel="gvatemal.G0o.GEOG">Gvatemalo</ref>,
      <ref tip="vid" cel="meksik.M0o.lando">Meksiko</ref>
      kaj Kariba Maro.
    </dif>
    </snc>
    <trd lng="be">&c_B;&c_e;&c_l;&c_ib;&c_z;</trd>
    <trd lng="bg">&c_B;&c_je;&c_l;&c_i;&c_z;</trd>
    <trd lng="de">Belize</trd>
    <trd lng="en">Belize</trd>
    <trdgrp lng="fr">
    <trd>Belize</trd>,
    <trd>B&eacute;lize</trd>
    </trdgrp>    
    <trd lng="id">Belize</trd>
    <trdgrp lng="pl">
      <trd>Belize</trd>
    </trdgrp>
    <trd lng="pt">Belize</trd>
</drv>

</art>

