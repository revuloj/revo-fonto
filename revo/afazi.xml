<kap>
  <rad>afazi</rad>/o
</kap>
<drv mrk="afazi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Perdo de la parolkapablo, sen paralizo de la langaj muskoloj:
      <ekz>
        simptomoj de apopleksio <klr>[&#x2026;]</klr>:
        problemoj kun kompreno a&ubreve; esprimo de lingvo (<tld/>o)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=51997">Apopleksio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        en la kazo de cerbdevena <tld/>o, la maldekstramanuloj pli facile rekapabli&gcirc;as
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=166259">Maldekstramanulo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_f;&c_a;&c_z;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_a;&c_f;&c_a;&c_z;&c_i;&c_ja;</trd>
  <trd lng="ca">af&agrave;sia</trd>
  <trd lng="cs">af&aacute;zie</trd>
  <trd lng="de">Aphasie</trd>
  <trd lng="en">aphasia</trd>
  <trd lng="es">afasia</trd>
  <trd lng="fa">&a_z;&a_b;&a_A;&a_n;&#x200C;&f_p;&a_r;&f_y;&a_sh;&f_y;</trd>
  <trd lng="fr">aphasie</trd>
  <trd lng="hu">af&aacute;zia</trd>
  <trd lng="it">afasia</trd>
  <trd lng="ja">&#x5931;&#x8A9E;&#x75C7;  <pr>&#x3057;&#x3064;&#x3054;&#x3057;&#x3087;&#x3046;</pr></trd>
  <trd lng="nl">afasie</trd>
  <trd lng="pl">afazja</trd>
  <trd lng="pt">afasia</trd>
  <trd lng="ro">afazie</trd>
  <trd lng="ru">&c_a;&c_f;&c_a;&c_z;&c_i;&c_ja;</trd>
  <trd lng="sk">af&aacute;zia</trd>
  <trdgrp lng="uk">
    <trd>&c_a;&c_f;&c_a;&c_z;&c_ib;&#x44F;</trd>,
    <trd>&c_u;&c_t;&c_r;&c_a;&c_t;&c_a; &#x43C;&#x43E;&c_v;&c_i;</trd>
  </trdgrp>
</drv>
