<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxere.xml,v 1.14 2023/12/05 19:19:39 revo Exp $">
<kap>
  <rad>&scirc;ere</rad>/o <fnt><bib>SPV</bib></fnt>,
  <var><kap><rad var="ks">kseres</rad>/o</kap></var>
</kap>

<drv mrk="sxere.0o">
  <kap><tld/>o, <var><kap><tld var="ks"/>o</kap></var></kap>
  <snc mrk="sxere.0o.VIN">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Andaluza <ref tip="super" cel="vin.0o.KUI">vino</ref> kun aldona
      brando:
      <ekz>
        &scirc;i uzis la &scirc;ancon <klr>[&#8230;]</klr> por indulgi sin per
        plia glaso da <tld/>o
        <fnt>
          <aut>-</aut>,
          <vrk>Fra&ubreve;lino Emily Barlaston (Ordeno de la Brita Imperio)
          </vrk>,
          <lok><url ref="http://www.homestead.com/barlaston/recenzoj.html">
          recenzoj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la preska&ubreve; nigra <tld var="ks"/>o <klr>[&#8230;]</klr> odoris kupron
        <fnt><bib>PatrojFiloj</bib>, <lok>&ccirc;apitro 20a, p.
        122a</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> donis al &scirc;i botelojn da <tld/>o, <klr>[&#8230;]</klr> kaj etajn
        skatolojn da likvora &ccirc;okolado
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#8470;1</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_je;&c_r;&c_a;&c_s;</trd>,
    <trd>&c_sh;&c_e;&c_r;&c_y; <klr tip="amb">(&c_v;&c_ib;&c_n;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="bg">&c_sh;&c_je;&c_r;&c_i;</trd>
  <trd lng="de">Sherry</trd>
  <trd lng="en">sherry</trd>
  <trd lng="fa">&a_sh;&a_kasra;&a_r;&f_y;</trd>
  <trd lng="fr">x&eacute;r&egrave;s</trd>
  <trd lng="it">sherry</trd>
  <trd lng="ja">&#12471;&#12455;&#12522;&#12540;&#37202;  <pr>&#12471;&#12455;&#12522;&#12540;&#12375;&#12421;</pr></trd>
  <trdgrp lng="nl">
    <trd>sherry</trd>,
    <trd>xeres</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>sherry</trd>,
    <trd>xeres <klr>(wino)</klr></trd>
  </trdgrp>
  <trd lng="pt">xerez</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_je;&c_r;&c_r;&c_i;</trd>,
    <trd>&c_h;&c_je;&c_r;&c_je;&c_s;</trd>
  </trdgrp>
  <trd lng="ro">xeres <klr>(vin)</klr></trd>
  <trd lng="uk">&c_h;&c_je;&c_r;&c_je;&c_s; (&c_v;&c_i;&c_n;&c_o;)</trd>
  <trd lng="zh">&#x96EA;&#21033;&#37202; <pr>xu&#x11B;l&#xEC;ji&#x1D4;</pr></trd>
</drv>
</art>

