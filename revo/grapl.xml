<art mrk="$Id: grapl.xml,v 1.19 2024/03/19 09:23:24 revo Exp $">
<kap>
  <rad>grapl</rad>/o
</kap>
<drv mrk="grapl.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">MAR</uzo>
    <dif>
      Malgranda <ref tip="super" cel="ankr.0o.MAR">ankro</ref>
      kun multaj brakoj:
      <ekz>
        li eksplodigis du pordojn, sed la enirejoj estis barikaditaj de lernotabloj kaj
        benkoj, oni provis fortiri ilin per <tld/>o, sed alfrontis mitralpafadon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=514388">Aleksandr Perov</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_sh;&c_k;&c_a; <klr tip="amb">(&c_ja;&c_k;&c_a;&c_r;)</klr></trd>
  <trd lng="bg">&c_k;&c_o;&c_t;&c_v;&c_i;&c_ch;&c_k;&c_a;</trd>
  <trd lng="ca">ruix&oacute;</trd>
  <trd lng="cs">mal&aacute; &ccaron;ty&rcaron; a&zcaron; &scaron;estiramenn&aacute; kotva</trd>
  <trdgrp lng="de">
     <trd>Dregganker</trd>,
     <trd>Draggen</trd>,
     <trd>Suchanker</trd></trdgrp>
  <trdgrp lng="en">
    <trd>grapnel</trd>,
    <trd>grappling iron</trd>
  </trdgrp>
  <trd lng="fr">grappin</trd>
  <trdgrp lng="hu">
    <trd>kutat&oacute;horgony</trd>,
    <trd>kap&oacute;cs&aacute;klya</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x56DB;&#x722A;&#x30A2;&#x30F3;&#x30AB;&#x30FC; 
    <pr>&#12424;&#12387;&#12388;&#12417;&#x30A2;&#x30F3;&#x30AB;&#x30FC;</pr></trd>,
    <trd>&#12402;&#12387;&#x304B;&#12369;&#37672;  <pr>&#12402;&#12387;&#x304B;&#12369;&#12356;&#x304B;&#x308A;</pr></trd>
  </trdgrp>
  <trd lng="nl">dreg</trd>
  <trd lng="pt">fateixa</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_ju;&c_k;</trd>,
    <trd>&c_sh;&c_t;&c_a;&c_n;&c_g;&c_o;&c_v;&c_a;&c_ja; &c_s;&c_k;&c_o;&c_b;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_sh;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">mnohoramenn&aacute; kotva</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_a;&#x43A;</trd>,
    <trd>&c_sh;&c_t;&c_a;&#x43D;&c_gu;&#x43E;&c_v;&c_a; &c_s;&#x43A;&#x43E;&c_b;&c_a;</trd>,
    <trd>&#x43A;&c_ib;&c_sh;&#x43A;&c_a;</trd>
  </trdgrp>
</drv>
</art>
