<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: eritrocit.xml,v 1.11 2025/04/24 02:05:15 revo Exp $">

<kap>
  <rad>eritrocit</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="eritrocit.0o">
  <kap><tld/>o</kap>
  <snc mrk="eritrocit.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      <ref tip="dif" cel="cxel.rugxasango0o.FIZL">Ru&gcirc;a
      sango&ccirc;elo</ref>:
      <ekz>
        la minerala substanco fero estas konsistiga parto de
        hemoglobino, la ru&gcirc;a pigmento en <tld/>oj
        <fnt><aut>W. F. Rolt</aut>, <vrk>El la Polvo de la Tero</vrk>,
        <lok>1967</lok></fnt>;
      </ekz>
      <ekz>
        la maturi&gcirc;intaj <tld/>oj de la homo (kaj la mamuloj) ne havas &ccirc;elkernon
        <fnt>
          <vrk><url ref="&Viki;/Eritrocito">Eritrocito - Vikipedio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_r;&c_y;&c_t;&c_r;&c_a;&c_c;&c_y;&c_t;</trd>,
    <trd>&c_ch;&c_y;&c_r;&c_v;&c_o;&c_n;&c_a;&c_ja; &c_k;&c_l;&c_je;&c_t;&c_k;&c_a; &c_k;&c_r;&c_y;&c_v;&c_ib;</trd>
  </trdgrp>
  <trd lng="bg">&c_je;&c_r;&c_i;&c_t;&c_r;&c_o;&c_c;&c_i;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>erytrocyt</trd>,
    <trd>&ccaron;erven&aacute; krvinka</trd>
  </trdgrp>
  <trd lng="de">Erythrozyt</trd>
  <trdgrp lng="en">
    <trd>erythrocyte</trd>,
    <trd>red <ind>blood</ind> cell</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;rythrocyte</trd>,
    <trd>globule rouge</trd>,
    <trd>h&eacute;matie</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>rode <ind>bloedcel</ind></trd>,
    <trd>erytrocyt</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>erytrocyt</trd>,
    <trd>czerwona <ind>krwinka</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>erytrocyt</trd>,
    <trd>&ccaron;erven&aacute; krvinka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_je;&c_r;&c_i;&c_t;&c_r;&c_o;&c_c;&c_i;&c_t;</trd>,
    <trd>&c_ch;&c_je;&c_r;&c_v;&c_o;&c_n;&c_je; &c_k;&c_r;&c_o;&c_v;&#8217;&c_ja;&c_n;&c_je; &c_t;&c_ib;&c_l;&c_mol;&c_c;&c_je;</trd>
  </trdgrp>
  <trd lng="zh">&#32005;&#34880;&#29699; <pr>h&oacute;ngxi&ecaron;qi&uacute;</pr></trd>
</drv>
</art>

