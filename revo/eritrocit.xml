<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: eritrocit.xml,v 1.8 2022/04/27 14:38:54 revo Exp $">

<kap>
  <rad>eritrocit</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="eritrocit.0o">
  <kap><tld/>o</kap>
  <snc mrk="eritrocit.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      <ref tip="dif" cel="cxel.rugxasango0o.FIZL">Ru&gcirc;a
      sango&ccirc;elo</ref>:
      <ekz>
        la minerala substanco fero estas konsistiga parto de
        hemoglobino, la ru&gcirc;a pigmento en <tld/>oj
        <fnt><aut>W. F. Rolt</aut>, <vrk>El la Polvo de la Tero</vrk>, 
        <lok>1967</lok></fnt>;
      </ekz>
      <ekz>
        la maturi&gcirc;intaj <tld/>oj de la homo (kaj la mamuloj) ne havas &ccirc;elkernon
        <fnt>
          <vrk><url ref="&Viki;/Eritrocito">Eritrocito - Vikipedio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_je;&c_r;&c_i;&c_t;&c_r;&c_o;&c_c;&c_i;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>erytrocyt</trd>,
    <trd>&ccaron;erven&aacute; krvinka</trd>
  </trdgrp>
  <trd lng="de">Erythrozyt</trd>
  <trdgrp lng="fr">
    <trd>&eacute;rythrocyte</trd>,
    <trd>globule rouge</trd>,
    <trd>h&eacute;matie</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>rode <ind>bloedcel</ind></trd>,
    <trd>erytrocyt</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>erytrocyt</trd>,
    <trd>czerwona <ind>krwinka</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>erytrocyt</trd>,
    <trd>&ccaron;erven&aacute; krvinka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x435;&#x440;&#x438;&#x442;&#x440;&#x43E;&#x446;&#x438;&#x442;</trd>,
    <trd>&#x447;&#x435;&#x440;&#x432;&#x43E;&#x43D;&#x435; &#x43A;&#x440;&#x43E;&#x432;&#x2019;&#x44F;&#x43D;&#x435; &#x442;&#x456;&#x43B;&#x44C;&#x446;&#x435;</trd>
  </trdgrp>
</drv>
</art>

