<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>apostrof</rad>/o
</kap>
<drv mrk="apostr.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Superlinia komoforma signo, montranta la forigon de vokalo.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Apostroph</trd>,
     <trd>Auslassungszeichen</trd>
  </trdgrp>
  <trd lng="en">apostrophe</trd>
  <trd lng="es">ap&oacute;strofe</trd>
  <trd lng="fr">apostrophe (ponctuation)</trd>
  <trdgrp lng="hu">
     <trd>hi&aacute;nyjel</trd>,
     <trd>aposztr&oacute;f</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_p;&c_o;&c_s;&c_t;&c_r;&c_o;&c_f;</trd>
  <trd lng="sk">apostrof</trd>
</drv>

<drv mrk="apostr.0i">
  <kap><tld/>i</kap>
  <snc>
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Meti <tld/>on.
    </dif>
    <trd lng="de">apostrophieren</trd>
    <trd lng="en">apostrophize</trd>
    <trd lng="fr">mettre une <ind>apostrophe</ind></trd>
    <trd lng="hu">hi&aacute;nyjelet tesz ki</trd>
    <trd lng="ru">&c_p;&c_o;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol; <ind>&c_a;&c_p;&c_o;&c_s;&c_t;&c_r;&c_o;&c_f;</ind></trd>
  </snc>
  <snc>
    <gra><vspec>tr</vspec></gra>
    <dif>
      Elizii:
      <ekz>
        en Esperanto oni povas <tld/>i la &leftquot;a&rightquot; de la artikolo, kaj la
        &leftquot;o&rightquot; de la substantivo.
      </ekz>
    </dif>
    <trd lng="de">auslassen</trd>
    <trd lng="en">elide</trd>
    <trd lng="fr">&eacute;lider</trd>
    <trd lng="hu">kihagy<klr tip="ind"> (hangot)</klr></trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_p;&c_u;&c_s;&c_k;&c_a;&c_t;&c_mol;<klr tip="amb"> (&c_k;&c_o;&c_n;&c_je;&c_ch;&c_n;&c_y;&c_j; &c_g;&c_l;&c_a;&c_s;&c_n;&c_y;&c_j;)</klr></trd>
  </snc>
</drv>
</art>
