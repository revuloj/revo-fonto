<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fratri.xml,v 1.16 2019/07/18 19:10:16 revo Exp $">
<kap>
  <rad>fratri</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fratri.0o">
  <kap><tld/>o</kap>
  <snc mrk="fratri.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      &Ccirc;e antikvaj Grekoj religia kaj socia grupi&gcirc;o de kelkaj familioj,
      simila al la itala <ref tip="vid" cel="kuri.0o.HIS">kurio</ref>,
      parto de <ref tip="vid" cel="trib.0o.HIS">tribo</ref>:
      <ekz>
        en la Iliado &ccirc;e la batalo de Trojo la greka armeo estas starigita la&ubreve; triboj kaj
        <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_r;&c_a;&c_t;&c_r;&c_y;&c_ja;</trd>
  <trd lng="cs">fr&aacute;trie</trd>
  <trd lng="de">Phratrie</trd>
  <trd lng="es">fratria</trd>
  <trd lng="fr">phratrie</trd>
  <trd lng="hu">fr&aacute;tria</trd>
  <trd lng="nl">fratrie</trd>
  <trd lng="pl">fratria</trd>
  <trd lng="pt">fratria</trd>
  <trd lng="ru">&c_f;&c_r;&c_a;&c_t;&c_r;&c_i;&c_ja;</trd>
  <trd lng="sk">frat&aacute;ria</trd>
</drv>
</art>
