<art mrk="$Id: lektor.xml,v 1.20 2024/01/15 16:14:09 revo Exp $">
<kap>
  <rad>lektor</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="lektor.0o">
  <kap><tld/>o</kap>
  <snc mrk="lektor.0o.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      Titolo de <ref tip="super" cel="instru.0isto.PED">Instruisto</ref>,
      universitata profesoro, diverse uzata la&ubreve; la landoj, kaj plej ofte
      koncernanta instruiston pri lingvoj:
      <ekz>
        el 580 000 instruistoj laborantaj en la pola eduksistemo nur
        &ccirc;irka&ubreve;
        30 000 estas <tld/>oj de fremdaj lingvoj<fnt><aut>Stanis&lstroke;aw
        &Sacute;migielski</aut>, 
        <vrk><url ref="&Monato;/2001/007193.html">Instruisto de la angla ser&ccirc;ata</url></vrk>,
        <bib>Monato</bib>, 
        <lok>2001/02, p. 19</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>o pri matematiko en la universitato de
        Oksfordo<fnt>
        <aut>Garbhan MacAoidh</aut>, 
        <vrk>Inversigita mondo
        (Lewis Carroll, Trans la spegulo kaj kion Alico trovis
        tie)</vrk>,
        <bib>Monato</bib>,
        <lok>2002/01, p. 25</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_je;&c_k;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_v;&c_y;&c_k;&c_l;&c_a;&c_d;&c_ch;&c_y;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>lektor</trd>,
    <trd>p&rcaron;edn&aacute;&scaron;ej&iacute;c&iacute;</trd>
  </trdgrp>
  <trd lng="de">Dozent</trd>
  <trd lng="en">lecturer<klr tip="amb"> (instructor)</klr></trd>
  <trdgrp lng="fr">
    <trd>lecteur<klr> (universit&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="hu">lektor<klr tip="ind"> (egyetemi)</klr></trd>
  <trd lng="ja">&#x8B1B;&#x5E2B;  <pr>&#x3053;&#x3046;&#x3057;</pr></trd>
  <trd lng="nl">lector</trd>
  <trd lng="pl">wyk&lstroke;adowca</trd>
  <trd lng="pt">professor <klr>(universit&aacute;rio, especialmente de
  l&iacute;nguas)</klr></trd>
  <trd lng="ro">lector universitar</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_je;&c_k;&c_t;&c_o;&c_r;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_p;&c_o;&c_d;&c_a;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">lektor <klr>(univerzitn&yacute;)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&#x43A;&#x43B;&c_a;&c_d;&c_a;&c_ch;</trd>,
    <trd>&#x43B;&c_je;&#x43A;&c_t;&#x43E;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8B1B;&#x5E2B; <pr>ji&#x1CE;ngsh&#x12B;</pr></trd>,
    <trd>&#x8BB2;&#x5E08; <pr>ji&#x1CE;ngsh&#x12B;</pr></trd>,
    <trd>&#x5927;&#x5B66;&#x8BB2;&#x5E08; <pr>d&#xE0;xu&#xE9;ji&#x1CE;ngsh&#x12B;</pr></trd>,
    <trd>&#x5927;&#x5B78;&#x8B1B;&#x5E2B; <pr>d&#xE0;xu&#xE9;ji&#x1CE;ngsh&#x12B;</pr></trd>,
    <trd>&#x79C1;&#x4EBA;&#x6559;&#x5E08; <pr>s&#x12B;r&#xE9;nji&#xE0;osh&#x12B;</pr></trd>,
    <trd>&#x79C1;&#x4EBA;&#x6559;&#x5E2B; <pr>s&#x12B;r&#xE9;nji&#xE0;osh&#x12B;</pr></trd>,
    <trd>&#x7DE8;&#x5916;&#x8B1B;&#x5E2B; <pr>bi&#x101;nw&#xE0;iji&#x1CE;ngsh&#x12B;</pr></trd>,
    <trd>&#x7F16;&#x5916;&#x8BB2;&#x5E08; <pr>bi&#x101;nw&#xE0;iji&#x1CE;ngsh&#x12B;</pr></trd>
