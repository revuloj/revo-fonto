<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: korbel.xml,v 1.22 2023/10/18 17:23:25 revo Exp $">
<kap>
  <rad>korbel</rad>/o
</kap>
<drv mrk="korbel.0o">
  <kap><tld/>o</kap>
  <snc mrk="korbel.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Elstara, penda konstrua&jcirc;eto almetita al &ccirc;efkonstrua&jcirc;o kaj
      subportata de anta&ubreve;i&gcirc;antaj apogiloj, <ref tip="vid" cel="konzol.0o.ARKI">konzoloj</ref> a&ubreve;
      similaj:
      <ekz>
        <tld/>e konstruita tureto;
      </ekz>
      <ekz>
        oni nun povas admiri la belegajn resta&jcirc;ojn de la mezepoko: <klr>[&#x2026;]</klr>
        belegajn domojn el ligno kaj argilmasona&jcirc;o, kun elstaraj <tld/>oj en &ccirc;iu eta&gcirc;o
        <fnt>
          <aut>Josette Ducloyer</aut>
          <vrk><url ref="&Monato;/2000/006851.php">Quimper</url></vrk>
          <bib>Monato</bib>
          <lok>2000/05, p. 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu konstrua&jcirc;o videblas <klr>[&#x2026;]</klr> kun siaj detalaj pordoj, fenestroj, e&ccirc;
        kornicoj, <tld/>oj kaj konzoloj
        <fnt>
          <bib>Monato</bib>
          <aut>Istv&aacute;n Ertl</aut>
          <vrk><url ref="&Monato;/2011/010457.php">Perspektivoj el rubujo</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kornic.0o">kornico</ref>,
      <ref cel="konzol.0o.ARKI">konzolo<sncref/></ref>,
      <ref cel="volb.0o">volbo</ref>
    </refgrp>
    <bld lok="&WCU;/thumb/a/ae/Ugarit_Corbel.jpg/320px-Ugarit_Corbel.jpg" prm="CC BY 3.0"><tld/>o de Ugarito
        <fnt>
          <aut>Disdero, propra verko</aut>
          <lok><url ref="&WCW;/File:Ugarit_Corbel.jpg">&WCW;/File:Ugarit_Corbel.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_s;&c_t;&c_u;&c_p; <klr>(&c_b;&c_u;&c_d;&c_y;&c_n;&c_k;&c_a;)</klr></trd>
  <trd lng="ca">m&egrave;nsula</trd>
  <trdgrp lng="cs">
    <trd>v&yacute;stupek zdi</trd>,
    <trd>v&yacute;&ccaron;n&ecaron;lek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kragbogen</trd>,
  </trdgrp>
  <trd lng="en">corbel</trd>
  <trd lng="es">m&eacute;nsula</trd>
  <trd lng="fr">encorbellement</trd>
  <trd lng="hu">falkisz&ouml;gell&eacute;s</trd>
  <trd lng="ja">&#x6301;&#x9001;&#x308A;&#x7A4D;&#x307F;  <pr>&#x3058;&#x304A;&#x304F;&#x308A;&#x3064;&#x307F;</pr></trd>
  <trdgrp lng="nl">
    <trd>kraagsteen</trd>,
    <trd>korbeel</trd>,
    <trd>karbeel</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wyst&eogonek;p</trd>,
    <trd>kroksztyn</trd>,
    <trd>wspornik</trd>,
    <trd>podpora</trd>
  </trdgrp>   
  <trd lng="pt">alpendre</trd>
  <trdgrp lng="ro">
    <trd>suport</trd>,
    <trd>bovindou</trd>
  </trdgrp> 
  <trd lng="ru">&c_s;&c_t;&c_u;&c_p;&c_je;&c_n;&c_ch;&c_a;&c_t;&c_y;&c_j; &c_v;&c_y;&c_s;&c_t;&c_u;&c_p;</trd>
  <trdgrp lng="sk">
    <trd>v&yacute;stupok</trd>,
    <trd>v&yacute;&ccaron;nelok</trd>
  </trdgrp>
</drv>
</art>
