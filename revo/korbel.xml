<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>korbel</rad>/o
</kap>
<drv mrk="korbel.0o">
  <kap><tld/>o</kap>
  <snc mrk="korbel.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Elstara, penda konstrua&jcirc;eto almetita al &ccirc;efkonstrua&jcirc;o kaj subportata
      de anta&ubreve;i&gcirc;antaj apogiloj, <ref tip="vid"
      cel="konzol.0o.ARKI">konzoloj</ref> a&ubreve; similaj:
      <ekz>
        <tld/>e konstruita tureto;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a roko.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kornic.0o">kornico</ref>
    </refgrp>
  </snc>
  <trd lng="ca">m&egrave;nsula</trd>
  <trdgrp lng="de">
    <trd>Korbel</trd>,
    <trd>Teil zwischen Abakus und Astragal einer korinthischen S&auml;ule</trd>
  </trdgrp>
  <trd lng="en">corbel</trd>
  <trd lng="es">m&eacute;nsula</trd>
  <trd lng="fr">encorbellement</trd>
  <trd lng="hu">falkisz&ouml;gell&eacute;s</trd>
  <trd lng="ru">&c_s;&c_t;&c_u;&c_p;&c_je;&c_n;&c_ch;&c_a;&c_t;&c_y;&c_j; &c_v;&c_y;&c_s;&c_t;&c_u;&c_p;</trd>
</drv>
</art>
