<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>muzik</rad>/o
</kap>
<drv mrk="muzik.0o">
  <kap><tld/>o</kap>
  <snc mrk="muzik.0o.belarto">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="art.bel0o">Belarto</ref> pri aran&gcirc;o de sonoj
      en maniero agrabla por homa a&ubreve;do.
    </dif>
  </snc>
  <snc mrk="muzik.0o.muzikajxo">
    <dif>
      <ref tip="dif" cel="muzik.0ajxo"><tld/>a&jcirc;o</ref>
    </dif>
  </snc>
  <trd lng="en">music</trd>
  <trd lng="be">&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;</trd>
  <trd lng="es">m&uacute;sica</trd>
  <trd lng="fr">musique</trd>
  <trd lng="de">Musik</trd>
  <trd lng="nl">muziek</trd>
  <trd lng="pl">muzyka</trd>
  <trd lng="ru">&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;</trd>
  <trd lng="sv">musik</trd>
</drv>

<drv mrk="muzik.0a">
  <kap><tld/>a</kap>
  <snc mrk="muzik.0a.belarta">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        noto <tld/>a
        <fnt><bib>F</bib>,
        <lok>UV, &ccirc;e &leftquot;noto&rightquot;</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_u;&c_z;&c_y;&c_ch;&c_n;&c_y;</trd>
  </snc>
  <snc mrk="muzik.0a.agrabla">
    <dif>
      Posedanta <tld/>an econ, agrabla por la orelo.
    </dif>
    <refgrp tip="vid">
      <ref cel="harmon1">harmonia</ref>,
      <ref cel="melodi.0a">melodia</ref>.
    </refgrp>
    <trd lng="be">&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
    <trd lng="sv">musikalisk</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>musical</trd>
  </trdgrp>
  <trd lng="nl">muzikaal</trd>
  <trd lng="ru">&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="muzik.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Sona&jcirc;o kun muzika efekto.      
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="muzik.0o.muzikajxo">muziko</ref>
  </refgrp>
  <trdgrp lng="fr">
    <trd>composition musicale</trd>,
    <trd>morceau de musique</trd>,
    <trd>musique <klr> (composition)</klr></trd>,
    <trd>pi&egrave;ce de musique</trd>
  </trdgrp>
</drv>
</art>
