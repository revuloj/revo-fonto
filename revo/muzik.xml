    <trd>gra&cacute;<klr> (muzyk&eogonek;)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_u;&c_z;&c_i;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
