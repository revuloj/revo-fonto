<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>muzik</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="muzik.0o">
  <kap><tld/>o</kap>
  <snc mrk="muzik.0o.belarto">
    <tezrad fak="MUZ"/>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="art.bel0o">Belarto</ref> pri aran&gcirc;o
      de sonoj agrabla por homa a&ubreve;do, precipe per
      <ref tip="prt" cel="ritm.0o.MUZ">ritmo</ref>,
      <ref tip="prt" cel="melodi.0o">melodio</ref>,
      <ref tip="prt" cel="harmon1.0o.MUZ">harmonio</ref>:
      <ekz>
        Arkadio, kvankam li efektive amis <tld/>on, nevolonte sekvis
        &scirc;in
        <fnt><bib>PatrojFiloj</bib>, <lok>&ccirc;apitro 16a, p. 88a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="muzik.0o.muzikajxo">
    <dif>
      <ref tip="dif" cel="muzik.0ajxo"><tld/>a&jcirc;o</ref>
      <ekz>
        en palacoj eburaj &gcirc;ojigas vin <tld/>o
        <fnt><bib>MT</bib>, <lok>&Psa; 45:8</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">music</trd>
  <trd lng="be">&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;</trd>
  <trd lng="es">m&uacute;sica</trd>
  <trd lng="fr">musique</trd>
  <trd lng="de">Musik</trd>
  <trd lng="id">musik</trd>
  <trd lng="nl">muziek</trd>
  <trd lng="pl">muzyka</trd>
  <trd lng="pt">m&uacute;sica</trd>
  <trd lng="ru">&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;</trd>
  <trd lng="sv">musik</trd>
  <trd lng="vo">musig</trd>
  <trdgrp lng="zh">
    <trd>&#38899;&#20048;</trd>,
    <trd><klr>tr. </klr>&#38899;&#27138;<klr> [y&#299;nyu&egrave;]</klr></trd>
  </trdgrp>
</drv>

<drv mrk="muzik.0a">
  <kap><tld/>a</kap>
  <snc mrk="muzik.0a.belarta">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        noto <tld/>a
        <fnt><bib>F</bib>,
        <lok>UV, &ccirc;e &leftquot;noto&rightquot;</lok></fnt>;
      </ekz>
      <ekz>
        en &ccirc;iu lingvo la <tld/>a terminaro estas en stato &hcirc;aosa
        <fnt><bib>MuzTerm</bib>, <lok>p. 3a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_u;&c_z;&c_y;&c_ch;&c_n;&c_y;</trd>
  </snc>
  <snc mrk="muzik.0a.agrabla">
    <dif>
      Posedanta <tld/>an econ, agrabla por la orelo.
    </dif>
    <trd lng="be">&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
    <trd lng="sv">musikalisk</trd>
  </snc>
  <trd lng="fr">musical</trd>
  <trd lng="nl">muzikaal</trd>
  <trd lng="ru">&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trd lng="vo">musigik</trd>
</drv>

<drv mrk="muzik.0i">
  <kap><tld/>i</kap>
  <snc mrk="muzik.0i.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzike sonigi vo&ccirc;on a&ubreve; instrumenton:
      <ekz>
        per tamburino kaj harpo ili <tld/>u al Li
        <fnt><bib>MT</bib>, <lok>&Psa; 149:3</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Musik (machen)</trd>,
    <trd>Musik (spielen)</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>jouer<klr> (de la musique)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_u;&c_z;&c_i;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="vo">musig&ouml;n</trd>
</drv>

<drv mrk="muzik.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="muzik.0ajxo.MUZ">
    <tezrad fak="MUZ"/>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Sona&jcirc;o agrable aran&gcirc;ita por homa a&ubreve;do:
      <ekz>
        kanono estas <tld/>a&jcirc;o, en kiu temo anoncita de unu
        vo&ccirc;o estas imitata strikte de alia
        <fnt><bib>MuzTerm</bib>, <lok>p. 18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="muzik.0o.muzikajxo">muziko <sncref/></ref>
  </refgrp>
  <trd lng="de">Musikst&uuml;ck</trd>
  <trdgrp lng="fr">
    <trd>morceau de musique</trd>,
    <trd>musique <klr> (pi&egrave;ce)</klr></trd>,
    <trd>pi&egrave;ce de musique</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_je; &c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_p;&c_mol;&c_je;&c_s;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="muzik.0igi">
  <kap><tld/>igi</kap> <fnt><bib>MuzTerm</bib></fnt>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="muzik.0igi.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Fari muzika&jcirc;on per ekzistanta teksto, ofte poezia&jcirc;o, 
      verkante por &gcirc;i ta&ubreve;gan melodion.
    </dif>
    <rim>
      Kvankam PIVa, tiu deriva&jcirc;o estas gramatike dubinda. Oni ne
      faras versa&jcirc;on muziko, oni provizas &gcirc;in je muziko.
      Konvena sufikso estus -iz (se oni &gcirc;in akceptas).
      <aut>S. Pokrovskij</aut>
    </rim>
  </snc>
  <trd lng="fr">mettre en <ind>musique</ind></trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_mol; &c_n;&c_a; <ind>&c_m;&c_u;&c_z;&c_y;&c_k;&c_u;</ind></trd>
</drv>

<drv mrk="muzik.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="muzik.0ilo.muzikinstrumento">
    <uzo tip="fak">MUZ</uzo>
    <ref tip="dif" cel="instru2.muzik0o">muzikinstrumento</ref>
    <ekz>
      venis unu rusa vila&gcirc;a junulo kun sia man-orgeneto (rusa <tld/>ilo)
      <fnt><bib>Voja&gcirc;Imp</bib>, <lok>en Peterburgo</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="de">Musikinstrument</trd>
  <trd lng="fr">instrument de musique</trd>
  <trd lng="ru">&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>
</drv>

<drv mrk="muzik.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="muzik.0isto.MUZ">
    <tezrad fak="MUZ"/>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Profesiulo a&ubreve; kompententulo pri muzikado:
      <ekz>
        la <tld/>istoj agordas siajn instrumentojn en la Spegul&ccirc;ambro
        <fnt><bib>EncJap</bib>, <lok>artikolo &leftquot;noo&rightquot;,
        p. 121a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Musiker</trd>
  <trd lng="fr">musicien</trd>
  <trd lng="ru">&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;&c_n;&c_t;</trd>
</drv>

<drv mrk="muzik.0istaro">
  <kap><tld/>istaro</kap>
  <snc mrk="muzik.0istaro.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="grup.muzik0o.MUZ">Muzikgrupo</ref>, precipe
      trejnita a&ubreve; profesiula:
      <ekz>
        rakontas Raita Pyh&auml;l&auml; <klr>...</klr> &leftquot;Oni mar&scirc;is, malanta&ubreve;
        uniformita <tld/>istaro&rightquot;
        <fnt><aut>K. Kniivil&auml;</aut>, <vrk>Duobla Bulonjo en Libera
        Folio</vrk>, <lok>soc.culture.esperanto, 2005-04-06</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Orchester</trd>
  <trd lng="fr">orchestre</trd>
</drv>

<drv mrk="muzik.0oscienco">
  <kap><tld/>oscienco, <var><kap><tld/>scienco</kap></var></kap>
  <snc mrk="muzik.0oscienco.MUZ">
    <uzo tip="fak">SCI</uzo>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Scienco pri <tld/>o, <tld/>sistemoj, &ccirc;io rilata al sonarto:
      <ekz>
        lingvistoj kaj filologoj tre ofte eluzas la terminologion kaj 
        la metodologion de la <tld/>scienco dum siaj fakaj esploroj
        <fnt>
          <aut>S. &Gcirc;u&gcirc;ev</aut>,
          <vrk><url ref="http://miresperanto.narod.ru/esperantologio/estetiko.htm">
          Estetikaj problemoj en interlingvistiko</url></vrk>,
          <lok>2005-01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">musicologie</trd>
</drv>

<drv mrk="muzik.0osciencisto">
  <kap><tld/>osciencisto, <var><kap><tld/>fakulo</kap></var></kap>
  <snc mrk="muzik.0osciencisto.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Fakulo pri <tld/>oscienco:
      <ekz>
        estante <tld/>fakulo, vi certe scias, ke <tld/>o, kiu ta&ubreve;gas por 
        nia radiostacio kaj a&ubreve;skultantaro, ne tro abundas
        <fnt>
          <aut>Ursula</aut>,
          <vrk>Triangulujo</vrk>,
          <lok><url ref="http://thor.prohosting.com/jesuo/k46.htm">
          Kajeroj el la Sudo, 2000-07, numero 46a</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        La verkojn de Zelenka kunigis kaj katalogis la <tld/>sciencisto 
        Wolfgang Reich
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;Jan Dismas Zelenka&rightquot;, 
        2007-02-24</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">musicologue</trd>
</drv>
</art>
