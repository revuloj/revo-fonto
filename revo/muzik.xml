</drv>

<drv mrk="muzik.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="muzik.0ilo.muzikinstrumento">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="dif" cel="instru2.muzik0o">Muzikinstrumento</ref>:
      <ekz>
        venis unu rusa vila&gcirc;a junulo kun sia man-orgeneto (rusa
        <tld/>ilo)
        <fnt><bib>Voja&gcirc;Imp</bib>, <lok>en Peterburgo</lok></fnt>;
      </ekz>
      <ekz>
        stereotipa a&ubreve;tomata akompanado helpe de modernaj teknika&jcirc;oj, ne
        <tld/>iloj sed sonsinteziloj
        <fnt>

          <aut>Guido van Damme</aut>
          <vrk><url ref="&Monato;/2001/007106.php">&Ccirc;armaj
          kantoj por infanoj</url></vrk>
          <bib>Monato</bib>
          <lok>2001/01, p. 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni &gcirc;uis belan harpmuzikon surstrate, ludatan de franca
        <tld/>isto, lo&gcirc;anta en <nom>Galway</nom>, kie li lernas konstrui
        <tld/>ilojn
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2004/008094.html">Galway
          - urbo de triboj</url></vrk>
          <bib>Monato</bib>
          <lok>2004/01, p. 18</lok>
        </fnt>;
      </ekz>
      <ekz>
        sonaton vi a&ubreve;skultas plenumata de ordinara violon&ccirc;elisto per
        mezkvalita <tld/>ilo
        <fnt>
          <aut>Sen Rodin (Filippo Franceschi)</aut>
          <vrk><url ref="&Monato;/2007/009131.php">Tri diskoj kun ses
          paroladoj de Ivo Lapenna</url></vrk>
          <bib>Monato</bib>
          <lok>2007/06, p. 22</lok>
        </fnt>;
      </ekz>
      <ekz>
        Antonio <nom>Stradivari</nom> (1644-1737), la plej mondfama metiisto,
        neniam superita, de ar&ccirc;o-<tld/>iloj
        <fnt>
          <aut>Sen Rodin (Filippo Franceschi)</aut>
          <vrk><url ref="&Monato;/2007/009131.php">Tri diskoj kun ses
          paroladoj de Ivo Lapenna</url></vrk>
          <bib>Monato</bib>
          <lok>2007/06, p. 22</lok>
        </fnt>;
      </ekz>
      <ekz>
        sonoj de originala bulgara <tld/>ilo gadulko (ribeko)
        <fnt>
          <aut>Dimitar Ha&gcirc;iev</aut>
          <vrk><url ref="&Monato;/2008/009495.php">Adia&ubreve;o al la
          dancantaj ursoj</url></vrk>
          <bib>Monato</bib>
          <lok>2008/10, p. 18</lok>
        </fnt>;
      </ekz>
      <ekz>
        tondradas <tld/>iloj modernaj, anka&ubreve; tamtamoj kaj tamburoj, ne
        silentas gongoj kaj kastanjetoj
        <fnt>
          <aut>Gbeglo Koffi</aut>
          <vrk><url ref="&Monato;/2010/009910.php">Ventreligio, dua r
          aldoni&gcirc;u</url></vrk>
          <bib>Monato</bib>
          <lok>2010/01, p. 20</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_z;&c_y;&c_ch;&c_n;&c_y; <ind>&c_ib;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_e;&c_n;&c_t;</ind></trd>
