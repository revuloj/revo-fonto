<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc><ofc>1</ofc><ofc>8</ofc>
  <rad>muzik</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="muzik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="muzik.0o.belarto">
    <tezrad fak="MUZ"/>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="lst" cel="art.bel0o" lst="voko:artoj">Belarto</ref>
      pri aran&gcirc;o de sonoj agrabla por homa a&ubreve;do, precipe per
      <ref tip="prt" cel="ritm.0o.MUZ">ritmo</ref>,
      <ref tip="prt" cel="melodi.0o">melodio</ref>,
      <ref tip="prt" cel="harmon1.0o.MUZ">harmonio</ref>:
      <ekz>
        li aran&gcirc;adis la <tld/>on, &ccirc;ar li estis kompetentulo
        <fnt>
          <bib>MT</bib>
          <lok>I. Kroniko 15:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        Arkadio, kvankam li efektive amis <tld/>on, nevolonte sekvis
        &scirc;in
        <fnt><bib>PatrojFiloj</bib>, <lok>&ccirc;apitro 16a, p.
        88a</lok></fnt>.
      </ekz>
      <ekz>
        li komprenas predikon, kiel bovo muzikon
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="muzik.0o.muzikado">
    <dif>
      La prezentado de <ref tip="dif" cel="muzik.0ajxo.MUZ"><tld/>a&jcirc;oj</ref>;
      <tld/>ado:
      <ekz>
        kiam li venis kaj alproksimi&gcirc;is al la domo, li a&ubreve;dis <tld/>on kaj dancadon
        <fnt>
          <bib>NT</bib>
          <lok>Luko 15:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        en palacoj eburaj &gcirc;ojigas vin <tld/>o
        <fnt><bib>MT</bib>, <lok>&Psa; 45:8</lok></fnt>;
      </ekz>
      <ekz>
        kiam la bela <tld/>o ludis &ccirc;iam plue, &scirc;i ne povis plu elteni,
        <klr>[... &scirc;i]</klr> balancadis la kapon la&ubreve; la takto de la bela <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        nun komenci&gcirc;is la <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi jam ne a&ubreve;dados la <tld/>on de la ondoj
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;</trd>
  <trd lng="bg">&c_m;&c_u;&c_z;&c_i;&c_k;&c_a;</trd>
  <trd lng="ca">m&uacute;sica</trd>
  <trdgrp lng="cs">
    <trd>hudba</trd>,
    <trd>muzika</trd>
  </trdgrp>
  <trd lng="de">Musik</trd>
  <trd lng="en">music</trd>
  <trd lng="es">m&uacute;sica</trd>
  <trd lng="fr">musique</trd>
  <trd lng="id">musik</trd>
  <trd lng="nl">muziek</trd>
  <trd lng="pl">muzyka</trd>
  <trd lng="pt">m&uacute;sica</trd>
  <trd lng="ro">muzic&abreve;</trd>
  <trd lng="ru">&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;</trd>
  <trd lng="sk">hudba</trd>
  <trd lng="sv">musik</trd>
  <trd lng="vo">musig</trd>
  <trdgrp lng="zh">
    <trd>&#38899;&#20048;</trd>,
    <trd><klr>tr. </klr>&#38899;&#27138;<klr>
    [y&#299;nyu&egrave;]</klr></trd>
  </trdgrp>
</drv>

<drv mrk="muzik.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="muzik.0a.belarta">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        noto <tld/>a
        <fnt><bib>F</bib>,
          <lok>UV, &ccirc;e &leftquot;noto&rightquot;</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a signo
        <fnt>
          <bib>EE</bib>
        </fnt>,
        tono
        <fnt>
          <bib>IK</bib>
        </fnt>,
        talento
        <fnt>
          <bib>IK</bib>
        </fnt>,
        klereco
        <fnt>
          <bib>Marta</bib>
        </fnt>,
        repertuaro
        <fnt>
          <bib>Marta</bib>
        </fnt>,
        kolegio
        <fnt>
          <bib>FK</bib>
        </fnt>,
        terminaro
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili starigu siajn fratojn la kantistojn kun <tld/>aj instrumentoj, 
        psalteroj, harpoj, kaj la&ubreve;taj cimbaloj
        <fnt>
          <bib>MT</bib>
          <lok>I. Kroniko 15:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        en &ccirc;iu lingvo la <tld/>a terminaro estas en stato
        &hcirc;aosa
        <fnt><bib>MuzTerm</bib>, <lok>p. 3a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_u;&c_z;&c_y;&c_ch;&c_n;&c_y;</trd>
    <trd lng="de">Musik-</trd>
    <trd lng="pl">muzyczny</trd>
  </snc>
  <snc mrk="muzik.0a.agrabla">
    <dif>
      Posedanta <tld/>an econ, agrabla por la orelo.
    </dif>
    <trd lng="be">&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
    <trd lng="de">melodisch</trd>
    <trd lng="pl">muzykalny</trd>
    <trd lng="sv">musikalisk</trd>
  </snc>
  <trd lng="ca">musical</trd>
  <trd lng="en">musical</trd>
  <trd lng="es">musical</trd>
  <trd lng="fr">musical</trd>
  <trd lng="id">musikal</trd>
  <trdgrp lng="nl">
    <trd>muziek-</trd>,
    <trd>muzikaal</trd>
  </trdgrp>
  <trd lng="ro">muzical</trd>
  <trd lng="ru">&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trd lng="vo">musigik</trd>
</drv>

<drv mrk="muzik.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="muzik.0i.MUZ">
    <dif>
      Muzike sonigi vo&ccirc;on a&ubreve; instrumenton:
      <ekz>
        per tamburino kaj harpo ili <tld/>u al Li
        <fnt><bib>MT</bib>, <lok>&Psa; 149:3</lok></fnt>;
      </ekz>
      <ekz>
        la popolo <tld/>is per flutoj kaj estis tre gaja
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 1:40</lok>
        </fnt>;
      </ekz>
      <ekz>
        la servistoj, por pasigi la tempon, man&gcirc;is, trinkis kaj <tld/>is
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la turhorlo&gcirc;o <tld/>as simplan kaj samtempe tian
        kortu&scirc;an 
        melodion
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>tocar</trd>,
    <trd>fer m&uacute;sica</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hr&aacute;t</trd>,
    <trd>koncertovat</trd>,
    <trd>muzic&iacute;rovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Musik (machen)</trd>,
    <trd>Musik (spielen)</trd>
  </trdgrp>
   <trdgrp lng="en">
    <trd>make (music)</trd>,
    <trd>play (music)</trd>
  </trdgrp>
 <trd lng="es">hacer m&uacute;sica</trd>
  <trd lng="fr">jouer<klr> (de la musique)</klr></trd>
  <trd lng="id">[<ind>musik</ind>] bermusik</trd>
  <trdgrp lng="nl">
    <trd><ind>muziek</ind> maken</trd>,
    <trd><ind>muziek</ind> spelen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>muzykowa&cacute;</trd>,
    <trd>gra&cacute;<klr> (muzyk&eogonek;)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_u;&c_z;&c_i;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>h&uacute;s&tcaron;</trd>,
    <trd>vyhr&aacute;va&tcaron;</trd>
  </trdgrp>
  <trd lng="vo">musig&ouml;n</trd>
</drv>

<drv mrk="muzik.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="muzik.0ajxo.MUZ">
    <tezrad fak="MUZ"/>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Sona&jcirc;o agrable aran&gcirc;ita por homa a&ubreve;do:
      <ekz>
        melankolie sonas ankora&ubreve; multaj versoj, sed <klr>[&#8230;]</klr> jen subite blovas vigla <tld/>a&jcirc;o
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        li verkis poemojn kaj komponis <tld/>a&jcirc;ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="http://www.monato.net/2009/009823.php">Postaj tempoj eble pli bone komprenos min</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kanono estas <tld/>a&jcirc;o, en kiu temo anoncita de unu
        vo&ccirc;o estas imitata strikte de alia
        <fnt><bib>MuzTerm</bib>, <lok>p. 18a</lok></fnt>;
      </ekz>
      <ekz>
        ili estas multpartaj plurhoraj <tld/>a&jcirc;oj, pli similaj al operoj por unu vo&ccirc;o ol al kantoj la&ubreve; e&ubreve;ropa tradicio
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007063.html">Um Keltum'</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        estas kvaza&ubreve; la <tld/>a&jcirc;oj de Bach tiom influus, ke neniu muzikkonstrua stilo poste ekzistus, nek akirus publikon
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="http://www.esperanto.be/fel/2004/008327.html">Pozitivaj kaj negativaj aspektoj de Dante</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la Kolekto de <tld lit="M"/>a&jcirc;oj konsistas el 51&nbsp;000 manskribitaj 
        kaj 130&nbsp;000 presitaj <tld/>notdokumentoj, 70&nbsp;000 libroj, 
        41&nbsp;000 hereditaj objektoj kaj 20&nbsp;000 sondokumentoj
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="http://www.monato.net/2006/008865.php">Muzearo notinde planita</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        oni kulpigu pri tio la interpretan magion de Nanne kaj 
        Ankie, kiuj kapablas fari e&ccirc; el teksto ne tre ekscita kaj
        melodio simpla kaj tradicia <tld/>a&jcirc;on vere atentokaptan
        <fnt>
          <bib>Monato</bib>
          <aut>Edmund Grimley Evans</aut>
          <vrk><url ref="http://www.monato.net/2014/011289.php">Kajto flugas plu</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
       <ref cel="kant.0o.kantado">kanto<sncref/></ref>,
       <ref cel="kompon.0ajxo">kompona&jcirc;o</ref>,
       <ref cel="simfon.0o">simfonio</ref>,
       <ref cel="oper.0o">opero</ref>
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>can&ccedil;&oacute;</trd>,
    <trd>pe&ccedil;a</trd>
  </trdgrp>
  <trd lng="de">Musikst&uuml;ck</trd>
  <trdgrp lng="en">
    <trd>piece of music</trd>,
    <trd>musical piece</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>composici&oacute;n musical</trd>,
    <trd>pieza musical</trd>
    <trd>m&uacute;sica</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>morceau de musique</trd>,
    <trd>musique <klr> (pi&egrave;ce)</klr></trd>,
    <trd>pi&egrave;ce de musique</trd>
  </trdgrp>
  <trd lng="nl">muziekstuk</trd>
  <trd lng="pl">utw&oacute;r muzyczny</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_je;
    &c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja;
    &c_p;&c_mol;&c_je;&c_s;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="muzik.0ajxisto">
  <kap>
    <tld/>a&jcirc;isto
</kap>
  <snc>
    <dif>
      Kortegano respondeca pri la prezentado de <tld/>a&jcirc;oj:
      <ekz>
        &ccirc;i tiu ne estas kulpa, diris la <tld/>a&jcirc;isto, &ccirc;i tiu severe observas la takton kaj estas tute el mia lernejo
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Spielmeister</trd>
</drv>


<drv mrk="muzik.0igi">
  <kap><tld/>igi</kap> <fnt><bib>MuzTerm</bib></fnt>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="muzik.0igi.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Fari muzika&jcirc;on por ekzistanta teksto, ofte poezia&jcirc;o, 
      verkante por &gcirc;i ta&ubreve;gan kantan formon:
      <ekz>
        kelkaj liaj juna&gcirc;aj versa&jcirc;oj estis <tld/>igitaj kaj prezentataj en bardaj koncertoj
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk><url ref="http://www.esperanto.be/fel/2003/007812.html">Estas dan&gcirc;ere esti poeto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la Kajto-kantoj, kiujn mi plej &scirc;atas, estas la <tld/>igitaj poemoj Orangutango, La soldato kiu mar&scirc;as kaj Rabista dormo
        <fnt>
          <bib>Monato</bib>
          <aut>Edmund Grimley Evans</aut>
          <vrk><url ref="http://www.monato.net/2014/011289.php">Kajto flugas plu</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      Kvankam PIVa, tiu deriva&jcirc;o estas gramatike dubinda. Oni ne
      faras versa&jcirc;on muziko, oni &gcirc;in provizas je muziko.
      Konvena sufikso estus -iz (se oni &gcirc;in akceptas); a&ubreve;
      eblus
      uzi ion pli priskriban (en<tld/>igi, sur<tld/>igi &dash; kp
      <ctl>surscenigi</ctl> ktp).
      <aut>S. Pokrovskij</aut>
    </rim>
  </snc>
  <trd lng="ca">musicar</trd>
  <trd lng="cs">zhudebnit</trd>
  <trd lng="de">vertonen</trd>
  <trdgrp lng="en">
    <trd>set to music</trd>,
    <trd>score</trd>
  </trdgrp>
  <trd lng="es">musicalizar</trd>
  <trd lng="fr">mettre en <ind>musique</ind></trd>
  <trd lng="id">[<ind>musik</ind>] memusikkan</trd>
  <trd lng="nl">op <ind>muziek</ind> zetten</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_mol; &c_n;&c_a;
  <ind>&c_m;&c_u;&c_z;&c_y;&c_k;&c_u;</ind></trd>
  <trd lng="sk">zhudobni&tcaron;</trd>
</drv>

<drv mrk="muzik.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="muzik.0ilo.muzikinstrumento">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="dif" cel="instru2.muzik0o">Muzikinstrumento</ref>:
      <ekz>
        venis unu rusa vila&gcirc;a junulo kun sia man-orgeneto (rusa
        <tld/>ilo)
        <fnt><bib>Voja&gcirc;Imp</bib>, <lok>en Peterburgo</lok></fnt>;
      </ekz>
      <ekz>
        stereotipa a&ubreve;tomata akompanado helpe de modernaj teknika&jcirc;oj, ne <tld/>iloj sed sonsinteziloj
        <fnt>
          <bib>Monato</bib>
          <aut>Grigori Arosev</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007106.html">&Ccirc;armaj kantoj por infanoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ni &gcirc;uis belan harpmuzikon surstrate, ludatan de franca <tld/>isto, lo&gcirc;anta en Galway, kie li lernas konstrui <tld/>ilojn
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="http://www.esperanto.be/fel/2004/008094.html">Galway - urbo de triboj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        sonaton vi a&ubreve;skultas plenumata de ordinara violon&ccirc;elisto per mezkvalita <tld/>ilo
        <fnt>
          <bib>Monato</bib>
          <aut>Sen Rodin (Filippo Franceschi)</aut>
          <vrk><url ref="http://www.monato.net/2007/009131.php">Tri diskoj kun ses paroladoj de Ivo Lapenna</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Antonio Stradivari (1644-1737), la plej mondfama metiisto, neniam superita, de ar&ccirc;o-<tld/>iloj
        <fnt>
          <bib>Monato</bib>
          <aut>Sen Rodin (Filippo Franceschi)</aut>
          <vrk><url ref="http://www.monato.net/2007/009131.php">Tri diskoj kun ses paroladoj de Ivo Lapenna</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        sonoj de originala bulgara <tld/>ilo gadulko (ribeko)
        <fnt>
          <bib>Monato</bib>
          <aut>Dimitar Ha&gcirc;iev</aut>
          <vrk><url ref="http://www.monato.net/2008/009495.php">Adia&ubreve;o al la dancantaj ursoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        tondradas <tld/>iloj modernaj, anka&ubreve; tamtamoj kaj tamburoj, ne silentas gongoj kaj kastanjetoj
        <fnt>
          <bib>Monato</bib>
          <aut>Gbeglo Koffi</aut>
          <vrk><url ref="http://www.monato.net/2010/009910.php">Ventreligio, dua r aldoni&gcirc;u</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">instrument musical</trd>
  <trd lng="cs">hudebn&iacute; n&aacute;stroj</trd>
  <trd lng="de">Musikinstrument</trd>
  <trd lng="en">musical instrument</trd>
  <trd lng="es">instrumento <klr>musical</klr></trd>
  <trd lng="fr">instrument de musique</trd>
  <trd lng="id"><ind>alat</ind> musik</trd>
  <trd lng="nl">muziekinstrument</trd>
  <trd lng="pl">instrument muzyczny</trd>
  <trd lng="ro">instrument muzical</trd>
  <trd lng="ru">&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;
  &c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>
  <trd lng="sk">hudobn&yacute; n&aacute;stroj</trd>
</drv>

<drv mrk="muzik.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="muzik.0isto.MUZ">
    <tezrad fak="MUZ"/>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Profesiulo a&ubreve; kompetentulo pri muzikado:
      <ekz>
        la Levidoj &ccirc;iuj estis kompetentaj <tld/>istoj 
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 34:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        kantistoj kaj <tld/>istoj
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 87:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bardo, duone <tld/>isto kaj duone militisto, kiu partoprenis
        en la militiro kaj estis vidanta atestanto de la faroj, kiujn li 
        prezentis, kantis al ili kanton, kiu gloris iliajn grandajn militajn agojn
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bremenaj <tld/>istoj <klr>(fabelo)</klr>,
        urba <tld/>isto
        <fnt>
          <bib>ElektFab</bib>
          <lok>La bremenaj muzikistoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la spektaklo komenci&gcirc;is per procesio de <tld/>istoj, kantistoj kaj dancistinoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>istoj agordas siajn instrumentojn en la
        Spegul&ccirc;ambro
        <fnt><bib>EncJap</bib>,
          <lok>artikolo &leftquot;noo&rightquot;, p. 121a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">m&uacute;sic</trd>
  <trdgrp lng="cs">
    <trd>hudebn&iacute;k</trd>,
    <trd>muzikant</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Musiker</trd>,
    <trd>Spielmann</trd>
  </trdgrp>
  <trd lng="es">m&uacute;sico</trd>
  <trd lng="en">musician</trd>
  <trd lng="fr">musicien</trd>
  <trdgrp lng="id">
    <trd>musikus</trd>,
    <trd>musisi</trd>,
    <trd>[<ind>musik</ind>] pemusik</trd>
  </trdgrp>
  <trd lng="nl">musicus</trd>
  <trd lng="pl">muzyk</trd>
  <trd lng="ro">muzician</trd>
  <trd lng="ru">&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;&c_n;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>hudobn&iacute;k</trd>,
    <trd>muzikant</trd>
  </trdgrp>
</drv>

<drv mrk="muzik.0istaro">
  <kap><tld/>istaro</kap>
  <snc mrk="muzik.0istaro.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="grup.muzik0o.MUZ">Muzikgrupo</ref>,
      precipe trejnita a&ubreve; profesiula,
      <ref tip="sub" cel="ensemb.0o.MUZ.TEA">ensemblo</ref>
      a&ubreve; <ref tip="sub" cel="orkest.0o.MUZ_grupo">orkestro</ref>:
      <ekz>
        folklora <tld/>istaro akompanas la voja&gcirc;antojn
        <fnt>
          <bib>Monato</bib>
          <aut>Marta Sol&iacute;s</aut>
          <vrk><url ref="http://www.monato.net/2005/008399.php">Unika fervojo en Andoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        rakontas Raita Pyh&auml;l&auml; <klr>...</klr> &leftquot;oni
        mar&scirc;is,
        malanta&ubreve; uniformita <tld/>istaro&rightquot;
        <fnt><aut>K. Kniivil&auml;</aut>,
          <vrk>Duobla Bulonjo en Libera Folio</vrk>,
          <lok>soc.culture.esperanto, 2005-04-06</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>orquestra</trd>,
    <trd>banda</trd>,
    <trd>grup</trd>
  </trdgrp>
  <trd lng="cs">kapela</trd>
  <trdgrp lng="de">
    <trd>Orchester</trd>,
    <trd>Musikgruppe</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>orquesta</trd>,
    <trd>banda <klr>de m&uacute;sica</klr></trd>,
    <trd>grupo musical</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>orchestra</trd>,
    <trd>band</trd>,
    <trd>ensemble</trd>,
    <trd>(music) group</trd>
  </trdgrp>
  <trd lng="fr">groupe musical</trd>
  <trd lng="id">orkestra</trd>
  <trdgrp lng="nl">
    <trd>fanfare</trd>,
    <trd>orkest</trd>
  </trdgrp>
  <trd lng="pl">orkiestra</trd>
  <trd lng="ro">forma&#539;ie</trd>
  <trd lng="sk">kapela</trd>
</drv>

<drv mrk="muzik.cxambro0o">
  <kap>&ccirc;ambro<tld/>o,
  <var><kap>&ccirc;ambra <tld/>o</kap></var></kap>
  <snc mrk="muzik.cxambro0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muziko de malgranda muzikistaro, &gcirc;is ok- a&ubreve; dekhoma,
      a&ubreve;debla en ne tre vasta spaco de ne tre granda publiko, 
      kontraste al
      <ref tip="ant" cel="muzik.orkestro0o.MUZ">orkestromuziko</ref>:
      <ekz>
        en loka mona&hcirc;ejo okazis eta koncerto de &ccirc;ambra
        <tld/>o
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.esperanto.cz/eo/aktuality/13-a-kongreso-de-cea.html">
          13-a kongreso de &Ccirc;EA</url></vrk>,
          <lok>Esperanto.cz, 2011-11-01</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ensemb.0o.MUZ.TEA">ensemblo<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="de">Kammermusik</trd>
  <trd lng="en">chamber music</trd>
  <trd lng="es">m&uacute;sica de c&aacute;mara</trd>
  <trd lng="fr">musique de chambre</trd>
</drv>

<drv mrk="muzik.orkestro0o">
  <kap>orkestro<tld/>o,
  <var><kap>orkestra <tld/>o</kap></var></kap>
  <snc mrk="muzik.orkestro0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muziko de
      <ref tip="vid" cel="orkest.0o.MUZ_grupo">orkestro</ref>
      por vasta spaco kaj mulnombra publiko,
      kontraste al 
      <ref tip="ant" cel="muzik.cxambro0o.MUZ">&ccirc;ambromuziko</ref>:
           
      <ekz>
        vi a&ubreve;das la orkestran <tld/>on <ctl>Kanto pri la
        Ru&gcirc;a 
        Flago</ctl> 
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url ref="http://esperanto.cri.cn/1/2003/12/09/1@927.htm">
          Pri la 6-a Pekina Internacia Muzika Festivalo</url></vrk>,
           <lok>2003-12-09</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Orchestermusik</trd>
  <trd lng="en">orchestral music</trd>
  <trd lng="es">m&uacute;sica orquestal</trd>
  <trd lng="fr">musique d'orchestre</trd>
</drv>

<drv mrk="muzik.0oscienco">
  <kap><tld/>oscienco, <var><kap><tld/>scienco</kap></var></kap>
  <snc mrk="muzik.0oscienco.MUZ">
    <uzo tip="fak">SCI</uzo>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Scienco pri <tld/>o, <tld/>sistemoj, &ccirc;io rilata al sonarto:
      <ekz>
        lingvistoj kaj filologoj tre ofte eluzas la terminologion
        kaj 
        la metodologion de la <tld/>scienco dum siaj fakaj esploroj
        <fnt>
          <aut>S. &Gcirc;u&gcirc;ev</aut>,
          <vrk><url ref="http://miresperanto.narod.ru/esperantologio/estetiko.htm">
              Estetikaj problemoj en interlingvistiko</url></vrk>,
          <lok>2005-01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">musicologia</trd>
  <trd lng="cs">hudebn&iacute; v&ecaron;da</trd>
  <trd lng="de">Musikwissenschaft</trd>
  <trd lng="en">musicology</trd>
  <trd lng="es">musicolog&iacute;a</trd>
  <trd lng="fr">musicologie</trd>
  <trd lng="id">musikologi</trd>
  <trdgrp lng="nl">
    <trd>muziekwetenschap</trd>,
    <trd>musicologie</trd>
  </trdgrp>
  <trd lng="pl">muzykologia</trd>
  <trd lng="ro">muzicologie</trd>
  <trd lng="ru">&c_m;&c_u;&c_z;&c_y;&c_k;&c_o;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">hudobn&aacute; veda</trd>
</drv>

<drv mrk="muzik.0osciencisto">
  <kap><tld/>osciencisto, <var><kap><tld/>fakulo</kap></var></kap>
  <snc mrk="muzik.0osciencisto.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Fakulo pri <tld/>oscienco:
      <ekz>
        estante <tld/>fakulo, vi certe scias, ke <tld/>o, kiu
        ta&ubreve;gas por 
        nia radiostacio kaj a&ubreve;skultantaro, ne tro abundas
        <fnt>
          <aut>Ursula</aut>,
          <vrk>Triangulujo</vrk>,
          <lok><url ref="http://thor.prohosting.com/jesuo/k46.htm">
              Kajeroj el la Sudo, 2000-07, numero 46a</url></lok>
        </fnt>;
      </ekz><ekz>
        la verkojn de Zelenka kunigis kaj katalogis la
        <tld/>sciencisto 
        Wolfgang Reich
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;Jan Dismas
        Zelenka&rightquot;, 
            2007-02-24</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">music&ograve;leg</trd>
  <trd lng="de">Musikwissenschaftler</trd>
  <trd lng="en">musicologist</trd>
  <trd lng="es">music&oacute;logo</trd>
  <trd lng="fr">musicologue</trd>
  <trd lng="id">musikolog</trd>
  <trdgrp lng="nl">
    <trd>muziekwetenschapper</trd>,
    <trd>musicoloog</trd>
  </trdgrp>
  <trd lng="pl">muzykolog</trd>
  <trd lng="ru">&c_m;&c_u;&c_z;&c_y;&c_k;&c_o;&c_v;&c_je;&c_d;</trd>
</drv>
