<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kar</rad>/a  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kar.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Amata:
      <ekz>
        <tld/>a patrino;
      </ekz>
      <ekz>
        nia <tld/>memora majstro;
      </ekz>
      <ekz>
        esti <tld/>a al iu<fnt>Z</fnt>,
      </ekz>
      <ekz>
        por la koro de iu<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>estimat</trd>,
      <trd>amat</trd>
    </trdgrp>
    <trd lng="de">lieb</trd>
    <trdgrp lng="en">
      <trd>beloved</trd>,
      <trd>cherished</trd>,
      <trd>loved</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>querido</trd>,
      <trd>querida</trd>
    </trdgrp> 
    <trd lng="hu">kedves</trd>
    <trd lng="ku">delal</trd>
    <trd lng="nl">lief</trd>
    <trdgrp lng="pl">
      <trd>drogi</trd>,
      <trd>mi&lstroke;y</trd>,
      <trd>kochany</trd>
    </trdgrp>
  <trdgrp lng="pt">
    <trd>querido</trd>,
    <trd>amado</trd>,
    <trd>caro</trd>,
    <trd>bem-amado</trd>
  </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_m;&c_i;&c_l;&c_y;&c_j;</trd>,
      <trd>&c_d;&c_o;&c_r;&c_o;&c_g;&c_o;&c_j;</trd>,
      <trd>&c_l;&c_ju;&c_b;&c_i;&c_m;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Alte &scirc;atata, grandvalora:
      <ekz>
        &ccirc;iu por si mem estas la plej <tld/>a
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>a estas dono en minuto de bezono
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        sia estas pli <tld/>a ol najbara<fnt>Z</fnt>;
      </ekz>
      <ekz>
        vi &scirc;telis de mi <tld/>an horon<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>preuat</trd>,
      <trd>apreciat</trd>
    </trdgrp>
    <trd lng="de">wert</trd>
    <trdgrp lng="en">
      <trd>highly <ind>esteemed</ind></trd>,
      <trd>precious</trd>,
      <trd>valuable</trd>
    </trdgrp>
    <trd lng="hu">kedves</trd>
    <trd lng="nl">dierbaar</trd>
    <trdgrp lng="pl">
      <trd>drogi</trd>,
      <trd>mi&lstroke;y</trd>
    </trdgrp>
  <trdgrp lng="pt">
    <trd>muito <ind>estimado</ind></trd>,
    <trd>querido</trd>,
    <trd>prezado</trd>,
    <trd>caro</trd>,
  </trdgrp>
    <trd lng="ru">&c_d;&c_o;&c_r;&c_o;&c_g;&c_o;&c_j;</trd>
  </snc>
  <snc mrk="kar.0a.EKON">
    <uzo tip="fak">EKON</uzo>
    <uzo tip="stl">ARK</uzo>
    <ref tip="dif" cel="kost.multe0a">multekosta</ref>
    <ekz>
      <tld/>a lo&gcirc;ejo;
    </ekz>
    <ekz>
      tro <tld/>a aran&gcirc;o por mal<tld/>a tagman&gcirc;o
      <klr>(<ref tip="vid" cel="kandel.0o">kandelo</ref>)</klr>
      <fnt><bib>PrV</bib></fnt>;
    </ekz>
    <ekz>
      pli<tld/>i&gcirc;o de la vivo;
    </ekz>
    <ekz>
      pli<tld/>igi la varojn;
    </ekz>
    <ekz>
      pagi <tld/>e<fnt>Z</fnt>;
    </ekz>
    <ekz>
      kosti <tld/>e<fnt>Z</fnt>, mal<tld/>e<fnt>Z</fnt>;
    </ekz>
    <ekz>
      tempo de <tld/>eco<fnt>Z</fnt>.
    </ekz>
    <trdgrp lng="ca">
      <trd>valu&oacute;s</trd>,
      <trd>cotitzat</trd>
    </trdgrp>
    <trd lng="de">teuer</trd>
    <trdgrp lng="en">
      <trd>costly</trd>,
      <trd>expensive</trd>,
      <trd>high-priced</trd>
    </trdgrp>
    <trd lng="hu">dr&aacute;ga</trd>
    <trd lng="nl">duur</trd>
    <trd lng="pl">drogi</trd>
  <trdgrp lng="pt">
    <trd>caro</trd>,
    <trd>oneroso</trd>,
    <trd>precioso</trd>
  </trdgrp>
    <trd lng="ru">&c_d;&c_o;&c_r;&c_o;&c_g;&c_o;&c_j;
    <klr>(&c_p;&c_o; &c_c;&c_je;&c_n;&c_je;)</klr></trd>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_r;&c_a;&c_g;&c_ib;</trd>
  <trdgrp lng="cs">
    <trd>drah&yacute;</trd>,
    <trd>mil&yacute;</trd>,
    <trd>vz&aacute;cn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cher<klr tip="amb"> (aim&eacute;)</klr></trd>,
    <trd>cher<klr tip="amb"> (co&ucirc;teux)</klr></trd>,
    <trd>aim&eacute;<klr tip="ind"> (adj.)</klr></trd>,
    <trd>bien-aim&eacute;<klr tip="ind"> (adj.)</klr></trd>,
    <trd>ch&eacute;ri<klr tip="ind"> (adj.)</klr></trd>,
    <trd>co&ucirc;teux</trd>,
    <trd>on&eacute;reux</trd>,
    <trd>pr&eacute;cieux</trd>
  </trdgrp>
  <trd lng="oc">car</trd>
  <trdgrp lng="ro">
    <trd>drag</trd>,
    <trd>scump</trd>
  </trdgrp> 
  <trdgrp lng="sk">
    <trd>drah&yacute;</trd>,
    <trd>mil&yacute;</trd>,
    <trd>vz&aacute;cny</trd>
  </trdgrp>
</drv>

<drv mrk="kar.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kar.0igi.KOMUNE">
    <dif>
      Igi <tld/>a, aminda:
      <ekz>
        Len tamen havis alian trajton, kiu <tld/>igis lin al Jano:
        li ne nur estis hardita aviadisto, sed anka&ubreve; hardita
        drinkulo 
        <fnt>
          <aut>J. Francis</aut>
          <vrk>La Granda Kaldrono</vrk>
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">rendre <ind>cher</ind></trd>
</drv>

<drv mrk="kar.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="kar.0ulo.KOMUNE">
    <dif>
      Kara vira persono.
    </dif>
  </snc>
  <trd lng="ca">estimat<klr> (subst.)</klr></trd>
  <trdgrp lng="fr">
    <trd>ch&eacute;ri<klr> (subst.)</klr></trd>,
    <trd>aim&eacute;<klr> (subst.)</klr></trd>,
    <trd>bien-aim&eacute;<klr> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>drogi <klr> (osoba)</klr></trd>,
    <trd>kochany <klr> (osoba)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="kar.0ulino">
  <kap><tld/>ulino</kap>
  <snc mrk="kar.0ulino.KOMUNE">
    <dif>
      Kara ina persono.
    </dif>
  </snc>
  <trd lng="ca">estimada<klr> (subst.)</klr></trd>
  <trdgrp lng="fr">
    <trd>ch&eacute;rie<klr> (subst.)</klr></trd>,
    <trd>aim&eacute;e<klr> (subst.)</klr></trd>,
    <trd>bien-aim&eacute;e<klr> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>droga <klr> (osoba)</klr></trd>,
    <trd>kochana <klr> (osoba)</klr></trd>
  </trdgrp>
</drv>
</art>
