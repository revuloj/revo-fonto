    <trd>&#23000;&#20245; <pr>p&imacron;nf&umacron;</pr></trd>,
    <trd>&#20146;&#29233;&#30340; <pr>q&imacron;n&agrave;ide</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kar.0ulino">
  <kap><tld/>ulino</kap>
  <snc mrk="kar.0ulino.KOMUNE">
    <dif>
      Inseksa <ref tip="super" cel="kar.0ulo"><tld/>ulo</ref>:
      <ekz>
        re&gcirc;idinoj estas inter viaj <tld/>ulinoj
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 45:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        nesteton fondos mi kun &scirc;i, kun mia <tld/>ulino
        <fnt>
          <bib>Fab3</bib>
          <lok>Io</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_r;&c_a;&c_g;&c_a;&c_ja; <klr>(&c_n;&c_a;&c_z;.)</klr></trd>
  <trd lng="ca">estimada<klr> (subst.)</klr></trd>
  <trdgrp lng="de">
    <trd>Teuerste</trd>,
    <trd>Geliebte</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ch&eacute;rie<klr> (subst.)</klr></trd>,
    <trd>aim&eacute;e<klr> (subst.)</klr></trd>,
