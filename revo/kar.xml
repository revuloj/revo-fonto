<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kar</rad>/a
</kap>
<drv mrk="kar.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Amata:
      <ekz>
        <tld/>a patrino;
      </ekz>
      <ekz>
        nia <ind><tld/>memora</ind> majstro;
        <trdgrp lng="fr">
          <trd>ch&eacute;ri<klr tip="ind"> (adj.)</klr></trd>,
          <trd>dont la m&eacute;moire [nous] est <ind>ch&egrave;re</ind></trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind><tld/>ul(in)o</ind>;
        <trdgrp lng="fr">
          <trd><ind>ch&eacute;ri</ind>(e)<klr tip="ind"> (subst.)</klr></trd>,
          <trd><ind>aim&eacute;</ind>(e)<klr tip="ind"> (subst.)</klr></trd>,
          <trd>bien-<ind>aim&eacute;</ind>(e)<klr tip="ind"> (subst.)</klr></trd>
        </trdgrp>
      </ekz>
      <ekz>
        esti <tld/>a al iu<fnt>Z</fnt>,
      </ekz>
      <ekz>
        por la koro de iu<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">lieb</trd>
    <trdgrp lng="en">
      <trd>beloved</trd>,
      <trd>cherished</trd>,
      <trd>loved</trd>
    </trdgrp>
    <trd lng="hu">kedves</trd>
    <trd lng="nl">lief</trd>
    <trdgrp lng="ru">
      <trd>&c_m;&c_i;&c_l;&c_y;&c_j;</trd>,
      <trd>&c_d;&c_o;&c_r;&c_o;&c_g;&c_o;&c_j;</trd>,
      <trd>&c_l;&c_ju;&c_b;&c_i;&c_m;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Alte &scirc;atata, grandvalora:
      <ekz>
        &ccirc;iu por si mem estas la plej <tld/>a
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>a estas dono en minuto de bezono
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        sia estas pli <tld/>a ol najbara<fnt>Z</fnt>;
      </ekz>
      <ekz>
        vi &scirc;telis de mi <tld/>an horon<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">wert</trd>
    <trdgrp lng="en">
      <trd>highly <ind>esteemed</ind></trd>,
      <trd>precious</trd>,
      <trd>valuable</trd>
    </trdgrp>
    <trd lng="hu">kedves</trd>
    <trd lng="nl">dierbaar</trd>
    <trd lng="ru">&c_d;&c_o;&c_r;&c_o;&c_g;&c_o;&c_j;</trd>
  </snc>
  <snc mrk="kar.0a.EKON">
    <uzo tip="fak">EKON</uzo>
    <ref tip="dif" cel="kost.multe0a">multekosta</ref>
    <ekz>
      <tld/>a lo&gcirc;ejo;
    </ekz>
    <ekz>
      tro <tld/>a aran&gcirc;o por mal<tld/>a tagman&gcirc;o
      <klr>(<ref tip="vid" cel="kandel.0o">kandelo</ref>)</klr>
      <fnt><bib>PrV</bib></fnt>;
    </ekz>
    <ekz>
      <uzo tip="stl">ARK</uzo>
      pli<tld/>i&gcirc;o de la vivo;
    </ekz>
    <ekz>
      <uzo tip="stl">ARK</uzo>
      pli<tld/>igi la varojn;
    </ekz>
    <ekz>
      <uzo tip="stl">ARK</uzo>
      pagi <tld/>e<fnt>Z</fnt>;
    </ekz>
    <ekz>
      <uzo tip="stl">ARK</uzo>
      kosti <tld/>e<fnt>Z</fnt>, mal<tld/>e<fnt>Z</fnt>;
    </ekz>
    <ekz>
      <uzo tip="stl">ARK</uzo>
      tempo de <tld/>eco<fnt>Z</fnt>.
    </ekz>
    <trd lng="de">teuer</trd>
    <trdgrp lng="en">
      <trd>costly</trd>,
      <trd>expensive</trd>,
      <trd>high-priced</trd>
    </trdgrp>
    <trd lng="hu">dr&aacute;ga</trd>
    <trd lng="nl">duur</trd>
    <trd lng="ru">&c_d;&c_o;&c_r;&c_o;&c_g;&c_o;&c_j; <klr>(&c_p;&c_o; &c_c;&c_je;&c_n;&c_je;)</klr></trd>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_r;&c_a;&c_g;&c_ib;</trd>
  <trd lng="cs">drah&yacute;</trd>
  <trdgrp lng="fr">
    <trd>cher<klr tip="amb"> (aim&eacute;)</klr></trd>,
    <trd>cher<klr tip="amb"> (co&ucirc;teux)</klr></trd>,
    <trd>aim&eacute;<klr tip="ind"> (adj.)</klr></trd>,
    <trd>bien-aim&eacute;<klr tip="ind"> (adj.)</klr></trd>,
    <trd>ch&eacute;ri<klr tip="ind"> (adj.)</klr></trd>,
    <trd>co&ucirc;teux</trd>,
    <trd>on&eacute;reux</trd>,
    <trd>pr&eacute;cieux</trd>
  </trdgrp>
</drv>
</art>
