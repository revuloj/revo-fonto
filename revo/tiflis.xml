<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: tiflis.xml,v 1.7 2024/01/18 21:23:00 revo Exp $">
    <kap>
      <rad>Tiflis</rad>/o,
      <var><kap><rad var="b">Tbilis</rad>/o</kap></var>
    </kap>

<drv mrk="tiflis.0o">
  <kap><tld/>o, <var><kap><tld var="b"/>o</kap></var></kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc mrk="tiflis.0o.cxefurbo">
    <dif>
      <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&Ccirc;efurbo</ref>
      de <ref tip="malprt" cel="kartvel.K0ujo">Kartvelujo</ref>;
      situo: (<frm>41&#x2DA;43&minute;N 44&#x2DA;47&minute;E</frm>):
      <ekz>
        jen estis fra&ubreve;lin' en <tld/>o, &scirc;in viro invitis al kiso
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url
          ref="https://cezarkulturo.blogspot.com/2013/05/raymond-schwartz-duoncento-da-fraulinoj.html#!">Duoncento
          Da Fra&ubreve;linoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kartveloj, rusoj, armenoj kaj aliaj naciuloj en malnova <tld/>o lernadis la
        lingvon kaj korespondadis kun aliaj samideanoj
        <fnt>
          <vrk><url ref="http://www.esperanto-chicago.org/newsletters/06_15.pdf">&Scirc;ota Rustaveli
          (1172-1216), Rustaveli kaj Esperanto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la komunumo de impostpagantaj bur&gcirc;oj de <tld/>o
        <fnt>
          <vrk><url ref="http://www.ipernity.com/blog/stanislavo-belov/439591">Jurij-Raul Ervje,
          franco, kiu malkovris Siberion ...</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en <tld var="b"/>o, la nuna &ccirc;efurbo de Kartvelio <klr>[&#8230;]</klr> oni protektas vesta&jcirc;ojn
        kaj tapi&scirc;ojn kontra&ubreve; insektoj per piretro-pulvoro
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2013/010891.php">De praavo al arto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
    <trdgrp lng="be">
      <trd>&c_T;&c_b;&c_ib;&c_l;&c_ib;&c_s;&c_ib;</trd>,
      <trd>&c_T;&c_y;&c_f;&c_l;&c_ib;&c_s;</trd>
    </trdgrp>
    <trd lng="de">Tiflis</trd>
    <trd lng="en">Tbilisi</trd>
    <trd lng="fr">Tbilissi</trd>
    <trd lng="ja">&#x30C8;&#x30D3;&#x30EA;&#x30B7;</trd>
    <trd lng="pl">Tbilisi</trd>
</drv>
  </art>

