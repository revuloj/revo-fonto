<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: harmon3.xml,v 1.20 2023/08/31 15:46:14 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>harmonium</rad>/o
</kap>
<drv mrk="harmon3.0o">
  <kap><tld/>o</kap>
  <snc mrk="harmon3.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Klavara orgensimila <ref tip="lst" cel="instru2.blov0o" lst="voko:blovinstrumentoj">blovinstrumento</ref> sen tuboj sed
      kun liberaj <ref tip="prt" cel="ancx.0o">an&ccirc;oj</ref>,
      kiujn piede premataj balgoj vibrigas:
      <ekz>
        la unua <tld/>o estis konstruita en 1810 de diversaj inventistoj, &ccirc;irka&ubreve; 1900 &gcirc;i
        estis la plej grava instrumento por hejma distra muziko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=207923">Harmoniumo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        dek kvin infanaj kantoj kun piana a&ubreve; <tld/>a akompano
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=9920">Karel Emanuel Macan</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        Geedzi&gcirc;opreludo &ndash; por mia kara filo <nom>Franz</nom> por la 15-a de januaro 1924 en
        B&#x266D;-ma&jcirc;oro por du <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19818">Richard Strauss</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/9/93/Harmonium%2C_mod&egrave;le_d&eacute;riv&eacute;_de_Mannborg%2C_restaur&eacute;_par_France_Harmonium.JPG/321px-Harmonium%2C_mod&egrave;le_d&eacute;riv&eacute;_de_Mannborg%2C_restaur&eacute;_par_France_Harmonium.JPG" prm="CC BY-SA 4.0"><tld/>o
        <fnt>
          <aut>Webinarts, propra verko</aut>
          <lok><url ref="&WCW;/File:Harmonium,_mod%C3%A8le_d%C3%A9riv%C3%A9_de_Mannborg,_restaur%C3%A9_par_France_Harmonium.JPG">&WCW;/File:Harmonium,_mod&egrave;le_d&eacute;riv&eacute;_de_Mannborg,_restaur&eacute;_par_France_Harmonium.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_s;&c_g;&c_a;&c_r;&c_m;&c_o;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="br">armolud</trd>
  <trd lng="de">Harmonium</trd>
  <trd lng="fr">harmonium</trd>
  <trd lng="hu">harm&oacute;nium</trd>
  <trd lng="id">harmonium</trd>
  <trd lng="it">armonium</trd>
  <trdgrp lng="ja">
    <trd>&#x30EA;&#x30FC;&#x30C9;&#x30AA;&#x30EB;&#x30AC;&#x30F3;</trd>,
    <trd>&#x30CF;&#x30FC;&#x30E2;&#x30CB;&#x30A6;&#x30E0;</trd>
  </trdgrp>
  <trd lng="nl">harmonium</trd>
  <trdgrp lng="pt">
    <trd>harm&ocirc;nio</trd>,
    <trd>harm&ocirc;nico <klr tip="amb">(subst.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_i;&c_s;&c_g;&c_a;&c_r;&c_m;&c_o;&c_n;&c_i;&c_ja;</trd>
  <trd lng="uk">&c_f;&c_ib;&c_s;&c_g;&c_a;&c_r;&c_m;&c_o;&c_n;&c_ib;&c_ja;</trd>
</drv>
</art>
