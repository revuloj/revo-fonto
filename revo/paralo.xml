<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>paralogism</rad>/o</kap>
<drv mrk="paralo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIL</uzo>
  <snc>
    <dif>
      Erariga, &scirc;ajne &gcirc;usta rezono, farita sen trompa intenco.
    </dif>
    <refgrp tip="sin">
      <ref cel="sofism.0o">sofismo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Paralogismus</trd>,
    <trd>Fehlschluss</trd>,
    <trd>Trugschluss</trd>
  </trdgrp>
  <trd lng="nl">paralogisme</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_r;&c_a;&c_l;&c_o;&c_g;&c_i;&c_z;&c_m;</trd>,
    <trd>&c_l;&c_o;&c_zh;&c_n;&c_o;&c_je; &c_u;&c_m;&c_o;&c_z;&c_a;&c_k;&c_l;&c_ju;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
