<kap><rad>paralogism</rad>/o</kap>

<drv mrk="paralo.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">FIL</uzo>
    <dif>
      Erariga, &scirc;ajne &gcirc;usta <ref tip="super" cel="rezon.0o">rezono</ref>,
      farita sen trompa
      intenco:
      <ekz>
        kiam mi estis studento, ni studis logikon, kaj &ccirc;iuj el tiuj
        argumentoj estas
        bazitaj sur <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2009/009682.php">La Pendantaj &Gcirc;ardenoj estis son&gcirc;o</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="sofism.0o">sofismo</ref>.
    </refgrp>
  </snc>
  <trd lng="bg">&c_p;&c_a;&c_r;&c_a;&c_l;&c_o;&c_g;&c_i;&c_z;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>paralogie</trd>,
    <trd>paralogizmus</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Paralogismus</trd>,
    <trd>Fehlschluss</trd>,
    <trd>Trugschluss</trd>
  </trdgrp>
  <trd lng="es">paralogismo</trd>
  <trd lng="fr">paralogisme</trd>
  <trd lng="hu">paralogizmus</trd>
  <trd lng="ja">&#x507D;&#x63A8;&#x7406;  [<ind>&#x304E;&#x3059;&#x3044;&#x308A;</ind>]</trd>
  <trd lng="nl">paralogisme</trd>
  <trd lng="pl">paralogizm</trd>
  <trd lng="pt">paralogismo</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_r;&c_a;&c_l;&c_o;&c_g;&c_i;&c_z;&c_m;</trd>,
    <trd>&c_l;&c_o;&c_zh;&c_n;&c_o;&c_je;
    &c_u;&c_m;&c_o;&c_z;&c_a;&c_k;&c_l;&c_ju;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dobromyse&lcaron;n&yacute; myln&yacute; uz&aacute;ver</trd>,
    <trd>paralogizmus</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x430;&#x440;&#x430;&#x43B;&#x43E;&#x433;&#x456;&#x437;&#x43C;</trd>,
    <trd>&#x43F;&#x43E;&#x43C;&#x438;&#x43B;&#x43A;&#x43E;&#x432;&#x438;&#x439; &#x443;&#x43C;&#x43E;&#x432;&#x438;&#x432;&#x456;&#x434;</trd>
  </trdgrp>
</drv>
