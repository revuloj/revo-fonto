<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>opal</rad>/o
</kap>
<drv mrk="opal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="sxton.juvel0o">Juvel&scirc;tono</ref> el lakteblanka
      a&ubreve; blueta siliko kun &scirc;an&gcirc;koloraj rebriloj:
      <ekz>
        manumo butonumita per granda <tld/>o<fnt><bib>PatrojFiloj</bib>
        <lok>&ccirc;apitro 4a, p. 17a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_a;&c_l;</trd>
  <trd lng="de">Opal</trd>
  <trd lng="el">&omikron;&pi;&alfa_ton;&lambda;&jota;&omikron;</trd>
  <trdgrp lng="fr">
    <trd>opale</trd>
  </trdgrp>
  <trd lng="hu">op&aacute;l</trd>
  <trd lng="nl">opaal</trd>
  <trd lng="pt">opala</trd>
  <trd lng="ru">&c_o;&c_p;&c_a;&c_l;</trd>
  <trd lng="sv">opal</trd>
</drv>
</art>
