<art mrk="$Id: opal.xml,v 1.25 2024/01/30 16:09:26 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>opal</rad>/o
</kap>
<drv mrk="opal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="sxton.juvel0o">Juvel&scirc;tono</ref> 
      el lakteblanka a&ubreve; blueta siliko kun &scirc;an&gcirc;koloraj
      rebriloj:
      <frm>
        SiO<sub>2</sub>+nH<sub>2</sub>O
      </frm>
      <ekz>
        manumo butonumita per granda <tld/>o<fnt><bib>PatrojFiloj</bib>
        <lok>&ccirc;apitro 4a, p. 17a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_a;&c_l;</trd>
  <trd lng="bg">&c_o;&c_p;&c_a;&c_l;</trd>
  <trd lng="br">opal</trd>
  <trd lng="ca">&ograve;pal</trd>
  <trd lng="cs">op&#xE1;l</trd>
  <trd lng="de">Opal</trd>
  <trd lng="el">&omikron;&pi;&alfa_ton;&lambda;&jota;&omikron;</trd>
  <trd lng="en">opal</trd>
  <trdgrp lng="fa">
    <trd>&a_ayn;&f_y;&a_n; &a_A;&a_l;&a_h;&a_r;</trd>&a_komo;
    <trd>&a_A;&a_w;&f_p;&a_A;&a_l;</trd>
  </trdgrp>
  <trd lng="fr">opale</trd>
  <trd lng="hu">op&aacute;l</trd>
  <trdgrp lng="ja">
    <trd>&#x30AA;&#x30D1;&#x30FC;&#x30EB;</trd>,
    <trd>&#x30BF;&#x30F3;&#x30D1;&#x30AF;&#x77F3;  <pr>&#x305F;&#x3093;&#x3071;&#x304F;&#x305B;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="nl">opaal</trd>
  <trd lng="pl">opal</trd>
  <trd lng="pt">opala</trd>
  <trd lng="ru">&c_o;&c_p;&c_a;&c_l;</trd>
  <trd lng="sk">op&#xE1;l</trd>
  <trd lng="sv">opal</trd>
  <trd lng="uk">&#x43E;&#x43F;&#x430;&#x43B;</trd>
  <trd lng="zh">&#x86CB;&#x767D;&#x77F3; <pr>d&#xE0;nb&#xE1;ish&#xED;</pr></trd>
</drv>
</art>
