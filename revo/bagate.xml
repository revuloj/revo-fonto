<drv mrk="bagate.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="bagate.0i.KOMUNE">
    <dif>
      Esti malgrava, ne signife roli en afero:
      <ekz>
        malgra&ubreve; la ironia tono de la &gcirc;isnunaj vortoj, mi agnoskas, ke la afero vo&ccirc;e
        levita neniel <tld/>as
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2012/010885.php">Vo&ccirc;o de ripro&ccirc;o</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        la amrakontetoj supra&jcirc;as, <tld/>as: komplezo al
        fra&ubreve;lino, minueto kun ambasadorino, amo al kuzino
        <fnt>
          <aut>P. Gubbins</aut>,
          <vrk>Supeo frandinda (Dantas J&uacute;lio, La supeo de la
          kardinaloj)</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 1998a, numero 3a, p. 19a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x65E0;&#x5173;&#x5B8F;&#x65E8; <pr>w&#xFA;gu&#x101;nh&#xF3;ngzh&#x1D0;</pr></trd>
</snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_n;&c_ja;&c_v;&c_a;&c_zh;&c_n;&c_y;&c_m;</ind></trd>,
    <trd>&c_n;&c_ja; &c_m;&c_je;&c_c;&c_mol; <ind>&c_z;&c_n;&c_a;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_ja;</ind></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>ser insignificant</trd>,
    <trd>mancar d'import&agrave;ncia</trd>
  </trdgrp>
  <trd lng="cs">b&yacute;t nepatrn&yacute;m</trd>
  <trdgrp lng="de">
    <trd><ind>unbedeutend</ind> sein</trd>,
    <trd><ind>unwichtig</ind> sein</trd>,
    <trd>keine <ind>Bedeutung</ind> haben</trd>
  </trdgrp>
  <trd lng="es">ser una bagatela</trd>
  <trdgrp lng="hu">
    <trd>bagatell dolog</trd>,
    <trd>nincs <ind>jelent&odblac;s&eacute;ge</ind></trd>
  </trdgrp>
  <trd lng="pl">bagatelizowa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>
    &c_b;&c_y;&c_t;&c_mol;
    <ind>&c_n;&c_je;&c_v;&c_a;&c_zh;&c_n;&c_y;&c_m;</ind>
    </trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol;
    <ind>&c_p;&c_u;&c_s;&c_t;&c_ja;&c_ch;&c_n;&c_y;&c_m;</ind>
    </trd>
  </trdgrp>
  <trd lng="sk">by&tcaron; nepatrn&yacute;m</trd>
  <trd lng="sv">vara <ind>bagatellartad</ind></trd>
</drv>

<drv mrk="bagate.0ema">
