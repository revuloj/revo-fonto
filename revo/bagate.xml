<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bagate.xml,v 1.36 2021/04/05 07:52:07 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bagatel</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="bagate.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bagate.0o.KOMUNE">
    <dif>
      Afero negrava, ne serioze atentinda, malmultekosta:
      <ekz>
        li mal&scirc;paras la tempon per <tld/>oj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pingl.0o">pinglo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_u;&c_s;&c_mol;&c_c;&c_ja;&c_k;&c_o;&c_v;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_d;&c_r;&c_o;&c_b;&c_ja;&c_z;&c_mol;</trd>,
    <trd>&c_d;&c_r;&c_a;&c_b;&c_ja;&c_z;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_d;&c_r;&c_je;&c_b;&c_o;&c_l;&c_i;&c_ja;</trd>
  <trdgrp lng="br">
    <trd>bitrak</trd>,
    <trd>disterajo&ugrave;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>fotesa</trd>,
    <trd>bagatel&middot;la</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hnida</trd>,
    <trd>lap&aacute;lie</trd>,
    <trd>malichernost</trd>,
    <trd>nicotnost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bagatelle</trd>,
    <trd>Kleinigkeit</trd>,
    <trd>Kleinkram</trd>,
    <trd>Nichtigkeit</trd>,
    <trd>Schnickschnack</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>trifle</trd>,
    <trd>trivium</trd>,
    <trd>bagatelle</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>bagatela</trd>,
    <trd>trivialidad</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bagatelle</trd>,
    <trd>babiole</trd>,
    <trd>broutille</trd>
  </trdgrp>
  <trd lng="he">&#x5D3;&#x5D1;&#x5E8; &#x5D7;&#x5E1;&#x5E8; &#x5E2;&#x5E8;&#x5DA;</trd>
  <trdgrp lng="hu">
    <trd>csek&eacute;lys&eacute;g</trd>,
    <trd>semmis&eacute;g</trd>,
    <trd>bagatell dolog</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>tetek</ind> bengek</trd>,
    <trd>trivia</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>sciocchezza<klr> (cosa da poco)</klr></trd>,
    <trd>bazzecola</trd>,
    <trd>bagatella</trd>,
    <trd>carabattola</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bagatel</trd>,
    <trd>kleinigheid</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bagatela</trd>,
    <trd>b&lstroke;ahostka</trd>,
    <trd>drobnostka</trd>,
    <trd>drobiazg</trd>
  </trdgrp>
  <trd lng="pt">bagatela</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_u;&c_s;&c_t;&c_ja;&c_k;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_d;&c_je;&c_l;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_m;&c_je;&c_l;&c_o;&c_ch;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bagate&lcaron;</trd>,
    <trd>nepatrn&aacute; vec</trd>
  </trdgrp>
  <trd lng="sv">bagatell</trd>
</drv>

<drv mrk="bagate.0a">
  <kap><tld/>a</kap>
  <snc mrk="bagate.0a.KOMUNE">
    <dif>
      Malgrava, malserioza, ne inda je konsidero:
      <ekz>
        komencante de aferoj <tld/>aj kaj transirante al aferoj plej 
        gravaj
        <fnt>
          <aut>L. L. Zamenhof</aut>,
          <vrk>Tria kongreso en Cambridge</vrk>,
          <bib>Paroloj</bib>,
          <lok>1907</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="grav.mal0a">malgrava</ref>,
      <ref cel="serioz.mal0a">malserioza</ref>
    </refgrp>    
    <refgrp tip="vid">
      <ref cel="neglek.0inda">neglektinda</ref>,
      <ref cel="signif.sen0a">sensignifa</ref>,
      <ref cel="valor.sen0a">senvalora</ref>,
      <ref cel="vant.0a">vanta</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_z;&c_n;&c_a;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_d;&c_r;&c_o;&c_b;&c_ja;&c_z;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_ja; &c_v;&c_a;&c_r;&c_t;&c_y; &c_w;&c_v;&c_a;&c_g;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_n;&c_je;&c_z;&c_n;&c_a;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_je;&c_n;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_g;&c_i;&c_n;&c_a;&c_l;&c_je;&c_n;</trd>
  </trdgrp>
  <trd lng="br">dister</trd>
  <trdgrp lng="ca">
    <trd>insignificant</trd>,
    <trd>trivial</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bagateln&iacute;</trd>,
    <trd>bezv&yacute;znamn&yacute;</trd>,
    <trd>miziv&yacute;</trd>,
    <trd>ned&uring;le&zcaron;it&yacute;</trd>,
    <trd>nepatrn&yacute;</trd>,
    <trd>nicotn&yacute;</trd>,
    <trd>tit&ecaron;rn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>unbedeutend</trd>,
    <trd>nichtig</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>trivial</trd>,
    <trd>marginal</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>intrascendente</trd>,
    <trd>insignificante</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>insignifiant</trd>,
    <trd>futile<klr> (insignifiant)</klr></trd>
  </trdgrp>
  <trd lng="he">&#x5D7;&#x5E1;&#x5E8; &#x5E2;&#x5E8;&#x5DA;</trd>
  <trdgrp lng="hu">
    <trd>bagatell</trd>,
    <trd>jelent&eacute;ktelen</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>remeh</trd>,
    <trd>trivial</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>banale</trd>,
    <trd>futile</trd>,
    <trd>irrisorio</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>b&lstroke;ahy</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_u;&c_s;&c_t;&c_ja;&c_k;&c_o;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_u;&c_s;&c_t;&c_ja;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_v;&c_a;&c_zh;&c_n;&c_y;&c_j;</trd>,
    <trd>
    &c_n;&c_je; &c_s;&c_t;&c_o;&c_ja;&c_shch;&c_i;&c_j;
    &c_v;&c_n;&c_i;&c_m;&c_a;&c_n;&c_i;&c_ja;
    </trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bezv&yacute;znamn&yacute;</trd>,
    <trd>ned&ocirc;le&zcaron;it&yacute;</trd>,
    <trd>nepatrn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>bagatellartad</trd>,
    <trd>ringa</trd>,
    <trd>obetydlig</trd>,
    <trd>betydelsel&ouml;s</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_r;&c_ib;&c_b;&#8217;&c_ja;&c_z;&c_k;&c_o;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_z;&c_n;&c_a;&c_ch;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_ib;&c_s;&c_t;&c_o;&c_t;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="bagate.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="bagate.0i.KOMUNE">
    <dif>
      Malgravi, ne signife roli en afero:
      <ekz>
        La amrakontetoj supra&jcirc;as, bagatelas: komplezo al
        fra&ubreve;lino, minueto kun ambasadorino, amo al kuzino<fnt>
        <aut>P. Gubbins</aut>,
        <vrk>Supeo frandinda (Dantas J&uacute;lio, La supeo de la
        kardinaloj)</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 1998a, numero 3a, p. 19a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>ser insignificant</trd>,
    <trd>mancar d'import&agrave;ncia</trd>
  </trdgrp>
  <trd lng="cs">b&yacute;t nepatrn&yacute;m</trd>
  <trdgrp lng="de">
    <trd>unbedeutend sein</trd>,
    <trd>unwichtig sein</trd>
  </trdgrp>
  <trd lng="es">ser una bagatela</trd>
  <trdgrp lng="hu">
    <trd>bagatell dolog</trd>,
    <trd>nincs <ind>jelent&odblac;s&eacute;ge</ind></trd>
  </trdgrp>
  <trd lng="pl">bagatelizowa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>
    &c_b;&c_y;&c_t;&c_mol;
    <ind>&c_n;&c_je;&c_v;&c_a;&c_zh;&c_n;&c_y;&c_m;</ind>
    </trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol;
    <ind>&c_p;&c_u;&c_s;&c_t;&c_ja;&c_ch;&c_n;&c_y;&c_m;</ind>
    </trd>
  </trdgrp>
  <trd lng="sk">by&tcaron; nepatrn&yacute;m</trd>
  <trd lng="sv">vara <ind>bagatellartad</ind></trd>
</drv>

<drv mrk="bagate.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="bagate.0ema.PSI">
    <dif>
      Inklina okupi&gcirc;i per negravaj aferoj.
    </dif>
    <refgrp tip="sin">
      <ref cel="frivol.0a">frivola</ref>;
    </refgrp>
    <refgrp tip="vid">
      <ref cel="pedant.0a">pedanta</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_o;&c_b;&c_ja;&c_z;&c_n;&c_y;</trd>
  <trd lng="br">dibarfet</trd>
  <trdgrp lng="ca">
    <trd>fr&iacute;vol</trd>,
    <trd>f&uacute;til</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>maj&iacute;c&iacute; sklon k nicotnostem</trd>,
    <trd>maj&iacute;c&iacute; sklon k tit&ecaron;rnostem</trd>,
    <trd>r&aacute;d se zab&yacute;vaj&iacute;c&iacute; mali&ccaron;kostmi</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>frivolous</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>futile</trd>,
    <trd>frivole</trd>
  </trdgrp>
  <trd lng="he">&#x5E7;&#x5B7;&#x5D8;&#x5E0;&#x5D5;&#x5BC;&#x5E0;&#x5B4;&#x5D9;</trd>
  <trdgrp lng="hu">
    <trd>apr&oacute;l&eacute;kos</trd>,
    <trd>kicsinyes</trd>
  </trdgrp>
  <trd lng="pl">ma&lstroke;ostkowy</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_l;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_m;&c_je;&c_l;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_u;&c_je;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">malichern&yacute;</trd>
  <trdgrp lng="sv">
    <trd>petig</trd>,
    <trd>pedantisk</trd>
  </trdgrp>
</drv>

<drv mrk="bagate.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="bagate.0igi.OPI">
    <dif>
      Taksi ion malgrava, konduti tiel, ke &gcirc;i aperas neatentinda:
      <ekz>
        Aulus, a&ubreve;skultinte la rakonton, esprimis la opinion, ke
        tiajn signojn oni tamen ne devas <tld/>igi
        <fnt><aut>H. Sienkewicz, trad. L. Zamenhof</aut>, <vrk>Quo 
        vadis?</vrk>, <lok>1933</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="grav.mal0igi">malgravigi</ref>,
      <ref cel="relati.0igi">relativigi</ref>,
      <ref cel="taks.sub0i">subtaksi</ref>
    </refgrp>
  </snc>
  <trd lng="bg">&c_m;&c_a;&c_r;&c_g;&c_i;&c_n;&c_a;&c_l;&c_i;&c_z;&c_i;&c_r;&c_a;</trd>
  <trd lng="br">disteraat</trd>
  <trdgrp lng="ca">
    <trd>trivialitzar</trd>,
    <trd>desvalorar</trd>
  </trdgrp>
  <trd lng="cs">bagatelizovat</trd>
  <trdgrp lng="de">
    <trd>bagatellisieren</trd>,
    <trd>herunterreden</trd>,
    <trd>kleinreden</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>marginalize</trd>
  </trdgrp>
  <trd lng="es">trivializar</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;valoriser</trd>,
    <trd>minimiser</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bagateliz&aacute;l</trd>,
    <trd>lekicsinyel</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>sminuire</trd>,
    <trd>minimizzare</trd>,
    <trd>smontare<klr> (fig.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bagatelizowa&cacute;</trd>,
    <trd>umniejsza&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_n;&c_i;&c_z;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_je;&c_s;&c_c;&c_je;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">bagatelizova&tcaron;</trd>
  <trd lng="sv">bagatellisera</trd>
</drv>
</art>
