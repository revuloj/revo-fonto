    <trd>trivialidad</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bagatelle</trd>,
    <trd>babiole</trd>,
    <trd>broutille</trd>
  </trdgrp>
  <trd lng="he">&dalet;&bet;&resh; &het;&samekh;&resh; &ayin;&resh;&fkaf;</trd>
  <trdgrp lng="hu">
