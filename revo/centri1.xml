<vortaro>
<art mrk="$Id$">
<kap>
<rad>centripet</rad>/a
</kap>
<drv mrk="centri1.0a">
<kap><tld/>a</kap>
<snc num="1">
<uzo tip="fak">FIZ</uzo>
<dif>
Movanta, aganta al la centro, alcentrokura. 
</dif>
  <trdgrp lng="de">
     <trd>zentripetal</trd>,
     <trd>zum <ind>Mittelpunkt</ind> hinstrebend</trd>
  </trdgrp>
<trd lng="fr">centrip&egrave;te</trd>
<trd lng="hu">centripet&aacute;lis</trd>
<trdgrp lng="nl">
    <trd>middelpuntzoekend</trd>,
    <trd>centripetaal</trd>
  </trdgrp>
<trd
lng="ru">&c_c;&c_je;&c_n;&c_t;&c_r;&c_o;&c_s;&c_t;&c_r;&c_je;&c_m;&c_i;&c_t;
&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</snc>
<snc num="2">
<uzo tip="fak">BOT</uzo>
<dif>
<klr>(p.p. <ref cel="inflor.0o">infloresko</ref>)</klr>
&Ccirc;e kiu la eksteraj a&ubreve; la malsupraj floroj elvolvi&gcirc;as la
unuaj.  
</dif>
  <trdgrp lng="de">
     <trd>zentripetal</trd>,
     <trd>botrytisch</trd>
  </trdgrp>
<trd lng="hu">f&uuml;rt&ouml;s<klr tip="ind"> (vir&aacute;gzat)</klr></trd>
<trd lng="nl">centripetaal</trd>
</snc>
<ref tip="ant" cel="centri.0a">centrifuga.</ref>
</drv>
</art>
