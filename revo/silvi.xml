<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>silvi</rad>/o
</kap>
<drv mrk="silvi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="silvi.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Sylvia</trd>), kampobirdo agrable kantanta.
    </dif>
  </snc>
  <trd lng="cs">p&ecaron;nice</trd>
  <trd lng="de">Grasm&uuml;cke</trd>
  <trd lng="en">warbler</trd>
  <trd lng="hu">posz&aacute;ta</trd>
  <trd lng="pl">pokrzewka</trd>
  <trd lng="ru">&c_s;&c_l;&c_a;&c_v;&c_k;&c_a;</trd>
</drv>
<drv mrk="silvi.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="paser.0oformaj">paseroformaj
      birdoj</ref> (<trd lng="la">Sylviidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Grasm&uuml;cken <klr tip="ind">(Familie)</klr></trd>
  <trd lng="fr">fauvettes de l'ancien</trd>
</drv>
</art>
