<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: silvi.xml,v 1.16 2019/01/07 17:10:19 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>silvi</rad>/o
</kap>
<drv mrk="silvi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="silvi.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio <ref tip="super" cel="silvie.0oj"><tld/>edoj</ref>
      (<trd lng="la">Sylvia</trd>), kampo<ref tip="lst" lst="voko:birdoj" cel="bird.0o.ZOO">birdo</ref>
      agrable kantanta:
      <ekz>
        la &gcirc;arden<tld/>o preska&ubreve; neniam renkonti&gcirc;as en &gcirc;ardenoj, &ccirc;ar &gcirc;i postulas
        ampleksajn sova&gcirc;ajn terenojn ne &gcirc;enata de homa &ccirc;eesto, kaj entenanta kaj
        arbojn kaj arbedajn densa&jcirc;ojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19151"
              >&Gcirc;ardensilvio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/4/43/Sylvia_communis_insect.jpg/617px-Sylvia_communis_insect.jpg"
      prm="CC BY-SA 3.0"
      ><tld/>o (Sylvia communis)
        <fnt>
          <aut>Thermos, propra verko</aut>
          <lok><url ref="&WCW;/File:Sylvia_communis_insect.jpg"
              >https://commons.wikimedia.org/wiki/File:Sylvia_communis_insect.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_l;&c_a;&c_s;&c_ja;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>p&ecaron;nice</trd>,
    <trd>p&ecaron;nice</trd>
  </trdgrp>
  <trd lng="de">Grasm&uuml;cke</trd>
  <trd lng="en">warbler</trd>
  <trd lng="fr">fauvette</trd>
  <trd lng="hu">posz&aacute;ta</trd>
  <trd lng="nl">grasmussen</trd>
  <trd lng="pl">pokrzewka</trd>
  <trd lng="ru">&c_s;&c_l;&c_a;&c_v;&c_k;&c_a;</trd>
  <trd lng="sk">penica</trd>
  <trd lng="sv">s&aring;ngf&aring;gel</trd>
</drv>
</art>
