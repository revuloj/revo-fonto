<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: citade.xml,v 1.29 2023/10/31 13:37:03 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>citadel</rad>/o
</kap>
<drv mrk="citade.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Precipa <ref tip="super" cel="fortik.0ajxo">fortika&jcirc;o</ref>,
      rezistejo en urbo:
      <ekz>
        &scirc;ajnis al li, ke li estas komandanto de <tld/>o, kiun subfosas la malamiko
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la 14a jarcento oni konstruis la unuan moskeon en la <tld/>o apud <nom>Afrosiab</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>Anatoli Ionesov</aut>
          <vrk><url ref="&Monato;/2006/008920.php">Perlo &ccirc;e la Silka Vojo</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        dum longa tempo Rusio estis la &ccirc;efa, e&ccirc; la sola
        <tld/>o de la
        Ortodoksio
        <fnt>
        <aut>Sergio Pokrovskij</aut>,
        <vrk>Moskvo jubileas</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 1997a, numero 9a, p. 22a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
       <ref cel="burg.0o">burgo</ref>,
       <ref cel="kreml.0o">kremlo</ref>
    </refgrp>
    <refgrp tip="vid">
       <ref cel="bastio.0o.fortikajxo">bastiono<sncref/></ref>,
       <ref cel="fortres.0o">fortreso</ref>,
       <ref cel="kastel.0o.MIL">kastelo<sncref/></ref>,
    </refgrp>
    <bld lok="&WCU;/thumb/b/bc/Fortaleza_de_Bam%2C_Ir&aacute;n%2C_2016-09-23%2C_DD_09.jpg/640px-Fortaleza_de_Bam%2C_Ir&aacute;n%2C_2016-09-23%2C_DD_09.jpg" prm="CC BY-SA 4.0">rigardo al la antikva <tld/>o de Bam &ccirc;e la Silkvojo, Irano
        <fnt>
          <aut>Diego Delso, propra verko</aut>
          <lok><url ref="&WCW;/File:Fortaleza_de_Bam,_Ir%C3%A1n,_2016-09-23,_DD_09.jpg">&WCW;/File:Fortaleza_de_Bam,_Ir&aacute;n,_2016-09-23,_DD_09.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_e;&c_p;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_c;&c_y;&c_t;&c_a;&c_d;&c_e;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_c;&c_i;&c_t;&c_a;&c_d;&c_je;&c_l;&c_a;</trd>
  <trd lng="ca">ciutadella</trd>
  <trdgrp lng="cs">
    <trd>citadela</trd>,
    <trd>pevnost</trd>,
    <trd>tvrz</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Zitadelle</trd>,
    <trd>Festung</trd>
  </trdgrp>
  <trd lng="en">citadel</trd>
  <trd lng="fr">citadelle</trd>
  <trd lng="he">&mem;&#x5B0;&tsadi;&vav;&#x5BC;&dalet;&#x5B8;&he;</trd>
  <trdgrp lng="hu">
    <trd>fellegv&aacute;r</trd>,
    <trd>citadella</trd>
  </trdgrp>
  <trd lng="it">cittadella<klr> (mil.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x57CE;&#x585E;  <pr>&#x3058;&#x3087;&#x3046;&#x3055;&#x3044;</pr></trd>,
    <trd>&#x7826;  <pr>&#x3068;&#x308A;&#x3067;</pr></trd>,
    <trd>&#x7259;&#x57CE;  <pr>&#x304C;&#x3058;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">citadel</trd>
  <trd lng="pl">cytadela</trd>
  <trd lng="pt">cidadela</trd>
  <trdgrp lng="ru">
    <trd>&c_c;&c_i;&c_t;&c_a;&c_d;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_k;&c_r;&c_je;&c_p;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>citadela</trd>,
    <trd>pevnos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x446;&#x438;&#x442;&#x430;&#x434;&#x435;&#x43B;&#x44C;</trd>,
    <trd>&#x444;&#x43E;&#x440;&#x442;&#x435;&#x446;&#x44F;</trd>
  </trdgrp>
  <trd lng="zh">&#x6839;&#x636E;&#x5730; <pr>g&#x113;nj&#xF9;d&#xEC;</pr></trd>
</drv>
</art>
