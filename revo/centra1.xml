      <trd>&c_c;&c_je;&c_n;&c_t;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_t;&c_je;&c_l;&c_je;&c_g;&c_r;&c_a;&c_f;</trd>,
