</kap>
<drv mrk="centra1.0o">
  <kap><tld/>o</kap>
  <snc mrk="centra1.0o.TEL">
    <uzo tip="fak">TEL</uzo>
    <dif>
      Komutejo, <ref tip="malprt" cel="nod.0o.centro">nodo</ref> en komunikada reto:
      <ekz>
        ankora&ubreve; iuj regionoj (e&ccirc; urboj) estas senigitaj de fiksaj telefonoj pro
        nefunkciado de la tieaj malnovaj telefon<tld/>oj &scirc;tataj
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2006/009032.php">Sendrata telefonado regata de
          monopolistoj</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="centra1.sxalt0o">&scirc;altcentralo</ref>
    <ref tip="vid" cel="nab.0o.FIG">nabo<sncref/></ref>
    <trdgrp lng="be">
      <trd>&c_v;&c_u;&c_z;&c_je;&c_l;
      &c_s;&c_u;&c_v;&c_ja;&c_z;&c_ib;</trd>,
      <trd>&c_t;&c_e;&c_l;&c_je;&c_f;&c_o;&c_n;&c_n;&c_a;&c_ja;
      &c_s;&c_t;&c_a;&c_n;&c_c;&c_y;&c_ja;</trd>
    </trdgrp>
    <trd lng="de">Zentrale</trd>
    <trdgrp lng="en">
      <trd>central</trd>,
      <trd>telephone exchange</trd>
    </trdgrp>
    <trd lng="fr">centrale<klr> (t&eacute;l&eacute;phonie)</klr></trd>
    <trdgrp lng="hu">
      <trd>k&ouml;zpont<klr tip="ind">
      (centr&aacute;l&eacute;)</klr></trd>;
      <trd>centr&aacute;l&eacute;</trd>
    </trdgrp>
    <trd lng="pl">centrala<klr> telefoniczna</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_u;&c_z;&c_je;&c_l; &c_s;&c_v;&c_ja;&c_z;&c_i;</trd>;
      <trd>&c_t;&c_je;&c_l;&c_je;&c_f;&c_o;&c_n;&c_n;&c_a;&c_ja;
      &c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_ja;</trd>
    </trdgrp>
    <trd lng="zh">&#x603B;&#x673A; <pr>z&#x1D2;ngj&#x12B;</pr></trd>
</snc>
  <snc mrk="centra1.0o.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      <ref tip="super" cel="fabrik.0o">Fabriko</ref> produktanta elektran energion
      (sin. <ref tip="sin" cel="elektr.0ejo">elektrejo</ref>)
      a&ubreve; varmon:
      <ekz>
        oni ankora&ubreve; ne konstruis tiun <tld/>on, kaj savi&gcirc;is valora apudrivera
        naturprotektita arbaro
        <fnt><aut>Walter Klag</aut>,
        <vrk><url ref="&Monato;/2000/006938.php">Friedensreich Hundertwasser 1928 -
        2000</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/07, p. 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la 70aj kaj 80aj jaroj la grandaj elektroproduktantoj konstruis nukleajn
        <tld/>ojn
        <fnt>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2000/007052.php">Verda
          nature, science, socie</url></vrk>
          <bib>Monato</bib>
          <lok>2000/08, p. 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        la katastrofo en la &ccirc;ernobila nuklea elektra <tld/>o
        <fnt>
          <aut>LAST</aut>
          <vrk><url ref="&Monato;/2004/008448.html">Fungoj litova
          eksportvaro</url></vrk>
          <bib>Monato</bib>
          <lok>2004/11, p. 13</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj produktantaj elektron el <klr>[&#x2026;]</klr> vento
        <fnt>
          <aut>Evgeni Georgiev</aut>
            <vrk><url ref="&Monato;/2001/007271.php">Ventelektrejoj en ofensivo</url></vrk>
            <bib>Monato</bib>
            <lok>2001/01, p. 21</lok>
          </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="genera2.0o">generatoro</ref>
    <trd lng="be">&c_c;&c_e;&c_n;&c_t;&c_r;&c_a;&c_l;&c_mol;</trd>
    <trd lng="de">Kraftwerk</trd>
    <trdgrp lng="en">
      <trd>power plant</trd>,
      <trd>power station</trd>,
      <trd>generating station</trd>
    </trdgrp>
    <trd lng="fr">centrale <klr>(&eacute;nergie)</klr></trd>
    <trd lng="hu">er&odblac;m&udblac;</trd>
    <trd lng="ru">&c_c;&c_je;&c_n;&c_t;&c_r;&c_a;&c_l;&c_mol;</trd>
    <trd lng="zh">&#x53D1;&#x7535;&#x7AD9; <pr>f&#x101;di&#xE0;nzh&#xE0;n</pr></trd>
</snc>
  <snc mrk="centra1.0o.oficejo">
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref cel="ofic.centra_0ejo">Centra oficejo</ref>:
      <ekz>
        <ind>sindikata <tld/>o</ind>.
        <trd lng="ca"><ind>central</ind> sindical</trd>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_sh;&c_t;&c_a;&c_b;-&c_k;&c_v;&c_a;&c_t;&c_e;&c_r;&c_a;
      <klr tip="amb">(&c_f;&c_ib;&c_r;&c_m;&c_y;,
      &c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ib;)</klr></trd>,
      <trd>&c_g;&c_a;&c_l;&c_o;&c_w;&c_n;&c_y;
      &c_o;&c_f;&c_ib;&c_s;</trd>
    </trdgrp>
    <trd lng="de">Zentrale</trd>
    <trdgrp lng="en">
      <trd>central office</trd>
    </trdgrp>
    <trd lng="fr">centrale</trd>
    <trd lng="hu">k&ouml;zpont<klr tip="ind">
    (sz&eacute;khely)</klr></trd>
    <trdgrp lng="pl">
      <trd>centrala</trd>,
      <trd>kwatera g&lstroke;&oacute;wna</trd>
    </trdgrp>
    <trd lng="ru">&c_sh;&c_t;&c_a;&c_b;-&c_k;&c_v;&c_a;&c_r;&c_t;&c_i;&c_r;&c_a;
    <klr>(&c_o;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_a;&c_c;&c_i;&c_i;)</klr></trd>
    <adm>
      PIV havas alian difinon, kaj la tuta senco &scirc;ajnas arkaika
      a&ubreve; nacilingva.
      <aut>Sergio</aut>
    </adm>
    <trd lng="zh">&#x4E2D;&#x67A2; <pr>zh&#x14D;ngsh&#x16B;</pr></trd>
</snc>
  <trd lng="ca">central</trd>
  <trdgrp lng="cs">
    <trd>centr&aacute;la</trd>,
    <trd>v&yacute;robna energie</trd>,
    <trd>&uacute;st&rcaron;edna</trd>
  </trdgrp>
  <trd lng="es">central</trd>
  <trd lng="he">&mem;&resh;&kaf;&zayin;&yod;&he;</trd>
  <trd lng="it">centrale</trd>
  <trdgrp lng="ja">
    <trd>&#x4E2D;&#x592E;&#x65BD;&#x8A2D;  <pr>&#x3061;&#x3085;&#x3046;&#x304A;&#x3046;&#x3057;&#x305B;&#x3064;</pr></trd>,
    <trd>&#x672C;&#x90E8;  <pr>&#x307B;&#x3093;&#x3076;</pr></trd>,
    <trd>&#x767A;&#x96FB;&#x6240;  <pr>&#x306F;&#x3064;&#x3067;&#x3093;&#x3057;&#x3087;</pr></trd>,
    <trd>&#x96FB;&#x8A71;&#x4EA4;&#x63DB;&#x5C40;  <pr>&#x3067;&#x3093;&#x308F;&#x3053;&#x3046;&#x304B;&#x3093;&#x304D;&#x3087;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">centrale</trd>
  <trd lng="pt">central</trd>
  <trdgrp lng="sk">
    <trd>centr&aacute;la</trd>,
    <trd>&uacute;stred&ncaron;a</trd>
  </trdgrp>
  <trd lng="uk">&c_c;&c_je;&#x43D;&c_t;&c_r;&c_a;&#x43B;&#x44C;</trd>
</drv>

<drv mrk="centra1.akvo0o">
  <kap>
    akvo<tld/>o,
    <var><kap>akvoenergia <tld/>o</kap></var>,
    <var><kap>hidroelektra <tld/>o</kap></var>
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="centra1.0o.ELE">Centralo<sncref/></ref>,
      en kiu oni uzas akvogeneratorojn por produkti elektron:
      <ekz>
        estis konstruitaj pluraj akvo<tld/>oj sur la rivero <nom>Drin</nom>
        <fnt>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2008/009384.php">Energia debato</url></vrk>
           <bib>Monato</bib>
          <lok>2008/01, p. 13</lok>
        </fnt>;
      </ekz>
      <ekz>
        la akvoenergiaj <tld/>oj, kiuj dependas de akvo, havos gravajn problemojn
        <fnt>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2008/009632.php">Temperaturlevi&gcirc;o en
          Tirolo</url></vrk>
          <bib>Monato</bib>
          <lok>2008/12, p. 25</lok>
        </fnt>;
      </ekz>
      <ekz>
        grandaj akvobara&jcirc;oj por funkciigi hidroelektrajn <tld/>ojn estas mal&scirc;atataj de
        mediprotektantoj pro siaj kromefikoj
        <fnt>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2007/009344.php">&Ccirc;u biokarbura&jcirc;oj helpas a&ubreve;
          malhelpas?</url></vrk>
          <bib>Monato</bib>
          <lok>2007/12, p. 16</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_ib;&c_d;&c_r;&c_a;&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_a;&c_s;&c_t;&c_a;&c_n;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_G;&c_E;&c_S;</trd>
  </trdgrp>
  <trd lng="de">Wasserkraftwerk</trd>
  <trd lng="ja">&#x6C34;&#x529B;&#x767A;&#x96FB;&#x6240;  <pr>&#x3059;&#x3044;&#x308A;&#x3087;&#x304F;&#x306F;&#x3064;&#x3067;&#x3093;&#x3057;&#x3087;</pr></trd>
  <trdgrp lng="zh">
    <trd>&#x6C34;&#x7535;&#x7AD9; <pr>shu&#x1D0;di&#xE0;nzh&#xE0;n</pr></trd>,
    <trd>&#x6C34;&#x529B;&#x53D1;&#x7535;&#x5382; <pr>shu&#x1D0;l&#xEC;f&#x101;di&#xE0;nch&#x1CE;ng</pr></trd>,
    <trd>&#x6C34;&#x529B;&#x53D1;&#x7535;&#x7AD9; <pr>shu&#x1D0;l&#xEC;f&#x101;di&#xE0;nzh&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="centra1.atom0o">
  <kap>
    atom<tld/>o,
    <var><kap>nuklea <tld/>o</kap></var>
  </kap>
  <snc mrk="centra1.atom0o.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      <ref tip="super" cel="centra1.elektro0o.ELE">Elektro<tld/>o</ref>
      produktanta elektron per kernenergia reakcio:
      <ekz>
        dum la tridekjara ekzisto de atom<tld/>oj oni ne ankora&ubreve;
        decidis en Svisio pri la definitiva stokado de la nukleaj
        ruba&jcirc;oj
        <fnt>
          <aut>Ri&scirc;o</aut>
          <vrk><url ref="http://karapacopanoramo.blogspot.fr/2011/04/riso-atomcentraloj-en-svisio.html">
          Atomcentraloj en Svisio</url></vrk>
          <lok>2011-04-13</lok>
        </fnt>;
      </ekz>
      <ekz>
        Litovio akceptis la devon fermi sian nuklean <tld/>on en <nom>Ignalina</nom>, kie funkciis
        du &Ccirc;ernobil-tipaj reaktoroj
        <fnt>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2006/008809.php">Litovio volas resti nukle-energia
          &scirc;tato</url></vrk>
          <bib>Monato</bib>
          <lok>2006/01, p. 15</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <klr>[&#x2026;]</klr> venta energio instalita en Hispanio superis en novembro
        unuafoje tiun de la sep atom<tld/>oj
        <fnt>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2005/008577.php">Venta energio superas la
          atoman</url></vrk>
          <bib>Monato</bib>
          <lok>2005/04, p. 21</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x6838;&#x7535;&#x7AD9; <pr>h&#xE9;di&#xE0;nzh&#xE0;n</pr></trd>,
    <trd>&#x6838;&#x7535;&#x5382; <pr>h&#xE9;di&#xE0;nch&#x1CE;ng</pr></trd>,
    <trd>&#x6838;&#x53D1;&#x7535;&#x5382; <pr>h&#xE9;f&#x101;di&#xE0;nch&#x1CE;ng</pr></trd>,
    <trd>&#x6838;&#x80FD;&#x53D1;&#x7535;&#x7AD9; <pr>h&#xE9;n&#xE9;ngf&#x101;di&#xE0;nzh&#xE0;n</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_t;&c_a;&c_m;&c_n;&c_a;&c_ja; &c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_a;&c_s;&c_t;&c_a;&c_n;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_A;&c_E;&c_S;</trd>
  </trdgrp>
  <trd lng="cs">atomov&aacute; elektr&aacute;rna</trd>
  <trdgrp lng="de">
    <trd>Atomkraftwerk</trd>,
    <trd>Kernkraftwerk</trd>
  </trdgrp>
  <trd lng="es">central nuclear</trd>
  <trd lng="fr">centrale nucl&eacute;aire</trd>
  <trd lng="id">PLTN</trd>
  <trd lng="it">centrale nucleare</trd>
  <trd lng="ja">&#x539F;&#x5B50;&#x529B;&#x767A;&#x96FB;&#x6240;  <pr>&#x3052;&#x3093;&#x3057;&#x308A;&#x3087;&#x304F;&#x306F;&#x3064;&#x3067;&#x3093;&#x3057;&#x3087;</pr></trd>
  <trd lng="nl">atoomcentrale</trd>
  <trd lng="ru">
  &c_a;&c_t;&c_o;&c_m;&c_n;&c_a;&c_ja;
&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_ja;
</trd>
  <trd lng="sk">at&oacute;mov&aacute; elektr&aacute;re&ncaron;</trd>
  <trd lng="uk">&c_a;&c_t;&#x43E;&#x43C;&#x43D;&c_a; &c_je;&#x43B;&c_je;&#x43A;&c_t;&c_r;&#x43E;&c_s;&c_t;&c_a;&#x43D;&c_c;&c_ib;&#x44F;</trd>
</drv>

<drv mrk="centra1.biomasa_0o">
  <kap>
    biomasa <tld/>o
  </kap>
  <snc>
    <dif>
      <ref cel="centra1.0o.ELE"><tld lit="C"/>o<sncref/></ref>,
      kiu produktas varmon (kaj eventuale elektron) per bruligo a&ubreve; gasigo de organikaj
      resta&jcirc;oj:
      <ekz>
        ili liveras sega&jcirc;-lignopecetojn al biomasa <tld/>o kaj kompense ricevas varmon
        kaj elektron
        <fnt>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2012/010595.php">Novtipa ekologia turismo</url></vrk>
          <bib>Monato</bib>
          <lok>2012/06, p. 14</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili estas provizataj per varma akvo, kiu fluas rekte el la biomasa centralo
        <fnt>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2012/010595.php">Novtipa ekologia turismo</url></vrk>
          <bib>Monato</bib>
          <lok>2012/06, p. 14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ib;&c_ja;&c_m;&c_a;&c_s;&c_a;&c_v;&c_a;&c_ja; <ind>&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_a;&c_s;&c_t;&c_a;&c_n;&c_c;&c_y;&c_ja;</ind></trd>
  <trd lng="de">Biomassekraftwerk</trd>
</drv>

<drv mrk="centra1.elektro0o">
  <kap>elektro<tld/>o</kap>
  <snc mrk="centra1.elektro0o.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      <ref cel="centra1.0o.ELE"><tld lit="C"/>o<sncref/></ref>
      produktanta elektron; <ref tip="sin" cel="elektr.0ejo">elektrejo</ref>:
      <ekz>
        venenis la rilatojn la elektro-<tld/>o sur la rivero Danubo: slovakoj, malgra&ubreve;
        hungaraj protestoj, deflankigis la riveron por produkti elektron
        <fnt>
          <aut>Karlo Juh&aacute;sz</aut>
          <vrk><url ref="&Monato;/2007/009352.php">Nubo super Danubo</url></vrk>
          <bib>Monato</bib>
          <lok>2007/12, p. 9</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iujare centmiloj da homoj mortas pro la uzo de karbo kiel
        fuelo por elektro<tld/>oj
        <fnt>
          <aut>Cindy</aut>
          <vrk>Elektraj nazioj</vrk>
          <lok><url ref="https://www.mondogazeto.com">
          Mondogazeto</url>, 2013-09-30</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x53D1;&#x7535;&#x7AD9; <pr>f&#x101;di&#xE0;nzh&#xE0;n</pr></trd>
</snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_a;&c_s;&c_t;&c_a;&c_n;&c_c;&c_y;&c_ja;</trd>
  <trd lng="ca"><ind>central</ind> el&egrave;ctrica</trd>
  <trd lng="de">Kraftwerk</trd>
  <trd lng="fr">centrale &eacute;lectrique</trd>
  <trd lng="ja">&#x767A;&#x96FB;&#x6240;  <pr>&#x306F;&#x3064;&#x3067;&#x3093;&#x3057;&#x3087;</pr></trd>
  <trd lng="pl">elektrownia</trd>
</drv>

<drv mrk="centra1.karbo0o">
  <kap>karbo<tld/>o</kap>
  <snc mrk="centra1.karbo0o.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      <ref tip="super" cel="centra1.elektro0o.ELE">Elektro<tld/>o</ref>
      produktanta elektron per bruligo de karbo:
      <ekz>
        rubgaso el karbo<tld/>oj
        <fnt>
          <aut>A. Oberndorfer</aut>
          <vrk><url ref="http://esperantaretradio.blogspot.fr/2013/08/verdigo-de-dezertoj-kontrau-klimata.html">
          Verdigo de dezertoj kontra&ubreve; klimata &scirc;an&gcirc;o</url></vrk>
          <lok>Esperanta Retradio, 2013-08-14</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x706B;&#x529B;&#x53D1;&#x7535; <pr>hu&#x1D2;l&#xEC;f&#x101;di&#xE0;n</pr></trd>
</snc>
  <trd lng="be">&c_v;&c_u;&c_g;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_a;&c_s;&c_t;&c_a;&c_n;&c_c;&c_y;&c_ja;</trd>
  <trd lng="de">Kohlekraftwerk</trd>
  <trd lng="fr">centrale &agrave; charbon</trd>
  <trd lng="ja">&#x77F3;&#x70AD;&#x706B;&#x529B;&#x767A;&#x96FB;&#x6240;  <pr>&#x305B;&#x304D;&#x305F;&#x3093;&#x304B;&#x308A;&#x3087;&#x304F;&#x306F;&#x3064;&#x3067;&#x3093;&#x3057;&#x3087;</pr></trd>
</drv>

<drv mrk="centra1.sxalt0o">
  <kap>
    &scirc;alt<tld/>o
  </kap>
  <snc>
    <dif>
      <ref cel="centra1.0o.TEL">Centralo<sncref/></ref>, el kiu oni reguligas
      provizreton de elektro, akvo, komunikado, trafiko ks:
      <ekz>
        atako kontra&ubreve; la elektronikaj &scirc;altcentraloj de &scirc;tato
        <fnt>
