<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
  <ofc>8</ofc>
  <rad>central</rad>/o
  </kap>

    <drv mrk="centra1.0o">
      <kap><tld/>o</kap>
      <snc mrk="centra1.0o.telefona">
	<dif>
           Komutejo, nodo en komunikada reto telefona a&ubreve; telegrafa.
        </dif>
	<trd lng="fr">centrale</trd>
	<trdgrp lng="ru">
  	  <trd>&c_u;&c_z;&c_je;&c_l; &c_s;&c_v;&c_ja;&c_z;&c_i;</trd>;
    	  <trd>&c_t;&c_je;&c_l;&c_je;&c_f;&c_o;&c_n;&c_n;&c_a;&c_ja; &c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_ja;</trd>
        </trdgrp>
      </snc>
      <snc mrk="centra1.0o.elektrejo">
	<dif>
  	   Fabriko produktanta elektran a&ubreve; termikan energion:
           <ekz>
              <ind>Varmo<tld/>o</ind>;
              <trd lng="ru">&c_t;&c_je;&c_p;&c_l;&c_o;&c_c;&c_je;&c_n;&c_t;&c_r;&c_a;&c_l;&c_mol;</trd>
           </ekz><ekz>
              elektro<tld/>o
              <klr>(<ref tip="sub" cel="elektr.0ejo">elektrejo</ref>)</klr>
           </ekz>
        </dif>
	<trd lng="fr">centrale</trd>
	<trd lng="ru">&c_c;&c_je;&c_n;&c_t;&c_r;&c_a;&c_l;&c_mol;</trd>
      </snc>
      <snc mrk="centra1.0o.oficejo">
	<dif>
  	  Centra oficejo:
          <ekz>
	     sindikata <tld/>o.
          </ekz>
        </dif>
	<trd lng="fr">centrale</trd>
	<trd lng="ru">&c_sh;&c_t;&c_a;&c_b;-&c_k;&c_v;&c_a;&c_r;&c_t;&c_i;&c_r;&c_a; <klr>(&c_o;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_a;&c_c;&c_i;&c_i;)</klr></trd>
	<adm>
           PIV havas alian difinon, kaj la tuta senco &scirc;ajnas arkaika
           a&ubreve; nacilingva.
           <aut>Sergio</aut>
        </adm>
      </snc>
      <trd lng="es">central</trd>
      <trd lng="nl">centrale</trd>
      <trd lng="pt">central</trd>
    </drv>
  </art>

