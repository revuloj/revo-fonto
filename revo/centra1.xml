    </trdgrp>
  </snc>
  <snc num="3">
    <dif>
      Centra oficejo: <ekz>sindikata <tld/>o</ekz>.
    </dif>
    <trd lng="fr">centrale</trd>
  </snc>
</drv>
</art>

