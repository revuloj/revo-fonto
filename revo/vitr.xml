<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>vitr</rad>/o
</kap>
<drv mrk="vitr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vitr.0o.substanco">
    <dif>
      Malmola, travidebla, rompebla substanco, produktata per fando de
      silika sablo kun potaso a&ubreve; sodo:
      <ekz>
        <tld/>o kaj feli&ccirc;o ne estas fortikaj
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="nl">glas</trd>
    <trdgrp lng="fr">
      <trd>verre<klr> (mat&eacute;riau)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="vitr.0o.ajxo">
    <dif>
      Objekto el <tld/>o:
      <ekz>
        fenestra <tld/>o;
      </ekz>
      <ekz>
        okul<tld/>o;
      </ekz>
      <ekz>
        &ccirc;ion mezuri tra sia, persona <tld/>o<fnt>Z</fnt>
        <klr>(ju&gcirc;i arbitre, la&ubreve; propra vidpunkto)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mezur.0i">mezuri</ref>,
      <ref cel="uln.0o">ulno</ref>.
    </refgrp>
    <trd lng="nl">ruit</trd>
  </snc>
  <trd lng="be">&c_sh;&c_k;&c_l;&c_o;</trd>
  <trd lng="de">Glas</trd>
  <trdgrp lng="el">
    <trd>(&gamma;)&ypsilon;&alfa;&lambda;&jota_ton;</trd>,
    <trd>&tau;&zeta;&alfa_ton;&my;&jota;</trd>
  </trdgrp>
  <trd lng="en">glass</trd>
  <trd lng="es">vidrio</trd>
  <trd lng="he">&zayin;&kaf;&vav;&kaf;&yod;&tav;</trd>
  <trd lng="hu">&uuml;veg</trd>
  <trd lng="pt">vidro</trd>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_k;&c_l;&c_o;</trd>
  <trd lng="sv">glas</trd>
</drv>

<drv mrk="vitr.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      El <tld/>o:
      <ekz>
        <ind><tld/>a glaso</ind>.
        <trd lng="hu">&uuml;vegpoh&aacute;r</trd>
      </ekz>
    </dif>
    <trd lng="hu">&uuml;veg-</trd>
  </snc>
  <snc>
    <dif>
      <tld lit="V"/>ita:
      <ekz>
        oni ne &jcirc;etu &scirc;tonojn, sidante en <tld/>a domo.
      </ekz>
    </dif>
    <trd lng="he">&mem;&zayin;&vav;&gimel;&gimel;</trd>
    <trdgrp lng="hu">
      <trd>&uuml;vegezett</trd>,
      <trd>&uuml;veges</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_sh;&c_k;&c_l;&c_ja;&c_n;&c_y;</trd>
  <trd lng="de">gl&auml;sern</trd>
  <trd lng="el">(&gamma;)&ypsilon;&alfa_ton;&lambda;&jota;&ny;&omikron;&sigma_fina;</trd>
  <trdgrp lng="fr">
    <trd>en <ind>verre</ind></trd>
  </trdgrp>
  <trd lng="nl">glazen</trd>
  <trd lng="pt">de vidro</trd>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_k;&c_l;&c_ja;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="vitr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Garni per <tld/>oj:
      <ekz>
        <tld/>ita pordo, galerio.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_k;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_sh;&c_k;&c_l;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="el">&tau;&zeta;&alfa;&my;&omega_ton;&ny;&omega;</trd>
  <trdgrp lng="fr">
    <trd>vitrer</trd>
  </trdgrp>
  <trd lng="he">&lamed;&zayin;&gimel;&gimel;</trd>
  <trd lng="hu">(be)<ind>&uuml;vegez</ind></trd>
  <trd lng="nl">beglazen</trd>
  <trd lng="pt">envidra&ccedil;ar</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_s;&c_t;&c_je;&c_k;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_je;&c_k;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_s;&c_t;&c_je;&c_k;&c_l;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="vitr.0eca">
  <kap><tld/>eca</kap>
  <snc>
    <dif>
      Havanta aspekton de <tld/>o:
      <ekz>
        <tld/>eca okulo, rigardo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_k;&c_l;&c_ja;&c_n;&c_y;</trd>,
    <trd>&c_sh;&c_k;&c_l;&c_o;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="el">(&gamma;)&ypsilon;&alfa;&lambda;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trdgrp lng="fr">
    <trd>vitreux</trd>
  </trdgrp>
  <trd lng="hu">&uuml;veges <klr tip="ind">(&uuml;vegszer&udblac;)</klr></trd>
  <trd lng="nl">glasachtig</trd>
  <trd lng="pt">v&iacute;treo</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_je;&c_k;&c_l;&c_ja;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_t;&c_je;&c_k;&c_l;&c_o;&c_v;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="vitr.0ego">
  <kap><tld/>ego</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="vitral.0o">vitralo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_t;&c_r;&c_a;&c_zh;</trd>
  <trd lng="es">vitral</trd>
  <trdgrp lng="hu">
    <trd>&oacute;lomkeretes <ind>ablak</ind></trd>,
    <trd>sz&iacute;nes <ind>&uuml;vegablak</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_i;&c_t;&c_r;&c_a;&c_zh;</trd>
</drv>

<drv mrk="vitr.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Transformi ion en <tld/>aspektan substancon:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la larmoj <tld/>igis la pupilojn.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="nebul">nebuligi</ref>,
      <ref cel="vual.0i">vuali</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>vitrifier</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>meg&uuml;veges&iacute;t</trd>,
    <trd>&uuml;vegg&eacute; olvaszt</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_v;&c_r;&c_a;&c_shch;&c_a;&c_t;&c_mol; &c_v; <ind>&c_s;&c_t;&c_je;&c_k;&c_l;&c_o;</ind></trd>,
    <trd>&c_o;&c_s;&c_t;&c_je;&c_k;&c_l;&c_o;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="vitr.dom0o">
  <kap>dom<tld/>o</kap>
  <snc>
    <dif>
      Glata, sed ne polurita <tld/>o.
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="glac.0o">glaco</ref>
  </refgrp>
  <trd lng="hu">ablak&uuml;veg</trd>
</drv>

<drv mrk="vitr.grandiga0o">
  <kap>grandiga <tld/>o</kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="lupe.0o">Lupeo</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_v;&c_je;&c_l;&c_ib;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_je; <ind>&c_sh;&c_k;&c_l;&c_o;</ind></trd>,
    <trd>&c_l;&c_u;&c_p;&c_a;</trd>
  </trdgrp>
  <trd lng="es">lupa</trd>
  <trd lng="hu">nagy&iacute;t&oacute;(&uuml;veg)</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_v;&c_je;&c_l;&c_i;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je;
    &c_s;&c_t;&c_je;&c_k;&c_l;&c_o;</trd>,
    <trd>&c_l;&c_u;&c_p;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="vitr.okul0oj">
  <kap>okul<tld/>oj</kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      Optikaj vitroj por amba&ubreve; okuloj,
      garnitaj per munta&jcirc;o, per kiu oni fiksas ilin
      &ccirc;u sur nazo kiel nazumo, &ccirc;u sur nazo kaj oreloj kiel
      bran&ccirc;vitroj:
      <ekz>
        okul<tld/>oj garnitaj per testuda&jcirc;o;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        rigardi la mondon tra la okul<tld/>oj de l' optimismo
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vitr.0o">vitro</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_u;&c_l;&c_ja;&c_r;&c_y;</trd>
  <trd lng="ca">ulleres</trd>
  <trd lng="de">Brille</trd>
  <trd lng="el">&gamma;&ypsilon;&alfa;&lambda;&jota;&alfa_ton;</trd>
  <trd lng="en">glasses</trd>
  <trdgrp lng="es">
    <trd>gafas</trd>,
    <trd>lentes</trd>
  </trdgrp>
  <trd lng="fr">lunettes</trd>
  <trd lng="he">&mem;&shin;&qof;&pe;&yod;&yod;&fmem;</trd>
  <trd lng="hu">szem&uuml;veg</trd>
  <trd lng="nl">bril</trd>
  <trd lng="pt">&oacute;culos</trd>
  <trd lng="ru">&c_o;&c_ch;&c_k;&c_i;</trd>
  <trd lng="sv">glas&ouml;gon</trd>
  <trd lng="tr">g&ouml;zl&uuml;k</trd>
</drv>
</art>
