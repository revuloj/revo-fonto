<drv mrk="vitr.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      El <tld/>o:
      <ekz>
        <ind><tld/>a glaso</ind>;
        <trd lng="hu">&uuml;vegpoh&aacute;r</trd>
      </ekz>
      <ekz>
        <tld/>a krenujo
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj perloj
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        pupoj kun <tld/>aj okuloj
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        li havis <tld/>an globeton, el kiu fluis parfumoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;ielo staras kiel <tld/>a klo&scirc;o super la maro
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a dinosa&ubreve;ra skeleto, majstroverko de brila <tld/>isto
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9402-filmotondro.txt">Filmotondro</url></vrk>
          <lok>94/02</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">&uuml;veg-</trd>
  </snc>
  <snc>
    <dif>
      <tld lit="V"/>ita:
      <ekz>
        oni ne &jcirc;etu &scirc;tonojn, sidante en <tld/>a domo;
      </ekz>
      <ekz>
        <tld/>a &ccirc;erko
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="he">&mem;&zayin;&vav;&#x2137;&#x2137;</trd>
    <trdgrp lng="hu">
      <trd>&uuml;vegezett</trd>,
      <trd>&uuml;veges</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_sh;&c_k;&c_l;&c_ja;&c_n;&c_y;</trd>
  <trd lng="bg">&c_s;&c_t;&c_malmol;&c_k;&c_l;&c_je;&c_n;</trd>
  <trd lng="cs">sklen&ecaron;n&yacute;</trd>
  <trd lng="de">gl&auml;sern</trd>
  <trd lng="el">(&gamma;)&ypsilon;&alfa_ton;&lambda;&jota;&ny;&omikron;&sigma_fina;</trd>
  <trd lng="en">glass</trd>
  <trdgrp lng="fr">
    <trd>en <ind>verre</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>glazen</trd>,
    <trd>van <ind>glas</ind></trd>
  </trdgrp>
  <trd lng="pt">de vidro</trd>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_k;&c_l;&c_ja;&c_n;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">sklen&yacute;</trd>
</drv>

<drv mrk="vitr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Garni per <tld/>oj:
      <ekz>
        <tld/>ita pordo, galerio;
      </ekz>
      <ekz>
        ru&gcirc;a &scirc;ranko <tld/>ita
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        tablo kovrita de <tld/>itaj skatoloj kun oraj objektoj
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_k;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_sh;&c_k;&c_l;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_o;&c_s;&c_t;&c_malmol;&c_k;&c_l;&c_ja;&c_v;&c_a;&c_m;</trd>
  <trd lng="cs">zaskl&iacute;vat</trd>
  <trd lng="de">verglasen</trd>
  <trd lng="el">&tau;&zeta;&alfa;&my;&omega_ton;&ny;&omega;</trd>
  <trdgrp lng="es">
     <trd>acristalar</trd>,
     <trd>encristalar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>vitrer</trd>
  </trdgrp>
  <trd lng="he">&lamed;&zayin;&#x2137;&#x2137;</trd>
  <trd lng="hu">(be)<ind>&uuml;vegez</ind></trd>
  <trd lng="nl">beglazen</trd>
  <trd lng="pt">envidra&ccedil;ar</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_s;&c_t;&c_je;&c_k;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_je;&c_k;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_s;&c_t;&c_je;&c_k;&c_l;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zaskli&tcaron;</trd>
</drv>

<drv mrk="vitr.0eca">
  <kap><tld/>eca</kap>
  <snc>
    <dif>
      Havanta aspekton de <tld/>o:
      <ekz>
        &scirc;iaj palpebroj &scirc;velis, la pupiloj kovri&gcirc;is per <tld/>eca vualo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia rigardo fari&gcirc;is <tld/>eca kaj retirita internen
        <klr>(ne<ref cel="fokus.0i.KOMUNE">fokusi</ref>ta, 
        <ref cel="klar.mal0a">malklara</ref>)</klr>
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        kun okuloj, kies <tld/>eca rigardo vagadis sur la viza&gcirc;oj de la pasantoj
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_k;&c_l;&c_ja;&c_n;&c_y;</trd>,
    <trd>&c_sh;&c_k;&c_l;&c_o;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>skeln&yacute; <klr>(jako sklo)</klr></trd>,
    <trd>sklovit&yacute;</trd>
  </trdgrp>
  <trd lng="de">glasig</trd>
  <trd lng="el">(&gamma;)&ypsilon;&alfa;&lambda;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>glassy</trd>,
    <trd>vitreous</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>vitreux</trd>
  </trdgrp>
  <trd lng="hu">&uuml;veges<klr tip="ind">
  (&uuml;vegszer&udblac;)</klr></trd>
  <trdgrp lng="nl">
    <trd>glazen</trd>,
    <trd>glasachtig</trd>,
    <trd>hyalo&iuml;deus</trd>,
    <trd>vitreus</trd>
  </trdgrp>
  <trd lng="pt">v&iacute;treo</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_je;&c_k;&c_l;&c_ja;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_t;&c_je;&c_k;&c_l;&c_o;&c_v;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">sklovit&yacute;</trd>
</drv>

<drv mrk="vitr.0ego">
  <kap><tld/>ego</kap>
  <snc>
    <dif>
      Granda <tld/>ofenestro a&ubreve; <ref cel="vitral.0o">vitralo</ref>:
      <ekz>
        <klr>[&gcirc;i]</klr> staras malanta&ubreve; la montro-<tld/>ego, kaj beda&ubreve;rinde
        malanta&ubreve;
        ferkrado
        <fnt>
          <vrk><url ref="http://www.esperantobruselo.org/drupalo/eo/aggregator/sources/4">Sat-Amikaro |
          Esperanto en Bruselo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
<!-- tradukojn necesas kontroli pro adapto de difino:
  <trd lng="be">&c_v;&c_ib;&c_t;&c_r;&c_a;&c_zh;</trd>
  <trd lng="de">Bleiglas(fenster)</trd>
  <trd lng="es">vitral</trd>
  <trdgrp lng="hu">
    <trd>&oacute;lomkeretes <ind>ablak</ind></trd>,
    <trd>sz&iacute;nes <ind>&uuml;vegablak</ind></trd>
  </trdgrp>
  <trd lng="nl">loodglas</trd>
  <trd lng="ru">&c_v;&c_i;&c_t;&c_r;&c_a;&c_zh;</trd>
-->
<trd lng="de">Schaufenster</trd>
</drv>

<drv mrk="vitr.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Transformi ion en <tld/>aspektan substancon:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        larmoj <klr>[&#x2026;]</klr> <tld/>igis la pupilojn
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igi homojn <klr>(el<ref cel="spion.0i">spioni</ref>)</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2008/009660.php">Du mil ok</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="nebul">nebuligi</ref>,
      <ref cel="vual.0i">vuali</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">p&rcaron;em&ecaron;nit na sklo</trd>
  <trdgrp lng="fr">
    <trd>vitrifier</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>meg&uuml;veges&iacute;t</trd>,
    <trd>&uuml;vegg&eacute; olvaszt</trd>
  </trdgrp>
  <trd lng="nl">verglazen</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_v;&c_r;&c_a;&c_shch;&c_a;&c_t;&c_mol; &c_v;
    <ind>&c_s;&c_t;&c_je;&c_k;&c_l;&c_o;</ind></trd>,
    <trd>&c_o;&c_s;&c_t;&c_je;&c_k;&c_l;&c_o;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>robi&tcaron; sklen&yacute;m</trd>,
    <trd>sklovit&yacute;m</trd>
  </trdgrp>
</drv>

