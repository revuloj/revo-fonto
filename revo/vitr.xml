<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vitr</rad>/o
</kap>
<drv mrk="vitr.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="vitr.0o.substanco">
    <dif>
      Malmola, travidebla, rompebla substanco, produktata per fando de
      silika sablo kun potaso a&ubreve; sodo:
      <ekz>
        <tld/>o kaj feli&ccirc;o ne estas fortikaj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="nl">glas</trd>
    <trdgrp lng="fr">
      <trd>verre<klr> (mat&eacute;riau)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="vitr.0o.ajxo">
    <dif>
      Objekto el <tld/>o:
      <ekz>
        fenestra <tld/>o;
      </ekz>
      <ekz>
        okul<tld/>o;
      </ekz>
      <ekz>
        &ccirc;ion mezuri tra sia, persona <tld/>o<fnt>Z</fnt>
      </ekz>
      (ju&gcirc;i arbitre, la&ubreve; propra vidpunkto.
    </dif>
    <refgrp tip="vid">
      <ref cel="mezur.0i">mezuri</ref>,
      <ref cel="uln.0o">ulno</ref>.
    </refgrp>
    <trd lng="nl">ruit</trd>
  </snc>
  <trd lng="de">Glas</trd>
  <trd lng="en">glass</trd>
  <trd lng="sv">glas</trd>
</drv>

<drv mrk="vitr.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      El <tld/>o:
      <ekz>
        <tld/>a glaso.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      <tld/>ita:
      <ekz>
        oni ne &jcirc;etu &scirc;tonojn, sidante en <tld/>a domo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">gl&auml;sern</trd>
  <trdgrp lng="fr">
    <trd>en <ind>verre</ind></trd>
  </trdgrp>
  <trd lng="nl">glazen</trd>
</drv>

<drv mrk="vitr.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Garni per <tld/>oj:
      <ekz>
        <tld/>ita pordo, galerio.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>vitrer</trd>
  </trdgrp>
  <trd lng="nl">beglazen</trd>
</drv>

<drv mrk="vitr.0eca">
  <kap>
    <tld/>eca
  </kap>
  <snc>
    <dif>
      Havanta aspekton de <tld/>o:
      <ekz>
        <tld/>eca okulo, rigardo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>vitreux</trd>
  </trdgrp>
  <trd lng="nl">glasachtig</trd>
</drv>

<drv mrk="vitr.0ego">
  <kap>
    <tld/>ego
  </kap>
  <snc>
    <dif>
      <ref tip="dif" cel="vitral.0o">vitralo</ref>.
    </dif>
  </snc>
</drv>

<drv mrk="vitr.0igi">
  <kap>
    <tld/>igi
  </kap>
  <snc>
    <dif>
      Transformi ion en <tld/>aspektan substancon: (f)
      <ekz>
        la larmoj <tld/>igis la pupilojn.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="nebul">nebuligi</ref>,
      <ref cel="vual.0i">vuali</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>vitrifier</trd>
  </trdgrp>
</drv>

<drv mrk="vitr.dom0o">
  <kap>
    dom<tld/>o
  </kap>
  <snc>
    <dif>
      Glata, sed ne polurita <tld/>o.
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="glac.0o">glaco</ref>
  </refgrp>
</drv>

<drv mrk="vitr.grandiga.0o">
  <kap>
    grandiga <tld/>o
  </kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="lupe.0o">Lupeo</ref>.
    </dif>
  </snc>
</drv>

<drv mrk="vitr.okul0oj">
  <kap>
    okul<tld/>oj
  </kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      Optikaj vitroj por amba&ubreve; okuloj,
      garnitaj per munta&jcirc;o, per kiu oni fiksas ilin
      &ccirc;u sur nazo kiel nazumo, &ccirc;u sur nazo kaj oreloj kiel bran&ccirc;vitroj:
      <ekz>
        okul<tld/>oj garnitaj per testuda&jcirc;o;
      </ekz>
      <ekz>
        <klr>(f)</klr> rigardi la mondon tra la okul<tld/>oj de l' optimismo
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vitr.0o">vitro</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Brille</trd>
  <trd lng="en">glasses</trd>
  <trd lng="fr">lunettes</trd>
  <trd lng="nl">bril</trd>
  <trd lng="sv">glas&ouml;gon</trd>
  <trd lng="tr">g&ouml;zl&uuml;k</trd>
</drv>
</art>
