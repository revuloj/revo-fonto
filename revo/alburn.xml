<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>alburn</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="alburn.0o">
  <kap><tld/>o</kap>
  <snc mrk="alburn.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      La unua ligna tavolo &gcirc;enerale blanka, kiu trovi&gcirc;as senpere
      sub la &scirc;elo, kaj formas &ccirc;iujare novan rondon &ccirc;irka&ubreve; la
      basto.
    </dif>
  </snc>
  <rim mrk="alburn.0o.RIM_HOM">
    <ref tip="hom" cel="alburn.0o">Alburno</ref> estas anka&ubreve;
    fi&scirc;ospeco.
  </rim>
  <trd lng="be">&c_a;&c_b;&c_a;&c_l;&c_o;&c_n;&c_a;</trd>
  <trd lng="de">Splintholz</trd>
  <trd lng="en">sapwood</trd>
  <trd lng="fr">aubier</trd>
  <trdgrp lng="hu">
    <trd>sz&iacute;j&aacute;cs</trd>,
    <trd>alburnum</trd>
  </trdgrp>
  <trd lng="nl">spint</trd>
  <trdgrp lng="pt">
    <trd>alburno</trd>,
    <trd>borne</trd>,
    <trd>branco da &aacute;rvore</trd>    
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_b;&c_o;&c_l;&c_o;&c_n;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_o;&c_l;&c_o;&c_n;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">belica</trd>
</drv>
</art>
