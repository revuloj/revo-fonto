<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>atut</rad>/o
</kap>
<drv mrk="atut.0o">
<kap><ofc>*</ofc><tld/>o</kap>
<snc>
<dif>
Gajniga koloro en la kartludoj.
</dif>
</snc>
<trd lng="de">Trumpf</trd>
<trd lng="fr">atout</trd>
<trd lng="ru">&#1082;&#1086;&#1079;&#1099;&#1088;&#1100;</trd>
</drv>
<drv mrk="atut.0i">
<kap><tld/>i</kap>
<gra><vspec>ntr</vspec></gra>
<snc>
<dif>
Meti, ludi <tld/>on.
</dif>
</snc>
<trdgrp lng="de">
   <trd>trumpfen</trd>,
   <trd>auftrumpfen</trd>,
   <trd>Trumpf spielen</trd></trdgrp>
<trdgrp lng="fr">
   <trd>jouer <ind>atout</ind></trd>,
   <trd>couper (carte)</trd></trdgrp>
<trdgrp lng="ru">
<trd>&#1082;&#1086;&#1079;&#1099;&#1088;&#1103;&#1090;&#1100;</trd>,
<trd>&#1093;&#1086;&#1076;&#1080;&#1090;&#1100; &#1089; 
&#1082;&#1086;&#1079;&#1099;&#1088;&#1103;</trd>
</trdgrp>
</drv>
<drv mrk="atut.super0i">
<kap>super<tld/>i</kap>
<gra><vspec>x</vspec></gra>
<snc>
<dif>
Meti superan <tld/>on en la batalon.
</dif>
</snc>
<trdgrp lng="de">
   <trd>&uuml;bertrumpfen</trd>,
   <trd>ausstechen</trd>,
   <trd>&uuml;berbieten</trd></trdgrp>
<trd lng="fr">surcouper</trd>
</drv>
</art>
