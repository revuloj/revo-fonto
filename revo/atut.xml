<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: atut.xml,v 1.22 2022/12/04 20:25:21 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>atut</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="atut.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="atut.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Gajniga <ref tip="super" cel="kolor.0o.ludkarta">koloro</ref> en la kartludoj:
      <ekz>
        la pastro <klr>[&#8230;]</klr> kovris lian ason per <tld/>o
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        por scii la veron, &scirc;i ludis sian lastan <tld/>on
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Oka</lok>
        </fnt>;
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_o;&c_z;&c_y;&c_r;</trd>
    <trd lng="de">Trumpf</trd>
    <trd lng="en">trump</trd>
    <trdgrp lng="hu">
      <trd>adu</trd>,
      <trd>&uuml;t&odblac;k&aacute;rtya</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>atout</trd>,
      <trd>briscola</trd>
    </trdgrp>
    <trdgrp lng="pl">
    <trd>atu</trd>,
    <trd>atut</trd>
    </trdgrp>
    <trd lng="pt">trunfo</trd>
    <trd lng="ru">&c_k;&c_o;&c_z;&c_y;&c_r;&c_mol;</trd>
    <trd lng="sv">trumf</trd>
  </snc>
  <snc mrk="atut.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="rimed.0o">Rimedo</ref> donanta 
      <ref cel="avanta.0o">avanta&gcirc;o</ref>n:
      <ekz>
        Antonio montras sian plej fortan <tld/>on: la testamenton de Cezaro
        <fnt>
          <bib>Ret</bib>
          <lok>Aldono</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia &ccirc;efa profesia <tld/>o estas liaj okuloj, kaj efektive, lia maniero vide
        inspekti
        <fnt>
          <bib>CKv</bib>
          <lok>3</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Lanti</nom> mem en 1928 iniciatis la <tld/>on de SAT, la Plenan Vortaron
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Apertium</nom> havas esencajn <tld/>ojn:
        &gcirc;i estas malfermkoda kaj senpage uzebla
        <fnt>
          <aut>J. Nordfalk, H. Al&ograve;s i Font</aut>,
          <vrk><url ref="http://www.teleskopo.com/apertium.pdf">
          Apertium kaj Esperanto</url></vrk>,
          <lok>Teleskopo, 2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_o;&c_z;&c_y;&c_r;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_a;&c_v;&c_a;&c_g;&c_a;</trd>
    </trdgrp>
    <trd lng="pl">aut</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>adut</trd>,
    <trd>atut</trd>,
    <trd>trumf <klr>(v kart&aacute;ch)</klr></trd>
  </trdgrp>
  <trd lng="fr">atout</trd>
  <trd lng="he">&qof;&lamed;&fpe; &nun;&tsadi;&het;&vav;&fnun;</trd>
  <trdgrp lng="sk">
    <trd>adut</trd>,
    <trd>tromf</trd>
  </trdgrp>
  <trd lng="uk">&c_k;&c_o;&c_z;&c_i;&c_r;&nbsp;(&c_t;&c_zh; &c_p;&c_je;&c_r;&c_je;&c_n;.)</trd>
</drv>

<drv mrk="atut.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="atut.0i.SPO">
    <dif>
      Meti, ludi <tld/>on:
      <ekz>
        lastinstance mi <tld/>as
        <fnt>
          <bib>LSB</bib>
          <lok>Kremkaful'!</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_z;&c_y;&c_r;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_h;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol; &c_z; <ind>&c_k;&c_o;&c_z;&c_y;&c_r;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="cs">v kart&aacute;ch hr&aacute;t trumfy</trd>
  <trdgrp lng="de">
    <trd>trumpfen</trd>,
    <trd>auftrumpfen</trd>,
    <trd>Trumpf spielen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>trump</trd>,
    <trd>play the <ind>trump</ind> card</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>jouer <ind>atout</ind></trd>,
    <trd>couper (carte)</trd>
  </trdgrp>
  <trd lng="he">&lamed;&nun;&tsadi;&het;</trd>
  <trd lng="hu">adut h&iacute;v</trd>
  <trdgrp lng="it">
    <trd>calare una <ind>briscola</ind></trd>,
    <trd>giocare una <ind>briscola</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bi&cacute; <ind>atutem</ind></trd>,
    <trd>k&lstroke;a&sacute;&cacute; <ind>atu</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_z;&c_y;&c_r;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol; &c_s; &c_k;&c_o;&c_z;&c_y;&c_r;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>adutova&tcaron;</trd>,
    <trd>tromfova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_k;&c_o;&c_z;&c_i;&c_r;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_h;&c_o;&c_d;&c_i;&c_t;&c_i; &c_k;&c_o;&c_z;&c_i;&c_r;&c_je;&c_m;</trd>
  </trdgrp>
</drv>

<drv mrk="atut.mal0o">
  <kap>mal<tld/>o</kap>
  <snc mrk="atut.mal0o.karto">
    <dif>
      Senvalora karto <klr>(figure: <ref cel="gxen.0ajxo">&gcirc;ena&jcirc;o</ref>)</klr>:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mal<tld/>oj de la esperanta editorado estas ver&scirc;ajne tri
        <klr>[&#8230;]</klr>
        <fnt>
          <aut>C. Minnaja</aut>,
          <vrk><url ref="http://esperanto.net/literaturo/lf/konfliktrec.html">
          La konflikto de la epokoj [recenzo]</url></vrk>,
          <lok>Literatura foiro, 1988-04 (112)</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>d&eacute;savantage</trd>,
      <trd>handicap<klr> (fig.)</klr></trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>svantaggio</trd>,
      <trd>handicap<klr> (svantaggio)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="de">Lusche</trd>
  <trd lng="en">blank <klr>(playing card)</klr></trd>
</drv>

<drv mrk="atut.super0i">
  <kap>super<tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="atut.super0i.SPO">
    <dif>
      Meti superan <tld/>on en la batalon:
      <ekz>
        certe, tiel ni povus super<tld/>i la <nom>Pennington</nom>&#8217;ojn, tiun favan bandon
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 6</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_b;&c_ib;&c_c;&c_mol; &c_k;&c_o;&c_z;&c_y;&c_r;&c_a;&c_m;</trd>
  <trd lng="cs">p&rcaron;eb&iacute;t trumfy</trd>
  <trdgrp lng="de">
    <trd>&uuml;bertrumpfen</trd>,
    <trd>ausstechen</trd>,
    <trd>&uuml;berbieten</trd>
  </trdgrp>
  <trd lng="en">trump</trd>
  <trd lng="fr">surcouper</trd>
  <trdgrp lng="hu">
    <trd>fel&uuml;l&uuml;t</trd>,
    <trd>letromfol</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>dominare<klr> (svincere)</klr></trd>,
    <trd>primeggiare<klr> (vincere)</klr></trd>,
    <trd>battere<klr> (vincere)</klr></trd>
  </trdgrp>
  <trd lng="pl">przebija&cacute; <klr>(atutem)</klr></trd>
  <trd lng="sk">pretromfn&uacute;&tcaron;</trd>
</drv>
</art>
