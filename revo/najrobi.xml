  <art mrk="$Id: najrobi.xml,v 1.9 2024/01/29 15:21:35 revo Exp $">
    <kap>
      <rad>Najrobi</rad>/o, <var><kap><rad var="b">Najrob</rad>/o</kap></var>
    </kap>

<drv mrk="najrobi.0o">
  <kap><tld/>o, <var><kap><tld var="b"/>o</kap></var></kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&Ccirc;efurbo</ref>
      de <ref tip="malprt" cel="kenj.K0o">Kenjo</ref>:
      <ekz>
        simile en Novjorko, &Gcirc;enevo, <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2013/010911.php">Kial diplomatoj?</url></vrk>
          <lok>2013/02, p. 6</lok>
        </fnt>;
      </ekz>
      <ekz>
        en Kenjo estas unusola milionurbo: <tld var="b"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=23817">Listo de urbegoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_N;&c_a;&c_j;&c_r;&c_o;&c_b;&c_ib;</trd>
    <trd lng="de">Nairobi</trd>
    <trd lng="en">Nairobi</trd>
    <trd lng="fr">Nairobi</trd>
    <trd lng="nl">Nairobi</trd>
    <trd lng="pl">Nairobi</trd>
    <trd lng="ro">Nairobi</trd>
  </snc>
  <trd lng="ja">&#x30CA;&#x30A4;&#x30ED;&#x30D3;</trd>
  <trd lng="uk">&#x41D;&c_a;&c_j;&c_r;&#x43E;&c_b;&c_ib; (&c_s;&c_t;&#x43E;&#x43B;&c_i;&c_c;&#x44F;)</trd>
  <trdgrp lng="zh">
    <trd>&#x5167;&#x7F85;&#x7562; <pr>N&#xE8;ilu&#xF3;b&#xEC;</pr></trd>,
    <trd>&#x5185;&#x7F57;&#x6BD5; <pr>N&#xE8;ilu&#xF3;b&#xEC;</pr></trd>,
    <trd>&#x5948;&#x6D1B;&#x6BD4; <pr>N&#xE0;ilu&#xF2;b&#x1D0;</pr></trd>
  </trdgrp>
</drv>

