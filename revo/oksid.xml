        plateno estas ne<tld/>igebla.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_a;&c_k;&c_ib;&c_s;&c_mol;&c_l;&c_ja;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trd lng="de">nicht <ind>oxidierbar</ind></trd>
  <trd lng="en">inoxydable</trd>
  <trd lng="fr">inoxydable</trd>
