<drv mrk="oksid.ses0o">
  <kap>ses<tld/>o</kap>
  <snc mrk="oksid.ses0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      <tld lit="O"/>o kies molekuloj entenas po ses oksigenajn atomojn:
