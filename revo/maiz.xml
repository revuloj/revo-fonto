<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>maiz</rad>/o
</kap>
<drv mrk="maiz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="po1.0acoj">poacoj</ref>
      (<trd lng="la">Zea</trd>),
      kies grandaj flavaj grajnoj estas man&gcirc;eblaj kaj
      farunigeblaj.
    </dif>
  </snc>
  <trd lng="de">Mais</trd>
  <trd lng="fr">ma&iuml;s</trd>
  <trd lng="nl">ma&iuml;s</trd>
  <trd lng="pl">kukurydza</trd>
  <trd lng="tr">m&inodot;s&inodot;r</trd>
</drv>
</art>
