<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>maiz</rad>/o
</kap>
<drv mrk="maiz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="maiz.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Zea</trd>), amerikdevena <ref tip="super"
      cel="gren.0o.planto">greno</ref>, kies grandaj flavaj grajnoj estas
      man&gcirc;eblaj kaj farunigeblaj:
      <ekz>
        inter vinber&gcirc;ardenoj kaj <tld/>aj kampoj
        <fnt><bib>Fab3</bib>, <lok>sub la saliko</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_u;&c_k;&c_u;&c_r;&c_u;&c_z;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_ib;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>blat de moro</trd>,
    <trd>moresc</trd>
  </trdgrp>
  <trd lng="br">maiz</trd>
  <trd lng="de">Mais</trd>
  <trd lng="en">corn</trd>
  <trd lng="es">maiz</trd>
  <trd lng="fr">ma&iuml;s</trd>
  <trdgrp lng="hu">
    <trd>kukorica</trd>,
    <trd>tengeri</trd>
  </trdgrp>
  <trd lng="nl">ma&iuml;s</trd>
  <trd lng="pl">kukurydza</trd>
  <trd lng="sv">majs</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_k;&c_u;&c_r;&c_u;&c_z;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_i;&c_s;</trd>
  </trdgrp>
  <trd lng="tr">m&inodot;s&inodot;r</trd>
</drv>

<drv mrk="maiz.0flokoj">
  <kap><ofc>*</ofc><tld/>flokoj</kap>
  <snc mrk="maiz.0flokoj.mangxo">
    <dif>
      Maizflokoj estas farita de kuirita maizo, sukero kaj vitaminoj en
     tre maldikaj plataj flokoj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Cornflakes</trd>
  </trdgrp>
  <trd lng="hu">kukoricapehely</trd>
</drv>
</art>
