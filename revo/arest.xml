<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>arest</rad>/i
</kap>
<drv mrk="arest.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Kapti iun por lin malliberigi:
      <ekz>
        <tld/>i &scirc;teliston.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_r;&c_y;&c_sh;&c_t;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_r;&c_y;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>festnehmen</trd>,
     <trd>verhaften</trd>,
     <trd>arrestieren</trd>
  </trdgrp>
  <trd lng="en">arrest</trd>
  <trd lng="es">arrestar</trd>
  <trdgrp lng="fr">
    <trd>arr&ecirc;ter (un malfaiteur)</trd>,
    <trd>appr&eacute;hender</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>letart&oacute;ztat</trd>,
     <trd>&odblac;rizetbe vesz</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_r;&c_je;&c_s;&c_t;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">uv&auml;zni&tcaron;</trd>
</drv>

<drv mrk="arest.0ito">
  <kap><tld/>ito</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Homo arestita:
      <ekz>la soldatoj kondukis la <tld/>itojn tra la stratoj
      <fnt><bib>F</bib>, <lok>&FE; 22</lok></fnt>.</ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_r;&c_y;&c_sh;&c_t;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_r;&c_y;&c_sh;&c_t;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_v;&c_ja;&c_z;&c_je;&c_n;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Festgenommener</trd>,
     <trd>Verhafteter</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>arrestee</trd>,
     <trd>detainee</trd>
  </trdgrp>
  <trd lng="fr">pr&eacute;venu</trd>
  <trdgrp lng="hu">
     <trd>letart&oacute;ztatott</trd>,
     <trd>&odblac;rizetbe vett</trd>,
     <trd>&odblac;rizetes</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_je;&c_s;&c_t;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_a;&c_r;&c_je;&c_s;&c_t;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_l;&c_ju;&c_ch;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">v&auml;ze&ncaron;</trd>
</drv>

<drv mrk="arest.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago de <tld/>anto.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_y;&c_sh;&c_t;</trd>
  <trdgrp lng="de">
     <trd>Haft</trd>,
     <trd>Arrest</trd>
  </trdgrp>
  <trd lng="en">arrest</trd>
  <trd lng="es">arresto</trd>
  <trd lng="fr">arrestation</trd>
  <trd lng="hu">letart&oacute;ztat&aacute;s</trd>
  <trd lng="ru">&c_a;&c_r;&c_je;&c_s;&c_t;</trd>
</drv>

<drv mrk="arest.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Festnahme</trd>,
     <trd>Verhaftung</trd>
  </trdgrp>
  <trd lng="en">arrest</trd>
  <trd lng="fr">arrestation</trd>
</drv>

<drv mrk="arest.0ejo">
  <kap><tld/>ejo</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Loko, kie oni tenas la <tld/>iton.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Arrestzelle</trd>,
     <trd>Haftzelle</trd>,
     <trd>Kotter</trd>
  </trdgrp>
  <trd lng="en">jail</trd>
  <trd lng="fr">maison d'arr&ecirc;t</trd>
  <trd lng="hu">fogda</trd>
  <trd lng="sk">v&auml;zenie</trd>
</drv>

<drv mrk="arest.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc>
    <dif>
      Stato de <tld/>ato.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_y;&c_sh;&c_t; <klr tip="amb">(&c_s;&c_t;&c_a;&c_n; &c_a;&c_r;&c_y;&c_sh;&c_t;&c_a;&c_v;&c_a;&c_n;&c_a;&c_g;&c_a;)</klr></trd>
  <trd lng="en">arrest</trd>
  <trd lng="fr">arrestation</trd>
</drv>
</art>
