<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: arest.xml,v 1.26 2021/02/28 18:35:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>arest</rad>/i  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="arest.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="arest.0i.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Kapti iun por lin malliberigi:
      <ekz>
        <tld/>i &scirc;teliston.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_r;&c_y;&c_sh;&c_t;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_r;&c_y;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">herzel<klr tip="ind"> (ul laer)</klr></trd>
  <trdgrp lng="cs">
    <trd>arestovat</trd>,
    <trd>uvalit vazbu</trd>,
    <trd>uv&ecaron;znit</trd>,
    <trd>vz&iacute;t do vazby</trd>,
    <trd>v&ecaron;znit</trd>,
    <trd>zatknout</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>festnehmen</trd>,
     <trd>verhaften</trd>,
     <trd>arrestieren</trd>
  </trdgrp>
  <trd lng="en">arrest</trd>
  <trd lng="es">arrestar</trd>
  <trdgrp lng="fr">
    <trd>appr&eacute;hender</trd>,
    <trd>arr&ecirc;ter<klr> (un malfaiteur)</klr></trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5E2;&#x5E6;&#x5D5;&#x5E8;</trd>
  <trdgrp lng="hu">
     <trd>letart&oacute;ztat</trd>,
     <trd>&odblac;rizetbe vesz</trd>
  </trdgrp>
  <trdgrp lng="pl">
     <trd>aresztowa&cacute;</trd>,
     <trd>uj&aogonek;&cacute;</trd>
  </trdgrp>
  <trd lng="pt">prender</trd>
  <trd lng="ru">&c_a;&c_r;&c_je;&c_s;&c_t;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>uv&auml;zni&tcaron;</trd>,
    <trd>zatkn&uacute;&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="arest.0ito">
  <kap><tld/>ito</kap>
  <snc mrk="arest.0ito.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Homo arestita:
      <ekz>
        la soldatoj kondukis la <tld/>itojn tra la stratoj
        <fnt><bib>F</bib>, <lok>&FE; 22</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_r;&c_y;&c_sh;&c_t;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_r;&c_y;&c_sh;&c_t;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_v;&c_ja;&c_z;&c_je;&c_n;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">den <ind>harzet</ind></trd>
  <trd lng="cs">zat&ccaron;en&yacute;</trd>
  <trdgrp lng="de">
     <trd>Festgenommener</trd>,
     <trd>Verhafteter</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>arrestee</trd>,
     <trd>detainee</trd>
  </trdgrp>
  <trd lng="fr">pr&eacute;venu</trd>
  <trd lng="he">&#x5E2;&#x5E6;&#x5D5;&#x5E8;</trd>
  <trdgrp lng="hu">
     <trd>letart&oacute;ztatott</trd>,
     <trd>&odblac;rizetbe vett</trd>,
     <trd>&odblac;rizetes</trd>
  </trdgrp>
  <trdgrp lng="pl">
     <trd>aresztant</trd>
     <trd>aresztowany</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_je;&c_s;&c_t;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_a;&c_r;&c_je;&c_s;&c_t;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_l;&c_ju;&c_ch;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rab</trd>,
    <trd>v&auml;ze&ncaron;</trd>
  </trdgrp>
</drv>

<drv mrk="arest.0o">
  <kap><tld/>o</kap>
  <snc mrk="arest.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Ago de <tld/>anto:
      <ekz>
        tuj post mia <tld/>o mi estis plurfoje pridemandita
        <fnt>
          <aut>A. Camus, trad. M. Duc Goninaz</aut>,
          <vrk>La Fremdulo</vrk>,
          <lok>1993</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_y;&c_sh;&c_t;</trd>
  <trd lng="br">harzidigezh</trd>
  <trdgrp lng="cs">
    <trd>uv&ecaron;zn&ecaron;n&iacute; <klr>(v&yacute;sledek)</klr></trd>,
    <trd>vazba</trd>,
    <trd>zat&ccaron;en&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Haft</trd>,
     <trd>Arrest</trd>
     <trd>Beugehaft</trd>,
     <trd>Zwangshaft</trd>,
  </trdgrp>
  <trd lng="en">arrest</trd>
  <trd lng="es">arresto</trd>
  <trd lng="fr">arrestation</trd>
  <trd lng="he">&#x5DE;&#x5B7;&#x5E2;&#x5B0;&#x5E6;&#x5B8;&#x5E8;</trd>
  <trd lng="hu">letart&oacute;ztat&aacute;s</trd>
  <trd lng="pl">uj&eogonek;cie</trd>
  <trd lng="ru">&c_a;&c_r;&c_je;&c_s;&c_t;<klr tip="amb"> (&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">zatknutie</trd>
</drv>

<drv mrk="arest.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="arest.0ado.JUR">
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <trd lng="br">harzadur</trd>
  <trd lng="cs">zat&yacute;k&aacute;n&iacute;</trd>
  <trdgrp lng="de">
   <trd>Festnahme</trd>,
   <trd>Verhaftung</trd>
  </trdgrp>
  <trd lng="en">arrest</trd>
  <trd lng="fr">arrestation</trd>
  <trd lng="he">&#x5DE;&#x5E2;&#x5E6;&#x5E8;</trd>
  <trd lng="pl">aresztowanie</trd>
  <trd lng="ru">&c_a;&c_r;&c_je;&c_s;&c_t;<klr tip="amb"> (&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">zat&yacute;kanie</trd>
</drv>

<drv mrk="arest.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="arest.0ejo.JUR">
  <uzo tip="fak">JUR</uzo>
    <dif>
      Loko, kie oni tenas la <tld/>iton.
    </dif>
  </snc>
  <trd lng="br">toull-bac'h</trd>
  <trdgrp lng="cs">
    <trd>vazebn&iacute; m&iacute;stnost</trd>,
    <trd>v&ecaron;znice <klr>(pro vazby)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Arrestzelle</trd>,
     <trd>Haftzelle</trd>,
     <trd>Kotter</trd>
  </trdgrp>
  <trd lng="en">jail</trd>
  <trd lng="fr">maison d'arr&ecirc;t</trd>
  <trd lng="he">&#x5D1;&#x5D9;&#x5EA; &#x5DE;&#x5B7;&#x5E2;&#x5B0;&#x5E6;&#x5B8;&#x5E8;</trd>
  <trd lng="hu">fogda</trd>
  <trd lng="pl">areszt</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_ju;&c_r;&c_mol;&c_m;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_m;&c_je;&c_r;&c_a;</trd>,
    <trd>&c_m;&c_je;&c_s;&c_t;&c_o; &c_z;&c_a;&c_k;&c_l;&c_ju;&c_ch;&c_je;&c_n;&c_i;&c_ja;</trd>,
    <trd>&c_a;&c_r;&c_je;&c_s;&c_t;&c_a;&c_n;&c_t;&c_s;&c_k;&c_a;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">v&auml;znica</trd>
</drv>

<drv mrk="arest.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc mrk="arest.0igxo.JUR">
    <dif>
      Stato de <tld/>ato.
    </dif>
  </snc>
  <trd lng="br">dalc'h<klr tip="ind"> (stad un den harzet)</klr></trd>
  <trd lng="be">&c_a;&c_r;&c_y;&c_sh;&c_t; <klr tip="amb">(&c_s;&c_t;&c_a;&c_n;
  &c_a;&c_r;&c_y;&c_sh;&c_t;&c_a;&c_v;&c_a;&c_n;&c_a;&c_g;&c_a;)</klr></trd>
  <trd lng="en">arrest</trd>
  <trd lng="fr">arrestation</trd>
  <trd lng="ru">&c_a;&c_r;&c_je;&c_s;&c_t;<klr tip="amb"> (&c_s;&c_o;&c_s;&c_t;&c_o;&c_ja;&c_n;&c_i;&c_je;
  &c_a;&c_r;&c_je;&c_s;&c_t;&c_o;&c_v;&c_a;&c_n;&c_n;&c_o;&c_g;&c_o;)</klr></trd>
</drv>

<drv mrk="arest.hejma0o">
  <kap>hejma <tld/>o</kap>
  <snc mrk="arest.hejma0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Deviga hejma restado de <tld/>ito kun pli
      a&ubreve; malpli strikta administra kontrolo de liaj
      movi&gcirc;oj kaj agadoj:
      <ekz>
        la ju&gcirc;isto decidis, ke [li] restu en hejma <tld/>o
        <klr>...</klr> li ne rajtas forlasi sian lo&gcirc;ejon sen
        la permeso de la krimesploristo, nek uzi komunikilojn
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.liberafolio.org/2017/07/26/dmitrij-bogatov-nun-en-hejma-aresto/">
          Dmitrij Bogatov nun en hejma aresto</url></vrk>,
          <lok>libera folio, 2017-07-26</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">assignation &agrave; r&eacute;sidence</trd>
</drv>
</art>
