<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: suicid.xml,v 1.13 2019/10/19 10:10:24 revo Exp $">
<kap>
  <rad>suicid</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="suicid.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="suicid.0i.sin_mortigi">
    <dif>
      <ref tip="dif" cel="mort.sin0igi">Sin mortigi</ref>:
      <ekz>
        &scirc;i timis, ke <nom>Siqi</nom> <tld/>os &ccirc;i-nokte post ilia foriro
        <fnt>
          <bib>RugxDom</bib>,
          <lok>&ccirc;apitro 74a, p. 567a</lok>
        </fnt>;
      </ekz>
      <ekz>
        tanatologoj <klr>[&#8230;]</klr> helpas, inter ceteraj taskoj, al <tld/>inklinaj homoj
        dece adia&ubreve;i la vivon
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006895.html">En spegulo de l' sarkasmo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[lia]</klr> <tld/>emo devigis lin ludi &leftquot;rusan ruleton&#8221; &ccirc;iujare je certa dato
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="https://www.monato.be/2006/009002.php">Memoroj neniam perdas sian mal&gcirc;ojon</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_b;&c_ib;&c_c;&c_mol; &c_s;&c_ja;&c_b;&c_je;</trd>,
    <trd>&c_z;&c_mol;&c_d;&c_z;&c_je;&c_j;&c_s;&c_mol;&c_n;&c_ib;&c_c;&c_mol; <ind>&c_s;&c_a;&c_m;&c_a;&c_g;&c_u;&c_b;&c_s;&c_t;&c_v;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="cs">vz&iacute;t si &zcaron;ivot</trd>
  <trd lng="de">suizidieren</trd>
  <trd lng="es">suicidarse</trd>
  <trd lng="fa">&a_kh;&a_w;&a_d;&f_k;&a_sh;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">se <ind>suicider</ind></trd>
  <trdgrp lng="hu">
    <trd>&ouml;ngyilkoss&aacute;got k&ouml;vet el</trd>,
    <trd>&ouml;ngyilkos lesz</trd>
  </trdgrp>
  <trd lng="pl">samob&oacute;jstwo pope&lstroke;ni&cacute;</trd>
  <trd lng="pt"><ind>suicidar</ind>-se</trd>
  <trd lng="ru">&c_p;&c_o;&c_k;&c_o;&c_n;&c_ch;&c_i;&c_t;&c_mol; &c_s; &c_s;&c_o;&c_b;&c_o;&c_j;</trd>
  <trd lng="sk">vzia&tcaron; si &zcaron;ivot</trd>
</drv>

<drv mrk="suicid.0o">
  <kap><tld/>o</kap>
  <snc mrk="suicid.0o.sinmortigo">
    <dif>
      <ref tip="dif" cel="mort.sin0igo">Sinmortigo</ref>:
      <ekz>
        la&ubreve; Monda Organiza&jcirc;o pri Sano okupas Rusio la unuan lokon pri <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Dimitrij Cibulevskij</aut>
          <vrk><url ref="https://www.monato.be/2003/008172.html">Trafiko amba&ubreve;direkta</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        baltaj landoj rimarkindas pro negativaj indicoj: tie oni konstatas plej grandan
        kvanton de tuberkuloz-okazoj, mortoj pro kormalsanoj kaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="https://www.monato.be/2005/008462.php">In-dicoj</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fanatikuloj <klr>[&#8230;]</klr> per <tld/>-atencoj murdas amase ordinarajn homojn,
        kiel okazis anta&ubreve; kvin jaroj en Novjorko per avioj detruintaj komercajn turojn
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2007/009127.php">Sukcesa terorismo</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_m;&c_a;&c_g;&c_u;&c_b;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_a;&c_z;&c_a;&c_b;&c_o;&c_j;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_ib;&c_c;&c_y;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>sebevra&zcaron;da</trd>,
    <trd>suicidium</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Suizid</trd>,
    <trd>Selbstmord</trd>
  </trdgrp>
  <trd lng="es">suicidio</trd>
  <trdgrp lng="fa">
    <trd>&a_kh;&a_w;&a_d;&f_k;&a_sh;&f_y;</trd>,
    <trd>&a_A;&a_n;&a_t;&a_H;&a_A;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">suicide</trd>
  <trd lng="hu">&ouml;ngyilkoss&aacute;g</trd>
  <trd lng="pl">samob&oacute;jstwo</trd>
  <trd lng="pt">suic&iacute;dio</trd>
  <trd lng="ro">suicid</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_i;&c_c;&c_i;&c_d;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_o;&c_u;&c_b;&c_i;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">sebevra&zcaron;da</trd>
</drv>
</art>

