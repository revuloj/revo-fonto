<vortaro>
<art mrk="$Id$">
<kap>
  <rad>suicid</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="suicid.0i">
  <kap><tld/>o</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <ref tip="dif" cel="mort.sin0igi">sinmortigi</ref>
    <ekz>
      &scirc;i timis, ke Siqi <tld/>os &ccirc;i-nokte post ilia foriro
      <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 74a, p. 567a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="fr">se <ind>suicider</ind></trd>
</drv>

<drv mrk="suicid.0o">
  <kap><tld/>o</kap>
  <snc>
    <ref tip="dif" cel="mort.sin0igo">sinmortigo</ref>
  </snc>
  <trd lng="fr">suicide</trd>
</drv>
