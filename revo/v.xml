<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>*</ofc><rad>v</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="v.0">
  <kap>duobla <tld/>o</kap>
    <snc mrk="v.0.duobla">
    <uzo tip="fak">TIP</uzo>
    <dif>
    Neesperanta <ref tip="super" cel="liter.0o.propra">litero</ref>
    <em>w</em>, <em>W</em>.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>w</trd>,
    <trd>W <klr> (double v)</klr></trd>
  </trdgrp>
</drv>
 
<drv mrk="v.0.w">
  <kap><tld/>avo</kap>
    <snc mrk="v.0.vavo">
    <uzo tip="fak">TIP</uzo>
    <dif>
    <ref tip="sin" cel="v.0.duobla">=duobla <tld/>o</ref>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>w</trd>,
    <trd>W <klr> (double v)</klr></trd>
  </trdgrp>
  </drv>
</art>

