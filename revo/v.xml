<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>*</ofc><rad>v</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="v.0">
  <kap>duobla <tld/>o</kap>
    <snc mrk="v.0.duobla">
    <uzo tip="fak">TIP</uzo>
    <dif>
    Neesperanta <ref tip="super" cel="liter.0o.propra">litero</ref> (w,
    W) uzata i.a. kiel:
    </dif>
    <trdgrp lng="fr">
    <trd>w</trd>,
    <trd>W<klr> (double v)</klr></trd>
    </trdgrp>
  <subsnc>
  <uzo tip="fak">KEM</uzo>
  <dif>
  (maj.) simbolo de volframo: <ekz><tld lit="W"/></ekz>;
  </dif>
  <trd lng="fr">tungst&egrave;ne</trd>
  </subsnc>
  <subsnc>
  <uzo tip="fak">FIZ</uzo>
  <dif>
  (maj.) simbolo de vato: <ekz><tld lit="W"/></ekz>;
  </dif>
  <trd lng="fr">Watt</trd>
  </subsnc>
  <ref tip="sin" cel="vav.0o">vavo</ref>   
  </snc> 
</drv>
</art>

