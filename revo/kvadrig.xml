<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kvadrig.xml,v 1.11 2023/12/03 09:59:05 revo Exp $">
<kap>
  <rad>kvadrig</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="kvadrig.0o">
  <kap><tld/>o</kap>
  <snc mrk="kvadrig.0o.SPO">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">SPO</uzo>
    <dif>
      Antikva greka a&ubreve; romia durada <ref cel="cxar1.0o" tip="super">&ccirc;aro</ref> tirata de kvar &ccirc;evaloj, kun staranta 
      veturigisto, uzata en vetkuroj:
      <ekz>
        atinginte grandegan fontanon, starantan en la mezo de du 
        kruci&gcirc;antaj vojoj, li deiris de la <tld/>o
        <fnt><aut>H. Sienkewicz, trad. L. Zamenhof</aut>, <vrk>Quo 
        vadis?</vrk>, <lok>1933</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_v;&c_a;&c_d;&c_r;&c_y;&c_g;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>kvadriga</trd>,
    <trd>quadrila</trd>
  </trdgrp>
  <trd lng="de">Quadriga</trd>
  <trd lng="en">quadriga</trd>
  <trd lng="es">cuadriga</trd>
  <trdgrp lng="fi">
    <trd>quadriga</trd>,
    <trd><klr tip="amb">(antiikin) </klr>nelivaljakko</trd>
  </trdgrp>
  <trd lng="fr">quadrige</trd>
  <trd lng="hu">quadriga</trd>
  <trd lng="it">quadriga</trd>
  <trd lng="ja">&#x30AF;&#x30A1;&#x30C9;&#x30EA;&#x30AC;</trd>
  <trd lng="nl">quadriga</trd>
  <trd lng="pl">kwadryga</trd>
  <trdgrp lng="pt">
    <trd>quadriga</trd>
  </trdgrp>
  <trd lng="ro">cvadrig&abreve;</trd>
  <trd lng="ru">&c_k;&c_v;&c_a;&c_d;&c_r;&c_i;&c_g;&c_a;</trd>
  <trd lng="sk">kv&aacute;drovanie</trd>
  <trd lng="uk">&c_k;&c_v;&c_a;&c_d;&c_r;&c_i;&c_g;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x99DF;&#x99AC; <pr>s&#xEC;m&#x1CE;</pr></trd>,
    <trd>&#x9A77;&#x9A6C; <pr>s&#xEC;m&#x1CE;</pr></trd>
  </trdgrp>
</drv>
</art>
