    <trd>&#x30C9;&#x30C3;&#x30DA;&#x30EB;&#x30B2;&#x30F3;&#x30AC;&#x30FC;</trd>,
    <trd>&#x3046;&#x308A;&#x4E8C;&#x3064;&#x306E;&#x4EBA;  [<ind>&#x3046;&#x308A;&#x3075;&#x305F;&#x3064;&#x306E;&#x3058;&#x3093;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>dubbelganger</trd>,
    <trd>kloon</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_v;&c_o;&c_j;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">dvojn&#xED;k</trd>
