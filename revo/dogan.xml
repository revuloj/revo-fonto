<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dogan.xml,v 1.24 2022/09/03 15:33:07 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>dogan</rad>/o
</kap>

<drv mrk="dogan.0o">
  <kap><tld/>o</kap>
  <snc mrk="dogan.0o.imposto">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="impost.0o">Imposto</ref>, starigita pri iuj
      komerca&jcirc;oj, kiam ili eniras teritorion:
      <ekz>
        &ccirc;iu &scirc;tato protektis sian industrion per altaj <tld/>oj
        kontra&ubreve; importo
        <fnt><aut>Franz Josef Braun</aut>: <vrk>&Ccirc;u kapitalismo
        disfalonta a&ubreve; triomfanta?</vrk>, 
        <bib>Monato</bib><lok>jaro 1998a,
        numero 9a, p. 20a</lok></fnt>;
      </ekz>
      <ekz>
        pro granda korupteco, oni forrabas &ccirc;iujare 800 milionojn da e&ubreve;roj per <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006858.html"
              >Albanio: Diktaturo de impostoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &Gcirc;enerala Konvencio pri Dogano kaj Komerco
        <fnt>
          <bib>Monato</bib>
          <aut>Josef Mendl</aut>
          <vrk><url ref="https://www.monato.be/2006/008959.php"
              >&Ccirc;u militoj savos Usonon?</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        kastelaj sinjoroj, kiuj postulis impostojn kaj <tld/>ojn por si
        mem
        <fnt><aut>Bo Sandelin</aut>: <vrk>Anta&ubreve;merkantilisma
        ekonomia pensado</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 2a, p.
        16a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="impost.dogan0o">doganimposto</ref>,
      <ref cel="impost.lim0o">limimposto</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="akciz.0o">akcizo</ref>
    </refgrp>
    <trd lng="be">&c_m;&c_y;&c_t;&c_a;</trd>
    <trd lng="bg">&c_m;&c_i;&c_t;&c_o;</trd>
    <trd lng="ca">aranzel</trd>
    <trd lng="de">Zoll(geb&uuml;hr)</trd>
    <trd lng="es">arancel</trd>
    <trdgrp lng="fr">
      <trd>droit de <ind>douane</ind></trd>,
      <trd>douane<klr tip="ind"> (droit de douane)</klr></trd>
    </trdgrp>
    <trd lng="hu">v&aacute;m</trd>
    <trdgrp lng="nl">
      <trd>invoerrechten</trd>,
      <trd>tol<klr tip="ind"> (invoerrechten)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_t;&c_a;&c_m;&c_o;&c_zh;&c_je;&c_n;&c_n;&c_a;&c_ja;
&c_p;&c_o;&c_sh;&c_l;&c_i;&c_n;&c_a;</trd>
  </snc>
  <snc mrk="dogan.0o.administracio">
    <dif>
      <ref tip="super" cel="admini1.0o">Administracio</ref> zorganta pri la <tld/>aferoj; <tld/>ejo:
      <ekz>
        Svislando refari&gcirc;is nura ligo de suverenaj &scirc;tatoj, havantaj apartajn landlimojn,
        <tld/>ojn, po&scirc;tojn
        <fnt>
          <bib>FSp</bib>
          <lok>Dua Parto: Svisa Sperto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la trajno venos al la landlimo, kaj la junulinoj timas la <tld/>on
        <fnt>
          <bib>KrB</bib>
          <lok>Virino &ccirc;e landlimo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la <tld/>o nek <nom>Morris</nom> nek <nom>Brown</nom> trovis ian malfacila&jcirc;on
        <fnt>
          <bib>TAA</bib>
          <lok>&Ccirc;apitro III.</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_y;&c_t;&c_n;&c_a;&c_ja;
&c_s;&c_l;&c_u;&c_zh;&c_b;&c_a;</trd>
    <trd lng="bg">&c_m;&c_i;&c_t;&c_n;&c_i;&c_c;&c_a;</trd>
    <trd lng="ca">duana</trd>
    <trd lng="de">Zollbeh&ouml;rde</trd>
    <trd lng="fr">douane<klr tip="ind"> (administration)</klr></trd>
    <trd lng="hu">v&aacute;mhat&oacute;s&aacute;g</trd>
    <trd lng="nl">douane<klr tip="ind"> (dienst)</klr></trd>
  <trdgrp lng="pt">
    <trd>alf&acirc;ndega</trd>,
    <trd>aduana</trd>
  </trdgrp>
    <trd lng="ru">&c_t;&c_a;&c_m;&c_o;&c_zh;&c_je;&c_n;&c_n;&c_a;&c_ja;
&c_s;&c_l;&c_u;&c_zh;&c_b;&c_a;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>celn&iacute; poplatky</trd>,
    <trd>clo</trd>,
    <trd>douane</trd>,
    <trd>m&yacute;tn&eacute;</trd>
  </trdgrp>
  <trd lng="en">customs</trd>
  <trd lng="fa">&f_g;&a_m;&a_r;&f_k;</trd>
  <trdgrp lng="sk">
    <trd>clo</trd>,
    <trd>hrani&ccaron;n&yacute; poplatok</trd>,
    <trd>m&yacute;tne</trd>
  </trdgrp>
  <trd lng="sv">tull</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_i;&c_t;&c_o;</trd>,
    <trd>&c_m;&c_i;&c_t;&c_n;&c_i;&c_c;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="dogan.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      <ref tip="super" cel="ofic.0ejo">Oficejo</ref>, &ccirc;e kiu oni pagas limimposton:
      <ekz>
        ili ne havis ian &gcirc;enon &ccirc;e la <tld/>ejo, tuj oni aran&gcirc;is la detalojn kaj ili
        forveturis de la kajo
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 4</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;oforoj de kamionoj pro la longaj vicoj &ccirc;e la <tld/>ejo volonte venadis man&gcirc;i en
        <klr>[&#8230;]</klr> apudlimaj kafejoj
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="https://www.monato.be/2008/009416.php"
              >Libereco, &gcirc;ojiga ne por &ccirc;iuj</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        informsistemo, kiu devas kunligi la <tld/>ejojn de la
        Balkan-landoj
        <fnt><aut>Evgeni Georgiev</aut>: <vrk>Elektronika
        borso de investoprojektoj</vrk>, <bib>Monato</bib><lok>jaro
        2002a, numero 5a, p. 9a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_y;&c_t;&c_n;&c_ja;</trd>
    <trd lng="bg">&c_m;&c_i;&c_t;&c_n;&c_i;&c_c;&c_a;</trd>
    <trd lng="ca">duana</trd>
    <trd lng="de">Zollamt</trd>
    <trd lng="en">customs office</trd>
    <trd lng="fa">&a_A;&a_d;&a_A;&a_r;&a_h;&a_hamza_sure; <ind>&f_g;&a_m;&a_r;&f_k;</ind></trd>
    <trd lng="fr">bureau de douane</trd>
    <trd lng="hu">v&aacute;mhivatal</trd>
    <trd lng="nl">douanekantoor</trd>
    <trd lng="ru">&c_t;&c_a;&c_m;&c_o;&c_zh;&c_n;&c_ja;</trd>
  </snc>
  <trd lng="cs">m&yacute;to <klr>(m&iacute;sto v&yacute;b&ecaron;ru)</klr></trd>
  <trd lng="he">&bet;&yod;&tav; &he;&mem;&kaf;&samekh;</trd>
  <trd lng="sk">m&yacute;to <klr>(miesto v&yacute;beru)</klr></trd>
  <trd lng="uk">&c_m;&c_i;&c_t;&c_n;&c_i;&c_c;&c_ja;</trd>
</drv>

<drv mrk="dogan.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      <ref tip="super" cel="ofic.0isto">Oficisto</ref> de la <tld/>a administracio:
      <ekz>
        <tld/>istoj <klr>[&#8230;]</klr> en siaj ru&gcirc;aj surtutoj, kun blankaj bastonetoj kaj
        blankaj viza&gcirc;oj saltadis de &scirc;ipo al &scirc;ipo
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>isto asertis, ke mia kompatinda muelilo estas tia
        impostbezona varo
        <fnt><aut>Stefan Maul</aut>: <vrk>Pozitive pri
        EU</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero 8a, p.
        5a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_y;&c_t;&c_n;&c_ib;&c_k;</trd>
  <trd lng="bg">&c_m;&c_i;&c_t;&c_n;&c_i;&c_ch;&c_a;&c_r;</trd>
  <trd lng="ca">duaner</trd>
  <trdgrp lng="cs">
    <trd>celn&iacute; &uacute;&rcaron;edn&iacute;k</trd>,
    <trd>celn&iacute;k</trd>
  </trdgrp>
  <trd lng="de">Zollbeamter</trd>
  <trd lng="en">customs officer</trd>
  <trdgrp lng="fa">
    <trd>&f_g;&a_m;&a_r;&f_k;&f_ch;&f_y;</trd>,
    <trd>&a_m;&a_A_hamza_sure;&a_m;&a_w;&a_r; <ind>&f_g;&a_m;&a_r;&f_k;</ind></trd>
  </trdgrp>
  <trd lng="fr">douanier</trd>
  <trd lng="he">&pe;&qof;&yod;&dalet; &mem;&kaf;&samekh;</trd>
  <trdgrp lng="hu">
    <trd>v&aacute;mos</trd>,
    <trd>v&aacute;mhivatalnok</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>douanebeambte</trd>,
    <trd>douanier</trd>,
    <trd>douaneambtenaar</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_m;&c_o;&c_zh;&c_je;&c_n;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">coln&iacute;k</trd>
  <trd lng="sv">tulltj&auml;nsteman</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_i;&c_t;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_m;&c_i;&c_t;&c_n;&c_i;&c_j; &c_s;&c_l;&c_u;&c_zh;&c_b;&c_o;&c_v;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
