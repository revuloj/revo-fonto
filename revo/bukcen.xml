<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bukcen.xml,v 1.11 2023/08/31 08:48:54 revo Exp $">
<kap>
  <rad>bukcen</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="bukcen.0o">
  <kap><tld/>o</kap>
  <snc mrk="bukcen.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Praa metala <ref tip="lst" cel="instru2.blov0o" lst="voko:blovinstrumentoj">blovinstrumento</ref>, rekta-tuba
      trumpeto
      sen
      klavoj, ordinare jam kun funelforma klo&scirc;o:
      <ekz>
        kornetoj, kornoj, klarionoj, trumpetoj, <tld/>oj
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 30a, p.
        590a</lok></fnt>...
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_k;&c_c;&c_y;&c_n;&c_u;&c_m;</trd>
  <trd lng="es">bocina<klr> (m&uacute;sica)</klr></trd>
  <trd lng="fr">buccin</trd>
  <trd lng="he">&#x5E7;&#x5D5;&#x5E0;&#x5DB;&#x5D9;&#x5EA; &#x5D4;&#x5D7;&#x5E6;&#x5D5;&#x5E6;&#x5E8;&#x5D4;</trd>
  <trd lng="it">buccina</trd>
  <trdgrp lng="ja">
    <trd>&#x30D6;&#x30C3;&#x30AD;&#x30FC;&#x30CA;</trd>,
    <trd>&#x30A8;&#x30BE;&#x30D0;&#x30A4;</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_u;&c_k;&c_c;&c_i;&c_n;&c_u;&c_m;</trd>
  <trd lng="uk">&#x431;&#x443;&#x43A;&#x446;&#x438;&#x43D;&#x430;</trd>
</drv>
</art>
