<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>scintil</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="scintil.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="scintil.0i.KOMUNE">
    <ref tip="dif" cel="bril.trem0i.FIZ">trembrili</ref>
    <ekz>
      la miloj da lumoj de la popolaj kvartaloj <tld/>as &ccirc;e la horizonto
      <fnt>
        <aut>P.-E. Dupret, trad. Vilhelmo Lutermano</aut>,
        <vrk><url ref="http://eo.mondediplo.com/article961.html">Demokratia festo en Venezuelo</url></vrk>,
        <lok>Le Monde diplomatique, 2004-09</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="fr">
    <trd>&eacute;tinceller</trd>,
    <trd>scintiller</trd>
  </trdgrp>
</drv>

<drv mrk="scintil.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="scintil.0o.KOMUNE">
    <dif>
      Trembrilo:
      <ekz>
        &leftquot;neniel,&rightquot; komentis Ja&ccirc;jo, kun <tld/>eto en siaj rondaj okuloj
        <fnt><bib>DOz</bib>, <lok>&leftquot;mirinda eskapo&rightquot;</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>clignotement</trd>,
    <trd>scintillement</trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
