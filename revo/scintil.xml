<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: scintil.xml,v 1.12 2024/03/24 20:00:00 revo Exp $">
<kap>
  <rad>scintil</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="scintil.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="scintil.0i.KOMUNE">
    <dif>
      <ref tip="dif" cel="bril.trem0i.FIZ">Trembrili</ref>:
      <ekz>
        pli belas kiso, kies lum' fajrera &ccirc;e plor' <tld/>as
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="&Monato;/2013/010730.php">Brile versform-fidela traduko &gcirc;uinda</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        la miloj da lumoj de la popolaj kvartaloj <tld/>as &ccirc;e la
        horizonto
        <fnt>
          <aut>P.-E. Dupret, trad. Vilhelmo Lutermano</aut>,
          <vrk><url ref="&Mondediplo;article961.html">Demokratia festo
          en Venezuelo</url></vrk>,
          <lok>Le Monde diplomatique, 2004-09</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">schillern</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ib;&c_g;&c_c;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_ib;&c_g;&c_a;&c_c;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_b;&c_l;&c_ib;&c_s;&c_k;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_ib;&c_s;&c_k;&c_r;&c_y;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bl&yacute;skat se</trd>,
    <trd>jisk&rcaron;it</trd>,
    <trd>t&rcaron;pytit se</trd>
  </trdgrp>
  <trd lng="de">szintillieren</trd>
  <trdgrp lng="en">
    <trd>to glitter</trd>,
    <trd>sparkle</trd>, <trd>glisten</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>centellear</trd>,
    <trd>brillar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;tinceller</trd>,
    <trd>scintiller</trd>
  </trdgrp>
  <trd lng="it">scintillare</trd>
  <trdgrp lng="pl">
    <trd>migota&cacute;</trd>,
    <trd>pe&lstroke;ga&cacute;</trd>,
    <trd>skrzy&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>blika&tcaron;</trd>,
    <trd>scintilova&tcaron; <klr>(o hviezdach)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_c;&c_i;&#x43D;&c_t;&c_i;&#x43B;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_ib;&c_s;&#x43A;&c_r;&c_i;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&c_b;&#x43B;&c_i;&c_s;&#x43A;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43C;&c_je;&c_r;&c_je;&c_h;&c_t;&c_ib;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5FFD;&#x9583; <pr>h&#x16B;sh&#x1CE;n</pr></trd>,
    <trd>&#x5FFD;&#x95EA; <pr>h&#x16B;sh&#x1CE;n</pr></trd>,
    <trd>&#x9583;&#x8000; <pr>sh&#x1CE;ny&#xE0;o</pr></trd>,
    <trd>&#x95EA;&#x8000; <pr>sh&#x1CE;ny&#xE0;o</pr></trd>,
    <trd>&#x70C1; <pr>shu&#xF2;</pr></trd>,
    <trd>&#x720D; <pr>shu&#xF2;</pr></trd>,
    <trd>&#x9583;&#x9583; <pr>sh&#x1CE;nsh&#x1CE;n</pr></trd>,
    <trd>&#x95EA;&#x95EA; <pr>sh&#x1CE;nsh&#x1CE;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="scintil.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="scintil.0o.KOMUNE">
    <dif>
      Trembrilo:
      <ekz>
        &leftquot;neniel,&rightquot; komentis Ja&ccirc;jo, kun <tld/>eto en siaj rondaj
        okuloj
        <fnt><bib>DOz</bib>, <lok>&leftquot;mirinda eskapo&rightquot;</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">AST</uzo>
        la rapide nigri&gcirc;anta &ccirc;ielo ekmontris tie kaj jen siajn unuajn <tld/>ojn stelajn
        <fnt>
          <bib>Monato</bib>
          <aut>Frank Roger</aut>
          <vrk><url ref="&Monato;/2004/008248.html">La brulanta virino</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Schillern</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ib;&c_g;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_m;&c_ib;&c_g;&c_a;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_b;&c_l;&c_ja;&c_s;&c_k;</trd>,
    <trd>&c_s;&c_c;&c_y;&c_n;&c_t;&c_y;&c_l;&c_ja;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_ib;&c_s;&c_k;&c_r;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="cs">scintilace</trd>
  <trd lng="de">Szintillation</trd>
  <trd lng="en">sparkle</trd>
  <trdgrp lng="es">
    <trd>destello</trd>,
    <trd>centelleo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>clignotement</trd>,
    <trd>scintillement</trd>
  </trdgrp>
  <trd lng="it">scintillio</trd>
  <trd lng="sk">scintil&aacute;cia</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_c;&c_i;&#x43D;&c_t;&c_i;&#x43B;&#x44F;&c_c;&c_ib;&#x44F;</trd>,
    <trd>&#x43C;&c_je;&c_r;&c_je;&c_h;&c_t;&c_ib;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_b;&#x43B;&c_i;&c_s;&#x43A;</trd>
  </trdgrp>
</drv>
</art>

