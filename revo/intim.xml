<kap>
  <ofc>*</ofc>
  <rad>intim</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="intim.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="intim.0a.sento">
    <dif>
      Karakterizata per profunde komunaj sentoj kaj plena konfido kaj
      sen&gcirc;eneco:
      <ekz>
        <tld/>a amiko;
      </ekz>
      <ekz>
        vivi kun iu en <tld/>a amikeco
        <fnt><bib>PrV</bib></fnt>
      </ekz>
      <ekz>
        <tld/>aj interrilatoj;
      </ekz>
      <ekz>
        <tld/>a interparolado;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Esperanto estas al li same <tld/>a <klr>(malfremda,
        sen&gcirc;ene uzata)</klr> kiel la gepatra lingvo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="famili1.0a">familiara</ref>,
      <ref cel="hejm.0eca">hejmeca</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>intim</trd>,
      <trd>vertraut</trd>,
      <trd>innig</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>intim</trd>,
      <trd>bens&odblac;s&eacute;ges</trd>,
      <trd>meghitt</trd>
    </trdgrp>
    <trd lng="nl">intiem</trd>
    <trd lng="pt">familiar</trd>
    <trdgrp lng="ru">
      <trd>&c_i;&c_n;&c_t;&c_i;&c_m;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_b;&c_l;&c_i;&c_z;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_z;&c_a;&c_d;&c_u;&c_sh;&c_je;&c_v;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="intim.0a.nepublika">
    <dif>
      Ekzistanta tute interne; ne aperanta ekstere; nepublika:
      <ekz>
        <tld/>aj sentoj, konvinkoj;
      </ekz>
      <ekz>
        la <tld/>a vivo;
      </ekz>
      <ekz>
        <tld/>aj aferoj;
      </ekz>
      <ekz>
        <tld/>a <klr>(senceremonia)</klr> vizito, festo.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>intim</trd>,
      <trd>privat</trd>
    </trdgrp>
    <trd lng="hu">bens&odblac;</trd>
    <trdgrp lng="pt">
     <trd>&iacute;ntimo</trd>, 
     <trd>particular</trd> 
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_i;&c_n;&c_t;&c_i;&c_m;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_l;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_v;&c_a;&c_t;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>d&uring;v&ecaron;rn&yacute;</trd>,
    <trd>d&uring;v&ecaron;rn&yacute;</trd>,
    <trd>intimn&iacute;</trd>,
    <trd>&uacute;tuln&yacute;</trd>
  </trdgrp>
  <trd lng="es">&iacute;ntimo</trd>
  <trdgrp lng="fr">
    <trd>intime<klr tip="ind"> (adj.)</klr></trd>,
    <trd>priv&eacute;<klr tip="ind"> (intime)</klr></trd>,
    <trd>secret<klr tip="ind"> (intime)</klr></trd>,
    <trd>profond<klr tip="ind"> (intime)</klr></trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>akrab</trd>,
    <trd>intim</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x89AA;&#x5BC6;&#x306A;  <pr>&#x3057;&#x3093;&#x307F;&#x3064;&#x306A;</pr></trd>,
    <trd>&#x89AA;&#x3057;&#x3044;  <pr>&#x3061;&#x304B;&#x3057;&#x3044;</pr></trd>,
    <trd>&#x5185;&#x8F2A;&#x306E;  <pr>&#x3046;&#x3061;&#x308F;&#x306E;</pr></trd>,
    <trd>&#x79C1;&#x7684;&#x306A;  <pr>&#x3057;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x5185;&#x306B;&#x79D8;&#x3081;&#x305F;  <pr>&#x306A;&#x3044;&#x306B;&#x3072;&#x3081;&#x305F;</pr></trd>,
    <trd>&#x5FC3;&#x306E;&#x4E2D;&#x306E;  <pr>&#x3053;&#x3053;&#x308D;&#x306E;&#x306A;&#x304B;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>d&ocirc;vern&yacute;</trd>,
    <trd>int&iacute;mny</trd>,
    <trd>&uacute;zky <klr>(vz&tcaron;ah)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x456;&#x43D;&#x442;&#x438;&#x43C;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x431;&#x43B;&#x438;&#x437;&#x44C;&#x43A;&#x438;&#x439;</trd>,
    <trd>&#x437;&#x430;&#x434;&#x443;&#x448;&#x435;&#x432;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x79C1;&#x5BC6; <pr>s&#x12B;m&#xEC;</pr></trd>,
    <trd>&#x5BC6; <pr>m&#xEC;</pr></trd>,
    <trd>&#x5BC6;&#x5207; <pr>m&#xEC;qi&#xE8;</pr></trd>,
    <trd>&#x77E5;&#x5FC3; <pr>zh&#x12B;x&#x12B;n</pr></trd>,
    <trd>&#x4EB2;&#x5BC6; <pr>q&#x12B;nm&#xEC;</pr></trd>,
    <trd>&#x4EB2;&#x8FD1; <pr>q&#x12B;nj&#xEC;n</pr></trd>,
    <trd>&#x8CBC;&#x5FC3; <pr>ti&#x113;x&#x12B;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="intim.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="intim.0eco.KOMUNE">
    <dif>
      Eco de io <tld/>a:
      <ekz>
        vivi en granda <tld/>eco kun iu;
      </ekz>
      <ekz>
        <tld/>ece briletis la teleretoj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>intimita</trd>,
    <trd>intimnost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Intimit&auml;t</trd>,
    <trd>Vertrautheit</trd>,
    <trd>Innigkeit</trd>
  </trdgrp>
  <trd lng="es">intimidad</trd>
  <trd lng="fr">intimit&eacute;</trd>
  <trdgrp lng="hu">
    <trd>intimit&aacute;s</trd>,
    <trd>bens&odblac;s&eacute;gess&eacute;g</trd>,
    <trd>meghitts&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x89AA;&#x5BC6;&#x3055;  <pr>&#x3057;&#x3093;&#x307F;&#x3064;&#x3055;</pr></trd>,
    <trd>&#x89AA;&#x4EA4;  <pr>&#x3057;&#x3093;&#x3053;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">intimiteit</trd>
