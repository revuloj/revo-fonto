<art mrk="$Id: intim.xml,v 1.28 2025/11/17 08:41:12 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>intim</rad>/a <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="intim.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="intim.0a.sento">
    <dif>
      Karakterizata per profunde komunaj sentoj kaj plena konfido kaj
      sen&gcirc;eneco:
      <ekz>
        ni restos en <tld/>a rilato kun ili, <klr>[&#x2026;]</klr> dividos inter ni mal&gcirc;ojon
        kaj &gcirc;ojon
        <fnt>
          <bib>Fab3</bib>
          <lok>Muta libro</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni kune <tld/>e paroladis
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 55:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi estas &jcirc;aluza <klr>[&#x2026;]</klr>, mi do petas vin, ne permesu al li <tld/>a&jcirc;ojn
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi deziris havi amikinon, sed <tld/>an, koran, kun kiu mi povus vivi komunan
        vivon
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Esperanto estas al li same <tld/>a <klr>(malfremda,
        sen&gcirc;ene uzata)</klr> kiel la gepatra lingvo;
      </ekz>
      <ekz>
        vivi kun iu en <tld/>a amikeco
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="famili1.0a">familiara</ref>,
      <ref cel="hejm.0eca">hejmeca</ref>,
      <ref cel="konfid.0a">konfida</ref>,
      <ref cel="proksi.0a.FIG">proksima<sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>intim</trd>,
      <trd>vertraut</trd>,
      <trd>innig</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>intim</trd>,
      <trd>bens&odblac;s&eacute;ges</trd>,
      <trd>meghitt</trd>
    </trdgrp>
    <trd lng="nl">intiem</trd>
    <trd lng="pt">familiar</trd>
    <trdgrp lng="ru">
      <trd>&c_i;&c_n;&c_t;&c_i;&c_m;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_b;&c_l;&c_i;&c_z;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_z;&c_a;&c_d;&c_u;&c_sh;&c_je;&c_v;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="intim.0a.nepublika">
    <dif>
      Ekzistanta tute interne; ne aperanta ekstere; nepublika:
      <ekz>
        la kvar iom babiletis <tld/>vo&ccirc;e kaj duonflustre
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili tre multe parolis pri la intimaj korpopartoj de viroj kaj virinoj
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        per tiu boranta rigardo la juna virino sin sentis tiel <klr>[&#x2026;]</klr> profunde
        fosita &gcirc;is la plej <tld/>aj fibroj de sia esta&jcirc;o, ke tremis tuje &ccirc;iuj &scirc;iaj
        membroj
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili devigis min montri miajn plej-<tld/>a&jcirc;ojn, kaj ili mezuris
        <fnt>
          <bib>ChM</bib>
          <lok>11</lok>
        </fnt>;
      </ekz>
      <ekz>
        tion li faris <klr>[&#x2026;]</klr> pro <tld/>a konvinko
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        el la propra <tld/>o &scirc;i lasas aperi siajn pasiojn kaj turmentojn, tiel nudigante
        anta&ubreve; la leganto sian animon
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konfid.0a">konfida</ref>,
      <ref cel="privat.0a">privata</ref>,
      <ref cel="sekret.0a">sekreta</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>intim</trd>,
      <trd>privat</trd>
    </trdgrp>
    <trd lng="hu">bens&odblac;</trd>
    <trdgrp lng="pt">
     <trd>&iacute;ntimo</trd>,
     <trd>particular</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_i;&c_n;&c_t;&c_i;&c_m;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_l;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_v;&c_a;&c_t;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>d&uring;v&ecaron;rn&yacute;</trd>,
    <trd>d&uring;v&ecaron;rn&yacute;</trd>,
    <trd>intimn&iacute;</trd>,
    <trd>&uacute;tuln&yacute;</trd>
  </trdgrp>
  <trd lng="es">&iacute;ntimo</trd>
  <trdgrp lng="fr">
    <trd>intime<klr tip="ind"> (adj.)</klr></trd>,
    <trd>priv&eacute;<klr tip="ind"> (intime)</klr></trd>,
    <trd>secret<klr tip="ind"> (intime)</klr></trd>,
    <trd>profond<klr tip="ind"> (intime)</klr></trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>akrab</trd>,
    <trd>intim</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x89AA;&#x5BC6;&#x306A;  <pr>&#x3057;&#x3093;&#x307F;&#x3064;&#x306A;</pr></trd>,
    <trd>&#x89AA;&#x3057;&#x3044;  <pr>&#x3061;&#x304B;&#x3057;&#x3044;</pr></trd>,
    <trd>&#x5185;&#x8F2A;&#x306E;  <pr>&#x3046;&#x3061;&#x308F;&#x306E;</pr></trd>,
    <trd>&#x79C1;&#x7684;&#x306A;  <pr>&#x3057;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x5185;&#x306B;&#x79D8;&#x3081;&#x305F;  <pr>&#x306A;&#x3044;&#x306B;&#x3072;&#x3081;&#x305F;</pr></trd>,
    <trd>&#x5FC3;&#x306E;&#x4E2D;&#x306E;  <pr>&#x3053;&#x3053;&#x308D;&#x306E;&#x306A;&#x304B;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>d&ocirc;vern&yacute;</trd>,
    <trd>int&iacute;mny</trd>,
    <trd>&uacute;zky <klr>(vz&tcaron;ah)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ib;&c_n;&c_t;&c_i;&c_m;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_l;&c_i;&c_z;&c_mol;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_u;&c_sh;&c_je;&c_v;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x79C1;&#x5BC6; <pr>s&imacron;m&igrave;</pr></trd>,
    <trd>&#x5BC6; <pr>m&igrave;</pr></trd>,
    <trd>&#x5BC6;&#x5207; <pr>m&igrave;qi&egrave;</pr></trd>,
    <trd>&#x77E5;&#x5FC3; <pr>zh&imacron;x&imacron;n</pr></trd>,
    <trd>&#x4EB2;&#x5BC6; <pr>q&imacron;nm&igrave;</pr></trd>,
    <trd>&#x4EB2;&#x8FD1; <pr>q&imacron;nj&igrave;n</pr></trd>,
    <trd>&#x8CBC;&#x5FC3; <pr>ti&emacron;x&imacron;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="intim.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="intim.0eco.KOMUNE">
    <dif>
      Eco de io <tld/>a:
      <ekz>
        &scirc;i pensis pri la viza&gcirc;o de sia fratino, kaj &scirc;ia okulo kun ama <tld/>eco firme
        sin tenis sur &gcirc;i
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i kredis, ke la <tld/>eco de la geedza vivo malligos la langon de Petro
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Oka</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>eco de tiu fripono al vi povas alkonduki grandan malfeli&ccirc;on
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ece briletis la teleretoj
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>intimita</trd>,
    <trd>intimnost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Intimit&auml;t</trd>,
    <trd>Vertrautheit</trd>,
    <trd>Innigkeit</trd>
  </trdgrp>
  <trd lng="es">intimidad</trd>
  <trd lng="fr">intimit&eacute;</trd>
  <trdgrp lng="hu">
    <trd>intimit&aacute;s</trd>,
    <trd>bens&odblac;s&eacute;gess&eacute;g</trd>,
    <trd>meghitts&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x89AA;&#x5BC6;&#x3055;  <pr>&#x3057;&#x3093;&#x307F;&#x3064;&#x3055;</pr></trd>,
    <trd>&#x89AA;&#x4EA4;  <pr>&#x3057;&#x3093;&#x3053;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">intimiteit</trd>
  <trd lng="pt">intimidade</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_t;&c_i;&c_m;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_l;&c_i;&c_z;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_u;&c_sh;&c_je;&c_v;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_je;&c_r;&c_d;&c_je;&c_ch;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">d&ocirc;vernos&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_ib;&c_n;&c_t;&c_i;&c_m;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_l;&c_i;&c_z;&c_mol;&c_k;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_u;&c_sh;&c_je;&c_v;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_je;&c_r;&c_d;&c_je;&c_ch;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x79C1;&#x751F;&#x6D3B; <pr>s&imacron;sh&emacron;nghu&oacute;</pr></trd>,
    <trd>&#x4EA4;&#x60C5; <pr>ji&amacron;oq&iacute;ng</pr></trd>,
    <trd>&#x4EB2;&#x6635; <pr>q&imacron;nn&igrave;</pr></trd>,
    <trd>&#x79C1;&#x901A; <pr>s&imacron;t&omacron;ng</pr></trd>,
    <trd>&#x4EB2;&#x5207;&#x611F; <pr>q&imacron;nqi&egrave;g&#x1CE;n</pr></trd>,
    <trd>&#x6027;&#x884C;&#x4E3A; <pr>x&igrave;ngx&iacute;ngw&eacute;i</pr></trd>,
    <trd>&#x79C1;&#x4E8B; <pr>s&imacron;sh&igrave;</pr></trd>,
    <trd>&#x4EB2;&#x6635;&#x884C;&#x4E3A; <pr>q&imacron;nn&igrave;x&iacute;ngw&eacute;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="intim.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="intim.0igxi.PSI">
    <dif>
      I&gcirc;i <tld/>a, konfidenca kun iu a&ubreve; io:
      <ekz>
        post iom da trinkado kaj man&gcirc;ado ili pli <tld/>i&gcirc;is
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        malgra&ubreve; ke multaj admiris kaj respektis &scirc;in,
        malmultaj homoj iam vere <tld/>i&gcirc;is kun &scirc;i
        <fnt>
          <aut>W. Heller, trad. B. Westerhoff</aut>
          <vrk>Lidia, la Vivo de Lidia Zamenhof, Filino de Esperanto</vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        iliaj reciprokaj rilatoj ne <tld/>i&gcirc;is plie, kaj neniam preteriris simplan
        inter&scirc;an&gcirc;on de mondumaj &gcirc;entila&jcirc;oj
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>miteinander <ind>vertraut</ind> werden</trd>,
    <trd>sich <ind>n&auml;her</ind> kommen</trd>
  </trdgrp>
  <trd lng="fr">devenir intime</trd>
</drv>

<drv mrk="intim.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="intim.0ismo.BELA">
    <uzo tip="fak">BELA</uzo>
    <uzo tip="fak">BELE</uzo>
    <dif>
      Prefero por temoj kaj esprimoj <tld/>aj,
      kor-proksimaj,
      mal<ref tip="ant" cel="patos.0o.BELA">patosaj</ref>:
      <ekz>
        <nom>Kalocsay</nom>, en sep cikloj, poetas kelkfoje pure lirikan, <tld/>isman poezion
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        lirikaj esprimoj <tld/>ismaj <klr>[&#x2026;]</klr> fluas serene kaj
        simple kiel klara akvo el fonto
        <fnt>
          <aut>A. M. Botella</aut>,
          <vrk><url ref="https://sites.google.com/site/zaragozafrateco/antonio-marco/beletraj-pagoj/antonio-machado">
          La poeto de la intimismaj kolokvoj</url></vrk>,
          <lok>2000</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">intimisme</trd>
  </snc>
<trd lng="de">Intimismus</trd>
</drv>

<drv mrk="intim.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="intim.0ulo.KOMUNE">
    <dif>
      Persono, alligata al alia per reciprokaj sentoj kaj plena
      <ref cel="konfid.0o">konfido</ref>:
      <ekz>
        estu singarda rilate la elekton de viaj <tld/>uloj
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu sola eta detalo, kiun povis konstati nur la <tld/>uloj
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        abomenas min &ccirc;iuj miaj <tld/>uloj
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 19:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        amiko &dash; <tld/>ulo
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konfid1">konfidenculo</ref>
    </refgrp>
  </snc>
  <trd lng="cs">d&uring;v&ecaron;rn&iacute;k</trd>
  <trdgrp lng="de">
    <trd>Intimus</trd>,
    <trd>Vertraute</trd>,
    <trd>Vertrauter</trd>
  </trdgrp>
  <trd lng="es">amigo <ind>&iacute;ntimo</ind></trd>
  <trdgrp lng="fr">
    <trd>intime<klr tip="ind"> (subst.)</klr></trd>,
    <trd>proche<klr tip="ind"> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>meghitt bar&aacute;t</trd>,
    <trd>bizalmas<klr tip="amb">(a vkinek)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x89AA;&#x3057;&#x3044;&#x4EBA;  <pr>&#x3057;&#x305F;&#x3057;&#x3044;&#x3058;&#x3093;</pr></trd>,
    <trd>&#x89AA;&#x53CB;  <pr>&#x3057;&#x3093;&#x3086;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">intieme vriend</trd>
  <trd lng="pt">pessoa <ind>&iacute;ntima</ind></trd>
  <trd lng="sk">d&ocirc;vern&iacute;k</trd>
  <trd lng="uk">&c_z;&c_a;&c_d;&c_u;&c_sh;&c_je;&c_v;&c_n;&c_i;&c_j; &c_d;&c_r;&c_u;&c_g;/&c_p;&c_r;&c_i;&c_ja;&c_t;&c_je;&c_l;&c_mol;</trd>
</drv>

<drv mrk="intim.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="intim.mal0a.KOMUNE">
    <dif>
      Ekstere videbla, publika,
      <ref tip="vid" cel="fremd.0a.aliula">fremda<sncref/></ref>:
      <ekz>
        li &scirc;ajne akceptis kiel naturan la mal<tld/>an vivmanieron al li truditan de la
        grafino
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekkvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;ambro &scirc;ajnis vasta, kruda kaj mal<tld/>a
        <fnt>
          <aut>Hasegawa T.</aut>,
          <vrk><url ref="http://wikilivres.ca/wiki/En_%C4%88inio_Batalanta">
          En &Ccirc;inio Batalanta</url></vrk>,
          <lok>1954</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>unpers&ouml;nlich</trd>,
    <trd>fremd</trd>,
    <trd>k&uuml;hl <klr>(Beziehung)</klr></trd>
  </trdgrp>
  <trd lng="fr">impersonnel</trd>
  <trd lng="id">impersonal</trd>
</drv>
</art>
