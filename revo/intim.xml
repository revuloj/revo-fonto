<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>intim</rad>/a
</kap>
<drv mrk="intim.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Karakterizata per profunde komunaj sentoj kaj plena konfido kaj
      sen&gcirc;eneco:
      <ekz>
        <tld/>a amiko;
      </ekz>
      <ekz>
        vivi kun iu en <tld/>a amikeco
        <fnt><bib>PrV</bib></fnt>
      </ekz>
      <ekz>
        <tld/>aj interrilatoj;
      </ekz>
      <ekz>
        <tld/>a interparolado;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Esperanto estas al li same <tld/>a <klr>(malfremda,
        sen&gcirc;ene uzata)</klr> kiel la gepatra lingvo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="famili1.0a">familiara</ref>,
      <ref cel="hejm.0eca">hejmeca</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>intim</trd>,
      <trd>vertraut</trd>,
      <trd>innig</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>intim</trd>,
      <trd>bens&odblac;s&eacute;ges</trd>,
      <trd>meghitt</trd>
    </trdgrp>
    <trd lng="nl">intiem</trd>
    <trd lng="pt">familiar</trd>
    <trdgrp lng="ru">
      <trd>&c_i;&c_n;&c_t;&c_i;&c_m;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_b;&c_l;&c_i;&c_z;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_z;&c_a;&c_d;&c_u;&c_sh;&c_je;&c_v;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Ekzistanta tute interne; ne aperanta ekstere; nepublika:
      <ekz>
        <tld/>aj sentoj, konvinkoj;
      </ekz>
      <ekz>
        la <tld/>a vivo;
      </ekz>
      <ekz>
        <tld/>aj aferoj;
      </ekz>
      <ekz>
        <tld/>a <klr>(senceremonia)</klr> vizito, festo.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>intim</trd>,
      <trd>privat</trd>
    </trdgrp>
    <trd lng="hu">bens&odblac;</trd>
    <trdgrp lng="pt">
     <trd>&iacute;ntimo</trd>, 
     <trd>particular</trd> 
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_i;&c_n;&c_t;&c_i;&c_m;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_l;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_v;&c_a;&c_t;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="es">&iacute;ntimo</trd>
  <trdgrp lng="fr">
    <trd>intime<klr tip="ind"> (adj.)</klr></trd>,
    <trd>priv&eacute;<klr tip="ind"> (intime)</klr></trd>,
    <trd>secret<klr tip="ind"> (intime)</klr></trd>,
    <trd>profond<klr tip="ind"> (intime)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="intim.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Eco de io <tld/>a:
      <ekz>
        vivi en granda <tld/>eco kun iu;
      </ekz>
      <ekz>
        <tld/>ece briletis la teleretoj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Intimit&auml;t</trd>,
    <trd>Vertrautheit</trd>,
    <trd>Innigkeit</trd>
  </trdgrp>
  <trd lng="es">intimidad</trd>
  <trd lng="fr">intimit&eacute;</trd>
  <trdgrp lng="hu">
    <trd>intimit&aacute;s</trd>,
    <trd>bens&odblac;s&eacute;gess&eacute;g</trd>,
    <trd>meghitts&eacute;g</trd>
  </trdgrp>
  <trd lng="nl">intimiteit</trd>
  <trd lng="pt">intimidade</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_t;&c_i;&c_m;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_l;&c_i;&c_z;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_u;&c_sh;&c_je;&c_v;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_je;&c_r;&c_d;&c_je;&c_ch;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="intim.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Persono, alligata al alia per reciprokaj sentoj kaj plena
      <ref cel="konfid.0o">konfido</ref>.
    </dif>
    <refgrp tip="vid">
      <ref cel="konfid1">konfidenculo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Intimus</trd>,
    <trd>Vertraute</trd>,
    <trd>Vertrauter</trd>
  </trdgrp>
  <trd lng="es">amigo <ind>&iacute;ntimo</ind></trd>
  <trdgrp lng="fr">
    <trd>intime<klr tip="ind"> (subst.)</klr></trd>,
    <trd>proche<klr tip="ind"> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>meghitt bar&aacute;t</trd>,
    <trd>bizalmas<klr tip="amb">(a vkinek)</klr></trd>
  </trdgrp>
  <trd lng="nl">intieme vriend</trd>
  <trd lng="pt">pessoa <ind>&iacute;ntima</ind></trd>
</drv>
</art>
