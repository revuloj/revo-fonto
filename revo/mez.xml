<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mez</rad>/o
</kap>
<drv mrk="mez.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="mez.0o.punkto">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="mez.0o.punkto.elementa">
      <uzo tip="klr">(elementa geometrio)</uzo>
      <fnt><bib>MatVort</bib></fnt>
      <dif>
        <klr>(de <ref cel="strek.0o.elementa">streko</ref>)</klr>
        <ref tip="super" cel="punkt.0o.spaco">Punkto</ref> egaldistanca
        de &gcirc;iaj du <ref cel="rand.0o.figuro">randoj</ref>:
        <ekz>
          la du strekoj, kiuj ligas la <tld/>on al la randoj de la
          originala streko, estas
          <ref cel="egal.0a.izometria">egalaj</ref>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="mez.0o.punkto.afina">
      <uzo tip="klr">(afina geometrio)</uzo>
      <dif>
        <klr>(de <ref cel="par.punkto0o">punktoparo</ref> en
        <ref cel="spac.afina0o">afina spaco</ref>)</klr>
        La <ref tip="super" cel="centr.pezo0o.MAT">pezocentro</ref> kun
        egalaj koeficientoj de la du punktoj.
      </dif>
    </subsnc>
    <trd lng="pl">&sacute;rodek</trd>
  </snc>
  <snc>
    <dif>
      Punkto egaldistanca de du ekstremoj, de du limoj, en spaco a&ubreve;
      tempo:
      <ekz>
        <tld/>o de bastono, strato, rivero, urbo;
      </ekz>
      <ekz>
        <tld/>o de la nokto, jaro, vivo;
      </ekz>
      <ekz>
        <tld/>o de voja&gcirc;o, romano, kanto.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="centr.0o">centro</ref>,
      <ref cel="kern.0o">kerno</ref>,
      <ref cel="sin.0o">sino</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Tio, kio estas egale malproksima de kontra&ubreve;aj ekstremaj gradoj:
      <ekz>
        vero kaj virto ku&scirc;as en <tld/>o;
      </ekz>
      <ekz>
        ser&ccirc;i la &gcirc;ustan <tld/>on.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">st&rcaron;ed</trd>
  <trd lng="de">Mitte</trd>
  <trd lng="en">middle</trd>
  <trd lng="fr">milieu</trd>
  <trd lng="nl">midden</trd>
  <trd lng="ru">&c_s;&c_je;&c_r;&c_je;&c_d;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">stred</trd>
  <trd lng="zh">&#20013;</trd>
</drv>

<drv mrk="mez.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Estanta en la <tld/>o:
      <ekz>
        <tld/>a parto;
      </ekz>
      <ekz>
        <tld/>a pezo, kresko, lernejo;
      </ekz>
      <ekz>
        <tld/>kvalita lano.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="inter">intera</ref>,
      <ref cel="ir.trans0a">transira</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="pont.0o">ponto</ref>.
    </refgrp>
  </snc>
  <trd lng="fr">moyen</trd>
  <trdgrp lng="nl">
    <trd>midden-</trd>,
    <trd>middelbaar</trd>,
    <trd>gemiddeld</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_r;&c_je;&c_d;&c_n;&c_i;&c_j;</trd>
  <trd lng="sk">prostredn&yacute;</trd>
</drv>

<drv mrk="mez.0e">
  <kap><tld/>e de, <tld/>e en, <tld/>e inter, en la <tld/>o de</kap>
  <snc>
    <dif>
      En loko a&ubreve; momento egaldistanca de ekstrema&jcirc;oj:
      <ekz>
        <tld/>e de la nokto.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_s;&c_je;&c_r;&c_je;&c_d;&c_i;&c_n;&c_je;</trd>,
      <trd>&c_n;&c_a; &c_s;&c_je;&c_r;&c_je;&c_d;&c_i;&c_n;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      En loko, kie oni estas &ccirc;iuflanke &ccirc;irka&ubreve;ata:
      <ekz>
        <tld/>e de la nebulo;
      </ekz>
      <ekz>
        <tld/>e en la akvo;
      </ekz>
      <ekz>
        <tld/>en en la bukedon oni enmetadis kandelon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la Eternulo parolis al ili el <tld/>e de la fajro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li faligis &ccirc;ion <tld/>en de ilia tendaro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ne metu ilian nombron <tld/>en de la Izraelidoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        halti <tld/>e dum la danco;
      </ekz>
      <ekz>
        <tld/>e inter la malamikoj.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_s;&c_r;&c_je;&c_d;&c_i;</trd>,
      <trd>&c_s;&c_r;&c_je;&c_d;&c_i;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>au <ind>milieu</ind> de</trd>,
    <trd>parmi</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>te <ind>midden</ind> van</trd>,
    <trd>middenin</trd>
  </trdgrp>
  <trd lng="sk">v <ind>strede</ind></trd>
</drv>

<drv mrk="mez.antauxtag0o">
  <kap>anta&ubreve;tag<tld/>o</kap>
  <uzo tip="fak">KAL</uzo>
  <snc>
    <dif>
      Unua duono de la hela tago.
    </dif>
    <refgrp tip="malprt">
      <ref cel="diurn.0o">diurno</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>matin&eacute;e</trd>
  </trdgrp>
</drv>

<drv mrk="mez.nokto0o">
  <kap>nokto<tld/>o</kap>
  <uzo tip="fak">KAL</uzo>
  <snc>
    <dif>
      Mezo de la nokto.
    </dif>
    <refgrp tip="malprt">
      <ref cel="diurn.0o">diurno</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>minuit</trd>
  </trdgrp>
</drv>

<drv mrk="mez.posttag0o">
  <kap>posttag<tld/>o</kap>
  <uzo tip="fak">KAL</uzo>
  <snc>
    <dif>
      Dua duono de la hela tago.
    </dif>
    <refgrp tip="malprt">
      <ref cel="diurn.0o">diurno</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>apr&egrave;s-midi</trd>
  </trdgrp>
</drv>

<drv mrk="mez.tag0o">
  <kap>tag<tld/>o</kap>
  <uzo tip="fak">KAL</uzo>
  <snc>
    <dif>
      Mezo de la hela tago.
    </dif>
    <refgrp tip="malprt">
      <ref cel="diurn.0o">diurno</ref>
    </refgrp>
  </snc>
  <trd lng="de">Mittag</trd>
  <trdgrp lng="fr">
    <trd>midi</trd>
  </trdgrp>
  <trd lng="vo">zendel</trd>
</drv>

<drv mrk="mez.0epoko">
  <kap><tld/>epoko</kap>
  <snc>
    <dif>
      Epoko inter la antikva kaj la moderna historio, de post falo de la
      okcidenta romana imperio (475) &gcirc;is venka eniro de Turkoj en
      Konstantinoplon (1453).
    </dif>
  </snc>
  <trd lng="de">Mittelalter</trd>
  <trd lng="fr">moyen-&acirc;ge</trd>
  <trd lng="nl">Middeleeuwen</trd>
  <trd lng="ru">&c_s;&c_r;&c_je;&c_d;&c_n;&c_je;&c_v;&c_je;&c_k;&c_o;&c_v;&c_mol;&c_je;</trd>
  <trd lng="sk">stredovek</trd>
</drv>
</art>
