<drv mrk="mez.00a">
  <kap><tld/><tld/>a</kap>
  <snc mrk="mez.00a.KOMUNE">
    <dif>
      En la ekzakta <tld/>o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li estis trafita <tld/><tld/>e per la riposto
        <fnt>
          <aut>M. J&oacute;kai, trad. J. Horvath</aut>
          <vrk>La du salikoj de Nagyenyed</vrk>
          <lok>2002</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_k;&c_l;&c_a;&c_d;&c_n;&c_a; &c_w; <ind>&c_s;&c_ja;&c_r;&c_e;&c_d;&c_z;&c_ib;&c_n;&c_je;</ind></trd>
  <trd lng="fr">en plein milieu</trd>
</drv>

<drv mrk="mez.0umi">
  <kap>
    <tld/>umi
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="mez.0umi.kalkuli">
    <dif>
      Kalkuli meznombron:
      <ekz>
        ni <tld/>umis la&ubreve; du manieroj: la ununan fojon ni utiligis la procentajn
        ciferojn por avera&gcirc;ado, la duan fojon la absolutajn nombrojn
        <fnt>
          <aut>H. D. Maas</aut>
          <vrk><url ref="https://scienca-revuo.info/files/170-1-308-1-10-20140426.pdf">Semantiko kaj
          Statistiko de Rusaj Substantivoj</url></vrk>
          <lok>Scienca Revuo, 1970:4-5 (84-85), p. 133a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>  
  <trd lng="be">&c_v;&c_y;&c_l;&c_ib;&c_ch;&c_a;&c_c;&c_mol; <ind>&c_s;&c_ja;&c_r;&c_e;&c_d;&c_n;&c_ja;&c_je;</ind> <klr>(&c_p;&c_a;&c_k;&c_a;&c_z;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;, &c_l;&c_ib;&c_k;, &c_v;&c_je;&c_l;&c_ib;&c_ch;&c_y;&c_n;&c_ju;)</klr></trd>
  <trd lng="fr">moyenner<klr> (calculer une moyenne)</klr></trd>
  <trd lng="uk">&c_v;&c_i;&c_z;&#x43D;&c_a;&c_ch;&c_a;&c_t;&c_i; &c_s;&c_je;&c_r;&c_je;&c_d;&#x43D;&c_jeu; &c_ch;&c_i;&c_s;&#x43B;&#x43E;</trd>
</drv>

<drv mrk="mez.0umo">
  <kap>
    <tld/>umo
  </kap>
  <snc mrk="mez.0umo.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      <ref tip="dif" cel="nombr.mez0o.MAT">Meznombro</ref>:
      <ekz>
        en la jaro 2000 la <tld/>uma salajro en Pollando atingis 480 usonajn dolarojn
        <fnt>
          <aut>S. &Sacute;migielski</aut>
          <vrk>Laboro tro multekosta</vrk>
          <lok>Monato, 2002:08, p. 14a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_ja;&c_r;&c_e;&c_d;&c_n;&c_ib; &c_l;&c_ib;&c_k;</trd>,
    <trd>&c_s;&c_ja;&c_r;&c_e;&c_d;&c_n;&c_ja;&c_je;</trd>,
    <trd>&c_s;&c_ja;&c_r;&c_e;&c_d;&c_n;&c_ja;&c_ja; &c_v;&c_je;&c_l;&c_ib;&c_ch;&c_y;&c_n;&c_ja;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_r;&c_je;&c_d;&c_n;&c_a; &c_s;&c_t;&c_a;&c_j;&c_n;&c_o;&c_s;&c_t;</trd>
  <trd lng="br">keidenn</trd>
  <trdgrp lng="cs">
    <trd>pr&uring;m&ecaron;r</trd>,
    <trd>st&rcaron;edn&iacute; hodnota</trd>
  </trdgrp>
  <trd lng="de">Mittel</trd>
  <trdgrp lng="en">
    <trd>average</trd>,
    <trd>mean</trd>
  </trdgrp>
  <trd lng="fr">moyenne</trd>
  <trdgrp lng="hu">
    <trd>&aacute;tlag</trd>,
    <trd>k&ouml;z&eacute;p&eacute;rt&eacute;k</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&sacute;rednia liczbowa</trd>,
    <trd>warto&sacute;&cacute; &sacute;rednia</trd>,
    <trd>przeci&eogonek;tna</trd>
  </trdgrp>
  <trd lng="pt">m&eacute;dia</trd>
  <trdgrp lng="ro">
    <trd>medie numeric&abreve;</trd>,
    <trd>valoare medie</trd>,
    <trd>medie</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_r;&c_je;&c_d;&c_n;&c_je;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>priemer</trd>,
    <trd>priemern&yacute; po&ccaron;et</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_je;&c_r;&c_je;&c_d;&#x43D;&c_jeu; &c_ch;&c_i;&c_s;&#x43B;&#x43E;</trd>
</drv>

<drv mrk="mez.0ede">
  <kap><tld/>e de, <var><kap><tld/>e en</kap></var>, <var><kap><tld/>e
  inter</kap></var>, <var><kap>en la <tld/>o de</kap></var></kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="mez.0ede.inter_du">
    <dif>
      En loko a&ubreve; momento egaldistanca de ekstrema&jcirc;oj:
      <ekz>
        <ind><tld/>e de la nokto</ind>.
        <trd lng="hu">az &eacute;jszaka
        <ind>k&ouml;zep&eacute;n</ind></trd>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_s;&c_ja;&c_r;&c_e;&c_d;&c_z;&c_ib;&c_n;&c_je;</trd>,
      <trd>&c_n;&c_a; &c_s;&c_ja;&c_r;&c_e;&c_d;&c_z;&c_ib;&c_n;&c_je;</trd>
    </trdgrp>
    <trd lng="pl">w &sacute;rodku</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_s;&c_je;&c_r;&c_je;&c_d;&c_i;&c_n;&c_je;</trd>,
      <trd>&c_n;&c_a; &c_s;&c_je;&c_r;&c_je;&c_d;&c_i;&c_n;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>mitten in</trd>
    </trdgrp>
  </snc>
  <snc mrk="mez.0ede.inter_multaj">
    <dif>
      En loko, kie oni estas &ccirc;iuflanke &ccirc;irka&ubreve;ata:
      <ekz>
        <tld/>e de la nebulo;
      </ekz>
      <ekz>
        <tld/>e en la akvo;
      </ekz>
      <ekz>
        <tld/>en en la bukedon oni enmetadis kandelon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la Eternulo parolis al ili el <tld/>e de la fajro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li faligis &ccirc;ion <tld/>en de ilia tendaro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ne metu ilian nombron <tld/>en de la Izraelidoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        halti <tld/>e dum la danco;
      </ekz>
      <ekz>
        <tld/>e inter la malamikoj.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_s;&c_ja;&c_r;&c_o;&c_d;</trd>,
      <trd>&c_s;&c_ja;&c_r;&c_o;&c_d;</trd>,
      <trd>&c_u; &c_s;&c_ja;&c_r;&c_e;&c_d;&c_z;&c_ib;&c_n;&c_je;</trd>
    </trdgrp>
    <trd lng="pl">w &sacute;rodku</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_s;&c_r;&c_je;&c_d;&c_i;</trd>,
      <trd>&c_s;&c_r;&c_je;&c_d;&c_i;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>mitten in</trd>
    </trdgrp>
  </snc>
  <trd lng="br">e-kreiz</trd>
  <trd lng="ca">al <ind>mig</ind> de</trd>
  <trdgrp lng="en">
  	<trd>amid</trd>,
  	<trd>among</trd>,
    <trd>in the middle of</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>en <ind>medio</ind> de</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>au <ind>milieu</ind> de</trd>,
    <trd>parmi</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&ouml;z&eacute;p&uuml;tt</trd>,
    <trd>k&ouml;z&eacute;pen</trd>
  </trdgrp>
