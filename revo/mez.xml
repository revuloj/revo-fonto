<drv mrk="mez.0a">
  <kap><tld/>a</kap>
  <snc mrk="mez.0a.KOMUNE">
    <dif>
      Estanta en la <tld/>o:
      <ekz>
        <tld/>a parto;
      </ekz>
      <ekz>
        <tld/>a pezo, kresko, lernejo;
      </ekz>
      <ekz>
        <ind><tld/>kvalita</ind> lano.
        <trd lng="hu">&aacute;tlagos
        min&odblac;s&eacute;g&udblac;</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="inter">intera</ref>,
      <ref cel="ir.trans0a">transira</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="pont.0o">ponto</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_ja;&c_r;&c_e;&c_d;&c_n;&c_ib;</trd>
  <trdgrp lng="br">
    <trd>-<ind>kreiz</ind></trd>,
    <trd>krenn</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>prost&rcaron;edn&iacute;</trd>,
    <trd>pr&uring;m&ecaron;rn&yacute;</trd>,
    <trd>st&rcaron;edn&iacute;</trd>
  </trdgrp>
  <trd lng="en">middle</trd>
  <trd lng="es">medio</trd>
  <trdgrp lng="fr">
    <trd>m&eacute;dian</trd>,
    <trd>moyen</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&ouml;z&eacute;p-</trd>,
    <trd>k&ouml;z&eacute;ps&odblac;</trd>,
    <trd>k&ouml;z&eacute;pponti</trd>,
    <trd>k&ouml;zepes</trd>,
    <trd>&aacute;tlagos</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>mediano<klr> (agg.)</klr></trd>,
    <trd>intermedio</trd>,
    <trd>medio</trd>,
    <trd>in <ind>mezzo</ind></trd>,
    <trd>centrale<klr> (in mezzo)</klr></trd>
