    <trd>&icirc;nainte de pr&acirc;nz</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_o;
  <ind>&c_p;&c_o;&c_l;&c_u;&c_d;&c_n;&c_ja;</ind></trd>
