<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
-<rad>ec</rad></kap>
<subart>
  <dif>
    Sufikso esprimanta abstraktan ideon de kvalito a&ubreve; stato:
  </dif>
  <drv mrk="ec.0">
    <kap>-<tld/></kap>
  <snc>
    <ekz>
      mol<tld/>o, blank<tld/>o, bon<tld/>o, bel<tld/>o, cert<tld/>o,
      sol<tld/>o, infan<tld/>o, hom<tld/>o, re&gcirc;<tld/>o;
    </ekz>
    <ekz>
      la alt<tld/>o de tiu alta&jcirc;o ne estas granda;
    </ekz>
    <ekz>
      sian amik<tld/>on li pruvis per multaj amika&jcirc;oj;
    </ekz>
    <ekz>
      penetrem<tld/>o;
    </ekz>
    <ekz>
      senkler<tld/>o;
    </ekz>
    <ekz>
      ventanim<tld/>o<fnt>Z</fnt>;
    </ekz>
    <ekz>
      la re&gcirc;o prenis sur sin la protektant<tld/>on super
      nia kongreso<fnt>Z</fnt>;
    </ekz>
    <ekz>
      silk<tld/>aj <klr>(havantaj la kvalitojn, aspekton, naturon de
      silko)</klr> haroj;
    </ekz>
    <ekz>
      lign<tld/>a trunketo;
    </ekz>
    <ekz>
      vitr<tld/>a rigardo;
    </ekz>
    <ekz>
      dezert<tld/>a silento<fnt>Z</fnt>;
    </ekz>
    <ekz>
      mi donas al vi unu pecon prefer<tld/>e anta&ubreve; viaj
      fratoj
      <fnt><bib>MT</bib>, <lok>&Gen; 48:22</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="de">
     <trd>-<ind>heit</ind></trd>,
     <trd>-<ind>keit</ind></trd>,
     <trd>-<ind>schaft</ind></trd>,
     <trd>-<ind>t&auml;t</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>-<ind>s&aacute;g</ind></trd>,
     <trd>-<ind>s&eacute;g</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>-<ind>ez</ind></trd>,
    <trd>-<ind>eza</ind></trd>,
    <trd>-<ind>d&atilde;o</ind></trd>,
    <trd>-<ind>id&atilde;o</ind></trd>,
    <trd>-<ind>itude</ind></trd>,
    <trd>-<ind>tude</ind></trd>
  </trdgrp>
  </drv>
</subart>
<subart>
  <dif>
    Samsignifa memstara vortero:
  </dif>
  <drv mrk="ec.0o">
    <kap><tld/>o</kap>
    <snc>
      <ekz>peko kaj eraro estas <tld/>oj de l' homaro<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        <tld/>e <klr>(en sia <tld/>o)</klr> kiel prezidanto li akceptis;
      </ekz>
      <ekz>
        <tld/>aro
        <klr>(karaktero, esenco, naturo)</klr>
      </ekz>
      <refgrp tip="vid">
        <ref cel="trajt.0o">trajto</ref>,
        <ref cel="distin.0ilo">distingilo</ref>.
      </refgrp>
    </snc>
    <trdgrp lng="be">
      <trd>&c_ja;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_u;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>caract&egrave;re<klr tip="ind"> (propri&eacute;t&eacute;)</klr></trd>,
      <trd>propri&eacute;t&eacute;</trd>,
      <trd>qualit&eacute;</trd>
    </trdgrp>
    <trd lng="es">cualidad</trd>
    <trd lng="hu">tulajdons&aacute;g</trd>
    <trd lng="nl">eigenschap</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_v;&c_o;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>,
      <trd>&c_k;&c_a;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
    </trdgrp>
  </drv>
  <trdgrp lng="de">
     <trd>Eigenschaft</trd>,
     <trd>Qualit&auml;t</trd>
  </trdgrp>
</subart>
</art>
