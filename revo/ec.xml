<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ec.xml,v 1.29 2017/12/19 15:10:20 revo Exp $">
<kap><ofc>*</ofc>-<rad>ec</rad>/ <fnt><bib>PV</bib></fnt>
</kap>
<subart>
<drv mrk="ec.0">
    <kap>-<tld/></kap>
  <snc>
  <dif>
    Sufikso esprimanta abstraktan ideon de kvalito a&ubreve; stato:
    <ekz>
      mol<tld/>o, blank<tld/>o, bon<tld/>o, bel<tld/>o, cert<tld/>o,
      sol<tld/>o, infan<tld/>o, hom<tld/>o, re&gcirc;<tld/>o;
    </ekz>
    <ekz>
      la alt<tld/>o de tiu alta&jcirc;o ne estas granda;
    </ekz>
    <ekz>
      sian amik<tld/>on li pruvis per multaj amika&jcirc;oj;
    </ekz>
    <ekz>
      penetrem<tld/>o;
    </ekz>
    <ekz>
      senkler<tld/>o;
    </ekz>
    <ekz>
      ventanim<tld/>o<fnt>Z</fnt>;
    </ekz>
    <ekz>
      la re&gcirc;o prenis sur sin la protektant<tld/>on super
      nia kongreso<fnt>Z</fnt>;
    </ekz>
    <ekz>
      silk<tld/>aj <klr>(havantaj la kvalitojn, aspekton, naturon de
      silko)</klr> haroj;
    </ekz>
    <ekz>
      lign<tld/>a trunketo;
    </ekz>
    <ekz>
      vitr<tld/>a rigardo;
    </ekz>
    <ekz>
      dezert<tld/>a silento<fnt>Z</fnt>;
    </ekz>
    <ekz>
      mi donas al vi unu pecon prefer<tld/>e anta&ubreve; viaj
      fratoj
      <fnt><bib>MT</bib>, <lok>&Gen; 48:22</lok></fnt>.
    </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>-<ind>heit</ind></trd>,
     <trd>-<ind>keit</ind></trd>,
     <trd>-<ind>schaft</ind></trd>,
     <trd>-<ind>t&auml;t</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>-esse</trd>,
    <trd>-itude</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>-<ind>s&aacute;g</ind></trd>,
     <trd>-<ind>s&eacute;g</ind></trd>
  </trdgrp>
  <trd lng="pl">-<ind>o&sacute;&cacute;</ind></trd>
  <trdgrp lng="pt">
    <trd>-<ind>ez</ind></trd>,
    <trd>-<ind>eza</ind></trd>,
    <trd>-<ind>d&atilde;o</ind></trd>,
    <trd>-<ind>id&atilde;o</ind></trd>,
    <trd>-<ind>itude</ind></trd>,
    <trd>-<ind>tude</ind><klr> (sufixo que indica qualidade)</klr></trd>
  </trdgrp>
  </drv>
</subart>

<subart>
  <dif>
    Samsignifa memstara vortero:
  </dif>
  <drv mrk="ec.0o">
    <kap><tld/>o</kap>
    <snc mrk="ec.0o.KLA">
      <ekz>peko kaj eraro estas <tld/>oj de l'
      homaro<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        <tld/>e <klr>(en sia <tld/>o)</klr> kiel prezidanto li akceptis;
      </ekz>
      <ekz>
        <tld/>aro
        <klr>(karaktero, esenco, naturo)</klr>
      </ekz>
      <refgrp tip="vid">
        <ref cel="difere.0igilo.KLA">diferencigilo</ref>,
        <ref cel="distin.0ilo">distingilo</ref>,
        <ref cel="karakt1.0o.KLA">karakterizo</ref>,
        <ref cel="trajt.0o">trajto</ref>
      </refgrp>
    </snc>
  <trdgrp lng="be">
      <trd>&c_ja;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_u;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    </trdgrp>
  <trd lng="bg">&c_k;&c_a;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="cs">vlastnost</trd>
  <trdgrp lng="de">
      <trd>Eigenschaft</trd>,
      <trd>Qualit&auml;t</trd>,
      <trd>Beschaffenheit</trd>
    </trdgrp>
  <trdgrp lng="en">
      <trd>property</trd>,
      <trd>quality</trd>
    </trdgrp>
  <trd lng="es">cualidad</trd>
  <trdgrp lng="fr">
      <trd>caract&egrave;re<klr tip="ind">
      (propri&eacute;t&eacute;)</klr></trd>,
      <trd>propri&eacute;t&eacute;</trd>,
      <trd>qualit&eacute;</trd>
    </trdgrp>
  <trd lng="hu">tulajdons&aacute;g</trd>
  <trd lng="it">caratteristica</trd>
  <trd lng="nl">eigenschap</trd>
  <trdgrp lng="pl">
      <trd>cecha</trd>,
      <trd>przymiot</trd>
    </trdgrp>
  <trdgrp lng="ru">
      <trd>&c_s;&c_v;&c_o;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>,
      <trd>&c_k;&c_a;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
    </trdgrp>
  <trd lng="sk">vlastnos&tcaron;</trd>
</drv>

<drv mrk="ec.0a">
  <kap><ofc>*</ofc><tld/>a<fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url
  ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5011">ec'a</url></lok></fnt></kap>
   <snc>
    <dif>Kvalita </dif>
   </snc>
  <trd lng="fr">qualitatif</trd>  
</drv>
</subart>
</art>
