<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>komod</rad>/o
</kap>
<drv mrk="komod.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Meblo, atinganta alton de la brusto, kaj konsistanta el tirkestoj,
      metitaj unu super alia, por konservi tola&jcirc;on, galanteriojn kc.  
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_o;&c_d;&c_a;</trd>
  <trdgrp lng="de">
     <trd>Kommode</trd>,
     <trd>Schubladenkasten</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>commode</trd>,
    <trd>chest of drawers</trd>
  </trdgrp>
  <trd lng="fr">commode<klr tip="ind"> (meuble)</klr></trd>
  <trd lng="nl">ladenkast</trd>
  <trd lng="pt">c&ocirc;moda</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_o;&c_d;</trd>
</drv>
</art>
