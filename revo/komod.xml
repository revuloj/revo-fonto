<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: komod.xml,v 1.20 2018/05/31 22:10:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>komod</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="komod.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="komod.0o.MEB">
    <dif>
      <ref tip="lst" lst="voko:mebloj" cel="mebl.0o">Meblo</ref>, 
      atinganta alton de la kokso a&ubreve; brusto, kaj konsistanta el
      tirkestoj,
      metitaj unu super alia, por konservi tola&jcirc;on, galanteriojn 
      kaj ceterajn:
      <ekz>
        unu tirkesto de la <tld/>o estis duone el&scirc;ovita
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;parmonujo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la murojn garnis, sur unu flanko, lar&gcirc;a <tld/>o, ornamcele 
        &scirc;ar&gcirc;ita per vazoj kaj statuetoj
        <fnt><bib>KielAkvo</bib>, <lok>1:3</lok></fnt>;
      </ekz>
      <ekz>
        super la <tld/>o pendis granda pentra&jcirc;o en ri&ccirc;e orita kadro
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="komod1.0o">Komodo (insulo)</ref>
  <trd lng="be">&c_k;&c_a;&c_m;&c_o;&c_d;&c_a;</trd>
  <trd lng="bg">&c_s;&c_k;&c_r;&c_i;&c_n;</trd>
  <trd lng="ca">calaixera</trd>
  <trdgrp lng="cs">
    <trd>komoda</trd>,
    <trd>pr&aacute;deln&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kommode</trd>,
    <trd>Schubladenkasten</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>commode<klr tip="amb"> (furniture)</klr></trd>,
    <trd>chest of drawers</trd>
  </trdgrp>
  <trd lng="fr">commode<klr tip="ind"> (meuble)</klr></trd>
  <trdgrp lng="nl">
    <trd>commode</trd>,
    <trd>ladenkast</trd>
  </trdgrp>
  <trd lng="pl">komoda</trd>
  <trd lng="pt">c&ocirc;moda</trd>
  <trd lng="ro">comod&abreve;</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_o;&c_d;</trd>
  <trd lng="sk">bielizn&iacute;k</trd>
</drv>
</art>
