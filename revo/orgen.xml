<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc> <rad>orgen</rad>/o</kap>
<drv mrk="orgen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Granda muzika
      <ref tip="super" cel="instru2.blov0o">blovinstrumento</ref>,
      kun tuboj kaj klavaro, uzata precipe en pre&gcirc;ejoj.
    </dif>
  </snc>
  <trd lng="de">Orgel</trd>
  <trd lng="en">organ</trd>
  <trd lng="nl">orgel</trd>
  <trd lng="ru">&c_o;&c_r;&c_g;&c_a;&c_n; <klr tip="amb">(&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;)</klr></trd>
  <trd lng="sv">orgel</trd>
</drv>
</art>
