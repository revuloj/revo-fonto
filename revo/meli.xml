<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>meli</rad>/acoj
</kap>
<drv mrk="meli.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Meliaceae</trd>), de plej ofte
      tropikaj arboj.
    </dif>
    <ref tip="sub" cel="mahago.0o.arbo">mahagono</ref>
  </snc>
  <trd lng="de">Zedrachgew&auml;chse</trd>
</drv>
</art>
