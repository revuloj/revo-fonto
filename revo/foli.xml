<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id$">
    <kap><ofc>*</ofc>
      <rad>foli</rad>/o</kap>
    <drv mrk="foli.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="foli.0o.BOT">
 <uzo tip="fak">BOT</uzo>
 <dif>
   Diversforma organo de
   <ref tip="malprt" cel="veget.0ajxo">vegeta&jcirc;o</ref>,
   ordinare verda kaj maldika, kiu kreskas sur la tigo kaj servas por
   asimili la karbonacidon de la aero, necesan por la nutrado
   de la vegeta&jcirc;o:
   <ekz>
     <ind><tld/>plato</ind>
     <klr>(la plata parto de la <tld/>o)</klr>;
     <trd lng="de">Blattspreite</trd>
     <trd lng="fr">limbe<klr tip="ind"> (bot.)</klr></trd>
     <trdgrp lng="nl">
       <trd>bladschijf</trd>,
       <trd>limbus</trd>
     </trdgrp>
     <trd lng="hu">lemez<klr tip="amb"> (lev&eacute;len)</klr></trd>
   </ekz><ekz>
     <ind><tld/>tigo</ind>
     <klr>(tigo tenanta la <tld/>platon, petiolo)</klr>;
     <trd lng="de">Blattstiel</trd>
     <trd lng="fr">p&eacute;tiole</trd>
     <trd lng="nl">bladsteel</trd>
     <trd lng="hu">ny&eacute;l<klr tip="amb"> (lev&eacute;len)</klr></trd>
   </ekz><ekz>
     <ind><tld/>ingo, <tld/>bazo</ind>
     <klr>(baza parto de <tld/>o &ccirc;irka&ubreve;anta la tigon)</klr>;
     <trd lng="de">Blattgrund</trd>
     <trd lng="fr">base foliaire</trd>
     <trd lng="hu">alap<klr tip="amb"> (lev&eacute;len)</klr></trd>
   </ekz><ekz>
     elipsa, ovoforma, inverse ovoforma <klr>(obovala)</klr>,
     spatelforma, lancetforma, sagforma, ronda,
     lancoforma, renforma, linia, &scirc;ildforma, korforma,
     pingloforma, pluma <klr>(pinata)</klr>, triloba,
     manforma, piedforma <tld/>o;
   </ekz><ekz>
     glatranda, harranda, segildenta, denta,
     &scirc;arkna&gcirc;ila, no&ccirc;ita, loba,
     festonranda <tld/>o;
   </ekz><ekz>
     pinta, pintega, pikilpinta, obtuza <tld/>o;
   </ekz><ekz>
     strinerva, fingronerva, plumnerva <tld/>o.
   </ekz>
 </dif>
 <trd lng="de">Blatt</trd>
 <trd lng="nl">blad<klr tip="ind"> (plantk.)</klr></trd>
 <trd lng="hu">lev&eacute;l<klr tip="amb"> (fa~)</klr></trd>
      </snc>
      <snc mrk="foli.0o.TEK">
 <dif>
   Maldika, plata objekto:
   <ekz>
     kupra, ora, stana <tld/>o;
   </ekz><ekz>
     paper<tld/>o;
   </ekz><ekz>
     <tld/>oj de libro.
   </ekz>
 </dif>
 <refgrp tip="vid">
   <ref cel="plat.0o">plato</ref>,
   <ref cel="plak.0o">plako</ref>.
 </refgrp>
 <trdgrp lng="de">
   <trd>Blatt <klr>(papero)</klr></trd>,
   <trd>Folie <klr>(metalo, plasto)</klr></trd>
 </trdgrp>
 <trdgrp lng="nl">
   <trd>blad<klr tip="ind"> (papier)</klr></trd>,
   <trd>folie</trd>
 </trdgrp>
 <trdgrp lng="hu">
   <trd>lap<klr>(k&ouml;nyv~)</klr></trd>,
   <trd>f&oacute;lia</trd>
 </trdgrp>
      </snc>
      <trd lng="en">leaf</trd>
      <trd lng="es">hoja</trd>
      <trd lng="fr">feuille</trd>
 <trdgrp lng="pl">
   <trd>li&sacute;&cacute;</trd>,
   <trd>kartka</trd>
 </trdgrp>
      <trd lng="ru">&c_l;&c_i;&c_s;&c_t;</trd>
      <trd lng="tr">yaprak</trd>
    </drv>
    <drv mrk="foli.0i">
      <kap><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
 <dif>
   Naski <tld/>ojn.
 </dif>
      </snc>
      <trd lng="fr">feuiller</trd>
      <trd lng="hu">levelet hajt</trd>
      <trd lng="ru">&c_p;&c_o;&c_k;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja; &c_l;&c_i;&c_s;&c_t;&c_mol;&c_ja;&c_m;&c_i;</trd>
    </drv>
    <drv mrk="foli.0aro">
      <kap><tld/>aro</kap>
      <snc>
 <dif>
   La tuto de la <tld/>oj de vegeta&jcirc;o.
 </dif>
      </snc>
      <trd lng="de">Laub</trd>
      <trd lng="es">follage</trd>
      <trd lng="fr">feuillage</trd>
      <trd lng="hu">lomb(ozat)</trd>
      <trd lng="pl">ulistnienie</trd>
      <trd lng="ru">&c_l;&c_i;&c_s;&c_t;&c_v;&c_a;</trd>
    </drv>
    <drv mrk="foli.0ero">
      <kap><tld/>ero</kap>
      <snc mrk="foli.0ero.BOT">
 <uzo tip="fak">BOT</uzo>
 <dif>
   &Ccirc;iu el la <tld/>isimilaj partoj konsistigantaj kunmetitan
   <ref tip="malprt" cel="foli.0o.BOT"><tld/>on</ref>.
 </dif>
      </snc>
    </drv>
    <drv mrk="foli.0umi">
      <kap><tld/>i<fnt>Z</fnt>, <tld/>umi</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="foli.0umi.libron">
 <dif>
   Transturni la pa&gcirc;ojn de libro por rapide trarigardi
   kaj dise tralegi &gcirc;in.
 </dif>
 <trd lng="de">bl&auml;ttern</trd>
 <trd lng="es">hojear</trd>
 <trd lng="fr">feuilleter<klr tip="ind"> (tourner les pages)</klr></trd>
 <trd lng="hu">lapoz</trd>
 <trd lng="pl">kartkowa&cacute;</trd>
 <trdgrp lng="ru">
   <trd>&c_l;&c_i;&c_s;&c_t;&c_a;&c_t;&c_mol;</trd>,   
   <trd>&c_p;&c_je;&c_r;&c_je;&c_l;&c_i;&c_s;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
 </trdgrp>
      </snc>
      <snc mrk="foli.0umi.KOMP">
 <uzo tip="fak">KOMP</uzo>
 <dif>
   Esplori datumojn (dosierojn, dosierujojn, hipertekstojn) en
   la&ubreve;vola ordo, ne nepre en la rekta ordo.
 </dif>
 <trd lng="hu">b&ouml;ng&eacute;szik<klr tip="ind"> (pl. a
     vil&aacute;gh&aacute;l&oacute;n)</klr></trd>
 <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_m;&c_a;&c_t;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
      </snc>
      <trd lng="en">browse</trd>
      <trd lng="nl">bladeren</trd>
    </drv>
    <drv mrk="foli.0igi">
      <kap><tld/>igi</kap>
      <snc>
 <uzo tip="fak">TEK</uzo>
 <dif>
   Platigi metalon en <tld/>on:
   <ekz>
     <tld/>igita ar&gcirc;ento
     <fnt><bib>MT</bib>, <lok>&Jer; 10:9</lok></fnt>.
   </ekz>
 </dif>
      </snc>
      <trd lng="fr">laminer</trd>
      <trd lng="hu">lamin&aacute;l</trd>
      <trdgrp lng="ru">
 <trd>&c_p;&c_r;&c_o;&c_k;&c_a;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
 <trd>&c_v;&c_a;&c_l;&c_mol;&c_c;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
 <trd>&c_p;&c_l;&c_ju;&c_shch;&c_i;&c_t;&c_mol;</trd>
      </trdgrp>
    </drv>
    <drv mrk="foli.0umilo">
      <kap><tld/>umilo</kap>
      <uzo tip="fak">KOMP</uzo>
      <snc>
 <dif>
   <ref tip="super" cel="leg.0ilo.eliga">Legilo</ref> por
   <ref cel="foli.0umi"><tld/>umi</ref>, precipe
   TTT-legilo de <ref tip="malprt" cel="ret.0umilo">retumilo</ref>
 </dif>
      </snc>
      <trd lng="en">browser</trd>
      <trdgrp lng="es">
 <trd>explorador</trd>,
 <trd>navegador</trd>
      </trdgrp>
      <trdgrp lng="fr">
 <trd>fureteur</trd>,
 <trd>navigateur</trd>
      </trdgrp>
      <trd lng="hu">b&ouml;ng&eacute;sz&odblac;(program)</trd>
      <trdgrp lng="nl">
 <trd>browser</trd>,
 <trd>bladeraar</trd>
      </trdgrp>
      <trdgrp lng="pl">
 <trd>czytnik</trd>,
 <trd>paj&eogonek;czarka</trd>,
 <trd>przegl&aogonek;darka</trd>,
 <trd>broser</trd>
      </trdgrp>
      <trdgrp lng="ru">
 <trd>&c_b;&c_r;&c_a;&c_u;&c_z;&c_je;&c_r;</trd>,
 <trd>&c_p;&c_r;&c_o;&c_s;&c_m;&c_o;&c_t;&c_r;&c_shch;&c_i;&c_k;</trd>,
 <trd>&c_o;&c_b;&c_o;&c_z;&c_r;&c_je;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
      </trdgrp>
    </drv>
    <drv mrk="foli.flug0o">
      <kap>flug<tld/>o</kap>
      <uzo tip="fak">TIP</uzo>
      <snc>
 <dif>
   Unu<tld/>a a&ubreve; malmult<tld/>a presa&jcirc;o, ne bro&scirc;urita,
   ordinare uzata por propagando a&ubreve; reklamo.
 </dif>
      </snc>
      <trd lng="de">Flugblatt</trd>
      <trd lng="fr">prospectus</trd>
      <trdgrp lng="nl">
 <trd>strooibiljet</trd>,
 <trd>pamflet</trd>
      </trdgrp>
      <trdgrp lng="hu">
 <trd>r&ouml;plap</trd>,
 <trd>prospektus</trd>
      </trdgrp>
      <trd lng="pl">ulotka</trd>
      <trd lng="ru">&c_l;&c_i;&c_s;&c_t;&c_o;&c_v;&c_k;&c_a;</trd>
    </drv>
    <drv mrk="foli.tri0o">
      <kap><ofc>*</ofc>tri<tld/>o</kap>
      <uzo tip="fak">BOT</uzo>
      <snc>
 <dif>
   Genro el familio <ref tip="super" cel="fabac.0oj">fabacoj</ref>
   (<trd lng="la">Trifolium</trd>), kun tripartaj <tld/>oj.
 </dif>
      </snc>
      <bld lok="../bld/trifol.jpg">
 <ind>herbeja tri<tld/>o</ind>
 <trd lng="la">Trifolium pratense</trd>
      </bld>
      <trd lng="de">Klee</trd>
      <trd lng="fr">tr&egrave;fle</trd>
      <trd lng="hu">l&oacute;here</trd>
      <trd lng="nl">klaver</trd>
      <trd lng="pl">koniczyna</trd>
      <trdgrp lng="ru">
 <trd>&c_k;&c_l;&c_je;&c_v;&c_je;&c_r;</trd>,
 <trd>&c_t;&c_r;&c_i;&c_l;&c_i;&c_s;&c_t;&c_n;&c_i;&c_k;</trd>
      </trdgrp>
    </drv>
    <drv mrk="foli.kartezia0o">
      <kap>kartezia <tld/>o</kap>
      <uzo tip="fak">MAT</uzo>
      <fnt><bib>MatVort</bib></fnt>
      <snc>
 <dif>
   Speco de triagrada ebena
   <ref tip="super" cel="kurb.0o.MAT">kurbo</ref>:
   <ekz>
     la kartezia ekvacio de kartezia <tld/>o estas de la tipo
     <frm><k>x</k><sup>3</sup>+<k>y</k><sup>3</sup> =
       3<k>axy</k></frm>.
   </ekz>
 </dif>
      </snc>
      <bld tip="svg" lok="../bld/k_folio.svg" alt="300" lrg="300">
 kartezia <tld/>o
      </bld>
      <trd lng="de">Descartessches <ind>Blatt</ind></trd>
      <trd lng="en">Cartesian <ind>folium</ind></trd>
      <trd lng="fr">folium de Descartes</trd>
      <trd lng="pl">li&sacute;&cacute; Kartezjusza</trd>
      <trd lng="ru">&c_d;&c_je;&c_k;&c_a;&c_r;&c_t;&c_o;&c_v; <ind>&c_l;&c_i;&c_s;&c_t;</ind></trd>
    </drv>
  </art>
