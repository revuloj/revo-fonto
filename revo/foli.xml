<drv mrk="foli.tri0o">
  <kap><ofc>*</ofc>tri<tld/>o</kap>
  <snc mrk="foli.tri0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Trifolium</trd>), kun tripartaj <tld/>oj:
      <ekz>
        &ccirc;arma kaj dol&ccirc;a estis la aromo, kiu blovetis de la luksa kampo
        de tri<tld/>oj, kie la abeloj zumadis
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de la jaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        unuj verdis: la lino, tritiko, tri<tld/>o, la aliaj estis oraj
        de la maturi&gcirc;anta hordeo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni rikoltis la tri<tld/>on kaj seneson
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="&WCU;/thumb/9/9d/Trifolium_pratense_-_Keila.jpg/480px-Trifolium_pratense_-_Keila.jpg" 
       prm="CC BY-SA 4.0"><ind>herbeja tri<tld/>o</ind> (Trifolium pratense), Keila, Estonujo
       <fnt>
         <aut>Ivar Leidus, propra verko</aut>
         <lok><url ref="&WCW;/File:Trifolium_pratense_-_Keila.jpg">&WCW;/File:Trifolium_pratense_-_Keila.jpg</url></lok>
       </fnt>
    <trd lng="la">Trifolium pratense</trd>
  </bld>
  <trd lng="bg">&c_d;&c_je;&c_t;&c_je;&c_l;&c_i;&c_n;&c_a;</trd>
  <trd lng="de">Klee</trd>
  <trd lng="es">trebol</trd>
  <trd lng="fr">tr&egrave;fle</trd>
  <trd lng="hu">l&oacute;here</trd>
  <trd lng="nl">klaver</trd>
  <trd lng="pl">koniczyna</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_l;&c_je;&c_v;&c_je;&c_r;</trd>,
    <trd>&c_t;&c_r;&c_i;&c_l;&c_i;&c_s;&c_t;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">trojl&iacute;stok</trd>
  <trdgrp lng="cs">
    <trd>trifolium</trd>,
    <trd>trojl&iacute;stek</trd>,
    <trd>t&rcaron;&iacute;l&iacute;stek</trd>
  </trdgrp>
</drv>

