  <trd lng="zu">ishidi</trd>
</drv>

<drv mrk="foli.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="foli.0i.BOT">
    <dif>
      Naski <tld/>ojn.
    </dif>
  </snc>
  <trd lng="cs">vyh&aacute;n&ecaron;t listy</trd>
  <trd lng="fr">feuiller</trd>
  <trd lng="hu">levelet hajt</trd>
  <trd lng="ru">&c_p;&c_o;&c_k;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;
  &c_l;&c_i;&c_s;&c_t;&c_mol;&c_ja;&c_m;&c_i;</trd>
  <trd lng="sk">vyh&aacute;&ncaron;a&tcaron; listy</trd>
</drv>

<drv mrk="foli.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="foli.0aro.BOT">
    <dif>
      La tuto de la <tld/>oj de vegeta&jcirc;o.
    </dif>
  </snc>
  <trd lng="bg">&c_sh;&c_u;&c_m;&c_a;</trd>
  <trd lng="ca">fullatge</trd>
  <trdgrp lng="cs">
    <trd>listov&iacute; rostliny</trd>,
    <trd>list&iacute;</trd>,
    <trd>olist&ecaron;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Laub</trd>
  <trd lng="es">follaje</trd>
  <trd lng="fr">feuillage</trd>
  <trd lng="hu">lomb(ozat)</trd>
  <trd lng="pl">ulistnienie</trd>
  <trd lng="ru">&c_l;&c_i;&c_s;&c_t;&c_v;&c_a;</trd>
  <trd lng="sk">l&iacute;stie</trd>
</drv>

<drv mrk="foli.0ero">
  <kap><tld/>ero</kap>
  <snc mrk="foli.0ero.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      &Ccirc;iu el la <tld/>similaj partoj konsistigantaj kunmetitan
      <ref tip="malprt" cel="foli.0o.BOT"><tld/>on</ref>.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>l&iacute;ste&ccaron;ek slo&zcaron;en&eacute;ho listu</trd>,
    <trd>&scaron;upina obalov&aacute;</trd>
  </trdgrp>
  <trd lng="fr">foliole</trd>
  <trd lng="sk">l&iacute;sto&ccaron;ek <klr>(zlo&zcaron;it&yacute;ch kvetov)</klr></trd>
</drv>

<drv mrk="foli.0umi">
  <kap><tld/>i<fnt>Z</fnt>, <var><kap><tld/>umi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="foli.0umi.libron">
    <dif>
      Transturni la pa&gcirc;ojn de libro por rapide trarigardi
      kaj dise tralegi &gcirc;in.
    </dif>
    <trdgrp lng="bg">
      <trd>&c_r;&c_a;&c_z;&c_l;&c_i;&c_s;&c_t;&c_v;&c_a;&c_m;</trd>,
      <trd>&c_p;&c_r;&c_je;&c_l;&c_i;&c_s;&c_t;&c_v;&c_a;&c_m;</trd>
    </trdgrp>
    <trd lng="de">bl&auml;ttern</trd>
    <trd lng="es">hojear</trd>
    <trd lng="fr">feuilleter<klr tip="ind"> (tourner les
    pages)</klr></trd>
    <trd lng="hu">lapoz</trd>
    <trd lng="pl">kartkowa&cacute;</trd>
    <trdgrp lng="ru">
      <trd>&c_l;&c_i;&c_s;&c_t;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_je;&c_l;&c_i;&c_s;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="foli.0umi.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Esplori datumojn (dosierojn, dosierujojn, hipertekstojn) en
      la&ubreve;vola ordo, ne nepre en la rekta ordo.
    </dif>
    <trd lng="bg">&c_r;&c_a;&c_z;&c_g;&c_l;&c_je;&c_zh;&c_d;&c_a;&c_m;</trd>
    <trd lng="hu">b&ouml;ng&eacute;szik<klr tip="ind"> (pl. a
    vil&aacute;gh&aacute;l&oacute;n)</klr></trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_m;&c_a;&c_t;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>listovat</trd>,
    <trd>obracet listy <klr>(v knize)</klr></trd>
  </trdgrp>
  <trd lng="en">browse</trd>
  <trd lng="es">navegar <klr tip="amb">(por internet)</klr></trd>
  <trd lng="nl">bladeren</trd>
  <trd lng="sk">listova&tcaron;</trd>
</drv>

<drv mrk="foli.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="foli.0igi.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Platigi metalon en <tld/>on:
      <ekz>
        <tld/>igita ar&gcirc;ento
        <fnt><bib>MT</bib>, <lok>&Jer; 10:9</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="es">
       <trd>laminar</trd>,
       <trd>hacer planchas</trd>
    </trdgrp>
    <trd lng="fr">laminer</trd>
    <trd lng="hu">lamin&aacute;l</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_k;&c_a;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_a;&c_l;&c_mol;&c_c;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_l;&c_ju;&c_shch;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">l&iacute;stkovat</trd>
  <trd lng="sk">l&iacute;stkova&tcaron;</trd>
</drv>

<drv mrk="foli.0umilo">
  <kap><tld/>umilo</kap>
  <snc mrk="foli.0umilo.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <ref tip="super" cel="leg.0ilo.eliga">Legilo</ref> por
      <ref cel="foli.0umi"><tld/>umi</ref>, precipe
      TTT-legilo de <ref tip="malprt" cel="ret.0umilo">retumilo</ref>
    </dif>
  </snc>
  <trd lng="bg">&c_b;&c_r;&c_a;&c_u;&c_z;&c_malmol;&c_r;</trd>
  <trd lng="cs">prohl&iacute;&zcaron;e&ccaron;ka <klr>(program)</klr></trd>
  <trd lng="en">browser</trd>
  <trdgrp lng="es">
    <trd>explorador (programa)</trd>,
    <trd>navegador (programa)</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fureteur</trd>,
    <trd>navigateur</trd>
  </trdgrp>
  <trd lng="hu">b&ouml;ng&eacute;sz&odblac;(program)</trd>
  <trdgrp lng="nl">
    <trd>browser</trd>,
    <trd>bladeraar</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>czytnik</trd>,
    <trd>paj&eogonek;czarka</trd>,
    <trd>przegl&aogonek;darka</trd>,
    <trd>broser</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_a;&c_u;&c_z;&c_je;&c_r;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_m;&c_o;&c_t;&c_r;&c_shch;&c_i;&c_k;</trd>,
    <trd>&c_o;&c_b;&c_o;&c_z;&c_r;&c_je;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">prehliada&ccaron;</trd>
</drv>

<drv mrk="foli.fal0a">
  <kap>fal<tld/>a</kap>
  <snc mrk="foli.fal0a.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Havanta <tld/>ojn, kiuj velkas kaj falas komence de la malvarma
      sezono:
      <ekz>
        fal<tld/>aj kaj &ccirc;iamverdaj arboj
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro
        8a</lok></fnt>;
      </ekz>
      <ekz>
        sova&gcirc;a ajlo <klr>...</klr> kreskas sur humida kaj malseka
        sojlo
        de
        la ombraj fal<tld/>aj arbaroj kaj herbejoj de E&ubreve;ropo kaj
        Azio
        <fnt>
        <aut>Dr. Pandalis</aut>,
        <vrk><url ref="http://www.esp-ina.com/ajlo.htm">Sova&gcirc;a
        Ajlo</url></vrk>,
        <lok>[vidita je 2006-03-22]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_l;&c_i;&c_s;&c_t;&c_o;&c_p;&c_a;&c_d;&c_n;&c_o; &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="cs">opadav&yacute;</trd>
  <trdgrp lng="es">
    <trd>caducifolia</trd>,
    <trd>de hoja <ind>caduca</ind></trd>
  </trdgrp>
  <trd lng="fr">&agrave; feuilles <ind>caduques</ind></trd>
  <trdgrp lng="pt">
    <trd>caducif&oacute;lio</trd>,
    <trd>caduco <klr>(bot.)</klr></trd>,
    <trd>dec&iacute;duo</trd>,
    <trd>deciduif&oacute;lio</trd>    
  </trdgrp>
  <trd lng="sk">opadav&yacute;</trd>
</drv>

<drv mrk="foli.flug0o">
  <kap>flug<tld/>o</kap>
  <snc mrk="foli.flug0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Unu<tld/>a a&ubreve; malmult<tld/>a presa&jcirc;o, ne
      bro&scirc;urita,
      ordinare uzata por propagando a&ubreve; reklamo.
    </dif>
  </snc>
  <trd lng="bg">&c_l;&c_i;&c_s;&c_t;&c_o;&c_v;&c_k;&c_a;</trd>
  <trd lng="cs">let&aacute;k</trd>
  <trd lng="de">Flugblatt</trd>
  <trd lng="es">panfleto</trd>
  <trd lng="fr">prospectus</trd>
  <trdgrp lng="hu">
    <trd>r&ouml;plap</trd>,
    <trd>prospektus</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>strooibiljet</trd>,
    <trd>pamflet</trd>
  </trdgrp>
  <trd lng="pl">ulotka</trd>
  <trd lng="ru">&c_l;&c_i;&c_s;&c_t;&c_o;&c_v;&c_k;&c_a;</trd>
  <trd lng="sk">let&aacute;k</trd>
</drv>

<drv mrk="foli.kontraux0o">
  <kap>kontra&ubreve;<tld/>o</kap>
  <fnt>
    <aut>M. C. Butler</aut> <vrk>Step by Step in Esperanto</vrk>
    <lok>&para;440</lok>
  </fnt>
  <snc mrk="foli.kontraux0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Tiu responda, komplementa parto de &ccirc;eko, kvitanco, atestfolio
      ks, kiu restas &ccirc;e la registro, kajero, libreto ks, post
      apartigo de la transdonita parto kaj surhavas ordinare la samajn
      informojn;
      <ref tip="sin" cel="talon.0o.EKON">talono<sncref/></ref>.
    </dif>
  </snc>
  <trd lng="en">counterfoil</trd>
  <trd lng="fr">talon</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_r;&c_je;&c_sh;&c_o;&c_k;<klr> (&c_k;&c_v;&c_i;&c_t;&c_a;&c_n;&c_c;&c_i;&c_o;&c_n;&c_n;&c_o;&c_j;, &c_ch;&c_je;&c_k;&c_o;&c_v;&c_o;&c_j; &c_i; &c_t;.&c_d;.
    &c_k;&c_n;&c_i;&c_zh;&c_k;&c_i;)</klr></trd>,
    <trd>&c_t;&c_a;&c_l;&c_o;&c_n;<klr> (&c_b;&c_i;&c_l;&c_je;&c_t;&c_a;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="foli.sen0igi">
  <kap>sen<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="foli.sen0igi.KOMUNE">
    <dif>
      Senigi je <tld/>oj:
      <ekz>
        a&ubreve;tuno sen<tld/>igis la grandajn kverkojn
        <fnt>
          <aut>E. Boirac</aut>
          <vrk><url ref="https://arkivo.esperanto-france.org/divers/boirac/sorchist.htm">
          Perdita kaj Retrovita</url></vrk>
          <lok>1904</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">effeuiller</trd>
</drv>

<drv mrk="foli.tri0o">
  <kap><ofc>*</ofc>tri<tld/>o</kap>
  <snc mrk="foli.tri0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Trifolium</trd>), kun tripartaj <tld/>oj:
      <ekz>
        &ccirc;arma kaj dol&ccirc;a estis la aromo, kiu blovetis de la luksa kampo
        de tri<tld/>oj, kie la abeloj zumadis
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de la jaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        unuj verdis: la lino, tritiko, tri<tld/>o, la aliaj estis oraj
        de la maturi&gcirc;anta hordeo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni rikoltis la tri<tld/>on kaj seneson
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="&WCU;/thumb/9/9d/Trifolium_pratense_-_Keila.jpg/480px-Trifolium_pratense_-_Keila.jpg" prm="CC BY-SA 4.0"><ind>herbeja tri<tld/>o</ind> (Trifolium pratense),
  Keila, Estonujo
       <fnt>
         <aut>Ivar Leidus, propra verko</aut>
         <lok><url ref="&WCW;/File:Trifolium_pratense_-_Keila.jpg">&WCW;/File:Trifolium_pratense_-_Keila.jpg</url></lok>
       </fnt>
    <trd lng="la">Trifolium pratense</trd>
  </bld>
  <trd lng="bg">&c_d;&c_je;&c_t;&c_je;&c_l;&c_i;&c_n;&c_a;</trd>
  <trd lng="de">Klee</trd>
  <trd lng="es">trebol</trd>
  <trd lng="fr">tr&egrave;fle</trd>
  <trd lng="hu">l&oacute;here</trd>
  <trd lng="nl">klaver</trd>
  <trd lng="pl">koniczyna</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_l;&c_je;&c_v;&c_je;&c_r;</trd>,
    <trd>&c_t;&c_r;&c_i;&c_l;&c_i;&c_s;&c_t;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">trojl&iacute;stok</trd>
  <trdgrp lng="cs">
    <trd>trifolium</trd>,
    <trd>trojl&iacute;stek</trd>,
    <trd>t&rcaron;&iacute;l&iacute;stek</trd>
  </trdgrp>
</drv>

<drv mrk="foli.kartezia0o">
  <kap>kartezia <tld/>o</kap>
  <fnt><bib>MatVort</bib></fnt>
  <snc mrk="foli.kartezia0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <dif>
      Speco de triagrada ebena
      <ref tip="super" cel="kurb.0o.MAT">kurbo</ref>:
      <ekz>
        la kartezia ekvacio de kartezia <tld/>o estas de la tipo
        <frm am="x^3+y^3=3axy"><k>x</k><sup>3</sup>+<k>y</k><sup>3</sup> = 3<k>axy</k></frm>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/k_folio.svg" tip="svg" alt="400" lrg="400">
    kartezia <tld/>o
  </bld>
  <trd lng="bg">&c_d;&c_je;&c_k;&c_a;&c_r;&c_t;&c_o;&c_v; &c_l;&c_i;&c_s;&c_t;</trd>
  <trd lng="de">Descartessches <ind>Blatt</ind></trd>
  <trd lng="en">Cartesian <ind>folium</ind></trd>
  <trd lng="es">hoja de Descartes<klr tip="amb"> (curva
  geom&eacute;trica)</klr></trd>
  <trd lng="fr">folium de Descartes</trd>
  <trd lng="hu">Descartes-f&eacute;le lev&eacute;lg&ouml;rbe</trd>
  <trd lng="pl">li&sacute;&cacute; Kartezjusza</trd>
  <trd lng="ru">&c_d;&c_je;&c_k;&c_a;&c_r;&c_t;&c_o;&c_v;
  <ind>&c_l;&c_i;&c_s;&c_t;</ind></trd>
  <trd lng="sk">Descartesov list</trd>
  <trdgrp lng="cs">
    <trd>Descartes&uring;v list</trd>,
    <trd>Descart&uring;v list</trd>
  </trdgrp>
</drv>
