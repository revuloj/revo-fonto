<drv mrk="foli.0i">
  <kap><tld/>i</kap>
  <snc mrk="foli.0i.BOT">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Naski <tld/>ojn.
    </dif>
    <trd lng="cs">vyh&aacute;n&ecaron;t listy</trd>
    <trd lng="de">austreiben <klr>(Bl&auml;tter)</klr></trd>
    <trd lng="fr">feuiller</trd>
    <trd lng="hu">levelet hajt</trd>
    <trd lng="ru">&c_p;&c_o;&c_k;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;
    &c_l;&c_i;&c_s;&c_t;&c_mol;&c_ja;&c_m;&c_i;</trd>
    <trd lng="sk">vyh&aacute;&ncaron;a&tcaron; listy</trd>
  </snc>
  <snc mrk="foli.0i.foliumi">
    <ref tip="dif" cel="foli.0umi"><tld/>umi</ref>
    <trdgrp lng="cs">
      <trd>listovat</trd>,
      <trd>obracet listy <klr>(v knize)</klr></trd>
    </trdgrp>
    <trd lng="en">browse</trd>
    <trd lng="es">navegar <klr tip="amb">(por internet)</klr></trd>
    <trd lng="nl">bladeren</trd>
    <trd lng="sk">listova&tcaron;</trd>
  </snc>
</drv>

<drv mrk="foli.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="foli.0aro.BOT">
    <dif>
      La tuto de la <tld/>oj de vegeta&jcirc;o.
    </dif>
  </snc>
  <trd lng="bg">&c_sh;&c_u;&c_m;&c_a;</trd>
  <trd lng="ca">fullatge</trd>
  <trdgrp lng="cs">
    <trd>listov&iacute; rostliny</trd>,
    <trd>list&iacute;</trd>,
    <trd>olist&ecaron;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Laub</trd>
  <trd lng="es">follaje</trd>
  <trd lng="fr">feuillage</trd>
  <trd lng="hu">lomb(ozat)</trd>
  <trd lng="pl">ulistnienie</trd>
  <trd lng="ru">&c_l;&c_i;&c_s;&c_t;&c_v;&c_a;</trd>
  <trd lng="sk">l&iacute;stie</trd>
</drv>

<drv mrk="foli.0ero">
  <kap><tld/>ero</kap>
  <snc mrk="foli.0ero.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      &Ccirc;iu el la <tld/>similaj partoj konsistigantaj kunmetitan
      <ref tip="malprt" cel="foli.0o.BOT"><tld/>on</ref>.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>l&iacute;ste&ccaron;ek slo&zcaron;en&eacute;ho listu</trd>,
    <trd>&scaron;upina obalov&aacute;</trd>
  </trdgrp>
  <trd lng="fr">foliole</trd>
  <trd lng="sk">l&iacute;sto&ccaron;ek <klr>(zlo&zcaron;it&yacute;ch kvetov)</klr></trd>
</drv>

<drv mrk="foli.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="foli.0igi.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Platigi metalon en <tld/>on:
      <ekz>
        <tld/>igita ar&gcirc;ento
        <fnt><bib>MT</bib>, <lok>&Jer; 10:9</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="es">
       <trd>laminar</trd>,
       <trd>hacer planchas</trd>
    </trdgrp>
    <trd lng="fr">laminer</trd>
    <trd lng="hu">lamin&aacute;l</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_k;&c_a;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_a;&c_l;&c_mol;&c_c;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_l;&c_ju;&c_shch;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">l&iacute;stkovat</trd>
  <trd lng="sk">l&iacute;stkova&tcaron;</trd>
</drv>

<drv mrk="foli.0umi">
  <kap>
    <tld/>umi
  </kap>
  <snc mrk="foli.0umi.foliojn">
    <dif>
      <ref tip="super" cel="mov.0i.transloki_parton">Movi</ref>, turni la foliojn de io:
      <ekz>
        malrapida vento <tld/>umis iom en la arbfoliaro
        <fnt>
          <bib>VRA</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">rascheln <klr>(Bl&auml;tter)</klr></trd>
  </snc>
  <snc mrk="foli.0umi.pagxojn">
    <dif>
      Turni la pa&gcirc;ojn de libro, kajero kc 
      por supra&jcirc;e <ref tip="super" cel="rigard.tra0i">trarigardi</ref> &gcirc;in:
      <ekz>
        <klr>[li]</klr> rigardis la librojn, <tld/>umis en kelkaj kaj legis peceton tie
        kaj tie
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i rapide malfermis fotografa&jcirc;oplenan albumon, kiun &scirc;i ofte <tld/>umis
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>umis kelkajn paperojn sur la skribotablo
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        nur la <tld/>umado a&ubreve;di&gcirc;is <klr>[kiel]</klr> en niaj bibliotekoj
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>umas al la titolvorto kaj tie mi legas, ke la persan tapi&scirc;arton ni
        konas ekde la dinastio &leftquot;<nom>Safida</nom>&rightquot;
        <fnt>
          <bib>SatirRak</bib>
        </fnt>;
      </ekz>
      <ekz>
        Jano dume plu <tld/>umis la leterojn, subite li fajfis eksciti&gcirc;e
        <fnt>
          <bib>ChB</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="foli.0umi.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Esplori datumojn (dosierojn, dosierujojn, hipertekstojn) en
      la&ubreve;vola ordo, ne nepre en la rekta ordo.
    </dif>
    <trd lng="bg">&c_r;&c_a;&c_z;&c_g;&c_l;&c_je;&c_zh;&c_d;&c_a;&c_m;</trd>
    <trd lng="hu">b&ouml;ng&eacute;szik<klr tip="ind"> (pl. a
    vil&aacute;gh&aacute;l&oacute;n)</klr></trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_m;&c_a;&c_t;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="de">bl&auml;ttern</trd>
  <trd lng="en">browse <klr>(a book)</klr></trd>
</drv>


<drv mrk="foli.0umilo">
  <kap><tld/>umilo</kap>
  <snc mrk="foli.0umilo.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <ref tip="super" cel="leg.0ilo.eliga">Legilo</ref> por
      <ref cel="foli.0umi"><tld/>umi</ref>, precipe
      TTT-legilo de <ref tip="malprt" cel="ret.0umilo">retumilo</ref>
    </dif>
  </snc>
  <trd lng="bg">&c_b;&c_r;&c_a;&c_u;&c_z;&c_malmol;&c_r;</trd>
  <trd lng="cs">prohl&iacute;&zcaron;e&ccaron;ka <klr>(program)</klr></trd>
  <trd lng="en">browser</trd>
  <trdgrp lng="es">
    <trd>explorador (programa)</trd>,
    <trd>navegador (programa)</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fureteur</trd>,
    <trd>navigateur</trd>
  </trdgrp>
  <trd lng="hu">b&ouml;ng&eacute;sz&odblac;(program)</trd>
  <trdgrp lng="nl">
    <trd>browser</trd>,
    <trd>bladeraar</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>czytnik</trd>,
    <trd>paj&eogonek;czarka</trd>,
    <trd>przegl&aogonek;darka</trd>,
    <trd>broser</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_a;&c_u;&c_z;&c_je;&c_r;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_m;&c_o;&c_t;&c_r;&c_shch;&c_i;&c_k;</trd>,
    <trd>&c_o;&c_b;&c_o;&c_z;&c_r;&c_je;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">prehliada&ccaron;</trd>
</drv>

<drv mrk="foli.fal0a">
  <kap>fal<tld/>a</kap>
  <snc mrk="foli.fal0a.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Havanta <tld/>ojn, kiuj velkas kaj falas komence de la malvarma
      sezono:
      <ekz>
        fal<tld/>aj kaj &ccirc;iamverdaj arboj
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro
        8a</lok></fnt>;
      </ekz>
      <ekz>
        sova&gcirc;a ajlo <klr>[&#8230;]</klr> kreskas sur humida kaj malseka
        sojlo
        de
        la ombraj fal<tld/>aj arbaroj kaj herbejoj de E&ubreve;ropo kaj
        Azio
        <fnt>
        <aut>Dr. Pandalis</aut>,
        <vrk><url ref="http://www.esp-ina.com/ajlo.htm">Sova&gcirc;a
        Ajlo</url></vrk>,
        <lok>[vidita je 2006-03-22]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_l;&c_i;&c_s;&c_t;&c_o;&c_p;&c_a;&c_d;&c_n;&c_o; &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="cs">opadav&yacute;</trd>
  <trdgrp lng="es">
    <trd>caducifolia</trd>,
    <trd>de hoja <ind>caduca</ind></trd>
  </trdgrp>
  <trd lng="fr">&agrave; feuilles <ind>caduques</ind></trd>
  <trdgrp lng="pt">
    <trd>caducif&oacute;lio</trd>,
    <trd>caduco <klr>(bot.)</klr></trd>,
    <trd>dec&iacute;duo</trd>,
    <trd>deciduif&oacute;lio</trd>    
  </trdgrp>
