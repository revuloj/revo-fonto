<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>foli</rad>/o
</kap>
<drv mrk="foli.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Tre diversforma organo de arboj kaj aliaj vegeta&jcirc;oj,
      ordinare verda kaj maldika, kiu kreskas sur bran&ccirc;oj a&ubreve;
      trunko, kaj servas por asimili la karbonacidon de la aero,
      necesan por la nutrado de la vegeta&jcirc;o.
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Maldika, plata objekto, plimalpli komparebla kun arba <tld/>o:
      <ekz>
        kupra, ora, stana <tld/>o;
      </ekz>
      <ekz>
        paper<tld/>o;
      </ekz>
      <ekz>
        <tld/>oj de libro.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="plat.0o">plato</ref>,
      <ref cel="plak.0o">plako</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Blatt</trd>
  <trd lng="en">leaf</trd>
  <trd lng="fr">feuille</trd>
  <trd lng="tr">yaprak</trd>
</drv>
<drv mrk="foli.0aro">
  <kap><tld/>aro</kap>
  <snc>
    <dif>
      La tuto de la <tld/>oj de vegeta&jcirc;o.
    </dif>
  </snc>
  <trd lng="de">Laub</trd>
  <trd lng="fr">feuillage</trd>
</drv>
<drv mrk="foli.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Naski <tld/>ojn.
    </dif>
  </snc>
</drv>
<drv mrk="foli.0umi">
  <kap><tld/>i<fnt>Z</fnt>, <tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Transturni la pa&gcirc;ojn de libro por rapide trarigardi
      kaj dise tralegi &gcirc;in.
    </dif>
  </snc>
  <trd lng="de">bl&auml;ttern</trd>
  <trd lng="fr">feuilleter</trd>
</drv>
<drv mrk="foli.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Platigi metalon en <tld/>on:
      <ekz>
        <tld/>igita ar&gcirc;ento
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Jeremia 10, 9</lok>)</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">laminer</trd>
</drv>
<drv mrk="foli.flug0o">
  <kap>flug<tld/>o</kap>
  <snc>
    <dif>
      Unu<tld/>a a&ubreve; malmult<tld/>a presa&jcirc;o, ne bro&scirc;urita,
      ordinare uzata por propagando a&ubreve; reklamo.
    </dif>
  </snc>
  <trd lng="de">Flugblatt</trd>
  <trd lng="fr">prospectus</trd>
</drv>
<drv mrk="foli.tri0o">
  <kap>tri<tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="fab.0acoj">fabacoj</ref>
      (<trd lng="la">Trifolium</trd>), kun tripartaj <tld/>oj.
    </dif>
  </snc>
  <bld lok="../bld/trifol.jpg">
    <ind>herbeja tri<tld/>o</ind>
    <trd lng="la">Trifolium pratense</trd>
  </bld>
  <trd lng="de">Klee</trd>
  <trd lng="fr">tr&egrave;fle</trd>
</drv>
</art>
