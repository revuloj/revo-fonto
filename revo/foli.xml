    <trd>&c_v;&c_i;&c_p;&c_u;&c_s;&c_k;&c_a;&c_t;&c_i; &c_l;&c_i;&c_s;&c_t;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="foli.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="foli.0aro.BOT">
    <dif>
      Amaseto de <tld/>oj kreskantaj &ccirc;e bran&ccirc;o a&ubreve; vegeta&jcirc;o:
      <ekz>
        bran&ccirc;oj kun densa <tld/>aro
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 31:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[arbo]</klr>, sur kiu estis bela <tld/>aro, multe da fruktoj
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 4:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        la daktilpalmo etendis sian &scirc;irman <tld/>aron tre alte, kvaza&ubreve; <klr>[&#8230;]</klr>
        sunombrelo
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        kamufli sin inter la <tld/>aro
        <fnt>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2012/010786.php">Nokti&gcirc;as ... kaj la papilio
          noktui&gcirc;as</url></vrk>
          <bib>Monato</bib>
          <lok>2012/08, p. 27</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malluma <tld/>aro de la arbo
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        sunradioj penetris tra la juna <tld/>aro
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Ar&gcirc;enta Urbo Amarganto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_l;&c_ib;&c_s;&c_mol;&c_c;&c_je;</trd>
    <trd lng="bg">&c_sh;&c_u;&c_m;&c_a;</trd>
    <trdgrp lng="ca">
      <trd>fullatge</trd>,
      <trd>fullam</trd>
    </trdgrp>
    <trdgrp lng="cs">
      <trd>listov&iacute; rostliny</trd>,
      <trd>list&iacute;</trd>,
      <trd>olist&ecaron;n&iacute;</trd>
    </trdgrp>
    <trd lng="de">Laub</trd>
    <trd lng="es">follaje</trd>
    <trd lng="fr">feuillage</trd>
    <trd lng="hu">lomb(ozat)</trd>
    <trd lng="id"><baz>daun</baz>dedaunan</trd>
    <trd lng="pl">ulistnienie</trd>
    <trd lng="ru">&c_l;&c_i;&c_s;&c_t;&c_v;&c_a;</trd>
    <trd lng="sk">l&iacute;stie</trd>
    <trd lng="zh">&#32160;&#27193; <pr>l&uuml;sh&ugrave;</pr></trd>
</snc>
  <snc mrk="foli.0aro.papero">
    <dif>
      Staplo de paper<tld/>oj:
      <ekz>
        TEJO celis ne simple liveri <klr>[&#8230;]</klr> nur ankora&ubreve; alian nova&jcirc;-<tld/>aron a&ubreve;
        anoncaron, sed veran gazeton
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
