<drv mrk="foli.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="foli.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Diversforma organo de
      <ref tip="malprt" cel="veget.0ajxo">vegeta&jcirc;o</ref>,
      ordinare verda kaj maldika, kiu kreskas sur la tigo kaj servas por
      asimili la karbonacidon de la aero, necesan por la nutrado
      de la vegeta&jcirc;o:
      <ekz>
        <ind><tld/>plato</ind>
        <klr>(la plata parto de la <tld/>o)</klr>;
        <trd lng="de">Blattspreite</trd>
        <trd lng="es">limbo<klr tip="amb"> (bot.)</klr></trd>
        <trd lng="fr">limbe<klr tip="ind"> (bot.)</klr></trd>
        <trdgrp lng="nl">
          <trd>bladschijf</trd>,
          <trd>limbus</trd>
        </trdgrp>
        <trd lng="hu">lemez<klr tip="amb"> (lev&eacute;len)</klr></trd>
      </ekz>
      <ekz>
        <ind><tld/>tigo</ind>
        <klr>(tigo tenanta la <tld/>platon, petiolo)</klr>;
        <trd lng="de">Blattstiel</trd>
        <trd lng="es">peciolo<klr tip="amb"> (bot)</klr></trd>
        <trd lng="fr">p&eacute;tiole</trd>
        <trd lng="nl">bladsteel</trd>
        <trd lng="hu">ny&eacute;l<klr tip="amb">
        (lev&eacute;len)</klr></trd>
      </ekz>
      <ekz>
        <ind><tld/>ingo, <tld/>bazo</ind>
        <klr>(baza parto de <tld/>o &ccirc;irka&ubreve;anta la
        tigon)</klr>;
        <trd lng="de">Blattgrund</trd>
        <trd lng="es">base (de la hoja de &aacute;rbol)<klr tip="amb">
        (bot)</klr></trd>
        <trd lng="fr">base foliaire</trd>
        <trd lng="hu">alap<klr tip="amb"> (lev&eacute;len)</klr></trd>
      </ekz>
      <ekz>
        elipsa, ovoforma, inverse ovoforma <klr>(obovala)</klr>,
        spatelforma, lancetforma, sagforma, ronda,
        lancoforma, renforma, linia, &scirc;ildforma, korforma,
        pingloforma, pluma <klr>(pinata)</klr>, triloba,
        manforma, piedforma <tld/>o;
      </ekz>
      <ekz>
        glatranda, harranda, segildenta, denta,
        &scirc;arkna&gcirc;ila, no&ccirc;ita, loba,
        festonranda <tld/>o;
      </ekz>
      <ekz>
        pinta, pintega, pikilpinta, obtuza <tld/>o;
      </ekz>
      <ekz>
        strinerva, fingronerva, plumnerva <tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="bg">
      <trd>&c_l;&c_i;&c_s;&c_t;</trd>,
      <trd>&c_l;&c_i;&c_s;&c_t;&c_o;</trd>
    </trdgrp>
    <trd lng="de">Blatt</trd>
    <trd lng="es">hoja<klr tip="amb"> (&aacute;rbol,planta)</klr></trd>
    <trd lng="nl">blad<klr tip="ind"> (plantk.)</klr></trd>
    <trd lng="hu">lev&eacute;l<klr tip="amb"> (fa~)</klr></trd>
    <trd lng="pt">folha</trd>
  </snc>
  <snc mrk="foli.0o.TEK">
    <dif>
      Maldika, plata objekto:
      <ekz>
        kupra, ora, stana <tld/>o;
      </ekz>
      <ekz>
        paper<tld/>o;
      </ekz>
      <ekz>
        <tld/>oj de libro.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="film.0o.tavolo">filmo</ref>,
      <ref cel="plat.0o">plato</ref>
    </refgrp>
    <trd lng="bg">&c_l;&c_i;&c_s;&c_t;</trd>
    <trdgrp lng="de">
      <trd>Blatt <klr>(papero)</klr></trd>,
      <trd>Folie <klr>(metalo, plasto)</klr></trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>hoja <klr tip="amb">(papel, metal,
      pl&aacute;stico)</klr></trd>
      <trd>plancha <klr tip="amb">(metal)</klr></trd>
      <trd>l&aacute;mina <klr>(metal, pl&aacute;stico)</klr></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>blad<klr tip="ind"> (papier)</klr></trd>,
      <trd>folie</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>lap<klr>(k&ouml;nyv~)</klr></trd>,
      <trd>f&oacute;lia</trd>
    </trdgrp>
  <trdgrp lng="pt">
    <trd>folha</trd>,
    <trd>chapa</trd>,
    <trd>l&acirc;mina</trd>
  </trdgrp>
  </snc>
  <trd lng="af">blad</trd>
  <trd lng="am">&#x1209;&#x1205;</trd>
  <trd lng="ar">&#x648;&#x631;&#x642;&#x629;</trd>
  <trd lng="az">hesabat&#x131;</trd>
  <trd lng="bn">&#x9B6;&#x9C0;&#x99F;</trd>
  <trd lng="bs">list</trd>
  <trd lng="co">fogghiu</trd>
  <trdgrp lng="cs">
    <trd>list pap&iacute;ru</trd>,
    <trd>list rostliny</trd>
  </trdgrp>
  <trd lng="da">ark</trd>
  <trd lng="en">leaf</trd>
  <trd lng="es">hoja</trd>
  <trd lng="et">leht</trd>
  <trd lng="eu">fitxa</trd>
  <trd lng="fr">feuille</trd>
  <trd lng="fy">fel</trd>
  <trd lng="gl">folla</trd>
  <trd lng="gu">&#xAB6;&#xAC0;&#xA9F;</trd>
  <trd lng="hi">&#x91A;&#x93E;&#x926;&#x930;</trd>
  <trd lng="hr">list</trd>
  <trd lng="hy">&#x569;&#x565;&#x580;&#x569;</trd>
  <trd lng="is">bla&#xF0;</trd>
  <trd lng="ja">&#x30B7;&#x30FC;&#x30C8;</trd>
  <trd lng="ka">&#x10E4;&#x10E3;&#x10E0;&#x10EA;&#x10D4;&#x10DA;&#x10D8;</trd>
  <trd lng="km">&#x179F;&#x1793;&#x17D2;&#x179B;&#x17B9;&#x1780;</trd>
  <trd lng="ko">&#xC2DC;&#xD2B8;</trd>
