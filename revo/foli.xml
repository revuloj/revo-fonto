<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>foli</rad>/o
</kap>
<drv mrk="foli.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="foli.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Diversforma organo de <ref tip="malprt" cel="plant.0o">planto</ref>, 
      ordinare verda kaj maldika, kiu kreskas sur la tigo kaj servas por asimili
      la karbonacidon de la aero, necesan por la nutrado de la planto:
      <ekz>
        <ind><tld/>plato</ind>
        <klr>(la plata parto de la <tld/>o)</klr>;
        <trd lng="de">Blattspreite</trd>
        <trd lng="fr">limbe<klr tip="ind"> (bot.)</klr></trd>
      </ekz><ekz>
        <ind><tld/>tigo</ind>
        <klr>(tigo tenanta la <tld/>platon, petiolo)</klr>;
        <trd lng="de">Blattstiel</trd>
        <trd lng="fr">p&eacute;tiole</trd>
      </ekz><ekz>
        <ind><tld/>ingo, <tld/>bazo</ind>
        <klr>(baza parto de <tld/>o &ccirc;irka&ubreve;anta la tigon)</klr>;
        <trd lng="de">Blattgrund</trd>
        <trd lng="fr">base foliaire</trd>
      </ekz><ekz>
        elipsa, ovoforma, inverse ovoforma <klr>(obovala)</klr>,
        spatelforma, lancetforma, sagforma, ronda,
        lancoforma, renforma, linia, &scirc;ildforma, korforma,
        pingloforma, pluma <klr>(pinata)</klr>, triloba,
        manforma, piedforma <tld/>o;
      </ekz><ekz>
        glatranda, harranda, segildenta, denta,
        &scirc;arkna&gcirc;ila, no&ccirc;ita, loba,
        festonranda <tld/>o;
      </ekz><ekz>
        pinta, pintega, pikilpinta, obtuza <tld/>o;
      </ekz><ekz>
        strinerva, fingronerva, plumnerva <tld/>o.
      </ekz>
    </dif>
    <trd lng="de">Blatt</trd>
  </snc>
  <snc mrk="foli.0o.TEK">
    <dif>
      Maldika, plata objekto:
      <ekz>
        kupra, ora, stana <tld/>o;
      </ekz><ekz>
        paper<tld/>o;
      </ekz><ekz>
        <tld/>oj de libro.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="plat.0o">plato</ref>,
      <ref cel="plak.0o">plako</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Blatt <klr>(papero)</klr></trd>,
      <trd>Folie <klr>(metalo, plasto)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="en">leaf</trd>
  <trd lng="fr">feuille</trd>
  <trd lng="ru">&c_l;&c_i;&c_s;&c_t;</trd>
  <trd lng="tr">yaprak</trd>
</drv>

<drv mrk="foli.0aro">
  <kap><tld/>aro</kap>
  <snc>
    <dif>
      La tuto de la <tld/>oj de vegeta&jcirc;o.
    </dif>
  </snc>
  <trd lng="de">Laub</trd>
  <trd lng="fr">feuillage</trd>
  <trd lng="ru">&c_l;&c_i;&c_s;&c_t;&c_v;&c_a;</trd>
</drv>

<drv mrk="foli.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Naski <tld/>ojn.
    </dif>
  </snc>
  <trd lng="fr">feuiller</trd>
  <trd lng="ru">&c_p;&c_o;&c_k;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja; &c_l;&c_i;&c_s;&c_t;&c_mol;&c_ja;&c_m;&c_i;</trd>
</drv>

<drv mrk="foli.0umi">
  <kap><tld/>i<fnt>Z</fnt>, <tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="foli.0umi.libron">
    <dif>
      Transturni la pa&gcirc;ojn de libro por rapide trarigardi
      kaj dise tralegi &gcirc;in.
    </dif>
    <trd lng="de">bl&auml;ttern</trd>
    <trd lng="fr">feuilleter<klr tip="ind"> (tourner les pages)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_l;&c_i;&c_s;&c_t;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_je;&c_l;&c_i;&c_s;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="foli.0umi.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Esplori datumojn (dosierojn, dosierujojn, hipertekstojn) en
      la&ubreve;vola ordo, ne nepre en la rekta ordo.
    </dif>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_m;&c_a;&c_t;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="en">browse</trd>
</drv>

<drv mrk="foli.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Platigi metalon en <tld/>on:
      <ekz>
        <tld/>igita ar&gcirc;ento
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
          <lok>Jeremia 10, 9</lok>)</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">laminer</trd>
</drv>

<drv mrk="foli.0umilo">
  <kap><tld/>umilo</kap>
  <uzo tip="fak">KOMP</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="leg.0ilo.eliga">Legilo</ref> por
      <ref cel="foli.0umi"><tld/>umi</ref>, precipe
      TTT-legilo de <ref tip="malprt" cel="ret.0umilo">retumilo</ref>
    </dif>
  </snc>
  <trd lng="en">browser</trd>
  <trdgrp lng="fr"><trd>fureteur</trd>, <trd>navigateur</trd></trdgrp>
  <trdgrp lng="pl">
    <trd>czytnik</trd>, <trd>paj&eogonek;czarka</trd>,
    <trd>przegl&aogonek;darka</trd>, <trd>broser</trd></trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_a;&c_u;&c_z;&c_je;&c_r;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_m;&c_o;&c_t;&c_r;&c_shch;&c_i;&c_k;</trd>,
    <trd>&c_o;&c_b;&c_o;&c_z;&c_r;&c_je;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd></trdgrp>
</drv>

<drv mrk="foli.flug0o">
  <kap>flug<tld/>o</kap>
  <uzo tip="fak">TIP</uzo>
  <snc>
    <dif>
      Unu<tld/>a a&ubreve; malmult<tld/>a presa&jcirc;o, ne bro&scirc;urita,
      ordinare uzata por propagando a&ubreve; reklamo.
    </dif>
  </snc>
  <trd lng="de">Flugblatt</trd>
  <trd lng="fr">prospectus</trd>
  <trd lng="ru">&c_l;&c_i;&c_s;&c_t;&c_o;&c_v;&c_k;&c_a;</trd>
</drv>

<drv mrk="foli.tri0o">
  <kap><ofc>*</ofc>tri<tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Trifolium</trd>), kun tripartaj <tld/>oj.
    </dif>
  </snc>
  <bld lok="../bld/trifol.jpg">
    <ind>herbeja tri<tld/>o</ind>
    <trd lng="la">Trifolium pratense</trd>
  </bld>
  <trd lng="de">Klee</trd>
  <trd lng="fr">tr&egrave;fle</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_l;&c_je;&c_v;&c_je;&c_r;</trd>,
    <trd>&c_t;&c_r;&c_i;&c_l;&c_i;&c_s;&c_t;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>

</art>
