  <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="foli.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="foli.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Diversforma organo de
      <ref tip="malprt" cel="veget.0ajxo">vegeta&jcirc;o</ref>,
      ofte verda (kolora &ccirc;e la floroj) kaj maldika, kiu kreskas sur la tigo kaj servas por
      asimili la karbonacidon de la aero per <ref cel="fotosintez.0o">fotosintezo</ref>:
      <ekz>
        la kolombo <klr>[&#8230;]</klr> havis en sia bu&scirc;o de&scirc;iritan <tld/>on de olivarbo
        <fnt>
          <bib>MT</bib>
          <lok>&Gen; 8:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        terebintarbo, kies <tld/>oj velkis
        <fnt>
          <bib>MT</bib>
          <lok>&Jes; 1:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        la floro <klr>[&#8230;]</klr> etendis <klr>[&#8230;]</klr> &ccirc;iujn siajn blankajn <tld/>ojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Lekanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la trunketoj kaj <tld/>oj sin movis sen&ccirc;ese
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        flavaj kaj rozaj <tld/>oj falis de la arboj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        grandaj <tld/>okreska&jcirc;oj <klr>[&#8230;]</klr> formigis vere tropikan la&ubreve;bon en tiu &ccirc;i
        malgranda Edeno
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> tenis en la bu&scirc;o iajn torditajn kaj ekbruligitajn <tld/>ojn,
        kies fumon ili elspiradis
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        li tremas, kiel a&ubreve;tuna <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        pelos ilin brueto de falanta <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>&Lev; 26:36</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>plato</ind>
        <klr>(la plata parto de la <tld/>o, <ref cel="limb.0o.de_folio">limbo</ref>)</klr>;
        <trd lng="ca">limbe<klr tip="amb"> (bot.)</klr></trd>
        <trd lng="de">Blattspreite</trd>
        <trd lng="es">limbo<klr tip="amb"> (bot.)</klr></trd>
        <trd lng="fr">limbe<klr tip="ind"> (bot.)</klr></trd>
        <trdgrp lng="nl">
          <trd>bladschijf</trd>,
          <trd>limbus</trd>
        </trdgrp>
        <trd lng="hu">lemez<klr tip="amb"> (lev&eacute;len)</klr></trd>
      </ekz>
      <ekz>
        <ind><tld/>tigo</ind>
        <klr>(tigo tenanta la <tld/>platon, <ref cel="petiol.0o">petiolo</ref>)</klr>;
        <trd lng="ca">pec&iacute;ol</trd>
        <trd lng="de">Blattstiel</trd>
        <trd lng="es">peciolo<klr tip="amb"> (bot)</klr></trd>
        <trd lng="fr">p&eacute;tiole</trd>
        <trd lng="nl">bladsteel</trd>
        <trd lng="hu">ny&eacute;l<klr tip="amb">
        (lev&eacute;len)</klr></trd>
      </ekz>
      <ekz>
        <ind><tld/>ingo, <tld/>bazo</ind>
        <klr>(baza parto de <tld/>o &ccirc;irka&ubreve;anta la
        tigon)</klr>;
        <trd lng="ca">base <klr tip="amb">(d'una fulla)</klr></trd>
        <trd lng="de">Blattgrund</trd>
        <trd lng="es">base (de la hoja de &aacute;rbol)<klr tip="amb">
        (bot)</klr></trd>
        <trd lng="fr">base foliaire</trd>
        <trd lng="hu">alap<klr tip="amb"> (lev&eacute;len)</klr></trd>
      </ekz>
      <ekz>
        elipsa, ovoforma, inverse ovoforma <klr>(obovala)</klr>,
        spatelforma, lancetforma, sagforma, ronda,
        lancoforma, renforma, linia, &scirc;ildforma, korforma,
        pingloforma, pluma <klr>(pinata)</klr>, triloba,
        manforma, piedforma <tld/>o;
      </ekz>
      <ekz>
        glatranda, harranda, segildenta, denta,
        &scirc;arkna&gcirc;ila, no&ccirc;ita, loba,
        festonranda <tld/>o;
      </ekz>
      <ekz>
        pinta, pintega, pikilpinta, obtuza <tld/>o;
      </ekz>
      <ekz>
        strinerva, fingronerva, plumnerva <tld/>o.
      </ekz>
    </dif>
    <bld lok="../bld/foliformoj.png">foliformoj</bld>
    <trd lng="be">&c_l;&c_ib;&c_s;&c_t;</trd>
    <trdgrp lng="bg">
      <trd>&c_l;&c_i;&c_s;&c_t;</trd>,
      <trd>&c_l;&c_i;&c_s;&c_t;&c_o;</trd>
    </trdgrp>
    <trd lng="ca">fulla</trd>
    <trd lng="cv">&#1195;&c_u;&c_l;&#1195;&#1233;</trd>
    <trd lng="de">Blatt</trd>
    <trd lng="es">hoja<klr tip="amb"> (&aacute;rbol,planta)</klr></trd>
    <trd lng="nl">blad<klr tip="ind"> (plantk.)</klr></trd>
    <trd lng="hu">lev&eacute;l<klr tip="amb"> (fa~)</klr></trd>
    <trd lng="id">daun</trd>
    <trd lng="pt">folha</trd>
    <trdgrp lng="zh">
    <trd>&#21494; <pr>y&egrave;</pr></trd>,
    <trd>&#38913; <pr>y&egrave;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="foli.0o.TEK">
    <dif>
      Maldika, plata objekto:
      <ekz>
        ili batetendis <tld/>ojn da oro kaj tran&ccirc;is ilin en formo de fadenoj
        <fnt>
          <bib>MT</bib>
          <lok>&Eli; 39:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> komencis transturnadi la foliojn en la notolibro
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> plenskribis du grandajn <tld/>ojn da po&scirc;ta papero
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="film.0o.tavolo">filmo</ref>,
      <ref cel="plat.0o">plato</ref>
    </refgrp>
    <trd lng="be">&c_a;&c_r;&c_k;&c_u;&c_sh;</trd>
    <trd lng="bg">&c_l;&c_i;&c_s;&c_t;</trd>
    <trdgrp lng="ca">
      <trd>full</trd>,
      <trd>fulla</trd>,
      <trd>fullola</trd>,
      <trd>l&agrave;mina</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Blatt <klr>(papero)</klr></trd>,
      <trd>Folie <klr>(metalo, plasto)</klr></trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>hoja <klr tip="amb">(papel, metal,
      pl&aacute;stico)</klr></trd>
      <trd>plancha <klr tip="amb">(metal)</klr></trd>
      <trd>l&aacute;mina <klr>(metal, pl&aacute;stico)</klr></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>blad<klr tip="ind"> (papier)</klr></trd>,
      <trd>folie</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>lap<klr>(k&ouml;nyv~)</klr></trd>,
      <trd>f&oacute;lia</trd>
    </trdgrp>
  <trdgrp lng="id">
    <trd>bilah</trd>,
    <trd>lamina</trd>,
    <trd>lembar</trd>,
    <trd>lempeng<klr> (logam)</klr></trd>,
    <trd>papan</trd>,
    <trd>pelat</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>folha</trd>,
    <trd>chapa</trd>,
    <trd>l&acirc;mina</trd>
  </trdgrp>
  </snc>
  <trd lng="af">blad</trd>
  <trd lng="am">&#4617;&#4613;</trd>
  <trd lng="ar">&a_w;&a_r;&a_q;&a_t_marbuta;</trd>
