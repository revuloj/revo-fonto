<drv mrk="foli.0i">
  <kap><tld/>i</kap>
  <snc mrk="foli.0i.BOT">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      <ref tip="super" cel="kresk.0igi">Kreskigi</ref> <tld/>ojn:
      <ekz>
        da&ubreve;re ru&gcirc;o floras, verd' <tld/>as
        <fnt>
          <aut>Sibayama Zyun'iti</aut>
          <vrk><url ref="http://www.hh.e-mansion.com/~sibazyun/sib-orig/poe-hirosimo.htm">En la
          Parko de Paco, Hiro&scirc;imo, 1974</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        efemereco <klr>[&#8230;]</klr> &#8208; <tld/>as la horoj, bur&gcirc;onas la tagoj
        <fnt>
          <vrk><url ref="http://www.gazetoteko.com/hale/kels011.pdf">Efemereco</url></vrk>
          <lok>Kajeroj el la Sudo, n-ro 11, jul-sep 1991</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_k;&c_r;&c_y;&c_v;&c_a;&c_c;&c_c;&c_a;
    <ind>&c_l;&c_ib;&c_s;&c_mol;&c_c;&c_je;&c_m;</ind></trd>
    <trdgrp lng="ca">
      <trd>fullar</trd>,
      <trd>treure <ind>fulla</ind></trd>,
      <trd>traure <ind>fulla</ind></trd>
    </trdgrp>
    <trd lng="cs">vyh&aacute;n&ecaron;t listy</trd>
    <trd lng="de">austreiben <klr>(Bl&auml;tter)</klr></trd>
    <trd lng="fr">feuiller</trd>
    <trd lng="hu">levelet hajt</trd>
    <trd lng="id"><baz>daun</baz>berdaun</trd>
    <trd lng="ru">&c_p;&c_o;&c_k;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;
    &c_l;&c_i;&c_s;&c_t;&c_mol;&c_ja;&c_m;&c_i;</trd>
    <trd lng="sk">vyh&aacute;&ncaron;a&tcaron; listy</trd>
  </snc>
  <snc mrk="foli.0i.foliumi">
    <dif>
      <ref tip="dif" cel="foli.0umi"><tld lit="F"/>umi</ref>:
      <ekz>
        <klr>[&scirc;i]</klr> prenas la Biblion kaj <tld/>as
        <fnt>
          <bib>Rabistoj</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_g;&c_a;&c_r;&c_t;&c_a;&c_c;&c_mol;</trd>
    <trd lng="ca">fullejar <klr tip="amb">(un llibre, una publicaci&oacute;)</klr></trd>
    <trdgrp lng="cs">
      <trd>listovat</trd>,
      <trd>obracet listy <klr>(v knize)</klr></trd>
    </trdgrp>
    <trd lng="en">browse</trd>
    <trd lng="es">navegar <klr tip="amb">(por internet)</klr></trd>
    <trdgrp lng="id">
      <trd><baz>jelajah</baz>menjelajah</trd>,
      <trd><baz>jelajah</baz>menjelajahi</trd>,
      <trd><baz>ramban</baz>meramban</trd>
    </trdgrp>
    <trd lng="nl">bladeren</trd>
    <trd lng="sk">listova&tcaron;</trd>
  </snc>
  <trd lng="ja">&#33865;&#12434;&#12388;&#12369;&#12427;  <pr>&#12399;&#12434;&#12388;&#12369;&#12427;</pr></trd>
  <trdgrp lng="uk">
