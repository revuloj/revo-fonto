  </fnt>
  <snc mrk="foli.kontraux0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Tiu responda, komplementa parto de &ccirc;eko, kvitanco, atestfolio
      ks, kiu restas &ccirc;e la registro, kajero, libreto ks, post
      apartigo de la transdonita parto kaj surhavas ordinare la samajn
      informojn;
      <ref tip="sin" cel="talon.0o.EKON">talono<sncref/></ref>.
    </dif>
  </snc>
  <trd lng="en">counterfoil</trd>
  <trd lng="fr">talon</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_r;&c_je;&c_sh;&c_o;&c_k;<klr> (&c_k;&c_v;&c_i;&c_t;&c_a;&c_n;&c_c;&c_i;&c_o;&c_n;&c_n;&c_o;&c_j;, &c_ch;&c_je;&c_k;&c_o;&c_v;&c_o;&c_j; &c_i; &c_t;.&c_d;.
    &c_k;&c_n;&c_i;&c_zh;&c_k;&c_i;)</klr></trd>,
    <trd>&c_t;&c_a;&c_l;&c_o;&c_n;<klr> (&c_b;&c_i;&c_l;&c_je;&c_t;&c_a;)</klr></trd>
  </trdgrp>
</drv>

