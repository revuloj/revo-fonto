<drv mrk="foli.sen0igi">
  <kap>sen<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="foli.sen0igi.KOMUNE">
    <dif>
      Senigi je <tld/>oj:
      <ekz>
        la ventego sen<tld/>igas la plej delikatajn rozojn
        <fnt>
          <bib>RBa</bib>
        </fnt>;
      </ekz>
      <ekz>
        a&ubreve;tuno sen<tld/>igis la grandajn kverkojn
        <fnt>
          <aut>E. Boirac</aut>
          <vrk><url ref="https://arkivo.esperanto-france.org/divers/boirac/sorchist.htm">
          Perdita kaj Retrovita</url></vrk>
          <lok>1904</lok>
        </fnt>:
      </ekz>
      <ekz>
        proceso kontra&ubreve; la <nom>Dow Chemical</nom>, kiu produktis la faman kemian
        senfoliiga&jcirc;on<klr>[&#8230;]</klr> &leftquot;<nac>agent orange</nac>&rightquot;
        <fnt>
          <bib>MD02</bib>
          <vrk>Lasta perfido &#8208; Mortigi kaj Morti a&ubreve; la oferitaj de la Usonaj Militoj</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_a;&c_r;&c_v;&c_a;&c_c;&c_mol; &c_l;&c_ib;&c_s;&c_mol;&c_c;&c_je;</trd>
