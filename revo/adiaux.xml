<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>adia&ubreve;</rad></kap>
<drv mrk="adiaux.0">
    <kap><ofc>*</ofc><tld/></kap>
    <snc>
      <dif>
        Vorto per kiu oni salutas iun, kiam oni foriras de li:
        <ekz>
          mi diris <tld/> al miaj lastaj esperoj;
        </ekz>
        <ekz>
          Li diris <tld/> al la mondo surtera
          <fnt>
            (<aut>Zamenhof</aut>:
             <vrk>Proverbaro</vrk>)
          </fnt>
          <klr>(morti)</klr>;
        </ekz>
        <ekz>
          kapsigni <tld/>on al iu<fnt>Z</fnt>;
        </ekz>
        <ekz>
          ni faris por la  foriro de nia prezidanto <tld/>an kunvenon.
        </ekz>
      </dif>
    </snc>
    <trd lng="de">tsch&uuml;ss</trd>
    <trd lng="fr">adieu</trd>
    <trdgrp lng="nl"><trd>adieu</trd>,
       <trd>vaarwel</trd>,
       <trd>tot <ind>ziens</ind></trd>,
       <trd>tot <ind>kijk</ind></trd>
    </trdgrp>
    <trd lng="pl">no to <ind>cze&sacute;&cacute;</ind></trd>
    <trd lng="ru">&#x43f;&#x440;&#x43e;&#x449;&#x430;&#x439;</trd>
    <trd lng="sv">adj&ouml;</trd>
</drv>
<drv mrk="adiaux.0i">
    <kap><tld/>i</kap>
    <gra><vspec>x</vspec></gra>
    <snc>
      <dif>
        Kapsigni <tld/>on al iu<sup>Z</sup>;
        diri <tld/> al:
        <ekz>
          li <tld/>is &scirc;in, al &scirc;i kun larmoj.
        </ekz>
      </dif>
      <trdgrp lng="en">
        <trd>bid adieu (to)</trd>,
        <trd>say goodbye (to)</trd>,
        <trd>say farewell (to)</trd>
      </trdgrp>  
      <trd lng="de">verabschieden</trd>
      <trd lng="fr">faire ses <ind>adieux</ind></trd>
      <trdgrp lng="nl">
        <trd>afscheid nemen</trd>,
        <trd>vaarwel zeggen</trd>
      </trdgrp>  
      <trd lng="ru">&#x43f;&#x43e;&#x43f;&#x440;&#x43e;&#x449;&#x430;&#x442;&#x44c;&#x441;&#x44f;</trd>
      <trd lng="sv">s&auml;ga <ind>farv&auml;l</ind></trd>
    </snc>
    <snc mrk="adiaux.0i.komp">
      <uzo tip="fak">KOMP</uzo>
      <fnt><lok><url ref="&KompLeks;/A.html#ADIAUxI">KompLeks</url></lok></fnt>
      <fnt><lok><url
   ref="http://www.rano.demon.co.uk/pivkomp/_adiaux.html">PIV-2Komp</url></lok></fnt>
      <dif>
        Indiki al komputilo finon de seanco kun dialoga programo.
      </dif>
      <ref tip="ant" cel="salut.0i.komp">saluti</ref>
      <ref tip="malprt" cel="seanc.0o.komp">seanco</ref>
      <trd lng="en">logout</trd>
      <trd lng="nl">uitloggen</trd>
      <trd lng="ru">&#x432;&#x44b;&#x439;&#x442;&#x438;</trd>
    </snc>
</drv>
</art>
