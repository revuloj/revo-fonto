<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
  <kap><ofc>*</ofc><rad>adia&ubreve;</rad></kap>

<drv mrk="adiaux.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Vorto per kiu oni salutas iun, kiam oni foriras de li:
      <ekz>
        mi diris <tld/> al miaj lastaj esperoj;
      </ekz>
      <ekz>Li diris <tld/> al la mondo surtera
        <klr>(morti)</klr><fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        kapsigni <tld/>on al iu<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ni faris por la  foriro de nia prezidanto <tld/>an kunvenon.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_y;&c_v;&c_a;&c_j;</trd>
  <trd lng="de">tsch&uuml;ss</trd>
  <trd lng="en">farewell</trd>
  <trd lng="es">adios</trd>
  <trd lng="fr">adieu</trd>
  <trd lng="he">&shin;&lamed;&vav;&fmem;<klr tip="amb"> (&bet;&resh;&kaf;&tav; &pe;&resh;&yod;&dalet;&he;)</klr></trd>
  <trdgrp lng="nl"><trd>adieu</trd>,
    <trd>vaarwel</trd>,
    <trd>tot <ind>ziens</ind></trd>,
    <trd>tot <ind>kijk</ind></trd>
  </trdgrp>
  <trd lng="pl">no to <ind>cze&sacute;&cacute;</ind></trd>
  <trd lng="pt">adeus</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_shch;&c_a;&c_j;</trd>
  <trd lng="sk">zbohom</trd>
  <trd lng="sv">adj&ouml;</trd>
</drv>

<drv mrk="adiaux.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Kapsigni <tld/>on al iu<sup>Z</sup>;
      diri <tld/> al:
      <ekz>
        li <tld/>is &scirc;in, al &scirc;i kun larmoj.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_z;&c_mol;&c_v;&c_ib;&c_t;&c_a;&c_c;&c_c;&c_a;</trd>
    <trd lng="de">verabschieden</trd>
    <trdgrp lng="en">
      <trd>bid adieu</trd>,
      <trd>say goodbye</trd>,
      <trd>say farewell</trd>
    </trdgrp>  
    <trdgrp lng="es">
      <trd>decir adios</trd>,
      <trd>despedirse</trd>
    </trdgrp> 
    <trd lng="fr">faire ses <ind>adieux</ind></trd>
    <trd lng="he">&lamed;&vav;&mem;&resh; <ind>&shin;&lamed;&vav;&fmem;</ind><klr tip="ind"> (&lamed;&pe;&resh;&yod;&dalet;&he;)</klr></trd>
    <trdgrp lng="nl">
      <trd>afscheid nemen</trd>,
      <trd>vaarwel zeggen</trd>
    </trdgrp>  
    <trdgrp lng="pt">
      <trd>dizer adeus</trd>,
      <trd>despedir-se</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_o;&c_p;&c_r;&c_o;&c_shch;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
    <trd lng="sk">rozl&uacute;&ccaron;i&tcaron; sa</trd>
    <trd lng="sv">s&auml;ga <ind>farv&auml;l</ind></trd>
  </snc>
  <snc mrk="adiaux.0i.komp">
    <uzo tip="fak">KOMP</uzo>
    <fnt><lok><url ref="&KompLeks;/A.html#ADIAUxI">KompLeks</url></lok></fnt>
    <fnt><lok><url
   ref="http://www.rano.demon.co.uk/pivkomp/_adiaux.html">PIV-2Komp</url></lok></fnt>
    <dif>
      Indiki al komputilo finon de seanco kun dialoga programo.
    </dif>
    <ref tip="ant" cel="salut.0i.komp">saluti</ref>
    <ref tip="malprt" cel="seanc.0o.komp">seanco</ref>
    <trdgrp lng="de">
      <trd>abmelden</trd>,
      <trd>ausloggen</trd></trdgrp>
    <trd lng="en">logout</trd>
    <trd lng="he">&lamed;&he;&tav;<ind>&nun;&tav;&qof;</ind><klr tip="amb"> (&bet;&mem;&het;&shin;&bet;&yod;&fmem;)</klr></trd>
    <trd lng="nl">uitloggen</trd>
    <trd lng="pt"><ind>desligar</ind> o microcomputador</trd>
    <trd lng="ru">&c_v;&c_y;&c_j;&c_t;&c_i;</trd>
    <trd lng="sk">odhl&aacute;si&tcaron; sa</trd>
  </snc>
</drv>
</art>
