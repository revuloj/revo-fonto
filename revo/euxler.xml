<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: euxler.xml,v 1.8 2023/11/07 20:34:12 revo Exp $">
<kap>
  <rad>E&ubreve;ler</rad>/o
</kap>
<drv mrk="euxler.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <fnt><bib>KompLeks</bib></fnt>
  <snc>
    <dif>
      Germanlingve: <nom>Leonhard</nom> <trd lng="de">Euler</trd>, 1707-1783,
      svisa <ref tip="lst" lst="voko:historiaj_personoj" cel="matema.0isto">matematikisto</ref>:
      <ekz>
        du matematikistoj ludis gravan rolon en la vivo de <nom>Lagrange</nom>, la unua estis
        <tld/>o, kun kiu li abunde leterkorespondis, kaj kiu kura&gcirc;igis kaj inspiris
        lin en liaj matematikaj esploroj, la dua estis lia amiko <nom>d'Alembert</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=6823">Joseph-Louis Lagrange</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_O;&c_j;&c_l;&c_je;&c_r;<klr tip="amb"> (&c_L;&c_je;&c_o;&c_n;&c_a;&c_r;&c_d;)</klr></trd>
  <trd lng="en">Euler</trd>
  <trd lng="es">Euler<klr tip="amb"> (Leonhard Euler,
  matem&aacute;tico)</klr></trd>
  <trd lng="fr">Euler</trd>
  <trd lng="hu">Euler</trd>
  <trd lng="nl">Euler</trd>
  <trd lng="pl">Euler</trd>
  <trd lng="pt">Euler</trd>
  <trd lng="ru">&c_E;&c_j;&c_l;&c_je;&c_r;</trd>
  <trd lng="zh">&#x6B27;&#x62C9; <pr>&#x14D;ul&#x101;</pr></trd>
</drv>

<drv mrk="euxler.e0a">
  <kap><tld lit="e"/>a</kap>
  <snc>
    <dif>
      Iel rilatanta kun la esploroj de <tld/>o.
    </dif>
    <trdgrp lng="de">
      <trd>von <ind>Euler</ind></trd>,
      <trd>Eulersch</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>Euler['s]</trd>,
      <trd>Eulerian</trd>
    </trdgrp>
    <trd lng="es">euleriano</trd>
    <trdgrp lng="fr">
      <trd>d'<ind>Euler</ind></trd>,
      <trd>eul&eacute;rien</trd>
    </trdgrp>
    <trd lng="pl">Eulera</trd>
    <trdgrp lng="ru">
      <trd>&c_E;&c_j;&c_l;&c_je;&c_r;&c_a;</trd>,
      <trd>&c_e;&c_j;&c_l;&c_je;&c_r;&c_o;&c_v;</trd>
    </trdgrp>
  </snc>
  <snc mrk="euxler.e0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="euxler.e0a.cxeno">
      <fnt><bib>KompLeks</bib><lok>&leftquot;problemo pri la Konigsbergaj
      pontoj&rightquot;</lok></fnt>
      <dif>
        <klr>(p.p. <ref cel="cxen.0o.grafeo">&ccirc;eno</ref> a&ubreve;
        <ref cel="cikl.0o.grafeo">ciklo</ref>
        en <ref cel="grafe.0o.strukturo">grafeo</ref>)</klr>
        Tia, ke &gcirc;i precize unufoje trairas &ccirc;iun e&gcirc;on de la
        grafeo.
      </dif>
      <trd lng="de">Eulersch</trd>
      <trdgrp lng="en">
        <trd>Euler['s]</trd>,
        <trd>Eulerian</trd>
      </trdgrp>
      <trd lng="fr">eul&eacute;rien</trd>
      <trd lng="pl">Eulera</trd>
      <trd lng="ru">&c_e;&c_j;&c_l;&c_je;&c_r;&c_o;&c_v;</trd>
    </subsnc>
    <subsnc mrk="euxler.e0a.grafeo">
      <fnt><bib>KompLeks</bib><lok>&leftquot;problemo pri la Konigsbergaj
      pontoj&rightquot;</lok></fnt>
      <dif>
        <klr>(p.p. <ref cel="grafe.0o.strukturo">grafeo</ref>)</klr>
        Tia, ke ekzistas en &gcirc;i
        <tld lit="e"/>a <sncref ref="euxler.e0a.cxeno"/> ciklo.
      </dif>
      <trd lng="de">Eulersch</trd>
      <trdgrp lng="en">
        <trd>Euler['s]</trd>,
        <trd>Eulerian</trd>
      </trdgrp>
      <trd lng="fr">eul&eacute;rien</trd>
      <trd lng="pl">Eulera</trd>
      <trd lng="ru">&c_e;&c_j;&c_l;&c_je;&c_r;&c_o;&c_v;</trd>
    </subsnc>
    <subsnc mrk="euxler.e0a.diversaj">
      <refgrp tip="vid">
        <ref cel="cirkl.euxlera0o"><tld lit="e"/>a cirklo</ref>,
        <ref cel="rekt.euxlera0o"><tld lit="e"/>a rekto</ref>.
      </refgrp>
    </subsnc>
  </snc>
  <trd lng="bg">&c_o;&c_j;&c_l;&c_je;&c_r;&c_o;&c_v;&c_je;&c_n;</trd>
  <trdgrp lng="hu">
    <trd>Euler-f&eacute;le</trd>,
    <trd>Euler-</trd>
  </trdgrp>
</drv>
</art>

