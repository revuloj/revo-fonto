<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>piedestal</rad>/o
</kap>
<drv mrk="piedes.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="piedes.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Masiva <ref tip="super" cel="baz.0o.postamento">bazo</ref> de
      ekzemple kolono aŭ statuo, ofte ornamita de kornico:
      <ekz>
        laŭtvoĉa estro, suprengrimpinte <tld/>on de obelisko
        apud la pordego, kriegis
        <fnt><bib>Iŝtar</bib>, <lok>ĉapitro 22a, La fino de la
intrigoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>п'едэстал</trd>,
    <trd>падножжа</trd>,
    <trd>пастамэнт</trd>
  </trdgrp>
  <trd lng="br">sichenn<klr tip="ind"> (un delwenn)</klr></trd>
  <trd lng="de">Sockel</trd>
  <trdgrp lng="en">
    <trd>pedestal</trd>,
    <trd>base</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>piédestal</trd>,
    <trd>piédouche</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>oszlopszék</trd>,
    <trd>szobortalapzat</trd>,
    <trd>piedesztál</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>piëdestal</trd>,
    <trd>sokkel</trd>,
    <trd>voetstuk</trd>
  </trdgrp>
  <trd lng="pt">pedestal</trd>
  <trd lng="ru">пьедестал</trd>
</drv>
</art>
