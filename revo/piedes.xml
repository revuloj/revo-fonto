<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>piedestal</rad>/o
</kap>
<drv mrk="piedes.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="piedes.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Masiva <ref tip="super" cel="baz.0o.postamento">bazo</ref> de
      ekzemple kolono a&ucirc; statuo, ofte ornamita de kornico:
      <ekz>
        la&ucirc;tvo&ccirc;a estro, suprengrimpinte <tld/>on de obelisko
        apud la pordego, kriegis
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 22a, La fino de la
intrigoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Sockel</trd>
  <trdgrp lng="fr">
    <trd>pi&eacute;destal</trd>,
    <trd>pi&eacute;douche</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>oszlopsz&eacute;k</trd>,
    <trd>szobortalapzat</trd>,
    <trd>piedeszt&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>pi&euml;destal</trd>,
    <trd>sokkel</trd>,
    <trd>voetstuk</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_mol;&c_je;&c_d;&c_je;&c_s;&c_t;&c_a;&c_l;</trd>
</drv>
</art>
