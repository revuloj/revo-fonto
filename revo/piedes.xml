<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: piedes.xml,v 1.24 2024/02/17 06:43:07 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>piedestal</rad>/o
</kap>
<drv mrk="piedes.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="piedes.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Masiva <ref tip="super" cel="baz.0o.postamento">bazo</ref> de
      ekzemple kolono a&ubreve; statuo, ofte ornamita de kornico:
      <ekz>
        &scirc;i prezentis tre belan figuron sur la <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de Sfinkso
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        faru kupran lavujon kun kupra <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 30:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve;tvo&ccirc;a estro, suprengrimpinte <tld/>on de obelisko
        apud la pordego, kriegis
        <fnt>
          <bib>I&scirc;tar</bib>,
          <lok>&ccirc;apitro 22a, La fino de la intrigoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="sokl.0o">soklo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;'&c_je;&c_d;&c_e;&c_s;&c_t;&c_a;&c_l;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_n;&c_o;&c_zh;&c_zh;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_t;&c_a;&c_m;&c_e;&c_n;&c_t;</trd>
  </trdgrp>
  <trd lng="br">sichenn<klr tip="ind"> (un delwenn)</klr></trd>
  <trdgrp lng="cs">
    <trd>patka</trd>,
    <trd>piedestal</trd>,
    <trd>podstavec</trd>
  </trdgrp>
  <trd lng="de">Sockel</trd>
  <trdgrp lng="en">
    <trd>pedestal</trd>,
    <trd>base</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>pi&eacute;destal</trd>,
    <trd>pi&eacute;douche</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>oszlopsz&eacute;k</trd>,
    <trd>szobortalapzat</trd>,
    <trd>piedeszt&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x53F0;  <pr>&#x305F;&#x3044;</pr></trd>,
    <trd>&#x53F0;&#x5EA7;  <pr>&#x3060;&#x3044;&#x3056;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>pi&euml;destal</trd>,
    <trd>sokkel</trd>,
    <trd>voetstuk</trd>
  </trdgrp>
  <trd lng="pl">piedesta&lstroke;</trd>
  <trd lng="pt">pedestal</trd>
  <trd lng="ro">piedestal</trd>
  <trd lng="ru">&c_p;&c_mol;&c_je;&c_d;&c_je;&c_s;&c_t;&c_a;&c_l;</trd>
  <trd lng="sk">podstavec</trd>
  <trd lng="uk">&#x43F;&#x2019;&#x454;&#x434;&#x435;&#x441;&#x442;&#x430;&#x43B;</trd>
  <trd lng="zh">&#x5EA7;&#x5B50; <pr>zu&#xF2;zi</pr></trd>
</drv>
</art>
