<kap>
<rad>konvolvul</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="konvol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="konvol.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Convolvulus</trd>), volvegrimpanta herbo kun 
      funelformaj floroj.
   </dif>
 </snc>
 <trd lng="de">Winde</trd>
</drv>
<drv mrk="konvol.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Convolvulaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Windengew&auml;chse</trd>
</drv>
</art>
