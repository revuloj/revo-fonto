<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>konvolvul</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="konvol.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="konvol1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Convolvulus</trd>), volvegrimpanta herbo kun
      funelformaj floroj.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_r;&c_o;&c_z;&c_k;&c_a; <klr tip="amb">(&c_t;&c_r;&c_a;&c_v;&c_a;)</klr></trd>
  <trd lng="de">Winde</trd>
  <trdgrp lng="en">
    <trd>convolvulus</trd>,
    <trd>bindweed</trd>
  </trdgrp>
  <trd lng="hu">szul&aacute;k</trd>
  <trd lng="pt">conv&oacute;lvulo</trd>
  <trd lng="ru">&c_v;&c_mol;&c_ju;&c_n;&c_o;&c_k;</trd>
</drv>
</art>
