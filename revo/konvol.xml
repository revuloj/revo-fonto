<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>konvolvul</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="konvol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="konvol.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Convolvulus</trd>), volvegrimpanta herbo kun
      funelformaj floroj.
    </dif>
  </snc>
  <trd lng="de">Winde</trd>
</drv>
<drv mrk="konvol.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Convolvulaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Windengew&auml;chse</trd>
</drv>
</art>
