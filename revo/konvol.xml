<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>konvolvul</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="konvol.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="konvol1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Convolvulus</trd>), volvegrimpanta herbo kun
      funelformaj floroj.
    </dif>
  </snc>
  <trd lng="de">Winde</trd>
  <trdgrp lng="en">
    <trd>convolvulus</trd>,
    <trd>bindweed</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_mol;&c_ju;&c_n;&c_o;&c_k;</trd>
</drv>
</art>
