<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: konvol.xml,v 1.19 2023/09/01 15:46:43 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>konvolvul</rad>/o
  <fnt><bib>Fab2</bib></fnt>
</kap>
<drv mrk="konvol.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="konvol1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Convolvulus</trd>), volve grimpanta herbo kun
      funelformaj floroj:
      <ekz>
        &ccirc;e la bariloj volvi&gcirc;adis supren sova&gcirc;a lupolo kaj florantaj <tld/>oj
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        florantaj <tld/>oj kaj trunketa&jcirc;oj de rubuso en formo 
        de longaj girlandoj pendis inter la arboj
        <fnt>
          <bib>Fab2</bib>
          <lok>Sonorilo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_r;&c_o;&c_z;&c_k;&c_a; <klr tip="amb">(&c_t;&c_r;&c_a;&c_v;&c_a;)</klr></trd>
  <trd lng="de">Winde</trd>
  <trdgrp lng="en">
    <trd>convolvulus</trd>,
    <trd>bindweed</trd>,
    <trd>morning glory</trd>
  </trdgrp>
  <trd lng="fr">liseron</trd>
  <trd lng="hu">szul&aacute;k</trd>
  <trdgrp lng="ja">
    <trd>&#x30BB;&#x30A4;&#x30E8;&#x30A6;&#x30D2;&#x30EB;&#x30AC;&#x30AA;</trd>,
    <trd>&#x30D2;&#x30EB;&#x30AC;&#x30AA;</trd>
  </trdgrp>
  <trd lng="pl">pow&oacute;j</trd>
  <trd lng="pt">conv&oacute;lvulo</trd>
  <trd lng="ro">rochi&#x21B;a-r&acirc;ndunelei</trd>
  <trd lng="ru">&c_v;&c_mol;&c_ju;&c_n;&c_o;&c_k;</trd>
</drv>
</art>
