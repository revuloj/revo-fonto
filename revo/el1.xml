<kap>
  <rad>el</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="el1.0o">
  <kap><tld/>o</kap>
  <snc mrk="el1.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Angla <ref tip="super" cel="bier.0o.KUI">biero</ref>, hela kaj
      malforta:
      <ekz>
        &leftquot;<klr>...</klr> vi povas havi glason da
        <tld/>o&rightquot;, mi diris
        <fnt><aut>R. L. Stevenson, trad. A. Goodheir</aut>,
        <vrk>Forkaptita</vrk>, <lok>Kardo, 1976</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>angla biero</ke>.
  </rim>

  <trd lng="be">&c_e;&c_l;&c_mol;</trd>
  <trd lng="ca">cervesa ale</trd>
  <trd lng="cs">ale</trd>
  <trd lng="de">Ale</trd>
  <trd lng="en">ale</trd>
  <trd lng="fr">ale<klr tip="ind"> (bi&egrave;re anglaise)</klr></trd>
  <trdgrp lng="he">
    <trd>&#1513;&#1497;&#1499;&#1512;</trd>,
    <trd>&#1489;&#1497;&#1512;&#1492; &#1488;&#1504;&#1490;&#1500;&#1497;&#1514;</trd>
  </trdgrp>
  <trd lng="hu">ale<klr tip="ind"> (angol s&ouml;r)</klr></trd>
  <trd lng="ja">&#12456;&#12540;&#12523;  <pr>&#12360;&#12540;&#12427;</pr></trd>
  <trd lng="nl">ale</trd>
  <trd lng="pl">ale<klr tip="amb"> (piwo)</klr></trd>
  <trdgrp lng="pt">
    <trd>ale</trd>,
    <trd>cerveja ale</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_l;&c_mol;</trd>
  <trd lng="sk">ale</trd>
  <trd lng="uk">&#1077;&#1083;&#1100;</trd>
</drv>
