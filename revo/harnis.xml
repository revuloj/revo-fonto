<vortaro>
<art mrk="$Id$">
<kap>
  <rad>harnis</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="harnis.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">AGR</uzo>
    <uzo tip="fak">SPO</uzo>
    <dif>
      Rimena ekipa&jcirc;o de rajdo- a&ubreve; &scirc;ar&gcirc;o-besto: bridoj, kondukiloj
      kaj
      aliaj:
      <ekz>
        pompa <tld/>o de &ccirc;evalo
        <fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro 55a, p..
        340a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_b;&c_r;&c_u;&c_ja;</trd>
  <trd lng="en">harness</trd>
  <trd lng="es">arn&eacute;s</trd>
  <trd lng="fa">&a_s;&a_A;&a_z; &a_w; &a_b;&a_r;&f_g;</trd>
  <trd lng="fr">harnais</trd>
  <trdgrp lng="hu">
    <trd>h&aacute;m</trd>,
    <trd>l&oacute;szersz&aacute;m</trd>
  </trdgrp>
  <trd lng="it">finimenti</trd>
  <trdgrp lng="pt">
    <trd>arn&ecirc;s</trd>,
    <trd>arreios</trd>,
    <trd>arreio</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_p;&c_r;&c_ja;&c_zh;&c_mol;</trd>,
    <trd>&c_s;&c_b;&c_r;&c_u;&c_ja;</trd>
  </trdgrp>
</drv>
</art>

