<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: harnis.xml,v 1.14 2023/11/15 16:45:17 revo Exp $">
<kap>
  <rad>harnis</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="harnis.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="harnis.0o.RAJ">
    <dif>
      <ref tip="dif" cel="jung.0ilaro.RAJ">Jungilaro</ref>,
      eventuale kun batalprotekta karapaco:
      <ekz>
        pompa <tld/>o de &ccirc;evalo
        <fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro 55a, p..
        340a</lok></fnt>;
      </ekz>
      <ekz>
        <nom>Alan Colk</nom> kaj <nom>Michael Hill</nom> de A&ubreve;stralio aldonis harnison kaj enmezan
        pendopunkton kiuj igis la aktivecon sekuran sporton
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=539477">Svingrajdado</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/c/c5/Pferdeharnisch.JPG/640px-Pferdeharnisch.JPG" prm="PD"><tld/>a
    karapaco
        <fnt>
          <aut>A. Ocram, propra verko</aut>
          <lok><url ref="&WCW;/File:Pferdeharnisch.JPG">&WCW;/File:Pferdeharnisch.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_b;&c_r;&c_u;&c_ja;</trd>,
    <trd>&c_v;&c_u;&c_p;&c_r;&c_a;&c_zh;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Geschirr <klr>(Zugtier)</klr></trd>,
    <trd>Pferdeharnisch</trd>
  </trdgrp>
  <trd lng="zh">&#x633D;&#x5177; <pr>w&#x1CE;nj&#xF9;</pr></trd>
</drv>
</art>

