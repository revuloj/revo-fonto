<kap>
  <ofc>9</ofc>
  <rad>salikok</rad>/o
  <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="salikok.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="salikok.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Na&gcirc;a <ref tip="lst" cel="krust.0ulo" lst="voko:krustuloj">krustulo</ref> kun longa korpo 
      kaj kiraso el sinsekvaj loboj:
      <ekz>
        movplena popolsvarmo elfluas el la fa&ubreve;koj de la
        &leftquot;metro&rightquot; kaj
        invadas akceptemajn drinkejojn por gustumi revigligan vermuton 
        kaj skarlatajn <tld/>ojn kun pimenta sa&ubreve;co
        <fnt>
        <aut>J. Ribillard</aut>,
        <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr36/majorkaj.html">Majorkaj
        azule&hcirc;oj</url></vrk>,
        <lok>Nica Literatura Revuo, 1958-07 &gcirc;is 08, numero 3/6, p.
        
        211-217a</lok> 
        </fnt>;
      </ekz>
      <ekz>
        lupfi&scirc;o kun pureo de <tld/>oj
        <fnt>
        <vrk><url ref="http://ebg.skynetblogs.be/">Esperanto
        en (kaj ekster) Bruselo</url></vrk>,
        <lok>2003-11-12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_e;&c_v;&c_e;&c_t;&c_k;&c_a;</trd>
  <trd lng="de">Garnele</trd>
  <trd lng="en">shrimp</trd>
  <trd lng="es">gamba</trd>
  <trd lng="fr">crevette</trd>
  <trd lng="id">udang</trd>
  <trd lng="it">gamberetto</trd>
  <trdgrp lng="ja">
    <trd>&#x30B3;&#x30A8;&#x30D3;</trd>,
    <trd>&#x30A8;&#x30D3;  <pr>&#x3048;&#x3073;</pr></trd>
  </trdgrp>
  <trd lng="nl">garnaal</trd>
  <trd lng="pl">krewetka</trd>
  <trd lng="pt">camar&atilde;o</trd>
  <trd lng="uk">&#x43A;&#x440;&#x435;&#x432;&#x435;&#x442;&#x43A;&#x430;</trd>
  <trdgrp lng="zh">
    <trd>&#x867E; <pr>xi&#x101;</pr></trd>,
    <trd>&#x8766; <pr>xi&#x101;</pr></trd>,
    <trd>&#x867E;&#x5B50; <pr>xi&#x101;z&#x1D0;</pr></trd>,
    <trd>&#x8766;&#x5B50; <pr>xi&#x101;z&#x1D0;</pr></trd>,
    <trd>&#x5C0F;&#x867E; <pr>xi&#x1CE;oxi&#x101;</pr></trd>,
    <trd>&#x5C0F;&#x8766; <pr>xi&#x1CE;oxi&#x101;</pr></trd>,
    <trd>&#x660E;&#x867E; <pr>m&#xED;ngxi&#x101;</pr></trd>,
    <trd>&#x660E;&#x8766; <pr>m&#xED;ngxi&#x101;</pr></trd>,
    <trd>&#x9B75; <pr>f&#xE9;n</pr></trd>
  </trdgrp>
</drv>
</art>

