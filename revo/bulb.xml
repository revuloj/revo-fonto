<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>bulb</rad>/o
</kap>
<drv mrk="bulb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bulb.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Ordinare subtera diskforma <ref tip="super" cel="tig.0o">tigo</ref>
      kun multaj, dense sidantaj, skvamformaj, karnecaj folioj:
      <ekz>
        kesto kun neordinaraj floraj bulboj
        <fnt>(<aut>Andersen, trad. Zamenhof</aut>: <vrk>Fabeloj,
        Historio el la dunoj</vrk>)</fnt>;
      </ekz>
      <ekz>
        <tld/>o de lilio, de tulipo, de cepo.
      </ekz>
    </dif>
  </snc>
  <snc mrk="bulb.0o.cepo">
    <uzo tip="fak">KUI</uzo>
    <dif>
      La <tld/>o de <ref tip="malprt" cel="cep.0o">cepo</ref>:
      <ekz>
        la kukumojn kaj la melonojn kaj la poreojn kaj la bulbojn kaj la ajlojn
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Nombroj 11, 5</lok>)</fnt>;
      </ekz>
      <ekz>
        mi ricevis bulbojn kaj rezine odoretantan vinon
        <fnt>(<aut>Andersen, trad. Zamenhof</aut>: <vrk>Fabeloj,
        Ligo de amikeco</vrk>)</fnt>;
      </ekz>
    </dif>
  </snc>
  <snc mrk="bulb.0o.formo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <tld/>oforma objekto:
      <ekz>
        oni de malproksime rekonis la <tld/>ojn de
        la rusaj pre&gcirc;ejoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Zwiebel</trd>
  <trd lng="ru">&c_l;&c_u;&c_k;&c_o;&c_v;&c_i;&c_c;&c_a;</trd>
</drv>
</art>
