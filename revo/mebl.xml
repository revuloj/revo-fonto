<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mebl</rad>/o
</kap>
<drv mrk="mebl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="mebl.0o.MEBL">
    <dif>
      Movebla objekto, plej ofte farita el ligno kaj servanta por praktika
      a&ubreve; ornama uzo en lo&gcirc;ejo:
      <ekz>
        vasta lo&gcirc;ejo, multekostaj <tld/>oj, lukso<fnt><aut>Julian
        Modest</aut>: <vrk>En la ombro de la silento</vrk>,
        <bib>Monato</bib><lok>jaro 1997a, numero 3a, p. 14a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_d;&c_m;&c_je;&c_t; <ind>&c_m;&c_e;&c_b;&c_l;&c_ib;</ind></trd>
  <trd lng="de">M&ouml;bel</trd>
  <trd lng="cs">n&aacute;bytek</trd>
  <trd lng="en">a piece of <ind>furniture</ind></trd>
  <trdgrp lng="fr">
    <trd>meuble</trd>
  </trdgrp>
  <trd lng="hu">b&uacute;tor</trd>
  <trd lng="nl">meubel</trd>
  <trd lng="ru">&c_m;&c_je;&c_b;&c_je;&c_l;&c_mol; <klr tip="amb">(&c_p;&c_r;&c_je;&c_d;&c_m;&c_je;&c_t; &c_m;&c_je;&c_b;&c_je;&c_l;&c_i;)</klr></trd>
  <trd lng="sv">m&ouml;bel</trd>
</drv>

<drv mrk="mebl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="mebl.0i.MEBL">
    <dif>
      Provizi je <tld/>oj:
      <ekz>
        multaj vila&gcirc;anoj konstruis jam belajn vilaojn, <tld/>itajn kaj
        provizitajn per akvo<fnt><aut>Bardhyl Selimi</aut>:
        <vrk>Voja&gcirc;impresoj, somero 2000</vrk>, <bib>Monato</bib><lok>jaro
        2001a, numero 1a, p. 26a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_b;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">m&ouml;blieren</trd>
  <trdgrp lng="fr">
    <trd>meubler</trd>
  </trdgrp>
  <trd lng="hu">beb&uacute;toroz</trd>
  <trd lng="nl">meubileren</trd>
  <trd lng="ru">&c_m;&c_je;&c_b;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">m&ouml;blera</trd>
</drv>

<drv mrk="mebl.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="mebl.0aro.MEBL">
    <dif>
      &Ccirc;iuj <tld/>oj de &ccirc;ambro, de lo&gcirc;ejo; sortimento de kunharmoniaj
      <tld/>oj:
      <ekz>
        mi &scirc;an&gcirc;is la tutan <tld/>aron sennecese<fnt><aut>Higinio
        Garc&iacute;a</aut>: <vrk>El la atesto de Mustafao Vanesavir',
        submastro</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 3a, p.
        25a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_e;&c_b;&c_l;&c_ja;</trd>,
    <trd>&c_m;&c_e;&c_b;&c_l;&c_ib;&c_r;&c_o;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ameublement</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_b;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_m;&c_je;&c_b;&c_l;&c_i;&c_r;&c_o;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="mebl.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="mebl.0isto.MEBL">
    <dif>
      Faranto, produktanto de kofroj, tabloj, se&gcirc;oj, &scirc;rankoj, litoj kaj
      similaj.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_b;&c_je;&c_l;&c_mol;&c_sh;&c_ch;&c_y;&c_k;</trd>
  <trdgrp lng="de">
    <trd>Schreiner</trd>,
    <trd>Tischler</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ensemblier</trd>,
    <trd>menuisier</trd>
  </trdgrp>
  <trd lng="hu">b&uacute;torasztalos</trd>
  <trdgrp lng="nl">
    <trd>meubelmaker</trd>,
    <trd>schrijnwerker</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_b;&c_je;&c_l;&c_mol;&c_shch;&c_i;&c_k;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_l;&c_ja;&c_r;</trd>
  </trdgrp>
</drv>
</art>
