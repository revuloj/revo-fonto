<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mebl</rad>/o
</kap>
<drv mrk="mebl.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Movebla objekto, plej ofte farita el ligno kaj servanta por praktika
      a&ubreve; ornama uzo en lo&gcirc;ejo.
    </dif>
  </snc>
  <trd lng="de">M&ouml;bel</trd>
  <trd lng="cs">n&aacute;bytek</trd>
  <trd lng="en">a piece of <ind>furniture</ind></trd>
  <trd lng="nl">meubel</trd>
  <trd lng="ru">&c_m;&c_je;&c_b;&c_je;&c_l;&c_mol; <klr>(&c_p;&c_r;&c_je;&c_d;&c_m;&c_je;&c_t; &c_m;&c_je;&c_b;&c_je;&c_l;&c_i;)</klr></trd>
  <trd lng="sv">m&ouml;bel</trd>
</drv>

<drv mrk="mebl.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Provizi je <tld/>oj.
    </dif>
  </snc>
  <trd lng="de">m&ouml;blieren</trd>
  <trd lng="nl">meubileren</trd>
  <trd lng="ru">&c_m;&c_je;&c_b;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">m&ouml;blera</trd>
</drv>

<drv mrk="mebl.0isto">
  <kap>
    <tld/>isto
  </kap>
  <snc>
    <dif>
      Tiu, kiu faras tablojn, se&gcirc;ojn, &scirc;rankojn, litojn ks.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Schreiner</trd>,
    <trd>Tischler</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>meubelmaker</trd>,
    <trd>schrijnwerker</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_b;&c_je;&c_l;&c_mol;&c_shch;&c_i;&c_k;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_l;&c_ja;&c_r;</trd>
  </trdgrp>
</drv>
</art>
