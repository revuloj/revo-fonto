<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>salvador</rad>/o
</kap>

<drv mrk="salvador.0o">
  <kap>
    <tld lit="S"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="salvador.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la centra <ref tip="malprt"
      cel="amerik.A0o">Ameriko</ref> kun &ccirc;efurbo San-Salvadoro.
      Teritorio de Respubliko Salvadoro.
      ISO-3166 kodo: SV.
      &Gcirc;i limas kun
      <ref tip="vid" cel="gvatemal.0o.GEOG">Gvatemalo</ref>,
      <ref tip="vid" cel="hondur.0o.GEOG">Honduro</ref>
      kaj <ref tip="vid" cel="pacifik.0o">Pacifiko</ref>.
    </dif>
    <trdgrp lng="pl">
      <trd>Salwador</trd>,
      <trd>Republika <ind>Salwadoru</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

