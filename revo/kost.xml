<drv mrk="kost.cxia0e">
  <kap>&ccirc;ia<tld/>e</kap>
  <snc>
    <dif>
      Firme provante, e&ccirc; kun granda peno, ofero, elspezo:
      <ekz>
        la vic-re&gcirc;o ordonis <klr>[&#8230;]</klr> eltrovi &ccirc;ia<tld/>e, kiu li
        estas
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 64a, p.
        765a</lok></fnt>;
      </ekz>
      <ekz>
        la generalo <nom>Erich Ludendorff</nom> sendis al la frontoj ordonon la
        24an de 
        oktobro 1918 rezisti &ccirc;ia<tld/>e, kio pliigis kompreneble la 
        nombro<klr>[n]</klr> de mortintoj, vunditoj kaj suferado
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=11951">Unua
          mondmilito</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ju;&c_b;&c_y;&c_m; &c_k;&c_o;&c_sh;&c_t;&c_a;&c_m;</trd>
  <trd lng="ca">a tot preu</trd>
  <trd lng="fr">&agrave; tout prix</trd>
  <trdgrp lng="hu">
    <trd>minden <ind>&aacute;ron</ind></trd>,
    <trd>b&aacute;rmi <ind>&aacute;ron</ind></trd>
  </trdgrp>
  <trd lng="pl">za wszelk&aogonek; cen&eogonek;</trd>     
  <trd lng="ro">cu orice pre&#x21B;</trd>
</drv>

<drv mrk="kost.multe0a">
  <kap><ofc>*</ofc>multe<tld/>a</kap>
  <snc mrk="kost.multe0a.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Altpreza, kostanta grandan monsumon:
      <ekz>
        multe<tld/>a &scirc;tono
        <fnt><bib>F</bib>, <lok>&FE; 15</lok></fnt>;
      </ekz>
      <ekz>
        sur &ccirc;iuj altaroj brulis bonodora oleo en multe<tld/>aj ar&gcirc;entaj
        lampoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        palisandra kanapeto, tegita per multe<tld/>a damasko
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kar.0a.EKON">kara <sncref/></ref>,
      <ref cel="kost.0a.multekosta"><tld/>a</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_r;&c_a;&c_g;&c_ib;</trd>,
    <trd>&c_k;&c_a;&c_sh;&c_t;&c_o;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_k;&c_malmol;&c_p;</trd>
  <trd lng="ca">car</trd>
  <trd lng="cs">drah&yacute;</trd>
  <trd lng="de">teuer</trd>
  <trdgrp lng="en">
    <trd>costly</trd>,
    <trd>dear</trd>,
