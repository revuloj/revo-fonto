<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kost</rad>/i
</kap>

<drv mrk="kost.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kost.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Esti akirebla nur je difinita prezo:
      <ekz>
        kvar metroj da tiu &ccirc;i &scirc;tofo <tld/>as na&ubreve; frankojn
        <fnt><bib>F</bib>, <lok>&FE; 14</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i dek markojn po metro, &ccirc;iumetre;
      </ekz>
      <ekz>
        la reklamo <tld/>is dek mil dolarojn;
      </ekz>
      <ekz>
        <tld/>i neniom, nenion.
      </ekz>
    </dif>
    <trd lng="en">cost</trd>
  </snc>
  <snc mrk="kost.0i.FIG">
    <dif>
      Esti akirebla, atingebla, nur dank' al iu peno:
      <ekz>
        kio multe <tld/>as, multe valoras<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        senpaga estas la morto, sed &gcirc;i <tld/>as la vivon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la milito <tld/>is la vivon de multaj miloj da homoj;
      </ekz>
      <ekz>
        tiu libro <tld/>is al mi multajn laborojn kaj lacojn;
      </ekz>
      <ekz>
        la konfeso <tld/>is al li multe<fnt>B</fnt>;
      </ekz>
      <ekz>
        &ccirc;iu pa&scirc;o anta&ubreve;en <tld/>as doloron kaj suferon<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_sh;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">costar</trd>
  <trd lng="cs">st&aacute;t (o cen&ecaron;)</trd>
  <trd lng="de">kosten</trd>
  <trd lng="en">cost</trd>
  <trd lng="es">costar<klr tip="amb"> (tener precio)</klr></trd>
  <trd lng="fr">co&ucirc;ter</trd>
  <trd lng="hu">ker&uuml;l <klr tip="amb">(pl. p&eacute;nzbe,
  f&aacute;rads&aacute;gba)</klr></trd>
  <trdgrp lng="pt">
    <trd>custar <klr tip="amb">(ter determinado valor)</klr></trd>,
    <trd>importar em</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_o;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sv">kosta</trd>
</drv>

<drv mrk="kost.0a">
  <kap><tld/>a</kap>
  <snc mrk="kost.0a.multekosta">
    <ref tip="dif" cel="kost.multe0a">multe<tld/>a</ref>
    <ekz>
      la produktado de dokumentoj estos pli simpla kaj malpli <tld/>a
      <fnt>
      <aut>Johano Rapley</aut>,
      <vrk>Kiom valoras &leftquot;aliaj lingvoj&rightquot;?</vrk>,
      <bib>Monato</bib>,
      <lok>jaro 2001a, numero 12a, p. 11a</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_r;&c_a;&c_g;&c_ib;</trd>,
    <trd>&c_k;&c_a;&c_sh;&c_t;&c_o;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_k;&c_malmol;&c_p;</trd>
  <trd lng="ca">car</trd>
  <trd lng="de">teuer</trd>
  <trdgrp lng="en">
    <trd>costly</trd>,
    <trd>dear</trd>,
    <trd>expensive</trd>,
    <trd>high-priced</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cher<klr> (co&ucirc;teux)</klr></trd>,
    <trd>co&ucirc;teux</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&ouml;lts&eacute;ges</trd>,
    <trd>dr&aacute;ga</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_o;&c_r;&c_o;&c_g;&c_o;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_i;&c_j;</trd>
  <trd lng="sv">dyr</trd>
</drv>

<drv mrk="kost.0ede">
  <kap><tld/>e de</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="malgra.0">Malgra&ubreve;</ref> iu neevitata peno,
      forofero a&ubreve; malavanta&gcirc;o:
      <ekz>
        ili lernas paroli la &ccirc;irka&ubreve;an lingvon, <tld/>e de konstanta
        <klr>...</klr> korektado
        <fnt><bib>BonaLingvo</bib>, <lok>p. 69a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>al preu de</trd>,
    <trd>a costa de</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>au <ind>prix de</ind><klr> (inconv&eacute;nient)</klr></trd>,
    <trd>en <ind>subissant</ind><klr> (inconv&eacute;nient)</klr></trd>
  </trdgrp>
  <trd lng="hu">(vmi) <ind>&aacute;r&aacute;n</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_c;&c_je;&c_n;&c_o;&c_j;<klr> (&c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>,
    <trd>&c_z;&c_a; &c_s;&c_ch;&c_jo;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="kost.0o">
  <kap><tld/>o</kap>
  <snc mrk="kost.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Baza monsumo a&ubreve; ofero necesa por produkti, havigi, akiri ion:
      <ekz>
        la <tld/>oj de la voja&gcirc;o;
      </ekz>
      <ekz>
        ne valoras la faro la <tld/>on de l' preparo
        (<refgrp tip="vid">
          <ref cel="kandel.0o">kandelo</ref>,
          <ref cel="fel.0o">felo</ref>
        </refgrp>)<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        per mia propra <tld/>opago mi fari&gcirc;is sa&gcirc;a.
      </ekz>
    </dif>
    <ref tip="vid" cel="prez.0o">prezo</ref>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_sh;&c_t;</trd>
  <trdgrp lng="ca">
    <trd>cost</trd>,
    <trd>preu</trd>
  </trdgrp>
  <trd lng="de">Kosten</trd>
  <trd lng="fr">co&ucirc;t</trd>
  <trd lng="hu">k&ouml;lts&eacute;g</trd>
  <trd lng="ru">&c_s;&c_t;&c_o;&c_i;&c_m;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sv">kostnad</trd>
</drv>

<drv mrk="kost.cxia0e">
  <kap>&ccirc;ia<tld/>e</kap>
  <snc>
    <dif>
      Firme provante, e&ccirc; kun granda peno, ofero, elspezo:
      <ekz>
        la vic-re&gcirc;o ordonis <klr>...</klr> eltrovi &ccirc;ia<tld/>e, kiu li
        estas
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 64a, p.
        765a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">a tot preu</trd>
  <trd lng="fr">&agrave; tout prix</trd>
  <trdgrp lng="hu">
    <trd>minden <ind>&aacute;ron</ind></trd>,
    <trd>b&aacute;rmi <ind>&aacute;ron</ind></trd>
  </trdgrp>
</drv>

<drv mrk="kost.multe0a">
  <kap><ofc>*</ofc>multe<tld/>a</kap>
  <snc mrk="kost.multe0a.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Altapreza, kostanta grandan monsumon:
      <ekz>
        multe<tld/>a &scirc;tono
        <fnt><bib>F</bib>, <lok>&FE; 15</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kar.0a.EKON">kara <sncref/></ref>
      <ref cel="kost.0a.multekosta"><tld/>a</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_r;&c_a;&c_g;&c_ib;</trd>,
    <trd>&c_k;&c_a;&c_sh;&c_t;&c_o;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_k;&c_malmol;&c_p;</trd>
  <trd lng="ca">car</trd>
  <trd lng="de">teuer</trd>
  <trdgrp lng="en">
    <trd>costly</trd>,
    <trd>dear</trd>,
    <trd>expensive</trd>,
    <trd>high-priced</trd>
  </trdgrp>
  <trd lng="es">caro<klr tip="amb"> (precio alto)</klr></trd>
  <trdgrp lng="fr">
    <trd>cher<klr> (co&ucirc;teux)</klr></trd>,
    <trd>co&ucirc;teux</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&ouml;lts&eacute;ges</trd>,
    <trd>dr&aacute;ga</trd>
  </trdgrp>
  <trd lng="nl">duur</trd>
<trdgrp lng="oc">
  <trd>car</trd>,
  <trd>caresti&oacute;s</trd>
</trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_r;&c_o;&c_g;&c_o;&c_j; <klr tip="amb">(&c_o; &c_c;&c_je;&c_n;&c_je;)</klr></trd>,
    <trd>&c_d;&c_o;&c_r;&c_o;&c_g;&c_o;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">dyr</trd>
</drv>
</art>
