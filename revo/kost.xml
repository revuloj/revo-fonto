<drv mrk="kost.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kost.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Esti akirebla nur je difinita prezo:
      <ekz>
        kvar metroj da tiu &ccirc;i &scirc;tofo <tld/>as na&ubreve; frankojn
        <fnt><bib>F</bib>, <lok>&FE; 14</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i dek markojn po metro, &ccirc;iumetre;
      </ekz>
      <ekz>
        la reklamo <tld/>is dek mil dolarojn;
      </ekz>
      <ekz>
        <tld/>i neniom, nenion.
      </ekz>
    </dif>
    <trd lng="en">cost</trd>
  </snc>
  <snc mrk="kost.0i.FIG">
    <dif>
      Esti akirebla, atingebla, nur dank' al iu peno:
      <ekz>
        kio multe <tld/>as, multe valoras<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        senpaga estas nur la morto, sed &gcirc;i <tld/>as la vivon
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la milito <tld/>is la vivon de multaj miloj da homoj;
      </ekz>
      <ekz>
        tiu libro <tld/>is al mi multajn laborojn kaj lacojn;
      </ekz>
      <ekz>
        la konfeso <tld/>is al li multe<fnt>B</fnt>;
      </ekz>
      <ekz>
        &ccirc;iu pa&scirc;o anta&ubreve;en <tld/>as doloron kaj suferon
        <fnt>
          <aut>L. L. Zamenhof</aut>
          <vrk><url ref="&LOdE;Novaj/Nov09-29.htm">Respondoj al
          amikoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_sh;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">costar</trd>
  <trdgrp lng="cs">
    <trd>ko&scaron;tovat</trd>,
    <trd>m&iacute;t cenu</trd>,
    <trd>st&aacute;t (o cen&ecaron;)</trd>
  </trdgrp>
  <trd lng="de">kosten</trd>
  <trd lng="en">cost</trd>
  <trd lng="es">costar<klr tip="amb"> (tener precio)</klr></trd>
  <trd lng="fr">co&ucirc;ter</trd>
  <trd lng="hu">ker&uuml;l <klr tip="amb">(pl. p&eacute;nzbe,
  f&aacute;rads&aacute;gba)</klr></trd>
  <trd lng="id">ber<ind>harga</ind></trd>
  <trd lng="ne">&#2354;&#x93E;&#2327;&#2340;</trd>
  <trd lng="pl">kosztowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>custar <klr tip="amb">(ter determinado valor)</klr></trd>,
    <trd>importar em</trd>
  </trdgrp>
  <trd lng="ro">costa</trd>
  <trd lng="ru">&c_s;&c_t;&c_o;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>ko&scaron;tova&tcaron;</trd>,
    <trd>ma&tcaron; cenu</trd>,
    <trd>st&aacute;&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">kosta</trd>
</drv>

<drv mrk="kost.0a">
  <kap><tld/>a</kap>
  <snc mrk="kost.0a.multekosta">
    <dif>
      Altpreza a&ubreve; postulanta grandan penon:
      <ekz>
        la pretigado de vortaroj returnitaj estas laboro tre facila kaj
        malmulte <tld/>a
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        la produktado de dokumentoj estos pli simpla kaj malpli <tld/>a
        <fnt>
        <aut>Johano Rapley</aut>,
        <vrk>Kiom valoras &leftquot;aliaj lingvoj&rightquot;?</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2001a, numero 12a, p. 11a</lok>
        </fnt>.
      </ekz>
   </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_r;&c_a;&c_g;&c_ib;</trd>,
    <trd>&c_k;&c_a;&c_sh;&c_t;&c_o;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_k;&c_malmol;&c_p;</trd>
  <trd lng="ca">car</trd>
  <trdgrp lng="cs">
    <trd>cenn&yacute;</trd>,
    <trd>drah&yacute;</trd>
  </trdgrp>
  <trd lng="de">teuer</trd>
  <trdgrp lng="en">
    <trd>costly</trd>,
    <trd>dear</trd>,
    <trd>expensive</trd>,
    <trd>high-priced</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cher<klr> (co&ucirc;teux)</klr></trd>,
    <trd>co&ucirc;teux</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&ouml;lts&eacute;ges</trd>,
    <trd>dr&aacute;ga</trd>
  </trdgrp>
  <trd lng="id">mahal</trd>
  <trd lng="pl">kosztowny</trd>
  <trdgrp lng="ro">
    <trd>scump</trd>,
    <trd>drag</trd>,
    <trd>luxos</trd>
  </trdgrp> 
  <trd lng="ru">&c_d;&c_o;&c_r;&c_o;&c_g;&c_o;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>cenn&yacute;</trd>,
    <trd>drah&yacute;</trd>
  </trdgrp>
  <trd lng="sv">dyr</trd>
</drv>

<drv mrk="kost.0ede">
  <kap><tld/>e de</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="kost.0ede.spite">
    <dif>
      <klr>(io)</klr> <ref tip="super" cel="malgra.0">Malgra&ubreve;</ref> iu
      neevitata peno,
      forofero a&ubreve; malavanta&gcirc;o:
      <ekz>
        ili lernas paroli la &ccirc;irka&ubreve;an lingvon, <tld/>e de konstanta
        <klr>[&#8230;]</klr> korektado
        <fnt><bib>BonaLingvo</bib>, <lok>p. 69a</lok></fnt>;
      </ekz>
      <ekz>
        la poemoj en la kolekto estas &leftquot;videble&#x201D; mulditaj al la celata
        versformo iugrade <tld/>e de la enhavo
        <fnt>
          <bib>Monato</bib>
          <aut>Pejno Simono</aut>
          <vrk><url ref="&Monato;/2013/010449.php">Versa&jcirc;oj
          elver&scirc;itaj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="kost.0ede.je">
    <dif>
      <klr>(iu)</klr> Je <tld/>oj de:
      <ekz>
        en nuna Japanio maljunuloj havas sufi&ccirc;e agrablan socialan
        sekurecon <tld/>e de gejunuloj
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2015/011554.php">Post ni,
          la diluvo!</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">auf <ind>Kosten</ind> von</trd>
  </snc>
  <trdgrp lng="ca">
    <trd>al preu de</trd>,
    <trd>a costa de</trd>
  </trdgrp>
  <trd lng="cs">za cenu</trd>
  <trdgrp lng="fr">
    <trd>au <ind>prix de</ind><klr> (inconv&eacute;nient)</klr></trd>,
    <trd>en <ind>subissant</ind><klr> (inconv&eacute;nient)</klr></trd>
  </trdgrp>
  <trd lng="hu">(vmi) <ind>&aacute;r&aacute;n</ind></trd>
  <trd lng="pl">kosztem</trd>     
  <trd lng="ro">&icirc;n detrimentul</trd>
  <trdgrp lng="ru">
    <trd>&c_c;&c_je;&c_n;&c_o;&c_j;<klr> (&c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>,
    <trd>&c_z;&c_a; &c_s;&c_ch;&c_jo;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">za cenu</trd>
</drv>

