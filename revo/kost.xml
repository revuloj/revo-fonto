<drv mrk="kost.0ede">
  <kap><tld/>e de</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="kost.0ede.spite">
    <dif>
      <klr>(io)</klr> <ref tip="super" cel="malgra.0">Malgra&ubreve;</ref> iu
      neevitata peno,
      forofero a&ubreve; malavanta&gcirc;o:
      <ekz>
        ili lernas paroli la &ccirc;irka&ubreve;an lingvon, <tld/>e de konstanta
        <klr>[&#8230;]</klr> korektado
        <fnt><bib>BonaLingvo</bib>, <lok>p. 69a</lok></fnt>;
      </ekz>
      <ekz>
        la poemoj en la kolekto estas &leftquot;videble&#8221; mulditaj al la celata
        versformo iugrade <tld/>e de la enhavo
        <fnt>
          <bib>Monato</bib>
          <aut>Pejno Simono</aut>
          <vrk><url ref="&Monato;/2013/010449.php">Versa&jcirc;oj
          elver&scirc;itaj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="kost.0ede.je">
    <dif>
      <klr>(iu)</klr> Je <tld/>oj de:
      <ekz>
        en nuna Japanio maljunuloj havas sufi&ccirc;e agrablan socialan
        sekurecon <tld/>e de gejunuloj
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2015/011554.php">Post ni,
          la diluvo!</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_o;&c_sh;&c_t;&c_a;&c_m;</trd>,
    <trd>&c_c;&c_a;&c_n;&c_o;&c_j;</trd>,
    <trd>&c_z;&c_a; <ind>&c_k;&c_o;&c_sh;&c_t;</ind></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>al preu de</trd>,
    <trd>a costa de</trd>
  </trdgrp>
  <trd lng="cs">za cenu</trd>
  <trdgrp lng="de">
    <trd>auf <ind>Kosten</ind> von</trd>,
    <trd>um den <ind>Preis</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>au <ind>prix de</ind><klr> (inconv&eacute;nient)</klr></trd>,
    <trd>en <ind>subissant</ind><klr> (inconv&eacute;nient)</klr></trd>
  </trdgrp>
  <trd lng="hu">(vmi) <ind>&aacute;r&aacute;n</ind></trd>
  <trdgrp lng="ja">
    <trd>&#65374;&#12398;&#20195;&#20385;&#12391;  [&#65374;<ind>&#12398;&#12384;&#12356;&#12363;&#12391;</ind>]</trd>,
    <trd>&#65374;&#12392;&#24341;&#12365;&#25563;&#12360;&#12395;
    [&#65374;<ind>&#12392;&#12402;&#12365;&#12363;&#12360;&#12395;</ind>]</trd>
  </trdgrp>
  <trd lng="pl">kosztem</trd>
  <trd lng="ro">&icirc;n detrimentul</trd>
  <trdgrp lng="ru">
    <trd>&c_c;&c_je;&c_n;&c_o;&c_j;<klr> (&c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>,
    <trd>&c_z;&c_a; &c_s;&c_ch;&c_jo;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">za cenu</trd>
</drv>

