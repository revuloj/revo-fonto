    <trd>dr&aacute;ga</trd>
  </trdgrp>
  <trd lng="id">mahal</trd>
  <trd lng="ja">&#39640;&#20385;&#12394;  [<ind>&#12371;&#12358;&#12363;&#12394;</ind>]</trd>
  <trd lng="pl">kosztowny</trd>
  <trdgrp lng="ro">
    <trd>scump</trd>,
    <trd>drag</trd>,
    <trd>luxos</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_o;&c_r;&c_o;&c_g;&c_o;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>cenn&yacute;</trd>,
    <trd>drah&yacute;</trd>
