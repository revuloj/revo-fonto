<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bolid.xml,v 1.19 2023/10/14 08:39:48 revo Exp $">
<kap>
  <rad>bolid</rad>/o
</kap>
<drv mrk="bolid.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      <ref cel="meteor.0o.meteoroido">Meteoro</ref>,
      kiu trairante la atmosferon postlasas luman trena&jcirc;on kaj
      alproksimi&gcirc;as sufi&ccirc;e al la tero, por ke oni distingu &gcirc;ian
      amplekson:
      <ekz>
        eniris la atmosferon de la Tero <klr>[&#x2026;]</klr> <tld/>o je &ccirc;irka&ubreve; 09:13
        Jekaterinburga tempo dum la mateno de 15-a de februaro 2013, kun rapideco de
        almena&ubreve; 15 km/s <klr>[&#x2026;]</klr> kaj frakasi&gcirc;is super la grandurbo de <nom>&Ccirc;eljabinsk</nom>, 
        <klr>[&#x2026;]</klr> la Rusia Akademio de Sciencoj taksis ke la
        meteoro, nomita KEF-2013, havis mason de 10 tunoj kaj la aereksplodo okazis
        en alteco inter 30&ndash;50 km super la grundo, la objekto havis diametron
        proksimume du &gcirc;is kvar metrojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=429088">Meteoro de &Ccirc;elabinsk</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        lia temperamento ne povis lumi longan tempon, konsumante sin mem &gcirc;i balda&ubreve;
        malaperis kiel bela <tld/>o, kiu postlasas longan fajran strion en la nokta
        &ccirc;ielo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1115">E. T. A. Hoffmann</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
    <ref cel="aeroli.0o">aerolito</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_l;&c_ib;&c_d;</trd>
  <trdgrp lng="cs">
    <trd>bolid</trd>,
    <trd>pov&ecaron;tro&ncaron;</trd>
  </trdgrp>
  <trd lng="de">Bolide</trd>
  <trd lng="en">bolide</trd>
  <trd lng="es">b&oacute;lido</trd>
  <trd lng="fr">bolide<klr tip="amb"> (m&eacute;t&eacute;ore)</klr></trd>
  <trd lng="he">&#x5DB;&#x5D5;&#x5DB;&#x5D1; &#x5E0;&#x5D5;&#x5E4;&#x5DC;</trd>
  <trdgrp lng="hu">
   <trd>t&udblac;zg&ouml;mb</trd>,
   <trd>bolida</trd>
 </trdgrp>
  <trd lng="it">bolide</trd>
  <trd lng="ja">&#x706B;&#x7403;  <pr>&#x304B;&#x304D;&#x3085;&#x3046;</pr></trd>
  <trd lng="nl">bolide</trd>
  <trdgrp lng="pl">
   <trd>bolid</trd>,
   <trd>meteor</trd>,
   <trd>meteoryt</trd>
 </trdgrp>
  <trd lng="pt">b&oacute;lido</trd>
  <trd lng="ru">&c_b;&c_o;&c_l;&c_i;&c_d;</trd>
  <trd lng="sk">bolid</trd>
  <trdgrp lng="uk">
    <trd>&#x431;&#x43E;&#x43B;&#x456;&#x434;</trd>,
    <trd>&#x43C;&#x435;&#x442;&#x435;&#x43E;&#x440;</trd>,
    <trd>&#x43C;&#x435;&#x442;&#x435;&#x43E;&#x440;&#x438;&#x442;</trd>
  </trdgrp>
</drv>
</art>
