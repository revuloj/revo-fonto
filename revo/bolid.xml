<vortaro>
<art mrk="$Id$">
<kap>
 <rad>bolid</rad>/o
</kap>
<drv mrk="bolid.0o">
 <kap><tld/>o</kap>
 <uzo tip="fak">AST</uzo>
 <snc>
  <dif>
   Meteoro, kiu trairante la atmosferon postlasas luman trena&jcirc;on kaj
   alproksimi&gcirc;as sufi&ccirc;e al la tero, por ke oni distingu &gcirc;ian
   amplekson. 
  </dif>
  <refgrp tip="vid">
   <ref cel="aeroli.0o">aerolito</ref>.
  </refgrp>
 </snc>
 <trd lng="be">&c_b;&c_a;&c_l;&c_ib;&c_d;</trd>
 <trd lng="cs">bolid</trd>
 <trd lng="de">Bolid</trd>
 <trd lng="en">bolide</trd>
 <trd lng="es">b&oacute;lido</trd>
 <trd lng="fr">bolide<klr tip="amb"> (m&eacute;t&eacute;ore)</klr></trd>
 <trd lng="it">bolide</trd>
 <trdgrp lng="hu">
   <trd>t&udblac;zg&ouml;mb</trd>,
   <trd>bolida</trd>
 </trdgrp>
 <trd lng="nl">bolide</trd>
 <trd lng="pt">b&oacute;lido</trd>
 <trd lng="ru">&c_b;&c_o;&c_l;&c_i;&c_d;</trd>
</drv>
</art>
