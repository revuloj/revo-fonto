<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tuj</rad>
</kap>
<drv mrk="tuj.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc mrk="tuj.0.tempe">
    <dif>
      Senprokraste post tiu momento:
      <ekz>
        mi <tld/> foriras;
      </ekz>
      <ekz>
        pli valoras <tld/> ovo ol poste bovo
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        atendu, la mastro <tld/> venos;
      </ekz>
      <ekz>
        iru <tld/>
        <fnt><bib>F</bib>, <lok>&FE; 19</lok></fnt>;
      </ekz>
      <ekz>
        li <tld/> faris, kion mi volis
        <fnt><bib>F</bib>, <lok>&FE; 31</lok></fnt>;
      </ekz>
      <ekz>
        mi estis <tld/> finonta la leteron, kiam vi alvenis;
      </ekz>
      <ekz>
        se li scius, ke mi estas tie &ccirc;i, li <tld/> venus
        <fnt><bib>F</bib>, <lok>&FE; 20</lok></fnt>;
      </ekz>
      <ekz>
        la plej nekredeblajn aferojn <klr>...</klr> li <tld/> kredas
        <fnt><bib>F</bib>, <lok>&FE; 41</lok></fnt>;
      </ekz>
      <ekz>
        <tld/> kiam li min vidis, li kuris al mi renkonte;
      </ekz>
      <ekz>
        <tld/> en (de) la komenco de l' jaro;
      </ekz>
      <ekz>
        li foriris <tld/> post mia alveno;
      </ekz>
      <ekz>
        <tld/>ege.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_d;&c_r;&c_a;&c_z;&c_u;</trd>,
      <trd>&c_n;&c_je;&c_a;&c_d;&c_k;&c_l;&c_a;&c_d;&c_n;&c_a;</trd>,
      <trd>&c_t;&c_u;&c_t; &c_zh;&c_a; <klr>(&c_p;&c_r;&c_a;
      &c_ch;&c_a;&c_s;)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_t;&c_o;&c_t;&c_ch;&c_a;&c_s;</trd>,
      <trd>&c_s;&c_r;&c_a;&c_z;&c_u;</trd>,
      <trd>&c_s;&c_je;&c_j;&c_ch;&c_a;&c_s; &c_zh;&c_je;</trd>,
      <trd>&c_n;&c_je;&c_m;&c_je;&c_d;&c_l;&c_je;&c_n;&c_n;&c_o;</trd>,
      <trd>&c_t;&c_u;&c_t; &c_zh;&c_je; <klr tip="amb">(&c_o;
      &c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_i;)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="tuj.0.space">
    <dif>
      Tute proksime, tute apude:
      <ekz>
        <tld/> post la pordo estas &scirc;ranko;
      </ekz>
      <ekz>
        <tld/> sur la unua &scirc;tupo;
      </ekz>
      <ekz>
        la cikonioj flugis unu <tld/> post la alia
        <fnt><bib>Fab2</bib>, <lok>dio de dormo, merkredo</lok></fnt>;
      </ekz>
      <ekz>
        urbo <klr>...</klr> sin trovas <tld/> sur la bordo de la maro
        <fnt><bib>Fab1</bib>, <lok>la virineto de maro</lok></fnt>;
      </ekz>
      <ekz>
        <tld/> sub li la flanko de l' monto estis krutega<fnt>B</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_u;&c_t; &c_zh;&c_a; <klr>(&c_p;&c_r;&c_a; &c_m;&c_je;&c_s;&c_c;&c_a;)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_a;&c_k; &c_r;&c_a;&c_z;</trd>,
      <trd>&c_t;&c_u;&c_t; &c_zh;&c_je; <klr tip="amb">(&c_o; &c_m;&c_je;&c_s;&c_t;&c_je;)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="cs">hned</trd>
  <trdgrp lng="de">
    <trd>sofort</trd>,
    <trd>unmittelbar</trd>,
    <trd>gleich</trd>
  </trdgrp>
  <trd lng="en">immediately</trd>
  <trdgrp lng="fr">
    <trd>aussit&ocirc;t</trd>,
    <trd>tout de suite</trd>,
    <trd>imm&eacute;diatement</trd>,
    <trd>juste <klr>(lieu)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>onmiddellijk</trd>,
    <trd>dadelijk</trd>,
    <trd>meteen</trd>
  </trdgrp>
</drv>

<drv mrk="tuj.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="T"/> okazonta, <tld/> fari&gcirc;onta, senprokrasta:
      <ekz>
        mi dankis lin por la <tld/>a plenumo de mia deziro
        <fnt><bib>F</bib>, <lok>&FE; 31</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_a;&c_d;&c_k;&c_l;&c_a;&c_d;&c_n;&c_y;</trd>
  <trdgrp lng="de">
    <trd>sofortig</trd>,
    <trd>unmittelbar</trd>
  </trdgrp>
  <trd lng="fr">imm&eacute;diat</trd>
  <trd lng="nl">onmiddellijk</trd>
  <trd lng="ru">&c_n;&c_je;&c_m;&c_je;&c_d;&c_l;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="tuj.pret0a">
  <kap><tld/>preta</kap>
  <snc mrk="tuj.pret0a.KUI">
    <uzo tip="fak">KUI</uzo>
    <uzo tip="klr">(pri man&gcirc;a&jcirc;o a&ubreve; trinka&jcirc;o)</uzo>
    <dif>
      Kiu postulas nur mallongan preparadon:
      <ekz>
        <tld/>preta kafo
        <fnt><bib>HejmVort</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>preta vermi&ccirc;elo
        <fnt><bib>HejmVort</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_u;&c_t;&c_k;&c_a;&c_g;&c_a;
    &c_p;&c_r;&c_y;&c_g;&c_a;&c_t;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ja;
    <klr>(&c_p;&c_r;&c_a; &c_je;&c_zh;&c_u;)</klr></trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_u;&c_sh;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_y;
    <klr>(&c_p;&c_r;&c_a; &c_n;&c_a;&c_p;&c_ib;&c_t;&c_a;&c_k;)</klr></trd>
  </trdgrp>
  <trd lng="de">Fertig-</trd>
  <trd lng="en">instant</trd>
  <trd lng="es">instant&aacute;neo</trd>
  <trd lng="fr">instantan&eacute;</trd>
  <trd lng="nl">instant-</trd>
  <trd lng="pt">instant&acirc;neo</trd>
  <trd lng="ru">&c_b;&c_y;&c_s;&c_t;&c_r;&c_o;&c_g;&c_o; &c_p;&c_r;&c_i;&c_g;&c_o;&c_t;&c_o;&c_v;&c_l;&c_je;&c_n;&c_i;&c_ja; <klr tip="amb">(&c_o; &c_je;&c_d;&c_je;)</klr></trd>
</drv>
</art>
