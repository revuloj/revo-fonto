<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tuj.xml,v 1.37 2023/10/21 14:37:42 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>tuj</rad> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tuj.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc mrk="tuj.0.tempe">
    <dif>
      <ref tip="sin" cel="prokra.sen0e">Senprokraste</ref>
      post tiu momento:
      <ekz>
         li <tld/> faris, kion mi volis
         <fnt><bib>F</bib>, <lok>&FE; 31</lok></fnt>;
      </ekz>
      <ekz>
         iru <tld/>
         <fnt><bib>F</bib>, <lok>&FE; 19</lok></fnt>;
      </ekz>
      <ekz>
         se li scius, ke mi estas tie &ccirc;i, li <tld/> venus
         <fnt><bib>F</bib>, <lok>&FE; 20</lok></fnt>;
      </ekz>
      <ekz>
         la plej nekredeblajn aferojn <klr>[&#x2026;]</klr> li <tld/> kredas
         <fnt><bib>F</bib>, <lok>&FE; 41</lok></fnt>;
      </ekz>
      <ekz>
        <tld/> matene, komenci&gcirc;is tondroj kaj fulmoj
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 19:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        donu <tld/>, alie mi prenos per forto
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 2:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/> kiam naski&gcirc;is la infano, la re&gcirc;ino mortis
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam la pomoj maturi&gcirc;is, oni kalkulis ilin, sed <tld/> en la posta tago unu
        mankis
        <fnt>
          <bib>ElektFab</bib>
          <lok>La ora birdo</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi devas <tld/> foriri, anta&ubreve; ol homoj vidos vin
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
         <tld/> alveninte al la puto, li eka&ubreve;dis vokon de la onklino el
         la dometo
         <fnt>
           <aut>A. Strindberg, trad. S. Johansson</aut>,
           <vrk>Insulanoj De Hems&ouml;</vrk>,
           <lok>p. 46</lok>
         </fnt>;
      </ekz>
      <ekz>
        <tld/> en la komenco de nia movado mi deklaris, ke mi ne volas esti mastro de
        Esperanto
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Oka Kongreso Esperantista en Krakow en la 11a de a&ubreve;gusto 1912</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/> de sinjoroj estas multe da horoj
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
       <ekz>
         <ind>pli valoras <tld/> ovo ol poste bovo</ind>
         <fnt><bib>PrV</bib></fnt>.
         <trd lng="hu">jobb ma egy ver&eacute;b, mint holnap
           egy t&uacute;zok</trd>
       </ekz>
     </dif>
    <refgrp tip="vid">
      <ref cel="moment.sam0e">sammomente</ref>,
      <ref cel="hezit.sen0e">senhezite</ref>,
      <ref cel="per.sen0e">senpere</ref>
    </refgrp>
     <trdgrp lng="be">
       <trd>&c_a;&c_d;&c_r;&c_a;&c_z;&c_u;</trd>,
       <trd>&c_n;&c_je;&c_a;&c_d;&c_k;&c_l;&c_a;&c_d;&c_n;&c_a;</trd>,
       <trd>&c_t;&c_u;&c_t; &c_zh;&c_a; <klr>(&c_p;&c_r;&c_a;
       &c_ch;&c_a;&c_s;)</klr></trd>
     </trdgrp>
     <trdgrp lng="fi">
       <trd>heti</trd>,
       <trd>viipym&auml;tt&auml;</trd>
     </trdgrp>
     <trdgrp lng="hu">
       <trd>r&ouml;gt&ouml;n</trd>,
       <trd>azonnal</trd>,
       <trd>nyomban</trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>&c_t;&c_o;&c_t;&c_ch;&c_a;&c_s;</trd>,
       <trd>&c_s;&c_r;&c_a;&c_z;&c_u;</trd>,
       <trd>&c_s;&c_je;&c_j;&c_ch;&c_a;&c_s; &c_zh;&c_je;</trd>,
       <trd>&c_n;&c_je;&c_m;&c_je;&c_d;&c_l;&c_je;&c_n;&c_n;&c_o;</trd>,
       <trd>&c_t;&c_u;&c_t; &c_zh;&c_je; <klr tip="amb">(&c_o;
       &c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_i;)</klr></trd>
     </trdgrp>
  </snc>
  <snc mrk="tuj.0.space">
    <dif>
      Tute <ref cel="proksi.0ede">proksime</ref>, 
      tute <ref cel="apud.0ede">apude</ref>:
      <ekz>
        <klr>[ili]</klr> lo&gcirc;is kune en malnova budo <tld/> apud
        la maro
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fi&scirc;kaptisto kaj lia edzino</lok>
        </fnt>;
      </ekz>
      <ekz>
        se tio <tld/> en la komenco de la libro estus skribita kaj presita, tiam
        ver&scirc;ajne kelkaj legantoj metus la libron flanken
        <fnt>
          <bib>Fab3</bib>
          <lok>Bona humoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi estis tie &ccirc;i proksime <tld/> post la pordego
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/> sur la unua stacio de tiu nekonata vojo, &scirc;i komencis diveni la ekzistadon
        de aferoj teruraj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
         la cikonioj flugis <ind>unu <tld/> post la alia</ind>
         <fnt><bib>Fab2</bib>, <lok>dio de dormo, merkredo</lok></fnt>;
         <trd lng="fi">aivan per&auml;kk&auml;in</trd>
      </ekz>
      <ekz>
         urbo <klr>[&#x2026;]</klr> sin trovas <tld/> sur la bordo de la maro
         <fnt><bib>Fab1</bib>, <lok>la virineto de maro</lok></fnt>;
       </ekz>
       <ekz>
         <tld/> sub li la flanko de l' monto estis krutega
        <fnt>
          <bib>IKr</bib>
          <lok>La Miraklo De Purun Bhagat</lok>
        </fnt>.
       </ekz>
     </dif>
     <trd lng="be">&c_t;&c_u;&c_t; &c_zh;&c_a; <klr>(&c_p;&c_r;&c_a;
&c_m;&c_je;&c_s;&c_c;&c_a;)</klr></trd>
     <trdgrp lng="fi">
       <trd>heti</trd>,
       <trd>aivan (l&auml;hell&auml;)</trd>
     </trdgrp>
     <trdgrp lng="hu">
       <trd>k&ouml;zvetlen&uuml;l</trd>,
       <trd>mindj&aacute;rt</trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>&c_k;&c_a;&c_k; &c_r;&c_a;&c_z;</trd>,
       <trd>&c_t;&c_u;&c_t; &c_zh;&c_je; <klr tip="amb">(&c_o;
&c_m;&c_je;&c_s;&c_t;&c_je;)</klr></trd>
     </trdgrp>
   </snc>
  <trd lng="bg">&c_v;&c_je;&c_d;&c_n;&c_a;&c_g;&c_a;</trd>
  <trd lng="bo">&t_la;&t_ma;&t_tsheg;&t_sa;&t_drengbu;&t_nga;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>bezodkladn&ecaron;</trd>,
    <trd>hned</trd>,
    <trd>ihned</trd>,
    <trd>neprodlen&ecaron;</trd>,
    <trd>obratem</trd>,
    <trd>okam&zcaron;it&ecaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>sofort</trd>,
     <trd>unmittelbar</trd>,
     <trd>gleich</trd>
   </trdgrp>
  <trdgrp lng="en">
    <trd>immediately</trd>,
    <trd>right away</trd>,
    <trd>promptly</trd>,
    <trd>at once</trd>
  </trdgrp>
  <trd lng="es">inmediatamente</trd>
  <trdgrp lng="fr">
     <trd>aussit&ocirc;t</trd>,
     <trd>tout de suite</trd>,
     <trd>imm&eacute;diatement</trd>,
     <trd>juste <klr>(lieu)</klr></trd>
   </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3059;&#x3050;</trd>,
    <trd>&#x3059;&#x3050;&#x306B;</trd>,
    <trd>&#x76F4;&#x3061;&#x306B;  <pr>&#x305F;&#x3060;&#x3061;&#x306B;</pr></trd>,
    <trd>&#x3059;&#x3050;&#x305D;&#x3070;&#x306B;</trd>,
    <trd>&#x3059;&#x3050;&#x8FD1;&#x304F;&#x306B;  <pr>&#x3059;&#x3050;&#x3061;&#x304B;&#x304F;&#x306B;</pr></trd>
  </trdgrp>
  <trd lng="ne">&#x924;&#x924;&#x94D;&#x915;&#x93E;&#x932;</trd>
  <trdgrp lng="nl">
     <trd>onmiddellijk</trd>,
     <trd>meteen</trd>,
     <trd>direct</trd>,
     <trd>dadelijk<klr tip="ind"> (onmiddelijk)</klr></trd>
   </trdgrp>
  <trdgrp lng="pl">
     <trd>natychmiast</trd>,
     <trd>zaraz</trd>,
   </trdgrp>
  <trdgrp lng="ro">
     <trd>imediat</trd>,
     <trd>&icirc;ndat&abreve;</trd>,
     <trd>numaidec&acirc;t</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>hne&dcaron;</trd>,
    <trd>ihne&dcaron;</trd>,
    <trd>obratom</trd>,
    <trd>okam&zcaron;ite</trd>,
    <trd>zaraz</trd>
  </trdgrp>
  <trdgrp lng="tr">
     <trd>hemen </trd>,
     <trd>&ccedil;ar &ccedil;abuk</trd>
   </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x437;&#x430;&#x440;&#x430;&#x437;</trd>,
    <trd>&#x437;&#x440;&#x430;&#x437;&#x443; &#x436;</trd>,
    <trd>&#x43D;&#x435;&#x433;&#x430;&#x439;&#x43D;&#x43E;</trd>,
    <trd>&#x441;&#x430;&#x43C;&#x435;</trd>,
    <trd>&#x44F;&#x43A;&#x440;&#x430;&#x437;</trd>,
    <trd>&#x442;&#x43E;&#x447;&#x43D;&#x43E; (&#x43F;&#x440;&#x43E; &#x43C;&#x456;&#x441;&#x446;&#x435;)</trd>
  </trdgrp>
</drv>

<drv mrk="tuj.0a">
  <kap><tld/>a</kap>
  <snc mrk="tuj.0a.KOMUNE">
    <dif>
      <tld lit="T"/> okazonta, <tld/> fari&gcirc;onta, senprokrasta:
      <ekz>
         mi dankis lin por la <tld/>a plenumo de mia deziro
         <fnt><bib>F</bib>, <lok>&FE; 31</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;iu <tld/>a dan&gcirc;ero &scirc;ajnis provizore malaperinta
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        profunda malespero, kun soifo al <tld/>a ven&gcirc;o
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu rigardo <klr>[&#x2026;]</klr> &scirc;ajnis postuli <tld/>an respondon
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Deka</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiun <tld/>an &scirc;an&gcirc;on nenio klarigis
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekdua</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio ne donis <tld/>an rezulton, li devis plurfoje reiri
        <fnt>
          <bib>HsT</bib>
          <lok>Feli&ccirc;a homo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i volas neniel, per <tld/>a reveno, malkvietigi
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_a;&c_d;&c_k;&c_l;&c_a;&c_d;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>bezodkladn&yacute;</trd>,
    <trd>okam&zcaron;it&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sofortig</trd>,
    <trd>unmittelbar</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>immediate</trd>,
    <trd>prompt</trd>
  </trdgrp>
  <trdgrp lng="fi">
     <trd>pikainen</trd>,
     <trd>pika-</trd>,
     <trd>heti tapahtuva</trd>
  </trdgrp>
  <trd lng="fr">imm&eacute;diat</trd>
  <trdgrp lng="hu">
     <trd>azonnali</trd>,
     <trd>k&ouml;zvetlen</trd>
   </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3059;&#x3050;&#x306E;</trd>,
    <trd>&#x5373;&#x6642;&#x306E;  <pr>&#x305D;&#x304F;&#x3058;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">onmiddellijk</trd>
  <trdgrp lng="pl">
     <trd>bezzw&lstroke;oczny</trd>,
     <trd>natychmiastowy</trd>,
   </trdgrp>
  <trd lng="ru">&c_n;&c_je;&c_m;&c_je;&c_d;&c_l;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">okam&zcaron;it&yacute;</trd>
  <trd lng="uk">&#x43D;&#x435;&#x433;&#x430;&#x439;&#x43D;&#x438;&#x439;</trd>
</drv>

<drv mrk="tuj.00">
  <kap><tld/><tld/>,
  <var><kap><tld/><tld/>e</kap></var></kap>
  <snc mrk="tuj.00.KOMUNE">
    <dif>
      Sen e&ccirc; plej eta atendo:
      <ekz>
        <tld/><tld/> ni ekbruligis la fuzeon de longa vico 
        de artfajra&jcirc;oj
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_r;&c_a;&c_z;&c_u; &c_zh;</trd>,
    <trd>&c_b;&c_je;&c_z; &c_a;&c_n;&c_ib;&c_ja;&c_k;&c_a;&c_j; &c_z;&c_a;&c_t;&c_r;&c_y;&c_m;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="cs">bez prodlen&iacute;</trd>
  <trdgrp lng="de">
    <trd>sofortigst</trd>,
    <trd>schnellstens</trd>
  </trdgrp>
  <trd lng="fr">sur le champ</trd>
  <trd lng="sk">bez oneskorenia</trd>
</drv>

<drv mrk="tuj.pret0a">
   <kap><tld/>preta</kap>
   <snc mrk="tuj.pret0a.KUI">
     <uzo tip="fak">KUI</uzo>
     <uzo tip="klr">(pri man&gcirc;a&jcirc;o a&ubreve; trinka&jcirc;o)</uzo>
     <dif>
       Kiu postulas nur mallongan preparadon:
       <ekz>
         <ind><tld/>preta kafo</ind>
         <fnt><bib>HejmVort</bib></fnt>;
         <trd lng="en">instant coffee</trd>
         <trdgrp lng="hu">
           <trd>azonnal old&oacute;d&oacute; k&aacute;v&eacute;</trd>,
           <trd>nescaf&eacute;</trd>
         </trdgrp>
         <trd lng="nl">instantkoffie</trd>
       </ekz>
       <ekz>
         <tld/>preta vermi&ccirc;elo
         <fnt><bib>HejmVort</bib></fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_h;&c_u;&c_t;&c_k;&c_a;&c_g;&c_a;
     &c_p;&c_r;&c_y;&c_g;&c_a;&c_t;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ja;
     <klr>(&c_p;&c_r;&c_a; &c_je;&c_zh;&c_u;)</klr></trd>,
     <trd>&c_r;&c_a;&c_s;&c_p;&c_u;&c_sh;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_y;
     <klr>(&c_p;&c_r;&c_a; &c_n;&c_a;&c_p;&c_ib;&c_t;&c_a;&c_k;)</klr></trd>
   </trdgrp>
   <trd lng="de">Fertig-</trd>
   <trd lng="en">instant</trd>
   <trd lng="es">instant&aacute;neo</trd>
   <trdgrp lng="fi">
     <trd>pika-</trd>,
     <trd>instant-</trd>
   </trdgrp>
   <trd lng="fr">instantan&eacute;</trd>
   <trdgrp lng="hu">
     <trd>konyhak&eacute;sz</trd>,
     <trd>azonnal old&oacute;d&oacute;</trd>,
     <trd>instant</trd>
   </trdgrp>
   <trdgrp lng="ja">
    <trd>&#x5373;&#x5E2D;&#x306E;  <pr>&#x305D;&#x304F;&#x305B;&#x304D;&#x306E;</pr></trd>,
    <trd>&#x30A4;&#x30F3;&#x30B9;&#x30BF;&#x30F3;&#x30C8;&#x306E;  <pr>&#x3044;&#x3093;&#x3059;&#x305F;&#x3093;&#x3068;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">instant-</trd>
   <trdgrp lng="pl">
     <trd>ekspresowe <klr>(danie)</klr></trd>,
     <trd>instant</trd>,
   </trdgrp>
   <trd lng="pt">instant&acirc;neo</trd>
   <trd lng="ru">&c_b;&c_y;&c_s;&c_t;&c_r;&c_o;&c_g;&c_o;
&c_p;&c_r;&c_i;&c_g;&c_o;&c_t;&c_o;&c_v;&c_l;&c_je;&c_n;&c_i;&c_ja; <klr tip="amb">(&c_o; &c_je;&c_d;&c_je;)</klr></trd>
</drv>
