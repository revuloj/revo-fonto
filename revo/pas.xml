      </ekz>
    </dif>
    <trd lng="hu">&aacute;tmegy (vhov&aacute;)</trd>
    <trd lng="nl">overgaan</trd>
  </snc>
  <snc>
    <gra><vspec>ntr</vspec></gra>
    <dif>
