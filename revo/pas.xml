<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pas</rad>/i
</kap>
<drv mrk="pas.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc mrk="pas.0i.ie">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Iri tra a&ubreve; preter iu loko por atingi alian:
      <ekz>
        <tld/>i sur la strato, la placo<fnt>Z</fnt>,
      </ekz><ekz>
        tra kampo<fnt>Z</fnt>,
      </ekz><ekz>
        tra la urbo, la pordo, la fenestro, super la muro;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        <ind><tld/>i al alia demando</ind>;
        <trd lng="hu">&aacute;tt&eacute;r m&aacute;sik k&eacute;rd&eacute;sre</trd>
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        la vojo <tld/>is tra malvasta kaj malluma koridoro
        <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 3a.</lok></fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;iuj &ccirc;i bildoj rapide <tld/>is tra lia cerbo.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>halad</trd>,
      <trd>&aacute;thalad</trd>,
      <trd>elhalad</trd>,
      <trd>megy</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_j;&c_t;&c_i;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>entlanggehen</trd>,
      <trd>durchqueren</trd>
      <trd>passieren <klr>(r&auml;umlich)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="pas.0i.aliigxi">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Transiri de iu stato al alia, alistati&gcirc;i, transformi&gcirc;i:
      <ekz>
        <tld/>i de ri&ccirc;eco al senhaveco.
      </ekz>
    </dif>
    <trd lng="hu">&aacute;tmegy <klr tip="amb">(&aacute;llapotba)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_je;&c_r;&c_je;&c_j;&c_t;&c_i;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_je;&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">in etw. <ind>&uuml;bergehen</ind></trd>
  </snc>
  <snc mrk="pas.0i.forpasi">
    <gra><vspec>ntr</vspec></gra>
    <ref tip="dif" cel="pas.for0i.KOMUNE">for<tld/>i</ref>
    <ekz>
      <tld/>as la tempo;
    </ekz><ekz>
      jam kvin jaroj <tld/>is de post tio;
    </ekz><ekz>
      kio <tld/>is ne revenos
      <fnt><bib>PrV</bib></fnt>;
    </ekz><ekz>
      la soleno <tld/>is tute silente<fnt>Z</fnt>;
    </ekz><ekz>
      lia tago <tld/>adis en studado<fnt>Z</fnt>;
    </ekz><ekz>
      ebrieco <tld/>as post dormo, malsa&gcirc;eco neniam
      <fnt><bib>PrV</bib></fnt>;
    </ekz><ekz>
      kiel <tld/>anta ventego, tiel malpiulo rapide malaperas
      <fnt><bib>MT</bib>, <lok>&Sen; 10:25</lok></fnt>;
    </ekz><ekz>
      kun komika mieno de <tld/>anta meditado<fnt>Z</fnt>;
    </ekz><ekz>
      <tld/>inta doloro for el memoro
      <fnt><bib>PrV</bib></fnt>;
    </ekz><ekz>
      ser&ccirc;i la <tld/>intan tagon<fnt>Z</fnt>
      <klr>(netrovebla&jcirc;on)</klr>;
    </ekz><ekz>
      le&gcirc;o <ind><tld/>inta&jcirc;o</ind>n ne tu&scirc;as;
      <trdgrp lng="hu">
        <trd>m&uacute;lbeli esem&eacute;ny</trd>,
        <trd>ami <ind>elm&uacute;lt</ind></trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_p;&c_r;&c_o;&c_sh;&c_l;&c_o;&c_je;</trd>,
        <trd>&c_p;&c_r;&c_o;&c_sh;&c_je;&c_d;&c_sh;&c_je;&c_je;</trd>
      </trdgrp>
      <fnt><bib>PrV</bib></fnt>
    </ekz><ekz>
      sen <ind><tld/>inteco</ind> ne fari&gcirc;as estonteco,
      diras &ccirc;ina proverbo;
      <trd lng="hu">m&uacute;lt</trd>
      <trdgrp lng="ru">
        <trd>&c_p;&c_r;&c_o;&c_sh;&c_l;&c_o;&c_je;</trd>,
        <trd>&c_p;&c_r;&c_o;&c_sh;&c_je;&c_d;&c_sh;&c_je;&c_je;</trd>
      </trdgrp>
    </ekz><ekz>
      <ind><tld/>intjara ne&gcirc;o</ind>
      <trd lng="hu">tavalyi h&oacute;</trd>
    </ekz>
    <trd lng="de">vergehen <klr>(zeitlich)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_j;&c_t;&c_i;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>telik</trd>,
      <trd>m&uacute;lik</trd>
    </trdgrp>
    <refgrp tip="vid">
      <ref cel="flu.0i">flui</ref>,
      <ref cel="flug.0i">flugi</ref>,
      <ref cel="glit.0i">gliti</ref>,
      <ref cel="kur.0i">kuri</ref>
    </refgrp>
  </snc>
  <snc mrk="pas.0i.trapasi">
    <uzo tip="stl">ARK</uzo>
    <gra><vspec>tr</vspec></gra>
    <ref tip="dif" cel="pas.tra0i.KOMUNE">tra<tld/>i</ref>
    <ekz>
      li <tld/>is akvon kaj fajron kaj mar&ccirc;ojn kaj marojn
      <klr>(li multe spertis)</klr>
      <fnt><bib>PrV</bib></fnt>;
    </ekz>
        <ekz>
          esta&jcirc;oj <klr>(...)</klr> jam lacigitaj de la mallonga
          <tld/>ita vojo
          <fnt><bib>Marta</bib></fnt>.
        </ekz>
    <trdgrp lng="hu">
      <trd>&aacute;tkel</trd>,
      <trd>&aacute;tmegy</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_j;&c_t;&c_i;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="br">
    <trd>paseal<klr tip="ind"> (v.g.)</klr></trd>,
    <trd>tremen<klr tip="ind"> (v.g.)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>minout</trd>,
    <trd>m&iacute;jet <klr>(&ccaron;as.)</klr></trd>,
    <trd>plynout</trd>,
    <trd>proj&iacute;t (lh&uring;ta)</trd>,
    <trd>ub&iacute;hat</trd>,
    <trd>uplynout</trd>,
    <trd>vypr&scaron;et</trd>
  </trdgrp>
  <trd lng="en">pass</trd>
  <trd lng="es">pasar</trd>
  <trd lng="fr">passer<klr tip="amb"> (intr.)</klr></trd>
  <trdgrp lng="nl">
    <trd>voorbijgaan</trd>,
    <trd>overgaan</trd>,
    <trd>passeren</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>mija&cacute;</trd>,
    <trd>przechodzi&cacute;</trd>,
  </trdgrp>
  <trd lng="pt">passar</trd>
  <trd lng="ro">a trece</trd>
  <trdgrp lng="sk">
    <trd>plyn&uacute;&tcaron;</trd>,
    <trd>pomin&uacute;&tcaron; sa</trd>,
    <trd>uplyn&uacute;&tcaron;</trd>,
    <trd>vypr&scaron;a&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="pas.0e">
  <kap><tld/>e</kap>
  <snc mrk="pas.0e.iam">
    <dif>
      Neda&ubreve;re:
      <ekz>
        Volap&uuml;k povis nur <tld/>e entuziasmigi la homojn<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="fr">passag&egrave;rement</trd>
  </snc>
  <trd lng="br">diwar dremen</trd>
  <trdgrp lng="cs">
    <trd>chvilkov&ecaron;</trd>,
    <trd>letmo</trd>,
    <trd>mimochodem</trd>,
    <trd>nedlouho</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>in passing</trd>,
    <trd>briefly</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fut&oacute;lag</trd>,
    <trd>&aacute;tmenetileg</trd>
  </trdgrp>
  <trd lng="nl">zijdelings</trd>
  <trd lng="pl">przej&sacute;ciowo</trd>
  <trd lng="sk">letmo</trd>
</drv>

<drv mrk="pas.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="pas.0ado.KOMUNE">
    <dif>
      Irado, transirado, da&ubreve;rado dum pli-malpli longa tempo:
      <ekz>
        la <tld/>ado tra la arbaro estas malfacila.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">tremenadenn</trd>
  <trdgrp lng="cs">
    <trd>ch&uring;ze</trd>,
    <trd>m&iacute;jen&iacute;</trd>
  </trdgrp>
  <trd lng="en">passage</trd>
  <trd lng="fr">passage<klr tip="amb"> (action)</klr></trd>
  <trdgrp lng="hu">
    <trd>halad&aacute;s</trd>,
    <trd>&aacute;thalad&aacute;s</trd>,
    <trd>m&uacute;l&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">het <ind>voorbijgaan</ind></trd>
  <trd lng="pl">przechodzenie</trd>
  <trd
lng="ru">&c_p;&c_r;&c_o;&c_h;&c_o;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">ch&ocirc;dza</trd>
</drv>

<drv mrk="pas.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="pas.0ajxo.KOMUNE">
    <dif>
      Grava, atentinda intermomento en da&ubreve;ra tuto:
      <ekz>
        ha kia <tld/>a&jcirc;o
        <fnt><bib>Revizoro</bib>, <lok>akto 4a, sceno 13a</lok></fnt>!
      </ekz><ekz>
        &scirc;iaj fingroj <klr>...</klr> konfuzi&gcirc;adis sur la klavoj
        <klr>...</klr>, &scirc;i eraradis en la <tld/>a&jcirc;oj
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">passage <klr>(fine arts)</klr></trd>
  <trdgrp lng="fr">
    <trd>moment d&eacute;cisif</trd>,
    <trd>transition</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>esem&eacute;ny</trd>,
    <trd>t&ouml;rt&eacute;n&eacute;s</trd>
  </trdgrp>
  <trd lng="nl">gebeurtenis</trd>
  <trd lng="pl">pasa&zdot;</trd>
</drv>

<drv mrk="pas.0anto">
  <kap><tld/>anto</kap>
  <snc mrk="pas.0anto.KOMUNE">
    <dif>
      Homo, kiu <tld/>as en strato, sur vojo:
      <ekz>
        li a&ubreve;dis interparoladon de du <tld/>antoj
        <fnt><bib>ElektFab</bib>, <lok>fingreto</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="br">
    <trd>tremeniad</trd>,
    <trd>tremener</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kolemjdouc&iacute;</trd>,
    <trd>okolojdouc&iacute;</trd>,
    <trd>pasant</trd>
  </trdgrp>
  <trd lng="de">Passant</trd>
  <trd lng="en">passerby</trd>
  <trd lng="fr">passant<klr tip="amb"> (subst.)</klr></trd>
  <trd lng="hu">j&aacute;r&oacute;kel&odblac;</trd>
  <trd lng="nl">voorbijganger</trd>
  <trd lng="pl">przechodzie&nacute;</trd>
  <trd lng="ro">pieton</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_h;&c_o;&c_zh;&c_i;&c_j;</trd>
  <trd lng="sk">chodec</trd>
</drv>

<drv mrk="pas.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="pas.0ejo.KOMUNE">
    <dif>
      Loko tra kiu oni povas <tld/>i:
      <ekz>
        <tld/>ejo en monto;
      </ekz><ekz>
        anta&ubreve; la oficaj &ccirc;ambroj estis <tld/>ejo
        <fnt><bib>MT</bib>, <lok>&Jeh; 42:4</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="ir">trairejo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="br">
    <trd>tremen<klr tip="ind"> (lec'h)</klr></trd>,
    <trd>tremenvan<klr tip="ind"> (lec'h)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pas&aacute;&zcaron;</trd>,
    <trd>pr&uring;chod</trd>,
    <trd>pr&uring;smyk</trd>,
    <trd>p&rcaron;echod</trd>,
    <trd>p&rcaron;ejezd</trd>
  </trdgrp>
  <trd lng="en">passage</trd>
  <trd lng="es">pasaje</trd>
  <trd lng="fr">passage<klr tip="amb"> (lieu)</klr></trd>
  <trd lng="hu">&aacute;tj&aacute;r&oacute;</trd>
  <trdgrp lng="nl">
    <trd>pas</trd>,
    <trd>passage</trd>,
    <trd>doorgang</trd>
  </trdgrp>
  <trd lng="pl">przej&sacute;cie</trd>
  <trd lng="ro">trecere</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_h;&c_o;&c_d;</trd>
  <trd lng="sk">priechod</trd>
</drv>

<drv mrk="pas.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="pas.0ema.KOMUNE">
    <dif>
      Facile <tld/>anta, ne stabila, ne longatempa:
      <ekz>
        &scirc;an&gcirc;i&gcirc;emaj kaj <tld/>emaj kiel akvaj ondoj
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="efemer.0a">efemera</ref>,
      <ref cel="dauxr.mal0a">malda&ubreve;ra</ref>
    </refgrp>
  </snc>
  <trdgrp lng="br">
    <trd>o tremen</trd>,
    <trd>berrbad</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>m&iacute;jej&iacute;c&iacute;</trd>,
    <trd>pom&iacute;jiv&yacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>passing</trd>,
    <trd>fleeting</trd>
  </trdgrp>
  <trd lng="fr">passager<klr tip="amb"> (adj.)</klr></trd>
  <trd lng="hu">m&uacute;l&eacute;kony</trd>
  <trd lng="nl">voorbijgaand</trd>
  <trdgrp lng="pl">
    <trd>przej&sacute;ciowy</trd>,
    <trd>przelotny</trd>
  </trdgrp>
  <trd lng="ro">trec&abreve;tor</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_h;&c_o;&c_d;&c_ja;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_m;&c_i;&c_m;&c_o;&c_l;&c_jo;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">pominute&lcaron;n&yacute;</trd>
</drv>

<drv mrk="pas.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="pas.0igi.ie">
    <dif>
      Irigi iun a&ubreve; ion tra iu loko al alia:
      <ekz>
        la pramisto <tld/>igis nin trans la rivero;
      </ekz><ekz>
        <klr>Dio</klr> fendis la maron kaj <tld/>igis ilin
        <fnt><bib>MT</bib>, <lok>&Psa; 78:13</lok></fnt>;
      </ekz><ekz>
        bonvolu <tld/>igi al mi la salon.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>to give passage</trd>,
      <trd>to pass</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&aacute;tvisz</trd>,
      <trd>&aacute;tad</trd>,
      <trd>&aacute;tny&uacute;jt</trd>
    </trdgrp>
    <trd lng="nl">laten <ind>doortrekken</ind></trd>
  </snc>
  <snc mrk="pas.0igi.iam">
    <dif>
      Uzi la tempon por io:
      <ekz>
        tie mi <tld/>igis du jarojn;
      </ekz><ekz>
        la libertempon mi <tld/>igos en la montaro;
      </ekz><ekz>
        la malsanulo <tld/>igis malkvietan nokton;
      </ekz><ekz>
        agrabla tempo<tld/>igo
        <fnt><bib>Fab1</bib>, <lok>fluganta kofro</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>to pass</trd>,
      <trd>spend</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>t&ouml;lt <klr tip="ind">(id&odblac;t)</klr></trd>,
      <trd>elt&ouml;lt</trd>
    </trdgrp>
    <trd lng="nl">doorbrengen</trd>
  </snc>
  <trd lng="br">tremen<klr tip="ind"> (v.k..)</klr></trd>
  <trdgrp lng="cs">
    <trd>str&aacute;vit <klr>(&ccaron;as)</klr></trd>,
    <trd>tr&aacute;vit</trd>
  </trdgrp>
  <trd lng="fr">passer<klr tip="amb"> (tr.)</klr></trd>
  <trdgrp lng="pl">
    <trd>przeprawi&cacute;</trd>,
    <trd>przeprowadzi&cacute;</trd>
    <trd>sp&eogonek;dzi&cacute; <klr tip="amb"> (czas)</klr></trd>,
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_v;&c_je;&c_s;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="sk">str&aacute;vi&tcaron; &ccaron;as</trd>
</drv>

<drv mrk="pas.for0i">
  <kap>for<tld/>i</kap>
  <snc mrk="pas.for0i.KOMUNE">
    <dif>
      Malaperi, iom post iom nenii&gcirc;i, senrevene:
      <ekz>
        &ccirc;io for<tld/>as;
      </ekz><ekz>
        &scirc;ia beleco for<tld/>is;
      </ekz><ekz>
        la okazo, la modo jam for<tld/>is
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="agoni.0i">agonii</ref>,
      <ref cel="fin.0igxi">fini&gcirc;i</ref>,
      <ref cel="mort.0i">morti</ref>,
      <ref cel="velk.0i.FIG">velki <sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="br">tremen<klr tip="amb"> (mont da get)</klr></trd>
  <trdgrp lng="cs">
    <trd>minout</trd>,
    <trd>m&iacute;jet <klr>(&ccaron;as.)</klr></trd>,
    <trd>plynout</trd>,
    <trd>proj&iacute;t (lh&uring;ta)</trd>,
    <trd>ub&iacute;hat</trd>,
    <trd>uplynout</trd>,
    <trd>vypr&scaron;et</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>pass away</trd>,
    <trd>die</trd>
  </trdgrp>
  <trd lng="es">pasar, desaparecer<klr tip="amb"> (una
  persona)</klr></trd>
  <trd lng="fr">passer<klr tip="amb"> (disparaitre)</klr></trd>
  <trdgrp lng="hu">
    <trd>elm&uacute;lik</trd>,
    <trd>v&eacute;get &eacute;r</trd>,
    <trd>elh&uacute;ny</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vergaan</trd>,
    <trd>sterven</trd>,
    <trd>overlijden</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przemija&cacute;</trd>,
    <trd>skona&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_j;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>plyn&uacute;&tcaron;</trd>,
    <trd>pomin&uacute;&tcaron; sa</trd>,
    <trd>uplyn&uacute;&tcaron;</trd>,
    <trd>vypr&scaron;a&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="pas.preter0i">
  <kap>preter<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pas.preter0i.ie">
    <dif>
      <tld lit="P"/>i flanke de io:
      <ekz>
        ni iris tro malproksimen kaj preter<tld/>is la domon;
      </ekz><ekz>
        pro senatento mi preter<tld/>is mian amikon sur la strato;
      </ekz><ekz>
        multe da pensoj kaj multe da bildoj preter<tld/>as anta&ubreve;
&scirc;i
        <fnt><bib>Fab2</bib>, <lok>avineto</lok></fnt>;
      </ekz><ekz>
        vi preter<tld/>is <klr>(evitis)</klr> gravan
dan&gcirc;eron<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>to pass by</trd>,
      <trd>move past</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>elmegy <klr tip="amb">(mellette)</klr></trd>,
      <trd>elhalad</trd>,
      <trd>elker&uuml;l</trd>,
      <trd>kiker&uuml;l</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_m;&c_i;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_j;&c_t;&c_i; &c_m;&c_i;&c_m;&c_o;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_p;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="pas.preter0i.neglekti">
    <dif>
      Lasi flanke, malzorgi, ne atenti:
      <ekz>
        preter<tld/>i detalon, linion, mirindan okazon
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="las.preter0i">preterlasi</ref>
    </refgrp>
    <trdgrp lng="en">
      <trd>to pass over</trd>,
      <trd>overlook</trd>
    </trdgrp>
    <trd lng="hu">figyelmen k&iacute;v&uuml;l hagy</trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_p;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="br">mont hebiou</trd>
  <trdgrp lng="cs">
    <trd>minout</trd>,
    <trd>m&iacute;jet <klr>(&ccaron;as.)</klr></trd>,
    <trd>plynout</trd>,
    <trd>proj&iacute;t (lh&uring;ta)</trd>,
    <trd>ub&iacute;hat</trd>,
    <trd>uplynout</trd>,
    <trd>vypr&scaron;et</trd>
  </trdgrp>
  <trd lng="fr">passer &agrave; c&ocirc;t&eacute; de</trd>
  <trd lng="nl">voorbijgaan</trd>
  <trdgrp lng="pl">
    <trd>opuszcza&cacute;</trd>,
    <trd>przechodzi&cacute; obok</trd>
    <trd>wymija&cacute;</trd>,
  </trdgrp>
  <trdgrp lng="sk">
    <trd>plyn&uacute;&tcaron;</trd>,
    <trd>pomin&uacute;&tcaron; sa</trd>,
    <trd>uplyn&uacute;&tcaron;</trd>,
    <trd>vypr&scaron;a&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="pas.tra0i">
  <kap>tra<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pas.tra0i.KOMUNE">
    <dif>
      Trairi tra iu loko a&ubreve; tempo:
      <ekz>
        tra<tld/>i riveron, urbon, pordon, homamason;
      </ekz><ekz>
        multe tra<tld/>ata strato<fnt>Z</fnt>;
      </ekz><ekz>
        tra<tld/>i (travivi, sperti) malfacilajn tagojn, penigan
        bataladon, dan&gcirc;eran krizon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="br">
    <trd>treizha&ntilde;</trd>,
    <trd>treuzi&ntilde;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>absolvovat</trd>,
    <trd>proch&aacute;zet <klr>(n&ecaron;co)</klr></trd>
  </trdgrp>
  <trd lng="en">to pass through</trd>
  <trd lng="fr">traverser</trd>
  <trdgrp lng="hu">
    <trd>&aacute;tmegy</trd>,
    <trd>&aacute;thalad</trd>
  </trdgrp>
  <trd lng="nl">doortrekken</trd>
  <trd lng="pl">przeby&cacute;</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_j;&c_t;&c_i;
<klr>(&c_ch;&c_je;&c_r;&c_je;&c_z; &c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>
  <trdgrp lng="sk">
    <trd>prejs&tcaron;</trd>,
    <trd>pre&zcaron;i&tcaron;</trd>,
    <trd>sk&uacute;si&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="pas.trans0i">
  <kap>trans<tld/>i</kap>
  <snc>
    <gra><vspec>tr</vspec></gra>
    <dif>
      Iri trans iu loko:
      <ekz>
        trans<tld/>i la Alpojn, la landlimojn;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        trans<tld/>i la mezuron, la limojn de la le&gcirc;eco<fnt>B</fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        trans<tld/>i Rubikonon
        <klr>(decidi sen posta ebla &scirc;an&gcirc;o)</klr>
        <fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>to pass over</trd>,
      <trd>to cross</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&aacute;tkel</trd>,
      <trd>&aacute;tmegy (vmin)</trd>,
      <trd>t&uacute;lmegy</trd>
    </trdgrp>
    <trd lng="nl">over (iets) <ind>gaan</ind></trd>
  </snc>
  <snc>
    <gra><vspec>tr</vspec></gra>
    <dif>
      Superi, esti pli granda ol:
      <ekz>
        tio trans<tld/>as mian komprenon, &ccirc;iun imagon.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>to pass</trd>,
      <trd>surpass</trd>
    </trdgrp>
    <trd lng="hu">meghalad</trd>
    <trd lng="nl">te <ind>bovengaan</ind></trd>
  </snc>
  <snc>
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Iri de iu loko al alia trans io:
      <ekz>
        li trans<tld/>is en Amerikon;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        la povo <klr>...</klr> trans<tld/>as en la manojn de Herhor
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 12a</lok></fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        trans<tld/>i de la avertoj al la agoj<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>to pass</trd>,
      <trd>go through</trd>
    </trdgrp>
    <trd lng="hu">&aacute;tmegy (vhov&aacute;)</trd>
    <trd lng="nl">overgaan</trd>
  </snc>
  <snc>
    <gra><vspec>ntr</vspec></gra>
    <dif>
      &Scirc;an&gcirc;i sian staton en alian, alistati&gcirc;i:
      <ekz>
        trans<tld/>i de la solida al la flueca stato.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>to pass</trd>,
      <trd>transform</trd>
    </trdgrp>
    <trd lng="hu">&aacute;tmegy <klr tip="amb">(&aacute;llapotba)</klr></trd>
    <trd lng="nl">overgaan</trd>
  </snc>
  <trd lng="br">tremen (hebiou)</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;ejet</trd>,
    <trd>p&rcaron;ej&iacute;t</trd>,
    <trd>p&rcaron;ekro&ccaron;it <klr>(&rcaron;eku, hranici)</klr></trd>
  </trdgrp>
  <trd lng="fr">d&eacute;passer</trd>
  <trd lng="pl">przekroczy&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_j;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol;<klr> (&c_ch;&c_t;&c_o;-&c_l;., &c_ch;&c_je;&c_r;&c_je;&c_z; &c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>prejs&tcaron;</trd>,
    <trd>prekro&ccaron;i&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="pas.tempo0igilo">
  <kap>tempo<tld/>igilo</kap>
  <snc mrk="pas.tempo0igilo.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Amuza&jcirc;o kiu forigas enuon dum senokupo.
    </dif>
  </snc>
  <trd lng="br">tremen-amzer</trd>
  <trd lng="en">pastime</trd>
  <trd lng="fr">passe-temps</trd>
  <trd lng="hu">id&odblac;t&ouml;lt&eacute;s</trd>
  <trd lng="nl">tijdverdrijf</trd>
  <trdgrp lng="pl">
    <trd>wype&lstroke;niacz <ind>czasu</ind></trd>,
    <trd>zabijacz <ind>czasu</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_r;&c_je;&c_m;&c_ja;&c_p;&c_r;&c_je;&c_p;&c_r;&c_o;&c_v;&c_o;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>
