<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pas</rad>/i
</kap>
<drv mrk="pas.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Iri tra a&ubreve; preter iu loko por atingi alian:
      <ekz>
        <tld/>i sur la
        strato, la placo<fnt>Z</fnt>,
      </ekz>
      <ekz>
        tra kampo<fnt>Z</fnt>,
      </ekz>
      <ekz>
        tra la urbo, la pordo, la fenestro, super la muro;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i al alia demando;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;iuj &ccirc;i bildoj rapide <tld/>is tra lia cerbo.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_j;&c_t;&c_i;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Transiri de iu stato al alia, alistati&gcirc;i, transformi&gcirc;i:
      <ekz>
        <tld/>i de ri&ccirc;eco al senhaveco.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_p;&c_je;&c_r;&c_je;&c_j;&c_t;&c_i;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_je;&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Transirante malaperi a&ubreve; nenii&gcirc;i; for<tld/>i, ne
      da&ubreve;ri, &ccirc;esi:
      <ekz>
        <tld/>as la tempo;
      </ekz>
      <ekz>
        jam kvin jaroj <tld/>is depost tio;
      </ekz>
      <ekz>
        kio <tld/>is ne revenos
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la soleno <tld/>is tute silente<fnt>Z</fnt>;
      </ekz>
      <ekz>
        lia tago <tld/>adis en studado<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ebrieco <tld/>as post dormo, malsa&gcirc;eco neniam
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        rapide malaperi kiel <tld/>anta ventego<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kun komika mieno de <tld/>anta meditado<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>inta doloro for el memoro
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ser&ccirc;i la <tld/>intan tagon<fnt>Z</fnt>
        <klr>(netrovebla&jcirc;on)</klr>;
      </ekz>
      <ekz>
        le&gcirc;o <ind><tld/>inta&jcirc;o</ind>n ne tu&scirc;as;
        <trdgrp lng="ru">
          <trd>&c_p;&c_r;&c_o;&c_sh;&c_l;&c_o;&c_je;</trd>,
          <trd>&c_p;&c_r;&c_o;&c_sh;&c_je;&c_d;&c_sh;&c_je;&c_je;</trd>
        </trdgrp>
        <fnt><bib>PrV</bib></fnt>
      </ekz>
      <ekz>
        sen <ind><tld/>inteco</ind> ne fari&gcirc;as estonteco,
        diras &ccirc;ina proverbo;
        <trdgrp lng="ru">
          <trd>&c_p;&c_r;&c_o;&c_sh;&c_l;&c_o;&c_je;</trd>,
          <trd>&c_p;&c_r;&c_o;&c_sh;&c_je;&c_d;&c_sh;&c_je;&c_je;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <tld/>intjara ne&gcirc;o
      </ekz>
      <trdgrp lng="ru">
        <trd>&c_p;&c_r;&c_o;&c_j;&c_t;&c_i;</trd>,
        <trd>&c_p;&c_r;&c_o;&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>
      </trdgrp>
    </dif>
    <refgrp tip="sin">
      <ref cel="flu.0i">flui</ref>,
      <ref cel="flug.0i">flugi</ref>,
      <ref cel="glit.0i">gliti</ref>,
      <ref cel="kur.0i">kuri</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      = tra<tld/>i:
      <ekz>
        li <tld/>is akvon kaj fajron kaj mar&ccirc;ojn kaj marojn
        <klr>(multe spertis)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_j;&c_t;&c_i;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">minout</trd>
  <trd lng="en">pass</trd>
  <trdgrp lng="nl">
    <trd>voorbijgaan</trd>,
    <trd>overgaan</trd>,
    <trd>passeren</trd>
  </trdgrp>
</drv>

<drv mrk="pas.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Neda&ubreve;re:
      <ekz>
        Volap&uuml;kaj povis nur <tld/>e entuziasmigi la homojn<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">zijdelings</trd>
</drv>

<drv mrk="pas.0ado">
  <kap><tld/>(ad)o</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        la <tld/>ado tra la arbaro estas malfacila;
      </ekz>
      <ekz>
        post <tld/>o de unu jaro<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">het <ind>voorbijgaan</ind></trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_h;&c_o;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="pas.0ajxo">
  <kap><tld/>a&jcirc;o<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Okaza&jcirc;o.
    </dif>
  </snc>
  <trd lng="nl">gebeurtenis</trd>
</drv>

<drv mrk="pas.0anto">
  <kap><tld/>anto</kap>
  <snc>
    <dif>
      Homo, kiu <tld/>as en strato, sur vojo.
    </dif>
  </snc>
  <trd lng="nl">voorbijganger</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_h;&c_o;&c_zh;&c_i;&c_j;</trd>
</drv>

<drv mrk="pas.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Loko tra kiu oni povas <tld/>i:
      <ekz>
        <tld/>ejo en monto
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="ir">trairejo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="nl">
    <trd>pas</trd>,
    <trd>passage</trd>,
    <trd>doorgang</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_h;&c_o;&c_d;</trd>
</drv>

<drv mrk="pas.0ema">
  <kap><tld/>(em)a</kap>
  <snc>
    <dif>
      Neda&ubreve;ra:
      <ekz>
        <tld/>ema &gcirc;ojo.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">voorbijgaand</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_h;&c_o;&c_d;&c_ja;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_m;&c_i;&c_m;&c_o;&c_l;&c_jo;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="pas.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Irigi iun a&ubreve; ion tra iu loko al alia:
      <ekz>
        la pramisto <tld/>igis nin trans la rivero;
      </ekz>
      <ekz>
        Dio fendis la maron kaj <tld/>igis ilin<fnt>Z</fnt>;
      </ekz>
      <ekz>
        bonvolu <tld/>igi al mi la salon.
      </ekz>
    </dif>
    <trd lng="nl">laten <ind>doortrekken</ind></trd>
  </snc>
  <snc>
    <dif>
      Uzi la tempon por io:
      <ekz>
        tie mi <tld/>igis du jarojn;
      </ekz>
      <ekz>
        la libertempon mi <tld/>igos en la montaro;
      </ekz>
      <ekz>
        la malsanulo <tld/>igis malkvietan nokton;
      </ekz>
      <ekz>
        agrabla tempo<tld/>igo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="nl">doorbrengen</trd>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_v;&c_je;&c_s;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="pas.for0i">
  <kap>for<tld/>i</kap>
  <snc>
    <dif>
      = <tld/>i 3:
      <ekz>
        &ccirc;io for<tld/>as;
      </ekz>
      <ekz>
        &scirc;ia beleco for<tld/>is;
      </ekz>
      <ekz>
        la okazo, la modo jam for<tld/>is
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fin.0igxi">fini&gcirc;i</ref>,
      <ref cel="mort.0i">morti</ref>,
      <ref cel="agoni.0i">agonii</ref>,
      <ref cel="sxangx.0igxi">&scirc;an&gcirc;i&gcirc;i</ref>,
      <ref cel="versx">renversi&gcirc;i</ref>,
      <ref cel="dron.0i">droni</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="detru.0i">detrui</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="nl">
    <trd>vergaan</trd>,
    <trd>sterven</trd>,
    <trd>overlijden</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_j;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="pas.preter0i">
  <kap>preter<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="P"/>i flanke de io:
      <ekz>
        ni iris tro malproksimen kaj preter<tld/>is la domon;
      </ekz>
      <ekz>
        pro senatento mi preter<tld/>is mian amikon sur la strato;
      </ekz>
      <ekz>
        multe da pensoj kaj bildoj preter<tld/>as anta&ubreve; &scirc;i<fnt>Z</fnt>;
      </ekz>
      <ekz>
        vi preter<tld/>is (evitis) gravan dan&gcirc;eron<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_m;&c_i;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_j;&c_t;&c_i; &c_m;&c_i;&c_m;&c_o;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_p;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Lasi flanke, malzorgi, ne atenti:
      <ekz>
        preter<tld/>i detalon, linion, mirindan okazon
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="las.preter0i">preterlasi</ref>.
    </refgrp>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_p;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="nl">voorbijgaan</trd>
</drv>

<drv mrk="pas.tra0i">
  <kap>tra<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Trairi tra iu loko a&ubreve; tempo:
      <ekz>
        tra<tld/>i riveron, urbon, pordon homamason;
      </ekz>
      <ekz>
        multe tra<tld/>ata strato<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tra<tld/>i (travivi, sperti) malfacilajn tagojn, penigan
        bataladon, dan&gcirc;eran krizon.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">doortrekken</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_j;&c_t;&c_i; <klr>(&c_ch;&c_je;&c_r;&c_je;&c_z; &c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>
</drv>

<drv mrk="pas.trans0i">
  <kap>trans<tld/>i</kap>
  <snc>
    <gra><vspec>tr</vspec></gra>
    <dif>
      Iri trans iu loko:
      <ekz>
        trans<tld/>i la Alpojn, la landlimojn;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        trans<tld/>i la mezuron, la limojn de la le&gcirc;eco<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        trans<tld/>i Rubikonon<fnt>Z</fnt>
      </ekz>
      (malebligi al si &ccirc;iun &scirc;an&gcirc;on de decido.
    </dif>
    <refgrp tip="vid">
      <ref cel="kub.0o">kubo</ref>,
      <ref cel="pont.0o">ponto</ref>,
      <ref cel="pasx.0i">pa&scirc;i</ref>.
    </refgrp>
    <trd lng="nl">over (iets) <ind>gaan</ind></trd>
  </snc>
  <snc>
    <gra><vspec>tr</vspec></gra>
    <dif>
      Superi, esti pli granda ol:
      <ekz>
        tio trans<tld/>as mian komprenon, &ccirc;iun imagon.
      </ekz>
    </dif>
    <trd lng="nl">te <ind>bovengaan</ind></trd>
  </snc>
  <snc>
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Iri de iu loko al alia trans io:
      <ekz>
        li trans<tld/>is en Amerikon;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la povo trans<tld/>is en la manojn de<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        trans<tld/>i de la avertoj al la agoj<fnt>B</fnt>.
      </ekz>
    </dif>
    <trd lng="nl">overgaan</trd>
  </snc>
  <snc>
    <gra><vspec>ntr</vspec></gra>
    <dif>
      &Scirc;an&gcirc;i sian staton en alian, alistati&gcirc;i:
      <ekz>
        trans<tld/>i de la solida al la flueca stato.
      </ekz>
    </dif>
    <trd lng="nl">overgaan</trd>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_j;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol; <klr>(&c_ch;&c_t;&c_o;-&c_l;., &c_ch;&c_je;&c_r;&c_je;&c_z; &c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="pas.tempo0igilo">
  <kap>tempo<tld/>igilo</kap>
  <snc>
    <dif>
      Distriga amuza&jcirc;o.
    </dif>
  </snc>
  <trd lng="nl">tijdverdrijf</trd>
  <trd lng="ru">&c_v;&c_r;&c_je;&c_m;&c_ja;&c_p;&c_r;&c_je;&c_p;&c_r;&c_o;&c_v;&c_o;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>
</art>
