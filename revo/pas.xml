    </dif>
    <trd lng="hu">&aacute;tmegy <klr tip="amb">(&aacute;llapotba)</klr></trd>
    <trd lng="nl">overgaan</trd>
  </snc>                       
  <trd lng="br">tremen (hebiou)</trd>
  <trd lng="fr">d&eacute;passer</trd>
