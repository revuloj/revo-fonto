<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kameleoned</rad>/oj
</kap>
<drv mrk="kamele1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo
      <ref tip="super" cel="skvam.0uloj">skvamuloj</ref>
      (<trd lng="la">Chamaeleonidae</trd>), kun envolvebla vosto,
      el&jcirc;etebla lango por kapti insektojn kaj
      kolor&scirc;an&gcirc;ema ha&ubreve;to.
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_m;&c_e;&c_l;&c_je;&c_o;&c_n;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Cham&auml;leons<klr tip="ind"> (Familie)</klr></trd>
  <trdgrp lng="fr">
    <trd>cam&eacute;l&eacute;onid&eacute;s</trd>,
    <trd>chamael&eacute;onid&eacute;s</trd>
  </trdgrp>
  <trd lng="hu">kam&eacute;leonf&eacute;l&eacute;k</trd>
  <trd lng="nl">kameleons<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="ru">&c_h;&c_a;&c_m;&c_je;&c_l;&c_je;&c_o;&c_n;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
