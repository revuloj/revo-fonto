<vortaro>
<art mrk="$Id$">
<kap>
  <rad>perikard</rad>/o
</kap>
<drv mrk="perika.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Membraneca sako, envolvanta la koron.
    </dif>
    <trd lng="en">pericardium</trd>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_r;&c_y;&c_k;&c_a;&c_r;&c_d;</trd>
  <trdgrp lng="de">
    <trd>Perikard</trd>,
    <trd>Herzbeutel</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&iacute;vburok</trd>,
    <trd>pericardium</trd>
  </trdgrp>
  <trd lng="nl">hardzak</trd>
  <trd lng="pl">osierdzie</trd>  
  <trd lng="pt">peric&aacute;rdio</trd> 
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_i;&c_k;&c_a;&c_r;&c_d;</trd>,
    <trd>&c_o;&c_k;&c_o;&c_l;&c_o;&c_s;&c_je;&c_r;&c_d;&c_je;&c_ch;&c_n;&c_a;&c_ja; &c_s;&c_u;&c_m;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
