<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: perika.xml,v 1.16 2024/02/17 06:43:06 revo Exp $">
<kap>
  <rad>perikard</rad>/o
</kap>
<drv mrk="perika.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">ANA</uzo>
    <dif>
      Membraneca sako, envolvanta la <ref tip="malprt" cel="kor.0o.ANA">koron</ref>:
      <ekz>
        li plenumis gravajn studojn pri la kortamponado, anka&ubreve; konata kiel &leftquot;perikarda
        tamponado&rightquot;, kiam la perikarda fluida&jcirc;o (a&ubreve; sako envolvanta la koron) formi&gcirc;as
        rezultante en kunpremo de la koro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=653347">Edmund Rose</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">pericardium</trd>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_r;&c_y;&c_k;&c_a;&c_r;&c_d;</trd>
  <trdgrp lng="de">
    <trd>Perikard</trd>,
    <trd>Herzbeutel</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_b;&a_r;&a_w;&a_n;&zwnj;&a_sh;&a_A;&a_m;&a_h;&a_hamza_sure; &a_q;&a_l;&a_b;</trd>,
    <trd>&f_p;&a_kasra;&a_r;&f_y;&f_k;&a_A;&a_r;&a_d;</trd>
  </trdgrp>
  <trd lng="fr">p&eacute;ricarde</trd>
  <trdgrp lng="hu">
    <trd>sz&iacute;vburok</trd>,
    <trd>pericardium</trd>
  </trdgrp>
  <trd lng="it">pericardio</trd>
  <trd lng="nl">hardzak</trd>
  <trd lng="pl">osierdzie</trd>  
  <trd lng="pt">peric&aacute;rdio</trd> 
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_i;&c_k;&c_a;&c_r;&c_d;</trd>,
    <trd>&c_o;&c_k;&c_o;&c_l;&c_o;&c_s;&c_je;&c_r;&c_d;&c_je;&c_ch;&c_n;&c_a;&c_ja; &c_s;&c_u;&c_m;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#x5FC3;&#x5305; <pr>x&#x12B;nb&#x101;o</pr></trd>
</drv>
</art>
