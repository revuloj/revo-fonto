<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>sekal</rad>/o
</kap>
<drv mrk="sekal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sekal.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="poac.0oj">poacoj</ref> (<trd
lng="la">Secale</trd>).
    </dif>
  </snc>
  <snc mrk="sekal.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
       Specio el la genro <ref tip="super" cel="sekal.0o.BOT"><tld/>o</ref>
       (<trd lng="la">Secale cereale</trd>), kulturata kiel 
       <ref tip="super" cel="gren.0o.planto">greno</ref> por produkti grizan
farunon,
       uzatan precipe por <ref tip="malprt" cel="pan.0o.mangxo">pano</ref>.
    </dif>
  </snc>
  <trd lng="de">Roggen</trd>
  <trd lng="en">rye</trd>
  <trd lng="hu">rozs</trd>
  <trd lng="nl">rogge</trd>
  <trd lng="ru">&c_r;&c_o;&c_zh;&c_mol;</trd>
</drv>
</art>
