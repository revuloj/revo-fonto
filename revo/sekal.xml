<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sekal.xml,v 1.28 2024/03/24 20:00:00 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sekal</rad>/o
</kap>

<drv mrk="sekal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sekal.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Secale</trd>).
    </dif>
    <trd lng="hu">rozs</trd>
  </snc>
  <snc mrk="sekal.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:botanikaj_specioj">Specio</ref> el la genro <ref tip="super" cel="sekal.0o.BOT"><tld/>o</ref>
      (<trd lng="la">Secale cereale</trd>), kulturata kiel <ref tip="super" cel="gren.0o.planto">greno</ref> por produkti grizan
      farunon, uzatan precipe por <ref tip="malprt" cel="pan.0o.mangxo">pano</ref>:
      <ekz>
        flugis odoro de varma <tld/>a pano
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 1a, p..
        8a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>k&ouml;z&ouml;ns&eacute;ges rozs</trd>,
      <trd>termesztett rozs</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_zh;&c_y;&c_t;&c_a;</trd>
  <trd lng="bg">&c_r;&c_malmol;&c_zh;</trd>
  <trd lng="ca">s&egrave;gol</trd>
  <trdgrp lng="cs">
    <trd>re&#x17E;</trd>,
    <trd>&#x17E;ito</trd>
  </trdgrp>
  <trd lng="de">Roggen</trd>
  <trd lng="en">rye</trd>
  <trd lng="es">centeno</trd>
  <trdgrp lng="fr">
    <trd>seigle</trd>
  </trdgrp>
  <trd lng="it">segale</trd>
  <trdgrp lng="ja">
    <trd>&#x30E9;&#x30A4;&#x30E0;&#x30AE;</trd>,
    <trd>&#x30E9;&#x30A4;&#x9EA6;  <pr>&#x30E9;&#x30A4;&#x3080;&#x304E;</pr></trd>
  </trdgrp>
  <trd lng="nl">rogge</trd>
  <trd lng="pl">&zdot;yto</trd>
  <trd lng="ru">&c_r;&c_o;&c_zh;&c_mol;</trd>
  <trd lng="sk">ra&#x17E;</trd>
  <trd lng="sv">r&aring;g</trd>
  <trd lng="uk">&c_zh;&c_i;&c_t;&#x43E;</trd>
  <trdgrp lng="zh">
    <trd>&#x9ED1;&#x9EA5; <pr>h&#x113;im&#xE0;i</pr></trd>,
    <trd>&#x9ED1;&#x9EA6; <pr>h&#x113;im&#xE0;i</pr></trd>,
    <trd>&#x88F8;&#x9EA5; <pr>lu&#x1D2;m&#xE0;i</pr></trd>,
    <trd>&#x88F8;&#x9EA6; <pr>lu&#x1D2;m&#xE0;i</pr></trd>
  </trdgrp>
</drv>
</art>
