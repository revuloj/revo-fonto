<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: renklod.xml,v 1.5 2023/06/09 14:28:28 revo Exp $">
  <kap>
    <rad>renklod</rad>/o
    <fnt><bib>IK</bib></fnt>
  </kap>

<drv mrk="renklod.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc mrk="renklod.0o.prunspeco">
    <dif>
      <ref tip="super" cel="prun.0o">Prun</ref>speco (<trd lng="la">Prunus domestica var.
      claudiana</trd>)
      kun globformaj flave verdaj dol&ccirc;aj sukaj fruktoj:
      <ekz>
        &ccirc;irka&ubreve;is &gcirc;in rozaj persikoj, flavaj abrikotoj, verdaj <tld/>oj, sub kiuj ru&gcirc;aj
        pomoj kaj grandaj piroj sin apogis al melono
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>   
      <ekz>
        diversaj tipoj de prunoj oni povas dividi en a) ronda pruno, b) oblonga slivo
        kaj c) verda <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=321310">Man&gcirc;ebla frukto</url></lok>
        </fnt>.
      </ekz>   
    </dif>
    <bld lok="&WCU;/thumb/2/20/Greengages.jpg/579px-Greengages.jpg" prm="PD"><tld/>oj
        <fnt>
          <aut>Ranveig</aut>
          <lok><url ref="&WCW;/File:Greengages.jpg">&WCW;/File:Greengages.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="de">
      <trd>Reneklode</trd>
    </trdgrp>
    <trd lng="en">greengage</trd>
    <trd lng="pl">renkloda</trd>
    <trd lng="ro">prun  <klr>(greengages)</klr></trd>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_n;&c_k;&c_l;&c_jo;&c_d;</trd>
  <trd lng="uk">&c_r;&c_je;&#x43D;&#x43A;&#x43B;&#x43E;&c_d; (&c_s;&#x43E;&c_r;&c_t; &c_s;&#x43B;&c_i;&c_v;)</trd>
</drv>

</art>

