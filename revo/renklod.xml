<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: renklod.xml,v 1.1 2019/08/01 22:10:21 revo Exp $">
  <kap>
    <rad>renklod</rad>/o
    <fnt><bib>IK</bib></fnt>
  </kap>

<drv mrk="renklod.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc mrk="renklod.0o.prunspeco">
    <dif>
      <ref tip="super" cel="prun.0o">Prun</ref>speco (<trd lng="la">Prunus domestica var. claudiana</trd>)
      kun globformaj flave verdaj dol&ccirc;aj sukaj fruktoj:
      <ekz>
        &ccirc;irka&ubreve;is &gcirc;in rozaj persikoj, flavaj abrikotoj, verdaj <tld/>oj, sub kiuj ru&gcirc;aj
        pomoj kaj grandaj piroj sin apogis al melono
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>   
      <ekz>
        diversaj tipoj de prunoj oni povas dividi en a) ronda pruno, b) oblonga slivo
        kaj c) verda <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=321310"
              >Man&gcirc;ebla frukto</url></lok>
        </fnt>.
      </ekz>   
    </dif>
    <bld
      lok="&WCU;/thumb/2/20/Greengages.jpg/579px-Greengages.jpg"
      prm="PD"
      ><tld/>oj
        <fnt>
          <aut>Ranveig</aut>
          <lok><url ref="&WCW;/File:Greengages.jpg"
              >https://commons.wikimedia.org/wiki/File:Greengages.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="de">
      <trd>Reneklode</trd>
    </trdgrp>
    <trd lng="en">greengage</trd>
  </snc>
</drv>

</art>

