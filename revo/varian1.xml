<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>varianc</rad>/o
</kap>
<drv mrk="varian1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <klr>(de <ref cel="variab.hazarda0o">hazarda variablo</ref> X)</klr>
      La <ref cel="ekspek.0o">ekspekto</ref> de (X-E(X))<sup>2</sup>; simb.
      Var(X) a&ubreve; &sigma;<sup>2</sup>(X):
      <ekz>
        la <tld/>o estas la <ref cel="momant.noa0o">dua centra momanto</ref>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Streuung</trd>,
    <trd>Varianz</trd>
  </trdgrp>
  <trd lng="en">variance</trd>
  <trd lng="fr">variance</trd>
  <trd lng="pl">wariancja</trd>
  <trd lng="ru">&c_d;&c_i;&c_s;&c_p;&c_je;&c_r;&c_s;&c_i;&c_ja;</trd>
</drv>
</art>

