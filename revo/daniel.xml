<vortaro>
  <art mrk="$Id: daniel.xml,v 1.18 2023/08/31 10:43:57 revo Exp $">
    <kap>
      <rad>Daniel</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>

    <drv mrk="daniel.0o">
      <kap><tld/>o, <var><kap><tld/></kap></var></kap>
      <snc mrk="daniel.0o.NOM">
        <uzo tip="fak">NOM</uzo>
        <dif>
          Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref>
          hebredevena.
        </dif>
        <trd lng="bg">&c_D;&c_a;&c_n;&c_i;&c_i;&c_l;</trd>
        <trd lng="de">Daniel</trd>
        <trd lng="pl">Daniel</trd>
        <trd lng="pt">Daniel</trd>
        <trdgrp lng="ru">
          <trd>&c_D;&c_a;&c_n;&c_i;&c_i;&c_l;</trd>,
          <trd>&c_D;&c_a;&c_n;&c_i;&c_l;&c_a;</trd>
        </trdgrp>
      </snc>
      <snc mrk="daniel.0o.PERS">
        <uzo tip="fak">BIB</uzo>
        <uzo tip="fak">PERS</uzo>
        <dif>
          &Ccirc;efrolulo de samnoma biblia libro, forkondukita en
          Babilonon, kie li
          profetas anta&ubreve; la re&gcirc;oj:
          <ekz>
            li vokis per mal&gcirc;oja vo&ccirc;o <tld/>on
            <fnt><bib>MT</bib>, <lok>&Dan; 6:20</lok></fnt>;
          </ekz><ekz>
            kvaza&ubreve; vi estus pli sa&gcirc;a ol <tld/>
            <fnt><bib>MT</bib>, <lok>&Jeh; 28:3</lok></fnt>.
          </ekz>
        </dif>
        <trd lng="bg">&c_D;&c_a;&c_n;&c_i;&c_i;&c_l;</trd>
        <trd lng="de">Daniel</trd>
        <trd lng="pl">Daniel</trd>
        <trd lng="pt">Daniel</trd>
        <trd lng="ru">&c_D;&c_a;&c_n;&c_i;&c_i;&c_l;</trd>
      </snc>
      <trdgrp lng="be">
        <trd>&c_D;&c_a;&c_n;&c_ib;&c_l;&c_a;</trd>,
        <trd>&c_D;&c_e;&c_n;&c_ib;&c_e;&c_l;</trd>,
        <trd>&c_D;&c_a;&c_n;&c_ib;&c_e;&c_l;&c_mol;</trd>
      </trdgrp>
      <trd lng="ca">Daniel</trd>
      <trd lng="de">Daniel</trd>
      <trd lng="en">Daniel</trd>
      <trd lng="fr">Daniel</trd>
      <trd lng="hu">D&aacute;niel</trd>
      <trd lng="id">Daniel</trd>
      <trd lng="it">Daniele</trd>
      <trd lng="ja">&#x30C0;&#x30CB;&#x30A8;&#x30EB;</trd>
  <trd lng="nl">Dani&euml;l</trd>
      <trdgrp lng="uk">
    <trd>&c_D;&c_a;&#x43D;&c_i;&#x43B;&#x43E;</trd>,
    <trd>&c_D;&c_a;&#x43D;&c_i;&c_ji;&#x43B;</trd>
  </trdgrp>
</drv>
