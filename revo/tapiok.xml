<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tapiok.xml,v 1.21 2024/05/10 08:31:29 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>tapiok</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="tapiok.0o">
  <kap><tld/>o</kap>
  <snc mrk="tapiok.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Nutra grio eltirita el radiko de
      <ref tip="vid" cel="maniok.0o.BOT">manioko</ref>:
      <ekz>
        kesto da rizo, <tld/>o kaj makaronoj
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i kuiris <tld/>an supon kaj kelkajn legomojn
        <fnt>
          <aut>J. L. Borges, trad. I. Ertl</aut>,
          <vrk><url ref="&LOdE;Tl-hisp3.htm">
          Emma Zunz</url></vrk>,
          <lok>La Ondo de Esperanto, 2008:7 (165)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sagu.0o">saguo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_p;&c_ib;&c_jo;&c_k;&c_a;</trd>
  <trd lng="ca">tapioca</trd>
  <trd lng="cs">tapioka</trd>
  <trd lng="de">Tapioka</trd>
  <trd lng="en">tapioca</trd>
  <trd lng="es">tapioca</trd>
  <trd lng="fr">tapioca</trd>
  <trd lng="hu">t&aacute;pi&oacute;ka</trd>
  <trd lng="ja">&#x30BF;&#x30D4;&#x30AA;&#x30AB;</trd>
  <trd lng="nl">tapioca</trd>
  <trd lng="pl">tapioka</trd>
  <trd lng="pt">tapioca</trd>
  <trd lng="ru">&c_t;&c_a;&c_p;&c_i;&c_o;&c_k;&c_a;</trd>
  <trd lng="sk">tapioka</trd>
  <trd lng="uk">&c_t;&c_a;&c_p;&c_ib;&c_o;&c_k;&c_a; (&c_k;&c_r;&c_u;&c_p;&c_a;)</trd>
  <trdgrp lng="zh">
    <trd>&#x6728;&#x85AF;&#x6DC0;&#x7C89; <pr>m&#xF9;sh&#x1D4;di&#xE0;nf&#x11B;n</pr></trd>,
    <trd>&#x6728;&#x85AF;&#x6FB1;&#x7C89; <pr>m&#xF9;sh&#x1D4;di&#xE0;nf&#x11B;n</pr></trd>
  </trdgrp>
</drv>
</art>
