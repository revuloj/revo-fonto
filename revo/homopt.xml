<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>homopter</rad>/oj
</kap>
<drv mrk="homopt.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="insekt.0oj">insektoj</ref>
      (<trd lng="la">Homoptera</trd>), kun kvar samspecaj molha&ubreve;taj
      flugiloj. La <tld/>oj nutras sin de plantsukoj.
    </dif>
    <ref tip="vid" cel="hemipt.0oj">hemipteroj</ref>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_w;&c_n;&c_a;&c_k;&c_r;&c_y;&c_l;&c_y;&c_ja; <klr
  tip="amb">(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trdgrp lng="de">
    <trd>Pflanzensauger</trd>,
    <trd>Gleichfl&uuml;gler</trd>
  </trdgrp>
  <trd lng="fr">homopt&egrave;res</trd>
  <trd lng="hu">egyenl&odblac;sz&aacute;rny&uacute;ak</trd>
  <trd lng="it">omotteri</trd>
  <trd lng="nl">plantenzuigers</trd>
  <trd lng="pt">hom&oacute;pteros</trd>
  <trd lng="ru">&c_r;&c_a;&c_v;&c_n;&c_o;&c_k;&c_r;&c_y;&c_l;&c_y;&c_je; <klr
  tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
</drv>
</art>
