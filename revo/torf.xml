<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>torf</rad>/o
</kap>
<drv mrk="torf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">MIN</uzo>
    <dif>
      Spongeca substanco, samnatura kiel <ref tip="vid"
      cel="karb.0o">karbo</ref>,
      esti&gcirc;anta en mar&ccirc;oj pro malkomponi&gcirc;o de vegeta&jcirc;oj,
      loke uzata kiel <ref tip="super" cel="brul.0igajxo">bruliga&jcirc;o</ref>:
      <ekz>
        la&ubreve; li, la konstrumetodo de la muroj (ver&scirc;ajne faritaj el <tld/>o)
        estas tipe nordnorvega, kaj malsimilas la &scirc;tonan konstruadon
        kutiman en suda Norvegio kaj en la Britaj Insuloj<fnt><aut>Garbhan
        MacAoidh</aut>: <vrk>Frua irlanda migrado al Islando</vrk>,
        <bib>Monato</bib><lok>jaro 1998a, numero 7a, p. 22a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_o;&c_r;&c_f;</trd>
  <trd lng="de">Torf</trd>
  <trd lng="en">peat</trd>
  <trd lng="fr">tourbe</trd>
  <trd lng="hu">t&odblac;zeg</trd>
  <trd lng="nl">turf</trd>
  <trd lng="pl">torf</trd>
  <trd lng="pt">turfa</trd>
  <trd lng="ru">&c_t;&c_o;&c_r;&c_f;</trd>
</drv>

<drv mrk="torf.0ejo">
  <kap><tld/>ejo</kap>
  <uzo tip="fak">EKOL</uzo>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Loko kie trovi&gcirc;as <tld/>amasoj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_r;&c_f;&c_ja;&c_n;&c_ib;&c_k;</trd>, 
    <trd>&c_t;&c_a;&c_r;&c_f;&c_ja;&c_n;&c_ib;&c_sh;&c_ch;&c_a;</trd>
  </trdgrp>  
  <trd lng="de">Torfmoor</trd>
  <trd lng="en">peat bog</trd>
  <trd lng="fr">tourbi&egrave;re</trd>
  <trd lng="hu">t&odblac;zegl&aacute;p</trd>
  <trd lng="nl">veenderij</trd>
  <trd lng="ru">&c_t;&c_o;&c_r;&c_f;&c_ja;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
