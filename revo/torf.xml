<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>torf</rad>/o
</kap>
<drv mrk="torf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">MIN</uzo>
    <dif>
      Spongeca bruligebla substanco, samnatura kiel <ref
      cel="karb.0o">karbo</ref>,
      kaj kiu esti&gcirc;as en mar&ccirc;oj per malkomponi&gcirc;o de vegeta&jcirc;oj.
    </dif>
    <refgrp tip="super">
      <ref cel="brul.0igajxo">bruliga&jcirc;o</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Torf</trd>
  <trd lng="en">peat</trd>
  <trd lng="fr">tourbe</trd>
  <trd lng="nl">turf</trd>
  <trd lng="ru">&c_t;&c_o;&c_r;&c_f;</trd>
  <trd lng="pl">torf</trd>
</drv>

<drv mrk="torf.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Loko, kie trovi&gcirc;as <tld/>amasoj.
    </dif>
  </snc>
  <trd lng="en">peat bog</trd>
  <trd lng="fr">tourbi&egrave;re</trd>
  <trd lng="nl">veenderij</trd>
  <trd lng="ru">&c_t;&c_o;&c_r;&c_f;&c_ja;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
