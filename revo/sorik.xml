<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>sorik</rad>/o
</kap>
<drv mrk="sorik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="sorik.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Sorex</trd>), insektoman&gcirc;a musosimila besto.
    </dif>
  </snc>
</drv>
<drv mrk="sorik.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super"
      cel="mangx.insekto0uloj">insektoman&gcirc;uloj</ref>
      (<trd lng="la">Soricidae</trd>), kun rostrosimila bu&scirc;o.
    </dif>
  </snc>
  <trd lng="de">Spitzm&auml;se</trd>
  <trd lng="en">shrews</trd>
</drv>
</art>
