<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&scirc;anaj</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="sxanaj.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Nord-hinda <ref tip="super"
      cel="instru2.blov0o">blovinstrumento</ref>
      el ligna, ok-trua tubo,
      provizita per kupraj klo&scirc;o kaj bu&scirc;peco,
      kun du paroj de an&ccirc;oj ligitaj al tubeto,      
      volonte uzata en eksteraj festoj
      pro brila kaj nuanca sonoro.
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="hoboj.0o">hobojo</ref>
  </refgrp>
  <trdgrp lng="fr">
    <trd>bombarde</trd>,
    <trd>shana&iuml;</trd>,
    <trd>surna&iuml;</trd>
  </trdgrp>
</drv>
</art>

