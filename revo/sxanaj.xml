<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sxanaj.xml,v 1.10 2022/05/05 14:43:49 revo Exp $">
<kap>
  <rad>&scirc;anaj</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="sxanaj.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Nord-hinda <ref tip="lst" cel="instru2.blov0o"
      lst="voko:blovinstrumentoj">blovinstrumento</ref>
      el ligna, ok-trua tubo, provizita per kupraj klo&scirc;o kaj bu&scirc;peco,
      kun du paroj de an&ccirc;oj ligitaj al tubeto,  
      volonte uzata en eksteraj festoj pro brila kaj nuanca sonoro:
      <ekz>
        en norda Barato <tld/>o estas uzata &ccirc;e familiaj festoj, religiaj
        procezioj kaj en la
        klasika muziko.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/0/0d/Shehnai.jpg/292px-Shehnai.jpg"
      prm="CC BY 3.0"
      ><tld/>o
        <fnt>
          <aut>Badagnani, propra verko</aut>
          <lok><url ref="&WCW;/File:Shehnai.jpg"
              >&WCW;/File:Shehnai.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <refgrp tip="vid">
    <ref cel="hoboj.0o">hobojo</ref>
  </refgrp>
  <trd lng="be">&c_sh;&c_a;&c_n;&c_a;&c_j;</trd>
  <trd lng="ca">xenai</trd>
  <trd lng="de">Shehnai</trd>
  <trdgrp lng="es">
    <trd>shehnai</trd>,
    <trd>shahnai</trd>,
    <trd>shenai</trd>,
    <trd>mangal vadya</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bombarde</trd>,
    <trd>shana&iuml;</trd>,
    <trd>surna&iuml;</trd>
  </trdgrp>
  <trd lng="it">shanai</trd>
  <trd lng="pt">shanai</trd>
  <trd lng="ru">&c_sh;&c_a;&c_n;&c_a;&c_j;</trd>
</drv>
</art>

