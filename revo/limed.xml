<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: limed.xml,v 1.9 2023/11/21 15:28:33 revo Exp $">
<kap>
  <rad>limed</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="limed.0o">
  <kap><tld/>o</kap>
  <snc mrk="limed.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;ebla <ref tip="super" cel="frukt.0o.KUI">frukto</ref> de
      <tld/>arbo, flava kiel citrono sed pli ronda kaj pli dol&ccirc;a:
      <ekz>
        <tld/>oj kreskas &ccirc;efe en la subtropikaj a&ubreve; tropikaj regionoj de la mondo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=481924">Citrus latifolia</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        li unue prezentis al klerikoj a&ubreve; pastroj
        kelkajn ne tre altvalorajn donacetojn:
        la&ubreve; la sezono, laktukon el Murcio, paron
        da <tld/>oj, oran&gcirc;oj, persikoj a&ubreve; piroj
        <fnt>
          <aut>E. de Bonaero</aut>,
          <vrk>La Friponeto de Tormes</vrk>,
          <lok>1973</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ib;&c_m;&c_e;&c_t;&c_a; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>,
    <trd>&c_s;&c_a;&c_l;&c_o;&c_d;&c_k;&c_ib; <ind>&c_l;&c_a;&c_j;&c_m;</ind></trd>
  </trdgrp>
  <trd lng="bg">&c_l;&c_i;&c_p;&c_a;</trd>
  <trd lng="cs">limetta</trd>
  <trd lng="de">Limette <klr>(Frucht)</klr></trd>
  <trd lng="en">sweet lime</trd>
  <trdgrp lng="fr">
    <trd>citron doux</trd>,
    <trd>limette</trd>
  </trdgrp>
  <trd lng="nl">limoen</trd>
  <trd lng="pl">cytryna rzymska</trd>
  <trd lng="sk">limeta</trd>
  <trd lng="uk">&c_b;&c_je;&c_r;&c_gu;&c_a;&#x43C;&#x43E;&c_t;</trd>
</drv>

<drv mrk="limed.0arbo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="limed.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_specioj" cel="speci.0o.BIO">Specio</ref>
      de <ref tip="super" cel="citrus.0o">citruso</ref>,
      malalta dorna, kun malrektaj bran&ccirc;oj kaj malgrandaj folioj
      (<trd lng="la">Citrus limetta</trd>).
    </dif>
    <bld
    lok="&WCU;/thumb/5/54/Citrus_limetta_'Millsweet'_-_Sweet_lemon.jpg/320px-Citrus_limetta_'Millsweet'_-_Sweet_lemon.jpg"
    prm="CC BY-SA 3.0"><tld/>ujo, Botanika &gcirc;ardeno de Missouri
        <fnt>
          <aut>James Steakley, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Citrus_limetta_'Millsweet'_-_Sweet_lemon.jpg">&WCW;/File:Citrus_limetta_'Millsweet'_-_Sweet_lemon.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Limette <klr>(Busch)</klr></trd>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_m;&c_e;&c_t;&c_a; <klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trdgrp lng="fr">
    <trd>limettier doux</trd>,
    <trd>limonette</trd>
  </trdgrp>
  <trd lng="pl">cytryna rzymska</trd>
  <trd lng="uk">&c_b;&c_je;&c_r;&c_gu;&c_a;&#x43C;&#x43E;&c_t;</trd>
</drv>
</art>

