<drv mrk="hitler.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc mrk="hitler.0o.diktatoro">
    <dif>
      Germane <trd lng="de">Hitler, Adolf</trd> (1889&#x202F;-&#x202F;1945), diktatoro de la Germana
      Regno de 1933
      &gcirc;is 1945:
      <ekz>
        <tld/>o en Germanio la&ubreve;le&gcirc;e akiris regpotencon sub la vajmara konstitucio
        <fnt>
          <bib>Monato</bib>
          <aut>Kawai Yuka</aut>
          <vrk><url ref="&Monato;/2013/011127.php">Lernu hitleran manieron ...</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Einstein</nom> <klr>[&#x2026;]</klr> havis nenian iluzion pri la nacisocialismo kaj, kiam li
        a&ubreve;dis pri la nomumo de <tld/>o kiel regna kanceliero, mem eksi&gcirc;is el la prusa
        Akademio de Sciencoj
        <fnt>
          <bib>Monato</bib>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url ref="&Monato;/2005/008440.php">Atoma bombo ne estas malproksime</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        germanaj generaloj <klr>[&#x2026;]</klr> organizis atencon kontra&ubreve; <tld/>o pro
        patriotaj kialoj
       <fnt>
        <aut>Michel Duc Goninaz</aut>
        <vrk><url ref="&Monato;/2000/006947.html">Eks-Jugoslavio: 
          kiu kiun mistifikas?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 8</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_G;&c_ib;&c_t;&c_l;&c_je;&c_r;</trd>
    <trd lng="en">Hitler, Adolf</trd>
    <trd lng="fr">Hitler, Adolf</trd>
    <trd lng="nl">Hitler, Adolf</trd>
    <trd lng="uk">&c_G;&c_ib;&c_t;&#x43B;&c_je;&c_r;</trd>
  </snc>
  
