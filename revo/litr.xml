<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>litr</rad>/o
</kap>
<drv mrk="litr.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Komunuza <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de
      kapacito, uzata por mezuri la volumenon de flua&jcirc;oj a&ubreve; &scirc;uteblaj 
      materioj, egala al milono de kuba <ref cel="metr.0o.unuo">metro</ref>;
      simb. l.
   </dif>
  </snc>
  <trd lng="es">litro</trd>
  <trd lng="fr">litre</trd>
  <trd lng="nl">liter</trd>
  <trd lng="ru">&c_l;&c_i;&c_t;&c_r;</trd>
</drv>
</art>
