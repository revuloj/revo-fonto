<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: litr.xml,v 1.34 2024/01/15 19:02:15 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>litr</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="litr.0o">
  <kap><tld/>o</kap>
  <mlg>l</mlg>
  <snc mrk="litr.0o.MEZ">
    <dif>
      Komunuza <ref tip="lst" cel="unu.mezur0o" lst="voko:kvantoj">mezurunuo</ref>, uzata
      por mezuri la <ref cel="volume.0o.FIZ">volumeno</ref>n de likvaj a&ubreve; &scirc;uteblaj materialoj, egala
      al milono de
      <ref cel="metr.kuba0o">kuba metro</ref>:
      <ekz>
        &scirc;i perdis almena&ubreve; kvin <tld/>ojn da sango
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis malgrandbienulo kaj kunportis kelkajn
        <tld/>ojn da lakto kaj dudekon da
        ovoj
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        litron malplenan <klr>(<tld/>ujon)</klr> <klr>[&#x2026;]</klr>
        &scirc;i plenigis da akvo per la
        fontano de la stacidomo
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Deka</lok>
        </fnt>;
      </ekz>
      <ekz>
        cianidenhavo en la eligota akvo <klr>[&#x2026;]</klr> superis
        7&#x202F;mg en litro
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006912.html">Poluado: Cinike cianide &ndash;
          &ccirc;u respondecas Okcidento?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la aerujo de la a&ubreve;to entenas 300 <tld/>ojn da aero
        kunpremita, kio sufi&ccirc;as por 200 km
        <fnt>
          <aut>Last</aut>,
          <vrk>&Ccirc;u ekologiaj a&ubreve;toj el Litovio?</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2001a, numero 12a, p. 15a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_t;&c_r;</trd>
  <trd lng="bg">&c_l;&c_i;&c_t;&c_malmol;&c_r;</trd>
  <trd lng="ca">litre</trd>
  <trdgrp lng="cs">
    <trd>litr</trd>,
    <trd>zn. l</trd>
  </trdgrp>
  <trd lng="de">Liter</trd>
  <trd lng="el">&lambda;&jota_ton;&tau;&rho;&omikron;</trd>
  <trdgrp lng="en">
    <trd>liter</trd>,
    <trd>litre</trd>
  </trdgrp>
  <trd lng="es">litro</trd>
  <trd lng="fa">&a_l;&f_y;&a_t;&a_r;</trd>
  <trd lng="fr">litre</trd>
  <trd lng="he">&lamed;&yod;&tet;&resh;</trd>
  <trd lng="hu">liter</trd>
  <trd lng="id">liter</trd>
  <trd lng="ja">&#x30EA;&#x30C3;&#x30C8;&#x30EB;</trd>
  <trd lng="nl">liter</trd>
  <trd lng="pl">litr</trd>
  <trd lng="pt">litro</trd>
  <trd lng="ro">litru</trd>
  <trd lng="ru">&c_l;&c_i;&c_t;&c_r;</trd>
  <trd lng="sk">liter</trd>
  <trd lng="uk">&c_l;&c_ib;&c_t;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#x516C;&#x5347; <pr>g&#x14D;ngsh&#x113;ng</pr></trd>,
    <trd>&#x5347; <pr>sh&#x113;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="litr._0ujo">
  <kap>
     <tld/>ujo
  </kap>
  <snc>
    <dif>
      Ujo kun kapacito de unu <tld/>o:
      <ekz>
        en la butiko &scirc;i a&ccirc;etis sunfloran oleon, kaj jen,
        kontra&ubreve; la turnokruco la
        <tld/>ujo rompi&gcirc;is
        <fnt>
          <bib>MkM</bib>
          <lok>4</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_t;&c_r;&c_o;&c_v;&c_a;&c_ja; &c_jo;&c_m;&c_ib;&c_s;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="de">Litergef&auml;&szlig;</trd>
  <trd lng="en">liter jar</trd>
  <trd lng="fr">pot d'un litre</trd>
</drv>

 </art>
