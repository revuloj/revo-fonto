<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: litr.xml,v 1.29 2022/03/22 18:26:51 revo Exp $">
    <kap>
      <ofc>2</ofc>
      <rad>litr</rad>/o <fnt><bib>PV</bib></fnt>
    </kap>

    <drv mrk="litr.0o">
      <kap><tld/>o</kap>
      <mlg>l</mlg>
      <snc mrk="litr.0o.MEZ">
        <dif>
          Komunuza <ref tip="lst" cel="unu.mezur0o" lst="voko:kvantoj">mezurunuo</ref> de
          kapacito, uzata por mezuri la volumenon de likvaj a&ubreve; 
          &scirc;uteblaj materialoj, egala al milono de 
          <ref cel="metr.kuba0o">kuba metro</ref>:
          <ekz>
            la aerujo de la a&ubreve;to entenas 300 <tld/>ojn da aero
            kunpremita, 
            kio sufi&ccirc;as por 200 km
            <fnt>
              <aut>Last</aut>,
              <vrk>&Ccirc;u ekologiaj a&ubreve;toj el Litovio?</vrk>, 
              <bib>Monato</bib>,
              <lok>jaro 2001a, numero 12a, p. 15a</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_l;&c_ib;&c_t;&c_r;</trd>
  <trd lng="bg">&c_l;&c_i;&c_t;&c_malmol;&c_r;</trd>
  <trd lng="ca">litre</trd>
  <trdgrp lng="cs">
    <trd>litr</trd>,
    <trd>zn. l</trd>
  </trdgrp>
  <trd lng="de">Liter</trd>
  <trd lng="el">&lambda;&jota_ton;&tau;&rho;&omikron;</trd>
  <trdgrp lng="en">
        <trd>liter</trd>,
        <trd>litre</trd>
      </trdgrp>
  <trd lng="es">litro</trd>
  <trd lng="fa">&a_l;&f_y;&a_t;&a_r;</trd>
  <trd lng="fr">litre</trd>
  <trd lng="he">&lamed;&yod;&tet;&resh;</trd>
  <trd lng="hu">liter</trd>
  <trd lng="id">liter</trd>
  <trd lng="nl">liter</trd>
  <trd lng="pl">litr</trd>
  <trd lng="pt">litro</trd>
  <trd lng="ro">litru</trd>
  <trd lng="ru">&c_l;&c_i;&c_t;&c_r;</trd>
  <trd lng="sk">liter</trd>
  <trd lng="uk">&#x43B;&#x456;&#x442;&#x440;</trd>
</drv>
  </art>
