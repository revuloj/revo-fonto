<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: trauxmatologi.xml,v 1.8 2022/03/28 14:44:12 revo Exp $">
<kap>
  <rad>tra&ubreve;matologi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="trauxmatologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="trauxmatologi.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Parto de kuracarto, precipe
      <ref tip="vid" cel="hxirur.0o.MED">&hcirc;irurgia</ref>,
      kiu koncernas gravajn vundojn, <ref tip="vid" cel="trauxmat.0o.MED">tra&ubreve;matojn</ref>:
      <ekz>
        la scienco pri diversspecaj vundi&gcirc;oj kaj ilia trakto nomi&gcirc;as 
        <tld/>o
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;tra&ubreve;mato&rightquot;, 
        2008-05-13</lok></fnt>;
      </ekz>
      <ekz>
        la usona registaro malpermesis al usonaj kuracistoj partopreni 
        la 20an Internacian Kongreson de Ortopedio kaj <tld lit="T"/>o
        <fnt>
          <aut>M. Gutierrez Gonz&aacute;lez</aut>,
          <vrk><url ref="http://www.radiohc.cu/esperanto/a_resumensemanal/septiembre/09/27/sciencistoj.htm">
          Malspermesas Usono...</url></vrk>,
          <lok>Radio Havano Kubo, 2009-09-27</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_a;&c_w;&c_m;&c_a;&c_t;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="en">traumatology</trd>
  <trd lng="fr">traumatologie</trd>
  <trd lng="it">traumatologia</trd>
  <trd lng="nl">traumatologie</trd>
  <trdgrp lng="pl">
    <trd>traumatologia</trd>,
    <trd>chirurgia urazowa</trd>
  </trdgrp>
  <trd lng="uk">&#x442;&#x440;&#x430;&#x432;&#x43C;&#x430;&#x442;&#x43E;&#x43B;&#x43E;&#x433;&#x456;&#x44F;</trd>
</drv>

<drv mrk="trauxmatologi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="trauxmatologi.0isto.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Fakulo pri <ref tip="vid" cel="trauxmatologi.0o.MED">tra&ubreve;matologio</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_a;&c_w;&c_m;&c_a;&c_t;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="en">traumatologist</trd>
  <trd lng="fr">traumatologiste</trd>
  <trd lng="it">traumatologo</trd>
  <trd lng="nl">traumatoloog</trd>
  <trdgrp lng="pl">
    <trd>traumatolog</trd>,
    <trd>chirurg urazowy</trd>
  </trdgrp>
</drv>
</art>

