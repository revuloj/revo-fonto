<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: precioz.xml,v 1.8 2024/08/06 15:29:37 revo Exp $">
<kap>
  <rad>precioz</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="precioz.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="precioz.0a.KOMUNE">
    <dif>
      <ref tip="vid" cel="rafin.0ita.FIG">Rafinita</ref>
      kaj malbanala, videble celanta artismon,
      eventuale afekta:
      <ekz>
        tia homo, mal&scirc;atanta &leftquot;la <tld/>an, lispan kaj manieran lingvon grekan&rightquot;
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 1 (51)</lok>
        </fnt>;
      </ekz>
      <ekz>
        la longaj, <tld/>aj frazoj de la monologanto, kaj la iomete
        eksmodaj afiksori&ccirc;aj vortoj aperas &ccirc;arme en la bu&scirc;o de &ccirc;i tiu
        simpatia aktoro
        <fnt>
          <aut>K. Kniivil&auml;</aut>,
          <vrk><url ref="http://www.liberafolio.org/2006/duonokulvitro/">
          La Duonokulvitro: duoble pionire</url></vrk>,
          <lok>libera folio, 2006-04-24</lok>
        </fnt>;
      </ekz>
      <ekz>
        Komedio de Moliere La Mokindaj <tld lit="P"/>ulinoj
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_e;&c_c;&c_y;&c_z;&c_y;&c_j;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>preci&oacute;zn&iacute;</trd>,
    <trd>uhlazen&yacute;</trd>,
    <trd>vyum&ecaron;lkovan&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>prezi&ouml;s</trd>,
    <trd>geschraubt <klr>(Stil)</klr></trd>,
    <trd>gek&uuml;nstelt</trd>,
    <trd>schw&uuml;lstig</trd>,
    <trd>&uuml;berladen <klr>(Stil)</klr></trd>
  </trdgrp>
  <trd lng="fr">pr&eacute;cieux<klr> (affect&eacute;)</klr></trd>
  <trdgrp lng="sk">
    <trd>uhladen&yacute;</trd>,
    <trd>vyumelkovan&yacute;</trd>
  </trdgrp>
  <trd lng="uk">&c_p;&c_r;&c_je;&c_c;&c_ib;&c_o;&c_z;&c_n;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#20540;&#37666; <pr>zh&iacute;qi&aacute;n</pr></trd>,
    <trd>&#20540;&#38065; <pr>zh&iacute;qi&aacute;n</pr></trd>,
    <trd>&#23453;&#36149; <pr>b&#462;ogu&igrave;</pr></trd>,
    <trd>&#23542;&#36020; <pr>b&#462;ogu&igrave;</pr></trd>,
    <trd>&#24357;&#36275;&#29645;&#36149; <pr>m&iacute;z&uacute;zh&emacron;ngu&igrave;</pr></trd>,
    <trd>&#24396;&#36275;&#29645;&#36020; <pr>m&iacute;z&uacute;zh&emacron;ngu&igrave;</pr></trd>,
    <trd>&#29645;&#36020; <pr>zh&emacron;ngu&igrave;</pr></trd>,
    <trd>&#29645;&#36149; <pr>zh&emacron;ngu&igrave;</pr></trd>,
    <trd>&#29645; <pr>zh&emacron;n</pr></trd>,
    <trd>&#29646; <pr>zh&emacron;n</pr></trd>,
    <trd>&#29614; <pr>w&ecaron;i</pr></trd>,
    <trd>&#29771; <pr>w&ecaron;i</pr></trd>,
    <trd>&#31054; <pr>y&imacron;</pr></trd>,
    <trd>&#31125; <pr>y&imacron;</pr></trd>,
    <trd>&#31125; <pr>y&imacron;</pr></trd>,
    <trd>&#36020;&#37325; <pr>gu&igrave;zh&ograve;ng</pr></trd>,
    <trd>&#36149;&#37325; <pr>gu&igrave;zh&ograve;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="precioz.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="precioz.0ismo.BELA">
    <uzo tip="fak">BELA</uzo>
    <uzo tip="fak">BELE</uzo>
    <dif>
      Arta <ref tip="super" cel="stil.0o.BELA.loka">stilo</ref>, kiu celas elitan rafinitecon;
      interalie tia beletra maniero de la
      fino de 16a kaj komenco de 17a jarcentoj en E&ubreve;ropo:
      <ekz>
        <tld/>ismo a&ubreve; gongorismo estis ludema per lingva&jcirc;oj
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Luis_de_G%C3%B3ngora">
          Luis de G&oacute;ngora</url>, 2010-07-10</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="barok.0o">baroko</ref>,
      <ref cel="dekade.0ismo.BELA">dekadencismo</ref>,
      <ref cel="manier.0ismo.BELA">manierismo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_e;&c_c;&c_y;&c_z;&c_y;&c_j;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol; <klr>(&c_u; &c_m;&c_a;&c_s;&c_t;&c_a;&c_c;&c_t;&c_v;&c_je;)</klr></trd>
  <trdgrp lng="de">
    <trd>Preziosit&auml;t</trd>,
    <trd>Euphuismus</trd>,
    <trd>Schwulst</trd>
  </trdgrp>
  <trd lng="en">euphuism</trd>
  <trd lng="fr">pr&eacute;ciosit&eacute;<klr> (style)</klr></trd>
</drv>
</art>

