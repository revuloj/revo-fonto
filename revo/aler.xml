<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aler.xml,v 1.10 2024/08/20 21:27:34 revo Exp $">
<kap>
  <rad>aler</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="aler.0o">
  <kap><tld/>o</kap>
  <snc mrk="aler.0o.KON">
    <uzo tip="fak">KON</uzo>
    <fnt><aut>F. Azor&iacute;n</aut> <vrk>Universala Terminologio de la Arkitekturo</vrk>
    <lok>1932</lok></fnt>
    <dif>
      Elstara parto de <ref cel="tegmen.0o">tegmento</ref>:
      <ekz>
        &ccirc;e la domo &ccirc;e <nom>Kr&oacute;lewska</nom> strato la hirundoj ankora&ubreve; nestis sub la <tld/>oj
        <fnt>
          <aut>W. Heller, trad. B. Westerhoff</aut>
          <vrk>Lidia, la Vivo de Lidia Zamenhof, Filino de Esperanto</vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Dach&uuml;berhang</trd>
    <trd lng="es">alero</trd>
    <trd lng="fr">avant-toit</trd>
    <trdgrp lng="pl">
      <trd>ganek</trd>,
      <trd>daszek nad wej&sacute;ciem</trd>
    </trdgrp>
    <trd lng="ro">prisp&abreve;</trd>
  </snc>
  <snc mrk="aler.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="dif" cel="markez.0o.ARKI">Markezo</ref>:
      <ekz>
        mi trovis malanta&ubreve;an pordegon el la korto,
        super kiu estis <tld/>o
        <fnt><aut>B. Vaha</aut>, <vrk>Adolesko</vrk>,
        <lok>1987</lok></fnt>;
      </ekz>
      <ekz>
        papago vokas sub la <tld/>o
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 78a, volumo 2a, p.
        654a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="cs">
      <trd>st&rcaron;&iacute;&scaron;ka nad oknem</trd>,
      <trd>st&rcaron;&iacute;&scaron;ka nade dve&rcaron;mi</trd>
    </trdgrp>
    <trd lng="de">Markise</trd>
    <trdgrp lng="fr">
      <trd>auvent</trd>,
      <trd>marquise</trd>
    </trdgrp>
    <trd lng="pl">markiza</trd>
    <trdgrp lng="sk">
      <trd>dverami</trd>,
      <trd>strie&scaron;ka nad oknom</trd>
    </trdgrp>
    <trd lng="zh">&#22825;&#x5E55; <pr>ti&amacron;nm&#xF9;</pr></trd>
</snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_v;&c_je;&c_s;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_k;&c_ib;&c_z;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&#x5DE;&#x5B7;&#x5E8;&#x5D6;&#x5B5;&#x5D1;</trd>,
    <trd>&#x5D2;&#x5D2;&#x5D5;&#x5DF; &#x5E0;&#x5D2;&#x5D3; &#x5D2;&#x5E9;&#x5DD;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_a;&c_v;&c_ib;&c_s;</trd>,
    <trd>&#x43F;&c_ib;&c_d;&c_d;&c_a;&c_sh;&c_sh;&#x44F;</trd>,
    <trd>&c_z;&c_v;&c_i;&c_s; &#x43F;&#x43E;&#x43A;&c_r;&c_ib;&c_v;&#x43B;&c_ib;</trd>
  </trdgrp>
</drv>
</art>

