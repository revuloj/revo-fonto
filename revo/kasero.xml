<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kasero.xml,v 1.18 2019/06/30 18:21:02 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kaserol</rad>/o
</kap>
<drv mrk="kasero.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Metala kuirpoto kun tenilo, pli profunda ol <ref
      cel="pat.0o">pato</ref>:
      <ekz>
        farunofero, kiu estas bakita en forno a&ubreve; pretigita en <tld/>o a&ubreve;
        sur pato
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 7:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sanktajn oferojn ili kuiris en <ref
        cel="kaldro.0o">kaldronoj</ref>,
        en <ref cel="pot.0o">potoj</ref>, kaj en <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Ii. kroniko 35:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        la krakado de la teleroj kaj pladoj, la mu&gcirc;ado de la patoj, la
        skui&gcirc;ado de la <tld/>oj
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni servas la mitulojn en kolosaj kvantoj, en grandaj <tld/>oj,
        en ilia &scirc;elo
        <fnt>
          <bib>Monato</bib>
          <aut>Petro Desmet'</aut>
          <vrk><url ref="&Monato;/2006/009004.php">Jubile,
          jubilade, gargantuece palatkareseme</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_s;&c_t;&c_r;&c_u;&c_l;&c_ja;</trd>      
  <trd lng="br">kastelodenn</trd>
  <trdgrp lng="ca">
     <trd>casserola (amb m&agrave;nec)</trd>,
     <trd>cassola (amb anses)</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Kasserolle</trd>,
     <trd>Schmorpfanne</trd>
  </trdgrp>
  <trd lng="en">stewpan</trd>
  <trdgrp lng="es">
     <trd>cacerola</trd>,
     <trd>cazo</trd>
  </trdgrp>
  <trd lng="fr">casserole</trd>
  <trd lng="hu">serpeny&odblac;</trd>
  <trdgrp lng="nl">
    <trd>kookpot</trd>,
    <trd>pan</trd>,
    <trd>kasserol</trd>
  </trdgrp>
  <trd lng="pl">rondel</trd>     
  <trd lng="ro">crati&#539;&abreve;</trd>
  <trd lng="ru">&c_k;&c_a;&c_s;&c_t;&c_r;&c_ju;&c_l;&c_ja; <klr>(&c_s; &c_o;&c_d;&c_n;&c_o;&c_j; &c_r;&c_u;&c_ch;&c_k;&c_o;&c_j;)</klr></trd>
  <trd lng="tr">sapl&inodot; tencere</trd>
</drv>
</art>
