<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kasero.xml,v 1.24 2025/07/18 14:34:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kaserol</rad>/o
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="kasero.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Metala kuirpoto kun tenilo, pli profunda ol <ref cel="pat.0o">pato</ref>:
      <ekz>
        farunofero, kiu estas bakita en forno a&ubreve; pretigita en <tld/>o a&ubreve;
        sur pato
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 7:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sanktajn oferojn ili kuiris en <ref cel="kaldro.0o">kaldronoj</ref>,
        en <ref cel="pot.0o">potoj</ref>, kaj en <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Ii. kroniko 35:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        la krakado de la teleroj kaj pladoj, la mu&gcirc;ado de la patoj, la
        skui&gcirc;ado de la <tld/>oj
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni servas la mitulojn en kolosaj kvantoj, en grandaj <tld/>oj,
        en ilia &scirc;elo
        <fnt>
          <bib>Monato</bib>
          <aut>Petro Desmet'</aut>
          <vrk><url ref="&Monato;/2006/009004.php">Jubile,
          jubilade, gargantuece palatkareseme</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_s;&c_t;&c_r;&c_u;&c_l;&c_ja;</trd>
  <trd lng="br">kastelodenn</trd>
  <trdgrp lng="ca">
     <trd>casserola <klr>(amb m&agrave;nec)</klr></trd>,
     <trd>cassola <klr>(amb anses)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Kasserolle</trd>,
     <trd>Schmorpfanne</trd>
  </trdgrp>
  <trd lng="en">stewpan</trd>
  <trdgrp lng="es">
     <trd>cacerola</trd>,
     <trd>cazo</trd>
  </trdgrp>
  <trd lng="fr">casserole</trd>
  <trd lng="hu">serpeny&odblac;</trd>
  <trdgrp lng="ja">
    <trd>&#x30AD;&#x30E3;&#x30BB;&#x30ED;&#x30FC;&#x30EB;</trd>,
    <trd>&#x30B7;&#x30C1;&#x30E5;&#x30FC;&#x934B; 
    <pr>&#x30B7;&#x30C1;&#x30E5;&#x30FC;&#x306A;&#12409;</pr></trd>,
    <trd>&#x30BD;&#x30FC;&#x30B9;&#x30D1;&#x30F3;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kookpot</trd>,
    <trd>pan</trd>,
    <trd>kasserol</trd>
  </trdgrp>
  <trd lng="pl">rondel</trd>
  <trd lng="ro">crati&#x21B;&abreve;</trd>
  <trd lng="ru">&c_k;&c_a;&c_s;&c_t;&c_r;&c_ju;&c_l;&c_ja; <klr>(&c_s; &c_o;&c_d;&c_n;&c_o;&c_j; &c_r;&c_u;&c_ch;&c_k;&c_o;&c_j;)</klr></trd>
  <trd lng="tr">sapl&inodot; tencere</trd>
  <trd lng="uk">&#x43A;&c_a;&c_s;&c_t;&c_r;&c_u;&#x43B;&#x44F; (&c_z; &#x43E;&c_d;&#x43D;&c_ib;&c_jeu;&#x44E;
  &c_r;&c_u;&c_ch;&#x43A;&#x43E;&#x44E;)</trd>
  <trdgrp lng="zh">
    <trd>&#28822;&#38149; <pr>d&#xF9;ngu&#x14D;</pr></trd>,
    <trd>&#x929A; <pr>di&#xE0;o</pr></trd>,
    <trd>&#x6C99;&#x934B; <pr>sh&amacron;gu&#x14D;</pr></trd>
  </trdgrp>
</drv>
</art>
