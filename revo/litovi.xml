<art mrk="$Id: litovi.xml,v 1.19 2024/02/20 09:23:14 revo Exp $">
<kap>
  <rad>Litovi</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="litovi.L0o">
  <kap><tld/>o</kap>
  <snc mrk="litovi.L0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="litov.L0ujo.GEOG">Litovujo</ref>:
      <ekz>
        <tld lit="L"/>o estas lando en la nordoriento de E&ubreve;ropo,
        &ccirc;e la Balta maro.
        <fnt><bib>Viki</bib>, <lok><url ref="&Viki;/Litovio">Litovio</url></lok></fnt>.
      </ekz>
      <ekz>
        &ccirc;iuj ministroj pozitive reagis al lia propono kaj ke neniu ministro de <tld/>o
        plu fumas
        <fnt>

          <vrk><url ref="&Monato;/1993/A930902.php">Noticoj</url></vrk>
          <bib>Monato</bib>
          <lok>1993/09, p. 4</lok>
        </fnt>;
      </ekz>
      <ekz>
        pro malfavora demografia situacio en <tld/>o registaro decidis altigi impostojn
        por socia asekuro
        <fnt>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2000/006806.php">&Ccirc;iea &scirc;parado</url></vrk>
          <bib>Monato</bib>
          <lok>2000/05, p. 7</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
      <trd>&c_L;&c_ib;&c_t;&c_v;&c_a;</trd>,
      <trd>&c_L;&c_je;&c_t;&c_u;&c_v;&c_a;</trd>
    </trdgrp>
  <trd lng="bg">&c_L;&c_i;&c_t;&c_v;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>Litu&agrave;nia</trd>,
    <trd>Rep&uacute;blica de Litu&agrave;nia</trd>
  </trdgrp>
  <trd lng="cs">Litva</trd>
  <trd lng="de">Litauen</trd>
  <trd lng="en">Lithuania</trd>
  <trdgrp lng="fr">
    <trd>Lituanie</trd>,
    <trd>R&eacute;publique de Lituanie</trd>
  </trdgrp>
  <trd lng="id">Lituania</trd>
  <trd lng="ja">&#x30EA;&#x30C8;&#x30A2;&#x30CB;&#x30A2;</trd>
  <trd lng="nl">Litouwen</trd>
  <trdgrp lng="pl">
      <trd>Litwa</trd>,
      <trd>Republika <ind>Litewska</ind></trd>
    </trdgrp>
  <trd lng="sk">Litva</trd>
  <trd lng="uk">&#x41B;&c_i;&c_t;&c_v;&c_a;</trd>
  <trd lng="zh">&#x7ACB;&#38518;&#x5B9B; <pr>L&#xEC;t&#xE1;ow&#x1CE;n</pr></trd>
</drv>
</art>

