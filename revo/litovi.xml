<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Litovi</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="litovi.L0o">
  <kap><tld/>o</kap>
  <snc mrk="litovi.L0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="dif" cel="litov.0ujo.GEOG">Litovujo</ref>
      <ekz>
        <tld lit="L"/>o estas lando en la nordoriento de E&ubreve;ropo,
        &ccirc;e la Balta maro.
        <fnt><bib>Viki</bib>, <lok><url 
        ref="http://eo.wikipedia.org/wiki/Litovio">Litovio</url></lok></fnt>.
      </ekz>
    </dif>
  <trd lng="bg">&c_L;&c_i;&c_t;&c_v;&c_a;</trd>
  <trd lng="de">Litauen</trd>
  <trd lng="en">Lithuania</trd>
  <trd lng="fr">Lituanie</trd>
  <trd lng="nl">Litouwen</trd>
    <trdgrp lng="pl">
      <trd>Litwa</trd>,
      <trd>Republika <ind>Litewska</ind></trd>
    </trdgrp>
  </snc>
</drv>
</art>

