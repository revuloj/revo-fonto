<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: litovi.xml,v 1.12 2019/09/11 22:10:28 revo Exp $">
<kap>
  <rad>Litovi</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="litovi.L0o">
  <kap><tld/>o</kap>
  <snc mrk="litovi.L0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="litov.L0ujo.GEOG">Litovujo</ref>:
      <ekz>
        <tld lit="L"/>o estas lando en la nordoriento de E&ubreve;ropo,
        &ccirc;e la Balta maro.
        <fnt><bib>Viki</bib>, <lok><url 
        ref="http://eo.wikipedia.org/wiki/Litovio">Litovio</url></lok></fnt>.
      </ekz>
      <ekz>
        &ccirc;iuj ministroj pozitive reagis al lia propono kaj ke neniu ministro de <tld/>o
        plu fumas
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9309-noticoj.txt"
              >Esperanto sen peno</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        pro malfavora demografia situacio en <tld/>o registaro decidis altigi impostojn
        por socia asekuro
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Straznickas</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006806.html"
              >Litovio: &Ccirc;iea &scirc;parado</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_L;&c_i;&c_t;&c_v;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>Litu&agrave;nia</trd>,
    <trd>Rep&uacute;blica de Litu&agrave;nia</trd>
  </trdgrp>
  <trd lng="cs">Litva</trd>
  <trd lng="de">Litauen</trd>
  <trd lng="en">Lithuania</trd>
  <trdgrp lng="fr">
    <trd>Lituanie</trd>,
    <trd>R&eacute;publique de Lituanie</trd>
  </trdgrp>
  <trd lng="id">Lituania</trd>
  <trd lng="nl">Litouwen</trd>
  <trdgrp lng="pl">
      <trd>Litwa</trd>,
      <trd>Republika <ind>Litewska</ind></trd>
    </trdgrp>
  <trd lng="sk">Litva</trd>
</drv>
</art>

