<drv mrk="materi1.krud0o">
  <kap>krud<tld/>o</kap>
  <snc mrk="materi1.krud0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Prilaborota <tld/>o,
      <ref tip="sin" cel="krud.0ajxo.materialo">kruda&jcirc;o<sncref/></ref>
      kontraste al <ref tip="ant" cel="produk.0ajxo.EKON">produkto</ref>:
      <ekz>
        montri&gcirc;as la kialoj de krizoj: trokresko de lo&gcirc;antaro, malabundi&gcirc;o de
        krud<tld/>o, poluado
        <fnt>
          <aut>Walter Klag</aut>
            <vrk><url ref="&Monato;/2000/007044.php">De perforto al paco</url></vrk>
            <bib>Monato</bib>
            <lok>2000/08, p. 12</lok>
          </fnt>;
      </ekz>
      <ekz>
        regionaj krud<tld/>oj, precipe ligno, metalo, ledo, &scirc;tofo, &scirc;tono
        <fnt>
          <bib>Monato</bib>
          <aut>Rita R&ouml;ssler-Buckel</aut>
          <vrk><url ref="&Monato;/2007/009174.php">Metio kaj formo tradicio kaj
          progreso</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        rubopapero estas tre valora krud<tld/>o por la paperindustrio
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2005/008409.php">Rubo neniu dubo!</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        kia estas la krud<tld/>o de duonkonduktantoj
        <fnt>
          <aut>K. Georgiev</aut>,
          <vrk><url ref="&STEB;ingxenierado/komputado/f871-p7.htm">
          De sablo &gcirc;is &ccirc;ipo</url></vrk>,
          <lok>Fokuso, 1987:1, p. 7a</lok>
        </fnt>?
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        transformi la krud<tld/>on, liveritan de la vivo, en arton
        <fnt>
          <bib>Monato</bib>
          <aut>Reinaldo Marcelo Ferreira</aut>
          <vrk><url ref="&Monato;/2005/008629.php">Ne nur intimaj temoj</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="materi1.natur0o.EKON">natur<tld/>o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_r;&c_a;&c_v;&c_ib;&c_n;&c_a;</trd>
  <trd lng="cs">surovina</trd>
  <trdgrp lng="de">
    <trd>Rohstoff</trd>,
    <trd>Grundstoff</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>raw material</trd>,
    <trd>crude material <klr>(often plural)</klr></trd>
  </trdgrp>
  <trd lng="fr">mati&egrave;re premi&egrave;re</trd>
  <trdgrp lng="ja">
    <trd>&#x539F;&#x6599;  <pr>&#x3052;&#x3093;&#x308A;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x7D20;&#x6750;  <pr>&#x305D;&#x3056;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="sk">surovina</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&c_r;&#x43E;&c_v;&c_i;&#x43D;&c_a;</trd>,
    <trd>&#x43D;&c_je;&#x43E;&c_b;&c_r;&#x43E;&c_b;&#x43B;&c_je;&#x43D;&c_i;&c_j; &#x43C;&c_a;&c_t;&c_je;&c_r;&c_ib;&c_a;&#x43B;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x539F;&#x6599; <pr>yu&#xE1;nli&#xE0;o</pr></trd>,
    <trd>&#x539F;&#x6750;&#x6599; <pr>yu&#xE1;nc&#xE1;ili&#xE0;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="materi1.natur0o">
  <kap>natur<tld/>o,
  <var><kap>natura <tld/>o</kap></var></kap>
  <snc mrk="materi1.natur0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      &Ccirc;ia <tld/>o trovebla en la naturo,
      sen homa estigo a&ubreve; prilaborado:
      <ekz>
        gaso produktita la&ubreve; naturaj metodoj el naturaj <tld/>oj, ekzemple gaso el
        best-urino
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2007/009177.php">Bone nutrataj a&ubreve;toj</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        Valdorfpupoj estas faritaj &ccirc;efe el natur<tld/>oj
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Valdorfpupo">
          Valdorfpupo</url>, 2010-12-17</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="materi1.krud0o.EKON">krud<tld/>o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_t;&c_u;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y; &c_m;&c_a;&c_t;&c_e;&c_r;&c_y;&c_ja;&c_l;</trd>
  <trd lng="de">Naturmaterial</trd>
  <trd lng="en">natural material <klr>(often plural)</klr></trd>
  <trdgrp lng="fr">
    <trd>mat&eacute;riau naturel</trd>,
    <trd>ressource naturelle</trd>
  </trdgrp>
</drv>
