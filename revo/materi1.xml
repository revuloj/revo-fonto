    <trd>anyagelv&udblac;s&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5B9F;&#x5229;&#x4E3B;&#x7FA9;  <pr>&#x3058;&#x3064;&#x308A;&#x3057;&#x3085;&#x304E;</pr></trd>,
    <trd>&#x7269;&#x8CEA;&#x4E07;&#x80FD;&#x4E3B;&#x7FA9;  <pr>&#x3076;&#x3063;&#x3057;&#x3064;&#x3070;&#x3093;&#x306E;&#x3046;&#x3057;&#x3085;&#x304E;</pr></trd>
  </trdgrp>
