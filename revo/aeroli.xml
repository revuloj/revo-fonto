<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>aerolit</rad>/o
</kap>
<drv mrk="aeroli.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      &Scirc;tono falinta sur teron el la kosma spaco.
    </dif>
    <refgrp tip="sin">
      <ref cel="sxton.aer0o">aer&scirc;tono</ref>,
      <ref cel="meteor.0sxtono">meteor&scirc;tono</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_t;&c_e;&c_a;&c_r;&c_y;&c_t;</trd>
  <trd lng="en">aerolite</trd>
  <trd lng="es">aerolito</trd>
  <trdgrp lng="fr">
    <trd>a&eacute;rolithe</trd>,
    <trd>m&eacute;t&eacute;orite</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>aerolit</trd>,
    <trd>meteork&odblac;</trd>
  </trdgrp>
  <trd lng="nl">a&euml;roliet</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_e;&c_r;&c_o;&c_l;&c_i;&c_t; <klr>(&c_u;&c_s;&c_t;.)</klr></trd>,
    <trd>&c_k;&c_a;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j; <ind>&c_m;&c_je;&c_t;&c_je;&c_o;&c_r;&c_i;&c_t;</ind></trd>
  </trdgrp>
  <trd lng="sk">meteorit</trd>
</drv>
</art>
