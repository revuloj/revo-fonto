<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>sugest</rad>/i
  <fnt>B</fnt>
</kap>
<drv mrk="sugest.0i">
  <kap><tld/>i</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="sugest1.0i">sugestii</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>suggerieren</trd>,
    <trd>einreden</trd>,
    <trd>vorschlagen</trd>
  </trdgrp>
  <trd lng="en">suggest</trd>
  <trdgrp lng="nl">
    <trd>voorstellen</trd>,
    <trd>oppperen</trd>,
    <trd>een <ind>wenk</ind> geven</trd>,
    <trd>suggereren</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_n;&c_u;&c_sh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_n;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
