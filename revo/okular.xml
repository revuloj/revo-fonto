   <trd>oculair</trd>,
   <trd>ooglens</trd>
  </trdgrp>
  <trd lng="pl">okular</trd>
  <trd lng="pt">ocular</trd>
  <trd lng="ru">&c_o;&c_k;&c_u;&c_l;&c_ja;&c_r;</trd>
  <trd lng="sk">okul&#xE1;r</trd>
  <trdgrp lng="tr">
   <trd>ok&uuml;ler</trd>,
   <trd>g&ouml;z merce&gbreve;i</trd>
  </trdgrp>
  <trd lng="uk">&#x43E;&#x43A;&c_u;&#x43B;&#x44F;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#x76EE;&#x93E1; <pr>m&#xF9;j&#xEC;ng</pr></trd>,
    <trd>&#x76EE;&#x955C; <pr>m&#xF9;j&#xEC;ng</pr></trd>,
