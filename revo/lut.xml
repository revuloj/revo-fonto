<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lut</rad>/i
</kap>
<drv mrk="lut.0i">
  <kap>
    <ofc>*</ofc><tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kunfiksi du &scirc;tonecajn pecojn per speciala pasto a&ubreve; mastiko.
    </dif>
  </snc>
  <snc>
    <dif>
      Kunfiksi du metalajn pecojn per metala fandebla miksa&jcirc;o.
    </dif>
    <trd lng="en">solder</trd>
    <trdgrp lng="es">
      <trd>soldar</trd>,
      <trd>esta&nacute;ar</trd>
    </trdgrp>
    <trd lng="nl">solderen</trd>
    <trd lng="pt">soldar</trd>
    <trd lng="ru">&c_p;&c_a;&c_ja;&c_t;&c_mol;</trd>
  </snc>
</drv>
</art>
