<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: raled.xml,v 1.14 2024/04/13 20:15:51 revo Exp $">
<kap><rad>raled</rad>/oj</kap>
<drv mrk="raled.0oj">
  <kap><tld/>oj</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref tip="malprt" cel="ral.0oformaj">raloformaj
      birdoj</ref> (<trd lng="la">Rallidae</trd>):
      <ekz>
        la <ref tip="prt" cel="ral.akvo0o">akvoralo</ref>
        estas akvobirdo de la familio de <tld/>oj 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=212568">Akvobirdoj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj <klr>[&#x2026;]</klr> vivas &ccirc;e akvaj medioj, escepte de la krekso, kiu lo&gcirc;as en
        terkulturejoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=128231">Raledoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_t;&c_u;&c_sh;&c_k;&c_o;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#x2019;&c_ja;)</klr></trd>
  <trd lng="cs">ch&rcaron;&aacute;stalovit&iacute;</trd>
  <trd lng="de">Rallen</trd>
  <trd lng="fr">Rallid&eacute;s</trd>
  <trd lng="hu">guvatf&eacute;l&eacute;k</trd>
  <trd lng="nl">rallen<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pl">chru&sacute;ciele  <klr>(ptaki)</klr></trd>
  <trd lng="pt">ral&iacute;deos</trd>
  <trd lng="ro">ralide</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_u;&c_sh;&c_k;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">chria&scaron;te&lcaron;ovit&eacute;</trd>
  <trd lng="uk">&#x43F;&c_a;&c_s;&c_t;&c_u;&c_sh;&#x43A;&#x43E;&c_v;&c_ib;</trd>
  <trdgrp lng="zh">
    <trd>&#x79E7;&#x96DE;&#x79D1; <pr>y&#x101;ngj&#x12B;k&#x113;</pr></trd>,
    <trd>&#x79E7;&#x9E21;&#x79D1; <pr>y&#x101;ngj&#x12B;k&#x113;</pr></trd>
  </trdgrp>
</drv>
</art>

