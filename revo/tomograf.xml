        <fnt>
          <aut>A. Burkhardt</aut>,
          <vrk><url
          ref="http://groups.yahoo.com/group/ikue/message/1457">
          Adolf salutas</url></vrk>,
          <lok>ikue, 2003-07-22</lok>
        </fnt>;
      </ekz>
      <ekz>
        magnetresonanca <tld/>o
        <fnt>
          <aut>B. Wennergren</aut>,
          <vrk><url
          ref="http://bertilow.com/blogo/2007/10/lambastonoj/">
          Lambastonoj</url></vrk>,
          <lok>2007-10-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">tom&ograve;graf</trd>
  <trd lng="fr">tomographe</trd>
  <trd lng="it">tomografo</trd>
