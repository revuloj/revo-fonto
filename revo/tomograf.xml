<art mrk="$Id: tomograf.xml,v 1.9 2022/03/28 14:44:11 revo Exp $">
<kap>
    <rad>tomograf</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="tomograf.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="tomograf.0o.TEK">
    <uzo tip="fak">SCI</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="vid" cel="tomografi.0o.TEK">Tomografi</ref>ilo;
      aparato kiu esploras objekton per sensadoj a&ubreve; mezuroj
      kaj bildigas &gcirc;ian internon:
      <ekz>
        esploro per komputila <tld/>o rivelis, ke mi suferas pro cerba
        tumoro (gliblastomo) en la dekstra tempia lobo
        <fnt>
          <aut>A. Burkhardt</aut>,
          <vrk><url ref="http://groups.yahoo.com/group/ikue/message/1457">
          Adolf salutas</url></vrk>,
          <lok>ikue, 2003-07-22</lok>
        </fnt>;
      </ekz>
      <ekz>
        magnetresonanca <tld/>o
        <fnt>
          <aut>B. Wennergren</aut>,
          <vrk><url ref="http://bertilow.com/blogo/2007/10/lambastonoj/">
          Lambastonoj</url></vrk>,
          <lok>2007-10-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_m;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
  <trd lng="ca">tom&ograve;graf</trd>
  <trd lng="cs">tomograf</trd>
  <trd lng="fr">tomographe</trd>
  <trd lng="it">tomografo</trd>
  <trd lng="pl">tomograf</trd>
  <trd lng="sk">tomograf</trd>
  <trd lng="uk">&#x442;&#x43E;&#x43C;&#x43E;&#x433;&#x440;&#x430;&#x444;</trd>
</drv>
</art>

