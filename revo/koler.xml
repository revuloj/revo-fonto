<kap>
  <ofc>*</ofc>
  <rad>koler</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="koler.0a">
  <kap><tld/>a</kap>
  <snc mrk="koler.0a.PSI">
    <dif>
      Sentanta fortan malkontenton kontra&ubreve; iu a&ubreve; io:
      <ekz>
        li kun malmodesta spirito kaj <tld/>aj okuloj <klr>...</klr> 
        respondis<klr>...</klr> 
        <fnt>
          <aut>Kristiano, trad. J. Peka&rcaron;</aut>,
          <vrk><url ref="http://www.ikue.org/cz/venceslao/venceslao.htm">
          La Vivoj kaj Martiraj Mortoj de Sankta Venceslao kaj de 
          lia Avino Sankta Ludmila</url></vrk>,
          <lok>1997</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bo">&#xF62;&#xFB3;&#xF74;&#xF44;&#xF0B;&#xF63;&#xF44;&#xF0B;&#xF54;&#xF7C;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>prchliv&yacute;</trd>,
    <trd>vzn&ecaron;tliv&yacute;</trd>,
    <trd>zlostn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>angry</trd>,
    <trd>mad</trd>
  </trdgrp>
  <trd lng="fr">en <ind>col&egrave;re</ind></trd>
  <trd lng="id">marah</trd>
  <trd lng="nl">kwaad</trd>
  <trd lng="no">sint</trd>
  <trd lng="ms">marah</trd>
  <trdgrp lng="pl">
    <trd>rozz&lstroke;oszczony</trd>,
    <trd>rozgniewany</trd>,
    <trd>wkurzony <klr>(pot.)</klr></trd>,
    <trd>wpieniony <klr>(pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hnevliv&yacute;</trd>,
    <trd>prchk&yacute;</trd>,
    <trd>zlostn&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="koler.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="koler.0i.PSI">
    <dif>
      Senti fortan agiton de la animo kontra&ubreve; iu, kiu ofendis
      a&ubreve; malkontentigis nin:
      <ekz>
        <tld/>i iun<fnt>Z</fnt>,
      </ekz>
      <ekz>
        kontra&ubreve; iu<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la batalantoj <tld/>kriis.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bol.0i">boli</ref>,
      <ref cel="eksplo.0i">eksplodi</ref>,
      <ref cel="flam.0igxi">flami&gcirc;i</ref>,
      <ref cel="furioz.0i">furiozi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_n;&c_je;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_l;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_je;&c_r;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hn&ecaron;vat se</trd>,
    <trd>zlobit se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>w&uuml;tend sein</trd>,
    <trd>w&uuml;ten</trd>,
    <trd><klr>(jemandem) </klr>b&ouml;se sein</trd>,
    <trd>zornig sein</trd>
  </trdgrp>
  <trdgrp lng="en"><trd> to feel anger</trd>, <trd>be
  angry</trd></trdgrp>
  <trdgrp lng="es">
    <trd>enfadarse</trd>,
    <trd>entrar en <ind>c&oacute;lera</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&ecirc;tre en <ind>col&egrave;re</ind></trd>,
    <trd>&ecirc;tre en <ind>boule</ind></trd>,
    <trd>&ecirc;tre <ind>courrouc&eacute;</ind></trd>,
    <trd>&ecirc;tre <ind>f&acirc;ch&eacute;</ind></trd>,
    <trd>&ecirc;tre en <ind>p&eacute;tard</ind></trd>,
    <trd>&ecirc;tre en <ind>rogne</ind></trd>,
    <trd>fulminer</trd>,
    <trd>rager</trd>
  </trdgrp>
  <trd lng="hu">haragszik</trd>
  <trd lng="id">marah</trd>
  <trdgrp lng="nl">
    <trd>kwaad zijn</trd>,
    <trd>boos zijn</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>z&lstroke;o&sacute;ci&cacute; si&eogonek;</trd>,
    <trd>gniewa&cacute; si&eogonek;</trd>,
    <trd>wkurza&cacute; si&eogonek; <klr>(pot.)</klr></trd>,
    <trd>wpienia&cacute; si&eogonek; <klr>(pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_r;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_g;&c_n;&c_je;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hneva&tcaron; sa</trd>,
    <trd>jedova&tcaron; sa</trd>,
    <trd>&ccaron;erti&tcaron; sa</trd>
  </trdgrp>
  <trd lng="vo">vut&ouml;n</trd>
  <trd lng="zh">&#x6012;</trd>
</drv>

<drv mrk="koler.0o">
  <kap><tld/>o</kap>
  <snc mrk="koler.0o.PSI">
    <dif>
      <ref tip="lst" cel="sent.0o.emocio" lst="voko:sentoj">Sento</ref> 
      de tiu, kiu <tld/>as:
      <ekz>
        milda respondo kvietigas <tld/>on;
      </ekz>
      <ekz>
        <tld/>o estas mallonga frenezo;
      </ekz>
      <ekz>
        eksciti, inciti ies <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        montri, aperigi sian <tld/>on<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="af">woede</trd>
  <trd lng="am">&#x1241;&#x1323;</trd>
  <trd lng="ar">&#x63A;&#x636;&#x628;</trd>
