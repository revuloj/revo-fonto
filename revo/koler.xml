<drv mrk="koler.0igi">
  <kap><tld/>igi<fnt>Z</fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="koler.0igi.PSI">
    <dif>
      Fari, ke iu <tld/>u.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_l;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_z;&c_l;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>doh&rcaron;&aacute;t</trd>,
    <trd>dop&aacute;lit</trd>,
    <trd>pohn&ecaron;vat</trd>,
    <trd>rozhn&ecaron;vat</trd>,
    <trd>rozzlobit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>w&uuml;tend machen</trd>,
    <trd>aufregen</trd>,
    <trd>zornig machen</trd>
  </trdgrp>
  <trd lng="en">to anger</trd>
  <trdgrp lng="fr">
    <trd>mettre en <ind>col&egrave;re</ind></trd>,
    <trd>mettre en <ind>boule</ind></trd>,
    <trd>mettre en <ind>p&eacute;tard</ind></trd>,
    <trd>mettre en <ind>rogne</ind></trd>,
    <trd>courroucer</trd>,
    <trd>f&acirc;cher</trd>,
    <trd>faire <ind>enrager</ind><klr tip="ind">
    (courroucer)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megharag&iacute;t</trd>,
    <trd>felbosszant</trd>
  </trdgrp>
  <trd lng="nl">kwaad maken</trd>
  <trdgrp lng="pl">
    <trd>z&lstroke;o&sacute;ci&cacute;</trd>,
    <trd>gniewa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_je;&c_r;&c_d;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">rozhneva&tcaron;</trd>
</drv>

<drv mrk="koler.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="koler.0igxi.PSI">
    <dif>
      Eksenti <tld/>on:
      <ekz>
        Aapo <klr>...</klr> <tld/>i&gcirc;is subite, kaptis de la 
        servistino la balailon kaj komencis per vigla mano 
        rebalai la &ccirc;ambregon
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>to become angry</trd>,
    <trd>lose one's temper</trd>
  </trdgrp>
  <trd lng="fr">se mettre en <ind>col&egrave;re</ind></trd>
  <trdgrp lng="pl">
    <trd>rozz&lstroke;o&sacute;ci&cacute; si&eogonek;</trd>,
    <trd>rozgniewa&cacute; si&eogonek;</trd>
  </trdgrp>
</drv>

<drv mrk="koler.0ema">
  <kap><tld/>ema<fnt>Z</fnt>, <var><kap><tld/>i&gcirc;ema</kap></var></kap>
  <snc mrk="koler.0ema.PSI">
    <dif>
      Facile <tld/>anta:
      <ekz>
        li estis aparte <tld/>i&gcirc;ema kaj kverelema
        <fnt>
          <aut>Lao She, trad. Wang Chongfang</aut>,
          <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0026/">
          Kamelo &Scirc;jangzi</url></vrk>,
          <lok>1988</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_n;&c_ja;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_l;&c_a;&c_s;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>quick-tempered</trd>,
    <trd>bad-tempered</trd>, <trd>irascible</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>col&eacute;rique</trd>,
    <trd>col&eacute;reux</trd>,
    <trd>emport&eacute;</trd>,
    <trd>irrascible</trd>,
    <trd>irritable</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schnell durch die Decke</trd>,
    <trd>empfindlich</trd>,
    <trd>schnell zornig</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>haragv&oacute; term&eacute;szet&udblac;</trd>,
    <trd>indulatos</trd>,
    <trd>kolerikus</trd>
  </trdgrp>
  <trd lng="id">pe<ind>marah</ind></trd>
  <trd lng="nl">opvliegend</trd>
  <trdgrp lng="pl">
    <trd>choleryczny</trd>,
    <trd>&lstroke;atwo wpadaj&aogonek;cy w <ind>gniew</ind></trd>,
    <trd>&lstroke;atwo wpadaj&aogonek;cy w <ind>z&lstroke;o&sacute;&cacute;</ind></trd>,
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="koler.0etigi">
  <kap><tld/>etigi</kap>
  <snc mrk="koler.0etigi.PSI">
    <dif>
      <ref tip="vid" cel="pik.0i.inciti">Piki<sncref/></ref>,
      <ref tip="vid" cel="incit.0eti">inciteti</ref>:
      <ekz>
        ku&scirc;is en la rigardo <klr>...</klr> moko,
        malforta moko, kiu <tld/>etigis, malkontentigis
        lin
        <fnt>
          <aut>Argus</aut>
          <vrk>Pro kio?</vrk>
          <lok>1920</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>pozlobit</trd>,
    <trd>&scaron;k&aacute;dlit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>necken</trd>,
    <trd>&auml;rgern</trd>,
    <trd>stipfen<klr> [sudgermane]</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>to irritate</trd>,
    <trd>annoy</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>asticoter</trd>,
    <trd>emb&ecirc;ter</trd>,
    <trd>provoquer<klr tip="ind"> (asticoter)</klr></trd>,
    <trd>taquiner</trd>,
    <trd>titiller<klr tip="ind"> (asticoter)</klr></trd>
  </trdgrp>
  <trd lng="hu">neheztel</trd>
  <trd lng="nl">plagen</trd>
  <trd lng="pl">denerwowa&cacute;</trd>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>dobera&tcaron; si</trd>,
    <trd>dobiedza&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="koler.post0o">
  <kap>post<tld/>o</kap>
  <fnt><bib>PV</bib><lok>post</lok></fnt>
  <snc mrk="koler.post0o.PSI">
    <dif>
      Malafabla sento, kiu restas post <tld/>o,
      <ref tip="sin" cel="rankor.0o">rankoro</ref>:
      <ekz>
        anny forvi&scirc;is la larmojn, kun ioma post<tld/>o
        <fnt>
          <aut>E. Ferber, trad. B. Fabo, red. F. Luin</aut>
          <vrk><url ref="http://i-espero.info/files/elibroj/eo%20-%20ferber,%20edna%20-%20fanny.pdf">
          Fanny</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">rancoeur</trd>
  <trd lng="hu">neheztel&eacute;s</trd>
  <trd lng="id">dendam</trd>
  <trd lng="pl">kac moralny <klr>(pot.)</klr></trd>
  <trd lng="sk">pomstychtivos&tcaron;</trd>
</drv>

<drv mrk="koler.sen0igxi">
  <kap>sen<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="koler.sen0igxi.PSI">
    <dif>
      Trankvili&gcirc;i;
      forlasi sian <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_p;&c_a;&c_k;&c_o;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_p;&c_a;&c_k;&c_o;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_u;&c_c;&c_ib;&c_h;&c_a;&c_m;&c_ib;&c_r;&c_y;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sich <ind>abregen</ind></trd>,
    <trd>sich <ind>beruhigen</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>to regain one's composure</trd>,
    <trd>calm down</trd>
