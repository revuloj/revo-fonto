<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>koler</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="koler.0a">
  <kap><tld/>a</kap>
  <snc mrk="koler.0a.PSI">
    <dif>
      Sentanta fortan malkontenton kontra&ubreve; iu a&ubreve; io:
      <ekz>
        li kun malmodesta spirito kaj <tld/>aj okuloj <klr>...</klr> 
        respondis<klr>...</klr> 
        <fnt>
          <aut>Kristiano, trad. J. Peka&rcaron;</aut>,
          <vrk><url
          ref="http://www.ikue.org/cz/venceslao/venceslao.htm">
          La Vivoj kaj Martiraj Mortoj de Sankta Venceslao kaj de 
          lia Avino Sankta Ludmila</url></vrk>,
          <lok>1997</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">en <ind>col&egrave;re</ind></trd>
</drv>

<drv mrk="koler.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="koler.0i.PSI">
    <dif>
      Senti fortan agiton de la animo kontra&ubreve; iu, kiu ofendis
      a&ubreve; malkontentigis nin:
      <ekz>
        <tld/>i iun<fnt>Z</fnt>,
      </ekz>
      <ekz>
        kontra&ubreve; iu<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la batalantoj <tld/>kriis.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bol.0i">boli</ref>,
      <ref cel="eksplo.0i">eksplodi</ref>,
      <ref cel="flam.0igxi">flami&gcirc;i</ref>,
      <ref cel="furioz.0i">furiozi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_n;&c_je;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_l;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_je;&c_r;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">zlobit se</trd>
  <trd lng="en">angry</trd>
  <trdgrp lng="fr">
    <trd>&ecirc;tre en <ind>col&egrave;re</ind></trd>,
    <trd>&ecirc;tre en <ind>boule</ind></trd>,
    <trd>&ecirc;tre <ind>courrouc&eacute;</ind></trd>,
    <trd>&ecirc;tre <ind>f&acirc;ch&eacute;</ind></trd>,
    <trd>&ecirc;tre en <ind>p&eacute;tard</ind></trd>,
    <trd>&ecirc;tre en <ind>rogne</ind></trd>,
    <trd>fulminer</trd>,
    <trd>rager</trd>
  </trdgrp>
  <trd lng="hu">haragszik</trd>  
  <trdgrp lng="nl">
    <trd>kwaad zijn</trd>,
    <trd>boos zijn</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_r;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_g;&c_n;&c_je;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>w&uuml;tend sein</trd>,
    <trd>w&uuml;ten</trd>,
    <trd><klr>(jemandem) </klr>b&ouml;se sein</trd>,
    <trd>zornig sein</trd>
  </trdgrp>
  <trd lng="vo">vut&ouml;n</trd>
</drv>

<drv mrk="koler.0o">
  <kap><tld/>o</kap>
  <snc mrk="koler.0o.PSI">
    <dif>
      <ref tip="lst" cel="sent.0o.emocio" lst="voko:sentoj">Sento</ref> 
      de tiu, kiu <tld/>as:
      <ekz>
        milda respondo kvietigas <tld/>on;
      </ekz>
      <ekz>
        <tld/>o estas mallonga frenezo;
      </ekz>
      <ekz>
        eksciti, inciti ies <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        montri, aperigi sian <tld/>on<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_l;&c_o;&c_b;&c_a;</trd>,
    <trd>&c_z;&c_l;&c_o;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_n;&c_je;&c_w;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>col&egrave;re</trd>,
    <trd>courroux</trd>,
    <trd>emportement</trd>,
    <trd><ind>foudre</ind>s<klr tip="ind"> (courroux)</klr></trd>,
    <trd>fureur</trd>,
    <trd>ire</trd>,
    <trd>rage</trd>,
    <trd>rogne</trd>
  </trdgrp>
  <trd lng="hu">harag</trd>  
  <trdgrp lng="nl">
    <trd>boosheid</trd>,
    <trd>woede</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_l;&c_o;&c_b;&c_a;</trd>,
    <trd>&c_z;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_g;&c_n;&c_je;&c_v;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Wut</trd>,
    <trd>Zorn</trd>
  </trdgrp>
  <trd lng="vo">vut</trd>
</drv>

<drv mrk="koler.0igi">
  <kap><tld/>igi<fnt>Z</fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="koler.0igi.PSI">
    <dif>
      Fari, ke iu <tld/>u.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_l;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_z;&c_l;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>w&uuml;tend machen</trd>,
    <trd>aufregen</trd>,
    <trd>zornig machen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>mettre en <ind>col&egrave;re</ind></trd>,
    <trd>mettre en <ind>boule</ind></trd>,
    <trd>mettre en <ind>p&eacute;tard</ind></trd>,
    <trd>mettre en <ind>rogne</ind></trd>,
    <trd>courroucer</trd>,
    <trd>f&acirc;cher</trd>,
    <trd>faire <ind>enrager</ind><klr tip="ind">
    (courroucer)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megharag&iacute;t</trd>,
    <trd>felbosszant</trd>
  </trdgrp>
  <trd lng="nl">kwaad maken</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_je;&c_r;&c_d;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="koler.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="koler.0igxi.PSI">
    <dif>
      Eksenti <tld/>on:
      <ekz>
        Aapo <klr>...</klr> <tld/>i&gcirc;is subite, kaptis de la 
        servistino la balailon kaj komencis per vigla mano 
        rebalai la &ccirc;ambregon
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">se mettre en <ind>col&egrave;re</ind></trd>
</drv>

<drv mrk="koler.0ema">
  <kap><tld/>ema<fnt>Z</fnt>, <var><kap><tld/>i&gcirc;ema</kap></var></kap>
  <snc mrk="koler.0ema.PSI">
    <dif>
      Facile <tld/>anta:
      <ekz>
        li estis aparte <tld/>i&gcirc;ema kaj kverelema
        <fnt>
          <aut>Lao She, trad. Wang Chongfang</aut>,
          <vrk><url
          ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0026/">
          Kamelo &Scirc;jangzi</url></vrk>,
          <lok>1988</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_n;&c_ja;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_l;&c_a;&c_s;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>col&eacute;rique</trd>,
    <trd>col&eacute;reux</trd>,
    <trd>emport&eacute;</trd>,
    <trd>irrascible</trd>,
    <trd>irritable</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schnell durch die Decke</trd>,
    <trd>empfindlich</trd>,
    <trd>schnell zornig</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>haragv&oacute; term&eacute;szet&udblac;</trd>,
    <trd>indulatos</trd>,
    <trd>kolerikus</trd>
  </trdgrp>
  <trd lng="nl">opvliegend</trd>
  <trd
  lng="ru">&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="koler.0etigi">
  <kap><tld/>etigi</kap>
  <snc mrk="koler.0etigi.PSI">
    <dif>
      <ref tip="vid" cel="pik.0i.inciti">Piki</ref>,
      <ref tip="vid" cel="incit.0eti">inciteti</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>necken</trd>,
    <trd>&auml;rgern</trd>,
    <trd>stipfen<klr> [sudgermane]</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>asticoter</trd>,
    <trd>emb&ecirc;ter</trd>,
    <trd>provoquer<klr tip="ind"> (asticoter)</klr></trd>,
    <trd>taquiner</trd>,
    <trd>titiller<klr tip="ind"> (asticoter)</klr></trd>
  </trdgrp>
  <trd lng="hu">neheztel</trd>  
  <trd lng="nl">plagen</trd>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="koler.post0o">
  <kap>post<tld/>o</kap>
  <fnt><bib>PV</bib><lok>post</lok></fnt>
  <snc mrk="koler.post0o.PSI">
    <dif>
      Malafabla sento, kiu restas post <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">rancoeur</trd>
  <trd lng="hu">neheztel&eacute;s</trd>  
</drv>

<drv mrk="koler.sen0igxi">
  <kap>sen<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="koler.sen0igxi.PSI">
    <dif>
      Trankvili&gcirc;i;
      forlasi sian <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_p;&c_a;&c_k;&c_o;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_p;&c_a;&c_k;&c_o;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_u;&c_c;&c_ib;&c_h;&c_a;&c_m;&c_ib;&c_r;&c_y;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sich <ind>abregen</ind></trd>,
    <trd>sich <ind>beruhigen</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>calmer</ind><klr tip="ind"> (apr&egrave;s une
    col&egrave;re)</klr></trd>,
    <trd>s'<ind>apaiser</ind><klr tip="ind"> (apr&egrave;s une
    col&egrave;re)</klr></trd>,
    <trd>calmer sa col&egrave;re</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megenyh&uuml;l <klr tip="ind">(harag ut&aacute;n)</klr></trd>,
    <trd>kiengesztel&odblac;dik</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_s;&c_p;&c_o;&c_k;&c_o;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>
</art>
