<drv mrk="koler.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="koler.0igi.PSI">
    <dif>
      Fari, ke iu <tld/>u:
      <ekz>
        li neniam donadis trinkmonon, tio &ccirc;i <tld/>igis la servanton
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u la pastra&ccirc;a babila&jcirc;o povas <tld/>igi tian filozofon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>?
      </ekz>
      <ekz>
        mi ne kura&gcirc;is lin <tld/>igi, malobeante
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sepa</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estas freneza, mi ne lin <tld/>igu
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        li konfuzi&gcirc;is kaj tio <tld/>igis lin
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        diskuti kontra&ubreve; li lin neutile <tld/>igus
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekdua</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;in <tld/>igis, ke li tiel komprenis &scirc;in
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        Dion <tld/>igis, homojn ridigis
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxagre.0i">&ccirc;agreni</ref>,
      <ref cel="furioz.0igi">furiozigi</ref>,
      <ref cel="incit.0i.kolerigi">inciti<sncref/></ref>,
      <ref cel="indign.0igi">indignigi</ref>,
      <ref cel="irit.0i.kolerigi">iriti<sncref/></ref>,
      <ref cel="provok.0i">provoki</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_l;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_z;&c_l;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>doh&rcaron;&aacute;t</trd>,
    <trd>dop&aacute;lit</trd>,
    <trd>pohn&ecaron;vat</trd>,
    <trd>rozhn&ecaron;vat</trd>,
    <trd>rozzlobit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><ind>w&uuml;tend</ind> machen</trd>,
    <trd>aufregen</trd>,
    <trd><ind>zornig</ind> machen</trd>
  </trdgrp>
  <trd lng="en">to anger</trd>
  <trdgrp lng="fr">
    <trd>mettre en <ind>col&egrave;re</ind></trd>,
    <trd>mettre en <ind>boule</ind></trd>,
    <trd>mettre en <ind>p&eacute;tard</ind></trd>,
    <trd>mettre en <ind>rogne</ind></trd>,
    <trd>courroucer</trd>,
    <trd>f&acirc;cher</trd>,
    <trd>faire <ind>enrager</ind><klr tip="ind">
    (courroucer)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megharag&iacute;t</trd>,
    <trd>felbosszant</trd>
  </trdgrp>
  <trd lng="nl">kwaad maken</trd>
  <trdgrp lng="pl">
    <trd>z&lstroke;o&sacute;ci&cacute;</trd>,
    <trd>gniewa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_je;&c_r;&c_d;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">rozhneva&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_n;&c_ib;&c_v;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_je;&c_r;&c_d;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_l;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_l;&c_o;&c_s;&c_t;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_l;&c_ju;&c_t;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_d;&c_r;&c_a;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_v;&c_o;&c_d;&c_i;&c_t;&c_i; &c_z; &c_s;&c_je;&c_b;&c_je;</trd>,
    <trd>&c_b;&c_ib;&c_s;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="koler.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="koler.0igxi.PSI">
    <dif>
      Eksenti <tld/>on:
      <ekz>
        a&ubreve;dante tion, ili <tld/>i&gcirc;is en la koro, kaj grincigis kontra&ubreve; li siajn dentojn
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 7:54</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i komprenis, ru&gcirc;i&gcirc;is kaj iom <tld/>i&gcirc;is
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ne &ccirc;agreni&gcirc;is, li nur <tld/>i&gcirc;is, tio estas alia afero
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ho, mia dol&ccirc;a edzo, ne tiele <tld/>i&gcirc;u kontra&ubreve; la patrino de viaj filoj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Aapo</nom> <klr>[&#8230;]</klr> <tld/>i&gcirc;is subite, kaptis de la 
        servistino la balailon kaj komencis per vigla mano 
        rebalai la &ccirc;ambregon
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>in <ind>Wut</ind> geraten</trd>,
    <trd><ind>zornig</ind> werden</trd>,
    <trd>erz&uuml;rnen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>to become angry</trd>,
    <trd>lose one's temper</trd>
  </trdgrp>
  <trd lng="fr">se mettre en <ind>col&egrave;re</ind></trd>
  <trdgrp lng="pl">
    <trd>rozz&lstroke;o&sacute;ci&cacute; si&eogonek;</trd>,
    <trd>rozgniewa&cacute; si&eogonek;</trd>
  </trdgrp>
</drv>

<drv mrk="koler.0ema">
  <kap><tld/>ema, <var><kap><tld/>i&gcirc;ema</kap></var></kap>
  <snc mrk="koler.0ema.PSI">
    <dif>
      Facile <tld/>anta:
      <ekz>
        <tld/>ema homo ka&ubreve;zas malpacon
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 15:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        malpacema kaj <tld/>ema edzino
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 21:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis <klr>[&#8230;]</klr> tiel <tld/>i&gcirc;ema, kvaza&ubreve; li havus en si pipron kaj
        tabakon
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun <tld/>ema spirito mi naski&gcirc;is
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio</lok>
        </fnt>;
      </ekz>
      <ekz>
        se vi lin indulgos, li fari&gcirc;os ankora&ubreve; pli <tld/>ema
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 19:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis aparte <tld/>i&gcirc;ema kaj kverelema
        <fnt>
          <aut>Lao She, trad. Wang Chongfang</aut>,
          <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0026/">
          Kamelo &Scirc;jangzi</url></vrk>,
          <lok>1988</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_n;&c_ja;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_l;&c_a;&c_s;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>quick-tempered</trd>,
    <trd>bad-tempered</trd>, <trd>irascible</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>col&eacute;rique</trd>,
    <trd>col&eacute;reux</trd>,
    <trd>emport&eacute;</trd>,
    <trd>irrascible</trd>,
    <trd>irritable</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schnell durch die Decke</trd>,
    <trd>empfindlich</trd>,
    <trd>schnell zornig</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>haragv&oacute; term&eacute;szet&udblac;</trd>,
    <trd>indulatos</trd>,
    <trd>kolerikus</trd>
  </trdgrp>
  <trd lng="id">pe<ind>marah</ind></trd>
  <trd lng="nl">opvliegend</trd>
  <trdgrp lng="pl">
    <trd>choleryczny</trd>,
    <trd>&lstroke;atwo wpadaj&aogonek;cy w <ind>gniew</ind></trd>,
    <trd>&lstroke;atwo wpadaj&aogonek;cy w <ind>z&lstroke;o&sacute;&cacute;</ind></trd>,
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_r;&c_a;&c_t;&c_ib;&c_v;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_d;&c_r;&c_a;&c_t;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_d;&c_r;&c_a;&c_z;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="koler.0etigi">
  <kap><tld/>etigi</kap>
  <snc mrk="koler.0etigi.PSI">
    <dif>
      <ref tip="vid" cel="pik.0i.inciti">Piki<sncref/></ref>,
      <ref tip="vid" cel="incit.0eti">inciteti</ref>:
      <ekz>
        ku&scirc;is en la rigardo <klr>[&#8230;]</klr> moko,
        malforta moko, kiu <tld/>etigis, malkontentigis
        lin
        <fnt>
          <aut>Argus</aut>
          <vrk>Pro kio?</vrk>
          <lok>1920</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>pozlobit</trd>,
    <trd>&scaron;k&aacute;dlit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>necken</trd>,
    <trd>&auml;rgern</trd>,
    <trd>stipfen<klr> [sudgermane]</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>to irritate</trd>,
    <trd>annoy</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>asticoter</trd>,
    <trd>emb&ecirc;ter</trd>,
    <trd>provoquer<klr tip="ind"> (asticoter)</klr></trd>,
    <trd>taquiner</trd>,
    <trd>titiller<klr tip="ind"> (asticoter)</klr></trd>
  </trdgrp>
  <trd lng="hu">neheztel</trd>
  <trd lng="nl">plagen</trd>
  <trd lng="pl">denerwowa&cacute;</trd>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_d;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>dobera&tcaron; si</trd>,
    <trd>dobiedza&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="koler.post0o">
  <kap>post<tld/>o</kap>
  <fnt><bib>PV</bib><lok>post</lok></fnt>
  <snc mrk="koler.post0o.PSI">
    <dif>
      Malafabla sento, kiu restas post <tld/>o,
      <ref tip="sin" cel="rankor.0o">rankoro</ref>:
      <ekz>
        <nom>Fanny</nom> forvi&scirc;is la larmojn, kun ioma post<tld/>o
        <fnt>
          <aut>E. Ferber, trad. B. Fabo, red. F. Luin</aut>
          <vrk><url
          ref="http://i-espero.info/files/elibroj/eo%20-%20ferber,%20edna%20-%20fanny.pdf">
          Fanny</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="rankor.0o">rankoro</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Groll</trd>,
    <trd>Verbitterung</trd>
  </trdgrp>
  <trd lng="fr">rancoeur</trd>
  <trd lng="hu">neheztel&eacute;s</trd>
  <trd lng="id">dendam</trd>
  <trd lng="pl">kac moralny <klr>(pot.)</klr></trd>
  <trd lng="sk">pomstychtivos&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_l;&c_o;&c_p;&c_a;&c_m;&#8217;&c_ja;&c_t;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_l;&c_o;&c_p;&c_a;&c_m;&#8217;&c_ja;&c_t;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_z;&c_l;&c_o;&c_p;&c_a;&c_m;&#8217;&c_ja;&c_t;&c_l;&c_i;&c_v;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_l;&c_o;&c_b;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="koler.sen0igxi">
  <kap>sen<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="koler.sen0igxi.PSI">
    <dif>
      <ref tip="super" cel="trankv.0igxi">Trankvili&gcirc;i</ref>; forlasi sian <tld/>on:
      <ekz>
        mi jam duoble mensogis, do <nom>Zukrula</nom> sen<tld/>i&gcirc;is
        <fnt>
          <bib>VaK</bib>
          <lok>Dekkvina &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_p;&c_a;&c_k;&c_o;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_p;&c_a;&c_k;&c_o;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_u;&c_c;&c_ib;&c_h;&c_a;&c_m;&c_ib;&c_r;&c_y;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sich <ind>abregen</ind></trd>,
    <trd>sich <ind>beruhigen</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>to regain one's composure</trd>,
    <trd>calm down</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>calmer</ind><klr tip="ind"> (apr&egrave;s une
    col&egrave;re)</klr></trd>,
    <trd>s'<ind>apaiser</ind><klr tip="ind"> (apr&egrave;s une
    col&egrave;re)</klr></trd>,
    <trd>calmer sa col&egrave;re</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megenyh&uuml;l <klr tip="ind">(harag ut&aacute;n)</klr></trd>,
    <trd>kiengesztel&odblac;dik</trd>
  </trdgrp>
  <trd lng="pl">uspokoi&cacute; si&eogonek;</trd>
  <trd lng="ru">&c_u;&c_s;&c_p;&c_o;&c_k;&c_o;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>
