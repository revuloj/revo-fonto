<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>hemipter</rad>/oj
</kap>
<drv mrk="hemipt.0oj">
<kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="insekt.0oj">insektoj</ref>
      (<trd lng="la">Heteroptera</trd>), de su&ccirc;insektoj kun du paroj da
      flugiloj, kies ekstera paro estas ofte korneca.
    </dif>
    <ref tip="vid" cel="homopt.0oj">homopteroj</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Wanzen</trd>,
    <trd>Ungleichfl&uuml;gler</trd>,
    <trd>Halbfl&uuml;gler</trd>
  </trdgrp>
</drv>
<drv mrk="hemipt.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Apartenanta al <tld/>oj.  
    </dif>
  </snc>
</drv>
</art>
<!--
$Log$
-->
</vortaro>