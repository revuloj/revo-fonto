<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hemipter</rad>/oj
</kap>
<drv mrk="hemipt.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="insekt.0oj">insektoj</ref>
      (<trd lng="la">Heteroptera</trd>), kun du paroj da
      flugiloj, kies ekstera paro estas ofte korneca.
    </dif>
    <ref tip="vid" cel="homopt.0oj">homopteroj</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Wanzen</trd>,
    <trd>Ungleichfl&uuml;gler</trd>,
    <trd>Halbfl&uuml;gler</trd>
  </trdgrp>
  <trd lng="fr">h&eacute;mipt&egrave;res</trd>
  <trd lng="nl">snavelinsecten</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_zh;&c_je;&c_s;&c_t;&c_k;&c_o;&c_k;&c_r;&c_y;&c_l;&c_y;&c_je;</trd>
</drv>

<drv mrk="hemipt.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Apartenanta al <tld/>oj.
    </dif>
  </snc>
  <trd lng="fr">h&eacute;mipt&egrave;re<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_zh;&c_je;&c_s;&c_t;&c_k;&c_o;&c_k;&c_r;&c_y;&c_l;&c_y;&c_j;</trd>
</drv>
</art>
