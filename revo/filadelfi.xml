    <kap>
      <rad>Filadelfi</rad>/o
    </kap>

<drv mrk="filadelfi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Granda <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      en <ref tip="malprt" cel="uson.U0o">Usono</ref>,
      &ccirc;. 130&#x202F;km sudokcidente de <ref cel="jork.nov0o.GEOG_urbo">Nov-Jorko</ref>:
      <ekz>
        la nomon &leftquot;megalopolo&rightquot; li donis <klr>[&#x2026;]</klr>, inter aliaj, <klr>[al]</klr> la
        urboj Novjorko, <tld/>o kaj Baltimoro
        <fnt>
          <aut>Dj&eacute;mil Kessous</aut>
          <vrk><url ref="&Monato;/2011/010326.php">Plaro, a&ubreve; la Flava Terpomo</url></vrk>
          <bib>Monato</bib>
          <lok>2011/5, p. 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        nia hodia&ubreve;a preleganto estas d-ro <nom>Brown</nom> el <tld/>o
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2003/008032.html">Monda lingvo a&ubreve; mondaj lingvoj</url></vrk>
          <bib>Monato</bib>
          <lok>2003/11, p. 19</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi estas instruisto de la angla por internaciaj universitataj studentoj en
        <tld/>o
        <fnt>
          <aut>Bill A.</aut>
          <vrk><url ref="https://www.meetup.com/de-DE/Esperanto-Philadelphia/member/184818189/">Esperanto
          Philadelphia</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_F;&c_ib;&c_l;&c_ja;&c_d;&c_e;&c_l;&c_mol;&c_f;&c_ib;&c_ja;</trd>
    <trd lng="de">Philadelphia</trd>
    <trd lng="en">Philadelphia</trd>
    <trd lng="fr">Philadelphie</trd>
    <trd lng="nl">Philadelphia</trd>
    <trd lng="ro">Philadelphia</trd>
  </snc>
  <trd lng="ja">&#x30D5;&#x30A3;&#x30E9;&#x30C7;&#x30EB;&#x30D5;&#x30A3;&#x30A2;</trd>
  <trd lng="uk">&c_F;&c_ib;&#x43B;&c_a;&c_d;&c_je;&#x43B;&#x44C;&c_f;&c_ib;&#x44F; (&c_sh;&c_t;&c_a;&c_t;)</trd>
</drv>

