<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: tuci.xml,v 1.2 2020/03/20 23:10:52 revo Exp $">
    <kap>
      <rad>tuci</rad>/o
    </kap>

<drv mrk="tuci.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Ano de socia grupo, <ref tip="malprt" cel="kast.0o">kasto</ref>,
      en la
      orientafrikaj &scirc;tatoj <ref tip="malprt"
      cel="ruand.R0o">Ruando</ref>,
      <ref tip="malprt" cel="burund.B0o">Burundo</ref> kaj en
      la orienta limregiono de <ref tip="malprt"
      cel="kong.K0oKinsxasa">Kongo Kin&scirc;asa</ref>:
      <ekz>
        <klr>[post la morto de]</klr> la unua hutua prezidanto
        <klr>[&#8230;]</klr> <tld/>oj
        prenis la povon
        <fnt>
          <bib>Monato</bib>
          <aut>Sabiyumva J&eacute;r&eacute;mie</aut>
          <vrk><url ref="https://www.monato.be/2004/008387.html"
              >Survoje al paco</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kiam oni enkondukos memortagon pri tiuj, kiuj estis bu&ccirc;itaj
        <klr>[&#8230;]</klr>:
        armenoj, a&ubreve;straliaj aborigenoj, bengaloj, hutuoj, <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="https://www.monato.be/2013/010969.php"
              >Amara kromgusto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Tutsi</trd>
    <trd lng="pt">tutsi</trd>
  </snc>
</drv>

  </art>

