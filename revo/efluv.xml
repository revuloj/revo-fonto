<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: efluv.xml,v 1.17 2023/11/07 19:22:22 revo Exp $">
<kap>
  <rad>efluv</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="efluv.0o">
  <kap><tld/>o</kap>
  <snc mrk="efluv.0o.FIZ">
  <uzo tip="fak">FIZ</uzo>
    <dif>
      Elradiado kiel fizika apera&jcirc;o, kiu ofte produktas videblajn
      fenomenojn. 
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_c;&c_ja;&c_k;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_v;&c_y;&c_d;&c_z;&c_ja;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Glimmentladung</trd>,
      <trd>Abstrahlung</trd>
    </trdgrp>
    <trd lng="en">glow discharge</trd>
    <trd lng="fa">&a_t;&a_kh;&a_l;&f_y;&a_h;&a_hamza_sure; &a_t;&a_A;&a_b;&a_A;&a_n;</trd>
    <trdgrp lng="hu">
      <trd>par&aacute;zsf&eacute;nykis&uuml;l&eacute;s</trd>,
      <trd>glimmkis&uuml;l&eacute;s</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>efl&uacute;vio</trd>,
      <trd>emana&ccedil;&atilde;o</trd>,
      <trd>exala&ccedil;&atilde;o</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_i;&c_s;&c_t;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_v;&c_y;&c_d;&c_je;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc mrk="efluv.0o.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Elektra mal&scirc;argo, kiu sin manifestas &ccirc;irka&ubreve; kondukilo
      trovi&gcirc;anta en la aero, sekve de l' jonizo de la
      atmosfero, kiam la elektra tensio sur la surfaco
      de tiuj &ccirc;i kondukiloj atingas tre altan valoron:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>o de impresoj
        <fnt>
          <aut>Larbar Sara</aut>
          <vrk><url ref="https://www.scribd.com/document/352322283/Karuseloj-Larbar-Sara">Karuseloj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_l;&c_je;&c_ju;&c_ch;&c_y; <ind>&c_r;&c_a;&c_z;&c_r;&c_a;&c_d;</ind></trd>
    <trdgrp lng="de">
      <trd>Koronaentladung</trd>,
      <trd>Elmsfeuer</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_t;&a_kh;&a_l;&f_y;&a_h;</trd>,
      <trd>&a_t;&a_kh;&a_l;&f_y;&a_h;&a_hamza_sure; &a_A;&a_l;&f_k;&a_t;&a_r;&f_y;&f_k;&f_y;</trd>
    </trdgrp>
    <trd lng="fr">d&eacute;charge luminescente</trd>
    <trd lng="hu">kis&uuml;l&eacute;s</trd>
    <trdgrp lng="pt">
      <trd>efl&uacute;vio</trd>,
      <trd>emana&ccedil;&atilde;o</trd>,
      <trd>exala&ccedil;&atilde;o</trd>
    </trdgrp>
    <trd lng="ru">&c_t;&c_l;&c_je;&c_ju;&c_shch;&c_i;&c_j; &c_r;&c_a;&c_z;&c_r;&c_ja;&c_d;</trd>
    <trd lng="zh">&#x5723;&#x827E;&#x5C14;&#x6469;&#x4E4B;&#x706B; <pr>sh&#xE8;ng&#xE0;i&#x11B;rm&#xF3;zh&#x12B;hu&#x1D2;</pr></trd>
</snc>
  <trdgrp lng="cs">
    <trd>nep&rcaron;&iacute;jemn&yacute; v&yacute;par</trd>,
    <trd>pach</trd>
  </trdgrp>
  <trd lng="ja">&#x30B0;&#x30ED;&#x30FC;&#x653E;&#x96FB;  <pr>&#x30B0;&#x30ED;&#x30FC;&#x307B;&#x3046;&#x3067;&#x3093;</pr></trd>
  <trdgrp lng="sk">
    <trd>nepr&iacute;jemn&yacute; v&yacute;par</trd>,
    <trd>pach</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x438;&#x442;&#x456;&#x43A;&#x430;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x432;&#x438;&#x434;&#x456;&#x43B;&#x435;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
