<drv mrk="foir.0ejo">
  <kap><tld/>ejo</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Vasta <ref tip="super" cel="vend.0ejo">vendejo</ref>,
      ordinare sub&ccirc;iela, en kiu okazas <tld/>o:
      <ekz>
        la Lepsika <tld/>ejo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        granda, &ccirc;irka&ubreve;ita de altaj frontonaj domoj urba <tld/>ejo
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="bazar.0o.placo">bazaro</ref>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_r;&c_m;&c_a;&c_sh;&c_o;&c_v;&c_a;&c_ja; &c_p;&c_l;&c_o;&c_sh;&c_ch;&c_a;</trd>
  <trdgrp lng="cs">
