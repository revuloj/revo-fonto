<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>foir</rad>/o
</kap>
<drv mrk="foir.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Granda komerca kunveno, je perioda dato, por elmontri k vendi varojn. 
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Messe</trd>,
     <trd>Markt</trd></trdgrp>
  <trd lng="en">fair</trd>
  <trd lng="ru">&c_ja;&c_r;&c_m;&c_a;&c_r;&c_k;&c_a;</trd>
</drv>
<drv mrk="foir.0ejo">
  <kap><tld/>ejo</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Loko en kiu okazas <tld/>o. 
    </dif>
    <refgrp tip="sin">
      <ref cel="bazar.0o">bazaro</ref>,
      <ref cel="merkat.0o">merkato</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Marktplatz</trd>,
     <trd>Messeplatz</trd></trdgrp>
</drv>
</art>
