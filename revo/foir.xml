<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>foir</rad>/o
</kap>
<drv mrk="foir.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Granda perioda kunveno por elmontro kaj vendo de komerca&jcirc;oj:
      <ekz>
        unu el la plej gravaj internaciaj <tld/>oj de ma&scirc;inoj por
        lignoprilaborado<fnt><aut>Johano Pachter</aut>: <vrk>&leftquot;Ligna
        Hannover&rightquot;: monda foiro pri ligno</vrk>, <bib>Monato</bib><lok>jaro
        1997a, numero 9a, p. 14a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_r;&c_m;&c_a;&c_sh;</trd>
  <trdgrp lng="de">
    <trd>Messe</trd>,
    <trd>Markt</trd>
  </trdgrp>
  <trd lng="en">fair</trd>
  <trd lng="es">feria</trd>
  <trd lng="fr">foire</trd>
  <trd lng="hu">v&aacute;s&aacute;r</trd>
  <trd lng="nl">beurs</trd>
  <trdgrp lng="pt">
    <trd>feira</trd>,
    <trd>exposi&#231;&#227;o</trd>
  </trdgrp>
  <trd lng="ru">&c_ja;&c_r;&c_m;&c_a;&c_r;&c_k;&c_a;</trd>
</drv>

<drv mrk="foir.0ejo">
  <kap><tld/>ejo</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Vasta <ref tip="super" cel="vend.0ejo">vendejo</ref>,
      ordinare sub&ccirc;iela, en kiu okazas <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Marktplatz</trd>,
    <trd>Messeplatz</trd>
  </trdgrp>
  <trd lng="en">fairgrounds</trd>
  <trdgrp lng="fr">
    <trd>champ de foire</trd>,
    <trd>foirail</trd>
  </trdgrp>
  <trd lng="hu">v&aacute;s&aacute;rt&eacute;r</trd>
  <trd lng="nl">beursplein</trd>
  <trdgrp lng="pt">
    <trd>feira <klr tip="amb">(local)</klr></trd>,
    <trd>exposi&#231;&#227;o <klr tip="amb">(local)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_ja;&c_r;&c_m;&c_a;&c_r;&c_o;&c_ch;&c_n;&c_a;&c_ja; &c_p;&c_l;&c_o;&c_shch;&c_a;&c_d;&c_mol;</trd>
</drv>
</art>
