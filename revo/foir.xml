<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: foir.xml,v 1.27 2021/05/30 19:15:57 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>foir</rad>/o
</kap>

<drv mrk="foir.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Granda perioda kunveno por elmontro kaj vendo de komerca&jcirc;oj:
      <ekz>
        unu el la plej gravaj internaciaj <tld/>oj de ma&scirc;inoj por
        lignoprilaborado<fnt><aut>Johano Pachter</aut>: <vrk>&leftquot;Ligna
        Hannover&rightquot;: monda foiro pri ligno</vrk>,
        <bib>Monato</bib><lok>jaro
        1997a, numero 9a, p. 14a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="af">billike</trd>
  <trd lng="am">&#x134D;&#x1275;&#x1203;&#x12CA;</trd>
  <trd lng="ar">&#x639;&#x627;&#x62F;&#x644;</trd>
  <trd lng="be">&c_k;&c_ib;&c_r;&c_m;&c_a;&c_sh;</trd>
  <trd lng="bg">&c_p;&c_a;&c_n;&c_a;&c_i;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>trh</trd>,
    <trd>trh. v&yacute;stava</trd>,
    <trd>veletrh</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Messe</trd>,
    <trd>Markt</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>fair</trd>,
    <trd>market</trd>
  </trdgrp>
  <trd lng="es">feria</trd>    
  <trdgrp lng="fa">
    <trd>&a_n;&a_m;&a_A;&f_y;&a_sh;&f_g;&a_A;&a_h;</trd>,
    <trd>&a_b;&a_A;&a_z;&a_A;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">foire</trd>
  <trd lng="hu">v&aacute;s&aacute;r</trd>
  <trd lng="nl">beurs</trd>
  <trdgrp lng="pt">
    <trd>feira</trd>,
    <trd>exposi&ccedil;&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_ja;&c_r;&c_m;&c_a;&c_r;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>trh</trd>,
    <trd>ve&lcaron;trh</trd>
  </trdgrp>
  <trd lng="sq">drejt&#xEB;</trd>
</drv>

<drv mrk="foir.0ejo">
  <kap><tld/>ejo</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Vasta <ref tip="super" cel="vend.0ejo">vendejo</ref>,
      ordinare sub&ccirc;iela, en kiu okazas <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>tr&zcaron;i&scaron;t&ecaron;</trd>,
    <trd>v&yacute;stavi&scaron;t&ecaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Marktplatz</trd>,
    <trd>Messeplatz</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>fairground</trd>,
    <trd>marketplace</trd>
  </trdgrp>
  <trd lng="es">recinto ferial</trd>    
  <trd lng="fa">&a_m;&a_H;&a_l; <ind>&a_n;&a_m;&a_A;&f_y;&a_sh;&f_g;&a_A;&a_h;</ind></trd>
  <trdgrp lng="fr">
    <trd>champ de foire</trd>,
    <trd>foirail</trd>
  </trdgrp>
  <trd lng="hu">v&aacute;s&aacute;rt&eacute;r</trd>
  <trd lng="nl">beursplein</trd>
  <trdgrp lng="pt">
    <trd>feira <klr tip="amb">(local)</klr></trd>,
    <trd>exposi&ccedil;&atilde;o <klr tip="amb">(local)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_ja;&c_r;&c_m;&c_a;&c_r;&c_o;&c_ch;&c_n;&c_a;&c_ja; &c_p;&c_l;&c_o;&c_shch;&c_a;&c_d;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>tr&zcaron;nica</trd>,
    <trd>ve&lcaron;tr&zcaron;n&aacute; hala</trd>
  </trdgrp>
</drv>
</art>
