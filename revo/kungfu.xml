<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kungfu.xml,v 1.9 2023/12/03 09:59:03 revo Exp $">
<kap>
  <rad>kungfu</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="kungfu.0o">
  <kap><tld/>o</kap>
  <snc mrk="kungfu.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="dif" cel="usxu.0o.SPO">U&scirc;uo</ref>:
      <ekz>
        karateo kaj <tld/>o <klr>...</klr> ne tre similas, la&ubreve; mi,
        kvankam amba&ubreve; havas tre diversajn formojn
        <fnt>
        <aut>E. Grimley-Evans</aut>,
        <vrk><url ref="http://www.rano.org/piv2.html">PIV2, Korektoj
        jam faritaj...</url></vrk>,
        <lok>2002</lok>
        </fnt>;
      </ekz>
      <ekz>
        praktikantoj de la luktarto <tld/>o enscenigis rakonttemon pri ven&gcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Fidilalao Henriel</aut>
          <vrk><url ref="&Monato;/2007/009064.php">Kino malagasa furoras</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="kungfu.0o.RIM_ETIM">
    La internacii&gcirc;inta en la 1970aj jaroj vorto <em>kungfuo</em>
    signifas nur &leftquot;arton&rightquot; &leftquot;majstradon&rightquot;. La &ccirc;ina nomo estas
    &leftquot;<nac>wu shu</nac>&rightquot; a&ubreve; &leftquot;<nac>quan fa</nac>&rightquot;. Pli novaj
    artikoloj el &Ccirc;inio
    esperantigas &gcirc;in per <em>u&scirc;uo</em>.
  </rim>
  <!-- prefere traduku nur &ccirc;e u&scirc;uo -->
  <trdgrp lng="en">
    <trd>kung fu</trd>,
    <trd>wushu</trd>
  </trdgrp>
  <trd lng="sk">kun-gfu</trd>
  <trd lng="cs">kungfu</trd>
  <trd lng="pl">kung-fu</trd>
  <trdgrp lng="ro">
    <trd>kung-fu</trd>,
    <trd>Wushu</trd>
  </trdgrp>   
  <trdgrp lng="uk">
    <trd>&#x43A;&#x443;&#x43D;&#x433;&#x444;&#x443;</trd>,
    <trd>&#x43A;&#x443;&#x43D;&#x433;-&#x444;&#x443;</trd>,
    <trd>&#x43A;&#x443;&#x43D;&#x444;&#x443;</trd>,
    <trd>&#x43A;&#x443;&#x43D;-&#x444;&#x443;</trd>
  </trdgrp>
  <trd lng="zh">&#x529F;&#x592B; <pr>g&#x14D;ngfu</pr></trd>
</drv>
</art>

