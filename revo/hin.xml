<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hin.xml,v 1.17 2025/10/12 22:29:13 revo Exp $">
<kap>
  <rad>hin</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="hin.0o">
  <kap><tld/>o</kap>
  <snc mrk="hin.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Ido de <ref cel="cxeval.vir0o">vir&ccirc;evalo</ref> kaj
      <ref cel="azen.0o.ZOO">azen</ref>ino:
      <ekz>
        la mulo, kiu estas ido de maskla azeno kaj femala
        &ccirc;evalo, pli similas al azeno ol la <tld/>o
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 8a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mul.0o">mulo</ref>
    </refgrp>
  </snc>
  <ref tip="hom" cel="hin1.0o">hino (antikva mezurunuo)</ref>
  <trd lng="be">&c_l;&c_a;&c_sh;&c_a;&c_k;</trd>
  <trd lng="bg">&c_m;&c_u;&c_l;&c_je;</trd>
  <trdgrp lng="ca">
    <trd>mul <klr tip="amb">(h&iacute;brid de cavall i somera)</klr></trd>,
    <trd>matxo <klr tip="amb">(h&iacute;brid de cavall i somera)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hin</trd>,
    <trd>h&iacute;n</trd>
  </trdgrp>
  <trd lng="de">Maulesel</trd>
  <trd lng="en">hinny</trd>
  <trdgrp lng="fa">
    <trd>&a_q;&a_A;&a_T;&a_r; &f_p;&a_w;&a_z;&f_g;&f_y;</trd>,
    <trd>&a_kh;&a_r; &f_p;&a_w;&a_z;&f_g;&f_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bardeau</trd>,
    <trd>bardot</trd>
  </trdgrp>
  <trd lng="it">bardotto</trd>
  <trd lng="pl">os&lstroke;omu&lstroke;</trd>
  <trd lng="pt">burro</trd>
  <trd lng="ru">&c_l;&c_o;&c_sh;&c_a;&c_k;</trd>
  <trd lng="sk">mulica</trd>
  <trd lng="uk">&c_o;&c_s;&c_l;&c_ju;&c_k;</trd>
  <trd lng="zh">&#39522;&#39486; <pr>l&uacute;:lu&oacute;</pr></trd>
</drv>
</art>
