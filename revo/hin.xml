<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>hin</rad>/o <fnt><bib>PIV2</bib></fnt>
    </kap>

    <drv mrk="hin.0o">
      <kap><tld/>o</kap>
      <snc mrk="hin.0o.ZOO">
        <uzo tip="fak">ZOO</uzo>
        <dif>
          Ido de <ref cel="cxeval.vir0o">vir&ccirc;evalo</ref> kaj
          <ref cel="azen.0o.ZOO">azen</ref>ino.
        </dif>
      </snc>
      <ref tip="vid" cel="mul.0o">mulo</ref>
      <trd lng="bg">&c_m;&c_u;&c_l;&c_je;</trd>
      <trd lng="de">Maulesel</trd>
      <trd lng="en">hinny</trd>
      <trdgrp lng="fr">
        <trd>bardeau</trd>,
        <trd>bardot</trd>
      </trdgrp>
      <trd lng="pt">burro</trd>
      <trd lng="ru">&c_l;&c_o;&c_sh;&c_a;&c_k;</trd>
    </drv>
  </art>
