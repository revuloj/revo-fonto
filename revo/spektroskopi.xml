<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: spektroskopi.xml,v 1.10 2024/03/25 07:28:35 revo Exp $">
<kap>
  <rad>spektroskopi</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="spektroskopi.0o">
  <kap><tld/>o</kap>
  <snc mrk="spektroskopi.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      <ref tip="vid" cel="spektr.0o.FIZ">Spektr</ref>observado,
      esploro de a&jcirc;oj per la ondoj, kiujn ili reflektas a&ubreve; adsorbas:
      <ekz>
        astronomio <klr>[&#x2026;]</klr> uzas &ccirc;iajn instrumentojn, <tld/>on,
        fotografion k.a.
        <fnt>
          <aut>A. Albaut</aut>,
          <vrk><url ref="http://per.esocom.de/triesencajdemandoj.pdf">
          Tri Esencaj Demandoj</url></vrk>,
          <lok>[vidita en 2012]</lok>
        </fnt>;
      </ekz>
      <ekz>
        li kunlaboris kun aliaj astronomoj, kiuj per <tld/>o sukcesis malkovri &gcirc;is tiam
        nekonatajn ekstersunsistemajn planedojn
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2010/009731.php">Tajg kaj Tomas</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        per <tld/>o, astronomoj e&ccirc; kapablas analizi, kiaj elementoj trovi&gcirc;as sur tiuj
        planedoj, kaj &ccirc;u ili posedas akvon
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2013/011081.php">Astrobiologio kaj
          religio</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="spektrogram.0o">spektrogramo</ref>,
      <ref cel="spektrometri.0o.FIZ">spektrometrio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_e;&c_k;&c_t;&c_r;&c_a;&c_s;&c_k;&c_a;&c_p;&c_ib;&c_ja;</trd>
  <trd lng="de">Spektroskopie</trd>
  <trd lng="en">spectroscopy</trd>
  <trd lng="fr">spectroscopie</trd>
  <trd lng="ja">&#x5206;&#x5149;&#x5B66;  <pr>&#x3076;&#x3093;&#x3053;&#x3046;&#x304C;&#x304F;</pr></trd>
  <trd lng="pl">spektroskopia</trd>
  <trd lng="uk">&c_s;&#x43F;&c_je;&#x43A;&c_t;&c_r;&#x43E;&c_s;&#x43A;&#x43E;&#x43F;&c_ib;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x5149;&#x8B5C;&#x5B78; <pr>gu&#x101;ngp&#x1D4;xu&#xE9;</pr></trd>,
    <trd>&#x5149;&#x8C31;&#x5B66; <pr>gu&#x101;ngp&#x1D4;xu&#xE9;</pr></trd>,
    <trd>&#x8B5C;&#x5B78; <pr>p&#x1D4;xu&#xE9;</pr></trd>,
    <trd>&#x8C31;&#x5B66; <pr>p&#x1D4;xu&#xE9;</pr></trd>,
    <trd>&#x983B;&#x8B5C;&#x5206;&#x6790; <pr>p&#xED;np&#x1D4;f&#x113;nx&#x12B;</pr></trd>,
    <trd>&#x9891;&#x8C31;&#x5206;&#x6790; <pr>p&#xED;np&#x1D4;f&#x113;nx&#x12B;</pr></trd>
  </trdgrp>
</drv>
</art>

