<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kazema.xml,v 1.18 2022/03/21 17:51:57 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>kazemat</rad>/o
</kap>
<drv mrk="kazema.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Arka&jcirc;a subtera <ref tip="super" cel="cxambr.0o.doma">&ccirc;ambro</ref>
      en citadelo a&ubreve; <ref tip="malprt" cel="fortik.0ajxo">fortika&jcirc;o</ref>:
      <ekz>
        la subteraj <klr>[&#x2026;]</klr> <tld/>oj, ebligis rapidan kaj
        &scirc;irmatan trans&scirc;ovon de
        soldatoj, sekura depono de materialo kaj pafadon kontra&ubreve;
        malamiko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=34272">Kazematoj en Vy&scaron;ehrad</url></lok>
        </fnt>;
      </ekz> 
      <ekz>
        dum la 16-a jarcento la fortika&jcirc;o estis adaptita al la evoluo de
        la artilerio
        per teraj remparoj kun <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=130047">Gisors</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_z;&c_e;&c_m;&c_a;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>kasemata</trd>,
    <trd>kasematy</trd>,
    <trd>kazemata</trd>,
    <trd>kazematy</trd>
  </trdgrp>
  <trd lng="de">Kasematte</trd>
  <trd lng="en">casemate</trd>
  <trd lng="fr">casemate</trd>
  <trd lng="hu">kazamata</trd>
  <trd lng="it">casamatta</trd>
  <trd lng="nl">kazemat</trd>
  <trd lng="pl">kazamata  <klr>(architektura)</klr></trd>     
  <trd lng="ro">cazemat&abreve;</trd>
  <trd lng="ru">&c_k;&c_a;&c_z;&c_je;&c_m;&c_a;&c_t;</trd>
  <trd lng="sk">kazematy</trd>
  <trd lng="uk">&#x43A;&#x430;&#x437;&#x435;&#x43C;&#x430;&#x442;</trd>
</drv>
</art>
