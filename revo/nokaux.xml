<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: nokaux.xml,v 1.16 2023/05/14 07:24:22 revo Exp $">
<kap>
  <rad>noka&ubreve;t</rad>/o
</kap>

<drv mrk="nokaux.0o">
  <kap><tld/>o
    <fnt>
      <aut>William Auld</aut>:
      <vrk>Pa&scirc;oj al plena posedo</vrk>.
      <lok><url
 ref="http://www.elerno.cn/elibro/pasoj%20al%20plena%20posedo/dudeksesa.htm"
              >Leciono 26&#170;.</url></lok>
        </fnt>
</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      En <ref tip="malprt" cel="boks.0o">bokso</ref>:
      bato, kiu senkonsciigas la batiton kaj perdigas al li la
      mat&ccirc;on, se li ne povas anta&ubreve; ol pasos 10 sekundoj reekstari kaj
      reekbatali:
      <ekz>
        Se la kalkulo da&ubreve;ras &gcirc;is dek, kaj la konkursanto restas plu
        batalsenpova, la sterno estas konsiderata jam <em><tld/>o</em>, 
        kiu finas la mat&ccirc;on kaj decidas pri &gcirc;ia rezulto
        <fnt>
          <aut>Tibor Ujlaky-Nagy</aut>:
          <vrk>La sporta lingvo en Esperanto</vrk>.
          <lok><url
          ref="http://www.freeweb.hu/eventoj/steb/sportalingvo.rtf"
              >Hungara Esperanto-Asocio, 1972</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_k;&c_a;&c_w;&c_t;</trd>
  <trdgrp lng="en">
    <trd>knock-out</trd>, <trd>K.O.</trd>
  </trdgrp>
  <trd lng="pl">nokaut</trd>
  <trd lng="ro">knockout</trd>
  <trd lng="ru">&c_n;&c_o;&c_k;&c_a;&c_u;&c_t;</trd>
</drv>

<drv mrk="nokaux.0i">
  <kap><tld/>i <fnt><bib>PV</bib></fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      Venki per <tld/>o:
      <ekz>
        oni <tld/>is min dum boksado-konkurso kiam mi estis
        deksesjara
        <fnt>
          <aut>David Curtis</aut>
          <vrk><url ref="&Monato;/2002/pdf/monato200201.pdf#page=28">Koratako</url></vrk>
          <bib>Monato</bib>
          <lok>2002/01, p. 28-34</lok>
        </fnt>.
        </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_k;&c_a;&c_w;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">k.o. <ind>schlagen</ind></trd>
  <trdgrp lng="fr">
    <trd>mettre <ind>k.o.</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ki&uuml;t <klr tip="ind">(bokszban)</klr></trd>,
    <trd>nockoutol</trd>
  </trdgrp>
  <trd lng="nl">knock-out slaan</trd>
  <trd lng="pl">nokautowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>nocautear</trd>,
    <trd>deixar <ind>nocaute</ind></trd>,
    <trd>p&ocirc;r <ind>nocaute</ind></trd>
  </trdgrp>
  <trd lng="ro">f&abreve;cut praf</trd>
  <trd lng="ru">&c_n;&c_o;&c_k;&c_a;&c_u;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
