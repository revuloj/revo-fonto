<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tra&ubreve;mat</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="trauxmat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <uzo tip="fak">PSI</uzo>
  <snc>
    <dif><ref tip="dif" cel="trauxm.0o">tra&ubreve;mo</ref>:</dif>
    <ekz>
    ofte intermiksi&gcirc;as ven&gcirc;emaj sentoj de tiuj, kiuj suferis psikajn 
    <tld/>ojn a&ubreve; e&ccirc; perdis proksimulojn
    <fnt><aut>Werner Schad</aut>, <vrk>Al pli justa mondo</vrk>, 
    <bib>Monato</bib>, <lok>2000/9, p. 7</lok></fnt>;
    </ekz>
    <ekz>
    &scirc;okego en sporto - pro <tld/>o de la kapo, ricevita dum piedpilka
    mat&ccirc;o
    <fnt><aut>Grigorij Arosev</aut>, <vrk>Tragika morto de golulo</vrk>,
     <bib>Monato</bib>, <lok>2001/11, p. 11</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_a;&c_w;&c_m;&c_a;</trd>
  <trd lng="bg">&c_t;&c_r;&c_a;&c_v;&c_m;&c_a;</trd>
  <trd lng="ca">trauma</trd>
  <trd lng="de">Trauma</trd>
  <trd lng="es">trauma</trd>
  <trd lng="fr">traumatisme</trd>
  <trdgrp lng="hu">
    <trd>s&eacute;r&uuml;l&eacute;s</trd>,
    <trd>trauma</trd>,
    <trd>lelki <ind>megr&aacute;zk&oacute;dtat&aacute;s</ind></trd>
  </trdgrp>
  <trd lng="nl">trauma</trd>
  <trd lng="ru">&c_t;&c_r;&c_a;&c_v;&c_m;&c_a;</trd>
</drv>
</art>

