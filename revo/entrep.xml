<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>entrepren</rad>/i
</kap>
<drv mrk="entrep.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Komenci la plenumadon de grava tasko, kiun oni prenis sur sin:
      <ekz>
        <tld/>i la tradukadon de la Biblio<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i ekspedicion, voja&gcirc;on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ni ne <tld/>os konvinki tiun &ccirc;i publikon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>ema homo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>unternehmen</trd>,
    <trd>in <ind>Angriff</ind> nehmen</trd>
  </trdgrp>
  <trd lng="en">undertake</trd>
  <trd lng="fr">entreprendre</trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_p;&c_r;&c_i;&c_n;&c_ja;&c_t;&c_mol;</trd>
</drv>

<drv mrk="entrep.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago de tiu, kiu <tld/>as.
    </dif>
    <trd lng="ru">&c_n;&c_a;&c_ch;&c_i;&c_n;&c_a;&c_n;&c_i;&c_je;</trd>
  </snc>
  <snc>
    <dif>
      <tld lit="E"/>ita&jcirc;o:
      <ekz>
        &ccirc;iuj liaj <tld/>oj prosperas.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_n;&c_a;&c_ch;&c_i;&c_n;&c_a;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_p;&c_r;&c_je;&c_d;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc mrk="entrep.0o.firmao">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Firm(a)o, kiu plenumas difinitajn taskojn.
    </dif>
    <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_i;&c_je;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>podnik</trd>,
    <trd>z&aacute;vod</trd>
  </trdgrp>
  <trd lng="de">Unternehmen</trd>
  <trd lng="fr">entreprise</trd>
</drv>

<drv mrk="entrep.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Homo, kiu profesie <tld/>as laborojn por aliaj personoj:
      <ekz>
        <tld/>isto por masonado, domkonstruado, transportado ktp.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Unternehmer</trd>
  <trd lng="fr">entrepreneur</trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_p;&c_r;&c_i;&c_n;&c_i;&c_m;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
</drv>
</art>
