<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: admini1.xml,v 1.32 2023/10/27 13:42:09 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>administraci</rad>/o
</kap>

<drv mrk="admini1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POL</uzo>
  <snc mrk="admini1.0o.ularo">
    <dif>
      <ref tip="dif" cel="admini.0antaro">Administrantaro</ref>:
      <ekz>
        post siaj studoj li laboris alterne en la &scirc;tata <tld/>o kaj kiel
        universitatano
        <fnt><aut>Bo Sandelin</aut>: <vrk>Makroekonomiko en la
          20a jarcento</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 9a, p.
          14a</lok>
        </fnt>;
      </ekz> 
      <ekz>
        naciaj po&scirc;t<tld/>oj
        <fnt>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2000/007065.php">VIPE 2000, a&ubreve; &ccirc;u la po&scirc;tmarkoj
          malaperos?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/09, p. 18</lok>
          </fnt>;
      </ekz>
      <ekz>
        flughaven<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Rob Ebenau</aut>
          <vrk><url ref="&Monato;/2005/008528.php">Kiel protekti kontra&ubreve; perdo kaj
          &scirc;telo?</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni devas pagi jaran imposton al la urba <tld/>o
        <fnt>
          <aut>Remigiusz Mielcarek</aut>
          
          <vrk><url ref="&Monato;/1994/A940101.php">Enmigrintoj en ora ka&gcirc;o</url></vrk>
          <bib>Monato</bib>
          <lok>1994/01, p. 15</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>igazgat&oacute;s&aacute;g</trd>,
      <trd>vezet&odblac;s&eacute;g</trd>,
      <trd>adminsztr&aacute;ci&oacute;</trd>
    </trdgrp>
    <trd lng="it">amministrazione<klr> (dirigenza)</klr></trd>
    <trd lng="sk">administrat&iacute;va <klr>(&uacute;radn&iacute;ci)</klr></trd>
  </snc>  
  <snc mrk="admini1.0o.administrado">
    <dif>
      Administrado:
      <ekz>
        kiam <nom>Nobel</nom> mortis en 1896, Norvegio estis sub sveda <tld/>o, sed kun propra
        parlamento
        <fnt>
          <bib>Monato</bib>
          <aut>Claude Rouget</aut>
          <vrk><url ref="&Monato;/2008/009374.php">&Ccirc;u Al Gore meritas pacpremion?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="admini1.0o.ejo">
    <dif>
      <ref tip="dif" cel="admini.0ejo">Administrejo</ref>:
      <ekz>
        ofte la ministro kaj la kabineto trovi&gcirc;as for de la ministreja
        <tld/>o<fnt><aut>Roland Rotsaert</aut>: <vrk>Dioksino-krizo</vrk>,
        <bib>Monato</bib><lok>jaro 1999a, numero 7a, p. 12a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="hu">igazgat&oacute;s&aacute;gi <ind>&eacute;p&uuml;let</ind></trd>
    <trd lng="it">amministrazione<klr> (sede)</klr></trd>
    <trd lng="sk">administr&aacute;cia <klr>(&uacute;rad)</klr></trd>
    <trdgrp lng="zh">
    <trd>&#x7BA1;&#x5BB6;&#x804C;&#x52A1; <pr>gu&#x1CE;nji&#x101;zh&#xED;w&#xF9;</pr></trd>,
    <trd>&#x7BA1;&#x5BB6;&#x8077;&#x52D9; <pr>gu&#x1CE;nji&#x101;zh&#xED;w&#xF9;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_d;&c_m;&c_ib;&c_n;&c_ib;&c_s;&c_t;&c_r;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_a;&c_d;&c_m;&c_i;&c_n;&c_i;&c_s;&c_t;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trd lng="bo">&#xF42;&#xF5E;&#xF74;&#xF44;&#xF0B;</trd>
  <trd lng="br">melestradurezh</trd>
  <trdgrp lng="cs">
    <trd>administrace</trd>,
    <trd>spr&aacute;va <klr>(budova, &uacute;&rcaron;ednictvo)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Verwaltung</trd>,
  </trdgrp>
  <trd lng="en">administration</trd>
  <trd lng="es">administraci&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>administration</trd>
  </trdgrp>
  <trd lng="he">&#x5DE;&#x5B4;&#x5E0;&#x5D4;&#x5B8;&#x5DC;</trd>
  <trdgrp lng="ja">
    <trd>&#x7BA1;&#x7406;&#x90E8;  <pr>&#x304B;&#x3093;&#x308A;&#x3076;</pr></trd>,
    <trd/>,
    <trd>&#x884C;&#x653F;  <pr>&#x304E;&#x3087;&#x3046;&#x305B;&#x3044;</pr></trd>,
    <trd>&#x884C;&#x653F;&#x5E9C;  <pr>&#x304E;&#x3087;&#x3046;&#x305B;&#x3044;&#x3075;</pr></trd>
  </trdgrp>
  <trd lng="nl">administratie</trd>
  <trd lng="pl">administracja</trd>
  <trd lng="pt">administra&ccedil;&atilde;o</trd>
  <trd lng="ro">administra&#x21B;ie</trd>
  <trd lng="ru">&c_a;&c_d;&c_m;&c_i;&c_n;&c_i;&c_s;&c_t;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>administrat&iacute;va; administr&aacute;cia <klr>(&uacute;rad)</klr></trd>,
    <trd>spr&aacute;va</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&c_d;&#x43C;&c_ib;&#x43D;&c_ib;&c_s;&c_t;&c_r;&c_a;&c_c;&c_ib;&#x44F;</trd>,
    <trd>&#x43F;&c_r;&c_a;&c_v;&#x43B;&c_ib;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_u;&#x43F;&c_r;&c_a;&c_v;&#x43B;&c_ib;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_u;&#x43F;&c_r;&c_a;&c_v;&c_a;</trd>
  </trdgrp>
</drv>

