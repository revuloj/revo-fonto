<drv mrk="postul.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Severe peti ion pro sia rajto, a&ubreve;toritato a&ubreve; forto:
      <ekz>
        <tld/>i sian parton de hereda&jcirc;o, la prezon de sia laboro,
        beda&ubreve;resprimojn;
      </ekz>
      <ekz>&scirc;iru vin en du partojn, la mondo trian <tld/>os
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>kion Dio ne donis, perforte ne <tld/>u
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        donu al la lupo iom, li <tld/>os &ccirc;iom;
      </ekz>
      <ekz>
        re<tld/>i rajton;
      </ekz>
      <ekz>
        la pago estas tuj <tld/>ebla.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="preten.0i">pretendi</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_t;&c_r;&c_a;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_n;&c_a;&c_s;&c_t;&c_o;&c_j;&c_l;&c_ib;&c_v;&c_a;
      &c_p;&c_r;&c_a;&c_s;&c_ib;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="ca">demanar</trd>
    <trdgrp lng="de">
      <trd>fordern</trd>,
      <trd>verlangen</trd>
    </trdgrp>
    <trd lng="es">demandar</trd>
    <trd lng="hu">k&ouml;vetel</trd>
    <trdgrp lng="pl">
      <trd>&zdot;&aogonek;da&cacute;</trd>,
      <trd>domaga&cacute; si&eogonek;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Igi nepre necesa:
      <ekz>
        la rapida industrii&gcirc;o de la lando nepre <tld/>as novajn
        vendejojn;
      </ekz>
      <ekz>
        tiu demando <tld/>as pripenson;
      </ekz>
      <ekz>
        tiu verko <tld/>is de ni trijaran laboron;
      </ekz>
      <ekz>
        &ccirc;iu prep. per si mem <tld/>as la nominativon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi profunde kredas, ke tion <tld/>as la interesoj de l'
        afero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tasko, kiu <tld/>as grandan sperton<fnt>B</fnt>;
      </ekz>
      <ekz>
        por ellernado de esp. estas <tld/>ataj de la lernanto neniaj
        anta&ubreve;aj scioj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &ccirc;i tiu tumulto, tertremo <tld/>is multajn viktimojn.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_t;&c_r;&c_a;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="ca">
      <trd>requerir</trd>,
      <trd>exigir</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>fordern</trd>,
      <trd>erfordern</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>requerir</trd>,
      <trd>exigir</trd>
    </trdgrp>
    <trd lng="hu">megk&ouml;vetel</trd>
    <trdgrp lng="pl">
      <trd>postulowa&cacute;</trd>,
      <trd>dopomina&cacute; si&eogonek;</trd>
    </trdgrp>
  </snc>
  <trd lng="af">vereis</trd>
  <trd lng="am">&#x12ED;&#x1320;&#x12ED;&#x1241;</trd>
  <trd lng="ar">&#x62A;&#x62A;&#x637;&#x644;&#x628;</trd>
  <trd lng="az">t&#x259;l&#x259;b</trd>
  <trd lng="bg">&c_i;&c_z;&c_i;&c_s;&c_k;&c_v;&c_a;&c_m;</trd>
  <trd lng="bn">&#x9AA;&#x9CD;&#x9B0;&#x9AF;&#x9BC;&#x9CB;&#x99C;&#x9A8;</trd>
  <trd lng="bs">zahtijevaju</trd>
  <trd lng="co">vurr&#xE0;</trd>
  <trdgrp lng="cs">
    <trd>po&zcaron;adovat</trd>,
    <trd>vy&zcaron;adovat</trd>
  </trdgrp>
  <trd lng="da">kr&#xE6;ver</trd>
  <trd lng="en">demand</trd>
  <trd lng="et">vaja</trd>
  <trd lng="eu">eskatzen</trd>
  <trdgrp lng="fr">
    <trd>exiger</trd>,
    <trd>r&eacute;clamer</trd>
  </trdgrp>
  <trd lng="fy">fereaskje</trd>
  <trd lng="gl">requiren</trd>
  <trd lng="gu">&#xA9C;&#xAB0;&#xAC2;&#xAB0;&#xAC0;</trd>
  <trd lng="ha">bukata</trd>
  <trd lng="hi">&#x906;&#x935;&#x936;&#x94D;&#x92F;&#x915;&#x924;&#x93E;</trd>
  <trd lng="hr">zahtijevati</trd>
  <trd lng="hy">&#x57A;&#x561;&#x570;&#x561;&#x576;&#x57B;&#x578;&#x582;&#x574;</trd>
  <trd lng="is">krefjast</trd>
  <trd lng="ja">&#x5FC5;&#x8981;&#x3068;&#x3059;&#x308B;</trd>
  <trd lng="jw">mbutuhake</trd>
  <trd lng="ka">&#x10DB;&#x10DD;&#x10D8;&#x10D7;&#x10EE;&#x10DD;&#x10D5;&#x10E1;</trd>
  <trd lng="km">&#x178F;&#x1798;&#x17D2;&#x179A;&#x17BC;&#x179C;&#x17B1;&#x17D2;&#x1799;&#x1798;&#x17B6;&#x1793;</trd>
  <trd lng="ko">&#xD544;&#xC694;</trd>
  <trd lng="lo">&#xEAE;&#xEBD;&#xE81;&#xEAE;&#xEC9;&#xEAD;&#xE87;&#xEC3;&#xEAB;&#xEC9;&#xEA1;&#xEB5;</trd>
  <trd lng="ml">&#xD06;&#xD35;&#xD36;&#xD4D;&#xD2F;&#xD2E;&#xD3E;&#xD2F;</trd>
  <trd lng="mr">&#x906;&#x935;&#x936;&#x94D;&#x92F;&#x915;</trd>
  <trd lng="my">&#x1010;&#x1031;&#x102C;&#x1004;&#x103A;&#x1038;&#x1006;&#x102D;&#x102F;</trd>
  <trd lng="ne">&#x906;&#x935;&#x936;&#x94D;&#x92F;&#x915;&#x924;&#x93E;</trd>
  <trd lng="nl">eisen</trd>
  <trd lng="ny">amafunika</trd>
  <trd lng="pa">&#xA26;&#xA40; &#xA32;&#xA4B;&#xA5C; &#xA39;&#xA48;</trd>
  <trd lng="ps">&#x62A;&#x647; &#x627;&#x693;&#x62A;&#x6CC;&#x627; &#x644;&#x631;&#x64A;</trd>
  <trdgrp lng="pt">
    <trd>exigir</trd>,
    <trd>postular</trd>,
    <trd>reclamar</trd>,
    <trd>demandar</trd>,
    <trd>requerer</trd>,
    <trd>pedir</trd>        
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_b;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sd">&#x62C;&#x64A; &#x636;&#x631;&#x648;&#x631;&#x62A; &#x622;&#x647;&#x64A;</trd>
  <trd lng="si">&#xD85;&#xDC0;&#xDC1;&#xDCA;&#xDBA;</trd>
  <trdgrp lng="sk">
    <trd>do&zcaron;adova&tcaron; sa</trd>,
    <trd>po&zcaron;adova&tcaron;</trd>,
    <trd>vy&zcaron;adova&tcaron;</trd>
  </trdgrp>
  <trd lng="sq">k&#xEB;rkojn&#xEB;</trd>
  <trd lng="st">hloka</trd>
  <trd lng="sv">kr&auml;va</trd>
  <trd lng="ta">&#xBA4;&#xBC7;&#xBB5;&#xBC8;&#xBAA;&#xBCD;&#xBAA;&#xB9F;&#xBC1;&#xB95;&#xBBF;&#xBB1;&#xBA4;&#xBC1;</trd>
  <trd lng="te">&#xC05;&#xC35;&#xC38;&#xC30;&#xC02;</trd>
  <trdgrp lng="th">
    <trd>&#xE15;&#xE49;&#xE2D;&#xE07;&#xE01;&#xE32;&#xE23;</trd>,
    <trd>&#xE15;&#xE49;&#xE2D;&#xE07;&#xE01;&#xE32;&#xE23;&#xE43;&#xE2B;&#xE49;&#xE21;&#xE35;</trd>,
    <trd>&#xE40;&#xE23;&#xE35;&#xE22;&#xE01;&#xE23;&#xE49;&#xE2D;&#xE07;</trd>
  </trdgrp>
  <trd lng="tl">nangangailangan ng</trd>
  <trd lng="ur">&#x6A9;&#x6CC; &#x636;&#x631;&#x648;&#x631;&#x62A; &#x6C1;&#x648;&#x62A;&#x6CC; &#x6C1;&#x6D2;</trd>
  <trd lng="vi">y&#xEA;u c&#x1EA7;u</trd>
