<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&ccirc;ambr</rad>/o
</kap>
<drv mrk="cxambr.0o">
  <kap><ofc>*</ofc> <tld/>o</kap>
  <snc mrk="cxambr.0o.doma">
    <dif>
      &Ccirc;iu el la apartaj ejoj de <ref tip="malprt" cel="dom.0o">domo</ref>,
      disigitaj per muroj a&ubreve; vandoj, kaj komuniki&gcirc;antaj per pordoj.
    </dif>
    <subsnc mrk="cxambr.0o.logxeja">
      <dif>
        <tld lit="&Ccirc;"/>o en lo&gcirc;ejo.
      </dif>
      <refgrp tip="sub">
        <ref cel="cxel.0o.cxambreto">&ccirc;elo<sncref/></ref>,
        <ref cel="kabine.0o.pensejo">kabineto<sncref/></ref>,
        <ref cel="kamer.0o.cxambro">kamero<sncref/></ref>
      </refgrp>
      <trd lng="ar">&Ha;&damma;&jim;&sukun;&ra;&fatha;&ta_marbuta;&dammatan; &jim; &Ha;&damma;&jim;&fatha;&ra;&dammatan;</trd>
      <trd lng="be">&c_p;&c_a;&c_k;&c_o;&c_j;</trd>
      <trdgrp lng="de">
        <trd>Raum</trd>,
        <trd>Zimmer</trd>,
        <trd>Kammer</trd>
      </trdgrp>
      <trdgrp lng="es">
        <trd>habitaci&oacute;n</trd>,
        <trd>cuarto</trd>
      </trdgrp>
      <trd lng="fr">pi&egrave;ce <klr tip="ind">(d'habitation)</klr></trd>
      <trd lng="hu">szoba</trd>
      <trd lng="pt">c&ocirc;modo<klr tip="ind"> (subst.)</klr></trd>
      <trd lng="sk">izba</trd>
      <trd lng="sv">rum</trd>
    </subsnc>
    <subsnc mrk="cxambr.0o.ofica">
      <dif>
        <tld lit="&Ccirc;"/>o ofica.
      </dif>
      <refgrp tip="vid">
        <ref cel="auxl.0o">a&ubreve;lo</ref>,
        <ref cel="salon.0o">salono</ref>,
        <ref cel="solen">solenejo</ref>
      </refgrp>
      <trd lng="be">&c_z;&c_a;&c_l;&c_ja; &c_p;&c_a;&c_s;&c_je;&c_d;&c_zh;&c_a;&c_n;&c_mol;&c_n;&c_ja;&c_w;</trd>
      <trd lng="de">Kammer</trd>
      <trd lng="es">c&aacute;mara</trd>
      <trd lng="fr">salle</trd>
      <trd lng="hu">helyis&eacute;g</trd>
      <trd lng="pt">c&acirc;mara</trd>
      <trd lng="sk">miestnos&tcaron;</trd>
    </subsnc>
    <trd lng="en">room</trd>
    <trd lng="ru">&c_k;&c_o;&c_m;&c_n;&c_a;&c_t;&c_a;</trd>
  </snc>
  <snc mrk="cxambr.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      &Scirc;tata a&ubreve; publika institucio, precipe asembleo, kaj ties
      oficejo a&ubreve; kunvenejo:
      <ekz>
        <ind><tld/>o de deputitoj</ind>;
        <trd lng="hu">k&eacute;pvisel&odblac;h&aacute;z</trd>
        <trd lng="fr">Chambre des Repr&eacute;sentants<klr> (Belgique)</klr></trd>
        <trdgrp lng="nl">
          <trd>Kamer van volksvertegenwoordigers<klr> (Belgi&euml;)</klr></trd>,
          <trd>Tweede <ind>Kamer</ind><klr> (Nederland)</klr></trd>
        </trdgrp>
        <trd lng="ru">&c_p;&c_a;&c_l;&c_a;&c_t;&c_a; &c_d;&c_je;&c_p;&c_u;&c_t;&c_a;&c_t;&c_o;&c_v;</trd>
      </ekz>
      <ekz>
        <ind>komerca <tld/>o</ind>.
        <trd lng="hu">kereskedelmi <ind>kamara</ind></trd>
        <trd lng="nl">kamer van koophandel</trd>
        <trd lng="ru">&c_t;&c_o;&c_r;&c_g;&c_o;&c_v;&c_a;&c_ja; &c_p;&c_a;&c_l;&c_a;&c_t;&c_a;</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_l;&c_a;&c_t;&c_a; <klr tip="ind">(&c_u;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_a;)</klr></trd>
    <trd lng="de">Kammer</trd>
    <trd lng="en">chamber</trd>
    <trd lng="es">c&aacute;mara</trd>
    <trd lng="fr">chambre <klr tip="ind">(institution)</klr></trd>
    <trdgrp lng="hu">
      <trd>h&aacute;z<klr> (pl. fels&odblac;h&aacute;z)</klr></trd>,
      <trd>kamara</trd>
    </trdgrp>
    <trd lng="pt">c&acirc;mara</trd>
    <trd lng="ru">&c_p;&c_a;&c_l;&c_a;&c_t;&c_a;</trd>
    <trd lng="sk">komora</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>pokoj</trd>,
    <trd>m&iacute;stnost</trd>
  </trdgrp>
  <trd lng="nl">kamer</trd>
  <trd lng="pl">pok&oacute;j</trd>
  <trd lng="tr">oda</trd>
</drv>

<drv mrk="cxambr.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      <ref tip="super" cel="serv.0isto">Servisto</ref>, kiu servas
      en lo&gcirc;o<tld/>oj:
      <ekz>
        li venis en tiu tempo, kiam la generalo nur elliti&gcirc;is kaj
        kiam lia <tld/>isto eble portis al li ar&gcirc;entan ujon por
        lavado
        <fnt><aut>N. Gogol, tr. V. Vy&ccirc;eg&jcirc;anin</aut>:
        <vrk>Malvivaj animoj</vrk>,
        <lok>Novelo pri kapitano Kopekin</lok>.
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_k;&c_a;&c_jo;&c_v;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_k;&c_a;&c_jo;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="es">camarista</trd>
  <trd lng="fr">cam&eacute;riste</trd>
  <trdgrp lng="hu">
    <trd>szobainas</trd>,
    <trd>komornyik</trd>
  </trdgrp>
  <trd lng="nl">kamenier</trd>
  <trd lng="pt">camareiro</trd>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_r;&c_d;&c_i;&c_n;&c_je;&c_r;</trd>
</drv>

<drv mrk="cxambr.0istino">
  <kap><tld/>istino</kap>
  <snc>
    <dif>
      <ref tip="super" cel="serv.0istino">Servistino</ref>, kies
      laboro estas purigi kai ordigi lo&gcirc;o<tld/>ojn:
      <ekz>
        ni havas diversajn servantojn: kuiriston, <tld/>istinon,
        infanistinon kaj veturigiston
        <fnt><bib>F</bib>, <lok>&FE;37.</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi vidis la <tld/>istinon aux la kuiristinon?
        <fnt><bib>LR</bib>, <lok>Respondo 29, La Revuo, 1908, Februaro</lok></fnt>;
      </ekz>
      <ekz>
        de dek du jaroj &scirc;i kudras por la bona sinjorino elegantajn
        vestojn dum tutaj noktoj, akre admonas &scirc;iajn <tld/>istinojn kaj
        lakeojn, &ccirc;iumatene metas sur &scirc;iajn piedojn &scirc;trumpojn kaj &scirc;uojn,
        &ccirc;iuvespere faras el &scirc;ia litkovrilo belajn drapira&jcirc;ojn...
        <fnt><aut>E. Orzeszko, trad. Kabe</aut>: <vrk>La bona sinjorino.</vrk></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Zimmerm&auml;dchen</trd>
  <trd lng="hu">szobal&aacute;ny</trd>  
  <trd lng="nl">kamermeisje</trd>
  <trd lng="pt">camareira</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_n;&c_i;&c_ch;&c_n;&c_a;&c_ja;</trd>
</drv>

<drv mrk="cxambr.antaux0o">
  <kap>anta&ubreve;<tld/>o</kap>
  <snc mrk="cxambr.antaux0o.porMantelo">
    <dif>
      Parto de lo&gcirc;ejo, &ccirc;e la enirejo,
      kie oni demetas la mantelojn, &ccirc;apelojn, pluvombrelojn, ktp.
    </dif>
    <refgrp tip="vid">
      <ref cel="korido.0o">koridoro</ref>,
      <ref cel="vestib.0o">vestiblo</ref>.
    </refgrp>
    <trd lng="be">&c_p;&c_ja;&c_r;&c_e;&c_d;&c_n;&c_ib; &c_p;&c_a;&c_k;&c_o;&c_j;</trd>
    <trd lng="de">Diele</trd>
    <trd lng="fr">entr&eacute;e<klr tip="ind"> (vestibule)</klr></trd>
    <trd lng="hu">el&odblac;szoba</trd>    
    <trd lng="nl">vestibule</trd>
    <trd lng="pt">vest&iacute;bulo</trd>
    <trd lng="ru">&c_p;&c_r;&c_i;&c_h;&c_o;&c_zh;&c_a;&c_ja;</trd>
  </snc>
  <snc mrk="cxambr.antaux0o.porAkcepto">
    <dif>
      Salono, en kiu la vizitantoj de oficialulo a&ubreve;
      eminentulo atendas la akcepton.
    </dif>
    <trd lng="be">&c_p;&c_r;&c_y;&c_jo;&c_m;&c_n;&c_y; &c_p;&c_a;&c_k;&c_o;&c_j;</trd>
    <trd lng="de">Vorzimmer</trd>
    <trd lng="fr">antichambre</trd>
    <trdgrp lng="hu">
      <trd>fogad&oacute;szoba</trd>,
      <trd>fogad&oacute;helyis&eacute;g</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>voorvertrek</trd>,
      <trd>voorkamer</trd>,
      <trd>wachtkamer</trd>
    </trdgrp>
    <trd lng="pt">antec&acirc;mera</trd>
  </snc>
  <trd lng="en">antechamber</trd>
  <trd lng="hu">el&odblac;szoba</trd>
  <trd lng="sk">predsie&ncaron;</trd>
</drv>

<drv mrk="cxambr.atendo0o">
  <kap>atendo<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="&Ccirc;"/>o, &gcirc;enerale kun se&gcirc;oj, por atendado:
      <ekz>
        mi atendis la kuraciston en atendo<tld/>o dum duonhoro.
      </ekz>
    </dif>
  </snc>
  <trd lng="hu">v&aacute;r&oacute;szoba</trd>  
  <trd lng="pt">sala de espera</trd>
</drv>

<drv mrk="cxambr.ban0o">
  <kap>ban<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="&Ccirc;"/>o por bano, du&scirc;o, korpa purigado kaj flegado.
    </dif>
  </snc>
  <trd lng="de">Badezimmer</trd>
  <trd lng="fr">salle de bain</trd>
  <trd lng="hu">f&uuml;rd&odblac;szoba</trd>
  <trd lng="nl">badkamer</trd>
  <trd lng="pt">banheiro</trd>
  <trd lng="ru">&c_v;&c_a;&c_n;&c_n;&c_a;&c_ja;</trd>
</drv>

<drv mrk="cxambr.dormo0o">
  <kap>dormo<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="&Ccirc;"/>o ordinare kvieta kaj ta&ubreve;ge ekipita por dormado.
      <ekz>
        je la deka vespere &ccirc;iuj estis en la dormo<tld/>oj, je la deka kaj
	duono la domo dormadis
        <fnt><aut>Machado de Assis, trad. Paulo Viana</aut>
        <vrk>La divenistino kaj aliaj rakontoj</vrk><lok>Oportuno, jaro 2005a, p.
	27a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Schlafzimmer</trd>
  <trd lng="fr">chambre &agrave; coucher</trd>
  <trd lng="hu">h&aacute;l&oacute;szoba</trd>
  <trd lng="nl">slaapkamer</trd>
  <trd lng="pt">quarto<klr tip="ind"> (de dormir)</klr></trd>
  <trd lng="ru">&c_s;&c_p;&c_a;&c_l;&c_mol;&c_n;&c_ja;</trd>
</drv>

<drv mrk="cxambr.klas0o">
  <kap>klas<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="&Ccirc;"/>o en lernejo, kie klaso lernas:
      <ekz>
        la klas<tld/>oj havis multajn fenestrojn por havigi sufi&ccirc;e da lumo
	<fnt><aut>Den Drown</aut><vrk>Elektoj</vrk><bib>Monato</bib>
	<lok>jaro 2003a, numero 10a, p. 23a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="hu">oszt&aacute;lyterem</trd>  
  <trd lng="pt">sala de aula</trd>
</drv>

<drv mrk="cxambr.mangxo0o">
  <kap>man&gcirc;o<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="&Ccirc;"/>o por man&gcirc;ado:
      <ekz>
        koridoro, kiu kondukis de la vizit<tld/>o al la man&gcirc;o<tld/>o
        <fnt><aut>Machado de Assis, trad. Paulo Viana</aut>
        <vrk>La divenistino kaj aliaj rakontoj</vrk><lok>Oportuno, jaro 2005a, p.
	29a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="hu">eb&eacute;dl&odblac;</trd>  
  <trdgrp lng="pt">
    <trd>refeit&oacute;rio</trd>,
    <trd>sala de jantar</trd>
  </trdgrp>
</drv>

<drv mrk="cxambr.masxin0o">
  <kap>ma&scirc;in<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="&Ccirc;"/>o en &scirc;ipo kie estas la ma&scirc;inoj:
      <ekz>
        oni enhejmi&gcirc;as, por tiel diri, en la ma&scirc;in<tld/>o
	<fnt><aut>Harry Harrison, trad. Reinhard F&ouml;ssmeier k.a.</aut>
	<vrk>Naski&gcirc;o de la rustimuna &scirc;talrato</vrk><lok>Sezonoj,
	jaro 1996a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="hu">g&eacute;pterem</trd>  
  <trd lng="pt">casa de m&aacute;quina</trd>
</drv>

<drv mrk="cxambr.skribo0o">
  <kap>skribo<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="&Ccirc;"/>o por skriba, pensa laborado:
    </dif>
    <refgrp tip="sin">
      <ref cel="kabine.0o.pensejo">kabineto<sncref/></ref>,
      <ref cel="skrib.0ejo">skribejo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Schreibzimmer</trd>,
    <trd>Schreibstube</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bureau<klr> (pi&egrave;ce pour travailler)</klr></trd>,
    <trd>cabinet d'&eacute;tude</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>iroda</trd>,
    <trd>&iacute;r&oacute;szoba</trd>
  </trdgrp>
  <trd lng="nl">kabinet</trd>
  <trd lng="ru">&c_r;&c_a;&c_b;&c_o;&c_ch;&c_i;&c_j; &c_k;&c_a;&c_b;&c_i;&c_n;&c_je;&c_t;</trd>
</drv>

<drv mrk="cxambr.vivo0o">
  <kap>vivo<tld/>o</kap>
  <fnt><bib>EBV</bib></fnt>
  <snc>
    <dif>
      Familia <tld/>o, &ccirc;efa parto de la lo&gcirc;ejo,
      kie la familio kunvenas.
    </dif>
  </snc>
  <trd lng="de">Wohnzimmer</trd>
  <trd lng="es">sala</trd>
  <trdgrp lng="fr">
    <trd>salle de s&eacute;jour</trd>,
    <trd>vivoir</trd>
  </trdgrp>
  <trd lng="hu">nappali (szoba)</trd>
  <trd lng="nl">woonkamer</trd>
  <trd lng="pt">sala de estar</trd>
  <trd lng="ru">&c_g;&c_o;&c_s;&c_t;&c_i;&c_n;&c_a;&c_ja;</trd>
  <trd lng="sk">ob&yacute;va&ccaron;ka</trd>
</drv>

<drv mrk="cxambr.vizit0o">
  <kap>vizit<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="&Ccirc;"/>o, kie oni ricevas vizitantojn:
      <ekz>
        koridoro, kiu kondukis de la vizit<tld/>o al la man&gcirc;o<tld/>o
        <fnt><aut>Machado de Assis, trad. Paulo Viana</aut>
        <vrk>La divenistino kaj aliaj rakontoj</vrk><lok>Oportuno, jaro 2005a, p.
	29a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="hu">szalon</trd>  
  <trd lng="pt">sala de visita</trd>
</drv>
</art>
