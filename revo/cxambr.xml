<drv mrk="cxambr.mangxo0o">
  <kap><ofc>*</ofc>man&gcirc;o<tld/>o</kap>
  <snc mrk="cxambr.mangxo0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <tld lit="&Ccirc;"/>o por man&gcirc;ado:
      <ekz>
        li eliris el la dormo<tld/>o kaj eniris en la man&gcirc;o<tld/>on
        <fnt><bib>F</bib>, <lok>&FE; 31</lok></fnt>;
      </ekz>
      <ekz>
        <nom>Samuel</nom> <klr>[&#8230;]</klr> enkondukis ilin en la man&gcirc;o<tld/>on, kaj donis al ili
        lokon supraloke
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 9:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Tafet</nom> en la man&gcirc;o<tld/>o kovris la tablon per blanka tuko, starigis la
        kandelingon <klr>[&#8230;]</klr> al&scirc;ovis tri se&gcirc;ojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni estis finantaj la tagman&gcirc;on en la malgranda salono, kiu estis samtempe
        man&gcirc;o<tld/>o
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        koridoro <klr>[&#8230;]</klr> kondukis de la vizit<tld/>o al la
        man&gcirc;o<tld/>o
        <fnt>
          <aut>M. de Assis, trad. P. Viana</aut>,
          <vrk>La divenistino kaj aliaj rakontoj</vrk>,
          <lok>Oportuno, 2005, p. 29a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bufed.0ejo">bufedejo</ref>,
      <ref cel="kantin.0o">kantino</ref>,
      <ref cel="mangx.0ejo">man&gcirc;ejo</ref>
    </refgrp>
  </snc>
  <trd lng="cs">j&iacute;delna</trd>
  <trd lng="de">Esszimmer</trd>
  <trd lng="es">comedor</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_t;&a_A;&a_q;
    &a_gh;&a_dh;&a_A;&a_kh;&a_w;&a_r;&f_y;</trd>,
    <trd>&a_n;&a_A;&a_h;&a_A;&a_r;&a_kh;&a_w;&a_r;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">salle &agrave; manger</trd>
  <trd lng="hu">eb&eacute;dl&odblac;</trd>
  <trd lng="id"><ind>ruang</ind> makan</trd>
  <trd lng="it">sala<klr> da pranzo</klr></trd>
