          <aut>D. Drown</aut>
          <vrk>Elektoj</vrk>
          <bib>Monato</bib>
          <lok>2003:10, p. 23a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_l;&c_ja;&c_s; <klr>(&c_p;&c_a;&c_m;&c_ja;&c_sh;&c_k;&c_a;&c_n;&c_mol;&c_n;&c_je;)</klr></trd>,
