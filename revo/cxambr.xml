    <trd>Empfangszimmer</trd>
  </trdgrp>
  <trd lng="fa">&a_A;&a_t;&a_A;&a_q;
  &f_p;&a_dh;&f_y;&a_r;&a_A;&f_y;&f_y;</trd>
