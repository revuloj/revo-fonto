<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&ccirc;ambr</rad>/o
</kap>

<drv mrk="cxambr.0o">
  <kap><ofc>*</ofc> <tld/>o</kap>
  <snc mrk="cxambr.0o.doma">
    <dif>
      &Ccirc;iu el la apartaj ejoj de <ref tip="malprt" cel="dom.0o">
      domo</ref>, disigitaj per muroj a&ubreve; vandoj, kaj
      komuniki&gcirc;antaj per pordoj.
    </dif>
    <subsnc mrk="cxambr.0o.logxeja">
      <dif>
        <tld lit="&Ccirc;"/>o en lo&gcirc;ejo.
      </dif>
      <refgrp tip="sub">
        <ref cel="cxel.0o.cxambreto">&ccirc;elo<sncref/></ref>,
        <ref cel="kabine.0o.pensejo">kabineto<sncref/></ref>,
        <ref cel="kamer.0o.cxambro">kamero<sncref/></ref>
      </refgrp>
      <trd lng="ar">
&a_H;&a_damma;&a_j;&a_sukun;&a_r;&a_fatha;&a_t_marbuta;&a_dammatan;
      &a_j; &a_H;&a_damma;&a_j;&a_fatha;&a_r;&a_dammatan;</trd>
      <trd lng="be">&c_p;&c_a;&c_k;&c_o;&c_j;</trd>
      <trdgrp lng="de">
        <trd>Raum</trd>,
        <trd>Zimmer</trd>,
        <trd>Kammer</trd>
      </trdgrp>
      <trdgrp lng="es">
        <trd>habitaci&oacute;n</trd>,
        <trd>cuarto</trd>
      </trdgrp>
      <trd lng="fr">pi&egrave;ce <klr tip="ind">(d'habitation)</klr>
      </trd>
      <trd lng="hu">szoba</trd>
      <trdgrp lng="pl">
        <trd>pok&oacute;j</trd>,
        <trd>izba</trd>,
        <trd>pomieszczenie</trd>,
        <trd>sala</trd>
      </trdgrp>
      <trd lng="pt">c&ocirc;modo<klr tip="ind"> (subst.)</klr></trd>
      <trd lng="sk">izba</trd>
      <trd lng="sv">rum</trd>
    </subsnc>
    <subsnc mrk="cxambr.0o.ofica">
      <dif>
        <tld lit="&Ccirc;"/>o ofica.
      </dif>
      <refgrp tip="vid">
        <ref cel="auxl.0o">a&ubreve;lo</ref>,
        <ref cel="salon.0o">salono</ref>,
        <ref cel="solen">solenejo</ref>
      </refgrp>
      <trd lng="be">
&c_z;&c_a;&c_l;&c_ja;&c_p;&c_a;&c_s;&c_je;&c_d;&c_zh;&c_a;&c_n;&c_mol;&c_n;&c_ja;&c_w;
</trd>
      <trd lng="de">Kammer</trd>
      <trd lng="es">c&aacute;mara</trd>
      <trd lng="fr">salle</trd>
      <trd lng="hu">helyis&eacute;g</trd>
      <trdgrp lng="pl">
        <trd>pok&oacute;j</trd>,
        <trd>pomieszczenie</trd>
      </trdgrp>
      <trd lng="pt">c&acirc;mara</trd>
      <trd lng="sk">miestnos&tcaron;</trd>
    </subsnc>
    <trd lng="en">room</trd> 
    <trdgrp lng="it">
      <trd>stanza</trd>, 
      <trd>camera</trd>,
      <trd>sala</trd>
    </trdgrp>   
    <trd lng="ru">&c_k;&c_o;&c_m;&c_n;&c_a;&c_t;&c_a;</trd>
    <trd lng="tp">tomo</trd>
  </snc>
  <snc mrk="cxambr.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      &Scirc;tata a&ubreve; publika institucio, precipe asembleo,
      kaj ties oficejo a&ubreve; kunvenejo:
      <ekz>
        <ind><tld/>o de deputitoj</ind>;
        <trd lng="hu">k&eacute;pvisel&odblac;h&aacute;z</trd>
        <trd lng="fa">&a_m;&a_j;&a_l;&a_s; &a_n;&a_m;&a_A;&f_y;&a_n;&a_d;&f_g;&a_A;&a_n;</trd>
        <trd lng="fr">Chambre des Repr&eacute;sentants<klr>(Belgique)
        </klr></trd>
        <trd lng="it">Camera dei Deputati</trd>
        <trdgrp lng="nl">
          <trd>Kamer van volksvertegenwoordigers<klr> (Belgi&euml;)
          </klr></trd>,
          <trd>Tweede <ind>Kamer</ind><klr> (Nederland)</klr></trd>
        </trdgrp>
        <trd lng="ru">&c_p;&c_a;&c_l;&c_a;&c_t;&c_a;
        &c_d;&c_je;&c_p;&c_u;&c_t;&c_a;&c_t;&c_o;&c_v;</trd>
      </ekz>
      <ekz>
        <ind>komerca <tld/>o</ind>.
        <trd lng="hu">kereskedelmi <ind>kamara</ind></trd>
        <trd lng="it">camera di commercio</trd>
        <trd lng="nl">kamer van koophandel</trd>
        <trd lng="ru">&c_t;&c_o;&c_r;&c_g;&c_o;&c_v;&c_a;&c_ja;
        &c_p;&c_a;&c_l;&c_a;&c_t;&c_a;</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_l;&c_a;&c_t;&c_a; <klr tip="ind">
    (&c_u;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_a;)</klr></trd>
    <trd lng="de">Kammer</trd>
    <trd lng="en">chamber</trd>
    <trd lng="es">c&aacute;mara</trd>
    <trd lng="fr">chambre <klr tip="ind">(institution)</klr></trd>
    <trdgrp lng="hu">
      <trd>h&aacute;z<klr> (pl. fels&odblac;h&aacute;z)</klr></trd>,
      <trd>kamara</trd>
    </trdgrp>
    <trd lng="it">camera</trd>
    <trdgrp lng="pl">
      <trd>izba</trd>
    </trdgrp>
    <trd lng="pt">c&acirc;mara</trd>
    <trd lng="ru">&c_p;&c_a;&c_l;&c_a;&c_t;&c_a;</trd>
    <trd lng="sk">komora</trd>
  </snc>
  <trd lng="bg">&c_s;&c_t;&c_a;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>pokoj</trd>,
    <trd>m&iacute;stnost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kammer</trd>,
    <trd>Zimmer</trd>
  </trdgrp>
  <trd lng="fa">&a_A;&a_t;&a_A;&a_q;</trd>
  <trd lng="nl">kamer</trd>
  <trd lng="tr">oda</trd>
</drv>

<drv mrk="cxambr.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      <ref tip="super" cel="serv.0isto">Servisto</ref>, kiu servas
      en lo&gcirc;o<tld/>oj:
      <ekz>
        li venis en tiu tempo, kiam la generalo nur elliti&gcirc;is kaj
        kiam lia <tld/>isto eble portis al li ar&gcirc;entan ujon por
        lavado
        <fnt><aut>N. Gogol, tr. V. Vy&ccirc;eg&jcirc;anin</aut>:
        <vrk>Malvivaj animoj</vrk>,
        <lok>Novelo pri kapitano Kopekin</lok>.
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_k;&c_a;&c_jo;&c_v;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_k;&c_a;&c_jo;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="es">camarista</trd>
  <trdgrp lng="fa">
    <trd>&f_p;&f_y;&a_sh;&a_kh;&a_d;&a_m;&a_t;</trd>,
    <trd>&a_kh;&a_d;&a_m;&a_t;&f_k;&a_A;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">cam&eacute;riste</trd>
  <trdgrp lng="hu">
    <trd>szobainas</trd>,
    <trd>komornyik</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>cameriere<klr> (domestico)</klr></trd>, 
    <trd>domestico<klr> (uomo di servizio)</klr></trd>
  </trdgrp>
  <trd lng="nl">kamenier</trd>
  <trd lng="pl">kamerdyner</trd>
  <trd lng="pt">camareiro</trd>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_r;&c_d;&c_i;&c_n;&c_je;&c_r;
  </trd>
</drv>

<drv mrk="cxambr.0istino">
  <kap><tld/>istino</kap>
  <snc>
    <dif>
      <ref tip="super" cel="serv.0istino">Servistino</ref>, kies
      laboro estas purigi kai ordigi lo&gcirc;o<tld/>ojn:
      <ekz>
        ni havas diversajn servantojn: kuiriston, <tld/>istinon,
        infanistinon kaj veturigiston
        <fnt><bib>F</bib>, <lok>&FE;37.</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi vidis la <tld/>istinon aux la kuiristinon?
        <fnt><bib>LR</bib>, <lok>Respondo 29, La Revuo, 1908, Februaro
        </lok></fnt>;
      </ekz>
      <ekz>
        de dek du jaroj &scirc;i kudras por la bona sinjorino elegantajn
        vestojn dum tutaj noktoj, akre admonas &scirc;iajn
        <tld/>istinojn kaj lakeojn, &ccirc;iumatene metas sur
        &scirc;iajn piedojn &scirc;trumpojn kaj &scirc;uojn,
        &ccirc;iuvespere faras el &scirc;ia litkovrilo belajn
        drapira&jcirc;ojn...
        <fnt><aut>E. Orzeszko, trad. Kabe</aut>:
        <vrk>La bona sinjorino.</vrk></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Zimmerm&auml;dchen</trd>
  <trd lng="fa">&a_kh;&a_d;&a_m;&a_t;&f_k;&a_A;&a_r;<klr tip="amb"> (&a_z;&a_n;)</klr></trd>
  <trd lng="hu">szobal&aacute;ny</trd>
  <trdgrp lng="it">
    <trd>cameriera<klr> (domestica)</klr></trd>,
    <trd>domestica<klr> (donna di servizio)</klr></trd>,  
    <trd>donna<klr> di servizio</klr></trd>
  </trdgrp>
  <trd lng="nl">kamermeisje</trd>
  <trd lng="pl">pokoj&oacute;wka</trd>
  <trd lng="pt">camareira</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_n;&c_i;&c_ch;&c_n;&c_a;&c_ja;</trd>
</drv>

<drv mrk="cxambr.antaux0o">
  <kap>anta&ubreve;<tld/>o</kap>
  <snc mrk="cxambr.antaux0o.porMantelo">
    <dif>
      Parto de lo&gcirc;ejo, &ccirc;e la enirejo,
      kie oni demetas la mantelojn, &ccirc;apelojn, pluvombrelojn, ktp.
    </dif>
    <refgrp tip="vid">
      <ref cel="korido.0o">koridoro</ref>,
      <ref cel="vestib.0o">vestiblo</ref>.
    </refgrp>
    <trd lng="be">
    &c_p;&c_ja;&c_r;&c_e;&c_d;&c_n;&c_ib; &c_p;&c_a;&c_k;&c_o;&c_j;
    </trd>
    <trd lng="de">Diele</trd>
    <trdgrp lng="fa">
      <trd>&a_A;&a_t;&a_A;&a_q;&f_k; &a_w;&a_r;&a_w;&a_d;&f_y;</trd>,
      <trd>&a_r;&a_A;&a_h;&a_r;&a_w;&f_y; &a_w;&a_r;&a_w;&a_d;&f_y;</trd>,
      <trd>&a_d;&a_h;&a_l;&f_y;&a_z;</trd>
    </trdgrp>
    <trd lng="fr">entr&eacute;e<klr tip="ind"> (vestibule)</klr></trd>
    <trd lng="hu">el&odblac;szoba</trd>
    <trdgrp lng="it">
      <trd>vestibolo</trd>,
      <trd>ingresso<klr> (stanza)</klr></trd>, 
      <trd>anticamera</trd>
    </trdgrp>
    <trd lng="nl">vestibule</trd>
    <trdgrp lng="pl">
      <trd>przedpok&oacute;j</trd>,
      <trd>sie&nacute;</trd>
    </trdgrp>
    <trd lng="pt">vest&iacute;bulo</trd>
    <trd lng="ru">&c_p;&c_r;&c_i;&c_h;&c_o;&c_zh;&c_a;&c_ja;</trd>
  </snc>
  <snc mrk="cxambr.antaux0o.porAkcepto">
    <dif>
      Salono, en kiu la vizitantoj de oficialulo a&ubreve;
      eminentulo atendas la akcepton.
    </dif>
    <trd lng="be">
    &c_p;&c_r;&c_y;&c_jo;&c_m;&c_n;&c_y; &c_p;&c_a;&c_k;&c_o;&c_j;</trd>
    <trd lng="de">Vorzimmer</trd>
    <trdgrp lng="fa">
      <trd>&a_A;&a_t;&a_A;&a_q; &a_A;&a_n;&a_t;&a_Z;&a_A;&a_r;</trd>,
      <trd>&f_p;&f_y;&a_sh;&zwnj;&a_t;&a_A;&a_l;&a_A;&a_r;</trd>
    </trdgrp>
    <trd lng="fr">antichambre</trd>
    <trdgrp lng="hu">
      <trd>fogad&oacute;szoba</trd>,
      <trd>fogad&oacute;helyis&eacute;g</trd>
    </trdgrp>
    <trd lng="it">anticamera</trd>
    <trdgrp lng="nl">
      <trd>voorvertrek</trd>,
      <trd>voorkamer</trd>,
      <trd>wachtkamer</trd>
    </trdgrp>
    <trd lng="pt">antec&acirc;mera</trd>
  </snc>
  <trd lng="en">antechamber</trd>
  <trd lng="hu">el&odblac;szoba</trd>
  <trd lng="sk">predsie&ncaron;</trd>
</drv>

<drv mrk="cxambr.atendo0o">
  <kap>atendo<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="&Ccirc;"/>o, &gcirc;enerale kun se&gcirc;oj, por
      atendado:
      <ekz>
        mi atendis la kuraciston en atendo<tld/>o dum duonhoro.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Wartezimmer</trd>
  <trd lng="fa">&a_A;&a_t;&a_A;&a_q; &a_A;&a_n;&a_t;&a_Z;&a_A;&a_r;</trd>
  <trd lng="hu">v&aacute;r&oacute;szoba</trd>
  <trd lng="it">sala<klr> d'attesa</klr></trd>
  <trd lng="pl">poczekalnia</trd>
  <trd lng="pt">sala de espera</trd>
</drv>

<drv mrk="cxambr.ban0o">
  <kap>ban<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="&Ccirc;"/>o por bano, du&scirc;o, korpa purigado kaj
      flegado.
    </dif>
  </snc>
  <trd lng="bg">&c_b;&c_a;&c_n;&c_ja;</trd>
  <trd lng="de">Badezimmer</trd>
  <trd lng="fa">&a_H;&a_m;&a_A;&a_m;</trd>
  <trd lng="fr">salle de bain</trd>
  <trd lng="hu">f&uuml;rd&odblac;szoba</trd>
  <trdgrp lng="it">
    <trd>bagno<klr> (stanza)</klr></trd>,
    <trd>stanza<klr> da bagno</klr></trd>
  </trdgrp>
  <trd lng="nl">badkamer</trd>
  <trd lng="pl">&lstroke;azienka</trd>
  <trd lng="pt">banheiro</trd>
  <trd lng="ru">&c_v;&c_a;&c_n;&c_n;&c_a;&c_ja;</trd>
</drv>

<drv mrk="cxambr.lav0o">
  <kap>lav<tld/>o</kap>
  <snc mrk="cxambr.lav0o.KOMUNE">
    <dif>
      <tld lit="&Ccirc;"/>o en kiu oni lavas sin a&ubreve;
      tola&jcirc;on.
      <ekz>
        Adamo estas en lav<tld/>o kaj lavas sin.
        <fnt>
          <vrk>Lekcje Esperanto
<url
ref="http://www.gora.com/cms_pobierz_dokument.php?id=706&amp;dok_id=74">
            1-15</url>
          </vrk>,
          <lok>Leciono dekdua</lok>
       </fnt>;
      </ekz>
    </dif>
    <ref tip="super" cel="lav.0ejo.KOMUNE">lavejo</ref>
    <ref tip="sin" cel="cxambr.ban0o">ban&ccirc;ambro</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Waschzimmer</trd>,
    <trd>Waschraum</trd>
  </trdgrp>
  <trd lng="fa">&a_A;&a_t;&a_A;&a_q; &a_sh;&a_s;&a_t;&a_sh;&a_w;</trd>
  <trdgrp lng="it">
    <trd>bagno<klr> (stanza)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&lstroke;azienka</trd>,
    <trd>pralnia</trd>
  </trdgrp>
</drv>

<drv mrk="cxambr.dormo0o">
  <kap>dormo<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="&Ccirc;"/>o ordinare kvieta kaj ta&ubreve;ge ekipita por
      dormado.
      <ekz>
        je la deka vespere &ccirc;iuj estis en la dormo<tld/>oj, je la
        deka  kaj duono la domo dormadis
        <fnt><aut>Machado de Assis, trad. Paulo Viana</aut>
        <vrk>La divenistino kaj aliaj rakontoj</vrk><lok>Oportuno, jaro
        2005a, p. 27a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_s;&c_p;&c_a;&c_l;&c_n;&c_ja;</trd>
  <trd lng="de">Schlafzimmer</trd>
  <trd lng="fa">&a_A;&a_t;&a_A;&a_q; &a_kh;&a_w;&a_A;&a_b;</trd>
  <trd lng="fr">chambre &agrave; coucher</trd>
  <trd lng="hu">h&aacute;l&oacute;szoba</trd>
  <trdgrp lng="it">
    <trd>camera<klr> (stanza da letto)</klr></trd>,
    <trd>camera<klr> da letto</klr></trd>
  </trdgrp>
  <trd lng="nl">slaapkamer</trd>
  <trd lng="pl">sypialnia</trd>
  <trd lng="pt">quarto<klr tip="ind"> (de dormir)</klr></trd>
  <trd lng="ru">&c_s;&c_p;&c_a;&c_l;&c_mol;&c_n;&c_ja;</trd>
</drv>

<drv mrk="cxambr.klas0o">
  <kap>klas<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="&Ccirc;"/>o en lernejo, kie klaso lernas:
      <ekz>
        la klas<tld/>oj havis multajn fenestrojn por havigi sufi&ccirc;e
        da lumo <fnt><aut>Den Drown</aut><vrk>Elektoj</vrk>
        <bib>Monato</bib> <lok>jaro 2003a, numero 10a, p. 23a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Klassenzimmer</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_t;&a_A;&a_q; &a_d;&a_r;&a_s;</trd>,
    <trd>&f_k;&a_l;&a_A;&a_s; &a_d;&a_r;&a_s;</trd>
  </trdgrp>
  <trd lng="hu">oszt&aacute;lyterem</trd>
  <trd lng="it">aula<klr> (scuola)</klr></trd>
  <trd lng="pl">klasa</trd>
  <trd lng="pt">sala de aula</trd>
</drv>

<drv mrk="cxambr.mangxo0o">
  <kap>man&gcirc;o<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="&Ccirc;"/>o por man&gcirc;ado:
      <ekz>
        koridoro, kiu kondukis de la vizit<tld/>o al la
        man&gcirc;o<tld/>o
        <fnt><aut>Machado de Assis, trad. Paulo Viana</aut>
        <vrk>La divenistino kaj aliaj rakontoj</vrk><lok>Oportuno, jaro
        2005a, p. 29a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Esszimmer</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_t;&a_A;&a_q; &a_gh;&a_dh;&a_A;&a_kh;&a_w;&a_r;&f_y;</trd>,
    <trd>&a_n;&a_A;&a_h;&a_A;&a_r;&a_kh;&a_w;&a_r;&f_y;</trd>
  </trdgrp>
  <trd lng="hu">eb&eacute;dl&odblac;</trd>
  <trd lng="it">sala<klr> da pranzo</klr></trd>
  <trd lng="pl">jadalnia</trd>
  <trdgrp lng="pt">
    <trd>refeit&oacute;rio</trd>,
    <trd>sala de jantar</trd>
  </trdgrp>
</drv>

<drv mrk="cxambr.masxin0o">
  <kap>ma&scirc;in<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="&Ccirc;"/>o en &scirc;ipo kie estas la ma&scirc;inoj:
      <ekz>
