<drv mrk="cxambr.0o">
  <kap><ofc>*</ofc> <tld/>o</kap>
  <snc mrk="cxambr.0o.doma">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      &Ccirc;iu el la apartaj ejoj de <ref tip="malprt" cel="dom.0o">
      domo</ref>, disigitaj per muroj a&ubreve; vandoj, kaj
      komuniki&gcirc;antaj per pordoj:
      <ekz>
        &ccirc;irka&ubreve; la templo kaj la plejsanktejo, kaj li faris flankajn <tld/>ojn
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 6:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>egon el kolonoj li faris, havantan la longon de kvindek ulnoj kaj la
        lar&gcirc;on de tridek ulnoj
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 7:6</lok>
        </fnt>.
      </ekz>
    </dif>
    <subsnc mrk="cxambr.0o.logxeja">
      <dif>
        <tld lit="&Ccirc;"/>o en lo&gcirc;ejo:
        <ekz>
          la patro estas en la <tld/>o
          <fnt>
            <bib>F</bib>
            <lok>&FE; 8</lok>
          </fnt>;
        </ekz>
      <ekz>
        en la <tld/>o sidis nur kelke da homoj
        <fnt>
          <bib>F</bib>
          <lok>&FE; 32</lok>
        </fnt>;
      </ekz>
        <ekz>
          li eniris en internan <tld/>on kaj ploris tie
          <fnt>
            <bib>MT</bib>
            <lok>Genezo 43:30</lok>
          </fnt>;
        </ekz>
        <ekz>
          en la dometo estis malgranda vestiblo, bela <tld/>o kun kamero
          <fnt>
            <bib>ElektFab</bib>
            <lok>Fi&scirc;kaptisto kaj lia edzino</lok>
          </fnt>;
        </ekz>
        <ekz>
          post mia morto vi montros al li la tutan kastelon, &ccirc;iujn <tld/>ojn, salonojn kaj
          kelojn
          <fnt>
            <bib>ElektFab</bib>
            <lok>Fidela Johano</lok>
          </fnt>;
        </ekz>
        <ekz>
          <nom>Karal</nom> havas <tld/>on en apartamento, kiun li kunluas kun tri aliaj studentoj
          <fnt>
            <bib>ChV</bib>
            <lok>3</lok>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="sub">
        <ref cel="cxel.0o.cxambreto">&ccirc;elo<sncref/></ref>,
        <ref cel="kabine.0o.pensejo">kabineto<sncref/></ref>,
        <ref cel="kamer.0o.cxambro">kamero<sncref/></ref>
      </refgrp>
      <trd lng="ar">&a_H;&a_damma;&a_j;&a_sukun;&a_r;&a_fatha;&a_t_marbuta;&a_dammatan;
      &a_j; &a_H;&a_damma;&a_j;&a_fatha;&a_r;&a_dammatan;</trd>
      <trd lng="be">&c_p;&c_a;&c_k;&c_o;&c_j;</trd>
      <trd lng="ca">habitaci&oacute;</trd>
      <trd lng="de">Raum</trd>
      <trdgrp lng="es">
        <trd>habitaci&oacute;n</trd>,
        <trd>cuarto</trd>,
        <trd>rec&aacute;mara</trd>
      </trdgrp>
      <trd lng="fr">pi&egrave;ce <klr tip="ind">(d'habitation)</klr>
      </trd>
      <trd lng="hu">szoba</trd>
      <trdgrp lng="pl">
        <trd>pok&oacute;j</trd>,
        <trd>izba</trd>,
        <trd>pomieszczenie</trd>,
        <trd>sala</trd>
      </trdgrp>
      <trd lng="pt">c&ocirc;modo<klr tip="ind"> (subst.)</klr></trd>
      <trdgrp lng="ro">
        <trd>camer&abreve;</trd>,
        <trd>odaie</trd>
      </trdgrp>
      <trd lng="sk">izba</trd>
      <trd lng="sv">rum</trd>
    </subsnc>
    <subsnc mrk="cxambr.0o.ofica">
      <dif>
        <tld lit="&Ccirc;"/>o ofica:
        <ekz>
          koran dankon al la glora Kembri&gcirc;a universitato, kiu pruntis al ni siajn <tld/>ojn
          <fnt>
            <bib>Paroloj</bib>
            <vrk>Parolado anta&ubreve; la Tria Kongreso Esperantista en Cambridge en la 12a de a&ubreve;gusto
            1907</vrk>
          </fnt>;
        </ekz>
        <ekz>
          tiu <tld/>o havis &scirc;ajnon de saloneto, sed en efektiveco &gcirc;i estis komercejo tia
          sama, kiaj estas &ccirc;iuj aliaj komercejoj
          <fnt>
            <bib>Marta</bib>
          </fnt>;
        </ekz>
        <ekz>
          <tld/>o 150 estas &scirc;ia oficejo
          <fnt>
            <bib>CKv</bib>
            <lok>4</lok>
          </fnt>;
        </ekz>
        <ekz>
          psikologino vizitos la knabon en ties hospitala <tld/>o
          <fnt>
            <bib>ChR</bib>
            <lok>Tien kaj reen</lok>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="auxl.0o">a&ubreve;lo</ref>,
        <ref cel="ofic.0ejo">oficejo</ref>,
        <ref cel="salon.0o">salono</ref>,
        <ref cel="solen">solenejo</ref>
      </refgrp>
      <trd lng="be">&c_z;&c_a;&c_l;&c_ja;&c_p;&c_a;&c_s;&c_je;&c_d;&c_zh;&c_a;&c_n;&c_mol;&c_n;&c_ja;&c_w;</trd>
      <trdgrp lng="de">
        <trd>Kammer</trd>,
        <trd>Amtsstube</trd>
      </trdgrp>
      <trd lng="es">c&aacute;mara</trd>
      <trd lng="fr">salle</trd>
      <trd lng="hu">helyis&eacute;g</trd>
      <trdgrp lng="pl">
        <trd>pok&oacute;j</trd>,
        <trd>pomieszczenie</trd>
      </trdgrp>
      <trd lng="pt">c&acirc;mara</trd>
      <trd lng="sk">miestnos&tcaron;</trd>
    </subsnc>
    <trd lng="de">Stube</trd>
    <trd lng="en">room</trd>
    <trd lng="he">&het;&dalet;&resh;</trd>
    <trdgrp lng="it">
      <trd>stanza</trd>, 
      <trd>camera</trd>,
      <trd>sala</trd>
