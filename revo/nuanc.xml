<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>nuanc</rad>/o
</kap>
<drv mrk="nuanc.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Delikata apena&ubreve; sentebla diferenco inter gradoj kaj kombinoj de
      iu
      koloro:
      <ekz>
        la <tld/>o de la granata ru&gcirc;o estas malhela.
      </ekz>
    </dif>
    <trd lng="en">shade</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Delikata diferenco inter samspecaj aferoj:
      <ekz>
        <tld/>o de stilo, de sento;
      </ekz>
      <ekz>
        tio estus nur <tld/>o, sed tiaj <tld/>oj ofte estas gravaj en la
        vivo;
      </ekz>
      <ekz>
        Esperanto povas esprimi &ccirc;iujn <tld/>ojn de la penso<fnt>Z</fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ju;&c_a;&c_n;&c_s;</trd>,
    <trd>&c_a;&c_d;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="de">Nuance</trd>
  <trd lng="en">nuance</trd>
  <trdgrp lng="hu">
    <trd>&aacute;rnyalat</trd>,
    <trd>n&uuml;ansz</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>nuance</trd>,
    <trd>schakering</trd>
  </trdgrp>
  <trd lng="pt">nuan&ccedil;a</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_t;&c_je;&c_n;&c_o;&c_k;</trd>,
    <trd>&c_n;&c_ju;&c_a;&c_n;&c_s;</trd>
  </trdgrp>
  <trd lng="sv">nyans</trd>
</drv>

<drv mrk="nuanc.0i">
  <kap><tld/>i<fnt>Z</fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Meti <tld/>ojn en ion, delikate gradigi:
      <ekz>
        <tld/>i sian stilon, siajn ripro&ccirc;ojn;
      </ekz>
      <ekz>
        la &ccirc;ielo <tld/>i&gcirc;is de mil diversaj koloroj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_ju;&c_a;&c_n;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">nuancieren</trd>
  <trd lng="hu">&aacute;rnyal</trd>
  <trd lng="nl">nuanceren</trd>
  <trd lng="pt">nuan&ccedil;ar</trd>
  <trd lng="ru">&c_n;&c_ju;&c_a;&c_n;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">nyansera</trd>
</drv>
</art>
