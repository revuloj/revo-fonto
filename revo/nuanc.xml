<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: nuanc.xml,v 1.25 2024/01/29 15:59:31 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>nuanc</rad>/o
    </kap>

    <drv mrk="nuanc.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc>
        <dif>
          Delikata apena&ubreve; sentebla diferenco inter gradoj kaj kombinoj
          de
          iu koloro:
          <ekz>
            la <tld/>o de la granata ru&gcirc;o estas malhela.
          </ekz>
        </dif>
        <trd lng="en">shade</trd>
        <trd lng="pl">odcie&nacute;</trd>
        <trd lng="ru">&c_o;&c_t;&c_t;&c_je;&c_n;&c_o;&c_k;</trd>
      </snc>
      <snc>
        <uzo tip="stl">FIG</uzo>
        <dif>
          Delikata diferenco inter samspecaj aferoj:
          <ekz>
            <tld/>o de stilo, de sento;
          </ekz><ekz>
            tio estus nur <tld/>o, sed tiaj <tld/>oj ofte estas gravaj
            en la
            vivo;
          </ekz><ekz>
            &ccirc;iuj <tld/>oj de la homa penso kaj sento povas esti
            esprimitaj en &gcirc;i <klr>(en Esperanto)</klr> en la plej bona
            maniero
            <fnt><bib>EE</bib>, <lok>&ccirc;ap. 5a</lok></fnt>.
          </ekz>
        </dif>
        <trdgrp lng="ru">
          <trd>&c_o;&c_t;&c_t;&c_je;&c_n;&c_o;&c_k;</trd>,
          <trd>&c_n;&c_ju;&c_a;&c_n;&c_s;</trd>
        </trdgrp>
      </snc>
  <trdgrp lng="be">
        <trd>&c_n;&c_ju;&c_a;&c_n;&c_s;</trd>,
        <trd>&c_a;&c_d;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
      </trdgrp>
  <trd lng="bo">&#xF5A;&#xF7C;&#xF53;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>jemn&yacute; rozd&iacute;l</trd>,
    <trd>nuance</trd>,
    <trd>n&aacute;dech</trd>,
    <trd>odst&iacute;n</trd>,
    <trd>p&rcaron;&iacute;dech</trd>,
    <trd>slab&yacute; n&aacute;znak</trd>
  </trdgrp>
  <trd lng="de">Nuance</trd>
  <trd lng="en">nuance</trd>
  <trd lng="es">matiz</trd>
      <trd lng="fr">nuance</trd>
  <trdgrp lng="hu">
        <trd>&aacute;rnyalat</trd>,
        <trd>n&uuml;ansz</trd>
      </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5FAE;&#x5999;&#x306A;&#x5DEE;&#x7570;  <pr>&#x3073;&#x307F;&#x3087;&#x3046;&#x306A;&#x3055;&#x3044;</pr></trd>,
    <trd>&#x8272;&#x5408;&#x3044;  <pr>&#x3044;&#x308D;&#x3042;&#x3044;</pr></trd>,
    <trd>&#x30CB;&#x30E5;&#x30A2;&#x30F3;&#x30B9;</trd>
  </trdgrp>
  <trdgrp lng="nl">
        <trd>nuance</trd>,
        <trd>schakering</trd>
      </trdgrp>
  <trd lng="pl">niuans</trd>
  <trd lng="pt">nuan&ccedil;a</trd>
  <trdgrp lng="sk">
    <trd>nuansa</trd>,
    <trd>odtie&ncaron;</trd>,
    <trd>sfarbenie</trd>
  </trdgrp>
  <trd lng="sv">nyans</trd>
  <trdgrp lng="uk">
    <trd>&#x432;&#x456;&#x434;&#x442;&#x456;&#x43D;&#x43E;&#x43A;</trd>,
    <trd>&#x43D;&#x44E;&#x430;&#x43D;&#x441;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7D30;&#x5FAE;&#x5DEE;&#x5225; <pr>x&#xEC;w&#x113;ich&#x101;bi&#xE9;</pr></trd>,
    <trd>&#x7EC6;&#x5FAE;&#x5DEE;&#x522B; <pr>x&#xEC;w&#x113;ich&#x101;bi&#xE9;</pr></trd>,
    <trd>&#x8272;&#x5F69;&#x5C42;&#x6B21; <pr>s&#xE8;c&#x1CE;ic&#xE9;ngc&#xEC;</pr></trd>,
    <trd>&#x8272;&#x5F69;&#x5C64;&#x6B21; <pr>s&#xE8;c&#x1CE;ic&#xE9;ngc&#xEC;</pr></trd>,
    <trd>&#x8272;&#x8ABF;&#x5DEE;&#x5225; <pr>s&#xE8;di&#xE0;och&#x101;bi&#xE9;</pr></trd>,
    <trd>&#x8272;&#x8C03;&#x5DEE;&#x522B; <pr>s&#xE8;di&#xE0;och&#x101;bi&#xE9;</pr></trd>
  </trdgrp>
</drv>

    <drv mrk="nuanc.0i">
      <kap><tld/>i<fnt>Z</fnt></kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Meti <tld/>ojn en ion, delikate gradigi:
          <ekz>
            <tld/>i sian stilon, siajn ripro&ccirc;ojn;
          </ekz><ekz>
            la &ccirc;ielo <tld/>i&gcirc;is de mil diversaj koloroj.
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_n;&c_ju;&c_a;&c_n;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>nuancovat</trd>,
    <trd>odst&iacute;nit</trd>
  </trdgrp>
  <trd lng="de">nuancieren</trd>
  <trdgrp lng="en">
    <trd>to nuance</trd>,
    <trd>gradate</trd>
  </trdgrp>
  <trd lng="es">matizar</trd>
      <trd lng="fr">nuancer</trd>
  <trd lng="hu">&aacute;rnyal</trd>
  <trdgrp lng="ja">
    <trd>&#x8272;&#x5408;&#x3044;&#x3092;&#x3064;&#x3051;&#x308B;  <pr>&#x3044;&#x308D;&#x3042;&#x3044;&#x3092;&#x3064;&#x3051;&#x308B;</pr></trd>,
    <trd>&#x5FAE;&#x5999;&#x306A;&#x5909;&#x5316;&#x3092;&#x3064;&#x3051;&#x308B;  <pr>&#x3073;&#x307F;&#x3087;&#x3046;&#x306A;&#x3078;&#x3093;&#x304B;&#x3092;&#x3064;&#x3051;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">nuanceren</trd>
  <trdgrp lng="pl">
    <trd>niuansowa&cacute;</trd>,
    <trd>wprowadza&cacute; <ind>niuanse</ind></trd>,
    <trd>pokazywa&cacute; <ind>niuanse</ind></trd>
  </trdgrp>
  <trd lng="pt">nuan&ccedil;ar</trd>
  <trd lng="ru">&c_n;&c_ju;&c_a;&c_n;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>nuansova&tcaron;</trd>,
    <trd>odtie&ncaron;ova&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">nyansera</trd>
</drv>
