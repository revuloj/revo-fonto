<vortaro>
  <art mrk="$Id: nuanc.xml,v 1.27 2025/10/03 20:30:24 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>nuanc</rad>/o
    <fnt><bib>UV</bib></fnt></kap>

<drv mrk="nuanc.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Delikata apena&ubreve; sentebla <ref tip="super" cel="difere.0o.KLA">diferenco</ref> inter gradoj kaj kombinoj
      de iu koloro:
      <ekz>
        kurtenoj havant<klr>[aj]</klr> koloron de delikate flava <tld/>o
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">shade</trd>
    <trd lng="pl">odcie&nacute;</trd>
    <trd lng="ru">&c_o;&c_t;&c_t;&c_je;&c_n;&c_o;&c_k;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Delikata diferenco inter samspecaj aferoj:
      <ekz>
        <tld/>o de stilo, de sento;
      </ekz>
      <ekz>
        kun ironia <tld/>o li ekparolis
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili prenis akvon en la bu&scirc;on, por doni al sia vo&ccirc;o ploretan <tld/>on
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj <tld/>oj de la homa penso kaj sento povas esti
        esprimitaj en &gcirc;i <klr>(en Esperanto)</klr> en la plej bona
        maniero
        <fnt><bib>EE</bib>, <lok>&ccirc;ap. 5a</lok></fnt>.
      </ekz>
      <ekz>
        raporti pri &ccirc;iuj cirkonstancoj kaj <tld/>oj
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Feinheit</trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_t;&c_t;&c_je;&c_n;&c_o;&c_k;</trd>,
      <trd>&c_n;&c_ju;&c_a;&c_n;&c_s;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ju;&c_a;&c_n;&c_s;</trd>,
    <trd>&c_a;&c_d;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="bo">&t_tsha;&t_naro;&t_na;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>jemn&yacute; rozd&iacute;l</trd>,
    <trd>nuance</trd>,
    <trd>n&aacute;dech</trd>,
    <trd>odst&iacute;n</trd>,
    <trd>p&rcaron;&iacute;dech</trd>,
    <trd>slab&yacute; n&aacute;znak</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Nuance</trd>,
    <trd>Abstufung</trd>,
    <trd>Schattierung</trd>
  </trdgrp>
  <trd lng="en">nuance</trd>
  <trd lng="es">matiz</trd>
  <trd lng="fr">nuance</trd>
  <trdgrp lng="hu">
    <trd>&aacute;rnyalat</trd>,
    <trd>n&uuml;ansz</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5FAE;&#x5999;&#x306A;&#x5DEE;&#x7570;  <pr>&#x3073;&#x307F;&#x3087;&#x3046;&#x306A;&#x3055;&#x3044;</pr></trd>,
    <trd>&#x8272;&#x5408;&#x3044;  <pr>&#x3044;&#x308D;&#x3042;&#x3044;</pr></trd>,
    <trd>&#x30CB;&#x30E5;&#x30A2;&#x30F3;&#x30B9;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>nuance</trd>,
    <trd>schakering</trd>
  </trdgrp>
  <trd lng="pl">niuans</trd>
  <trd lng="pt">nuan&ccedil;a</trd>
  <trdgrp lng="sk">
    <trd>nuansa</trd>,
    <trd>odtie&ncaron;</trd>,
    <trd>sfarbenie</trd>
  </trdgrp>
  <trd lng="sv">nyans</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_t;&c_ib;&c_n;&c_o;&c_k;</trd>,
    <trd>&c_n;&c_ju;&c_a;&c_n;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7D30;&#x5FAE;&#x5DEE;&#x5225; <pr>x&igrave;w&emacron;ich&amacron;bi&eacute;</pr></trd>,
    <trd>&#x7EC6;&#x5FAE;&#x5DEE;&#x522B; <pr>x&igrave;w&emacron;ich&amacron;bi&eacute;</pr></trd>,
    <trd>&#x8272;&#x5F69;&#x5C42;&#x6B21; <pr>s&egrave;c&#x1CE;ic&eacute;ngc&igrave;</pr></trd>,
    <trd>&#x8272;&#x5F69;&#x5C64;&#x6B21; <pr>s&egrave;c&#x1CE;ic&eacute;ngc&igrave;</pr></trd>,
    <trd>&#x8272;&#x8ABF;&#x5DEE;&#x5225; <pr>s&egrave;di&agrave;och&amacron;bi&eacute;</pr></trd>,
    <trd>&#x8272;&#x8C03;&#x5DEE;&#x522B; <pr>s&egrave;di&agrave;och&amacron;bi&eacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="nuanc.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Meti <tld/>ojn en ion, delikate <ref cel="grad.0igi">gradigi</ref>:
      <ekz>
        sur la &ccirc;ielo la vesperru&gcirc;o komencis <klr>[&#x2026;]</klr> <tld/>i, kiel opalo
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni venigis laboristojn el ekstere, &ccirc;efe siciliajn kaj albanajn, kiuj <tld/>is
        la urbon per sia speciala, propra etoso
        <fnt>
          <bib>ChB</bib>
          <lok>1</lok>
        </fnt>;
      </ekz>
      <ekz>
        eta parentezo por <tld/>i la lastajn alineojn
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam brito metas krucon sur balotilon <klr>[&#x2026;]</klr>, ne eblas <tld/>i la
        preferojn
        <fnt>
          <bib>Monato</bib>
          <aut>W. H. Simcock</aut>
          <vrk><url ref="&Monato;/2005/008793.php"
              >Iom da proporcio</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_ju;&c_a;&c_n;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>nuancovat</trd>,
    <trd>odst&iacute;nit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>nuancieren</trd>,
    <trd>abstufen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>to nuance</trd>,
    <trd>gradate</trd>
  </trdgrp>
  <trd lng="es">matizar</trd>
      <trd lng="fr">nuancer</trd>
  <trd lng="hu">&aacute;rnyal</trd>
  <trdgrp lng="ja">
    <trd>&#x8272;&#x5408;&#x3044;&#x3092;&#x3064;&#x3051;&#x308B;  <pr>&#x3044;&#x308D;&#x3042;&#x3044;&#x3092;&#x3064;&#x3051;&#x308B;</pr></trd>,
    <trd>&#x5FAE;&#x5999;&#x306A;&#x5909;&#x5316;&#x3092;&#x3064;&#x3051;&#x308B;  <pr>&#x3073;&#x307F;&#x3087;&#x3046;&#x306A;&#x3078;&#x3093;&#x304B;&#x3092;&#x3064;&#x3051;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">nuanceren</trd>
  <trdgrp lng="pl">
    <trd>niuansowa&cacute;</trd>,
    <trd>wprowadza&cacute; <ind>niuanse</ind></trd>,
    <trd>pokazywa&cacute; <ind>niuanse</ind></trd>
  </trdgrp>
  <trd lng="pt">nuan&ccedil;ar</trd>
  <trd lng="ru">&c_n;&c_ju;&c_a;&c_n;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>nuansova&tcaron;</trd>,
    <trd>odtie&ncaron;ova&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">nyansera</trd>
</drv>
