<drv mrk="konsen.inter0i">
  <kap>inter<tld/>i</kap>
  <snc mrk="konsen.inter0i.akordigxi">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Esti en akordo; akorde agi:
      <ekz>
        perfide, sekrete inter<tld/>i<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_g;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_d;&c_a;&c_m;&c_o;&c_v;&c_ib;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_p;&c_a;&c_r;&c_a;&c_z;&c_u;&c_m;&c_je;&c_c;&c_c;&c_a;</trd>
    </trdgrp>
    <trd lng="ru">&c_d;&c_o;&c_g;&c_o;&c_v;&c_o;&c_r;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
  <snc mrk="konsen.inter0i.kontrakti">
    <gra><vspec>x</vspec></gra>
    <dif>
      Aran&gcirc;i kontrakton kun iu pri difinita punkto; interakcepti:
      <ekz>
        ili inter<tld/>is la prezon, pri la prezo;
      </ekz>
      <ekz>
        inter<tld/>ita signo
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kondic1.0a">kondi&ccirc;a</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_u;&c_z;&c_g;&c_a;&c_d;&c_n;&c_ja;&c_c;&c_mol;</trd>,
      <trd>&c_u;&c_z;&c_g;&c_a;&c_d;&c_n;&c_ib;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>dohodnout se</trd>,
    <trd>domluvit se</trd>,
    <trd>umluvit se</trd>,
    <trd>uzav&rcaron;&iacute;t dohodu</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>&uuml;bereinstimmen</trd>,
    <trd>einer <ind>Meinung</ind> sein</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>come to terms with</trd>,
    <trd>come to an understanding with</trd>,
  </trdgrp>
  <trdgrp lng="es">
    <trd>convenir</trd>,
    <trd>acordar</trd>,
    <trd>pactar</trd>
  </trdgrp>
  <trd lng="fr">s'<ind>accorder</ind> pour</trd>
  <trd lng="nl">afspreken</trd>
  <trdgrp lng="pl">
    <trd>godzi&cacute; si&eogonek;</trd>,
    <trd>umawia&cacute; si&eogonek;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>primi</trd>,
    <trd>aranja</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dohovori&tcaron;  <klr>(sa)</klr></trd>,
    <trd>zhodn&uacute;&tcaron; sa</trd>
  </trdgrp>
</drv>

<drv mrk="konsen.inter0o">
  <kap>inter<tld/>o</kap>
  <snc mrk="konsen.inter0o.KOMUNE">
    <dif>
      Ago inter<tld/>i, rezulto de tiu ago:
      <ekz>
        esti en inter<tld/>o kun iu<fnt>Z</fnt>;
      </ekz>
      <ekz>
        pli valoras inter<tld/>o ol ju&gcirc;a dokumento
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>domluva</trd>,
    <trd>ujedn&aacute;n&iacute;</trd>,
    <trd>vz&aacute;jemn&aacute; dohoda</trd>,
    <trd>&uacute;mluva</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>acuerdo</trd>,
    <trd>pacto</trd>
  </trdgrp>
  <trd lng="fr">accord</trd>
  <trdgrp lng="nl">
    <trd>instemming</trd>,
    <trd>overeenstemming</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>konsensus</trd>,
    <trd>porozumienie</trd>,
    <trd>ugoda</trd>
  </trdgrp> 
  <trdgrp lng="ro">
    <trd>acord</trd>,
    <trd>consens</trd>,
    <trd>consemna&tcedil;iune</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dohoda</trd>,
    <trd>dohovor</trd>,
    <trd>zhoda</trd>
  </trdgrp>
</drv>

<drv mrk="konsen.mal0i">
  <kap>mal<tld/>i<fnt>Z</fnt></kap>
  <snc mrk="konsen.mal0i.malsamsenti">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Havi diferencajn, kontra&ubreve;ajn opiniojn a&ubreve; dezirojn:
      <ekz>
        okazas ofte ke fratoj mal<tld/>as inter si;
      </ekz>
      <ekz>
        internacia mal<tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="diskut">diskuto</ref>,
      <ref cel="konfli.0o">konflikto</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_n;&c_je;
      <ind>&c_z;&c_g;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_c;&c_a;</ind></trd>,
      <trd>&c_n;&c_je;
      <ind>&c_p;&c_a;&c_g;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_c;&c_a;</ind></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>nicht &uuml;bereinstimmen</trd>,
      <trd>unterschiedlicher <ind>Meinung</ind> sein</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>disagree</trd>,
      <trd>disssent</trd>
    </trdgrp>
    <trd lng="es">estar en desacuerdo</trd>
    <trd lng="fr">&ecirc;tre en <ind>d&eacute;saccord</ind></trd>
    <trd lng="nl">het <ind>oneens</ind> zijn</trd>
    <trdgrp lng="ru">
      <trd>&c_b;&c_y;&c_t;&c_mol; &c_n;&c_je;
      &c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_n;&c_y;&c_m;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_o;&c_j;&c_t;&c_i;&c_s;&c_mol; &c_v;&c_o;
      &c_m;&c_n;&c_je;&c_n;&c_i;&c_ja;&c_h;</trd>
    </trdgrp>
  </snc>
  <snc mrk="konsen.mal0i.rifuzi">
    <gra><vspec>x</vspec></gra>
    <dif>
      Malakcepti, rifuzi proponon a&ubreve; peton:
      <ekz>
        li mal<tld/>is, ke oni lin esploru;
      </ekz>
      <ekz>
        ni esperas, ke ili ne mal<tld/>os, se mi hodia&ubreve; sciigos
        publike, ke...<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mal<tld/>i kun iu pri io.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxikan.0i">&ccirc;ikani</ref>,
      <ref cel="kverel.0i">kvereli</ref>,
      <ref cel="pac.mal0i">malpaci</ref>,
      <ref cel="proces1">procesi</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_a;&c_d;&c_m;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_d;&c_m;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_a;&c_d;&c_h;&c_ib;&c_l;&c_ja;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_d;&c_r;&c_y;&c_n;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_d;&c_k;&c_ib;&c_d;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>ablehnen</trd>,
      <trd>zur&uuml;ckweisen</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>deny</trd>,
      <trd>refuse</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>rehusar</trd>,
      <trd>rechazar</trd>
    </trdgrp>
    <trd lng="fr">refuser</trd>
    <trd lng="nl">weigeren</trd>
    <trd lng="ru">&c_o;&c_t;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="cs">nesouhlasit</trd>
  <trd lng="pl">nie zgadza&cacute; si&eogonek;</trd>     
  <trd lng="ro">nu fi de acord</trd>
  <trdgrp lng="sk">
    <trd>nes&uacute;hlasi&tcaron;</trd>,
    <trd>odmietnu&tcaron;</trd>,
    <trd>oponova&tcaron;</trd>
  </trdgrp>
</drv>
