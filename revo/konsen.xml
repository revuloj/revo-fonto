<drv mrk="konsen.0o">
  <kap><tld/>o</kap>
  <snc mrk="konsen.0o.akordo">
    <dif>
      <ref tip="super" cel="akord.0o.samopinieco">Akordo</ref> inter pluraj
      personoj a&ubreve; a&jcirc;oj pri io:
      <ekz>
         tiuj &ccirc;i horlo&gcirc;oj iras tute regule kaj &ccirc;iuj en preciza <tld/>o
         <fnt><bib>FK</bib></fnt>;
      </ekz>
      <ekz>
        kiel belega &gcirc;i estas, kiam fratoj vivas en <tld/>o inter si
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        la du kontraktantoj rapide venis al <tld/>o
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        Fradeko kaj sinjorino Herbeno akordi&gcirc;is per neesprimita <tld/>o
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Knut</nom> havis la impreson, kvaza&ubreve; tio, kion &scirc;i legis, parolas &gcirc;uste pri lia amo,
        tiel plene &gcirc;i estis en <tld/>o kun liaj propraj pensoj
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o konstruas, malpaco detruas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pac.0o">paco</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Zustimmung</trd>,
      <trd>Konsens</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>accord</trd>,
      <trd>approval</trd>,
      <trd>assent</trd>,
      <trd>concurrence</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd><baz>mufakat</baz>pemufakatan</trd>,
      <trd><baz>sepakat</baz>kesepakatan</trd>,
      <trd><baz>tuju</baz>persetujuan</trd>
    </trdgrp>
    <trd lng="nl">eensgezindheid</trd>
  </snc>
  <snc mrk="konsen.0o.jeso">
    <dif>
      Volonta <ref tip="super" cel="jes.0o">jeso</ref> pri deziro a&ubreve; opinio de iu:
      <ekz>
        li demandis, <klr>[&#8230;]</klr> &ccirc;u &scirc;i volas esti lia edzino, kaj <klr>[&#8230;]</klr> &scirc;i
        donis al la bela re&gcirc;o sian <tld/>on
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        al tiu &ccirc;i propono alsendis sian <tld/>on 142 homoj
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        unuanima <tld/>o &gcirc;uste estas la plej neebla parto de la afero, pro la natura
        indiferenteco de la mondo
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>silento estas <tld/>o</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>.
        <trd lng="de">Schweigen ist <ind>Zustimmung</ind></trd>
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>acquiesence</trd>,
      <trd>approval</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Zustimmung</trd>,
      <trd>Einwilligung</trd>
    </trdgrp>
    <trd lng="id"><baz>benar</baz>pembenaran</trd>
    <trd lng="nl">instemming</trd>
    <trdgrp lng="zh">
    <trd>&#x9ED8;&#x8A31; <pr>m&#xF2;x&#x1D4;</pr></trd>,
    <trd>&#20801;&#x51C6; <pr>y&#x1D4;nzh&#x1D4;n</pr></trd>,
    <trd>&#x8D0A;&#25104; <pr>z&#xE0;nch&#xE9;ng</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="konsen.0o.akcepto">
    <dif>
      Volonta mal<ref tip="ant" cel="rifuz.0o">rifuzo</ref>:
      <ekz>
        la <tld/>o pri tiu privilegio estas tute provizora;
