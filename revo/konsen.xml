<drv mrk="konsen.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc mrk="konsen.0i.samsenti">
    <gra><vspec>x</vspec></gra>
    <dif>
      Fine <ref tip="super" cel="opini.sam0i">samopinii</ref> pri vereco,
      praveco a&ubreve; ta&ubreve;geco de io; trovi a&ubreve; konstati komunan
      manieron senti a&ubreve; <ref tip="super" cel="pens.0i.opinii">pensi</ref> pri io:
      <ekz>
        pri tio &ccirc;iuj <tld/>is
        <fnt>
          <bib>Fab4</bib>
          <vrk>Anneto</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la propono estis aprobata, la maljunulino anka&ubreve; <tld/>is
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXX</lok>
        </fnt>;
      </ekz>
      <ekz>
        tion, pri kio &ccirc;iuj opinioj <tld/>as, oni devas respekti
        <fnt>
          <bib>Fab3</bib>
          <lok>Koboldo &ccirc;e la butikisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        se vi <tld/>os, ke mi sidu apud vi &ccirc;e la tablo...
        <fnt>
          <bib>ElektFab</bib>
          <vrk>Re&gcirc;o rano a&ubreve; fera Henriko</vrk>
        </fnt>;
      </ekz>
      <ekz>
        ili &ccirc;iuj <tld/>is, ke <klr>[&#8230;]</klr>
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>as kun vi
        <fnt>
          <aut>G. Orwell, trad. D. Broadribb</aut>,
          <vrk><url
          ref="http://i-espero.info/files/elibroj/eo%20-%20orwell,%20george%20-%201984.pdf">
          Mil Na&ubreve;cent Okdek Kvar</url></vrk>,
          <lok>[2001?]</lok>
        </fnt>!
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aprob.0i">aprobi</ref>.
    </refgrp>
    <rim>
      <ref tip="sin" cel="akord.0i.samopinii">Akordi</ref> esprimas naturan kutiman komunecon de
      opinioj;
      <em>konsenti</em> signifas saman komunecon, sed pri speciala
      punkto, kaj akirita nur post hezito a&ubreve; diskutado.
    </rim>
    <trdgrp lng="en">
      <trd>admit (that)</trd>,
      <trd>agree (with)</trd>,
      <trd>approve (of)</trd>,
      <trd>concur (with)</trd>,
      <trd>consent</trd>,
      <trd>be in <ind>accord</ind> (with)</trd>,
      <trd>be in <ind>harmony</ind> with</trd>,
      <trd>share <ind>common opinion</ind> with</trd>
    </trdgrp>
    <trd lng="es">estar de acuerdo</trd>
    <trdgrp lng="de">
      <trd>zustimmen</trd>,
      <trd>der gleichen <ind>Meinung</ind> sein</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd><baz>dapat</baz>sependapat</trd>,
