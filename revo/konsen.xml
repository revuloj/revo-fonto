<drv mrk="konsen.0o">
  <kap><tld/>o</kap>
  <snc mrk="konsen.0o.akordo">
    <dif>
      Akordo inter pluraj personoj a&ubreve; a&jcirc;oj pri io:
      <ekz>
        vera&jcirc;o akceptita de la universala <tld/>o;
      </ekz>
      <ekz>
         tiuj &ccirc;i horlo&gcirc;oj iras tute regule kaj &ccirc;iuj en preciza <tld/>o
         <fnt><bib>FK</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pac.0o">paco</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Zustimmung</trd>,
      <trd>Konsens</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>accord</trd>,
      <trd>approval</trd>,
      <trd>assent</trd>,
      <trd>concurrence</trd>
    </trdgrp>
    <trd lng="nl">eensgezindheid</trd>
  </snc>
  <snc mrk="konsen.0o.jeso">
    <dif>
      Volonta jeso pri deziro a&ubreve; opinio de iu:
      <ekz>
        silento estas <tld/>o<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        kontrakto supozas la <tld/>on de la du partioj;
      </ekz>
      <ekz>
        doni sian <tld/>on al edzi&gcirc;o.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>acquiesence</trd>,
      <trd>approval</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Zustimmung</trd>,
      <trd>Einwilligung</trd>
    </trdgrp>
    <trd lng="nl">instemming</trd>
  </snc>
  <snc mrk="konsen.0o.akcepto">
    <dif>
      Volonta malrifuzo:
      <ekz>
        la <tld/>o pri tiu privilegio estas tute provizora.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Bewilligung</trd>,
      <trd>Gew&auml;hrung</trd>,
      <trd>Genehmigung</trd>
    </trdgrp>
    <trd lng="en">acceptance</trd>
    <trd lng="nl">instemming</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_g;&c_o;&c_d;&c_a;</trd>,
    <trd>&c_u;&c_g;&c_o;&c_d;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dohoda</trd>,
    <trd>konsens</trd>,
    <trd>konsenzus</trd>,
    <trd>p&rcaron;ivolen&iacute;</trd>,
    <trd>souhlas</trd>,
    <trd>svolen&iacute;</trd>
  </trdgrp>
  <trdgrp lng="es">
     <trd>acuerdo</trd>,
     <trd>consentimiento</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>accord</trd>,
    <trd>assentiment</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zgoda</trd>,
    <trd>porozumienie si&eogonek;</trd>,
    <trd>przyzwolenie </trd>,
    <trd>ugoda</trd>,
    <trd>umowa</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>acord</trd>,
    <trd>consim&#x21B;&abreve;m&acirc;nt</trd>,
    <trd>contract</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>dohoda</trd>,
    <trd>konsenzus</trd>,
    <trd>s&uacute;hlas</trd>
  </trdgrp>
</drv>

<drv mrk="konsen.0a">
  <kap><tld/>a</kap>
  <snc mrk="konsen.0a.KOMUNE">
    <dif>
      Karakterizata per <tld/>o:
      <ekz>
        e&ccirc; ses homoj ne havas hodia&ubreve; <tld/>an opinion pri
        tio;
      </ekz>
      <ekz>
        la ne<tld/>aj homaj pre&gcirc;oj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e gesti
        <klr>(montri per gesto, ke oni <tld/>as)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_g;&c_o;&c_d;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>konsensn&iacute;</trd>,
    <trd>souhlasn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>zustimmend</trd>,
    <trd>beipflichtend</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>approbateur</trd>,
    <trd>consentant</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>toestemmend</trd>,
    <trd>instemmend</trd>
  </trdgrp>
  <trd lng="pl">zgodny</trd>
  <trd lng="ro">conform</trd>
  <trd lng="ru">&c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">s&uacute;hlasn&yacute;</trd>
</drv>

<drv mrk="konsen.0ebla">
  <kap><tld/>ebla</kap>
  <snc mrk="konsen.0ebla.KOMUNE">
    <dif>
      Karakterizas ion, kion oni povas facile kaj racie <tld/>i:
      <ekz>
        la konkludo, ke tie agis spirito estas la sola <tld/>ebla
        <fnt>
          <bib>FK</bib>
          <aut>trad. Kabe</aut>
          <vrk>Rakonto pri spiritoj</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_r;&c_y;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
    <trdgrp lng="de">
      <trd>annehmbar</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>admissible</trd>,
      <trd>that one can <ind>agree with</ind></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>v souhlase</trd>,
    <trd>vyhovuj&iacute;c&iacute;</trd>
  </trdgrp>
  <trd lng="fr">admissible</trd>
  <trdgrp lng="nl">
    <trd>toelaatbaar<klr tip="ind"> (waarmee je kan
    instemmen)</klr></trd>,
    <trd>waarmee je kan <ind>instemmen</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>akceptowalny</trd>,
    <trd>bezkonfliktowy</trd>,
    <trd>dopuszczalny</trd>,
    <trd>uk&lstroke;adny</trd>,
    <trd>zgodny</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>acceptabil</trd>,
    <trd>neconflictual</trd>,
    <trd>permisibil</trd>,
    <trd>politico</trd>
  </trdgrp>
  <trd lng="ru">&c_s; &c_k;&c_o;&c_t;&c_o;&c_r;&c_y;&c_m; &c_m;&c_o;&c_zh;&c_n;&c_o; &c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="sk">vyhovuj&uacute;ci</trd>
</drv>

<drv mrk="konsen.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="konsen.0ema.KOMUNE">
    <dif>
      Kiu facile <tld/>as, ne kontra&ubreve;as:
      <ekz>
        ili dezirus, ke vi estu <tld/>ema al ili
        <fnt>
          <aut>trad. N. Vessella</aut>,
          <vrk><url ref="http://www.lernado.it/kurano/kur068.htm">
          La Sankta Kurano</url></vrk>,
          <lok>2006-</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>approbateur<klr> (adj.)</klr></trd>,
    <trd>docile</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pojednawczy</trd>,
    <trd>id&aogonek;cy na ugod&eogonek;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>conciliant</trd>,
    <trd>ajung&acirc;nd la o &icirc;n&#x21B;elegere</trd>
  </trdgrp>
</drv>

<drv mrk="konsen.antaux0i">
  <kap>anta&ubreve;<tld/>i</kap>
  <snc mrk="konsen.antaux0i.KOMUNE">
    <dif>
      Akcepti estontan sintenon, akordi&gcirc;i pri venonta afero:
      <ekz>
        anta&ubreve;<tld/>ita dato
        <fnt>
        <aut>A. L&ouml;wenstein</aut>,
        <vrk>La &scirc;tona urbo</vrk>, 
        <lok>1999</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="plan.0i">plani</ref>
    </refgrp>
  </snc>
  <trd lng="fr">convenir de</trd>
  <trd lng="nl">afspreken</trd>
  <trd lng="pl">uzgadnia&cacute;</trd>     
  <trd lng="ro">conveni</trd>
</drv>

