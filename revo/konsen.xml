<drv mrk="konsen.inter0o">
  <kap>inter<tld/>o</kap>
  <snc mrk="konsen.inter0o.KOMUNE">
    <dif>
      Ago inter<tld/>i, rezulto de tiu ago:
      <ekz>
        &jcirc;ura inter<tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 29:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili faris inter<tld/>on, ke ili &ccirc;iuj kune iru milite kontra&ubreve; Jerusalemon
        <fnt>
          <bib>MT</bib>
          <lok>Ne&hcirc;emja 4:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        post eltrinko ili kun silenta inter<tld/>o replenigis la tasojn duone
        <fnt>
          <bib>Tor</bib>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve; inter<tld/>o ili renkonti&gcirc;is tie por konsili&gcirc;o
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        se &scirc;i post unu semajno ne alvenos <klr>[&#8230;]</klr>, mi liberigos min de la
        inter<tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        artefaritaj signoj, kies signifo estis fiksita surbaze de reciproka inter<tld/>o
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> ne faras tion surbaze de ia eksplicita inter<tld/>o en formo de ia
        kontrakto
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        pli valoras inter<tld/>o ol ju&gcirc;a dokumento
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="kontra3.0o">kontrakto</ref>,
      <ref cel="konven1.0o.gxenerale">konvencio<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_g;&c_o;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_g;&c_o;&c_d;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_a;&c_z;&c_u;&c_m;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
