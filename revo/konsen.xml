      <ekz>
        per skuoj de kapo &ccirc;iam Heleno mal<tld/>is
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        malofte li kura&gcirc;as mal<tld/>i kun lia opinio
        <fnt>
          <bib>QuV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="diskut.0i">diskuti</ref>,
      <ref cel="polemi.0i">polemiki</ref>,
