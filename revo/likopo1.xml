<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: likopo1.xml,v 1.14 2023/10/19 14:32:17 revo Exp $">
<kap>
  <rad>likopodiac</rad>/oj
  <fnt>Z</fnt>
</kap>
<drv mrk="likopo1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref> de
      <ref tip="malprt" cel="likopo.0oplantoj">likopodioplantoj</ref>
      (<trd lng="la">Lycopodiaceae</trd>), kies &ccirc;efa tipo estas
      <ref tip="prt" cel="likopo.0o">likopodio</ref>:
      <ekz>
        nubaj arbaroj trovi&gcirc;as je altitudoj inter 800 kaj 1900 metroj
        <klr>[&#x2026;]</klr>,
        epifitoj, muskoj, filikoj, grundaj orkidacoj kaj likopodiacoj
        abundas
        <fnt>
          <vrk><url ref="https://www.wikiwand.com/eo/Kubaj_humidaj_arbaroj">Kubaj humidaj
          arbaroj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_je;&c_r;&c_a;&c_z;&c_o;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#x2019;&c_ja;)</klr></trd>
  <trd lng="ca">licopodi&agrave;cies</trd>
  <trd lng="de">B&auml;rlappgew&auml;chse</trd>
  <trd lng="hu">korpaf&udblac;f&eacute;l&eacute;k</trd>
  <trd lng="ja">&#x30D2;&#x30AB;&#x30B2;&#x30CE;&#x30AB;&#x30BA;&#x30E9;&#x79D1;  <pr>&#x30D2;&#x30AB;&#x30B2;&#x30CE;&#x30AB;&#x30BA;&#x30E9;&#x304B;</pr></trd>
  <trd lng="pl">wid&lstroke;akowate</trd>
  <trd lng="pt">licopodi&aacute;ceas</trd>
  <trd lng="ro">licopodiacee</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_u;&c_n;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&#x43F;&#x43B;&c_a;&c_u;&#x43D;&#x43E;&c_v;&c_ib;</trd>
</drv>
</art>

