<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>grek</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="grek.0o">
  <kap><tld/>o,
  <var><kap><tld/>ino</kap></var></kap>
  <snc mrk="grek.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref>, &scirc;tatanino de
      <ref tip="malprt" cel="grek.G0ujo.GEOG"><tld lit="G"/>ujo</ref>.
    </dif>
  </snc>
  <snc mrk="grek.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_e;&c_k;</trd>
  <trd lng="ca">grec<klr> (subst.)</klr></trd>
  <trd lng="cs">&Rcaron;ek</trd>
  <trd lng="en">Greek</trd>
   <trdgrp lng="fr">
    <trd>Grec</trd>,
    <trd>Grecque<klr> (subst.)</klr></trd>
   </trdgrp>
  <trd lng="sk">Gr&eacute;k</trd>
</drv>

<drv mrk="grek.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato.
    </dif>
   <trd lng="en">Greek</trd>
   <trdgrp lng="fr">
    <trd>grec</trd>,
    <trd>grecque<klr> (adj.)</klr></trd>
   </trdgrp>
  </snc>
  <snc>
    <dif>
       La <tld/>a lingvo.
    </dif>
  <trd lng="en">Greek</trd>
  <trd lng="fr">grec<klr> (langue)</klr></trd>
  </snc>  
  <trd lng="be">&c_g;&c_r;&c_e;&c_c;&c_k;&c_ib;</trd>
  <trd lng="ca">grec<klr> (adj.)</klr></trd>
  <trd lng="cs">&rcaron;eck&yacute;</trd>
  <trd lng="sk">gr&eacute;cky</trd>
</drv>

<drv mrk="grek.G0ujo">
  <kap>
    <tld lit="G"/>ujo</kap>
  <snc mrk="grek.G0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la suda <ref tip="malprt"
      cel="euxrop1.E0o.geog">E&ubreve;ropo</ref>
      (en Balkana Duoninsulo)
      kun &ccirc;efurbo Ateno;
      teritorio de Respubliko Grekujo;
      &gcirc;i limas kun
      <ref tip="vid" cel="alban.A0ujo">Albanujo</ref>,
      <ref tip="vid" cel="bulgar.B0ujo">Bulgarujo</ref>,
      <ref tip="vid" cel="makedon.M0ujo.GEOG">Makedonujo</ref>,
      <ref tip="vid" cel="turk.T0ujo">Turkujo</ref>
      kaj maroj: Egea, Ionia, Kreta
      kaj <ref tip="vid" cel="mediterane.M0o.GEOG">Mediteranea</ref>
      (ISO-3166 kodo: GR).
    </dif>
    <ref tip="sin" cel="greki.G0o">Grekio</ref>
    <rim>La grekoj mem nomas E-asocion: Helena Esperanto-Asocio, ne 
    Greka Esperanto-Asocio<aut>SC</aut>
    </rim>
    </snc>
    <trd lng="be">&c_G;&c_r;&c_e;&c_c;&c_y;&c_ja;</trd>
    <trd lng="bg">&c_G;&c_malmol;&c_r;&c_c;&c_i;&c_ja;</trd>
    <trdgrp lng="ca">
      <trd>Gr&egrave;cia</trd>,
      <trd>Rep&uacute;blica Hel&middot;l&egrave;nica</trd>
    </trdgrp>
    <trd lng="de">Griechenland</trd>
    <trdgrp lng="el">
      <trd>&Epsilon;&lambda;&lambda;&alfa_ton;&delta;&alfa;</trd>,
      <trd>&Epsilon;&lambda;&lambda;&alfa_ton;&sigma_fina;</trd>,
      <trd>&Epsilon;&lambda;&lambda;&eta;&ny;&jota;&kappa;&eta_ton;
      &Delta;&eta;&my;&omikron;&kappa;&rho;&alfa;&tau;&jota_ton;&alfa;</trd>
    </trdgrp>
    <trd lng="en">Greece</trd>
    <trd lng="fa">&f_y;&a_w;&a_n;&a_A;&a_n;</trd> 
    <trdgrp lng="fr">
      <trd>Gr&egrave;ce</trd>,
      <trd>R&eacute;publique hell&eacute;nique</trd>
    </trdgrp>  
    <trd lng="hu">G&ouml;r&ouml;gorsz&aacute;g</trd>
    <trd lng="id">Yunani</trd>
    <trd lng="nl">Griekenland</trd>
    <trd lng="no">Hellas</trd>
    <trdgrp lng="pl">
      <trd>Grecja</trd>,
      <trd>Republika <ind>Grecka</ind></trd>
    </trdgrp>
    <trd lng="pt">Gr&eacute;cia</trd>
</drv>

</art>

