<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: grek.xml,v 1.30 2023/11/11 11:36:36 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>grek</rad>/o <fnt><bib>MT</bib></fnt>
  <fnt><bib>FK</bib></fnt>
</kap>

<drv mrk="grek.0o">
  <kap><tld/>o</kap>
  <snc mrk="grek.0o.naciano">
    <dif>
      Homo apartenanta al la &ccirc;efa etno de la historia a&ubreve; moderna
      <tld lit="G"/>ujo:
      <ekz>
        la filoj de la <tld lit="G"/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Joel 3:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        inter tiuj, kiuj alvoja&gcirc;is por adorklini&gcirc;i &ccirc;e la festo, estis
        iuj <tld lit="G"/>oj
        <fnt>
          <bib>NT</bib>
          <lok>Johano 12:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili amba&ubreve; eniris en la sinagogon de la Judoj, kaj tiel parolis,
        ke granda amaso da Judoj kaj <tld lit="G"/>oj kredis 
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 14:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio scii&gcirc;is al &ccirc;iuj, kiuj lo&gcirc;is en Efeso, Judoj kaj <tld lit="G"/>oj
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 19:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekkoni per apartaj trajtoj la landon de la antikvaj <tld/>oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>oj, kvankam &ccirc;irka&ubreve;&scirc;utataj per liaj favoroj, tamen
        insultas lin
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        ri&ccirc;a <tld/>ino, vidvino 57-jara
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2008/009415.php">Varmo
          kaj malvarmo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="helen.0o">heleno</ref>
    <trd lng="ru">&c_g;&c_r;&c_je;&c_k;</trd>
  </snc>
  <snc mrk="grek.0o.sxtatano">
    <dif>
      <tld lit="G"/>ujano,
      <ref tip="super" cel="sxtat.0ano">&scirc;tatano</ref> de
      <ref tip="malprt" cel="grek.G0ujo.GEOG"><tld lit="G"/>ujo</ref>:
      <ekz>
        kelkaj e&ubreve;ropaj politikistoj kaj &scirc;tatoj volas da&ubreve;rigi la nunan
        sistemon, la&ubreve; kiu teorie &ccirc;iu oficiala EU-lingvo estu uzebla 
        kaj la kostoj de interpretado estu kovrataj el la EU-bu&gcirc;eto,
        <klr>[&#x2026;]</klr>
        estas fakte <tld/>oj kaj belgoj, kiuj pledas por la &gcirc;isnuna
        sistemo &ndash; &ccirc;u pro presti&gcirc;aj kialoj, &ccirc;u pro lingva egaleco
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2003/008051.html">Bruselo-Babelo:
          lingvo-kulturo en E&ubreve;ropa Unio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la anta&ubreve; kvar jaroj sukcesintaj <tld/>oj, kiuj post tri
        malvenkoj en la grupa fazo devis forlasi la konkurson
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2008/009595.php">Hispanio
          venkis, gastigantoj seniluziigis</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_g;&c_r;&c_a;&c_zh;&c_d;&c_a;&c_n;&c_i;&c_n; &c_G;&c_r;&c_je;&c_c;&c_i;&c_i;</trd>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_e;&c_k;</trd>
  <trd lng="ca">grec<klr> (subst.)</klr></trd>
  <trd lng="cs">&Rcaron;ek</trd>
  <trd lng="de">Grieche</trd>
  <trd lng="en">Greek</trd>
  <trdgrp lng="es">
    <trd>griego</trd>,
    <trd>griega<klr> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Grec</trd>,
    <trd>Grecque<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x30AE;&#x30EA;&#x30B7;&#x30A2;&#x4EBA;  <pr>&#x30AE;&#x30EA;&#x30B7;&#x30E3;&#x3058;&#x3093;</pr></trd>
  <trd lng="nl">Griek</trd>
  <trd lng="sk">Gr&eacute;k</trd>
  <trd lng="uk">&#x433;&#x440;&#x435;&#x43A;</trd>
  <trd lng="zh">&#x5E0C;&#x814A;&#x4EBA; <pr>x&#x12B;l&#xE0;r&#xE9;n</pr></trd>
</drv>

<drv mrk="grek.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato:
      <ekz>
        fari&gcirc;is murmurado de la <tld lit="G"/>aj Judoj kontra&ubreve; la
        Hebreaj
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 6:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi scias la <tld lit="G"/>an lingvon
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 21:37</lok>
        </fnt>?
      </ekz>
      <ekz>
        la <tld/>a regimento
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        vizito de <tld/>a prezidanto
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2006/008833.php">Kaj la
          prezidanto vane atendis</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        formeti &gcirc;is la <tld/>aj kalendoj
        <klr>(kiuj ne ekzistas, do por &ccirc;iam)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  <trdgrp lng="es">
    <trd>griego</trd>,
    <trd>griega<klr> (adj.)</klr></trd>
  </trdgrp>
   <trdgrp lng="fr">
    <trd>grec</trd>,
    <trd>grecque<klr> (adj.)</klr></trd>
   </trdgrp>
  </snc>
  <snc>
    <dif>
      La <tld/>a lingvo:
      <ekz>
        komparado de la <tld/>a kaj la hebrea
        <fnt>
          <bib>Monato</bib>
          <aut>Ji&rcaron;&iacute; Laube</aut>
          <vrk><url ref="&Monato;/2003/007734.html">La rolo de
          lingvoj &ccirc;e la pensado</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="es">griego<klr> (lengua)</klr></trd>
  <trd lng="fr">grec<klr> (langue)</klr></trd>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_e;&c_c;&c_k;&c_ib;</trd>
  <trd lng="ca">grec<klr> (adj.)</klr></trd>
  <trd lng="cs">&rcaron;eck&yacute;</trd>
  <trd lng="de">griechisch</trd>
  <trd lng="en">Greek</trd>
  <trd lng="fy">Gryksk</trd>
  <trd lng="ja">&#x30AE;&#x30EA;&#x30B7;&#x30A2;&#x306E;</trd>
  <trd lng="nl">Grieks</trd>
  <trd lng="sk">gr&eacute;cky</trd>
  <trd lng="uk">&#x433;&#x440;&#x435;&#x446;&#x44C;&#x43A;&#x438;&#x439;</trd>
  <trd lng="zh">&#x5E0C;&#x814A;&#x7684; <pr>x&#x12B;l&#xE0;de</pr></trd>
</drv>

<drv mrk="grek.G0ujo">
  <kap>
    <tld lit="G"/>ujo <fnt><bib>FK</bib></fnt>
  </kap>
  <mlg kod="ISO-3166 ">GR</mlg>
  <snc mrk="grek.G0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&Scirc;tato</ref>
      en la suda <ref tip="malprt" cel="euxrop1.E0o.geog">E&ubreve;ropo</ref>
      (en Balkana Duoninsulo)
      kun &ccirc;efurbo <ref tip="prt" cel="aten.0o">Ateno</ref>;
      teritorio de Respubliko Grekujo,
      limanta kun
      <ref tip="vid" cel="alban.A0ujo">Albanujo</ref>,
      <ref tip="vid" cel="bulgar.B0ujo">Bulgarujo</ref>,
      <ref tip="vid" cel="makedon.NordM0ujo.GEOG">Nord-Makedonujo</ref>,
      <ref tip="vid" cel="turk.T0ujo">Turkujo</ref>
      kaj maroj: Egea, Ionia, Kreta
      kaj <ref tip="vid" cel="mediterane.M0o.GEOG">Mediteranea</ref>:
      <ekz>
        la re&gcirc;o de <tld lit="G"/>ujo
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 8:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        per multaj paroloj kura&gcirc;iginte la tieulojn, li venis en <tld lit="G"/>ujon
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 20:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni parolis pri la mizero de <tld lit="G"/>ujo
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="greki.G0o">Grekio</ref>
    <rim>
      La <tld/>oj mem nomas E-asocion: Helena Esperanto-Asocio, ne 
      <tld lit="G"/>a Esperanto-Asocio
      <aut>SC</aut>
    </rim>
    </snc>
    <trd lng="be">&c_G;&c_r;&c_e;&c_c;&c_y;&c_ja;</trd>
    <trd lng="bg">&c_G;&c_malmol;&c_r;&c_c;&c_i;&c_ja;</trd>
    <trdgrp lng="ca">
      <trd>Gr&egrave;cia</trd>,
      <trd>Rep&uacute;blica Hel&middot;l&egrave;nica</trd>
    </trdgrp>
    <trd lng="de">Griechenland</trd>
    <trdgrp lng="el">
      <trd>&Epsilon;&lambda;&lambda;&alfa_ton;&delta;&alfa;</trd>,
      <trd>&Epsilon;&lambda;&lambda;&alfa_ton;&sigma_fina;</trd>,
      <trd>&Epsilon;&lambda;&lambda;&eta;&ny;&jota;&kappa;&eta_ton;
      &Delta;&eta;&my;&omikron;&kappa;&rho;&alfa;&tau;&jota_ton;&alfa;</trd>
    </trdgrp>
    <trd lng="en">Greece</trd>
    <trdgrp lng="es">
      <trd>Grecia</trd>,
      <trd>Rep&uacute;blica Hel&eacute;nica</trd>
    </trdgrp>  
    <trd lng="fa">&f_y;&a_w;&a_n;&a_A;&a_n;</trd> 
    <trdgrp lng="fr">
      <trd>Gr&egrave;ce</trd>,
      <trd>R&eacute;publique hell&eacute;nique</trd>
    </trdgrp>
    <trd lng="fy">Grikel&acirc;n</trd>
    <trd lng="hu">G&ouml;r&ouml;gorsz&aacute;g</trd>
    <trd lng="id">Yunani</trd>
    <trd lng="nl">Griekenland</trd>
    <trd lng="no">Hellas</trd>
    <trdgrp lng="pl">
      <trd>Grecja</trd>,
      <trd>Republika <ind>Grecka</ind></trd>
    </trdgrp>
    <trd lng="pt">Gr&eacute;cia</trd>
  <trd lng="zh">&#x5E0C;&#x814A; <pr>x&#x12B;l&#xE0;</pr></trd>
</drv>

</art>

