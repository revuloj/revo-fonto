<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>grek</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="grek.0o">
  <kap><tld/>o</kap>
  <snc mrk="grek.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="grek.G0ujo.GEOG"><tld lit="G"/>ujo</ref>.
    </dif>
  </snc>
  <snc mrk="grek.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco.
    </dif>
  </snc>
  <trd lng="en">Greek</trd>
  <trd lng="fr">Grec<klr> (subst.)</klr></trd>
</drv>

<drv mrk="grek.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato.
    </dif>
  </snc>
  <trd lng="en">Greek</trd>
  <trd lng="fr">grec<klr> (adj.)</klr></trd>
</drv>

<drv mrk="grek.G0ujo">
  <kap>
    <tld lit="G"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="grek.G0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la suda <ref tip="malprt"
      cel="euxrop1.E0o.geog">E&ubreve;ropo</ref>
      (en Balkana Duoninsulo)
      kun &ccirc;efurbo Ateno;
      teritorio de Respubliko Grekujo;
      &gcirc;i limas kun
      <ref tip="vid" cel="alban.A0ujo">Albanujo</ref>,
      <ref tip="vid" cel="bulgar.B0ujo">Bulgarujo</ref>,
      <ref tip="vid" cel="makedon.M0ujo.GEOG">Makedonujo</ref>,
      <ref tip="vid" cel="turk.T0ujo">Turkujo</ref>
      kaj maroj: Egea, Ionia, Kreta
      kaj <ref tip="vid" cel="mediterane.M0o.GEOG">Mediteranea</ref>
      (ISO-3166 kodo: GR).
    </dif>
    <rim>La grekoj mem nomas E-asocion: Helena Esperanto-Asocio, ne 
    Greka Esperanto-Asocio<aut>SC</aut></rim>
    <ref tip="sin" cel="greki.G0o">Grekio</ref>
    <trd lng="bg">&c_G;&c_malmol;&c_r;&c_c;&c_i;&c_ja;</trd>
    <trd lng="de">Griechenland</trd>
    <trdgrp lng="el">
      <trd>&Epsilon;&lambda;&lambda;&alfa_acute;&delta;&alfa;</trd>,
      <trd>&Epsilon;&lambda;&lambda;&alfa_acute;&sigma_fina;</trd>,
      <trd>&Epsilon;&lambda;&lambda;&eta;&ny;&jota;&kappa;&eta_ton;
      &Delta;&eta;&my;&omikron;&kappa;&rho;&alfa;&tau;&jota_acute;&alfa;</trd>
    </trdgrp>
    <trd lng="en">Greece</trd>
    <trd lng="fa">&f_y;&a_w;&a_n;&a_A;&a_n;</trd> 
    <trd lng="fr">Gr&egrave;ce</trd>   
    <trd lng="hu">G&ouml;r&ouml;gorsz&aacute;g</trd>
    <trd lng="nl">Griekenland</trd>
    <trd lng="no">Hellas</trd>
    <trdgrp lng="pl">
      <trd>Grecja</trd>,
      <trd>Republika <ind>Grecka</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

