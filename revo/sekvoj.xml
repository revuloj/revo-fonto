<vortaro>
<art mrk="$Id$">
<kap>
  <rad>sekvoj</rad>/o
</kap>
<drv mrk="sekvoj.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="taksodi.0oj">taksodiacoj</ref>
      (<trd lng="la">Sequoia</trd>), kun nur unu specio, giganta arbo el
      Kalifornio, kies alto atingas &gcirc;is 110 metrojn kaj fari&gcirc;as
      tre maljuna. Ankora&ubreve; pli maljuna fari&gcirc;as la
      <ref tip="vid" cel="sekvojadendr.0o">sekvojadendro</ref>.
    </dif>
  </snc>
  <trd lng="de">Mammutbaum</trd>
  <trd lng="nl">sequoia</trd>
  <trd lng="ru">&c_s;&c_je;&c_k;&c_v;&c_o;&c_j;&c_ja;</trd>
</drv>
</art>
