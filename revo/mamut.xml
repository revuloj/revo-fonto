<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mamut.xml,v 1.31 2022/05/01 12:05:12 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>mamut</rad>/o
  <fnt><bib>PrV</bib></fnt>
</kap>
<drv mrk="mamut.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="mamut.0o.PAL">
  <uzo tip="fak">PAL</uzo>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="elefan2.0oj">elefantedoj</ref>
      (<trd lng="la">Mammuthus</trd>), 
      <ref tip="lst" cel="rostr.0ulo" lst="voko:rostruloj">rostrulo</ref>
      &gcirc;is 3&#x202F;m alta kun longhara
      ru&gcirc;bruna felo, malgrandaj oreloj kaj kurbaj dentegoj; 
      vivinta en E&ubreve;ropo kaj norda Azio &gcirc;is la mezo de la 
      <ref tip="malprt" cel="kvater.0o">kvaternara</ref> geologia
      epoko:
      <ekz>
        en Siberio oni trovis likvan sangon de <tld/>o: do, la klonado
        de tiuj bestoj,
        malaperintaj fine de la lasta glacia epoko, alproksimi&gcirc;as
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011214.php">2013: dek donacoj al
          la posteuloj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        fascinaj kiel dinosa&ubreve;roj: estas tiel priskribeblaj la <tld/>oj,
        kiuj vivis &gcirc;is
        anta&ubreve; 3700 jaroj en la nordaj regionoj de E&ubreve;ropo, Azio kaj
        Ameriko
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2015/011414.php">Glaciaj mumioj sor&ccirc;as
          A&ubreve;strion</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/0/0b/Mammuthus_primigenius_fraasi.JPG/640px-Mammuthus_primigenius_fraasi.JPG" prm="CC BY-SA 3.0">ostaro de <tld/>o (inter Mammuthus primigenius kaj Mammuthus
      trogontherii), el
        Steinheim &ccirc;e Murr
        <fnt>
          <aut>Ghedoghedo, propra verko</aut>
          <lok><url ref="&WCW;/File:Mammuthus_primigenius_fraasi.JPG">&WCW;/File:Mammuthus_primigenius_fraasi.JPG</url></lok>
        </fnt>
    </bld>
  </snc>                    
  <trd lng="be">&c_m;&c_a;&c_m;&c_a;&c_n;&c_t;</trd>
  <trd lng="bg">&c_m;&c_a;&c_m;&c_u;&c_t;</trd>
  <trd lng="br">mammout</trd>
  <trd lng="ca">mamut</trd>
  <trd lng="de">Mammut</trd> 
  <trd lng="en">mammoth</trd>
  <trd lng="fa">&a_m;&a_A;&a_m;&a_w;&a_t;</trd>
  <trd lng="fr">mammouth</trd>
  <trd lng="hu">mamut</trd>
  <trd lng="id">mamut</trd>
  <trd lng="nl">mammoet</trd>
  <trd lng="pl">mamut</trd>
  <trd lng="pt">mamute</trd>
  <trd lng="ro">mamut</trd>
  <trd lng="ru">&c_m;&c_a;&c_m;&c_o;&c_n;&c_t;</trd>
  <trd lng="sk">mamut</trd>
  <trd lng="sv">mammut</trd>
  <trd lng="uk">&#x43C;&#x430;&#x43C;&#x43E;&#x43D;&#x442;</trd>
</drv>

<drv mrk="mamut.0a">
  <kap>
    <tld/>a
  </kap>
  <snc mrk="mamut.0a.rilata">
    <dif>
       Rilata al <tld/>o:
      <ekz>
        estas permesite libere vendi la eburon de <tld/>oj, sed ne tiun
        de elefantoj:
        tial elefanta eburo estas ofte vendita kiel <tld/>a
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2015/011414.php">Glaciaj mumioj sor&ccirc;as
          A&ubreve;strion</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="mamut.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="kolos.0a">Kolosa</ref>; malmoderna sed
      okulfrapa:
      <ekz>
        kresko <tld/>a, sed sa&gcirc;o liliputa
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili aspektis <tld/>e, veturis preska&ubreve; sen bruo, nur foje mi
        a&ubreve;dis la klakadon de
        kontaktoj <klr>[&#x2026;]</klr> &#x2010; la paket-liverantaj kamionoj de la
        A&ubreve;stra Po&scirc;to
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2014/011240.php">Elektro
          elektita</url></vrk>
        </fnt>.
      </ekz>
    </dif>
      <trdgrp lng="de">
      <trd>kolossal</trd>,
      <trd>gigantisch</trd>,
      <trd>steinzeitlich</trd>
    </trdgrp>
  </snc>
    <trdgrp lng="be">
      <trd>&c_v;&c_je;&c_l;&c_ib;&c_z;&c_a;&c_r;&c_n;&c_y;</trd>,
      <trd>&c_g;&c_ib;&c_g;&c_a;&c_n;&c_c;&c_k;&c_ib;</trd>,
      <trd>&c_k;&c_a;&c_l;&c_ja;&c_s;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>colossal</trd>,
      <trd>gegant&iacute;</trd>
    </trdgrp>
    <trd lng="en">mammoth</trd>
    <trdgrp lng="fa">
      <trd>&a_gh;&a_w;&a_l;&zwnj;&f_p;&f_y;&f_k;&a_r;</trd>,
      <trd>&a_ayn;&a_Z;&f_y;&a_m;&zwnj;&a_A;&a_l;&a_j;&a_th;&a_h;</trd>
    </trdgrp>
    <trd lng="fr">colossal</trd>
    <trdgrp lng="hu">
      <trd>&oacute;ri&aacute;si</trd>,
      <trd>hatalmas nagy</trd>
    </trdgrp>
    <trd lng="nl">kolossaal</trd>
    <trd lng="pl">mamuci</trd>     
    <trd lng="ro">mamut</trd>
    <trdgrp lng="ru">
      <trd>&c_g;&c_i;&c_g;&c_a;&c_n;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_k;&c_o;&c_l;&c_o;&c_s;&c_s;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
</drv>
</art>
