<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>mamut</rad>/o
</kap>
<drv mrk="mamut.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PAL</uzo>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="elefan.0edoj">elefantedoj</ref>
      (<trd lng="la">Mammuthus</trd>), &gcirc;is 3m alta kun longhara          
      ru&gcirc;bruna felo, malgrandaj oreloj kaj kurbaj dentegoj. &Gcirc;i     
      vivis en E&ubreve;ropo kaj norda Azio &gcirc;is la mezo de la kvaternara 
      geologia epoko.
    </dif>
  </snc>
  <trd lng="de">Mammut</trd>
</drv>
<drv mrk="mamut.0a">
  <kap><tld/>a</kap>
  <snc>
    <uzo tip="klr">(f)</uzo>
    <dif>
      <ref tip="dif" cel="kolos.0a">Kolosa</ref>.
    </dif>
  </snc>
</drv>
</art>
