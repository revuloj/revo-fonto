<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>mamut</rad>/o
</kap>
<drv mrk="mamut.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">PAL</uzo>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="elefan2.0oj">elefantedoj</ref>
      (<trd lng="la">Mammuthus</trd>), &gcirc;is 3m alta kun longhara
      ru&gcirc;bruna felo, malgrandaj oreloj kaj kurbaj dentegoj. &Gcirc;i
      vivis en E&ubreve;ropo kaj norda Azio &gcirc;is la mezo de la kvaternara
      geologia epoko.
    </dif>
  </snc>                    
  <trd lng="be">&c_m;&c_a;&c_m;&c_a;&c_n;&c_t;</trd>
  <trd lng="br">mammout</trd>
  <trd lng="de">Mammut</trd> 
  <trd lng="fr">mammouth</trd>
  <trd lng="hu">mamut</trd>
  <trd lng="nl">mammoet</trd>
  <trd lng="ru">&c_m;&c_a;&c_m;&c_o;&c_n;&c_t;</trd>
  <trd lng="sk">mamut</trd>
  <trd lng="sv">mammut</trd>
</drv>

<drv mrk="mamut.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="kolos.0a">Kolosa</ref>.
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_je;&c_l;&c_ib;&c_z;&c_a;&c_r;&c_n;&c_y;</trd>,
      <trd>&c_g;&c_ib;&c_g;&c_a;&c_n;&c_c;&c_k;&c_ib;</trd>,
      <trd>&c_k;&c_a;&c_l;&c_ja;&c_s;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&oacute;ri&aacute;si</trd>,
      <trd>hatalmas nagy</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_g;&c_i;&c_g;&c_a;&c_n;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_k;&c_o;&c_l;&c_o;&c_s;&c_s;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
</drv>
</art>
