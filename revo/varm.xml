    <drv mrk="varm.mal0umi">
      <kap><ofc>*</ofc>mal<tld/>umi</kap>
      <gra><vspec>ntr</vspec></gra>
      <uzo tip="fak">MED</uzo>
      <snc>
        <dif>
          Fari&gcirc;i malsana pro mal<tld/>o; ricevi kataron:
          <ekz>
            en malbona vetero oni povas facile mal<tld/>umi
            <fnt><bib>F</bib>, <lok>&FE; 42</lok></fnt>.</ekz>
        </dif>
      </snc>
      <trd
        lng="be">&c_p;&c_r;&c_a;&c_s;&c_t;&c_u;&c_d;&c_z;&c_ib;&c_c;&c_c;&c_a;</trd>
      <trdgrp lng="br">
        <trd>tapout <ind>anoued</ind></trd>,
        <trd>paka&ntilde; <ind>riv</ind></trd>
      </trdgrp>
      <trd lng="de"><ofc>*</ofc><klr>sich </klr>erk&auml;lten</trd>
      <trd lng="pl"><ofc>*</ofc>catch cold</trd>
      <trdgrp lng="es">
        <trd>costiparse</trd>,
        <trd>resfriarse</trd>,
        <trd>acatarrarse</trd>,
        <trd>enfriarse</trd>
      </trdgrp>
      <trd lng="fr">prendre <ind>froid</ind></trd>
      <trdgrp lng="he">
        <trd>&lamed;&he;&tsadi;&tet;&nun;&fnun;
          \<ind>&tsadi;&nun;&fnun;</ind>\</trd>,
        <trd>&lamed;&he;&tav;&qof;&resh;&resh;
          \<ind>&qof;&vav;&resh;</ind>\<klr tip="amb">
            (&lamed;&het;&lamed;&vav;&tav;)</klr></trd>
      </trdgrp>
      <trd lng="hu">megf&aacute;zik</trd>
      <trd lng="nl">kou vatten</trd>
      <trd lng="pl"><ofc>*</ofc>przezi&eogonek;bi&cacute; si&eogonek;</trd>
      <trd
        lng="ru"><ofc>*</ofc>&c_p;&c_r;&c_o;&c_s;&c_t;&c_u;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
      <trd lng="sv">bli <ind>f&ouml;rkyld</ind></trd>
      <trd lng="tr">&uuml;&scedil;&uuml;tmek</trd>
    </drv>

    <drv mrk="varm.mal0umo">
      <kap><ofc>*</ofc>mal<tld/>umo</kap>
