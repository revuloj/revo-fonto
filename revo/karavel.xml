<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>karavel</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="karavel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SHI</uzo>
  <snc>
    <dif>
      Negranda e&ubreve;ropa <ref tip="super" cel="sxip.vel0o">vel&scirc;ipo</ref>
      uzata por transmara komercado en la 15a kaj 16a jarcentoj.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>caravelle</trd>,
    <trd>caravel</trd>
  </trdgrp>
 <trdgrp lng="fr">
    <trd>caravelle</trd>
  </trdgrp>
  <trd lng="hu">karavella</trd>
  <trd lng="nl">karveel</trd>
</drv>
</art>

