<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>dekalitr</rad>/o
</kap>
<drv mrk="dekali.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Komunuza <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de
      kapacito,
      egala al dek <ref tip="prt" cel="litr.0o">litroj</ref>; simb. dal.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_k;&c_a;&c_l;&c_ib;&c_t;&c_r;</trd>
  <trd lng="de">Dekaliter</trd>
  <trdgrp lng="en">
    <trd>decaliter</trd>,
    <trd>decalitre</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;calitre</trd>
  <trdgrp lng="hu">
    <trd>t&iacute;z <ind>liter</ind></trd>,
    <trd>dekaliter</trd>
  </trdgrp>
  <trd lng="nl">dekaliter</trd>
  <trd lng="pl">dekalitr</trd>
  <trd lng="pt">decalitro</trd>
  <trd lng="ru">&c_d;&c_je;&c_k;&c_a;&c_l;&c_i;&c_t;&c_r;</trd>
  <trd lng="sk">dekaliter</trd>
</drv>
</art>
