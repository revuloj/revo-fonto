<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: etanol.xml,v 1.6 2023/11/07 20:34:12 revo Exp $">
    <kap>
      <rad>etanol</rad>/o
    </kap>

<drv mrk="etanol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc mrk="etanol.0o.KEM">
    <dif>
      <frm>C<sub>2</sub>H<sub>5</sub>OH</frm>, <ref tip="super" cel="alkoho.0o.KEM">alkoholo</ref>,
      kies molekulo enhavas du atomojn de karbono kaj enhavata en ebriigaj trinka&jcirc;oj kiel vino,
      brando ktp:
      <ekz>
        la komisiono de E&ubreve;ropa Unio preskribis jam anta&ubreve; kelkaj jaroj aldoni en benzinon
        kvin procentojn da <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2011/010226.php">Ekologio paradokse</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        la efika parto de la naturaj ebriiga&jcirc;oj estas kemie <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19024">Alkoholo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="alkoho.0o.KUI">alkoholo<sncref/></ref>
    <trd lng="be">&c_e;&c_t;&c_a;&c_n;&c_o;&c_l;</trd>
    <trd lng="de">Ethanol</trd>
    <trd lng="en">ethanol</trd>
    <trd lng="fr">&eacute;thanol</trd>
    <trd lng="zh">&#x4E59;&#x9187; <pr>y&#x1D0;ch&#xFA;n</pr></trd>
</snc>
  <trd lng="ja">&#x30A8;&#x30BF;&#x30CE;&#x30FC;&#x30EB;</trd>
  <trdgrp lng="uk">
    <trd>&#x435;&#x442;&#x430;&#x43D;&#x43E;&#x43B;</trd>,
    <trd>&#x435;&#x442;&#x438;&#x43B;&#x43E;&#x432;&#x438;&#x439; &#x441;&#x43F;&#x438;&#x440;&#x442;</trd>
  </trdgrp>
</drv>

  </art>

