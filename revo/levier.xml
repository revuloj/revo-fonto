<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: levier.xml,v 1.12 2024/04/15 13:24:36 revo Exp $">
<kap>
  <rad>levier</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="levier.0o">
  <kap><tld/>o</kap>
  <snc mrk="levier.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Komandostango kun baza akso, kiun oni pu&scirc;as supren, malsupren a&ubreve;
      flanken, uzebla kiel <ref tip="super" cel="reg.0ilo.TEK">regilo</ref> de aparato:
      <ekz>
        dum minuto a&ubreve;dis li longajn zumojn, poste premis la <tld/>on
        kaj
        telefonis al Jamburg
        <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 16a, p. 100a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lev.0ilo">levilo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_g;&c_a;&c_r;</trd>,
    <trd>&c_r;&c_y;&c_ch;&c_a;&c_g;</trd>
  </trdgrp>
  <trd lng="cs">p&aacute;ka</trd>
  <trdgrp lng="de">
    <trd>Steuerkn&uuml;ppel</trd>,
    <trd>Joystick</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>manette</trd>,
    <trd>levier</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>knuppel<klr tip="ind"> (stuurstang)</klr></trd>,
    <trd>stuurstang</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>d&zacute;wignia</trd>,
    <trd>lewarek</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>manete</trd>,
    <trd>joystick <klr>(EN)</klr></trd>
  </trdgrp>
  <trd lng="ro">p&acirc;rghie</trd>
  <trd lng="sk">p&aacute;ka</trd>
  <trd lng="uk">&c_v;&c_a;&c_zh;&c_ib;&#x43B;&#x44C;</trd>
  <trdgrp lng="zh">
    <trd>&#x63A7;&#21046;&#26438; <pr>k&#xF2;ngzh&#xEC;g&amacron;n</pr></trd>,
    <trd>&#x6E38;&#x620F;&#x64CD;&#x7EB5;&#26438; <pr>y&#xF3;ux&#xEC;c&amacron;oz&#xF2;ngg&#x1CE;n</pr></trd>,
    <trd>&#x904A;&#25138;&#x64CD;&#x7E31;&#x687F; <pr>y&#xF3;ux&#xEC;c&amacron;oz&#xF2;ngg&#x1CE;n</pr></trd>
  </trdgrp>
</drv>
</art>

