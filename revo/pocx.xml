<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pocx.xml,v 1.13 2024/02/17 07:52:34 revo Exp $">
<kap>
  <rad>po&ccirc;</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="pocx.0i">
  <kap><tld/>i</kap>
  <snc mrk="pocx.0i.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="kuir.0i.KUI">Kuiri</ref> en tre milde
      boletanta akvo a&ubreve; likva&jcirc;o:
      <ekz>
        <tld/>itaj ovoj sur telero kun <klr>[s]</klr>pinaco
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
          <lok>&Ccirc;e Lev Tolstoj en Jasnaja Poljana</lok>
        </fnt>;
      </ekz>
      <ekz>
        estos <tld/>itaj ovoj, fre&scirc;ega pano kaj pelvo da rizo
        <fnt>
          <aut>Mahbube Mirkadiri, trad. Nazi Solat, trad. Tomasz
          Chmielik</aut>,
          <vrk>Donaco el voja&gcirc;o</vrk>,
          <bib>LOdE</bib>,
          <lok>2003:3 (101)</lok>
        </fnt>;
      </ekz>
      <ekz>
        pli ol vian <tld/>on mi preferas fritadon de la maco, kiun mi poste ka&ccirc;igos
        <fnt>
          <bib>KrB</bib>
          <lok>Bervala&jcirc;o</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_r;&c_y;&c_c;&c_mol; &c_n;&c_a; &c_p;&c_a;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_m; &c_a;&c_g;&c_n;&c_ib;</trd>
  <trd lng="de">pochieren</trd>
  <trd lng="en">poach</trd>
  <trd lng="fr">pocher</trd>
  <trdgrp lng="hu">
    <trd>buggyant</trd>,
    <trd>pos&iacute;roz</trd>
  </trdgrp>
  <trd lng="es">pochar</trd>
  <trd lng="ja">&#x843D;&#x3068;&#x3057;&#x5375;&#x306B;&#x3059;&#x308B; 
  <pr>&#x304A;&#x3068;&#x3057;&#x305F;&#x307E;&#x3054;&#x306B;&#x3059;&#x308B;</pr></trd>
  <trd lng="nl">pocheren</trd>
  <trd lng="pl">gotowa&cacute;<klr> jajka w koszulkach</klr></trd>
  <trdgrp lng="pt">
    <trd>escalfar</trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_a;&c_r;&c_i;&c_t;&c_i; &c_ja;&c_j;&c_c;&c_ja; &c_b;&c_je;&c_z; &c_sh;&c_k;&c_a;&c_r;&c_a;&c_l;&c_u;&c_p;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x5077;&#x730E; <pr>t&#x14D;uli&#xE8;</pr></trd>,
    <trd>&#x5077;&#x7375; <pr>t&#x14D;uli&#xE8;</pr></trd>
  </trdgrp>
</drv>
</art>

