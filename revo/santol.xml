<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>santolin</rad>/o
</kap>
<drv mrk="santol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="aster.0acoj">asteracoj</ref>
      (<trd lng="la">Santolina</trd>), spicodoranta arbedo kun grize
      lanecaj folioj kaj rompi&gcirc;emaj bran&ccirc;oj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Heiligenblume</trd>,
    <trd>Zypressenkraut</trd>
  </trdgrp>
</drv>
</art>

