<art mrk="$Id: herald.xml,v 1.23 2023/11/15 16:45:18 revo Exp $">
<kap><rad>heraldik</rad>/o <fnt>Z</fnt></kap>
<drv mrk="herald.0o">
  <kap><tld/>o</kap>
  <snc mrk="herald.0o.SCI">
    <uzo tip="fak">HER</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="lst" lst="voko:sciencoj" cel="scienc.0o.fako">Scienco</ref>
      pri <ref cel="blazon.0o">blazonoj</ref>:
      <ekz>
        en <tld/>o la vorto &leftquot;blazono&#x201D; signifas specifajn figurojn a&ubreve; simbolajn
        pentra&jcirc;ojn, emblemojn, per kiuj oni distingas &scirc;tatojn, urbojn, nobelojn
        <fnt>
          <bib>Monato</bib>
          <aut>Dmitrij Cibulevskij</aut>
          <vrk><url ref="&Monato;/2008/009450.php">Blazonoj de &Hcirc;arkivo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_r;&c_a;&c_l;&c_mol;&c_d;&c_y;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>erbovnictv&iacute;</trd>,
    <trd>heraldika</trd>,
    <trd>znakoznalstv&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Heraldik</trd>,
    <trd>Wappenkunde</trd>
  </trdgrp>
  <trd lng="el">&omikron;&jota;&kappa;&omikron;&sigma;&eta;&my;&omikron;&lambda;&omikron;&gamma;&jota_ton;&alfa;</trd>
  <trd lng="es">her&aacute;ldica</trd>
  <trd lng="fr">h&eacute;raldique</trd>
  <trdgrp lng="hu">
    <trd>c&iacute;mertan</trd>,
    <trd>heraldika</trd>
  </trdgrp>
  <trd lng="it">araldica</trd>
  <trd lng="ja">&#x7D0B;&#x7AE0;&#x5B66;  <pr>&#x3082;&#x3093;&#x3057;&#x3087;&#x3046;&#x304C;&#x304F;</pr></trd>
  <trdgrp lng="nl">
    <trd>heraldiek</trd>,
    <trd>wapenkunde</trd>
  </trdgrp>
  <trd lng="pl">heraldyka</trd>
  <trd lng="pt">her&aacute;ldica</trd>
  <trd lng="ru">&c_g;&c_je;&c_r;&c_a;&c_l;&c_mol;&c_d;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">n&aacute;uka o erboch</trd>
  <trd lng="uk">&#x433;&#x435;&#x440;&#x430;&#x43B;&#x44C;&#x434;&#x438;&#x43A;&#x430;</trd>
  <trd lng="zh">&#x7D0B;&#x7AE0;&#x5B78; <pr>w&#xE9;nzh&#x101;ngxu&#xE9;</pr></trd>
</drv>
</art>
