<vortaro>
<art mrk="$Id$">
<kap><rad>heraldik</rad>/o <fnt>Z</fnt></kap>
<drv mrk="herald.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SCI</uzo>
  <snc>
    <dif>
      Scienco pri <ref cel="blazon.0o">blazono</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Heraldik</trd>,
    <trd>Wappenkunde</trd>
  </trdgrp>
  <trd lng="el">&omikron;&jota;&kappa;&omikron;&sigma;&eta;&my;&omikron;&lambda;&omikron;&gamma;&jota_ton;&alfa;</trd>
  <trd lng="es">her&aacute;ldica</trd>
  <trdgrp lng="fr">
    <trd>h&eacute;raldique</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>heraldiek</trd>,
    <trd>wapenkunde</trd>
  </trdgrp>
  <trd lng="pl">heraldyka</trd>
  <trd lng="pt">her&aacute;ldica</trd>
  <trd lng="ru">&c_g;&c_je;&c_r;&c_a;&c_l;&c_mol;&c_d;&c_i;&c_k;&c_a;</trd>
</drv>
</art>
