    <trd>empiricist</trd>,
    <trd>objectivist</trd>
  </trdgrp>
  <trd lng="fr">empiriste</trd>
  <trd lng="he">&alef;&mem;&pe;&yod;&resh;&yod;&qof;&fnun;</trd>
  <trdgrp lng="hu">
    <trd>empirista</trd>,
    <trd>empirikus filoz&oacute;fus</trd>
  </trdgrp>
  <trd lng="ja">&#x7D4C;&#x9A13;&#x8AD6;&#x8005;  <pr>&#x3051;&#x3044;&#x3051;&#x3093;&#x308D;&#x3093;&#x3057;&#x3083;</pr></trd>
  <trd lng="pl">empirysta</trd>
  <trd lng="ru">&c_e;&c_m;&c_p;&c_i;&c_r;&c_i;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>empirista</trd>,
    <trd>praktik</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_je;&c_m;&c_p;&c_ib;&c_r;&c_i;&c_k;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_k;&c_t;&c_i;&c_k;</trd>
  </trdgrp>
</drv>

