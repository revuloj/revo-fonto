<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: empiri.xml,v 1.25 2023/11/07 20:34:10 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>empiri</rad>/o
</kap>
<drv mrk="empiri.0o">
  <kap><tld/>o</kap>
  <snc mrk="empiri.0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Sperto konsiderata kiel bazo de filozofio kaj scienco:
      <ekz>
        kvankam <klr>[&#x2026;tio]</klr> ne estis bone komprenata, estis <tld/>e sciate ke
        diversaj cerbopartoj havas proprajn funkciojn
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Kvin</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>empirical evidence</trd>,
      <trd>empirical knowledge</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>emp&iacute;ria</trd>,
      <trd>tapasztalati tud&aacute;s</trd>
    </trdgrp>
    <trd lng="pl">empiria</trd>
  </snc>
  <snc mrk="empiri.0o.praktika">
    <dif>
      Nescienca rutina metodo bazita sur nura praktika nesufi&ccirc;e
      kontrolita sperto.
    </dif>
    <refgrp tip="vid">
      <ref cel="heuxristik.0o">he&ubreve;ristiko</ref>
    </refgrp>
    <trdgrp lng="hu">
      <trd>j&aacute;rtass&aacute;g</trd>,
      <trd>tapasztalati tud&aacute;s</trd>
    </trdgrp>
    <trd lng="pl">do&sacute;wiadczenie</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_m;&c_p;&c_ib;&c_r;&c_y;&c_ja;</trd>,
    <trd>&c_d;&c_o;&c_s;&c_mol;&c_v;&c_je;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>empirie</trd>,
    <trd>zku&scaron;enost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Empirie</trd>,
    <trd>Erfahrungswissen</trd>,
    <trd>Erfahrung</trd>
  </trdgrp>
  <trd lng="fr">empirique<klr> (subst.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x7D4C;&#x9A13;&#x7684;&#x65B9;&#x6CD5;  <pr>&#x3051;&#x3044;&#x3051;&#x3093;&#x3066;&#x304D;&#x307B;&#x3046;&#x307B;&#x3046;</pr></trd>,
    <trd>&#x7D4C;&#x9A13;  <pr>&#x3051;&#x3044;&#x3051;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="pt">empiria</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_m;&c_p;&c_i;&c_r;&c_i;&c_ja;</trd>,
    <trd>&c_o;&c_p;&c_y;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">sk&uacute;senos&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_je;&c_m;&c_p;&c_ib;&c_r;&c_ib;&c_ja;</trd>,
    <trd>&c_d;&c_o;&c_s;&c_v;&c_ib;&c_d;</trd>
  </trdgrp>
</drv>

<drv mrk="empiri.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Bazita sur metoda scienca sperto:
      <ekz>
        kritika raciismo estas dirmaniero for&gcirc;ita de la a&ubreve;stria filozofo <nom>Karl Popper</nom> kaj
        indikas la konvinkon la&ubreve; kiu la racio, rilate <tld/>ajn sciojn, ne povas havi
        pure rigoran demonstracian funkcion, sed nure taskon kritikan
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17842">Karl Popper</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="eksper">posteksperimenta</ref>,
      <ref cel="aposte.0a">aposteriora</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="spekul2.0a">spekulativa</ref>,
      <ref cel="teori.0o">teorio</ref>.
    </refgrp>
    <trd lng="hu">empirikus</trd>
    <trd lng="pl">do&sacute;wiadczalny</trd>
  </snc>
  <snc>
    <dif>
      Bazita sur senscienca rutina sperto:
      <ekz>
        <tld/>aj scioj, kiujn oni gajnis ekkonante la ecojn de la konstrumaterialoj,
        determinis la konstrumetodojn ek de la greka antikvo &gcirc;is la itala renesanco
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=24159">Konstruarto</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        surbaze de <tld/>aj esploroj, bazitaj sur la kvantoj de radono subtere, li estis
        certa pri la tujeco de terura tertremo
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2013/010777.php">&Gcirc;is la tero tremas</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>empirikus</trd>,
      <trd>tapasztalati</trd>
    </trdgrp>
    <trd lng="pl">empiryczny</trd>
  </snc>
  <trd lng="be">&c_e;&c_m;&c_p;&c_ib;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>masti&ccaron;k&aacute;&rcaron;sk&yacute;</trd>,
    <trd>&scaron;arlat&aacute;nsk&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>empirisch</trd>,
    <trd>auf Erfahrung beruhend</trd>,
    <trd>aus Erfahrung</trd>,
    <trd>Erfahrungs-</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>empirical</trd>,
    <trd>objective</trd>
  </trdgrp>
  <trd lng="fr">empirique<klr> (adj.)</klr></trd>
  <trd lng="he">&alef;&#x5B6;&mem;&pe;&#x5BC;&#x5B4;&yod;&resh;&#x5B4;&yod;</trd>
  <trdgrp lng="ja">
    <trd>&#x7D4C;&#x9A13;&#x306B;&#x57FA;&#x3065;&#x304F;  <pr>&#x3051;&#x3044;&#x3051;&#x3093;&#x306B;&#x3082;&#x3068;&#x3065;&#x304F;</pr></trd>,
    <trd>&#x7D4C;&#x9A13;&#x4E3B;&#x7FA9;&#x306E;  <pr>&#x3051;&#x3044;&#x3051;&#x3093;&#x3057;&#x3085;&#x304E;&#x306E;</pr></trd>,
    <trd>&#x7D4C;&#x9A13;&#x7684;&#x306A;  <pr>&#x3051;&#x3044;&#x3051;&#x3093;&#x3066;&#x304D;&#x306A;</pr></trd>
  </trdgrp>
  <trd lng="pt">emp&iacute;rico</trd>
  <trd lng="ru">&c_e;&c_m;&c_p;&c_i;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>masti&ccaron;k&aacute;rsky</trd>,
    <trd>&scaron;arlat&aacute;nsky</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_je;&c_m;&c_p;&c_ib;&c_r;&c_i;&c_ch;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_n;&c_o;&c_v;&c_a;&c_n;&c_i;&c_j; &c_n;&c_a; &c_d;&c_o;&c_s;&c_v;&c_ib;&c_d;</trd>
  </trdgrp>
  <trd lng="zh">&#x6839;&#x636E;&#x7ECF;&#x9A8C; <pr>g&#x113;nj&#xF9;j&#x12B;ngy&#xE0;n</pr></trd>
</drv>

<drv mrk="empiri.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      Filozofia <ref tip="super" cel="doktri.0o">doktrino</ref>, la&ubreve; kiu &ccirc;iuj ideoj devenas de la
      sperto
    </dif>
    <refgrp tip="vid">
      <ref cel="raci.0ismo">raciismo</ref>
    </refgrp>
    <trd lng="hu">empirizmus</trd>
    <trd lng="pl">empiryzm</trd>
    <trd lng="pt">empirismo</trd>
  </snc>
  <snc>
    <ref tip="dif" cel="empiri.0o.praktika"><tld/>o<sncref/></ref>
    <trdgrp lng="hu">
      <trd>j&aacute;rtass&aacute;g</trd>,
      <trd>tapasztalati tud&aacute;s</trd>
    </trdgrp>
    <trd lng="pl">do&sacute;wiadczenie</trd>
  </snc>
  <trd lng="be">&c_e;&c_m;&c_ib;&c_r;&c_y;&c_z;&c_m;</trd>
  <trd lng="cs">empirizmus</trd>
  <trdgrp lng="de">
    <trd>Empirismus</trd>,
    <trd>Erfahrungswissenschaft</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>empiricism</trd>,
    <trd>objectivism</trd>
  </trdgrp>
  <trd lng="fr">empirisme</trd>
  <trd lng="he">&alef;&#x5B6;&mem;&pe;&#x5BC;&#x5B4;&yod;&resh;&#x5B4;&yod;&tsadi;&#x5B4;&yod;&zayin;&#x5B0;&fmem;</trd>
  <trdgrp lng="ja">
    <trd>&#x7D4C;&#x9A13;&#x4E3B;&#x7FA9;  <pr>&#x3051;&#x3044;&#x3051;&#x3093;&#x3057;&#x3085;&#x304E;</pr></trd>,
    <trd>&#x7D4C;&#x9A13;&#x8AD6;  <pr>&#x3051;&#x3044;&#x3051;&#x3093;&#x308D;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_m;&c_p;&c_i;&c_r;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">empirizmus</trd>
  <trd lng="uk">&c_je;&c_m;&c_p;&c_ib;&c_r;&c_i;&c_z;&c_m;</trd>
  <trd lng="zh">&#x7D93;&#x9A57;&#x4E3B;&#x7FA9; <pr>j&#x12B;ngy&#xE0;nzh&#x1D4;y&#xEC;</pr></trd>
</drv>


