<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>4</ofc>
<rad>empiri</rad>/o
</kap>
<drv mrk="empiri.0o">
<kap><tld/>o</kap>
<snc num="1">
<uzo tip="fak">FIL</uzo>
<dif>
Sperto konsiderata kiel bazo de filozofio k scienco. 
</dif>
  <trdgrp lng="en">
     <trd>empiry</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>emp&iacute;ria</trd>,
     <trd>tapasztalati tud&aacute;s</trd>
  </trdgrp>
</snc>
<snc num="2">
<dif>
Nescienca rutina metodo bazita sur nura praktika nesufi&ccirc;e kontrolita sperto. 
</dif>
<trdgrp lng="hu">
  <trd>j&aacute;rtass&aacute;g</trd>,
  <trd>tapasztalati tud&aacute;s</trd>
</trdgrp>
</snc>
  <trdgrp lng="de">
     <trd>Empirie</trd>,
     <trd>Erfahrungswissen</trd>,
     <trd>Erfahrung</trd>
  </trdgrp>
<trdgrp lng="ru">
  <trd>&c_e;&c_m;&c_p;&c_i;&c_r;&c_i;&c_ja;</trd>,
  <trd>&c_o;&c_p;&c_y;&c_t;</trd>
</trdgrp>
</drv>
<drv mrk="empiri.0a">
<kap><tld/>a</kap>
<snc num="1">
<dif>
Bazita sur metoda scienca sperto. 
</dif>
<refgrp tip="sin">
<ref cel="eksper">posteksperimenta</ref>,
<ref cel="aposte.0a">aposteriora</ref>.
</refgrp>
<refgrp tip="vid">
<ref cel="spekul2.0a">spekulativa</ref>,
<ref cel="teori.0o">teorio</ref>.
</refgrp>
<trd lng="hu">empirikus</trd>
</snc>
<snc num="2">
<dif>
Bazita sur senscienca rutina sperto. 
</dif>
<trdgrp lng="hu">
  <trd>empirikus</trd>,
  <trd>tapasztalati</trd>
</trdgrp>
</snc>
  <trdgrp lng="de">
     <trd>empirisch</trd>,
     <trd>auf Erfahrung beruhend</trd>,
     <trd>aus Erfahrung</trd>,
     <trd>Erfahrungs-</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>empirical</trd>,
     <trd>objective</trd>
  </trdgrp>
<trd lng="ru">&c_e;&c_m;&c_p;&c_i;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
<drv mrk="empiri.0ismo">
<kap><tld/>ismo</kap>
<snc num="1">
<dif>
Filozofia doktrino, la&ubreve; kiu &ccirc;iuj ideoj devenas de la sperto 
</dif>
<refgrp tip="vid">
<ref cel="raci.0ismo">raciismo</ref>.
</refgrp>
<trd lng="hu">empirizmus</trd>
</snc>
<snc num="2">
<dif>
Empirio 2. 
</dif>
<trdgrp lng="hu">
  <trd>j&aacute;rtass&aacute;g</trd>,
  <trd>tapasztalati tud&aacute;s</trd>
</trdgrp>
</snc>
  <trdgrp lng="de">
     <trd>Empirismus</trd>,
     <trd>Erfahrungswissenschaft</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>empiricism</trd>,
     <trd>objectivism</trd>
  </trdgrp>
<trd lng="ru">&c_e;&c_m;&c_p;&c_i;&c_r;&c_i;&c_z;&c_m;</trd>
</drv>
<drv mrk="empiri.0acxa">
<kap><tld/>a&ccirc;a</kap>
<snc>
<dif>
Empiria 2. 
</dif>
</snc>
</drv>
<drv mrk="empiri.0isto">
<kap><tld/>isto</kap>
<snc>
<dif>
Sekvanto de <tld/>ismo. 
</dif>
</snc>
  <trdgrp lng="de">
     <trd>Empiriker</trd>,
     <trd>Erfahrungswissenschaftler</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>empiricist</trd>,
     <trd>objectivist</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>empirista</trd>,
     <trd>empirikus filoz&oacute;fus</trd>
  </trdgrp>
</drv>
<drv mrk="empiri.0ulo">
<kap><tld/>ulo</kap>
<snc>
<dif>
Homo fidanta la rutinan k krudan sperton.  
</dif>
</snc>
  <trdgrp lng="de">
     <trd>Praktiker</trd>,
     <trd>Erfahrungsmensch</trd>
  </trdgrp>
<trdgrp lng="en">
  <trd>empirikus<klr tip="ind"> (ember)</klr></trd>,
  <trd>gyakorlatias ember</trd>
</trdgrp>
<trdgrp lng="ru">
  <trd>&c_e;&c_m;&c_p;&c_i;&c_r;&c_i;&c_k;</trd>,
  <trd>&c_p;&c_r;&c_a;&c_k;&c_t;&c_i;&c_k;</trd>
</trdgrp>
</drv>
</art>
