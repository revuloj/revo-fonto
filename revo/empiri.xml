  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Ano de <tld/>ismo.
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_m;&c_p;&c_ib;&c_r;&c_y;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>empirista</trd>,
    <trd>hlasatel empirizmu</trd>,
    <trd>praktik</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Empiriker</trd>,
    <trd>Erfahrungswissenschaftler</trd>
  </trdgrp>
