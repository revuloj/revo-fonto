<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: id1.xml,v 1.19 2023/11/19 10:22:02 revo Exp $">
<kap>
  <rad>Id</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="id1.0o">
  <kap><tld/>o</kap>
  <snc mrk="id1.0o.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="lst" cel="lingv.0o.homa" lst="voko:lingvoj">Lingvo</ref>
      internacia kreita de la francoj Louis de Beaufront 
      (1855 &ndash; 1935) kaj Louis Couturat (1868 &ndash; 1914) 
      en 1907 kiel reformo de
      <ref tip="vid" cel="espera.0o">Esperanto</ref>:
      <ekz>
        Tolkien aliloke skribis &ccirc;i tiujn vortojn: &leftquot;Volapuko,
        Esperanto,
        <tld/>o, Novial, ktp, estas mortintaj<klr>...</klr>&rightquot;
        <fnt>
        <aut>Garbhan MacAoidh</aut>,
        <vrk>Lingvo, mito kaj legendo</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2002a, numero 9a, p. 17a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="delega.D0o">delegacio</ref>
  </snc>
  <ref tip="hom" cel="id.0o">ido (filo)</ref>
  <trd lng="be">&c_ib;&c_d;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>Ido - Linguo Internaciona di la Delegitaro</trd>,
    <trd>jazyk Ido</trd>
  </trdgrp>
  <trd lng="de">Ido</trd>
  <trd lng="en">Ido</trd>
  <trd lng="fr">Ido</trd>
  <trd lng="fy">Ido</trd>
  <trd lng="hu">ido</trd>
  <trd lng="id">Ido</trd>
  <trd lng="io">Ido</trd>
  <trd lng="ja">&#x30A4;&#x30C9;&#x8A9E;  <pr>&#x30A4;&#x30C9;&#x3054;</pr></trd>
  <trd lng="nl">Ido</trd>
  <trd lng="pl">ido</trd>
  <trd lng="pt">ido<klr tip="amb"> (l&iacute;ngua derivada do
  esperanto)</klr></trd>
  <trd lng="ru">&c_i;&c_d;&c_o;</trd>
  <trd lng="sk">Ido</trd>
  <trd lng="zh">&#x4F0A;&#x591A;&#x8A9E; <pr>y&#x12B;du&#x14D;y&#x1D4;</pr></trd>
</drv>

<drv mrk="id1.0isto">
  <kap><tld lit="i"/>isto</kap>
  <snc mrk="id1.0isto.LIN">
    <dif>
      Persono, kiu scias kaj uzas <tld/>on:
      <ekz>
        la <tld lit="i"/>istoj nun okazigas sian internacian kongreson
        <fnt>
        <aut>Klivo</aut>,
        <vrk>Idistoj kongresumas</vrk>,
        <lok>soc.culture.esperanto, 2001-06-18</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_d;&c_y;&c_s;&c_t;</trd>
  <trd lng="cs">idista</trd>
  <trd lng="de">Idist</trd>
  <trd lng="en">Idist</trd>
  <trd lng="fr">idiste<klr> (subst.)</klr></trd>
  <trd lng="fy">idist</trd>
  <trd lng="hu">idista</trd>
  <trd lng="io">Idisto</trd>
  <trd lng="nl">idist</trd>
  <trdgrp lng="pl">
    <trd>idista</trd>,
    <trd>idysta</trd>
  </trdgrp>
  <trd lng="pt">idista</trd>
  <trd lng="ru">&c_i;&c_d;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">idista</trd>
</drv>
</art>
