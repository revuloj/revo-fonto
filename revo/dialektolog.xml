<kap>
  <rad>dialektolog</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="dialektolog.0o">
  <kap><tld/>o</kap>
  <snc mrk="dialektolog.0o.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      Specialisto pri <ref tip="vid" cel="dialektologi.0o.LIN">dialektologio</ref>:
      <ekz>
        kiam <tld/>oj registras a&ubreve;tentikajn lingvekzemplerojn,
        ili &ccirc;iam indikas la nomon de la parolinto, tempon kaj lokon
        <fnt>
          <aut>M. Rauhamaa</aut>,
          <vrk><url ref="http://groups.yahoo.com/group/esperanto-l/message/15892">
          There is NO Esperanto Dialect</url></vrk>,
          <lok>sce, 2001-12-04</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_l;&c_je;&c_k;&c_t;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="bg">&c_d;&c_i;&c_a;&c_l;&c_je;&c_k;&c_t;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="cs">dialektolog</trd>
  <trd lng="de">Dialektologe</trd>
  <trd lng="en">dialectologist</trd>
  <trd lng="fr">dialectologue</trd>
  <trd lng="ja">&#x65B9;&#x8A00;&#x5B66;&#x8005;  <pr>&#x307B;&#x3046;&#x3052;&#x3093;&#x304C;&#x304F;&#x3057;&#x3083;</pr></trd>
  <trd lng="nl">dialectoloog</trd>
  <trd lng="pl">dialektolog</trd>
  <trd lng="ru">&c_d;&c_i;&c_a;&c_l;&c_je;&c_k;&c_t;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="sk">znalec dialektov</trd>
  <trd lng="uk">&c_d;&c_ib;&c_a;&#x43B;&c_je;&#x43A;&c_t;&#x43E;&#x43B;&#x43E;&c_g;</trd>
