<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pneuxmokok.xml,v 1.3 2022/04/06 21:28:20 revo Exp $">
<kap>
  <rad>pne&ubreve;mokok</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="pneuxmokok.0o">
  <kap><tld/>o</kap>
  <snc mrk="pneuxmokok.0o.BAK">
    <uzo tip="fak">BAK</uzo>
    <dif>
      <ref tip="super" cel="streptokok.0o.BAK">Streptokoko</ref>
      &ccirc;eesta en la pulmoj, kiu povas ka&ubreve;zi
      <ref tip="vid" cel="pneuxm2.0o">pne&ubreve;monion</ref>
      (<trd lng="la">Streptococcus pneumoniae</trd>):
      <ekz>
        <tld/>oj de tipo 1 estas sendan&gcirc;eraj <klr>...</klr>
        <tld/>oj de tipo 2 estas &ccirc;iam mortigaj
        <fnt>
          <aut>S. Norvell</aut>,
          <vrk>Profeto sen honoro</vrk>,
          <bib>Monato</bib>
          <lok>2003:01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">pneumococcus</trd>
  <trd lng="fr">pneumocoque</trd>
  <trdgrp lng="pl">
    <trd>dwoinka zapalenia p&lstroke;uc</trd>,
    <trd>pneumokok</trd>
  </trdgrp>
  <trd lng="ro">pneumococ</trd>
  <trd lng="uk">&#x43F;&#x43D;&c_je;&c_v;&#x43C;&#x43E;&#x43A;&#x43E;&#x43A;</trd>
</drv>
</art>

