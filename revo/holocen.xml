<art mrk="$Id: holocen.xml,v 1.9 2022/03/20 17:24:55 revo Exp $">
<kap>
  <rad>holocen</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="holocen.0o">
  <kap><tld/>o</kap>
  <snc mrk="holocen.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Lasta, &gcirc;isnuna geologia <ref tip="lst" cel="epok.0o.periodo" lst="voko:geologiaj_epokoj" val="-11430">epoko</ref> iom pli ol
      10000-jara,
      post la 
      glaciepokoj:
      <ekz>
        la pli juna kvaternara subperiodo, da&ubreve;ranta &gcirc;is nuntempe, estas 
        <tld/>o
        <fnt>
          <aut>J. Kavka</aut>,
          <vrk><url ref="http://www.freeweb.hu/eventoj/steb/miksitaj/sovagxsar/sovagxsar.htm">
          La protektata naturparko Divok&aacute; &Scaron;&aacute;rka</url></vrk>,
          <lok>1996</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_l;&c_ja;&c_c;&c_e;&c_n;</trd>
  <trd lng="ca">holoc&eacute;</trd>
  <trdgrp lng="cs">
    <trd>aluvium</trd>,
    <trd>holoc&eacute;n</trd>
  </trdgrp>
  <trd lng="fr">holoc&egrave;ne</trd>
  <trd lng="it">Olocene</trd>
  <trd lng="pt">Holoceno</trd>
  <trdgrp lng="sk">
    <trd>al&uacute;vium</trd>,
    <trd>holoc&eacute;n</trd>
  </trdgrp>
  <trd lng="uk">&#x433;&#x43E;&#x43B;&#x43E;&#x446;&#x435;&#x43D;</trd>
</drv>
</art>

