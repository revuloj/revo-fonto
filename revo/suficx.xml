<drv mrk="suficx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="suficx.0i.KOMUNE">
    <dif>
      Esti <tld/>a:
      <ekz>
        unu atestanto ne <tld/>as, por kondamni homon al morto
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 35:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        por pagi tian &scirc;uldon <tld/>os malgranda parto da miaj enspezoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        plue miaj fortoj ne <tld/>is
        <fnt>
          <bib>Fab3</bib>
          <lok>Io</lok>
        </fnt>;
      </ekz>
      <ekz>
        duono de tio, kion &scirc;i rakontas, estas mensogo, sed la dua duono jam <tld/>as
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
      <ekz>
        li decidis ke la atendo <tld/>as kaj ke li nun foriros
        <fnt>
          <bib>ChB</bib>
          <lok>9</lok>
        </fnt>;
      </ekz>
      <ekz>
        por sa&gcirc;ulo <tld/>as aludo
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_d;&c_a;&c_s;&c_t;&c_a;&c_t;&c_k;&c_o;&c_v;&c_y;&c_m;</ind></trd>,
    <trd>&c_h;&c_a;&c_p;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">bastar</trd>
  <trd lng="cs">sta&ccaron;it</trd>
  <trdgrp lng="de">
    <trd>ausreichen</trd>,
    <trd>gen&uuml;gen</trd>,
    <trd>langen</trd>
  </trdgrp>
  <trd lng="en">suffice</trd>
  <trd lng="es">ser <ind>suficiente</ind></trd>
  <trd lng="fr">suffire</trd>
  <trd lng="he">&lamed;<ind>&he;&samekh;&pe;&yod;&qof;</ind></trd>
  <trdgrp lng="hu">
    <trd>elegend&odblac;</trd>,
    <trd>el&eacute;gs&eacute;ges</trd>
  </trdgrp>
  <trd lng="id">cukup</trd>
  <trdgrp lng="ja">
    <trd>&#x5341;&#x5206;&#x3067;&#x3042;&#x308B;  <pr>&#x3058;&#x3085;&#x3046;&#x3076;&#x3093;&#x3067;&#x3042;&#x308B;</pr></trd>,
    <trd>&#x8DB3;&#x308A;&#x3066;&#x3044;&#x308B;  <pr>&#x305F;&#x308A;&#x3066;&#x3044;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>voldoende zijn</trd>,
    <trd>genoeg zijn</trd>
  </trdgrp>
  <trd lng="pl">wystarcza&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_v;&c_a;&c_t;&c_a;&c_t;&c_mol;<klr tip="ind">(&c_b;&c_y;&c_t;&c_mol; &c_d;&c_o;&c_s;&c_t;&c_a;&c_t;&c_o;&c_ch;&c_n;&c_y;&c_m;)</klr></trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol; &c_d;&c_o;&c_s;&c_t;&c_a;&c_t;&c_o;&c_ch;&c_n;&c_y;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_t;&c_i; &c_d;&c_o;&c_s;&c_t;&c_a;&c_t;&c_n;&c_ib;&c_m;</trd>,
    <trd>&c_v;&c_i;&c_s;&c_t;&c_a;&c_ch;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x8DB3; <pr>z&uacute;</pr></trd>
</drv>

<drv mrk="suficx.0ega">
  <kap><tld/>ega</kap>
  <snc mrk="suficx.0ega.KOMUNE">
    <dif>
      <ref cel="abund.0a">Abunda</ref>, pli ol <tld/>a:
      <ekz>
        mi ricevis <tld/>egan dankon, dol&ccirc;an kison kaj &ccirc;irka&ubreve;brakon
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi hejtis <tld/>ege, vidu mem
        <fnt>
          <bib>ElektFab</bib>
          <lok>Sesope oni trairas la tutan mondon</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_o;&c_l;&c_mol;&c_sh; &c_ch;&c_y;&c_m; <ind>&c_d;&c_a;&c_s;&c_t;&c_a;&c_t;&c_k;&c_o;&c_v;&c_y;</ind></trd>
  <trd lng="ca"><ind>m&eacute;s que</ind> suficient</trd>
  <trdgrp lng="de">
    <trd>mehr als <ind>genug</ind></trd>,
