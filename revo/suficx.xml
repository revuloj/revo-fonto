<drv mrk="suficx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="suficx.0i.KOMUNE">
    <dif>
      Esti <tld/>a:
      <ekz>
        tio <tld/>os por ni;
      </ekz>
      <ekz>
        tiu sumo ne <tld/>as;
      </ekz>
      <ekz>
        <tld/>as, ke vi promesis.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_d;&c_a;&c_s;&c_t;&c_a;&c_t;&c_k;&c_o;&c_v;&c_y;&c_m;</ind></trd>,
    <trd>&c_h;&c_a;&c_p;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">bastar</trd>
  <trd lng="cs">sta&ccaron;it</trd>
  <trdgrp lng="de">
    <trd>ausreichen</trd>,
    <trd>gen&uuml;gen</trd>,
    <trd>langen</trd>
  </trdgrp>
  <trd lng="en">suffice</trd>
  <trd lng="es">ser <ind>suficiente</ind></trd>
  <trd lng="fr">suffire</trd>
  <trd lng="he">&lamed;<ind>&he;&samekh;&pe;&yod;&qof;</ind></trd>
  <trdgrp lng="hu">
    <trd>elegend&odblac;</trd>,
    <trd>el&eacute;gs&eacute;ges</trd>
  </trdgrp>
  <trd lng="id">cukup</trd>
  <trdgrp lng="ja">
    <trd>&#21313;&#20998;&#12391;&#12354;&#x308B;  <pr>&#12376;&#12421;&#12358;&#12406;&#12435;&#12391;&#12354;&#x308B;</pr></trd>,
    <trd>&#x8DB3;&#x308A;&#12390;&#12356;&#x308B;  <pr>&#x305F;&#x308A;&#12390;&#12356;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>voldoende zijn</trd>,
    <trd>genoeg zijn</trd>
  </trdgrp>
  <trd lng="pl">wystarcza&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_v;&c_a;&c_t;&c_a;&c_t;&c_mol;<klr tip="ind">(&c_b;&c_y;&c_t;&c_mol; &c_d;&c_o;&c_s;&c_t;&c_a;&c_t;&c_o;&c_ch;&c_n;&c_y;&c_m;)</klr></trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol; &c_d;&c_o;&c_s;&c_t;&c_a;&c_t;&c_o;&c_ch;&c_n;&c_y;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_t;&c_i; &c_d;&#x43E;&c_s;&c_t;&c_a;&c_t;&#x43D;&c_ib;&#x43C;</trd>,
    <trd>&c_v;&c_i;&c_s;&c_t;&c_a;&c_ch;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

