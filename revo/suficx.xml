<drv mrk="suficx.mem0o">
  <kap>mem<tld/>o</kap>
  <snc mrk="suficx.mem0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Kontentigo de siaj bezonoj per propra agado,
      ebliganta
      <ref tip="vid" cel="pend.sende0a.KOMUNE">sendependecon</ref>;
      <ref tip="sin" cel="auxtarci.0o.EKON">a&ubreve;tarcio</ref>:
      <ekz>
        tiu agrikultura aktiveco <klr>...</klr> preska&ubreve; garantias
        mem<tld/>on por &ccirc;irka&ubreve; 50 lo&gcirc;antoj
        <fnt>
          <aut>Esperanto.be</aut>,
          <vrk><url ref="&UEA;vikio/Bona_Espero">
          Bona Espero</url></vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_a;&c_m;&c_a;&c_d;&c_a;&c_s;&c_t;&c_a;&c_t;&c_k;&c_o;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    <trdgrp lng="fr">
      <trd>autarcie</trd>,
      <trd>autosuffisance</trd>
    </trdgrp>
    <trd lng="nl">zelfvoorzienend</trd>
    <trd lng="no">selvforsynt</trd>
    <trd lng="pl">samowystarczalno&sacute;&cacute;</trd>
  </snc>
  <trdgrp lng="id">
    <trd>autarki</trd>,
    <trd>swasembada</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x81EA;&#x7D66;&#x81EA;&#x8DB3;  <pr>&#12376;&#x304D;&#12421;&#12358;&#12376;&#x305D;&#x304F;</pr></trd>,
    <trd>&#x81EA;&#x7ACB;  <pr>&#12376;&#x308A;&#12388;</pr></trd>,
    <trd>&#12358;&#x306C;&#x307C;&#x308C;</trd>,
    <trd>&#x81EA;&#x5DF1;&#x6E80;&#x8DB3;  <pr>&#12376;&#12371;&#x307E;&#12435;&#x305E;&#x304F;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="suficx.ne0e">
  <kap>ne <tld/>e,
  <var><kap>ne<tld/>e</kap></var>,
  <var><kap>mal<tld/>e</kap></var></kap>
  <snc mrk="suficx.ne0e.KOMUNE">
    <dif>
      Tro malmulte, tro malabunde, tro malintense,
      <ref tip="vid" cel="tro.mal0.KOMUNE">maltro</ref>:
      <ekz>
        ne <tld/>e kuirita viando;
      </ekz>
      <ekz>
        ankora&ubreve; ne <tld/>e bone posedi la lingvon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la lanternoj en la tute nelumigita urbo mal<tld/>e
        heligis la vojon
        <fnt><aut>H. Sienkewicz, trad. L. Zamenhof</aut>, <vrk>Quo
        vadis?</vrk>, <lok>1933</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_d;&c_a;&c_s;&c_t;&c_a;&c_t;&c_k;&c_o;&c_v;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>insuficientment</trd>,
    <trd>exiguament</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>insuffisamment</trd>,
    <trd>pas assez</trd>
  </trdgrp>
  <trd lng="ja">&#x4E0D;&#20805;&#20998;&#x306B;  <pr>&#12405;&#12376;&#12421;&#12358;&#12406;&#12435;&#x306B;</pr></trd>
  <trdgrp lng="ms">
    <trd>cukup</trd>,
    <trd>mencukupi</trd>
  </trdgrp>
  <trd lng="nl">onvoldoende</trd>
  <trdgrp lng="pl">
    <trd>niewystarczaj&aogonek;co</trd>,
    <trd>niedostatecznie</trd>,
    <trd>za ma&lstroke;o</trd>,
    <trd>nie do&sacute;&cacute;</trd>
  </trdgrp>
</drv>
