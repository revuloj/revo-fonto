<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>*</ofc>
    <rad>sufi&ccirc;</rad>/a
</kap>
<drv mrk="suficx.0a">
    <kap><ofc>*</ofc><tld/>a</kap>
    <snc>
      <dif>
        Estanta en tia kvanto, kia estas bezona:
        <ekz>
          unu fajrero estas <tld/>a por eksplodigi pulvon<fnt>Z</fnt>.
        </ekz>
      </dif>
    </snc>
    <trd lng="be">&c_d;&c_a;&c_s;&c_t;&c_a;&c_t;&c_k;&c_o;&c_v;&c_y;</trd>
    <trdgrp lng="de">
      <trd>ausreichend</trd>,
      <trd>gen&uuml;gend</trd>,
      <trd>hinreichend</trd>,
      <trd>hinl&auml;nglich</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>enough</trd>,
      <trd>sufficient</trd>,
      <trd>adequate</trd>
    </trdgrp>
    <trd lng="es">suficiente</trd>
    <trd lng="fr">suffisant</trd>
    <trd lng="he">&mem;&samekh;&pe;&yod;&qof;</trd>
    <trdgrp lng="hu">
      <trd>el&eacute;g</trd>,
      <trd>el&eacute;gs&eacute;ges</trd>,
      <trd>elegend&odblac;</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>genoeg</trd>,
      <trd>voldoende</trd>
    </trdgrp>
  <trdgrp lng="pt">
   <trd>suficiente</trd>
  </trdgrp>
    <trd
lng="ru">&c_d;&c_o;&c_s;&c_t;&c_a;&c_t;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="suficx.0e">
    <kap><tld/>e</kap>
    <snc>
      <dif>
        Tiom, kiom estas bezone:
        <ekz>
          estas <tld/>e da lakto por vin nutri<fnt>Z</fnt>;
        </ekz>
        <ekz>
          mi pensas, ke estos <tld/>e dek metroj<fnt>K</fnt>;
        </ekz>
        <ekz>
          ni havas ne<tld/>e energian policestron<fnt>Z</fnt>;
        </ekz>
        <ekz>
          <tld/>e pri tio<fnt>Z</fnt>;
        </ekz>
        <ekz>
          estos <tld/>e, se ni diros la jenon<fnt>Z</fnt>;
        </ekz>
        <ekz>
          vi havas <tld/>e da tempo por;
        </ekz>
        <ekz>
          ne <tld/>e kuirita viando;
        </ekz>
        <ekz>
          ankora&ubreve; ne <tld/>e bone posedi la lingvon<fnt>Z</fnt>.
        </ekz>
      </dif>
      <refgrp tip="sin">
        <ref cel="sat">sate</ref>.
      </refgrp>
      <rim>
        Ne uzu <em>sufi&ccirc;e</em> kun la senco de <em>iom, ete, 
meze</em>.
      </rim>
    </snc>
    <trdgrp lng="be">
      <trd>&c_d;&c_a;&c_s;&c_t;&c_a;&c_t;&c_k;&c_o;&c_v;&c_a;</trd>,
      <trd>&c_d;&c_a;&c_v;&c_o;&c_l;&c_ib;</trd>,
      <trd>&c_d;&c_o;&c_s;&c_y;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="cs">dost</trd>
    <trdgrp lng="de">
      <trd>genug</trd>,
      <trd>gen&uuml;gend</trd>,
      <trd>zur <ind>Gen&uuml;ge</ind></trd>,
      <trd>ausreichend</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>enough</trd>,
      <trd>sufficiently</trd>
    </trdgrp>
    <trd lng="es">suficientemente</trd>
    <trdgrp lng="fr">
      <trd>suffisamment</trd>,
      <trd>assez</trd>
    </trdgrp>
    <trd lng="he">&mem;&samekh;&pe;&yod;&qof;</trd>
    <trdgrp lng="nl">
      <trd>voldoende</trd>,
      <trd>genoeg</trd>
    </trdgrp>
  <trdgrp lng="hu">
    <trd>el&eacute;g</trd>,
    <trd>eleget</trd>,
    <trd>el&eacute;gg&eacute;</trd>
  </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_o;&c_s;&c_t;&c_a;&c_t;&c_o;&c_ch;&c_n;&c_o;</trd>,
      <trd>&c_d;&c_o;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_o;</trd>,
      <trd>&c_h;&c_v;&c_a;&c_t;&c_i;&c_t;</trd>
    </trdgrp>
</drv>

<drv mrk="suficx.0i">
    <kap><tld/>i</kap>
    <gra><vspec>ntr</vspec></gra>
    <snc>
      <dif>
        Esti <tld/>a:
        <ekz>
          tio <tld/>os por ni;
        </ekz>
        <ekz>
          tiu sumo ne <tld/>as;
        </ekz>
        <ekz>
          <tld/>as, ke vi promesis.
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="be">
      <trd>&c_b;&c_y;&c_c;&c_mol;
        &c_d;&c_a;&c_s;&c_t;&c_a;&c_t;&c_k;&c_o;&c_v;&c_y;&c_m;</trd>,
      <trd>&c_h;&c_a;&c_p;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="cs">sta&ccaron;it</trd>
    <trdgrp lng="de">
      <trd>ausreichen</trd>,
      <trd>gen&uuml;gen</trd>,
      <trd>langen</trd>
    </trdgrp>
    <trd lng="en">suffice</trd>
    <trd lng="es">ser <ind>suficiente</ind></trd>
    <trd lng="fr">suffire</trd>
    <trd lng="he">&lamed;<ind>&he;&samekh;&pe;&yod;&qof;</ind></trd>
    <trdgrp lng="hu">
      <trd>elegend&odblac;</trd>,
      <trd>el&eacute;gs&eacute;ges</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>voldoende zijn</trd>,
      <trd>genoeg zijn</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_h;&c_v;&c_a;&c_t;&c_a;&c_t;&c_mol;<klr tip="ind">
(&c_b;&c_y;&c_t;&c_mol;
&c_d;&c_o;&c_s;&c_t;&c_a;&c_t;&c_o;&c_ch;&c_n;&c_y;&c_m;)</klr></trd>,
      <trd>&c_b;&c_y;&c_t;&c_mol;
&c_d;&c_o;&c_s;&c_t;&c_a;&c_t;&c_o;&c_ch;&c_n;&c_y;&c_m;</trd>
    </trdgrp>
</drv>

<drv mrk="suficx.0ega">
    <kap><tld/>ega</kap>
    <snc>
      <dif>
        Abunda, pli ol <tld/>a.
      </dif>
    </snc>
    <trd lng="en">ample</trd>
    <trd lng="fr">grandement <ind>suffisant</ind></trd>
    <trdgrp lng="hu">
      <trd>b&odblac;ven elegend&odblac;</trd>,
      <trd>kiad&oacute;s</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>rijkelijk</trd>,
      <trd>overvloedig</trd>
    </trdgrp>
</drv>
</art>
