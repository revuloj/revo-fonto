<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sufi&ccirc;</rad>/a
</kap>
<drv mrk="suficx.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Estanta en tia kvanto, kia estas bezona:
      <ekz>
        unu fajrero estas <tld/>a por eksplodigi pulvon<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>ausreichend</trd>,
    <trd>gen&uuml;gend</trd>,
    <trd>hinreichend</trd>,
    <trd>hinl&auml;nglich</trd>
  </trdgrp>
  <trd lng="en">enough</trd>
  <trdgrp lng="nl">
    <trd>genoeg</trd>,
    <trd>voldoende</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_o;&c_s;&c_t;&c_a;&c_t;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="suficx.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Tiom, kiom estas bezone:
      <ekz>
        estas <tld/>e da lakto por vin nutri<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi pensas, ke estos <tld/>e dek metroj<fnt>K</fnt>;
      </ekz>
      <ekz>
        ni havas ne<tld/>e energian policestron<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e pri tio<fnt>Z</fnt>;
      </ekz>
      <ekz>
        estos <tld/>e, se ni diros la jenon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        vi havas <tld/>e da tempo por;
      </ekz>
      <ekz>
        ne <tld/>e kuirita viando;
      </ekz>
      <ekz>
        ankora&ubreve; ne <tld/>e bone posedi la lingvon<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="sat">sate</ref>.
    </refgrp>
    <rim>
      Ne uzu <em>sufi&ccirc;e</em> kun la senco de <em>iom, ete, meze</em>.
    </rim>
  </snc>
  <trd lng="cs">dost</trd>
  <trdgrp lng="de">
    <trd>genug</trd>,
    <trd>gen&uuml;gend</trd>,
    <trd>zur <ind>Gen&uuml;ge</ind></trd>,
    <trd>ausreichend</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>voldoende</trd>,
    <trd>genoeg</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_s;&c_t;&c_a;&c_t;&c_o;&c_ch;&c_n;&c_o;</trd>,
    <trd>&c_d;&c_o;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_o;</trd>,
    <trd>&c_h;&c_v;&c_a;&c_t;&c_i;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="suficx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>a:
      <ekz>
        tio <tld/>os por ni;
      </ekz>
      <ekz>
        tiu sumo ne <tld/>as;
      </ekz>
      <ekz>
        <tld/>as, ke vi promesis.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">sta&ccaron;it</trd>
  <trdgrp lng="de">
    <trd>ausreichen</trd>,
    <trd>gen&uuml;gen</trd>,
    <trd>langen</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>voldoende zijn</trd>,
    <trd>genoeg zijn</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_h;&c_v;&c_a;&c_t;&c_a;&c_t;&c_mol;<klr tip="ind"> (&c_b;&c_y;&c_t;&c_mol; &c_d;&c_o;&c_s;&c_t;&c_a;&c_t;&c_o;&c_ch;&c_n;&c_y;&c_m;)</klr></trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol; &c_d;&c_o;&c_s;&c_t;&c_a;&c_t;&c_o;&c_ch;&c_n;&c_y;&c_m;</trd>
  </trdgrp>
</drv>

<drv mrk="suficx.0ega">
  <kap><tld/>ega</kap>
  <snc>
    <dif>
      Abunda, pli ol <tld/>a.
    </dif>
  </snc>
  <trdgrp lng="nl">
    <trd>rijkelijk</trd>,
    <trd>overvloedig</trd>
  </trdgrp>
</drv>
</art>
