</drv>

<drv mrk="suficx.0ega">
  <kap><tld/>ega</kap>
  <snc mrk="suficx.0ega.KOMUNE">
    <dif>
      <ref cel="abund.0a">Abunda</ref>, pli ol <tld/>a:
      <ekz>
        mi ricevis <tld/>egan dankon, dol&ccirc;an kison kaj &ccirc;irka&ubreve;brakon
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi hejtis <tld/>ege, vidu mem
        <fnt>
          <bib>ElektFab</bib>
          <lok>Sesope oni trairas la tutan mondon</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_o;&c_l;&c_mol;&c_sh; &c_ch;&c_y;&c_m; <ind>&c_d;&c_a;&c_s;&c_t;&c_a;&c_t;&c_k;&c_o;&c_v;&c_y;</ind></trd>
  <trd lng="ca"><ind>m&eacute;s que</ind> suficient</trd>
  <trdgrp lng="de">
    <trd>mehr als <ind>genug</ind></trd>,
    <trd>reichlich</trd>
  </trdgrp>
  <trd lng="en">ample</trd>
  <trd lng="fr">grandement <ind>suffisant</ind></trd>
  <trdgrp lng="hu">
