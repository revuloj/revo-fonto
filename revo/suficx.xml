    <trd>kiad&oacute;s</trd>
  </trdgrp>
  <trd lng="id">lebih dari <ind>cukup</ind></trd>
  <trdgrp lng="ja">
    <trd>&#12383;&#12387;&#12407;&#12426;&#12398;</trd>,
    <trd>&#21313;&#20998;&#12377;&#12366;&#12427;&#12411;&#12393;&#12398;
