<drv mrk="suficx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="suficx.0i.KOMUNE">
    <dif>
      Esti <tld/>a:
      <ekz>
        unu atestanto ne <tld/>as, por kondamni homon al morto
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 35:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        por pagi tian &scirc;uldon <tld/>os malgranda parto da miaj enspezoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        plue miaj fortoj ne <tld/>is
        <fnt>
          <bib>Fab3</bib>
          <lok>Io</lok>
        </fnt>;
      </ekz>
      <ekz>
        duono de tio, kion &scirc;i rakontas, estas mensogo, sed la dua duono jam <tld/>as
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
      <ekz>
        li decidis ke la atendo <tld/>as kaj ke li nun foriros
        <fnt>
          <bib>ChB</bib>
          <lok>9</lok>
        </fnt>;
      </ekz>
      <ekz>
        por sa&gcirc;ulo <tld/>as aludo
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_d;&c_a;&c_s;&c_t;&c_a;&c_t;&c_k;&c_o;&c_v;&c_y;&c_m;</ind></trd>,
    <trd>&c_h;&c_a;&c_p;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">bastar</trd>
  <trd lng="cs">sta&ccaron;it</trd>
  <trdgrp lng="de">
    <trd>ausreichen</trd>,
    <trd>gen&uuml;gen</trd>,
    <trd>langen</trd>
  </trdgrp>
  <trd lng="en">suffice</trd>
  <trd lng="es">ser <ind>suficiente</ind></trd>
  <trd lng="fr">suffire</trd>
  <trd lng="he">&lamed;<ind>&he;&samekh;&pe;&yod;&qof;</ind></trd>
  <trdgrp lng="hu">
    <trd>elegend&odblac;</trd>,
    <trd>el&eacute;gs&eacute;ges</trd>
  </trdgrp>
  <trd lng="id">cukup</trd>
  <trdgrp lng="ja">
    <trd>&#21313;&#20998;&#12391;&#12354;&#12427;  <pr>&#12376;&#12421;&#12358;&#12406;&#12435;&#12391;&#12354;&#12427;</pr></trd>,
    <trd>&#36275;&#12426;&#12390;&#12356;&#12427;  <pr>&#12383;&#12426;&#12390;&#12356;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>voldoende zijn</trd>,
    <trd>genoeg zijn</trd>
  </trdgrp>
  <trd lng="pl">wystarcza&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_v;&c_a;&c_t;&c_a;&c_t;&c_mol;<klr tip="ind">(&c_b;&c_y;&c_t;&c_mol; &c_d;&c_o;&c_s;&c_t;&c_a;&c_t;&c_o;&c_ch;&c_n;&c_y;&c_m;)</klr></trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol; &c_d;&c_o;&c_s;&c_t;&c_a;&c_t;&c_o;&c_ch;&c_n;&c_y;&c_m;</trd>
