<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: veld.xml,v 1.27 2021/05/29 14:33:26 revo Exp $">
<kap>
  <rad>veld</rad>/i
</kap>

<drv mrk="veld.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="veld.0i.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Kunigi ekstremojn de du metalaj pecoj per specialaj aparatoj en
      inkandeska fajro.
    </dif>
    <refgrp tip="vid">
      <ref cel="lut.0i">luti</ref>,
      <ref cel="nit.0i">niti</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_v;&c_a;&c_r;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_z;&c_a;&c_v;&c_a;&c_r;&c_ja;&c_v;&c_a;&c_m;</trd>
  <trd lng="bo">&#xF5A;&#xF0B;&#xF63;&#xF0B;&#xF62;&#xF92;&#xFB1;&#xF42;&#xF0B;</trd>
  <trd lng="ca">soldar</trd>
  <trd lng="cs">sv&aacute;&rcaron;et</trd>
  <trd lng="de">schwei&szlig;en</trd>
  <trd lng="el">&sigma;&ypsilon;&gamma;&kappa;&omikron;&lambda;&lambda;&omega_ton;</trd>
  <trd lng="en">weld</trd>
  <trd lng="es">soldar<klr> (metal)</klr></trd>
  <trd lng="fa">&a_j;&a_w;&a_sh; &a_d;&a_A;&a_d;&a_n;</trd>
  <trd lng="fr">souder</trd>
  <trd lng="hu">hegeszt</trd>
  <trd lng="nl">lassen</trd>
  <trd lng="pl">spawa&cacute;</trd>
  <trd lng="pt">soldar</trd>
  <trd lng="ru">&c_s;&c_v;&c_a;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol;<klr tip="ind"> (&c_t;&c_je;&c_h;.)</klr></trd>
  <trdgrp lng="sk">
    <trd>zv&aacute;ra&tcaron;</trd>,
    <trd>zvari&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">svetsa</trd>
</drv>

<drv mrk="veld.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="veld.0isto.TEK">
    <dif>
      Homo, kiu profesie faras <tld/>on:
      <ekz>
        li klarigis al du veldistoj kelkajn ligojn, apena&ubreve;
        videblajn en
        la desegna&jcirc;kopio
        <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 5a, p.
        26a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_v;&c_a;&c_r;&c_sh;&c_ch;&c_y;&c_k;</trd>
  <trd lng="ca">soldador</trd>
  <trd lng="cs">sv&aacute;&rcaron;e&ccaron;</trd>
  <trd lng="fa">&a_j;&a_w;&a_sh;&f_k;&a_A;&a_r;</trd>
  <trd lng="fr">soudeur</trd>
  <trd lng="hu">hegeszt&odblac;</trd>
  <trd lng="pl">spawacz</trd>
  <trd lng="pt">soldador</trd>
  <trd lng="sk">zv&aacute;ra&ccaron;</trd>
  <trd lng="sv">svetsare</trd>
</drv>

<drv mrk="veld.0o">
  <kap><tld/>o</kap>
  <snc mrk="veld.0o.TEK">
    <dif>
      <ref tip="malprt" cel="junt.0o.kunigo">Junto</ref> kreita per
      <tld/>ado.
    </dif>
    <trd lng="be">&c_z;&c_v;&c_a;&c_r;&c_n;&c_o;&c_je; &c_sh;&c_v;&c_o;</trd>
    <trdgrp lng="ca">
      <trd>soldatge</trd>,
      <trd>soldadura</trd>
    </trdgrp>
    <trd lng="fa">&a_j;&a_w;&a_sh;<klr> (&f_p;&f_y;&a_w;&a_n;&a_d;&a_komo; &a_A;&a_t;&a_S;&a_A;&a_l;)</klr></trd>
    <trd lng="fr">soudure</trd>
    <trd lng="pl">spaw</trd>
  </snc>
  <trd lng="cs">svar</trd>
  <trd lng="sk">zvar</trd>
</drv>

</art>
