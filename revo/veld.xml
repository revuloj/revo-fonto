<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: veld.xml,v 1.24 2015/07/04 08:37:45 revo Exp $">
<kap>
  <rad>veld</rad>/i
</kap>

<drv mrk="veld.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="veld.0i.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Kunigi ekstremojn de du metalaj pecoj per specialaj aparatoj en
      inkandeska fajro.
    </dif>
    <refgrp tip="vid">
      <ref cel="lut.0i">luti</ref>,
      <ref cel="nit.0i">niti</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_v;&c_a;&c_r;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_z;&c_a;&c_v;&c_a;&c_r;&c_ja;&c_v;&c_a;&c_m;</trd>
  <trd lng="ca">soldar</trd>
  <trd lng="cs">svářet</trd>
  <trd lng="de">schwei&szlig;en</trd>
  <trd
  lng="el">&sigma;&ypsilon;&gamma;&kappa;&omikron;&lambda;&lambda;&omega_ton;</trd>
  <trd lng="en">weld</trd>
  <trd lng="es">soldar<klr> (metal)</klr></trd>
  <trd lng="fr">souder</trd>
  <trd lng="hu">hegeszt</trd>
  <trd lng="nl">lassen</trd>
  <trd lng="pl">spawa&cacute;</trd>
  <trd lng="pt">soldar</trd>
  <trd lng="ru">&c_s;&c_v;&c_a;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol;<klr
  tip="ind"> (&c_t;&c_je;&c_h;.)</klr></trd>
  <trdgrp lng="sk">
    <trd>zv&aacute;ra&tcaron;</trd>,
    <trd>zvariť</trd>
  </trdgrp>
  <trd lng="sv">svetsa</trd>
</drv>

<drv mrk="veld.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="veld.0isto.TEK">
    <dif>
      Homo, kiu profesie faras <tld/>on:
      <ekz>
        li klarigis al du veldistoj kelkajn ligojn, apena&ubreve;
        videblajn en
        la desegna&jcirc;kopio
        <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 5a, p.
        26a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">soldador</trd>
  <trd lng="cs">svářeč</trd>
  <trd lng="fr">soudeur</trd>
  <trd lng="hu">hegeszt&odblac;</trd>
  <trd lng="pl">spawacz</trd>
  <trd lng="pt">soldador</trd>
  <trd lng="sk">zvárač</trd>
  <trd lng="sv">svetsare</trd>
</drv>

<drv mrk="veld.0o">
  <kap><tld/>o</kap>
  <snc mrk="veld.0o.TEK">
    <dif>
      <ref tip="malprt" cel="junt.0o.kunigo">Junto</ref> kreita per
      <tld/>ado.
    </dif>
    <trdgrp lng="ca">
      <trd>soldatge</trd>,
      <trd>soldadura</trd>
    </trdgrp>
    <trd lng="fr">soudure</trd>
    <trd lng="pl">spaw</trd>
  </snc>
  <trd lng="cs">svar</trd>
  <trd lng="sk">zvar</trd>
</drv>

</art>
