<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>veld</rad>/i
</kap>
<drv mrk="veld.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="veld.0i.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Kunigi ekstremojn de du metalaj pecoj per specialaj aparatoj en
      inkandeska fajro.
    </dif>
    <refgrp tip="vid">
      <ref cel="lut.0i">luti</ref>,
      <ref cel="nit">niti</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_v;&c_a;&c_r;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">schwei&szlig;en</trd>
  <trd lng="el">&sigma;&ypsilon;&gamma;&kappa;&omikron;&lambda;&lambda;&omega_ton;</trd>
  <trd lng="en">weld</trd>
  <trd lng="es">soldar<klr> (metal)</klr></trd>
  <trd lng="fr">braser</trd>
  <trd lng="hu">hegeszt</trd>
  <trd lng="nl">lassen</trd>
  <trd lng="pt">soldar</trd>
  <trd lng="ru">&c_s;&c_v;&c_a;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol;<klr tip="ind"> (&c_t;&c_je;&c_h;.)</klr></trd>
  <trd lng="sk">zv&aacute;ra&tcaron;</trd>
</drv>

<drv mrk="veld.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="veld.0isto.TEK">
    <dif>
      Homo, kiu profesie faras <tld/>on:
      <ekz>
        li klarigis al du veldistoj kelkajn ligojn, apena&ubreve; videblajn en la desegna&jcirc;kopio
        <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 5a, p. 26a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
</drv>
</art>
