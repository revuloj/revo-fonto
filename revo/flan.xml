<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>flan</rad>/o
</kap>
<drv mrk="flan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Iu ajn maldika diskforma baka&jcirc;o el firma pasto.
     <ekz>
       maco<tld/>oj &scirc;miritaj per oleo
       <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
       <lok>Levidoj 2, 3</lok>)</fnt>.
      </ekz>  
    </dif>
    <ref tip="sub" cel="kuk.pat0o">patkuko</ref>
  </snc>
  <trd lng="de">Fladen</trd>
  <trd lng="fr">flan</trd>
</drv>
</art>
