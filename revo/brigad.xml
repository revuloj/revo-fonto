<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>brigad</rad>/o
</kap>
<drv mrk="brigad.0o">
  <kap><tld/>o</kap>
  <snc mrk="brigad.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Parto de armeo enhavanta &gcirc;enerale du infanteriajn regimentojn.
    </dif>
    <trd lng="de">Brigade</trd>
    <trd lng="hu">dand&aacute;r</trd>
  </snc>
  <snc mrk="brigad.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Organizita propracela aro.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_y;&c_g;&c_a;&c_d;&c_a;</trd>
  <trd lng="es">brigada</trd>
  <trd lng="fr">brigade</trd>
  <trd lng="hu">brig&aacute;d</trd>
  <trd lng="ru">&c_b;&c_r;&c_i;&c_g;&c_a;&c_d;&c_a;</trd>
  <trd lng="sk">brig&aacute;da</trd>
</drv>

<drv mrk="brigad.fajro0o">
  <kap>fajro<tld/>o</kap>
  <snc>
    <dif>
      Grupo de savistoj trejnitaj por tuje helpi okaze de brulego, inundo,
      a&ubreve; ajna natur- a&ubreve; hom-ka&ubreve;zita akcidento:
      <ekz>
        la tujaj komentoj la&ubreve;te la&ubreve;dis la lertan agadon de la nederlanda
        fajro<tld/>o kaj helpantaro<fnt><aut>Guido Van Damme</aut>:
        <vrk>Pli bone unu birdo en la mano</vrk>,
        <bib>Monato</bib><lok>jaro 1996a, numero 11a, p. 19a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_zh;&c_a;&c_r;&c_n;&c_a;&c_ja; &c_k;&c_a;&c_m;&c_a;&c_n;&c_d;&c_a;</trd>
  <trd lng="de">Feuerwehr</trd>
  <trd lng="es">cuerpo de bomberos</trd>
  <trdgrp lng="fr">
    <trd>pompiers<klr> (subst.)</klr></trd>,
    <trd>sapeurs-pompiers</trd>
  </trdgrp>
  <trd lng="hu">t&udblac;zolt&oacute;s&aacute;g</trd>
  <trd lng="ru">&c_p;&c_o;&c_zh;&c_a;&c_r;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_m;&c_a;&c_n;&c_d;&c_a;</trd>
</drv>

<drv mrk="brigad.fajro0isto">
  <kap>fajro<tld/>isto</kap>
  <snc>
    <dif>
      Ano de fajro<tld/>o, speciale trejnita precipe por estingi brulegojn kaj
      tuje helpi la viktimojn.
    </dif>
  </snc>
  <trd lng="es">bombero</trd>
</drv>

</art>
