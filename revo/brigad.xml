<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: brigad.xml,v 1.27 2021/03/05 17:02:56 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>brigad</rad>/o
</kap>
<drv mrk="brigad.0o">
  <kap><tld/>o</kap>
  <snc mrk="brigad.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Parto de armeo enhavanta &gcirc;enerale du infanteriajn
      regimentojn.
    </dif>
    <trd lng="de">Brigade</trd>
    <trd lng="hu">dand&aacute;r</trd>
    <trd lng="pl">brygada</trd>
    <trd lng="pt">brigada</trd>
  </snc>
  <snc mrk="brigad.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Organizita propracela aro.
    </dif>
    <trd lng="pl">brygada</trd>
    <trd lng="pt">brigada</trd>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_y;&c_g;&c_a;&c_d;&c_a;</trd>
  <trd lng="cs">pracovn&#xED; &#x10D;eta</trd>
  <trd lng="en">brigade</trd>
  <trd lng="es">brigada</trd>
  <trd lng="fr">brigade</trd>
  <trd lng="he">&#x5D7;&#x5B2;&#x5D8;&#x5B4;&#x5D9;&#x5D1;&#x5B8;&#x5D4;</trd>
  <trd lng="hu">brig&aacute;d</trd>
  <trd lng="it">brigata</trd>
  <trd lng="nl">brigade</trd>
  <trd lng="ru">&c_b;&c_r;&c_i;&c_g;&c_a;&c_d;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>brig&aacute;da</trd>,
    <trd>pracovn&#xE1; &#x10D;ata</trd>
  </trdgrp>
</drv>

<drv mrk="brigad.fajro0o">
  <kap>fajro<tld/>o</kap>
  <snc>
    <dif>
      Grupo de savistoj trejnitaj por tuje helpi okaze de brulego,
      inundo,
      a&ubreve; ajna natur- a&ubreve; hom-ka&ubreve;zita akcidento:
      <ekz>
        la tujaj komentoj la&ubreve;te la&ubreve;dis la lertan agadon de
        la nederlanda
        fajro<tld/>o kaj helpantaro<fnt><aut>Guido Van Damme</aut>:
        <vrk>Pli bone unu birdo en la mano</vrk>,
        <bib>Monato</bib><lok>jaro 1996a, numero 11a, p.
        19a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_zh;&c_a;&c_r;&c_n;&c_a;&c_ja;
  &c_k;&c_a;&c_m;&c_a;&c_n;&c_d;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>hasi&#x10D;i</trd>,
    <trd>hasi&#x10D;sk&#xE9; dru&#x17E;stvo</trd>,
    <trd>hasi&#x10D;sk&#xFD; odd&#xED;l</trd>,
    <trd>hasi&#x10D;sk&#xFD; sbor</trd>,
    <trd>po&#x17E;&#xE1;rn&#xED; dru&#x17E;stvo</trd>,
    <trd>po&#x17E;&#xE1;rn&#xED; sbor</trd>,
    <trd>po&#x17E;&#xE1;rn&#xED;ci</trd>
  </trdgrp>
  <trd lng="de">Feuerwehr</trd>
  <trd lng="en">fire brigade</trd>
  <trd lng="es">cuerpo de bomberos</trd>
  <trdgrp lng="fr">
    <trd>pompiers<klr> (subst.)</klr></trd>,
    <trd>sapeurs-pompiers</trd>
  </trdgrp>
  <trd lng="hu">t&udblac;zolt&oacute;s&aacute;g</trd>
  <trdgrp lng="it">
    <trd>pompieri</trd>,
    <trd>vigili del fuoco</trd>
  </trdgrp>
  <trd lng="nl">brandweer</trd>
  <trdgrp lng="pl">
    <trd>stra&zdot; po&zdot;arna</trd>,
    <trd>oddzia&lstroke; stra&zdot;y po&zdot;arnej</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>corpo de bombeiros</trd>,
    <trd>brigada de inc&ecirc;ndio</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_zh;&c_a;&c_r;&c_n;&c_a;&c_ja;
  &c_k;&c_o;&c_m;&c_a;&c_n;&c_d;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>hasi&#x10D;i</trd>,
    <trd>hasi&#x10D;sk&#xFD; oddiel</trd>
  </trdgrp>
</drv>

<drv mrk="brigad.fajro0isto">
  <kap>fajro<tld/>isto</kap>
  <snc>
    <dif>
      Profesiulo en fajro<tld/>o.
    </dif>
  </snc>
  <trd lng="de">Feuerwehrmann</trd>
  <trdgrp lng="en">
    <trd>firefighter</trd>,
    <trd>fireman</trd>
  </trdgrp>
  <trd lng="es">bombero</trd>
  <trd lng="fr">pompier</trd>
  <trdgrp lng="it">
    <trd>pompiere</trd>,
    <trd>vigile del fuoco</trd>
  </trdgrp>
  <trd lng="nl">brandweerman</trd>
  <trd lng="pl">stra&zdot;ak</trd>
  <trdgrp lng="pt">
    <trd>bombeiro</trd>
  </trdgrp>
</drv>

<drv mrk="brigad.fajro0ano">
  <kap>fajro<tld/>ano</kap>
  <snc>
    <dif>
      Ano de fajro<tld/>o, speciale trejnita precipe por estingi
      brulegojn kaj
      tuje helpi la viktimojn.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>hasi&#x10D;</trd>,
    <trd>po&#x17E;&#xE1;rn&#xED;k</trd>
  </trdgrp>
  <trd lng="fr">pompier</trd>
  <trdgrp lng="sk">
    <trd>hasi&#x10D;</trd>,
    <trd>po&#x17E;iarnik</trd>
  </trdgrp>
</drv>

</art>
