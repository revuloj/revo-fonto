     <trd>felavat&aacute;s</trd>,
     <trd>megnyit&aacute;s<klr tip="ind"> (int&eacute;zm&eacute;ny&eacute;)</klr></trd>,
     <trd>avat&oacute;&uuml;nneps&eacute;g</trd>,
