<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: atent.xml,v 1.48 2023/08/30 18:23:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>atent</rad>/a <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="atent.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="atent.0i.sinturni">
    <dif>
      Turni kaj fiksi sian spiriton al:
      <ekz>
        la cezaro mem <klr>[&#x2026;]</klr> nemulte <tld/>is &gcirc;is nun la spektaklon
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u la vojon, la&ubreve; kiu vi iris
        <fnt><bib>MT</bib>, <lok>&Jer; 31:21</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i venis]</klr> por <tld/>i la parolojn de Pa&ubreve;lo
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 16:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        bone <tld/>u, por ke la kru&ccirc;o ne falu en la maron
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        detaloj nesufi&ccirc;e <tld/>ataj &gcirc;is tiam
        <fnt>
          <bib>ChB</bib>
        </fnt>;
      </ekz>
      <ekz>
        se lupo ekpentis, &scirc;afo <tld/>u
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        okulo ne <tld/>as, dorso eksentas<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="auxsku.0i">a&ubreve;skulti</ref>,
      <ref cel="observ.0i.rigardi">observi</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="gard.0i">gardi</ref>,
      <ref cel="dorm.mal0i">maldormi</ref>,
      <ref cel="strecx.0i">stre&ccirc;i</ref>,
      <ref cel="zorg.0i">zorgi</ref>
    </refgrp>
    <trd lng="de">aufpassen</trd>
    <trdgrp lng="pl">
      <trd>zwa&zdot;a&cacute;</trd>,
      <trd>uwa&zdot;a&cacute;</trd>,
      <trd>baczy&cacute;</trd>,
      <trd>mie&cacute; <ind>baczenie</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="atent.0i.gravigi">
    <dif>
      Rigardi kiel gravan, <ref tip="sin" cel="zorg.0i.pri_io_bone">zorgi</ref>, 
      <ref tip="sin" cel="sekv.0i.atenti">sekvi</ref>, <ref tip="sin" cel="obe.0i">obei</ref>,
      <ref tip="sin" cel="respek.0i">respekti</ref>:
      <ekz>
        <tld/>i la konsilojn donitajn;
      </ekz>
      <ekz>
        ne <tld/>u mensogajn vortojn
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 5:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ne sufi&ccirc;e <tld/>is la riskojn
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        regulo rekomendas tre <tld/>i la etiketon
        <fnt>
          <bib>ChB</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu ne <tld/>as la se, tiu sentas la ve
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>sich <ind>halten</ind> <klr>(an)</klr></trd>,
      <trd>in <ind>Betracht</ind> ziehen</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>zwa&zdot;a&cacute;</trd>,
      <trd>uwa&zdot;a&cacute;</trd>,
      <trd>powa&zdot;a&cacute;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_v;&c_a;&c_zh;&c_a;&c_c;&c_mol;</trd>, 
    <trd>&c_z;&c_mol;&c_v;&c_ja;&c_r;&c_t;&c_a;&c_c;&c_mol; 
      <klr>(&c_s;&c_v;&c_a;&c_ju;)</klr> &c_u;&c_v;&c_a;&c_g;&c_u;
    </trd>,
    <trd>&c_m;&c_je;&c_c;&c_mol; &c_n;&c_a; &c_w;&c_v;&c_a;&c_z;&c_je;
    </trd>, 
    <trd>&c_p;&c_ib;&c_l;&c_mol;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd> 
  </trdgrp>
  <trdgrp lng="br">
    <trd>teuler <ind>evezh</ind></trd>, 
    <trd>teuler <ind>pled</ind></trd> 
  </trdgrp>
  <trdgrp lng="ca">
    <trd>posar atenci&oacute;</trd>,
    <trd>prendre esment</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bd&iacute;t</trd>,
    <trd>br&aacute;t z&rcaron;etel <klr>(na co)</klr></trd>,
    <trd>d&aacute;vat pozor</trd>,
    <trd>db&aacute;t na n&ecaron;co</trd>,
    <trd>dohl&iacute;&zcaron;et</trd>,
    <trd>m&iacute;t na z&rcaron;eteli</trd>,
    <trd>v&scaron;&iacute;mat si</trd>
  </trdgrp>
  <trd lng="de">beachten</trd>  
  <trd lng="en">pay <ind>attention</ind> (to)</trd>
  <trdgrp lng="es">
    <trd>estar <ind>atento</ind></trd>,
    <trd>prestar <ind>atenci&oacute;n</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire <ind>attention</ind> (&agrave;)</trd>,
    <trd>prendre <ind>garde</ind> (&agrave;)</trd>,
    <trd>diriger son <ind>attention</ind> (sur)</trd>,
    <trd>prendre en <ind>consid&eacute;ration</ind></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&lamed;&he;&yod;&zayin;&he;&resh;</trd>,
    <trd>&lamed;&shin;&yod;&fmem; &lamed;&bet;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>figyel</trd>,
    <trd>vigy&aacute;z</trd>
  </trdgrp>
  <trdgrp lng="it">
      <trd>fare <ind>attenzione</ind></trd>,
      <trd>prestare <ind>attenzione</ind></trd>,
      <trd>seguire<klr> (attenzione, considerazione)</klr></trd>,
      <trd>guardare<klr> (attenzione, considerazione)</klr></trd>,
      <trd>vigilare</trd>
    </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6C17;&#x3065;&#x304F;  [<ind>&#x304D;&#x3065;&#x304F;</ind>]</trd>,
    <trd>&#x7559;&#x610F;&#x3059;&#x308B;  [<ind>&#x308A;&#x3085;&#x3046;&#x3044;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>aandacht schenken aan</trd>,
    <trd>opletten</trd>,
    <trd>beschouwen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_r;&c_a;&c_shch;&c_a;&c_t;&c_mol; <klr>(&c_s;&c_v;&c_o;&c_jo;)</klr> &c_v;&c_n;&c_i;&c_m;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol; &c_v;&c_o; &c_v;&c_n;&c_i;&c_m;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>d&aacute;va&tcaron; <ind>pozor</ind></trd>,
    <trd>pozorova&tcaron;</trd>,
    <trd>str&aacute;&zcaron;i&tcaron;</trd>
  </trdgrp>
  <trd lng="sw">kuangalia</trd>
  <trd lng="tok">lukin</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&c_v;&c_a;&c_zh;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_v;&c_je;&c_r;&c_t;&c_a;&c_t;&c_i; <klr>(&c_s;&c_v;&c_o;&c_ju;)</klr> &c_u;&c_v;&c_a;&c_g;&c_u;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_s;&c_l;&c_u;&c_h;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_b;&c_u;&c_t;&c_i; &c_u;&c_v;&c_a;&c_zh;&c_n;&c_i;&c_m;</trd>
  </trdgrp>
</drv>

<drv mrk="atent.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="atent.0a.KOMUNE">
    <dif>
      Turnanta kaj fiksanta sian spiriton al io:
      <ekz>
        li petas, ke mi estu <tld/>a
        <fnt>
          <bib>F</bib>
          <lok>&FE; 26</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili obeas min per <tld/>aj oreloj
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 22:45</lok>
        </fnt>;
      </ekz>
      <ekz>
        li a&ubreve;skultis kun <klr>[&#x2026;]</klr> viza&gcirc;o <tld/>a kaj severa
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ekzameno tre <tld/>a de &ccirc;iu objekto
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu <tld/>a a&ubreve;skultanto povas mem konvinki&gcirc;i, ke en la plimulto de la publike
        farataj paroladoj trovi&gcirc;as sufi&ccirc;a kvanto ne nur da lingvaj eraroj, sed anka&ubreve;
        da &ccirc;iaspecaj aliaj
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiu &ccirc;i opinio <klr>[&#x2026;]</klr> post pli <tld/>a pripenso balda&ubreve; montri&gcirc;us al &ccirc;iu
        kiel bela
        <fnt>
          <bib>LR</bib>
          <lok>Dua Parto &ndash; Gramatiko</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi vidis ilian sa&gcirc;e <tld/>an rigardon
        <fnt>
          <bib>VaK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ib;&c_l;&c_mol;&c_n;&c_y;</trd>, 
    <trd>&c_u;&c_v;&c_a;&c_zh;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="br">war <ind>evezh</ind></trd>
  <trd lng="ca">atent</trd>
  <trdgrp lng="cs">
    <trd>pozorn&yacute;</trd>,
    <trd>soust&rcaron;ed&ecaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="de">aufmerksam</trd>
  <trd lng="en">attentive</trd>
  <trd lng="es">atento</trd>
  <trd lng="fr">attentif</trd>
  <trd lng="he">&qof;&#x5B7;&shin;&#x5C1;&vav;&#x5BC;&bet;</trd>
  <trd lng="hu">figyelmes</trd>
  <trd lng="it">attento</trd>
  <trd lng="ja">&#x6CE8;&#x610F;&#x6DF1;&#x3044;  [<ind>&#x3061;&#x3085;&#x3046;&#x3044;&#x3076;&#x304B;&#x3044;</ind>]</trd>
  <trd lng="nl">aandachtig</trd>
  <trdgrp lng="pl">
    <trd>uwa&zdot;ny</trd>,
    <trd>baczny</trd>
  </trdgrp>
  <trd lng="pt">atento</trd>
  <trd lng="ru">&c_v;&c_n;&c_i;&c_m;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>pozorn&yacute;</trd>,
    <trd>s&uacute;streden&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_u;&c_v;&c_a;&c_zh;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="atent.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="atent.0igi.KOMUNE">
    <dif>
      Turni la <tld/>on de iu al io:
      <ekz>
        <tld/>igi iun pri surskribo, dan&gcirc;ero;
      </ekz>
      <ekz>
        ili <tld/>igis lin, ke la vintro alproksimi&gcirc;as
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> <tld/>igis &ccirc;iun pri sia valoro
        <fnt>
          <bib>Fab2</bib>
          <lok>Konkurso de saltado</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni de tempo al tempo estas <tld/>igata pri tio, ke ni estas nenio, e&ccirc; se oni
        sidas tiel alte kiel mi
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi tuj en la komenco <tld/>igas vin, ke mia parolo ne estas io oficiala
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Tria Kongreso Esperantista en Cambridge en la 12a de a&ubreve;gusto
          1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        nur <ind>unu vorto &scirc;in <tld/>igis</ind>
        <fnt>
          <bib>KPr</bib>
        </fnt>.
        <trd lng="de">ein Wort lie&szlig; sie <ind>aufhorchen</ind></trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_v;&c_ja;&c_r;&c_t;&c_a;&c_c;&c_mol; <klr>(&c_ch;&c_y;&c_ju;&c_s;&c_mol;&c_c;&c_ib;)</klr>
    &c_u;&c_v;&c_a;&c_g;&c_u;</trd>
  <trd lng="br">sacha&ntilde; <ind>evezh</ind> (ub. war udb.)</trd>
  <trdgrp lng="ca">
    <trd>advertir</trd>,
    <trd>fer observar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pouk&aacute;zat</trd>,
    <trd>upozornit <klr>(na n&ecaron;co)</klr></trd>
  </trdgrp>
  <trd lng="de">hinweisen</trd>
  <trd lng="en">draw one's <ind>attention</ind> to</trd>
  <trdgrp lng="es">
    <trd>advertir</trd>,
    <trd>atraer la <ind>atenci&oacute;n</ind></trd>,
    <trd>poner en guardia</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>attirer l'<ind>attention</ind> (de, sur)</trd>,
    <trd>mettre en <ind>garde</ind></trd>
  </trdgrp>
  <trd lng="he">&lamed;&mem;&shin;&vav;&fkaf; &tav;&shin;&vav;&mem;&tav;
  &lamed;&bet;</trd>
  <trd lng="hu">figyelmeztet</trd>
  <trdgrp lng="it">
      <trd>attirare l'<ind>attenzione</ind></trd>,
      <trd>spingere l'<ind>attenzione</ind></trd>,
      <trd>mettere in <ind>guardia</ind></trd>,
      <trd>far <ind>osservare</ind></trd>
    </trdgrp>
  <trd lng="ja">&#x6CE8;&#x610F;&#x3092;&#x5411;&#x3051;&#x3055;&#x305B;&#x308B;  [<ind>&#x3061;&#x3085;&#x3046;&#x3044;&#x3092;&#x3080;&#x3051;&#x3055;&#x305B;&#x308B;</ind>]</trd>
  <trd lng="nl">aandacht vestigen op</trd>
  <trdgrp lng="pl">
    <trd>zwraca&cacute; <ind>uwag&eogonek;</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_r;&c_a;&c_shch;&c_a;&c_t;&c_mol; <klr>(&c_ch;&c_mol;&c_jo;-&c_l;&c_i;&c_b;&c_o;)</klr> &c_v;&c_n;&c_i;&c_m;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>da&tcaron; echo <klr>(niekomu)</klr></trd>,
    <trd>upozorni&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_z;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_i; <klr>(&c_ch;&c_i;&c_ju;&c_s;&c_mol;)</klr> &c_u;&c_v;&c_a;&c_g;&c_u;</trd>
</drv>

<drv mrk="atent.0o">
  <kap><tld/>o</kap>
  <snc mrk="atent.0o.KOMUNE">
    <dif>
      Ago a&ubreve; stato de tiu, kiu <tld/>as:
      <ekz>
        li a&ubreve;skultis <klr>[&#x2026;]</klr> kun granda <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 21:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi ne donas vian <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Mala&hcirc;i 2:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur lian filinon turnis sian <tld/>on granda sinjoro
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        al &scirc;i vi devas nun dedi&ccirc;i vian <tld/>on
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        deturni la <tld/>on
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne metinte <tld/>on sur la raporton de la komisio
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        plej multe altiris al si la <tld/>on <klr>[&#x2026;]</klr> bela kastelo el papero
        <fnt>
          <bib>Fab1</bib>
          <lok>Persista stana soldato</lok>
        </fnt>;
      </ekz>
      <ekz>
        de legado sen <tld/>o ne ri&ccirc;i&gcirc;as la prudento
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ib;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_v;&c_a;&c_g;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>evezh</trd>, 
    <trd>pled</trd>
  </trdgrp>
  <trd lng="ca">atenci&oacute;</trd>
  <trdgrp lng="cs">
    <trd>ohled</trd>,
    <trd>pozor</trd>,
    <trd>pozornost</trd>,
    <trd>zaujet&iacute;</trd>,
    <trd>z&aacute;jem</trd>,
    <trd>z&rcaron;etel</trd>
  </trdgrp>
  <trd lng="de">Aufmerksamkeit</trd>
  <trd lng="en">attention</trd>
  <trd lng="es">atenci&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>attention</trd>,
    <trd>vigilance</trd>
  </trdgrp>
  <trd lng="he">&zayin;&he;&yod;&resh;&vav;&tav;</trd>
  <trd lng="hu">figyelem</trd>
  <trd lng="it">attenzione</trd>
  <trd lng="ja">&#x6CE8;&#x76EE;  [<ind>&#x3061;&#x3085;&#x3046;&#x3082;&#x304F;</ind>]</trd>
  <trdgrp lng="nl">
    <trd>aandacht</trd>,
    <trd>opmerkzaamheid</trd>
  </trdgrp>
  <trd lng="oc">atencion</trd>
  <trdgrp lng="pl">
    <trd>uwaga</trd>,
    <trd>baczenie</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_n;&c_i;&c_m;&c_a;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>oh&lcaron;ad</trd>,
    <trd>pozornos&tcaron;</trd>,
    <trd>zaujatie</trd>,
    <trd>z&aacute;ujem</trd>
  </trdgrp>
  <trd lng="uk">&c_u;&c_v;&c_a;&c_g;&c_a;</trd>
</drv>

<drv mrk="atent.mal0i">
  <kap>mal<tld/>i</kap>
  <snc mrk="atent.mal0i.KOMUNE">
    <dif>
      Ne konsideri <tld/>inda,
      <ref cel="bagate.0igi.OPI">bagateligi</ref>,
      <ref tip="sin" cel="ignor.0i">ignori</ref>:
      <ekz>
        mal<tld/>i la ripro&ccirc;ojn;
      </ekz>
      <ekz>
        demandu la voja&gcirc;antojn, kaj ne mal<tld/>u iliajn atestojn
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 21:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;io estas preta, venu al la edzi&gcirc;a festo &dash; sed ili mal<tld/>is, kaj foriris
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 22:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        povra kamparano sidanta <klr>[&#x2026;]</klr> en angulo, tiu priridata kaj mal<tld/>ata
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ofte mal<tld/>as la decregulojn, kaj lia gajo degeneras en frenezon
        <fnt>
          <bib>Kandid</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i tute mal<tld/>is tiujn &ccirc;i vortojn
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi mal<tld/>u mian a&gcirc;on! &ndash; mi sentas min tiel juna, kvaza&ubreve; mi estas kun vi
        sama&gcirc;a
        <fnt>
          <bib>Metrop</bib>
        </fnt>!
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="respek.mal0i">malrespekti</ref>,
      <ref cel="neglek.0i">neglekti</ref>,
      <ref cel="atent.preter0i">preteratenti</ref>,
      <ref cel="vid.preter0i">pretervidi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_g;&c_n;&c_a;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_je; &c_z;&c_v;&c_a;&c_zh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">descuidar</trd>
  <trdgrp lng="cs">
    <trd>nedbat</trd>,
    <trd>opomenout</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ignorieren</trd>,
    <trd>missachten</trd>,
    <trd>&uuml;berh&ouml;ren</trd>,
    <trd>&uuml;bersehen</trd>
  </trdgrp><trdgrp lng="es">
    <trd>descuidar</trd>,
    <trd>desatender</trd>,
    <trd>ignorar</trd>
  </trdgrp>
  <trd lng="fr">ignorer</trd>
  <trd lng="ja">&#x7121;&#x8996;&#x3059;&#x308B;  [<ind>&#x3080;&#x3057;&#x3059;&#x308B;</ind>]</trd>
  <trdgrp lng="sk">
    <trd>nepozorova&tcaron;</trd>,
    <trd>nev&scaron;&iacute;ma&tcaron; si</trd>,
    <trd>prep&aacute;s&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_je; &c_z;&c_v;&c_je;&c_r;&c_t;&c_a;&c_t;&c_i; &c_u;&c_v;&c_a;&c_g;&c_i;</trd>,
    <trd>&c_ib;&c_gu;&c_n;&c_o;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_n;&c_je;&c_h;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="atent.mal0igi">
  <kap>mal<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="atent.mal0igi.PSI">
    <dif>
      Perturbi ies <tld/>on, <ref cel="turn.de0i">deturni</ref> lian spiriton de iu temo;
      <ref tip="vid" cel="distr.0i.PSI">distri<sncref/></ref>:
      <ekz>
        lia amantino eniris en la kajuton de la pordisto por lin mal<tld/>igi
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        nura orienti&gcirc;o al propra bonfarto mal<tld/>igas pri komunaj
        bonoj
        <fnt>
          <aut>Ivo</aut>,
          <vrk><url ref="https://groups.google.com/forum/#!topic/soc.culture.esperanto/-d734fm7kuA">
          Rubo produktas elektron</url></vrk>,
          <lok>sce, 1995-01-04</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="distr.0i.PSI">distri<sncref/></ref>
    <refgrp tip="vid">
      <ref cel="log.de0i">delogi</ref>,
      <ref cel="devi.0igi">deviigi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_c;&c_ja;&c_g;&c_v;&c_a;&c_c;&c_mol; <ind>&c_u;&c_v;&c_a;&c_g;&c_u;</ind></trd>
  <trd lng="de">ablenken</trd>
  <trd lng="es">desviar la atenci&oacute;n</trd>
  <trd lng="fr">d&eacute;tourner l'attention</trd>
</drv>

<drv mrk="atent.preter0i">
  <kap>preter<tld/>i</kap>
  <snc mrk="atent.preter0i.KOMUNE">
    <dif>
      Ne <ref tip="ant" cel="rimark.0i.mense">rimarki</ref> pro manko de <tld/>o,
      <ref tip="sin" cel="neglek.0i">neglekti</ref>:
      <ekz>
        restas unu ebleco, kiun vi ne rajtas preter<tld/>i <klr>[&#x2026;]</klr>, iu povis
        suba&ccirc;eti la gardiston
        <fnt>
          <bib>CKv</bib>
        </fnt>;
      </ekz>
      <ekz>
        teatra verkisto, kiun oni ne rajtus preter<tld/>i, estas la Sviso <nom>Edmond Privat</nom>
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        se li volas fari esplorojn pri iu ajn medicina bran&ccirc;o, li nepre devas scii
        3&#x202F;-&#x202F;4
        fremdajn lingvojn, se li ne volas preter<tld/>i en sia laboro gravajn
        kontribua&jcirc;ojn
        <fnt>
          <bib>EeP</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je; &c_z;&c_a;&c_w;&c_v;&c_a;&c_zh;&c_y;&c_c;&c_mol;</trd>, 
    <trd>&c_n;&c_je; &c_z;&c_mol;&c_v;&c_ja;&c_r;&c_n;&c_u;&c_c;&c_mol; &c_u;&c_v;&c_a;&c_g;&c_ib;</trd>, 
    <trd>&c_p;&c_r;&c_a;&c_p;&c_u;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;</trd> 
  </trdgrp>  
  <trd lng="br">na ober <ind>van</ind> (eus, ouzh)</trd>  
  <trdgrp lng="ca">
    <trd>desatendre</trd>,
    <trd>negligir</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verpassen</trd>,
    <trd>&uuml;bersehen</trd>,
    <trd>&uuml;bergehen</trd>
  </trdgrp>
  <trd lng="en">overlook</trd>
  <trdgrp lng="es">
    <trd>desatender<klr> (ocasi&oacute;n, consejo)</klr></trd>,
    <trd>dejar pasar</trd>,
    <trd>pasar por alto</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>n&eacute;gliger<klr> (occasion, conseil)</klr></trd>,
    <trd>laisser passer</trd>
  </trdgrp>
  <trd lng="hu">figyelmen k&iacute;v&uuml;l hagy</trd>
  <trd lng="it">sfuggire<klr> (sorvolare)</klr></trd>
  <trd lng="ja">&#x898B;&#x843D;&#x3068;&#x3059;  [<ind>&#x307F;&#x304A;&#x3068;&#x3059;</ind>]</trd>
  <trd lng="nl">geen <ind>aandacht</ind> schenken aan</trd>
  <trdgrp lng="pl">
    <trd>przeoczy&cacute;</trd>,
    <trd>nie zauwa&zdot;y&cacute;</trd>,
    <trd>przegapi&cacute;</trd>,
    <trd>przepu&sacute;ci&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je; &c_o;&c_b;&c_r;&c_a;&c_t;&c_i;&c_t;&c_mol; &c_v;&c_n;&c_i;&c_m;&c_a;&c_n;&c_i;&c_ja;</trd>,
    <trd>&c_n;&c_je; &c_z;&c_a;&c_m;&c_je;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_m;&c_o;&c_t;&c_r;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_p;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">prehliadnu&tcaron; <klr>(nezbada&tcaron;)</klr></trd>
  <trdgrp lng="uk">
