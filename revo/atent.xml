<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>atent</rad>/a
</kap>
<drv mrk="atent.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Turnanta kaj fiksanta sian spiriton al io:
      <ekz>
        estu <tld/>a pri la leciono.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ib;&c_l;&c_mol;&c_n;&c_y;</trd>, 
    <trd>&c_u;&c_v;&c_a;&c_zh;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>  
  <trd lng="cs">pozorn&yacute;</trd>
  <trd lng="de">aufmerksam</trd>
  <trd lng="fr">attentif</trd>
  <trd lng="en">attentive</trd>
  <trd lng="nl">aandachtig</trd>
  <trd lng="ru">&c_v;&c_n;&c_i;&c_m;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="atent.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Turni kaj fiksi sian spiriton al:
      <ekz>
        <tld/>i la paroladon de oratoro, la spektaklon;
      </ekz>
      <ekz>
        <tld/>u pri mia konsilo;
      </ekz>
      <ekz>
        <tld/>u la vojon, la&ubreve; kiu vi iris
        <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Jeremia 31, 21</lok>)</fnt>;
      </ekz>
      <ekz>
        <tld/>u, ke vi ne falu;
      </ekz>
      <ekz>
        okulo ne <tld/>as, dorso eksentas<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="gard.0i">gardi</ref>,
      <ref cel="dorm.mal0i">maldormi</ref>,
      <ref cel="strecx.0i">stre&ccirc;i</ref>,
      <ref cel="zorg.0i">zorgi</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Rigardi kiel gravan, zorgi, sekvi, obei:
      <ekz>
        <tld/>i la konsilojn donitajn;
      </ekz>
      <ekz>
        mal<tld/>i la ripro&ccirc;ojn.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_v;&c_a;&c_zh;&c_a;&c_c;&c_mol;</trd>, 
    <trd>&c_z;&c_mol;&c_v;&c_ja;&c_r;&c_t;&c_a;&c_c;&c_mol; 
      <klr>(&c_s;&c_v;&c_a;&c_ju;)</klr> &c_u;&c_v;&c_a;&c_g;&c_u;</trd>, 
    <trd>&c_m;&c_je;&c_c;&c_mol; &c_n;&c_a; &c_w;&c_v;&c_a;&c_z;&c_je;</trd>, 
    <trd>&c_p;&c_ib;&c_l;&c_mol;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd> 
  </trdgrp>  
  <trd lng="cs">d&aacute;vat pozor</trd>
  <trdgrp lng="de">
    <trd>aufpassen</trd>, <trd>beachten</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire <ind>attention</ind> (&agrave;)</trd>,
    <trd>prendre <ind>garde</ind> (&agrave;)</trd>,
    <trd>diriger son <ind>attention</ind> (sur)</trd>,
    <trd>prendre en <ind>consid&eacute;ration</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>aandacht schenken aan</trd>,
    <trd>opletten</trd>,
    <trd>beschouwen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_r;&c_a;&c_shch;&c_a;&c_t;&c_mol; <klr>(&c_s;&c_v;&c_o;&c_jo;)</klr> &c_v;&c_n;&c_i;&c_m;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol; &c_v;&c_o; &c_v;&c_n;&c_i;&c_m;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="atent.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Turni la <tld/>on de iu al io:
      <ekz>
        <tld/>igi iun pri surskribo, dan&gcirc;ero.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_v;&c_ja;&c_r;&c_t;&c_a;&c_c;&c_mol; 
    <klr>(&c_ch;&c_y;&c_ju;&c_s;&c_mol;&c_c;&c_ib;)</klr>
    &c_u;&c_v;&c_a;&c_g;&c_u;</trd>
  <trd lng="de">hinweisen</trd>
  <trdgrp lng="fr">
    <trd>attirer l'<ind>attention</ind> (de, sur)</trd>,
    <trd>mettre en <ind>garde</ind></trd>
  </trdgrp>
  <trd lng="nl">aandacht vestigen op</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_r;&c_a;&c_shch;&c_a;&c_t;&c_mol; <klr>(&c_ch;&c_mol;&c_jo;-&c_l;&c_i;&c_b;&c_o;)</klr> &c_v;&c_n;&c_i;&c_m;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="atent.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago a&ubreve; stato de tiu, kiu <tld/>as:
      <ekz>
        meti, turni<fnt>Z</fnt>,
      </ekz>
      <ekz>
        dedi&ccirc;i sian <tld/>on al io<fnt>Z</fnt>,
      </ekz>
      <ekz>
        sur ion;
      </ekz>
      <ekz>
        mi turnas vian <tld/>on sur tion, ke...<fnt>Z</fnt>;
      </ekz>
      <ekz>
        okupi la <tld/>on de iu.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ib;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>, 
    <trd>&c_u;&c_v;&c_a;&c_g;&c_a;</trd>
  </trdgrp>  
  <trd lng="de">Aufmerksamkeit</trd>
  <trd lng="fr">attention</trd>
  <trdgrp lng="nl">
    <trd>aandacht</trd>,
    <trd>opmerkzaamheid</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_n;&c_i;&c_m;&c_a;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="atent.preter0i">
  <kap>preter<tld/>i<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Ne rimarki pro manko de <tld/>o, ne rigardi kiel gravan.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je; &c_z;&c_a;&c_w;&c_v;&c_a;&c_zh;&c_y;&c_c;&c_mol;</trd>, 
    <trd>&c_n;&c_je; &c_z;&c_mol;&c_v;&c_ja;&c_r;&c_n;&c_u;&c_c;&c_mol; 
      &c_u;&c_v;&c_a;&c_g;&c_ib;</trd>, 
    <trd>&c_p;&c_r;&c_a;&c_p;&c_u;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;</trd> 
  </trdgrp>  
  <trd lng="de">verpassen</trd>
  <trdgrp lng="fr">
    <trd>n&eacute;gliger (occasion, conseil)</trd>,
    <trd>laisser passer</trd>
  </trdgrp>
  <trd lng="nl">geen <ind>aandacht</ind> schenken aan</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je; &c_o;&c_b;&c_r;&c_a;&c_t;&c_i;&c_t;&c_mol; &c_v;&c_n;&c_i;&c_m;&c_a;&c_n;&c_i;&c_ja;</trd>,
    <trd>&c_n;&c_je; &c_z;&c_a;&c_m;&c_je;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_m;&c_o;&c_t;&c_r;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_p;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="atent.sen0a">
  <kap>sen<tld/>a</kap>
  <snc>
    <dif>
      Ne <tld/>anta.
    </dif>
    <refgrp tip="vid">
      <ref cel="blind.0a">blinda</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_ja;&c_w;&c_v;&c_a;&c_zh;&c_l;&c_ib;&c_v;&c_y;</trd>
  <trd lng="de">unaufmerksam</trd>
  <trd lng="fr">inattentif</trd>
  <trdgrp lng="nl">
    <trd>onaandachtig</trd>,
    <trd>onopmerkzaam</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_je;&c_v;&c_n;&c_i;&c_m;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
