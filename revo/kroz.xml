<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kroz</rad>/i
</kap>
<drv mrk="kroz.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Marveturi zigzage en iu regiono por kontroli kaj
      observi la navigacion:
      <ekz>
        la &scirc;iparo <tld/>is en la &hcirc;ina maro.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_e;&c_j;&c_s;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">kreuzen</trd>
  <trd lng="en">cruise</trd>
  <trdgrp lng="fr">
    <trd>croiser<klr> (marine)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_j;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="kroz.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">KOMP</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="foli.0umilo">Foliumilo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_a;&c_w;&c_z;&c_e;&c_r;</trd>
  <trd lng="de">Browser</trd>
  <trdgrp lng="fr">
    <trd>butineur</trd>,
    <trd>navigateur<klr> (programme informatique)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="kroz.0anto">
  <kap><tld/>anto</kap>
  <snc>
    <ref tip="dif" cel="sxip.kroz0o"><tld/>&scirc;ipo</ref>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_e;&c_j;&c_s;&c_e;&c_r;</trd>
  <trd lng="de">Kreuzer</trd>
  <trd lng="en">crusier</trd>
  <trdgrp lng="fr">
    <trd>croiseur</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_j;&c_s;&c_je;&c_r;</trd>
</drv>

<drv mrk="kroz.0isto">
  <kap><tld/>isto<fnt>Z</fnt></kap>
  <snc>
    <ref tip="dif" cel="sxip.kroz0o"><tld/>&scirc;ipo</ref>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_e;&c_j;&c_s;&c_e;&c_r;</trd>
  <trd lng="de">Kreuzer</trd>
  <trd lng="en">crusier</trd>
  <trdgrp lng="fr">
    <trd>croiseur</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_j;&c_s;&c_je;&c_r;</trd>
</drv>
</art>
