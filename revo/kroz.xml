<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kroz</rad>/i
</kap>

<drv mrk="kroz.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Marveturi zigzage en iu regiono por kontroli kaj
      observi la navigacion:
      <ekz>
        la &scirc;iparo <tld/>is en la &hcirc;ina maro.
      </ekz> 
    </dif>
  </snc>
  <trd lng="de">kreuzen</trd>
  <trd lng="en">cruise</trd>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_j;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="kroz.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">KOMP</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="foli.0umilo">Foliumilo</ref>.
    </dif>
  </snc>
  <trd lng="de">Browser</trd>
</drv>

<drv mrk="kroz.0sxipo">
  <kap><tld/>anto, <tld/>isto<fnt>Z</fnt>, <tld/>&scirc;ipo</kap>
  <snc>
    <dif>
      Speciala rapida milit&scirc;ipo por <tld/>i.  
    </dif>
    <trd lng="en">crusier</trd>
    <trd lng="ru">&c_k;&c_r;&c_je;&c_j;&c_s;&c_je;&c_r;</trd>
  </snc>
  <trd lng="de">Kreuzer</trd>
</drv>

</art>
