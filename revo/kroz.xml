<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kroz.xml,v 1.24 2023/09/01 15:46:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kroz</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kroz.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kroz.0i.FIG">
    <dif>
      Veturi malrapide, zigzage, esplore en iu areo:
      <ekz>
        la polica&ubreve;to pasis kaj <tld/>is plu
        <fnt><bib>&Scirc;talrato</bib><lok>&ccirc;ap. 16</lok></fnt>.
      </ekz>
    </dif>
    <subsnc mrk="kroz.0i.MIL">
      <uzo tip="fak">MIL</uzo>
      <dif>
        <klr>(pri <ref cel="sxip.milit0o">milit&scirc;ipo</ref> a&ubreve; avio)</klr>
        Veturi zigzage en iu regiono por kontroli la trafikon:
        <ekz>
          la polico <tld/>as &ccirc;e la strando por sekureco de
          la na&gcirc;antoj;
        </ekz>
        <ekz>
          germanaj aeroplanoj <tld/>adis super la bombarditaj pozicioj
          <fnt><aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>, 
          <lok>1978</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_k;&c_r;&c_e;&c_j;&c_s;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
      <trd lng="de">kreuzen</trd>
      <trd lng="en">cruise</trd>
      <trdgrp lng="fr">
        <trd>croiser<klr tip="ind"> (en p. d'un navire
        militaire)</klr></trd>
      </trdgrp>
      <trd lng="hu">cirk&aacute;l</trd>
      <trdgrp lng="it">
        <trd>incrociare<klr> (mil.)</klr></trd>,
        <trd>battere<klr> (pattugliare)</klr></trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>kruisen<klr tip="ind"> (laveren)</klr></trd>,
        <trd>laveren</trd>
      </trdgrp>
      <trd lng="pt">cruzar <klr tip="amb">(diz-se de navio)</klr></trd>
      <trd lng="ru">&c_k;&c_r;&c_je;&c_j;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </subsnc>
    <subsnc mrk="kroz.0i.TUR">
      <uzo tip="fak">MAR</uzo>
      <dif>
        <klr>(pri <ref cel="sxip.0o">&scirc;ipo</ref> a&ubreve;
        <ref cel="pasagx.0o">pasa&gcirc;ero</ref>)</klr>
        Veturi sur &scirc;ipo plezurcele, turismocele:
        <ekz>
          <tld/>i sur Mediteraneo.
        </ekz>
      </dif>
      <trdgrp lng="fr">
        <trd>croiser<klr tip="ind"> (en p. d'un bateau de
        plaisance)</klr></trd>,
        <trd>naviguer<klr tip="amb"> (en p. d'un bateau de
        plaisance)</klr></trd>,
        <trd>naviguer<klr tip="amb"> (&agrave; bord d'un bateau de
        plaisance)</klr></trd>,
        <trd>faire une <ind>croisi&egrave;re</ind></trd>
      </trdgrp>
      <trdgrp lng="it">
        <trd>incrociare<klr> (mar.)</klr></trd>,
        <trd>fare una <ind>crociera</ind></trd>
      </trdgrp>
      <trd lng="nl">een <ind>cruise</ind> maken</trd>
    </subsnc>
    <subsnc mrk="kroz.0i.turisme_nesxipe">
      <uzo tip="stl">FIG</uzo>
      <uzo tip="klr">(iu)</uzo>
      <dif>
        Promenadi &ccirc;iudirekten turismocele:
        <ekz>
          <klr>[per TTT]</klr> oni povas <tld/>i tra multaj urboj de la
          mondo
          <fnt>
          <bib>Voja&gcirc;o</bib>
          <aut>B. Kolker</aut>
          <vrk>Tut-Tera Teksa&jcirc;o</vrk><lok>leciono 26a</lok>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="fr">
        <trd>d&eacute;ambuler</trd>,
        <trd>se <ind>balader</ind></trd>,
        <trd>vadrouiller</trd>
      </trdgrp>
      <trd lng="it">navigare<klr> (internet)</klr></trd>
      <trd lng="nl">surfen<klr tip="ind"> (Internet)</klr></trd>
    </subsnc>
  </snc>
  <snc mrk="kroz.0i.sekse">
    <uzo tip="stl">FIG</uzo>
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Promenadi &ccirc;iudirekten en difinita loko por tie trovi
      seksan partneron.
    </dif>
    <trd lng="fr">draguer<klr tip="ind"> (dans un lieu)</klr></trd>
    <trd lng="it">battere<klr> (pattugliare)</klr></trd>
    <trd lng="nl">fladderen<klr tip="ind"> (flirten)</klr></trd>
  </snc>
  <trd lng="cs">k&rcaron;i&zcaron;ovat</trd>
  <trdgrp lng="ja">
    <trd>&#x5DE1;&#x822A;&#x3059;&#x308B;  [<ind>&#x3081;&#x3050;&#x308A;&#x308F;&#x305F;&#x308B;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x904A;&#x5F0B;&#x3059;&#x308B;  [<ind>&#x3042;&#x305D;&#x5F0B;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kr&aogonek;&zdot;y&cacute; <klr>(na morzu)</klr></trd>,
    <trd>patrolowa&cacute;</trd>  
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>circulate  <klr>(pe mare)</klr></trd>,
    <trd>patrulare</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hliadkova&tcaron;</trd>,
    <trd>kri&zcaron;ova&tcaron;</trd>,
    <trd>patrolova&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&#x440;&#x435;&#x439;&#x441;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
</drv>

<drv mrk="kroz.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <trd lng="cs">k&rcaron;i&zcaron;ov&aacute;n&iacute;</trd>
  <trd lng="de">Kreuzen</trd>
  <trdgrp lng="fr">
    <trd>croisi&egrave;re</trd>,
    <trd>balade</trd>,
    <trd>vadrouille</trd>,
    <trd>drague</trd>
  </trdgrp>
  <trd lng="pl">rejs</trd>     
  <trd lng="ro">croazier&atilde;</trd>
  <trd lng="sk">kri&zcaron;ovanie</trd>
</drv>

<drv mrk="kroz.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">KOMP</uzo>
  <snc>
    <ref tip="dif" cel="foli.0umilo">foliumilo</ref>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_a;&c_w;&c_z;&c_e;&c_r;</trd>
  <trd lng="de">Browser</trd>
  <trdgrp lng="fr">
    <trd>butineur</trd>,
    <trd>navigateur<klr tip="ind"> (programme informatique)</klr></trd>
  </trdgrp>
  <trd lng="hu">b&ouml;ng&eacute;sz&odblac;<klr> (program)</klr></trd>
  <trd lng="it">browser</trd>
  <trd lng="nl">bladeraar</trd>
  <trd lng="pl">przegl&aogonek;darka internetowa</trd>
  <trd lng="ro">navigator web</trd>
  <trd lng="ru">&c_b;&c_r;&c_a;&c_u;&c_z;&c_je;&c_r;</trd>
</drv>

<drv mrk="kroz.0anto">
  <kap><tld/>anto</kap>
  <uzo tip="fak">MAR</uzo>
  <uzo tip="fak">MIL</uzo>
  <fnt><bib>PV</bib></fnt>
  <uzo tip="stl">EVI</uzo>
  <snc>
    <ref tip="dif" cel="sxip.kroz0o"><tld/>&scirc;ipo</ref>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_e;&c_j;&c_s;&c_e;&c_r;</trd>
  <trd lng="cs">k&rcaron;i&zcaron;n&iacute;k</trd>
  <trd lng="de">Kreuzer</trd>
  <trd lng="en">crusier</trd>
  <trd lng="hu">cirk&aacute;l&oacute; <klr tip="ind">(haj&oacute;)</klr></trd>
  <trd lng="it">incrociatore</trd>
  <trd lng="nl">kruiser</trd>
  <trd lng="pl">kr&aogonek;&zdot;ownik</trd>     
  <trd lng="ro">cruci&#x219;&abreve;tor</trd>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_j;&c_s;&c_je;&c_r;</trd>
  <trd lng="sk">kr&iacute;&zcaron;nik</trd>
</drv>

<drv mrk="kroz.0isto">
  <kap><tld/>isto<fnt>Z</fnt></kap>
  <uzo tip="fak">MAR</uzo>
  <uzo tip="fak">MIL</uzo>
  <uzo tip="stl">EVI</uzo>
  <snc>
    <ref tip="dif" cel="sxip.kroz0o"><tld/>&scirc;ipo</ref>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_e;&c_j;&c_s;&c_e;&c_r;</trd>
  <trd lng="cs">k&rcaron;i&zcaron;n&iacute;k</trd>
  <trd lng="de">Kreuzer</trd>
  <trd lng="en">crusier</trd>
  <trd lng="hu">cirk&aacute;l&oacute; <klr tip="ind">(haj&oacute;)</klr></trd>
  <trd lng="it">incrociatore</trd>
  <trd lng="nl">kruiser<klr tip="ind"> (ontr.)</klr></trd>
  <trd lng="pl">kr&aogonek;&zdot;ownik</trd>     
  <trd lng="ro">croazier&abreve;</trd>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_j;&c_s;&c_je;&c_r;</trd>
  <trd lng="sk">kr&iacute;&zcaron;nik</trd>
  <trd lng="uk">&#x43A;&#x440;&#x435;&#x439;&#x441;&#x435;&#x440;</trd>
</drv>
</art>
