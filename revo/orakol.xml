<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc> <rad>orakol</rad>/o
</kap>
<drv mrk="orakol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Respondo farita, je nomo de ia dia&jcirc;o, de &gcirc;iaj pastroj al
      tiuj, kiuj venas por &gcirc;in konsulti en &gcirc;ia templo:
      <ekz>
        la <tld/>o de Apolono en Delfoj;
      </ekz>
      <ekz>
        la <tld/>oj de Jesaja;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la guvernisto Panglos estis la hejma <tld/>diranto.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Orakel</trd>
  <trdgrp lng="hu">
    <trd>j&oacute;slat</trd>,
    <trd>or&aacute;kulum</trd>
  </trdgrp>
  <trd lng="nl">orakel</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_r;&c_a;&c_k;&c_u;&c_l;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_s;&c_k;&c_a;&c_z;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="orakol.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Eldiri <tld/>on.
    </dif>
  </snc>
  <trd lng="de">orakeln</trd>
  <trdgrp lng="hu">
    <trd>j&oacute;sol</trd>,
    <trd>j&ouml;vend&odblac;t mond</trd>
  </trdgrp>
</drv>
</art>
