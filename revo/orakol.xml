<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: orakol.xml,v 1.23 2024/01/30 16:09:26 revo Exp $">
    <kap>
      <ofc>*</ofc> <rad>orakol</rad>/o
    </kap>

    <drv mrk="orakol.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="orakol.0o.ulo">
	<dif>
	  A&ubreve;toritatulo respondanta, nome de dia&jcirc;o, je la demandoj
	  de tiuj kiuj venas &gcirc;in konsulti:
	  <ekz>
	    Marta rigardis la parolantan virinon kiel <tld/>on
	    <fnt><aut>E. Orzeszkowa, trad. &Z;</aut>: <vrk>Marta.</vrk></fnt>;
	  </ekz><ekz>
	    tute ne necesas esti <tld/>o por deklari, ke &gcirc;i neniam
	    atingos la 17 eldonojn de la Fundamenta Krestomatio
	    <fnt>
	      <aut>A. Kor&jcirc;enkov</aut>: <vrk>Ne &ccirc;io nova estas
		bona</vrk>. <lok><url ref="&LOdE;R-nek.htm">LOdE, 1992. n-ro 2 (3)</url></lok>
	    </fnt>;
	  </ekz><ekz>
	    <uzo tip="fak">SCI</uzo>
	    <uzo tip="stl">FIG</uzo>
	    <tld/>o foje respondas kaj foje ne, kaj ne eblas influi tion
	    <fnt><vrk>Orakolaj teknikoj</vrk><lok><url ref="http://www.ais-sanmarino.org/kursoj/kultprog">AIS San-Marino</url></lok></fnt>.
	  </ekz>
	</dif>
	<trd lng="ru">&c_o;&c_r;&c_a;&c_k;&c_u;&c_l;</trd>
      </snc>
      <snc mrk="orakol.0o.respondo">
	<dif>
	  <tld lit="O"/>a&jcirc;o, respondo de <tld/>o<sncref ref="orakol.0o.ulo"/>:
	  <ekz>
	    la <tld/>oj de Jesaja;
	  </ekz><ekz>
	    <uzo tip="stl">FIG</uzo>
	    la guvernisto Panglos estis la hejma <tld/>diranto.
	  </ekz>
	</dif>
        <trd lng="en">revelation</trd>
	<trdgrp lng="ru">
	  <trd>&c_p;&c_r;&c_je;&c_d;&c_s;&c_k;&c_a;&c_z;&c_a;&c_n;&c_i;&c_je;</trd>,
	  <trd>&c_p;&c_r;&c_o;&c_r;&c_i;&c_c;&c_a;&c_n;&c_i;&c_je;</trd>
	</trdgrp>
     </snc>
      <snc mrk="orakol.0o.ejo">
	<dif>
	  <tld lit="O"/>ejo, sanktejo kien oni venas por konsulti
	  <tld/>on<sncref ref="orakol.0o.ulo"/>:
	  <ekz>
	    la <tld/>o de Apolono en Delfo.
	  </ekz>
	</dif>
	<trd lng="ru">&c_o;&c_r;&c_a;&c_k;&c_u;&c_l;</trd>
      </snc>
  <trdgrp lng="be">
	<trd>&c_a;&c_r;&c_a;&c_k;&c_u;&c_l;</trd>,
	<trd>&c_p;&c_r;&c_a;&c_d;&c_k;&c_a;&c_z;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
      </trdgrp>
  <trdgrp lng="cs">
    <trd>or&aacute;kulum</trd>,
    <trd>proroctv&iacute;</trd>,
    <trd>v&ecaron;&scaron;tba</trd>
  </trdgrp>
  <trd lng="de">Orakel</trd>
  <trd lng="el">&chi;&rho;&eta;&sigma;&my;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">oracle</trd>
  <trdgrp lng="hu">
	<trd>j&oacute;slat</trd>,
	<trd>or&aacute;kulum</trd>
      </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x795E;&#x8A17;  <pr>&#x3057;&#x3093;&#x305F;&#x304F;</pr></trd>,
    <trd>&#x795E;&#x306E;&#x304A;&#x544A;&#x3052;  <pr>&#x304B;&#x307F;&#x306E;&#x304A;&#x3064;&#x3052;</pr></trd>
  </trdgrp>
  <trd lng="nl">orakel</trd>
  <trdgrp lng="pl">
    <trd>wyrocznia</trd>,
    <trd>wieszczba</trd>
  </trdgrp>
  <trd lng="pt">or&aacute;culo</trd>
  <trdgrp lng="ro">
    <trd>ma&#x219;in&abreve; oracol</trd>,
    <trd>profetesa</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>or&aacute;kulum</trd>,
    <trd>predpove&dcaron;</trd>,
    <trd>proroctvo</trd>,
    <trd>ve&scaron;tba</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x440;&#x430;&#x43A;&#x443;&#x43B;</trd>,
    <trd>&#x43F;&#x440;&#x43E;&#x440;&#x43E;&#x446;&#x442;&#x432;&#x43E;</trd>,
    <trd>&#x43F;&#x440;&#x43E;&#x440;&#x43E;&#x43A;&#x443;&#x432;&#x430;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x795E;&#x8AED; <pr>sh&#xE9;ny&#xF9;</pr></trd>,
    <trd>&#x795E;&#x8C15; <pr>sh&#xE9;ny&#xF9;</pr></trd>,
    <trd>&#x8AED;&#x793A; <pr>y&#xF9;sh&#xEC;</pr></trd>,
    <trd>&#x8C15;&#x793A; <pr>y&#xF9;sh&#xEC;</pr></trd>
  </trdgrp>
</drv>

    <drv mrk="orakol.0i">
      <kap><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
	<dif>
	  Eldiri <tld/>on.
	</dif>
      </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_d;&c_k;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>prorokovat</trd>,
    <trd>v&ecaron;&scaron;tit</trd>
  </trdgrp>
  <trd lng="de">orakeln</trd>
  <trdgrp lng="hu">
	<trd>j&oacute;sol</trd>,
	<trd>j&ouml;vend&odblac;t mond</trd>
      </trdgrp>
  <trd lng="ja">&#x795E;&#x8A17;&#x3092;&#x544A;&#x3052;&#x308B;  <pr>&#x3057;&#x3093;&#x305F;&#x304F;&#x3092;&#x3064;&#x3052;&#x308B;</pr></trd>
  <trd lng="nl">orakelen</trd>
  <trdgrp lng="pl">
    <trd>wydawa&cacute; wyroczni&eogonek;</trd>,
    <trd>wieszczy&cacute;</trd>
  </trdgrp>
  <trd lng="pt">oracular</trd>
  <trdgrp lng="ro">
    <trd>emite un oracol</trd>,
    <trd>profetiza</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>predpoveda&tcaron;</trd>,
    <trd>prorokova&tcaron;</trd>,
    <trd>ve&scaron;ti&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x440;&#x43E;&#x440;&#x43E;&#x447;&#x438;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x440;&#x43E;&#x440;&#x43E;&#x43A;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>
