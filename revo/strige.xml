<art mrk="$Id: strige.xml,v 1.15 2024/03/25 07:28:37 revo Exp $">
<kap>
  <rad>striged</rad>/oj
</kap>
<drv mrk="strige.0oj">
  <kap><tld/>oj</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo de
      <ref tip="malprt" cel="strig.0oformaj">strigoformaj
      birdoj</ref> (<trd lng="la">Strigidae</trd>), viandman&gcirc;antaj
      noktobirdoj kun anta&ubreve;endirektitaj okuloj kaj kun plumkovritaj
      piedfingroj:
      <ekz>
        arbarstrigo <klr>[&#x2026;]</klr> apartenas al genro Strigo, parto de pli granda grupo
        de noktaj rabobirdoj konataj kiel <tld lit="S"/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18879">Arbarstrigo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_v;&c_ib;&c_n;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#x2019;&c_ja;)</klr></trd>
  <trd lng="ca">estr&iacute;gids</trd>
  <trd lng="cs">pu&scaron;t&iacute;kovit&iacute;</trd>
  <trd lng="de">Eigentliche <ind>Eulen</ind></trd>
  <trdgrp lng="en">
    <trd>true owls</trd>,
    <trd>typical owls</trd>
  </trdgrp>
  <trd lng="fr">strigid&eacute;s</trd>
  <trd lng="hu">bagolyf&eacute;l&eacute;k</trd>
  <trd lng="nl">echte <ind>uilen</ind><klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pl">puszczykowate</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_v;&c_i;&c_n;&c_y;&c_je;<klr tip="ind"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_i;&c_je; <ind>&c_s;&c_o;&c_v;&c_y;</ind> <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="sk">sovovit&eacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43E;&c_v;&c_i;&#x43D;&c_ib;</trd>,
    <trd>&c_s;&#x43E;&c_v;&#x2019;&#x44F;&c_ch;&c_ib;</trd>,
    <trd>&c_s;&#x43F;&c_r;&c_a;&c_v;&c_zh;&#x43D;&c_ib; &c_s;&#x43E;&c_v;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9D1F;&#x9D1E;&#x79D1; <pr>ch&#x12B;xi&#x101;ok&#x113;</pr></trd>,
    <trd>&#x9E31;&#x9E2E;&#x79D1; <pr>ch&#x12B;xi&#x101;ok&#x113;</pr></trd>
  </trdgrp>
</drv>
</art>
