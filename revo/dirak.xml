<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>Dirak</rad>/o
</kap>
<drv mrk="dirak.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Anglalingve:
      Paul <trd lng="en">Dirac</trd>, 1902-1984. Angla fizikisto.
    </dif>
  </snc>
  <trd lng="de">Dirac</trd>
  <trd lng="fr">Dirac</trd>
  <trd lng="nl">Paul <ind>Dirac</ind></trd>
  <trd lng="ru">&c_D;&c_i;&c_r;&c_a;&c_k;</trd>
</drv>
<drv mrk="dirak.d0a">
  <kap><tld lit="d"/>a</kap>
  <snc>
    <refgrp tip="vid">
      <ref cel="distri2.diraka0o"><tld lit="d"/>a distribucio</ref>,
      <ref cel="komb.diraka0ilo"><tld lit="d"/>a kombilo</ref>,
      <ref cel="mezur.diraka0o"><tld lit="d"/>a mezuro</ref>.
    </refgrp>
  </snc>
    <trd lng="de">Diracsche</trd>
    <trd lng="en">Dirac['s]</trd>
    <trd lng="fr">de <ind>Dirac</ind></trd>
    <trd lng="nl">van <ind>Dirac</ind></trd>
    <trd lng="ru">&c_D;&c_i;&c_r;&c_a;&c_k;&c_a;</trd>
</drv>
</art>

