<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: dirak.xml,v 1.12 2019/01/20 18:10:16 revo Exp $">
<kap>
  <rad>Dirak</rad>/o
</kap>
<drv mrk="dirak.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <dif>
      Anglalingve: <nom>Paul</nom> <trd lng="en">Dirac</trd>, 1902-1984; 
      angla <ref tip="lst" lst="voko:historiaj_personoj" cel="fizik.0isto">fizikisto</ref>:
      <ekz>
        Pa&ubreve;lo <tld/>o inventis povan kaj intuician matematikan
        notacion por priskribi kvantumajn statojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=105381"
              >Kvantuma stato</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">Dirac Paul</trd>
  <trd lng="de">Dirac</trd>
  <trd lng="fa">&f_p;&a_A;&a_w;&a_l; <ind>&a_d;&f_y;&a_r;&a_A;&f_k;</ind></trd>
  <trd lng="fr">Dirac</trd>
  <trd lng="hu">Dirac</trd>
  <trd lng="it">Dirac</trd>
  <trd lng="nl">Paul <ind>Dirac</ind></trd>
  <trd lng="pl">Dirac</trd>
  <trd lng="pt">Dirac</trd>
  <trd lng="ru">&c_D;&c_i;&c_r;&c_a;&c_k;</trd>
  <trd lng="sk">Dirac Paul</trd>
</drv>

<drv mrk="dirak.d0a">
  <kap><tld lit="d"/>a</kap>
  <snc>
    <refgrp tip="vid">
      <ref cel="distri2.diraka0o"><tld lit="d"/>a distribucio</ref>,
      <ref cel="komb.diraka0ilo"><tld lit="d"/>a kombilo</ref>,
      <ref cel="mezur.diraka0o"><tld lit="d"/>a mezuro</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Diracsche</trd>
  <trd lng="en">Dirac['s]</trd>
  <trd lng="fr">de <ind>Dirac</ind></trd>
  <trdgrp lng="hu">
    <trd>Dirac-</trd>,
    <trd>Dirac-f&eacute;le</trd>
  </trdgrp>
  <trd lng="it">di <ind>Dirac</ind></trd>
  <trd lng="nl">van <ind>Dirac</ind></trd>
  <trd lng="pl">Diraca</trd>
  <trd lng="ru">&c_D;&c_i;&c_r;&c_a;&c_k;&c_a;</trd>
</drv>
</art>
