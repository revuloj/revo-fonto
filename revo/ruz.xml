  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12378;&#12427;&#12356;</trd>,
    <trd>&#24746;&#36066;&#12356; <pr>&#12431;&#12427;&#12364;&#12375;&#12371;&#12356;</pr></trd>,
    <trd>&#29409;&#29502;&#12394; <pr>&#12371;&#12358;&#12363;&#12388;&#12394;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
