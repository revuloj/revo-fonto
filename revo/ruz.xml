<drv mrk="ruz.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Artifike kaj lerte <ref tip="super" cel="tromp.0a">trompa</ref>:
      <ekz>
        urbanoj estas ordinare pli <tld/>aj, ol vila&gcirc;anoj
        <fnt>
          <bib>F</bib>
          <lok>&FE; 37</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a vila&gcirc;ano efektivigis sian projekton
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a kiel vulpo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Jehu</nom> faris <klr>[la kunvokon]</klr> kun <tld/>a intenco, por pereigi la
        servantojn de <nom>Baal</nom>
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 10:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        la serpento estis pli <tld/>a, ol &ccirc;iuj kampaj bestoj, kiujn kreis Dio
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 3:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj <klr>[&#8230;]</klr> efektive estis singardaj kontra&ubreve; la malbona Amoro, sed li
        tamen super<tld/>is ilin, &ccirc;ar li estas tre lerta kaj <tld/>a
        <fnt>
          <bib>Fab1</bib>
          <lok>Malbonkonduta knabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la nokto la <tld/>a rabisto palpas la pordon
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&gcirc;i]</klr> estis pli ruza <klr>[kaj]</klr> iris, ka&scirc;ante sin en anguloj
        tiel, ke neniu hundo &gcirc;in vidis
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        gardu vin kontra&ubreve; la maljunulino, &scirc;i estas <tld/>a kiel la diablo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi ludas la rolon de modestulino, sed mi konas vin jam de longe, vi estas
        <tld/>a kanajlo
        <fnt>
          <bib>GDn</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        tuj tiu &ccirc;i mieno &scirc;an&gcirc;i&gcirc;as per <tld/>a rideto
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        io tamen en lia vo&ccirc;tono kaj en lia mieno certigis al la <tld/>a komercisto, ke
        li ne eraris
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; vulpo plej <tld/>a fine estas kaptata
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="insid.0a">insida</ref>,
      <ref cel="subtil.0a">subtila</ref>,
      <ref cel="sagac.0a">sagaca</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_h;&c_ib;&c_t;&c_r;&c_y;</trd>
  <trd lng="bg">&c_h;&c_i;&c_t;&c_malmol;&c_r;</trd>
  <trd lng="bo">&t_ma;&t_ga;&t_naro;&t_tsheg;&t_ba;&t_da;&t_drengbu;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>
