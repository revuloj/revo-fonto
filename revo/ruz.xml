<drv mrk="ruz.0ulo">
  <kap>
    <tld/>ulo
  </kap>
  <snc>
    <dif>
      Homo <tld/>a:
      <ekz>
        Li detruas la intencojn de <tld/>uloj
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 5:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        nu, diris la <tld/>ulo kaj levis ordinaran &scirc;tonon, ku&scirc;antan apud li, jen prenu
        anka&ubreve; &ccirc;i tiun bonan &scirc;tonon, sur kiu vi povos bone for&gcirc;i
        <fnt>
          <bib>ElektFab</bib>
          <lok>Feli&ccirc;a Jo&ccirc;jo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis lerta kortegano, granda <tld/>ulo
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi estas granda <tld/>ulo, kaj neniu povos diri, ke mi havas ian rilaton al la
        afero
        <fnt>
          <bib>GDn</bib>
          <lok>Akto Unua</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="vulp.0o.ruzulo">vulpo<sncref/></ref>
  </snc>
  <trd lng="de">Fuchs <klr>(Schlaukopf)</klr></trd>
</drv>

<drv mrk="ruz.super0i">
  <kap>super<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Superi, <ref tip="super" cel="tromp.0i">trompi</ref> per <tld/>o:
      <ekz>
        trompi ne penu min: vi ne admonos min nek super<tld/>os
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        nun tiu &ccirc;i azeno lin
        super<tld/>as
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li kaj lia sinjoro estis tute same vestitaj, por povi pli efike super<tld/>i
        eventualajn persekutantojn
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        nun ili nepre kredas, ke ili faris bonan kapton! kiel ni super<tld/>is la
        senta&ubreve;gulojn
        <fnt>
          <bib>VaK</bib>
          <lok>Unua &Ccirc;apitro</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_h;&c_ib;&c_t;&c_r;&c_y;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_n;&c_a;&c_d;&c_h;&c_i;&c_t;&c_r;&c_ja;&c_v;&c_a;&c_m;</trd>
  <trd lng="cs">p&rcaron;elst&iacute;t</trd>
  <trd lng="de">&uuml;berlisten</trd>
  <trdgrp lng="es">
    <trd>burlar</trd>,
    <trd>buscarle a uno las vueltas</trd>
  </trdgrp>
  <trd lng="fr">rouler<klr tip="ind"> (quelqu'un)</klr></trd>
  <trd lng="he">&lamed;&he;<ind>&ayin;&resh;&yod;&fmem;</ind>
  &ayin;&lamed;</trd>
  <trdgrp lng="hu">
    <trd>r&aacute;szed</trd>,
    <trd>t&uacute;lj&aacute;r az esz&eacute;n</trd>
  </trdgrp>
  <trd lng="ja">&#20986;&#12375;&#25244;&#12367;  <pr>&#12384;&#12375;&#12396;&#12367;</pr></trd>
  <trdgrp lng="nl">
    <trd>te <ind>slim</ind> af zijn</trd>,
    <trd>belazeren</trd>,
    <trd>verschalken</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_h;&c_i;&c_t;&c_r;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">pre&lcaron;sti&tcaron;</trd>
  <trd lng="uk">&c_p;&c_je;&c_r;&c_je;&c_h;&c_i;&c_t;&c_r;&c_i;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#26177;&#19977;&#26286;&#22235; <pr>zh&amacron;os&amacron;nm&ugrave;s&igrave;</pr></trd>,
    <trd>&#26234;&#21213; <pr>zh&igrave;sh&egrave;ng</pr></trd>,
    <trd>&#26234;&#32988; <pr>zh&igrave;sh&egrave;ng</pr></trd>,
    <trd>&#30663;&#39449; <pr>m&emacron;ngpi&agrave;n</pr></trd>,
    <trd>&#33945;&#39575; <pr>m&emacron;ngpi&agrave;n</pr></trd>
  </trdgrp>
</drv>
