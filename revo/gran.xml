<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: gran.xml,v 1.4 2022/09/15 08:45:25 afido Exp $">
    <kap>
      <rad>gran</rad>/o <fnt><bib>Hamlet</bib></fnt>
    </kap>

<drv mrk="gran.0o">
  <kap><tld/>o</kap>
  <snc mrk="gran.0o.mezurunuo">
    <dif>
      <ref tip="lst" lst="voko:kvantoj" cel="unu.mezur0o">Mezurunuo</ref> de pezo,
      ekvivalenta al tiu de hordea a&ubreve; tritika <ref cel="grajn.0o.AGR">grajno</ref>
      kaj la&ubreve; tempoj variante inter 47&#x202F;mg &gcirc;is 65&#x202F;mg:
      <ekz>
        la romia <tld/>o estis la 9216-ono de <ref tip="malprt" cel="min1.0o.MEZ_PEZ">mino</ref>;
      </ekz>
      <ekz>
        <tld/>o da pura oro
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        iomete da revo kaj multe da obstino <klr>[&#8230;]</klr>, <tld/>o da kolero
        <klr>(kp <ref cel="grajn.0o.iometo">grajno<sncref/></ref>)</klr>
        <fnt>
          <aut>Pierre Lepape </aut>
          <vrk><url ref="&Mondediplo;article871.html">Eksterordinara vivmalsato</url></vrk>
          <lok>Le Monde diplomatique, Majo 1a, 2004 </lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_g;&c_r;&c_a;&c_n; <klr>(&c_a;&c_d;&c_z;&c_ib;&c_n;&c_k;&c_a; &c_v;&c_y;&c_m;&c_ja;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_ja; &c_v;&c_a;&c_g;&c_ib;)</klr></trd>
    <trd lng="de">Gran</trd>
    <trd lng="en">grain</trd>
    <trd lng="fr">grain</trd>
    <trd lng="ru">&c_g;&c_r;&c_a;&c_n;</trd>
  </snc>
  <trd lng="uk">&c_gu;&c_r;&c_a;&#x43D;&#xA0;(&#x43E;&c_d;&c_i;&#x43D;&c_i;&c_c;&#x44F;
  &c_a;&#x43F;&c_t;&c_je;&#x43A;&c_a;&c_r;&c_s;&#x44C;&#x43A;&#x43E;&c_ji; &c_v;&c_a;&c_g;&c_i;)</trd>
</drv>

  </art>

