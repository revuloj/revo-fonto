<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: gran.xml,v 1.2 2022/03/19 19:18:24 afido Exp $">
    <kap>
      <rad>gran</rad>/o <fnt><bib>Hamlet</bib></fnt>
    </kap>

<drv mrk="gran.0o">
  <kap><tld/>o</kap>
  <snc mrk="gran.0o.mezurunuo">
    <dif>
      <ref tip="lst" lst="voko:kvantoj" cel="unu.mezur0o">Mezurunuo</ref> de pezo,
      ekvivalenta al tiu de hordea a&ubreve; tritika <ref cel="grajn.0o.AGR">grajno</ref>
      kaj la&ubreve; tempoj variante inter 47&#x202F;mg &gcirc;is 65&#x202F;mg:
      <ekz>
        la romia <tld/>o estis la 9216-ono de <ref tip="malprt" cel="min1.0o.MEZ_PEZ">mino</ref>;
      </ekz>
      <ekz>
        <tld/>o da pura oro
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        iomete da revo kaj multe da obstino <klr>[&#x2026;]</klr>, <tld/>o da kolero
        <klr>(kp <ref cel="grajn.0o.iometo">grajno<sncref/></ref>)</klr>
        <fnt>
          <aut>Pierre Lepape </aut>
          <vrk><url ref="https://eo.mondediplo.com/article871.html">Eksterordinara vivmalsato</url></vrk>
          <lok>Le Monde diplomatique, Majo 1a, 2004 </lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Gran</trd>
    <trd lng="en">grain</trd>
    <trd lng="fr">grain</trd>
    <trd lng="ru">&c_g;&c_r;&c_a;&c_n;</trd>
  </snc>
  <trd lng="uk">&#x491;&#x440;&#x430;&#x43D;&#xA0;(&#x43E;&#x434;&#x438;&#x43D;&#x438;&#x446;&#x44F; &#x430;&#x43F;&#x442;&#x435;&#x43A;&#x430;&#x440;&#x441;&#x44C;&#x43A;&#x43E;&#x457; &#x432;&#x430;&#x433;&#x438;)</trd>
</drv>

  </art>

