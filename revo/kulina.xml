<kap>
  <rad>kulinar</rad>/a <fnt><bib>FK</bib></fnt>
</kap>
<drv mrk="kulina.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="kulina.0a.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="dif" cel="art.kuir0a.KUI">Kuirarta</ref>:
      <ekz>
        <tld/>a scienco kaj arto
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        Makdonaldo superregas tutmonde super &ccirc;iaj veraj <tld/>aj man&gcirc;a&jcirc;oj
        <fnt>
          <aut>Stefan Maul</aut>
            <vrk><url ref="&Monato;/2000/007121.php">Malbona venko</url></vrk>
            <bib>Monato</bib>
            <lok>2000/12, p. 30</lok>
          </fnt>;
      </ekz>
      <ekz>
        &ccirc;io iom post iom transformi&gcirc;os: mode, lingve, fizike, etne kaj <tld/>e
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2006/008827.php">Pri kutimoj kaj kalsonoj</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_l;&c_ib;&c_n;&c_a;&c_r;&c_n;&c_y;</trd>
  <trd lng="ca">culinari</trd>
  <trdgrp lng="cs">
    <trd>kucha&rcaron;sk&yacute;</trd>,
    <trd>kulin&aacute;rn&iacute;</trd>,
    <trd>kulin&aacute;&rcaron;sk&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
   <trd>kulinarisch</trd>,
   <trd>feinschmeckerisch</trd>
  </trdgrp>
  <trd lng="en">cullinary</trd>
  <trd lng="es">culinario</trd>
  <trd lng="fr">culinaire</trd>
  <trdgrp lng="hu">
   <trd>kulin&aacute;ris</trd>,
   <trd>konyhai</trd>
  </trdgrp>
  <trd lng="nl">culinair</trd>
  <trd lng="pl">kulinarny</trd>
  <trd lng="pt">culin&aacute;rio</trd>
  <trd lng="ru">&c_k;&c_u;&c_l;&c_i;&c_n;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">kuch&aacute;rsky</trd>
  <trd lng="uk">&#x43A;&c_u;&#x43B;&c_ib;&#x43D;&c_a;&c_r;&#x43D;&c_i;&c_j;</trd>
</drv>
