<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fiol.xml,v 1.13 2022/03/20 10:19:22 revo Exp $">
<kap>
  <rad>fiol</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="fiol.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="flakon.0o">Flakono</ref> kun longa,
      mallar&gcirc;a
      kolo:
      <ekz>
        se mi estus farinta <tld/>on da
        balzamo<fnt><bib>DonKihxoto</bib>,
        <lok>&ccirc;apitro 10a, p. 78a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_l;&c_ja;&c_k;&c_o;&c_n;</trd>
  <trdgrp lng="lat">
    <trd>aguncula</trd>,
    <trd>fiala<klr> (pop.)</klr></trd>, 
    <trd>phiala<klr> (class.)</klr></trd>
  </trdgrp>
  <trd lng="ca">fiola</trd>
  <trd lng="cs">fi&oacute;la</trd>
  <trd lng="de">Phiole</trd>
  <trdgrp lng="en">
    <trd>phial</trd>,
    <trd>vial</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>frasco<klr> (para medicamentos)</klr></trd>,
    <trd>vial<klr> (para medicamentos)</klr></trd>
  </trdgrp>
  <trd lng="fr">fiole</trd>
  <trd lng="hu">fiola</trd>
  <trd lng="it">fiala</trd>
  <trd lng="la">phiala</trd>
  <trd lng="pl">fiolka</trd>
  <trdgrp lng="pt">
    <trd>frasco <klr tip="amb">(de gargalo longo)</klr></trd>,
    <trd>garrafa <klr tip="amb">(de gargalo longo)</klr></trd>
  </trdgrp>
  <trd lng="sk">fiola</trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x43A;&#x43B;&#x44F;&#x43D;&#x43A;&#x430;</trd>,
    <trd>&#x43F;&#x43B;&#x44F;&#x448;&#x435;&#x447;&#x43A;&#x430;</trd>,
    <trd>&#x444;&#x43B;&#x430;&#x43A;&#x43E;&#x43D;</trd>
  </trdgrp>
</drv>
</art>

