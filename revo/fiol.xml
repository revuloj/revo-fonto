<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>fiol</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="fiol.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="flakon.0o">Flakono</ref> kun longa, mallar&gcirc;a
      kolo:
      <ekz>
        se mi estus farinta <tld/>on da balzamo<fnt><bib>DonKihxoto</bib>,
        <lok>&ccirc;apitro 10a, p. 78a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Phiole</trd>
  <trd lng="fr">fiole</trd>
  <trd lng="la">fiole</trd>
  <trdgrp lng="pt">
    <trd>frasco <klr tip="amb">(de gargalo longo)</klr></trd>,
    <trd>garrafa <klr tip="amb">(de gargalo longo)</klr></trd>
  </trdgrp>
</drv>
</art>

