<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>gasteroste</rad>/o
</kap>
<drv mrk="gaster.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="gaster.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Gasterosteus</trd>), fi&scirc;o, kiu havas
      dornojn sur sia dorso kaj kies viro konstruas neston por la ovoj.
    </dif>
    <ref tip="sin" cel="fisx.dorn0eto">dornfi&scirc;eto</ref>
  </snc>
  <trd lng="de">Stichling</trd>
</drv>

<drv mrk="gaster.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso
      <ref tip="super" cel="fisx.osto0oj">ostofi&scirc;oj</ref>
      (<trd lng="la">Gaterosteiformes</trd>).
    </dif>
  </snc>
  <trd lng="de">Stichlingsartige</trd>
</drv>
</art>
