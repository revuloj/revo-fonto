<vortaro>
<art mrk="$Id$">
<kap>
  <rad>izomorf</rad>/a
</kap>
<drv mrk="izomor.0a">
  <kap><tld/>a</kap>
  <snc num="1">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Havanta la saman formon kiel la alia. 
    </dif>
    <trdgrp lng="de">
      <trd>isomorph</trd>,
      <trd>formgleich</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Kristali&gcirc;anta la&ubreve; la sama a&ubreve; tre simila formo.  
    </dif>
    <trd lng="de">von gleicher Kristallform</trd>
  </snc>
  <trd lng="ru">&c_i;&c_z;&c_o;&c_m;&c_o;&c_r;&c_f;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
