<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>ale</rad>/o
</kap>

<drv mrk="ale.0o">
  <kap><tld/>o</kap>
  <snc mrk="ale.0o.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      Vojo en &gcirc;ardeno:
      <ekz>
        la litova &ccirc;efurbo Vilnius fari&gcirc;is la tria urbo en
        E&ubreve;ropo, post Vieno kaj Berlino, kiu posedas <tld/>on de
        japanaj &ccirc;erizarboj<fnt><aut>Last</aut>:
        <vrk>Aleo ina&ubreve;gurita</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 1a, p. 15a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="ca">passeig</trd>
    <trdgrp lng="en">
      <trd>alley<klr tip="amb"> (garden)</klr></trd>,
      <trd>path<klr tip="amb"> (garden)</klr></trd>
    </trdgrp>
    <trd lng="nl">pad</trd>
    <trd lng="pl">aleja</trd>
  </snc>
  <snc mrk="ale.0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      Lar&gcirc;a strato, ombrita per du vicoj da arboj; avenuo.
    </dif>
    <trdgrp lng="ca">
      <trd>avinguda</trd>,
      <trd>rambla</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>boulevard</trd>,
      <trd>avenue</trd>,
      <trd>tree-lined street</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>aleja</trd>,
      <trd>bulwar</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_je;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>alej</trd>,
    <trd>stromořadí</trd>
  </trdgrp>
  <trd lng="de">Allee</trd>
  <trd lng="es">alameda</trd>
  <trd lng="fr">all&eacute;e</trd>
  <trd lng="nl">laan</trd>
  <trdgrp lng="pt">
    <trd>alameda</trd>,
    <trd>al&eacute;ia</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_l;&c_l;&c_je;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>aleja</trd>,
    <trd>stromoradie</trd>
  </trdgrp>
</drv>
</art>
