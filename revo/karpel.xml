<art mrk="$Id: karpel.xml,v 1.14 2022/03/21 17:51:57 revo Exp $">
<kap>
  <rad>karpel</rad>/o
</kap>
<drv mrk="karpel.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="frukt.0ofolio">Fruktofolio</ref>:
      <ekz>
        la floroj estas <klr>[&#x2026;]</klr> estas dulatere simetriaj kun 5 kununuigitaj
        petaloj, 5 kununuigitaj sepaloj, 4 stamenoj kaj 2 <tld/>oj kun po du ovoloj,
        &ccirc;iu <tld/>o duoni&gcirc;as per vando, tiel ke la frukto aspektas kiel kvarakeno
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18066">Lamiacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_a;&c_d;&c_a;&c_l;&c_ib;&c_s;&c_mol;&c_c;&c_ib;&c_k;</trd>
  <trd lng="ca">carpel</trd>
  <trdgrp lng="de">
     <trd>Fruchtblatt</trd>,
     <trd>Karpell</trd>
  </trdgrp>
  <trd lng="en">carpel</trd>
  <trd lng="fr">carpelle</trd>
  <trd lng="hu">term&odblac;lev&eacute;l</trd>
  <trd lng="nl">vruchtblad</trd>
  <trd lng="pl">owocolistek</trd>     
  <trd lng="ro">carpel&abreve;</trd>
  <trd lng="ru">&c_p;&c_l;&c_o;&c_d;&c_o;&c_l;&c_i;&c_s;&c_t;&c_i;&c_k;</trd>
  <trd lng="uk">&#x43F;&#x43B;&#x43E;&#x434;&#x43E;&#x43B;&#x438;&#x441;&#x442;&#x438;&#x43A;</trd>
</drv>
</art>
