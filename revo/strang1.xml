<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: strang1.xml,v 1.19 2023/10/30 01:33:16 revo Exp $">
<kap>
  <ofc>9</ofc><rad>strangol</rad>/i <fnt><aut>P. Christaller</aut>,
  <vrk>Deutsch-Esperanto W&ouml;rterbuch</vrk>, <lok>1910</lok></fnt>
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="strang1.0i">
  <kap><ofc>9</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="strang1.0i.ANA">
    <dif>
      <ref tip="super" cel="sufok.0i.FIZL">Sufoki</ref> premegante la
      gor&gcirc;on:
      <ekz>
        oni &jcirc;us <tld/>is en Konstantinopolo du vezirojn
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXX</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lastan jaron <klr>[&#8230;]</klr> unu patrino de enlanda komunisto
        postulis de sia filo <klr>[&#8230;]</klr>, ke li <tld/>u &scirc;in
        <fnt><bib>&Ccirc;uk&ccirc;oj</bib>, <lok>La trimova &scirc;akproblemo</lok></fnt>;
      </ekz>
      <ekz>
        li levis brakon, evidente por &jcirc;eti la &scirc;nuron &ccirc;irka&ubreve; mian kolon kaj <tld/>i min
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 4</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kiam oni a&ubreve;das <klr>[&#8230;]</klr> la vorton &leftquot;Prusujo&rightquot;, <klr>[&#8230;]</klr> &gcirc;i estas
        sinonimo por la <tld/>ado de la libero kaj por polica kuratoreco
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 17</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        griza nebulo <tld/>as la lampobrilon
        <fnt>
          <bib>LGA</bib>
          <lok>II.</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="super">
      <ref cel="mort.0igi.malvivigi">mortigi <sncref/></ref>,
      <ref cel="string.0i">stringi</ref>
    </refgrp>
    <ref tip="vid" cel="pend.0umi">pendumi</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_d;&c_u;&c_sh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_u;&c_sh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_d;&c_a;&c_v;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">estrangular</trd>
  <trdgrp lng="de">
    <trd>strangulieren</trd>,
    <trd>w&uuml;rgen</trd>,
    <trd>erw&uuml;rgen</trd>,
    <trd>erdrosseln</trd>
  </trdgrp>
  <trd lng="en">strangle</trd>
  <trd lng="fr">&eacute;trangler</trd>
  <trd lng="hu">megfojt</trd>
  <trdgrp lng="ja">
    <trd>&#39318;&#12434;&#x7D5E;&#12417;&#x308B;  <pr>&#x304F;&#12403;&#12434;&#12375;&#12417;&#x308B;</pr></trd>,
    <trd>&#x7D5E;&#12417;&#x6BBA;&#12377;  <pr>&#12375;&#12417;&#12371;&#x308D;&#12377;</pr></trd>
  </trdgrp>
  <trd lng="nl">wurgen</trd>
  <trdgrp lng="pl">
    <trd>d&lstroke;awi&cacute;</trd>,
    <trd>dusi&cacute;<klr>  (za gard&lstroke;o)</klr></trd>,
    <trd>st&lstroke;umi&cacute;</trd>,
    <trd>udusi&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_d;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_u;&c_sh;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_d;&c_a;&c_v;&c_i;&c_t;&c_i; (&c_z;&c_a; &c_g;&c_o;&c_r;&c_l;&c_o;)</trd>
  </trdgrp>
</drv>
</art>
