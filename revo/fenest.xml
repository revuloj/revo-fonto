    <trd>&c_v;&c_ib;&c_k;&c_o;&c_n;&c_je;&c_ch;&c_k;&c_o;</trd>,
    <trd>&c_k;&c_v;&c_a;&c_t;&c_i;&c_r;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="fenest.el0igxi">
  <kap>el<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="fenest.el0igxi.KOMUNE">
    <dif>
      Eli&gcirc;i tra <tld/>o (kaj eventuale fali):
      <ekz>
        li sonorigas &ccirc;e la pordo, kaj tiam el<tld/>i&gcirc;as kapo de
        sinjorino
        <fnt>
          <aut>J. Shemer</aut>,
          <vrk>Paka&jcirc;o por S-ino Levi</vrk>,
          <lok><url ref="http://www.reu.ru/prenu/rego36.pdf">
          REGo, 2006-10 (36)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        amiko <klr>[&#8230;]</klr> el<tld/>i&gcirc;is, sin mortigante
        <fnt>
          <aut>A. Zecchin</aut>,
          <vrk><url ref="http://www.ueci.it/eldonoj/verkoj/la_hknstsm.pdf">
          La Homo kiu ne Sukcesis Trompi sin mem</url></vrk>,
          <lok>[2012]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_j;&c_s;&c_mol;&c_c;&c_ib; &c_z; &c_a;&c_k;&c_n;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_p;&c_a;&c_s;&c_mol;&c_c;&c_ib; &c_z; &c_a;&c_k;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>defenestrate</trd>,
