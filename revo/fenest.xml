    <trd>aktywny element okna</trd>
  </trdgrp>
  <trd lng="ru">
  &c_u;&c_p;&c_r;&c_a;&c_v;&c_l;&c_ja;&c_ju;&c_shch;&c_i;&c_j;
  &c_e;&c_l;&c_je;&c_m;&c_je;&c_n;&c_t;
  </trd>
</drv>

<drv mrk="fenest.0eto">
  <kap><tld/>eto</kap>
  <snc>
    <dif>
      Tre malgranda <tld/>o.
    </dif>
    <refgrp tip="vid">
      <ref cel="luk.0o">luko</ref>,
      <ref cel="gicxet.0o">gi&ccirc;eto</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_je;&c_n;&c_c;&c_a;</trd>
  <trd lng="cs">ok&eacute;nko</trd>
