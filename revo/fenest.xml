<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>fenestr</rad>/o
    </kap>

    <drv mrk="fenest.0o">
      <kap><ofc>*</ofc><tld/>o</kap>

      <snc mrk="fenest.0o.aperturo">
        <dif>
          Aperturo en muro de konstrua&jcirc;o por enlasi lumon kaj aeron,
          ordinare garnita per vitra&jcirc;o. 
        </dif>
      </snc>

      <snc mrk="fenest.0o.baro">
        <dif>
          Kadro(j) garnita(j) per vitra&jcirc;o(j) por fermi tian aperturon. 
        </dif>
      </snc>

      <snc mrk="fenest.0o.KOMP">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Parto de komputila <ref tip="malprt" cel="ekran.0o.vidiga">ekrano</ref>
          uzata kiel aparta ekrano:
          <ekz>
            dialoga, konzola, redakta <tld/>o.
          </ekz>
        </dif>
      </snc>

      <trd lng="be">&c_a;&c_k;&c_n;&c_o;</trd>
      <trd lng="cs">okno</trd>
      <trd lng="en">window</trd>
      <trd lng="es">ventana</trd>
      <trd lng="fr">fen&ecirc;tre</trd>
      <trd lng="de">Fenster</trd>
      <trd lng="hu">ablak</trd>
      <trd lng="nl">venster</trd>
      <trd lng="pl">okno</trd>
      <trd lng="ru">&c_o;&c_k;&c_n;&c_o;</trd>
    </drv>

    <drv mrk="fenest.0ajxo">
      <kap><tld/>a&jcirc;o</kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          En <ref tip="malprt" cel="fasad.grafika0o">grafika fasado</ref>,
          &ccirc;ia parto de <ref tip="malprt" cel="fenest.0o.KOMP"><tld/>o</ref>,
          uzata por ekigi agon: butono, menuo, rulumskalo ktp.
        </dif>
      </snc>
      <trd lng="en">widget</trd>
      <trdgrp lng="hu">
        <trd>ablakelem</trd>,
        <trd>eszk&ouml;zprimit&iacute;va</trd>
      </trdgrp>
    </drv>

    <drv mrk="fenest.0eto">
      <kap><tld/>eto</kap>
      <snc>
        <dif>
          Tre malgranda <tld/>o.
        </dif>
        <refgrp tip="vid">
          <ref cel="luk.0o">luko</ref>,
          <ref cel="gicxet.0o">gi&ccirc;eto</ref>.
        </refgrp>
      </snc>
      <trd lng="be">&c_a;&c_k;&c_je;&c_n;&c_c;&c_a;</trd>
      <trd lng="de">Fensterchen</trd>
      <trd lng="fr">fenestron</trd>
      <trdgrp lng="hu">
        <trd>ablakocska</trd>,
        <trd>kis <ind>ablak</ind></trd>
      </trdgrp>
      <trd lng="nl">venstertje</trd>
      <trdgrp lng="ru">
        <trd>&c_o;&c_k;&c_o;&c_sh;&c_k;&c_o;</trd>,
        <trd>&c_o;&c_k;&c_o;&c_sh;&c_je;&c_ch;&c_k;&c_o;</trd>,
        <trd>&c_f;&c_o;&c_r;&c_t;&c_o;&c_ch;&c_k;&c_a;</trd>
      </trdgrp>
    </drv>

    <drv mrk="fenest.montra0o">
      <kap>montra <tld/>o</kap>
      <snc>
        <dif>
          <tld lit="F"/>o en butiko por elmontri la komerca&jcirc;ojn. 
        </dif>
      </snc>
      <trd lng="be">&c_v;&c_ib;&c_t;&c_r;&c_y;&c_n;&c_a;</trd>
      <trd lng="de">Schaufenster</trd>
      <trd lng="fr">vitrine</trd>
      <trd lng="hu">kirakat</trd>
      <trd lng="nl">vitrine</trd>
      <trd lng="ru">&c_v;&c_i;&c_t;&c_r;&c_i;&c_n;&c_a;</trd>
    </drv>

    <drv mrk="fenest.0okruceto">
      <kap><tld/>okruceto</kap>
      <snc>
        <dif>
          &Ccirc;iu el la fostetoj, kiuj kruce dividas <tld/>on. 
        </dif>
      </snc>
      <trd lng="de">Fensterkreuz</trd>
      <trd lng="fr">croisillon<klr tip="ind"> (d'une fen&ecirc;tre)</klr></trd>
      <trd lng="hu">ablakkereszt</trd>
    </drv>

    <drv mrk="fenest.0opordo">
      <kap><tld/>opordo</kap>
      <snc>
        <dif>
          Pordo provizita per <tld/>o.  
        </dif>
      </snc>
      <trdgrp lng="fr">
        <trd>porte-fen&ecirc;tre</trd>,
        <trd>porte vitr&eacute;e</trd>
      </trdgrp>
     <trd lng="hu">&uuml;vegajt&oacute;</trd>
     <trd lng="nl">glazen <ind>deur</ind></trd>
</drv>

  </art>
