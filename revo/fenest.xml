<drv mrk="fenest.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fenest.0o.aperturo">
    <dif>
      <ref tip="super" cel="ferm.mal0ajxo.KOMUNE">Malferma&jcirc;o</ref> en 
      <ref tip="malprt" cel="mur.0o.ARKI">muro</ref> de konstrua&jcirc;o por enlasi lumon kaj aeron,
      ordinare garnita per vitra&jcirc;o:
      <ekz>
        la krajono ku&scirc;as sur la <tld/>o
        <fnt>
          <bib>F</bib>
          <lok>&FE; 6</lok>
        </fnt>;
      </ekz>
      <ekz>
        li staris tutan horon apud la <tld/>o
        <fnt>
          <bib>F</bib>
          <lok>&FE; 26</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; la <tld/>oj de sia &ccirc;ambro, malfermitaj en la direkto al Jerusalem, li tri
        fojojn &ccirc;iutage genuis kaj pre&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 6:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i malsuprenigis <klr>[la virojn]</klr> per &scirc;nuro tra la <tld/>o, &ccirc;ar &scirc;ia domo
        estis en la muro de la urbo, kaj &scirc;i lo&gcirc;is en la muro
        <fnt>
          <bib>MT</bib>
          <lok>Josuo 2:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la <tld/>o sidis junulo
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 20:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> &scirc;ovis sian viza&gcirc;on malanta&ubreve; la flankkurtenojn de la <tld/>oj,
        kie staris la belegaj floroj de &scirc;ia patrino
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        la luno brilis tra la <tld/>o sur la mezon de la planko
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la <tld/>o de la unua eta&gcirc;o pasis ombro
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        bela dometo kun ses frontaj <tld/>oj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        sen<tld/>a brikokolora muro
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la lumo, kiu envenis tra la <ind>tegment<tld/>o</ind>, nerimarkeble malintensi&gcirc;is
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La tri Magiaj Pordegoj</lok>
        </fnt>;
        <trd lng="de">Dachfenster</trd>
      </ekz>
      <ekz>
        pelu mizeron tra l' pordo, &gcirc;i revenos tra l' <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="luk.0o">luko</ref>
  </snc>
  <snc mrk="fenest.0o.baro">
    <dif>
      <ref tip="super" cel="kadr.0o.borderajxo">Kadro</ref> 
      garnita per vitra&jcirc;o(j) por fermi tian malferma&jcirc;on:
      <ekz>
        la pluvo faladis torente kaj la ventego furiozis &ccirc;e &ccirc;iuj <tld/>oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Malbonkonduta knabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li apogis la kapon al la kadro de la <tld/>o kaj profunde ek&gcirc;emis
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni malfermis la <tld/>ojn, por enlasi iom da fre&scirc;a aero
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Marta</nom> staris &ccirc;e la krad<tld/>o en la &ccirc;elo, rigardante senmove la &ccirc;ielon
        <fnt>
          <bib>Monato</bib>
          <lok><url ref="http://steloj.de/esperanto/monato2/007050.html"
              >http://steloj.de/esperanto/monato2/007050.html</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la vento fajfadis tra la krada <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la domo estas konstruita el pano kaj kovrita per kukoj, la <tld/>oj estis el
        hela sukero
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ha&ccirc;jo kaj Grenjo</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili kura&gcirc;is frapi delikate la <tld/>on
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="vitral.0o">vitralo</ref>
    <refgrp tip="vid"><ref cel="kruc.0o.objekto"><tld/>okruco</ref></refgrp>
  </snc>
  <snc mrk="fenest.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Parto de komputila <ref tip="malprt" cel="ekran.0o.vidiga">ekrano</ref>
      uzata kiel aparta ekrano:
      <ekz>
        dialoga, konzola, redakta <tld/>o;
      </ekz>
      <ekz>
        la tiama kompostilo montris en &leftquot;fenestreto&rightquot; nur la 20 laste tajpitajn signojn
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Peeraerts</aut>
          <vrk><url ref="https://www.monato.be/2004/008280.html"
              >Komposta revolucio kaj presada stabileco</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        a&ubreve;tomate malfermi&gcirc;os la retpo&scirc;ta programo kaj &gcirc;i montros <tld/>on, en kiu eblos
        tuj tajpi retmesa&gcirc;on
        <fnt>
          <bib>Monato</bib>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="https://www.monato.be/2008/009612.php"
              >Inteligenta po&scirc;amiko</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ser&ccirc;ilo alireblas per blanka <tld/>eto sube sur la ekrano
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="https://www.monato.be/2005/008753.php"
              >Google Desktop Search: ser&ccirc;ilo por la propra komputilo</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la retumilo]</klr> inkluzivas &scirc;ildon kontra&ubreve; &scirc;pruc<tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="https://www.monato.be/2005/008483.php"
              >Foliumiloj denove interbatalas</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="af">venster</trd>
