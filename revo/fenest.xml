<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>fenestr</rad>/o</kap>

<drv mrk="fenest.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fenest.0o.aperturo">
    <dif>
      Aperturo en muro de konstrua&jcirc;o por enlasi lumon kaj aeron,
      ordinare garnita per vitra&jcirc;o.
    </dif>
  </snc>
  <snc mrk="fenest.0o.baro">
    <dif>
      Kadro(j) garnita(j) per vitra&jcirc;o(j) por fermi tian aperturon.
    </dif>
  </snc>
  <snc mrk="fenest.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Parto de komputila <ref tip="malprt"
      cel="ekran.0o.vidiga">ekrano</ref>
      uzata kiel aparta ekrano:
      <ekz>
        dialoga, konzola, redakta <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_n;&c_o;</trd>
  <trd lng="bg">&c_p;&c_r;&c_o;&c_z;&c_o;&c_r;&c_je;&c_c;</trd>
  <trd lng="cs">okno</trd>
  <trd lng="en">window</trd>
  <trd lng="es">ventana</trd>
  <trd lng="fr">fen&ecirc;tre</trd>
  <trd lng="de">Fenster</trd>
  <trd lng="hu">ablak</trd>
  <trd lng="nl">venster</trd>
  <trd lng="pl">okno</trd>
  <trd lng="pt">janela</trd>
  <trd lng="ru">&c_o;&c_k;&c_n;&c_o;</trd>
  <trd lng="tp">lupa</trd>
</drv>

<drv mrk="fenest.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      En <ref tip="malprt" cel="fasad.grafika0o">grafika fasado</ref>,
      &ccirc;ia parto de <ref tip="malprt" cel="fenest.0o.KOMP">
      <tld/>o</ref>,
      uzata por ekigi agon: butono, menuo, rulumskalo ktp.
    </dif>
  </snc>
  <trd lng="en">widget</trd>
  <trdgrp lng="hu">
    <trd>ablakelem</trd>,
    <trd>eszk&ouml;zprimit&iacute;va</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kontrolka</trd>,
    <trd>aktywny element okna</trd>
  </trdgrp>
  <trd lng="ru">
  &c_u;&c_p;&c_r;&c_a;&c_v;&c_l;&c_ja;&c_ju;&c_shch;&c_i;&c_j;
  &c_e;&c_l;&c_je;&c_m;&c_je;&c_n;&c_t;
  </trd>
</drv>

<drv mrk="fenest.0eto">
  <kap><tld/>eto</kap>
  <snc>
    <dif>
      Tre malgranda <tld/>o.
    </dif>
    <refgrp tip="vid">
      <ref cel="luk.0o">luko</ref>,
      <ref cel="gicxet.0o">gi&ccirc;eto</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_je;&c_n;&c_c;&c_a;</trd>
  <trd lng="de">Fensterchen</trd>
  <trd lng="fr">fenestron</trd>
  <trdgrp lng="hu">
    <trd>ablakocska</trd>,
    <trd>kis <ind>ablak</ind></trd>
  </trdgrp>
  <trd lng="nl">venstertje</trd>
  <trdgrp lng="pl">
    <trd>okienko</trd>,
    <trd>lufcik</trd>,
    <trd>&sacute;wietlik</trd>,
    <trd>wizjer</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_k;&c_o;&c_sh;&c_k;&c_o;</trd>,
    <trd>&c_o;&c_k;&c_o;&c_sh;&c_je;&c_ch;&c_k;&c_o;</trd>,
    <trd>&c_f;&c_o;&c_r;&c_t;&c_o;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="fenest.montra0o">
  <kap>montra <tld/>o</kap>
  <snc>
    <ref tip="dif" cel="fenest.montro0o">montro<tld/>o</ref>
  </snc>
  </drv>

<drv mrk="fenest.montro0o">
  <kap>montro<tld/>o</kap>
    <snc>
    <dif> 
      <tld lit="F"/>o en butiko por elmontri la komerca&jcirc;ojn.
      <ekz>
       Infanoj &ccirc;e montrofenestro. 
<fnt><aut>K. Kalocsay</aut>, <vrk><url
ref="http://www.freeweb.hu/egalite/kalocsay/strecx/infanoj%20cxe%20montrofenestro.htm">Stre&ccirc;ita
kordo</url></vrk>,
<lok>&ccirc;apitro: Viza&gcirc;oj kaj pejza&gcirc;oj</lok></fnt>.
      </ekz></dif>
      </snc>
  <trd lng="be">&c_v;&c_ib;&c_t;&c_r;&c_y;&c_n;&c_a;</trd>
  <trd lng="de">Schaufenster</trd>
  <trdgrp lng="fr">
   <trd>vitrine</trd>,
   <trd>devanture</trd>
   </trdgrp>
  <trd lng="hu">kirakat</trd>
  <trd lng="nl">vitrine</trd>
  <trdgrp lng="pl">
    <trd>witryna</trd>,
    <trd>okno wystawowe</trd>,
    <trd>wystawa<klr> (pot.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_i;&c_t;&c_r;&c_i;&c_n;&c_a;</trd>
</drv>

<drv mrk="fenest.0okruceto">
  <kap><tld/>okruceto</kap>
  <snc>
    <dif>
      &Ccirc;iu el la fostetoj, kiuj kruce dividas <tld/>on.
    </dif>
  </snc>
  <trd lng="de">Fensterkreuz</trd>
  <trd lng="fr">croisillon<klr tip="ind"> (d'une fen&ecirc;tre)</klr>
  </trd>
  <trd lng="hu">ablakkereszt</trd>
  <trd lng="pl">szpros</trd>
  <trdgrp lng="ru">
    <trd>
    &c_o;&c_k;&c_o;&c_n;&c_n;&c_y;&c_j;
    &c_p;&c_je;&c_r;&c_je;&c_p;&c_l;&c_jo;&c_t;
    </trd>,
    <trd>&c_k;&c_r;&c_je;&c_s;&c_t;&c_o;&c_v;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="fenest.0opordo">
  <kap><tld/>opordo</kap>
  <snc>
    <dif>
      Pordo provizita per <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Fensterladen</trd>
  <trdgrp lng="fr">
    <trd>porte-fen&ecirc;tre</trd>,
    <trd>porte vitr&eacute;e</trd>
  </trdgrp>
  <trd lng="hu">&uuml;vegajt&oacute;</trd>
  <trd lng="nl">glazen <ind>deur</ind></trd>
  <trdgrp lng="pl">
    <trd>przeszklone <ind>drzwi</ind></trd>,
    <trd>oszklone <ind>drzwi</ind></trd>
  </trdgrp>
  <trd lng="ru">
  &c_z;&c_a;&c_s;&c_t;&c_je;&c_k;&c_l;&c_jo;&c_n;&c_n;&c_a;&c_ja;
  &c_d;&c_v;&c_je;&c_r;&c_mol;
  </trd>
</drv>

<drv mrk="fenest.sxpruc0o">
  <kap>&scirc;pruc<tld/>o</kap>
  <snc>
  <uzo tip="fak">KOMP</uzo>
    <dif>
      Tiu <ref cel="fenest.0o.KOMP"><tld/>o</ref> de komputila
      <ref tip="malprt" cel="ekran.0o.vidiga">ekrano</ref>, kiu
      subite aperas, plej ofte neatendite kaj kun reklamo, kaj parte
      kovras la aliajn <tld/>ojn.
      <ekz>
        Firefox estas kompakta versio de la programo Mozilo. &Gcirc;i
        inkluzivas &scirc;ildon kontra&ubreve; &scirc;pruc<tld/>oj,
        Guglo-ser&ccirc;ejon kaj la eblon navigi per
        <ref tip="vid" cel="lang.0eto">langetoj</ref>
        <fnt><aut>Marko Naoki LINS</aut>:
        <vrk>Foliumiloj denove interbatalas</vrk>,
        <bib>Monato</bib><lok>jaro 2005, numero 1a, p. 17a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Pop-up-Fenster</trd>
  <trd lng="en">pop-up window</trd>
  <trd lng="es">ventana emergente</trd>
  <trd lng="nl">pop-upvenster</trd>
  <trdgrp lng="pl">
    <trd>wyskakuj&aogonek;ce <ind>okienko</ind></trd>,
    <trd>pop-up</trd>
  </trdgrp>
  <trd lng="pt">janela pop-up</trd>
  <trd lng="ru">
  &c_v;&c_s;&c_p;&c_l;&c_y;&c_v;&c_a;&c_ju;&c_shch;&c_je;&c_je;
  &c_o;&c_k;&c_n;&c_o;
  </trd>
</drv>
</art>
