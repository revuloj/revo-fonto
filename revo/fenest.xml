  <trd lng="sk">okienko</trd>
</drv>

<drv mrk="fenest.el0igxi">
  <kap>el<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="fenest.el0igxi.KOMUNE">
    <dif>
      Eli&gcirc;i tra <tld/>o (kaj eventuale fali):
      <ekz>
        li sonorigas &ccirc;e la pordo, kaj tiam el<tld/>i&gcirc;as kapo de
        sinjorino
        <fnt>
          <aut>J. Shemer</aut>,
          <vrk>Paka&jcirc;o por S-ino Levi</vrk>,
          <lok><url ref="http://www.reu.ru/prenu/rego36.pdf">
          REGo, 2006-10 (36)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        amiko <klr>[&#8230;]</klr> el<tld/>i&gcirc;is, sin mortigante!
        <fnt>
          <aut>A. Zecchin</aut>,
          <vrk><url ref="http://www.ueci.it/eldonoj/verkoj/la_hknstsm.pdf">
          La Homo kiu ne Sukcesis Trompi sin mem</url></vrk>,
          <lok>[2012]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>se <ind>d&eacute;fenestrer</ind></trd>,
    <trd>sortir par la <ind>fen&ecirc;tre</ind></trd>
  </trdgrp>
</drv>

<drv mrk="fenest.montro0o">
  <kap>
    montro<tld/>o,
    <var><kap>montra <tld/>o</kap></var>,
    <var><kap>ekspozicia <tld/>o</kap></var>
  </kap>
  <snc>
    <dif> 
      <tld lit="F"/>o en butiko por elmontri la komerca&jcirc;ojn:
      <ekz>
        infanoj &ccirc;e montro<tld/>o 
        <fnt>
          <aut>K. Kalocsay</aut>, 
          <vrk><url ref="http://www.freeweb.hu/egalite/kalocsay/strecx/infanoj%20cxe%20montrofenestro.htm">Stre&ccirc;ita
            kordo</url>
          </vrk>,
          <lok>&ccirc;apitro: Viza&gcirc;oj kaj pejza&gcirc;oj</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni frakasis <tld/>ojn de kelkaj bankoj kaj la montro<tld/>ojn de du
        <klr>[&#8230;]</klr> rapidman&gcirc;ejoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007258.html"
              >Mondaj Financoj: Post la kunsido de financistoj en Prago</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la ekspoziciaj <tld/>oj de la magazenoj aspektis tre bele
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_t;&c_r;&c_y;&c_n;&c_a;</trd>
  <trd lng="bg">&c_v;&c_i;&c_t;&c_r;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>v&yacute;klad</trd>,
    <trd>v&yacute;kladn&iacute; sk&rcaron;&iacute;&ncaron;</trd>,
    <trd>v&yacute;loha</trd>
  </trdgrp>
  <trd lng="de">Schaufenster</trd>
  <trd lng="en">display window</trd>
  <trdgrp lng="fr">
    <trd>vitrine</trd>,
    <trd>devanture</trd>
  </trdgrp>
  <trd lng="hu">kirakat</trd>
  <trd lng="nl">vitrine</trd>
  <trdgrp lng="pl">
    <trd>witryna</trd>,
    <trd>okno wystawowe</trd>,
    <trd>wystawa<klr> (pot.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_i;&c_t;&c_r;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">v&yacute;klad</trd>
</drv>

<drv mrk="fenest.0opordo">
  <kap><tld/>opordo</kap>
  <snc>
    <dif>
      Pordo provizita per <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>balk&oacute;nov&eacute; dve&rcaron;e</trd>,
    <trd>prosklen&eacute; dve&rcaron;e</trd>
  </trdgrp>
  <trd lng="de">Fensterladen</trd>
  <trd lng="en">French window</trd>
  <trdgrp lng="fr">
    <trd>porte-fen&ecirc;tre</trd>,
    <trd>porte vitr&eacute;e</trd>
  </trdgrp>
  <trd lng="hu">&uuml;vegajt&oacute;</trd>
  <trd lng="nl">glazen <ind>deur</ind></trd>
  <trdgrp lng="pl">
    <trd>przeszklone <ind>drzwi</ind></trd>,
    <trd>oszklone <ind>drzwi</ind></trd>
