<drv mrk="fenest.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      En <ref tip="malprt" cel="fasad.grafika0o">grafika fasado</ref>,
      &ccirc;ia parto de <ref tip="malprt" cel="fenest.0o.KOMP">
      <tld/>o</ref>, uzata por ekigi agon: butono, menuo, rulumskalo ktp.:
      <ekz>
        la <tld/>a&jcirc;o estas akompanata de teksta prezento
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=109813">Plenumskalo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_r;&c_o;&c_w;&c_n;&c_y; <ind>&c_e;&c_l;&c_je;&c_m;&c_e;&c_n;&c_t;</ind>
