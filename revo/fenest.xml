<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fenestr</rad>/o
</kap>
<drv mrk="fenest.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Aperturo en muro de konstrua&jcirc;o por enlasi lumon k aeron,
      ordinare garnita per vitra&jcirc;o. 
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Kadro(j) garnita(j) per vitra&jcirc;o(j) por fermi tian aperturon. 
    </dif>
  </snc>
  <trd lng="cs">okno</trd>
  <trd lng="en">window</trd>
  <trd lng="fr">fen&ecirc;tre</trd>
  <trd lng="de">Fenster</trd>
  <trd lng="nl">venster</trd>
  <trd lng="pl">okno</trd>
  <trd lng="ru">&c_o;&c_k;&c_n;&c_o;</trd>
</drv>
<drv mrk="fenest.0eto">
  <kap><tld/>eto</kap>
  <snc>
    <dif>
      Tre malgranda <tld/>o 
    </dif>
    <refgrp tip="sin">
      <ref cel="luk.0o">luko</ref>,
      <ref cel="gicxet.0o">gi&ccirc;eto</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Fensterchen</trd>
  <trd lng="fr">fenestron</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_k;&c_o;&c_sh;&c_k;&c_o;</trd>,
    <trd>&c_o;&c_k;&c_o;&c_sh;&c_je;&c_ch;&c_k;&c_o;</trd>,
    <trd>&c_f;&c_o;&c_r;&c_t;&c_o;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
<drv mrk="fenest.montra0o">
  <kap>montra <tld/>o</kap>
  <snc>
    <dif>
      <tld/>o en butiko por elmontri la komerca&jcirc;ojn. 
    </dif>
  </snc>
  <trd lng="de">Schaufenster</trd>
  <trd lng="fr">vitrine</trd>
  <trd lng="ru">&c_v;&c_i;&c_t;&c_r;&c_i;&c_n;&c_a;</trd>
</drv>
<drv mrk="fenest.0okruceto">
  <kap><tld/>okruceto</kap>
  <snc>
    <dif>
      &Ccirc;iu el la fostetoj, kiuj kruce dividas <tld/>on. 
    </dif>
  </snc>
  <trd lng="de">Fensterkreuz</trd>
</drv>
<drv mrk="fenest.0opordo">
  <kap><tld/>opordo</kap>
  <snc>
    <dif>
      Pordo provizita per <tld/>o.  
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>porte fen&ecirc;tre</trd>,
    <trd>porte vitr&eacute;e</trd>
  </trdgrp>
</drv>
</art>
