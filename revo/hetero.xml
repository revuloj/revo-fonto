<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>heterogen</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="hetero.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">SCI</uzo>
  <snc>
    <dif>
      De malsama naturo, de malsama deveno:
      <ekz>
        <tld/>aj ecoj, principoj;
      </ekz>
      <ekz>
        korpo konsistanta el <tld/>aj elementoj.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="homoge.0a">homogena</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_je;&c_t;&c_e;&c_r;&c_a;&c_g;&c_je;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_n;&c_a;&c_r;&c_o;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_a;&c_d;&c_n;&c_a;&c_r;&c_o;&c_d;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>heterogen</trd>,
    <trd>ungleichartig</trd>,
    <trd>verschiedenartig</trd>
  </trdgrp>
  <trd lng="el">&epsilon;&tau;&epsilon;&rho;&omikron;&gamma;&epsilon;&ny;&eta_ton;&sigma_fina;</trd>
  <trd lng="es">heterog&eacute;neo</trd>
  <trd lng="fr">h&eacute;t&eacute;rog&egrave;ne</trd>
  <trd lng="hu">heterog&eacute;n</trd>
  <trdgrp lng="nl">
    <trd>heterogeen</trd>,
    <trd>ongelijksoortig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>heterogenicny</trd>,
    <trd>r&oacute;&zdot;norodny</trd>,
    <trd>niejednorodny</trd>
  </trdgrp>
  <trd lng="pt">heterog&ecirc;nio</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_n;&c_o;&c_r;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_g;&c_je;&c_t;&c_je;&c_r;&c_o;&c_g;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
