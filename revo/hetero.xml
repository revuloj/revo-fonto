<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>heterogen</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="hetero.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">SCI</uzo>
  <snc>
    <dif>
      De malsama naturo, de malsama deveno:
      <ekz><tld/>aj ecoj, principoj;</ekz>
      <ekz>korpo konsistanta el <tld/>aj elementoj.</ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="homoge.0a">homogena</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>heterogen</trd>,
     <trd>ungleichartig</trd>,
     <trd>verschiedenartig</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_n;&c_o;&c_r;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_g;&c_je;&c_t;&c_je;&c_r;&c_o;&c_g;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
