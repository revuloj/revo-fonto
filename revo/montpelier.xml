  <art mrk="$Id: montpelier.xml,v 1.5 2024/09/24 08:48:18 afido Exp $">
    <kap><ofc>10</ofc>
      <rad>Montpelier</rad>/o
    </kap>

<drv mrk="montpelier.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Franca <ref tip="lst" lst="voko:urboj" cel="urb.0o">urbo</ref>
      &ccirc;e Mediteraneo, &ccirc;. 200&#x202F;km oriente de Tuluzo:
      <ekz>
        la 83a UK en <tld/>o
        <fnt>
          <bib>LOdE</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="https://www.monato.be/2015/011584.php">Ru&gcirc;aj &scirc;limoj en
          Mediteraneo</url></vrk>
          <lok>1999, No 1 (51)</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lasta fojo, kiam mi vidis kaj parolis kun li, estis en <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj'</aut>
          <vrk><url ref="https://www.monato.be/2014/011373.php">Geraldo Mattos kaj Sutte
          All&aacute;</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        enmarigataj <klr>[&#x2026;]</klr> pezaj metaloj <klr>[&#x2026;]</klr> jam senvivigis grandan
        zonon inter &Gcirc;enovo en Italio kaj <tld/>o en Francio
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="https://www.monato.be/2015/011584.php">Ru&gcirc;aj &scirc;limoj en
          Mediteraneo</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Montpellier</trd>
    <trd lng="en">Montpellier</trd>
    <trd lng="fr">Montpellier</trd>
  </snc>
  <trd lng="be">&c_M;&c_a;&c_n;&c_p;&c_e;&c_l;&c_mol;&c_je;</trd>
  <trd lng="ja">&#x30E2;&#x30F3;&#x30DA;&#x30EA;&#x30A8;</trd>
  <trd lng="zh">&#x8499;&#x5F7C;&#x5229;&#x57C3; <pr>M&#xE9;ngb&#x1D0;l&#xEC;&amacron;i</pr></trd>
</drv>

