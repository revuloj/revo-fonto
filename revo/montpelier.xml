  <art mrk="$Id: montpelier.xml,v 1.2 2023/09/02 06:04:14 afido Exp $">
    <kap>
      <rad>Montpelier</rad>/o
    </kap>

<drv mrk="montpelier.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Franca <ref tip="lst" lst="voko:urboj" cel="urb.0o">urbo</ref>
      &ccirc;e Mediteraneo, &ccirc;. 200&#x202F;km oriente de Tuluzo:
      <ekz>
        la 83a UK en <tld/>o
        <fnt>
          <bib>LOdE</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="https://www.monato.be/2015/011584.php">Ru&gcirc;aj &scirc;limoj en Mediteraneo</url></vrk>
          <lok>1999, No 1 (51)</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lasta fojo, kiam mi vidis kaj parolis kun li, estis en <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj'</aut>
          <vrk><url ref="https://www.monato.be/2014/011373.php">Geraldo Mattos kaj Sutte All&aacute;</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        enmarigataj <klr>[&#x2026;]</klr> pezaj metaloj <klr>[&#x2026;]</klr> jam senvivigis grandan
        zonon inter &Gcirc;enovo en Italio kaj <tld/>o en Francio
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="https://www.monato.be/2015/011584.php">Ru&gcirc;aj &scirc;limoj en Mediteraneo</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Montpellier</trd>
    <trd lng="en">Montpellier</trd>
    <trd lng="fr">Montpellier</trd>
  </snc>
  <trd lng="ja">&#x30E2;&#x30F3;&#x30DA;&#x30EA;&#x30A8;</trd>
</drv>

  </art>

