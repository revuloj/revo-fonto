<art mrk="$Id: inkviz1.xml,v 1.23 2023/10/17 16:46:14 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>inkvizitor</rad>/o
</kap>
<drv mrk="inkviz1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KAT</uzo>
  <uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      Membro de la <ref tip="vid" cel="inkviz.0o">inkvizicia</ref>
      tribunalo:
      <ekz>
        en Hispanujo la granda <tld/>o Bartolomeo en la jaro 1659 malpermesis al la
        pastroj fumi tabakon en la da&ubreve;ro de unu horo anta&ubreve; la Diservo kaj du horoj
        post la Diservo, kaj sub minaco de ekskomuniko kaj de la plej severaj punoj
        malpermesis al &ccirc;iuj flari tabakon en la da&ubreve;ro de la Diservo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la bogomiloj ne estis &leftquot;kataroj&rightquot; (escepte en la menso de la <tld/>oj) kaj la
        bosniaj &leftquot;herezuloj&rightquot; neniam nomis sin &leftquot;bogomiloj&rightquot;
        <fnt>
        <aut>Michel Duc Goninaz</aut>
        <vrk><url ref="&Monato;/2000/006947.html">Eks-Jugoslavio: 
          kiu kiun mistifikas?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 8</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_k;&c_v;&c_ib;&c_z;&c_ib;&c_t;&c_a;&c_r;
  </trd>
  <trd lng="cs">inkvizitor</trd>
  <trd lng="de">Inquisitor</trd>
  <trd lng="en">inquisitor</trd>
  <trd lng="es">inquisidor</trd>
  <trd lng="fr">inquisiteur</trd>
  <trd lng="hu">inkvizitor</trd>
  <trd lng="it">inquisitore<klr> (membro dell'Inquisizione)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x7570;&#x7AEF;&#x5BE9;&#x554F;&#x5B98;  <pr>&#x3044;&#x305F;&#x3093;&#x3057;&#x3093;&#x3082;&#x3093;&#x304B;&#x3093;</pr></trd>,
    <trd>&#x5B97;&#x6559;&#x88C1;&#x5224;&#x5B98;  <pr>&#x3057;&#x3085;&#x3046;&#x304D;&#x3087;&#x3046;&#x3055;&#x3044;&#x3070;&#x3093;&#x304B;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">inquisiteur</trd>
  <trd lng="pl"><klr>&sacute;wi&eogonek;ty </klr>inkwizytor</trd>
  <trd lng="pt">inquisidor</trd>
  <trd lng="ru">&c_i;&c_n;&c_k;&c_v;&c_i;&c_z;&c_i;&c_t;&c_o;&c_r;</trd>
  <trd lng="sk">inkviz&iacute;tor</trd>
  <trd lng="uk">&c_ib;&#x43D;&#x43A;&c_v;&c_ib;&c_z;&c_i;&c_t;&#x43E;&c_r;</trd>
</drv>
</art>
