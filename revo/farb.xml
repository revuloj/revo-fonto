<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: farb.xml,v 1.32 2022/04/27 17:19:11 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>farb</rad>/o <fnt><bib>Fab1</bib></fnt>
</kap>
<drv mrk="farb.0o">
  <kap><tld/>o</kap>
  <snc mrk="farb.0o.KOL">
    <dif>
      Miksa&jcirc;o, konsistanta el likva&jcirc;o (ordinare oleo) kaj
      koloraj substancoj, uzata por &scirc;miri surfacon kaj &gcirc;in
      kolorigi:
      <ekz>
        mirindaj kreska&jcirc;oj, tiaj, kiajn oni vidas pentritajn per
        <tld/>oj kaj oro sur la rando de malnovaj libroj
        <fnt><bib>Fab1</bib>, <lok>&Gcirc;ardeno de la Paradizo</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[la tabakbutiko]</klr> odoris je la fre&scirc;a <tld/>o
        <fnt><bib>UrdHadda</bib>, <lok>p. 14a</lok></fnt>;
      </ekz>
      <ekz>
        helpu al la sinjoro <klr>[pentristo]</klr> porti lian keston kun
        <tld/>oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        granda, ronda horlo&gcirc;o sur la blank<tld/>a <klr>(blanke
        farbita)</klr> muro malrapide fortiktakis la minutojn
        <fnt><bib>UrdHadda</bib>, <lok>p. 5a</lok></fnt>;
      </ekz>
      <ekz>
        li &scirc;miris al si la viza&gcirc;on per bruna kaj nigra <tld/>o
        <klr>(vd <ref cel="sxmink.0o">&scirc;minko</ref>)</klr>
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tisto de porkoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen li staris kun sia <tld/>ujo, kiu estis lia tuta voja&gcirc;a
        paka&jcirc;o
        <fnt>
          <bib>Fab4</bib>
          <lok>Dekdu per la po&scirc;to</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>opistolo
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2009/009838.php">Kia konsekvenco!</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kolor.0ilo">kolorilo</ref>,
      <ref cel="kolor.0igilo">kolorigilo</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_f;&c_a;&c_r;&c_b;&c_a;</trd>,
      <trd>&c_f;&c_a;&c_r;&c_b;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>pintura</trd>,
      <trd>color</trd>
    </trdgrp>
    <trd lng="de">Farbe</trd>
    <trd lng="en">paint</trd>
    <trdgrp lng="fr">
      <trd>peinture<klr> (produit &agrave; base de pigments)</klr></trd>,
      <trd>couleur<klr> (peinture)</klr></trd>
    </trdgrp>
    <trd lng="hu">fest&eacute;k</trd>
    <trd lng="nl">verf</trd>
  <trdgrp lng="pt">
    <trd>tinta</trd>,
    <trd>mat&eacute;ria <ind>colorante</ind></trd>,
    <trd>corante</trd>
  </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_a;&c_s;&c_k;&c_a;</trd>,
      <trd>&c_k;&c_r;&c_a;&c_s;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
    </trdgrp>
    <trd lng="tr">boya</trd>
  </snc>
  <trd lng="bo">&#xF5A;&#xF7C;&#xF53;&#xF0B;</trd>
  <trd lng="cs">barva <klr>(n&aacute;t&ecaron;r.hmota)</klr></trd>
  <trd lng="es">pintura</trd>
  <trd lng="id">cat</trd>
  <trd lng="sk">farba</trd>
  <trd lng="tok">kule</trd>
  <trdgrp lng="uk">
    <trd>&c_f;&c_a;&c_r;&c_b;&c_a;</trd>,
    <trd>&c_b;&c_a;&c_r;&c_v;&#x43D;&c_i;&#x43A;</trd>
  </trdgrp>
</drv>

<drv mrk="farb.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="farb.0i.KOL">
    <dif>
      <ref tip="super" cel="kolor.0igi">Kolorigi</ref>
      per <tld/>o:
      <ekz>
        <klr>[la boato]</klr> estis <tld/>ita ru&gcirc;e kaj blanke
        <fnt><bib>Fab2</bib>, <lok>Dio de Dormo</lok></fnt>;
      </ekz>
      <ekz>
        bele <tld/>ita <klr>(&scirc;minkita)</klr> virina viza&gcirc;o;
      </ekz>
      <ekz>
        blonde <tld/>ita hararo
        <fnt>
          <bib>Monato</bib>
          <aut>Juh&aacute;sz K&aacute;roly</aut>
          <vrk><url ref="&Monato;/2006/008964.php">Konvinka venko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi a&ccirc;etis la domon el la propra po&scirc;o kaj mi mem
        re<tld/>igis &ccirc;ion nova
        <fnt><bib>UrdHadda</bib>, <lok>p. 14a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="pentr.0i.reprezenti">pentri</ref>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_r;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>colorar</trd>,
    <trd>pintar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>barvit</trd>,
    <trd>nabarvit</trd>,
    <trd>nat&iacute;rat barvou</trd>,
    <trd>zbarvit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>anmalen</trd>,
    <trd>anstreichen</trd>,
    <trd>f&auml;rben</trd>,
    <trd>schminken</trd>
  </trdgrp>
  <trd lng="en">paint</trd>
  <trd lng="es">pintar</trd>
  <trdgrp lng="fr">
    <trd>peindre<klr tip="ind"> (recouvrir de peinture)</klr></trd>,
    <trd>farder</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fest</trd>,
    <trd>m&aacute;zol</trd>
  </trdgrp>
  <trd lng="id">menge<ind>cat</ind></trd>
  <trdgrp lng="nl">
    <trd>verven</trd>,
    <trd>schilderen</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_r;&c_a;&c_s;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>farbi&tcaron;</trd>,
    <trd>natiera&tcaron;</trd>
  </trdgrp>
  <trd lng="tok">kule</trd>
  <trd lng="uk">&c_f;&c_a;&c_r;&c_b;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="farb.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="farb.0isto.KON">
    <uzo tip="fak">KON</uzo>
    <dif>
      Homo, kiu <ref tip="lst" lst="voko:metioj" cel="meti.0o">metie</ref> &scirc;miras murojn, vandojn,
      plafonojn;
      <ref tip="sin" cel="kolor.dom0igisto.KON">domkolorigisto</ref>:
      <ekz>
        veldistoj, masonistoj, frizistoj, dom<tld/>istoj ktp perlaboras
        multe pli ol
        altlerneje edukitaj instruistoj
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2012/010868.php">Lernejo baza, salajro
          baza</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        infanoj el bazlernejoj ne plu volas fari&gcirc;i <klr>[&#8230;]</klr>
        <tld/>isto,
        tegmentisto a&ubreve; terkulturisto
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2014/011284.php">Batalo por la studontoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li ellernis la <tld/>istan metion <klr>[&#8230;]</klr> kaj poste
        laboris kiel
        <tld/>isto-kolorigisto en diversaj konstrufirmaoj en Vieno
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2014/011335.php">Pentristo de la homo kaj de la
          naturo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="pentr.0isto">pentristo</ref>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_ja;&c_r;</trd>
  <trd lng="ca">pintor <klr>(parets)</klr></trd>
  <trd lng="cs">nat&ecaron;ra&ccaron;</trd>
  <trd lng="de">Maler <klr>(Handwerker)</klr></trd>
  <trd lng="en">painter</trd>
  <trd lng="es">pintor</trd>
  <trd lng="fr">peintre en b&acirc;timent</trd>
  <trdgrp lng="hu">
    <trd>fest&odblac;<klr tip="ind"> (m&aacute;zol&oacute;)</klr></trd>,
    <trd>m&aacute;zol&oacute;</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>penge<ind>cat</ind></trd>,
    <trd><ind>tukang</ind> cat</trd>
  </trdgrp>
  <trd lng="nl">huisschilder</trd>
  <trd lng="ru">&c_m;&c_a;&c_l;&c_ja;&c_r;</trd>
  <trd lng="sk">farbiar</trd>
  <trdgrp lng="uk">
    <trd>&c_f;&c_a;&c_r;&c_b;&c_a;&c_r;</trd>,
    <trd>&#x43C;&c_a;&#x43B;&#x44F;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="farb.akril0o">
  <kap>akril<tld/>o</kap>
  <snc mrk="farb.akril0o.KOL">
    <dif>
      <ref tip="super" cel="farb.0o.KOL"><tld lit="F"/>o</ref>
      por mura farbado kaj por pentrado, rapide seki&gcirc;anta,
      en kiu ligento estas emulsio de akrilo en akvo:
      <ekz>
        li voja&gcirc;as al <nom>New York</nom>, tie, en 1965, li malkovras
        novan teknikon, kiu estas akril<tld/>o
        <fnt>
          <aut>M. Chesnelle</aut>
          <vrk><url ref="https://veropedio.blogspot.com/2014/02/alechinsky-pierre.html">
          Alechinsky Pierre</url></vrk>
          <lok>Veropedio, [vidita en 2018]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_r;&c_y;&c_l;&c_a;&c_v;&c_a;&c_ja; &c_f;&c_a;&c_r;&c_b;&c_a;</trd>
  <trd lng="fr">peinture <ind>acrylique</ind></trd>
</drv>
</art>
