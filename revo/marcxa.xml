<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: marcxa.xml,v 1.31 2024/01/25 20:10:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>mar&ccirc;and</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="marcxa.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="marcxa.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <uzo tip="klr">(pri vendanto a&ubreve; a&ccirc;etanto)</uzo>
    <dif>
      Diskuti por atingi interkonsentitan prezon:
      <ekz>
        &scirc;i iam diris, ke por akiri ion oni devas kelkfoje
        <tld/>i per kiu ajn metodo
        <fnt><aut>W. De Smet</aut>: <vrk>Kiu murdis Dian
        Fossey?</vrk>, <bib>Monato</bib> <lok>1995:12,
        p. 23a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="negoc.0i">negoci</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_r;&c_g;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trd lng="br">marc'hata</trd>
  <trd lng="de">feilschen</trd>
  <trdgrp lng="en">
    <trd>bargain</trd>,
    <trd>haggle</trd>
  </trdgrp>
  <trd lng="fa">&f_ch;&a_A;&a_n;&a_h; &a_z;&a_d;&a_n;</trd>
  <trd lng="fr">marchander</trd>
  <trd lng="hu">alkudozik</trd>
  <trdgrp lng="ja">
    <trd>&#x5024;&#x5207;&#x308B;  <pr>&#x306D;&#x304E;&#x308B;</pr></trd>,
    <trd>&#x5024;&#x5F15;&#x304D;&#x4EA4;&#x6E09;&#x3059;&#x308B;  <pr>&#x306D;&#x3073;&#x304D;&#x3053;&#x3046;&#x3057;&#x3087;&#x3046;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>marchanderen<klr tip="ind"> (ww.)</klr></trd>,
    <trd>afdingen<klr tip="ind"> (ww.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>negocjowa&cacute;</trd>,
    <trd>targowa&cacute; si&eogonek;</trd>,
    <trd>dokonywa&cacute; <ind>transakcji</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>regatear</trd>,
    <trd>pechinchar</trd>,
    <trd>barganhar</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>negociaz&abreve;</trd>,
    <trd>negu&#x21B;a</trd>,
    <trd>face <ind>tranzac&#x21B;ii</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_o;&c_r;&c_g;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="uk">&c_t;&#x43E;&c_r;&c_g;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F; (&#x43F;&c_r;&#x43E; &c_c;&c_ib;&#x43D;&c_u;)</trd>
  <trdgrp lng="zh">
    <trd>&#x8FD8;&#x4EF7; <pr>hu&#xE1;nji&#xE0;</pr></trd>,
    <trd>&#x9084;&#x50F9; <pr>hu&#xE1;nji&#xE0;</pr></trd>,
    <trd>&#x8A0E;&#x50F9;&#x9084;&#x50F9; <pr>t&#x1CE;oji&#xE0;hu&#xE1;nji&#xE0;</pr></trd>,
    <trd>&#x8BA8;&#x4EF7;&#x8FD8;&#x4EF7; <pr>t&#x1CE;oji&#xE0;hu&#xE1;nji&#xE0;</pr></trd>,
    <trd>&#x780D;&#x4EF7; <pr>k&#x1CE;nji&#xE0;</pr></trd>,
    <trd>&#x780D;&#x50F9; <pr>k&#x1CE;nji&#xE0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="marcxa.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="marcxa.0ado.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Diskutado pri prezo de komerca&jcirc;o anta&ubreve;
      a&ccirc;etdecido:
      <ekz>
        <tld/>ado a&ccirc;eti ne devigas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_o;&c_r;&c_g;</trd>
  <trd lng="br">marc'hatadenn</trd>
  <trdgrp lng="de">
    <trd>Feilschen</trd>,
    <trd>Feilscherei</trd>,
    <trd>Gefeilsche</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bargaining</trd>,
    <trd>haggling</trd>
  </trdgrp>
  <trd lng="fa">&f_ch;&a_A;&a_n;&a_h;&zwnj;&a_z;&a_n;&f_y;</trd>
  <trd lng="fr">marchandage</trd>
  <trd lng="hu">alkudoz&aacute;s</trd>
  <trdgrp lng="nl">
    <trd>marchanderen<klr tip="ind"> (nw.)</klr></trd>,
    <trd>afdingen<klr tip="ind"> (nw.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>negocjacje</trd>,
    <trd>targi</trd>,
    <trd>targowanie si&eogonek;</trd>
  </trdgrp>
  <trd lng="pt">regateio</trd>
  <trdgrp lng="ro">
    <trd>negociere</trd>,
    <trd>t&acirc;rg</trd>,
    <trd>&#x21B;ig&abreve;ni</trd>
  </trdgrp> 
  <trd lng="ru">&c_t;&c_o;&c_r;&c_g;</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&#x43E;&c_r;&c_g;</trd>,
    <trd>&c_t;&#x43E;&c_r;&c_g;&c_u;&c_v;&c_a;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="marcxa.ne0ema">
  <kap>ne<tld/>ema</kap>
  <snc mrk="marcxa.ne0ema.EKON">
    <dif>
      Ne volonte <tld/>anta,
      konservanta la unuan prezon:
      <ekz>
        Petrovi&ccirc; estis, &scirc;ajne, sobra kaj tial severa, 
        ne<tld/>ema
        <fnt>
          <aut>N. Gogol, trad. A. Kor&jcirc;enkov</aut>,
          <vrk><url ref="&LOdE;Rn-gogl2.htm">
          Palto</url></vrk>,
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_k;&c_ib; &c_n;&c_je; &c_p;&c_a;&c_d;&c_l;&c_ja;&c_g;&c_a;&c_je; <ind>&c_a;&c_b;&c_m;&c_je;&c_r;&c_k;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ju;</ind></trd>
  <trd lng="fr">intraitable<klr> (en affaires)</klr></trd>
  <trdgrp lng="pl">
    <trd>bez <ind>negocjacji</ind></trd>,
    <trd>nie podlegaj&aogonek;ce <ind>negocjacjom</ind></trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>f&abreve;r&abreve; <ind>negocieri</ind></trd>,
    <trd>non-negociabil</trd>
  </trdgrp>
</drv>
</art>
