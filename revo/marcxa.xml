<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mar&ccirc;and</rad>/i
</kap>
<drv mrk="marcxa.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Diskuti pri la prezo de io por a&ccirc;eti &gcirc;in pli malkare:
      <ekz>
        &scirc;i iam diris, ke por akiri ion oni devas kelkfoje <tld/>i per
        kiu
        ajn metodo<fnt><aut>Wim De Smet</aut>: <vrk>Kiu murdis Dian
        Fossey?</vrk>, <bib>Monato</bib> <lok>jaro 1995a, numero 12a,
p. 23a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_r;&c_g;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trd lng="br">marc'hata</trd>
  <trd lng="de">feilschen</trd>
  <trdgrp lng="en">
    <trd>bargain</trd>,
    <trd>haggle</trd>
  </trdgrp>
  <trd lng="fa">&f_ch;&a_A;&a_n;&a_h; &a_z;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>marchander</trd>
  </trdgrp>
  <trd lng="hu">alkudozik</trd>
  <trdgrp lng="nl">
    <trd>marchanderen<klr tip="ind"> (ww.)</klr></trd>,
    <trd>afdingen<klr tip="ind"> (ww.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>regatear</trd>,
    <trd>pechinchar</trd>,
    <trd>barganhar</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_o;&c_r;&c_g;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="marcxa.0ado">
  <kap><tld/>ado</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Diskutado pri prezo de komerca&jcirc;o anta&ubreve;
      a&ccirc;etdecido:
      <ekz>
        <tld/>ado a&ccirc;eti ne devigas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_o;&c_r;&c_g;</trd>
  <trd lng="br">marc'hatadenn</trd>
  <trdgrp lng="de">
    <trd>Feilschen</trd>,
    <trd>Feilscherei</trd>,
    <trd>Gefeilsche</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bargaining</trd>,
    <trd>haggling</trd>
  </trdgrp>
  <trd lng="fa">&f_ch;&a_A;&a_n;&a_h;&zwnj;&a_z;&a_n;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>marchandage</trd>
  </trdgrp>
  <trd lng="hu">alkudoz&aacute;s</trd>
  <trdgrp lng="nl">
    <trd>marchanderen<klr tip="ind"> (nw.)</klr></trd>,
    <trd>afdingen<klr tip="ind"> (nw.)</klr></trd>
  </trdgrp>
  <trd lng="pt">regateio</trd>
  <trd lng="ru">&c_t;&c_o;&c_r;&c_g;</trd>
</drv>
</art>
