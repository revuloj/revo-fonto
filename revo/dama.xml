<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>dama</rad>/o
</kap>
<drv mrk="dama.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio de <ref tip="super" cel="cerved.0oj">cervedoj</ref>,
      (<trd lng="la">Dama</trd>), kun palmoforma kornaro kaj
      makulpunktita somera felo.
    </dif>
  </snc>
  <bld lok="../bld/dama.jpg">
    <tld/>o kun vintra felo
  </bld>
  <trd lng="de">Damwild</trd>
  <trd lng="en">fallow <ind>deer</ind></trd>
  <trdgrp lng="fr">
    <trd>daim</trd>,
    <trd>daine</trd>
  </trdgrp>
  <trd lng="he">&yod;&het;&mem;&vav;&resh;</trd>
  <trdgrp lng="hu">
    <trd>d&aacute;mszarvas</trd>,
    <trd>d&aacute;mvad</trd>
  </trdgrp>
  <trd lng="nl">damhert</trd>
  <trd lng="ru">&c_l;&c_a;&c_n;&c_mol;</trd>
</drv>
</art>
