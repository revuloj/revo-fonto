<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>somnambul</rad>/o
</kap>
<drv mrk="somnam.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Homo nerve malsana, kiu dum dormo senkonscie kaj senmemore iras,
      parolas kaj agas kvaza&ubreve; li estus maldormanta kaj konscia.
    </dif>
    <refgrp tip="vid">
      <ref cel="lunati.0o">lunatiko</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_m;&c_n;&c_a;&c_m;&c_b;&c_u;&c_l;&c_a;</trd>
  <trd lng="de">Schlafwandler</trd>
  <trd lng="nl">slaapwandelaar</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_m;&c_n;&c_a;&c_m;&c_b;&c_u;&c_l;&c_a;</trd>,
    <trd>&c_l;&c_u;&c_n;&c_a;&c_t;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
