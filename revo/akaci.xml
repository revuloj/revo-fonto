<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: akaci.xml,v 1.36 2023/08/30 18:23:31 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>akaci</rad>/o
</kap>
<drv mrk="akaci.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Acacia</trd>) kun sescento da arbaj kaj arbustaj
      specioj ofte dornaj,
      apartenantaj al subfamilio de 
      <ref tip="lst" cel="mimoz.0o" lst="voko:faboparencoj">mimozoj</ref>:
      <ekz>
        sole la&ubreve;bo el <tld/>oj kaj siringoj donis belan ombron
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 5a, p. 21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_a;&c_k;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="br">
   <trd>kasiaenn</trd>,
   <trd>gwezenn-gasia</trd>
  </trdgrp>
  <trd lng="ca">ac&agrave;cia</trd>
  <trdgrp lng="cs">
    <trd>ak&aacute;cie</trd>,
    <trd>kapinice</trd>
  </trdgrp>
  <trd lng="de">Akazie</trd>
  <trd lng="en">acacia</trd>
  <trd lng="fr">acacia</trd>
  <trd lng="he">&shin;&yod;&tet;&he;</trd>
  <trd lng="hu">ak&aacute;c(fa)</trd>
  <trd lng="id">akasia</trd>
  <trd lng="ja">&#x30A2;&#x30AB;&#x30B7;&#x30A2;</trd>
  <trd lng="nl">acacia</trd>
  <trd lng="pl">akacja</trd>
  <trd lng="pt">ac&aacute;cia</trd>
  <trd lng="ru">&c_a;&c_k;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sk">ak&aacute;cia</trd>
  <trd lng="sv">akacia</trd>
  <trd lng="uk">&#x430;&#x43A;&#x430;&#x446;&#x456;&#x44F;</trd>
</drv>
<drv mrk="akaci.sxajn0o">
  <kap>&scirc;ajn<tld/>o</kap>
  <snc>
    <uzo tip="stl">EVI</uzo>
      <ref tip="dif" cel="robini.akacia0o">&Scirc;ajnakacia
      robinio</ref>
  </snc>
  <trd lng="de">Falsche Akazie</trd>
  <trd lng="fr">faux-acacia</trd>
  <trdgrp lng="uk">
    <trd>&#x440;&#x43E;&#x431;&#x456;&#x43D;&#x456;&#x44F; &#x437;&#x432;&#x438;&#x447;&#x430;&#x439;&#x43D;&#x430;</trd>,
    <trd>&#x431;&#x456;&#x43B;&#x430; &#x430;&#x43A;&#x430;&#x446;&#x456;&#x44F;</trd>
  </trdgrp>
</drv>
  <bld lok="&WCU;/thumb/a/a5/Acacia_restiacea.jpg/640px-Acacia_restiacea.jpg" prm="CC BY 2.5">Acacia restiacea (cultivated labelled) Maranoa Gardens, Balwyn,
      Victoria,
        Australia
        <fnt>
          <aut>Melburnian, propra verko</aut>
          <lok><url ref="&WCW;/File:Acacia_restiacea.jpg">&WCW;/File:Acacia_restiacea.jpg</url></lok>
        </fnt>
    </bld>
</art>
