<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>akaci</rad>/o
</kap>
<drv mrk="akaci.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="mimoza.0oj">mimozacoj</ref>
      (<trd lng="la">Acacia</trd>) kun sescento da arbaj kaj arbustaj
      specioj ofte dornaj:
      <ekz>
        sole la&ubreve;bo el <tld/>oj kaj siringoj donis belan ombron
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 5a, p. 21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_a;&c_k;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="br">
   <trd>kasiaenn</trd>,
   <trd>gwezenn-gasia</trd>
  </trdgrp>
  <trd lng="ca">ac&agrave;cia</trd>
  <trd lng="de">Akazie</trd>
  <trd lng="fr">acacia</trd>
  <trd lng="en">acacia</trd>
  <trd lng="hu">ak&aacute;c(fa)</trd>
  <trd lng="nl">acacia</trd>
  <trd lng="pl">akacja</trd>
  <trd lng="pt">ac&aacute;cia</trd>
  <trd lng="ru">&c_a;&c_k;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sk">ak&aacute;cia</trd>
  <trd lng="sv">akacia</trd>
</drv>
</art>
