<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>4</ofc>
      <rad>konsekr</rad>/i
      <fnt>Z</fnt>
    </kap>
    <drv mrk="konsek.0i">
      <kap><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <uzo tip="fak">REL</uzo>
      <snc mrk="konsek.0i.iun">
        <dif>
          Solene enoficigi per religiaj ritoj pastron a&ubreve; re&gcirc;on. 
        </dif>
        <trd lng="ru">&c_p;&c_o;&c_s;&c_v;&c_ja;&c_t;&c_i;&c_t;&c_mol; <klr>(&c_v; &c_s;&c_a;&c_n;)</klr></trd>
      </snc>
      <snc mrk="konsek.0i.ion">
        <dif>
          Destini a&jcirc;ojn por dia servado per religiaj ritoj:
          <ekz>
            <tld/>i pre&gcirc;ejon.
          </ekz>
        </dif>
        <trd lng="ru">&c_o;&c_s;&c_v;&c_ja;&c_t;&c_i;&c_t;&c_mol;</trd>
      </snc>
    </drv>
  </art>
<!--
$Log$
-->
</vortaro>
