<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>4</ofc>
      <rad>konsekr</rad>/i
      <fnt>Z</fnt>
    </kap>
    <drv mrk="konsek.0i">
      <kap><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <uzo tip="fak">REL</uzo>
      <snc mrk="konsek.0i.iun">
        <dif>
          Solene enoficigi per religiaj ritoj pastron a&ubreve; re&gcirc;on. 
        </dif>
        <trdgrp lng="en">
          <trd>ordain <klr>(p. p. pastro)</klr></trd>,
          <trd>coronate <klr>(p. p. re&gcirc;o)</klr></trd>
        </trdgrp>
        <trd lng="de">weihen</trd>
        <trd lng="nl">inwijden</trd>
        <trd lng="ru">&c_p;&c_o;&c_s;&c_v;&c_ja;&c_t;&c_i;&c_t;&c_mol; <klr>(&c_v; &c_s;&c_a;&c_n;)</klr></trd>
      </snc>
      <snc mrk="konsek.0i.ion">
        <dif>
          Destini a&jcirc;ojn por dia servado per religiaj ritoj:
          <ekz>
            <tld/>i pre&gcirc;ejon.
          </ekz>
        </dif>
        <trd lng="de">einweihen</trd>
        <trdgrp lng="en">
          <trd>consecrate</trd>,
          <trd>decicate</trd>,
          <trd>devote</trd>,
          <trd>hallow</trd>,
          <trd>sanctify</trd>
        </trdgrp>
        <trd lng="nl">consacreren</trd>
        <trd lng="ru">&c_o;&c_s;&c_v;&c_ja;&c_t;&c_i;&c_t;&c_mol;</trd>
      </snc>
    </drv>
  </art>
