<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>deput</rad>/i
</kap>
<drv mrk="deput.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <uzo tip="fak">POL</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Elekti kaj sendi kiel reprezentanton en
      <ref cel="parlam.0o">parlamenton</ref>:
      <ekz>
        maldekstrula <ind><tld/>ito</ind>;
        <trdgrp lng="de">
           <trd>Abgeordneter</trd>,
           <trd>Entsandter</trd>
        </trdgrp>
        <trd lng="fr">d&eacute;put&eacute;</trd>
      </ekz>
      <ekz>
        &ccirc;iu moderna demokratio havas <ind><tld/>itaro</ind>n.
        <trdgrp lng="de">
           <trd>Abgeordnete</trd>,
           <trd>Abgeordnetenversammlung</trd>
        </trdgrp>
        <trd lng="fr">assembl&eacute;e<klr tip="ind"> (ensembles
        d&eacute;put&eacute;s)</klr></trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="deleg.0i">delegi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>abordnen</trd>,
     <trd>entsenden</trd>
  </trdgrp>
  <trd lng="en">depute</trd>
  <trd lng="fr">d&eacute;puter</trd>
  <trd lng="ru">&c_i;&c_z;&c_b;&c_r;&c_a;&c_t;&c_mol; &c_d;&c_je;&c_p;&c_u;&c_t;&c_a;&c_t;&c_a;</trd>
</drv>
</art>
