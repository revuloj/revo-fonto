<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fag</rad>/o
</kap>
<drv mrk="fag.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="fagac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Fagus</trd>), arbo kun triangulaj nuksoj,
      kies ligno estas multe uzata en &ccirc;arpentado.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_k;</trd>
  <trdgrp lng="br">
    <trd>faouenn</trd>,
    <trd>gwezenn-faou</trd>
  </trdgrp>
  <trd lng="de">Buche</trd>
  <trdgrp lng="fr">
    <trd>h&ecirc;tre</trd>,
    <trd>fau</trd>,
    <trd>fou<klr tip="ind"> (h&ecirc;tre)</klr></trd></trdgrp> 
  <trdgrp lng="hu">
    <trd>b&uuml;kk</trd>,
    <trd>b&uuml;kkfa</trd></trdgrp>
  <trd lng="nl">beuk</trd>
  <trd lng="ru">&c_b;&c_u;&c_k;</trd>
  <trd lng="ca">faig</trd>
</drv>

</art>
