<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fag</rad>/o
</kap>
<drv mrk="fag.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="fag.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Fagus</trd>), arbo kun triangulaj nuksoj,
      kies ligno estas multe uzata en &ccirc;arpentado.
    </dif>
  </snc>
  <trd lng="de">Buche</trd>
  <trd lng="fr">h&ecirc;tre</trd>
</drv>
<drv mrk="fag.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      arboj kaj arbedoj (<trd lng="la">Fagaceae</trd>), al kiu apartenas
      <refgrp tip="sub">
        <ref cel="fag.0o">fago</ref>,
        <ref cel="kverk.0o">kverko</ref>,
        <ref cel="kasxta.0o">ka&scirc;tano</ref>
      </refgrp>
      k.a.
    </dif>
  </snc>
  <trd lng="de">Buchengew&auml;chse</trd>
</drv>
</art>
