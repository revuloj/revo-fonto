<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fondu.xml,v 1.11 2023/11/09 20:48:33 revo Exp $">
<kap>
  <rad>fondu</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>

<drv mrk="fondu.0o">
  <kap><tld/>o</kap>
  <snc mrk="fondu.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="plad.0o.menu">Plado</ref> el varmega oleo, froma&gcirc;o,
      &ccirc;okolado...
      prezentata en komuna poto, en kiu &ccirc;iu trempe kuiras pecetojn de
      viando, pano, legomoj, fruktoj:
      <ekz>
        sabate vespere, <tld/>o &gcirc;ojigis la &ccirc;eestantojn
        <fnt>
          <aut>-</aut>,
          <lok>La Informilo, 1975, numero 11a</lok>
        </fnt>;
      </ekz>
      <ekz>
        s-ino <nom>Anita</nom> invitas al <tld/>o kun froma&gcirc;oj
        <fnt>
          <aut>K. Long</aut>,
          <vrk><url ref="http://esperanto.net/literaturo/uea/slovenrec.html">Ve al
          la verda arbo</url></vrk>,
          <bib>Esperanto</bib>,
          <lok>1993</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_n;&c_mol;&c_d;&c_z;&c_ju;</trd>
  <trd lng="bg">&c_f;&c_o;&c_n;&c_d;&c_ju;</trd>
  <trd lng="cs">fondue</trd>
  <trd lng="de">Fondue</trd>
  <trd lng="fa">&a_f;&a_w;&a_n;&a_d;&a_w;</trd>
  <trd lng="fr">fondue<klr> (cuisine)</klr></trd>
  <trd lng="pt">fondue</trd>
  <trd lng="sk">fondue</trd>
  <trd lng="uk">&c_f;&#x43E;&#x43D;&c_d;&#x44E;</trd>
  <trdgrp lng="zh">
    <trd>&#x6DAE; <pr>shu&#xE0;n</pr></trd>,
    <trd>&#x829D;&#x58EB;&#x706B;&#x934B; <pr>zh&#x12B;sh&#xEC;hu&#x1D2;gu&#x14D;</pr></trd>
  </trdgrp>
</drv>
</art>

