<vortaro>
  <art mrk="$Id: kiu.xml,v 1.67 2021/06/19 05:58:40 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>kiu</rad>
    </kap>

    <drv mrk="kiu.0">
      <kap><ofc>*</ofc><tld/></kap>
      <snc>
        <dif>
          Demanda pronomo uzata sen ia resendo al apuda substantivo
          kaj montranta personon:
          <ekz>
            <tld/> kura&gcirc;as rajdi sur leono?
            <fnt><bib>F</bib>, <lok>&FE;7.</lok></fnt>;
          </ekz><ekz>
            <ctl><tld lit="K"/>j estis tiuj du personoj?</ctl>
            demandis la ne&gcirc;ulo la &ccirc;enhundon
	    <fnt><bib>Fab4</bib>, <lok>Ne&gcirc;ulo</lok></fnt>.
          </ekz>
        </dif>
        <trd lng="be">&c_h;&c_t;&c_o;</trd>
        <trd lng="bo">&#xF66;&#xF74;&#xF0B;</trd>
        <trd lng="de">wer</trd>
        <trd lng="he">&mem;&yod;</trd>
        <trdgrp lng="hu">
          <trd>ki</trd>,
          <trd>kicsoda</trd>
        </trdgrp>
        <trd lng="id">siapa</trd>
        <trd lng="nl">wie</trd>
        <trd lng="pl">kto</trd>
        <trdgrp lng="ro">
          <trd>care</trd>,
          <trd>cine</trd>
        </trdgrp>
        <trd lng="ru">&c_k;&c_t;&c_o;</trd>
        <trd lng="sv">vem</trd>
        <trdgrp lng="vo">
          <trd>kiomik<klr> [vira]</klr></trd>,
          <trd>kiofik<klr> [ina]</klr></trd>,
          <trd>kiosik<klr> [a&jcirc;a]</klr></trd>
        </trdgrp>
        <trd lng="tr">kim</trd>
      </snc>
      <snc>
        <dif>
          Demanda pronomo uzata por resendi al apuda substantivo kaj
          montranta personon a&ubreve; objekton:
          <ekz>
            <tld/> el ni kapablus tion?;
          </ekz><ekz>
            el tiuj libroj <tld/>n vi prenos?.
          </ekz>
        </dif>
        <trdgrp lng="be">
          <trd>&c_h;&c_t;&c_o;</trd>,
          <trd>&c_k;&c_a;&c_t;&c_o;&c_r;&c_y;</trd>,
          <trd>&c_ja;&c_k;&c_ib;</trd>
        </trdgrp>
        <trd lng="bo">&#xF66;&#xF74;&#xF0B;</trd>
        <trdgrp lng="de">
          <trd>wer</trd>,
          <trd>welcher</trd>,
          <trd>welche</trd>,
          <trd>welches</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>ki</trd>,
          <trd>melyik</trd>
        </trdgrp>
        <trdgrp lng="nl">
          <trd>wie</trd>,
          <trd>welk</trd>,
          <trd>welke</trd>
        </trdgrp>
        <trdgrp lng="pl">
          <trd>kt&oacute;ry</trd>,
          <trd>kt&oacute;ra</trd>,
          <trd>kt&oacute;re</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_k;&c_t;&c_o;</trd>,
          <trd>&c_k;&c_o;&c_t;&c_o;&c_r;&c_y;&c_j;</trd>
        </trdgrp>
        <trdgrp lng="sv">
          <trd>vem</trd>,
          <trd>vilken</trd>,
          <trd>vilket</trd>
        </trdgrp>
      </snc>
      <snc>
        <dif>
          Demanda adjektivo pri personoj a&ubreve; a&jcirc;oj:
          <ekz>
            <tld/> lernanto tion faris?;
          </ekz><ekz>
            <tld/>n daton ni havas hodia&ubreve;?;
          </ekz><ekz>
            pri <tld/> libro vi parolas?;
          </ekz><ekz>
            <tld/>jn do objektojn vi povas kaj deziras instrui?
            <fnt><bib>Marta</bib></fnt>.
          </ekz>
        </dif>
        <trdgrp lng="be">
          <trd>&c_h;&c_t;&c_o;</trd>,
          <trd>&c_k;&c_a;&c_t;&c_o;&c_r;&c_y;</trd>,
          <trd>&c_ja;&c_k;&c_ib;</trd>
        </trdgrp>
        <trdgrp lng="de">
          <trd>welcher</trd>,
          <trd>welche</trd>,
          <trd>welches</trd>
        </trdgrp>
        <trd lng="he">&alef;&yod;&zayin;&he;</trd>
        <trd lng="hu">mely(ik)</trd>
        <trdgrp lng="id">
          <trd>apa</trd>,
          <trd><ind>yang</ind> mana</trd>
        </trdgrp>
        <trdgrp lng="nl">
          <trd>welk</trd>,
          <trd>welke</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_k;&c_t;&c_o;</trd>,
          <trd>&c_k;&c_o;&c_t;&c_o;&c_r;&c_y;&c_j;</trd>
        </trdgrp>
        <trdgrp lng="sv">
          <trd>vilken</trd>,
          <trd>vilket</trd>
        </trdgrp>
      </snc>
      <snc>
        <dif>
          Relativa pronomo pri personoj a&ubreve; a&jcirc;oj:
          <ekz>
            <ind><tld/> vivos, tiu vidos</ind>
            <fnt><bib>PrV</bib></fnt>;
            <trd lng="fr">qui <ind>vivra</ind> verra</trd>
            <trd lng="ru">&c_p;&c_o;&c_zh;&c_i;&c_v;&c_jo;&c_m; &ndash; &c_u;&c_v;&c_i;&c_d;&c_d;&c_i;&c_m;</trd>
          </ekz><ekz>
            <ind>plej bone ridas, <tld/> laste ridas</ind>
            <fnt><bib>PrV</bib></fnt>.
            <trd lng="fr">rira bien qui rira le dernier</trd>
            <trd lng="ru">&c_h;&c_o;&c_r;&c_o;&c_sh;&c_o; &c_s;&c_m;&c_je;&c_jo;&c_t;&c_s;&c_ja; &c_t;&c_o;&c_t;, &c_k;&c_t;&c_o; &c_s;&c_m;&c_je;&c_jo;&c_t;&c_s;&c_ja;
            &c_p;&c_o;&c_s;&c_l;&c_je;&c_d;&c_n;&c_i;&c_m;</trd>
            <trd lng="tr">son g&uuml;len iyi g&uuml;ler</trd>
          </ekz>
        </dif>
        <trdgrp lng="be">
          <trd>&c_h;&c_t;&c_o;</trd>,
          <trd>&c_k;&c_a;&c_t;&c_o;&c_r;&c_y;</trd>,
          <trd>&c_ja;&c_k;&c_ib;</trd>
        </trdgrp>
        <trd lng="bo">&#xF66;&#xF74;&#xF0B;</trd>
        <trdgrp lng="de">
          <trd>wer</trd>,
          <trd>der</trd>,
          <trd>die</trd>,
          <trd>das</trd>
        </trdgrp>
        <trdgrp lng="he">
          <trd>&shin;-</trd>,
          <trd>&mem;&yod; &shin;-</trd>,
          <trd>&alef;&vav;&tav;&vav;</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>aki</trd>,
          <trd>amelyik</trd>
        </trdgrp>
        <trdgrp lng="nl">
          <trd>welke</trd>,
          <trd>welk</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_k;&c_t;&c_o;</trd>,
          <trd>&c_k;&c_o;&c_t;&c_o;&c_r;&c_y;&c_j;</trd>
        </trdgrp>
        <trd lng="sv">som</trd>
        <trd lng="vo">kel</trd>
      </snc>
      <ref tip="vid" cel="ajn.0">ajn</ref>
