    <trd lng="bo">&t_sa;&t_shabchu;&t_tsheg;</trd>
    <trd lng="de">wer</trd>
    <trd lng="en">who</trd>
    <trd lng="he">&mem;&yod;</trd>
    <trdgrp lng="hu">
      <trd>ki</trd>,
      <trd>kicsoda</trd>
    </trdgrp>
    <trd lng="id">siapa</trd>
    <trd lng="nl">wie</trd>
    <trd lng="pl">kto</trd>
    <trdgrp lng="ro">
      <trd>care</trd>,
      <trd>cine</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_t;&c_o;</trd>
    <trd lng="sv">vem</trd>
    <trdgrp lng="th">
      <trd>&#xE43;&#xE04;&#xE23;</trd>,
      <trd>&#xE04;&#xE19;&#xE44;&#xE2B;&#xE19;</trd>
    </trdgrp>
