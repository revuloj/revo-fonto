<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: rezid.xml,v 1.7 2024/06/20 06:32:16 revo Exp $">
    <kap>
      <rad>rezid</rad>/i
    </kap>

<drv mrk="rezid.0i">
  <kap><tld/>i</kap>
  <snc mrk="rezid.0i.logxi">
    <dif>
      Oficiale <ref tip="super" cel="logx.0i">lo&gcirc;i</ref>:
      <ekz>
        tiu pre&gcirc;eja pentristo, kiu fine de sia vivo, vaste agnoskita, <tld/>is en Romo
        <fnt>
          <bib>Monato</bib>
          <aut>Rita R&ouml;ssler-Buckel</aut>
          <vrk><url ref="&Monato;/2007/009174.php">Metio kaj formo tradicio kaj progreso</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Claude Piron</nom> naski&gcirc;is belgo kaj i&gcirc;is svisa civitano nur en 1978, sed li longe
        <tld/>is en &Gcirc;enevo
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2013/010593.php">&Ccirc;e l' koro de E&ubreve;ropo ...</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ili devas atingi sian <tld/>an komunumon en la patrujo, okaze de la diversaj
        balotoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2013/010966.php">La&ubreve;ti&gcirc;as vo&ccirc;oj por vo&ccirc;doni</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Moskvo <klr>[&#8230;]</klr> estas dense lo&gcirc;ata: 11,5 milionoj da homoj tie <tld/>as
        <fnt>
          <bib>Monato</bib>
          <aut>Mihail Korotkov</aut>
          <vrk><url ref="&Monato;/2011/010465.php">Bridi la monstron</url></vrk>
        </fnt>;
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_r;&c_a;&c_zh;&c_y;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_d;&c_z;&c_je;&c_s;&c_mol;&c_c;&c_ib; &c_s;&c_t;&c_a;&c_l;&c_a;)</klr></trd>
    <trd lng="de"><ind>wohnhaft</ind> sein</trd>
    <trd lng="pl">mieszka&cacute;</trd>
    <trd lng="ro">tr&abreve;i</trd>
  </snc>
  <snc mrk="rezid.0i.ofici">
    <uzo tip="fak">POL</uzo>
    <dif>
      (pp &scirc;tatesto) Oficiale en iu loko funkcii, <ref tip="super" cel="ofic.0i">ofici</ref>:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ankora&ubreve; <tld/>as en justicoficoj tro multaj &leftquot;iamaj kamaradoj&#8221;
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2010/010109.php">Na&ubreve;zita de koruptado</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_a;&c_j;&c_m;&c_a;&c_c;&c_mol; &c_p;&c_a;&c_s;&c_a;&c_d;&c_u;</trd>
    <trd lng="de">residieren</trd>
    <trd lng="fr">r&eacute;sider</trd>
    <trd lng="pl">rezydowa&cacute;</trd>
    <trd lng="ro">locui</trd>
    <trdgrp lng="zh">
    <trd>&#23507; <pr>y&ugrave;</pr></trd>,
    <trd>&#23621;&#30041; <pr>j&umacron;li&uacute;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="uk">
    <trd>&c_m;&c_a;&c_t;&c_i; &c_p;&c_o;&c_s;&c_t;&c_ib;&c_j;&c_n;&c_je; &c_m;&c_ib;&c_s;&c_c;&c_je;
    &c_p;&c_r;&c_o;&c_zh;&c_i;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="rezid.0ejo">
  <kap>
    <tld/>ejo
  </kap>
  <snc mrk="rezid.0ejo.logxejo">
    <dif>
      Oficiala <ref tip="super" cel="logx.0ejo">lo&gcirc;ejo</ref>:
      <ekz>
        eksterlande rajtas vo&ccirc;doni nur civitanoj, kiuj pro grava kialo lokis oficiale
        kaj definitive sian <tld/>ejon en alian landon
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2013/010966.php">La&ubreve;ti&gcirc;as vo&ccirc;oj por vo&ccirc;doni</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_je;&c_s;&c_c;&c_a; <ind>&c_zh;&c_y;&c_h;&c_a;&c_r;&c_s;&c_t;&c_v;&c_a;</ind></trd>
    <trd lng="de">Wohnsitz</trd>
    <trdgrp lng="zh">
    <trd>&#23507; <pr>y&ugrave;</pr></trd>,
    <trd>&#25448; <pr>sh&ecaron;</pr></trd>,
    <trd>&#33293; <pr>sh&ecaron;</pr></trd>,
    <trd>&#23507;&#25152; <pr>y&ugrave;su&#466;</pr></trd>,
    <trd>&#22266;&#23450;&#20303;&#25152; <pr>g&ugrave;d&igrave;ngzh&ugrave;su&#466;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="rezid.0ejo.sxtatestra">
    <uzo tip="fak">POL</uzo>
    <dif>
      Loko, kie &scirc;tatestro oficiale oficas kaj reprezentas:
      <ekz>
        la &ccirc;efministraj oficejo kaj <tld/>ejo najbare situas proksime al la parlamento
        kaj administraj ministrejoj en la centro de la &ccirc;efurbo
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2013/011104.php">Fantomo-domo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Sintra</nom> &ndash; perlo de Portugalio <klr>[&#8230;]</klr> estis somera <tld/>ejo de portugalaj
        re&gcirc;oj en la mezepoko
        <fnt>
          <bib>Monato</bib>
          <aut>Tatjana Loskutova</aut>
          <vrk><url ref="&Monato;/2007/009164.php">Voja&gcirc;o en Iberio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li konstruigis &leftquot;malgrandan Versajlon&#8221; (la&ubreve; la ekzemplo de la fama <tld/>ejo de
        francaj re&gcirc;oj apud Parizo)
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2006/008899.php">Vizito al Novarto-urbo Nancy</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ejo de la irlanda &scirc;tatprezidanto
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2006/008699.php">Kara antikva malpura Dublino</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la registaro proponis al <nom>Adamkus</nom> neuzatan &ccirc;efministran <tld/>ejon &ndash; preska&ubreve;
        samgrandan kiel la prezidanta <tld/>ejo
        <fnt>
          <bib>Monato</bib>
          <aut>Last</aut>
          <vrk><url ref="&Monato;/2004/008143.html">Pri (p)rezidejoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="super">
       <ref cel="ofic.0ejo">oficejo</ref>,
       <ref cel="reprezent.0ejo">reprezentejo</ref>
    </refgrp>
    <trd lng="be">&c_r;&c_e;&c_z;&c_y;&c_d;&c_e;&c_n;&c_c;&c_y;&c_ja;</trd>
    <trdgrp lng="de">
      <trd>Amtssitz</trd>,
      <trd>Residenz</trd>
    </trdgrp>
  </snc>
  <snc mrk="rezid.0ejo.reprezentejo">
    <uzo tip="stl">EVI</uzo>
    <dif>
      Reprezentejo:
      <ekz>
        <tld/>ejo <klr>[&#8230;]</klr> de usona ambasadoro
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2006/008699.php">Kara antikva malpura Dublino</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iam pli da sekretaj servoj lokis sian <tld/>ejon en Vieno
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2010/010120.php">Malsekretaj sekretoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="be">&c_p;&c_r;&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_n;&c_ib;&c_c;&c_t;&c_v;&c_a;</trd>
  </snc>
  <trd lng="fr">r&eacute;sidence</trd>
  <trd lng="pl">rezydencja</trd>
  <trd lng="ro">re&scomma;edin&tcomma;&abreve;</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_je;&c_z;&c_i;&c_d;&c_je;&c_n;&c_c;&c_ib;&c_ja;</trd>,
    <trd>&c_o;&c_s;&c_ib;&c_d;&c_o;&c_k;</trd>,
    <trd>&c_s;&c_a;&c_d;&c_i;&c_b;&c_a;</trd>
  </trdgrp>
</drv>

  </art>

