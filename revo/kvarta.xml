<kap>
  <ofc>*</ofc>
  <rad>kvartal</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kvarta.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kvarta.0o.URB">
    <dif>
      Parto de urbo:
      <ekz>
        lo&gcirc;i en komerca <tld/>o;
      </ekz>
      <ekz>
        la latina (studenta) <tld/>o en Parizo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_v;&c_a;&c_r;&c_t;&c_a;&c_l; <klr>(&c_u; &c_g;&c_o;&c_r;&c_a;&c_d;&c_z;&c_je;)</klr></trd>
  <trd lng="ca">barri</trd>
  <trdgrp lng="de">
    <trd>Viertel</trd>,
    <trd>Stadtviertel</trd>,
    <trd>Stadtteil</trd>,
    <trd>Bezirk</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>neighborhood</trd>,
    <trd>district</trd>,
    <trd>quarter</trd>
  </trdgrp>
  <trd lng="es">barrio</trd>
  <trd lng="eu">auzo</trd>
  <trd lng="fr">quartier</trd>
  <trd lng="hu">lak&oacute;negyed</trd>
  <trd lng="it">quartiere</trd>
  <trdgrp lng="ja">
    <trd>&#x5E02;&#x533A;  [<ind>&#x3057;&#x304F;</ind>]</trd>,
    <trd>&#x533A;&#x753B;  [<ind>&#x304F;&#x304B;&#x304F;</ind>]</trd>,
    <trd>&#x8857;&#x533A;  [<ind>&#x304C;&#x3044;&#x304F;</ind>]</trd>,
    <trd>&#x5730;&#x533A;  [<ind>&#x3061;&#x304F;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>wijk</trd>,
    <trd>buurt</trd>
  </trdgrp>
  <trd lng="pl">dzielnica</trd>
  <trd lng="pt">bairro</trd>
  <trd lng="ru">&c_k;&c_v;&c_a;&c_r;&c_t;&c_a;&c_l; <klr>(&c_g;&c_o;&c_r;&c_o;&c_d;&c_s;&c_k;&c_o;&c_j;)</klr></trd>
</drv>

<drv mrk="kvarta.logx0o">
  <kap>lo&gcirc;<tld/>o</kap>
  <snc mrk="kvarta.logx0o.URB">
    <dif>
      <tld lit="K"/>o en kiu plimulte estas lo&gcirc;ejoj:
      <ekz>
        la mallar&gcirc;aj stratoj, inter la multeta&gcirc;aj domoj
        en la granda lo&gcirc;<tld/>o, estis silentaj kaj senhomaj
        <fnt>
          <aut>J. Modest</aut>,
          <vrk><url ref="http://verkoj.com/verkistoj/julian-modest/murdo-en-la-parko/">
          Murdo en la parko</url></vrk>,
          <lok>2018</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_y;&c_l;&c_mol;&c_l;&c_jo;&c_v;&c_y; &c_k;&c_v;&c_a;&c_r;&c_t;&c_a;&c_l;</trd>
  <trd lng="fr">quartier r&eacute;sidentiel</trd>
  <trdgrp lng="ja">
    <trd>&#x4F4F;&#x5B85;&#x8857;  [<ind>&#x3058;&#x3085;&#x3046;&#x305F;&#x304F;&#x304C;&#x3044;</ind>]</trd>,
    <trd>&#x4F4F;&#x5B85;&#x56E3;&#x5730;  [<ind>&#x3058;&#x3085;&#x3046;&#x305F;&#x304F;&#x3060;&#x3093;&#x3061;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">woonwijk</trd>
  <trd lng="uk">&#x436;&#x438;&#x442;&#x43B;&#x43E;&#x432;&#x438;&#x439; &#x43A;&#x432;&#x430;&#x440;&#x442;&#x430;&#x43B;</trd>
</drv>
