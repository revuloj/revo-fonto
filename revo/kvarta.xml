<art mrk="$Id: kvarta.xml,v 1.23 2025/03/16 02:04:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kvartal</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="kvarta.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kvarta.0o.URB">
    <dif>
      Parto de urbo:
      <ekz>
        lo&gcirc;i en komerca <tld/>o;
      </ekz>
      <ekz>
        la latina (studenta) <tld/>o en Parizo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_v;&c_a;&c_r;&c_t;&c_a;&c_l; <klr>(&c_u; &c_g;&c_o;&c_r;&c_a;&c_d;&c_z;&c_je;)</klr></trd>
  <trd lng="ca">barri</trd>
  <trdgrp lng="de">
    <trd>Viertel</trd>,
    <trd>Stadtviertel</trd>,
    <trd>Stadtteil</trd>,
    <trd>Bezirk</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>neighborhood</trd>,
    <trd>district</trd>,
    <trd>quarter</trd>
  </trdgrp>
  <trd lng="es">barrio</trd>
  <trd lng="eu">auzo</trd>
  <trd lng="fr">quartier</trd>
  <trd lng="hu">lak&oacute;negyed</trd>
  <trd lng="it">quartiere</trd>
  <trdgrp lng="ja">
    <trd>&#24066;&#21306;  <pr>&#12375;&#12367;</pr></trd>,
    <trd>&#21306;&#30011;  <pr>&#12367;&#12363;&#12367;</pr></trd>,
    <trd>&#34903;&#21306;  <pr>&#12364;&#12356;&#12367;</pr></trd>,
    <trd>&#22320;&#21306;  <pr>&#12385;&#12367;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>wijk</trd>,
    <trd>buurt</trd>
  </trdgrp>
  <trd lng="pl">dzielnica</trd>
  <trd lng="pt">bairro</trd>
  <trd lng="ru">&c_k;&c_v;&c_a;&c_r;&c_t;&c_a;&c_l; <klr>(&c_g;&c_o;&c_r;&c_o;&c_d;&c_s;&c_k;&c_o;&c_j;)</klr></trd>
  <trdgrp lng="zh">
    <trd>&#34903;&#22346; <pr>ji&emacron;f&amacron;ng</pr></trd>,
    <trd>&#23567;&#21306; <pr>xi&#462;oq&umacron;</pr></trd>,
    <trd>&#23567;&#21312; <pr>xi&#462;oq&umacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kvarta.logx0o">
  <kap>lo&gcirc;<tld/>o</kap>
  <snc mrk="kvarta.logx0o.URB">
    <dif>
      <tld lit="K"/>o en kiu plimulte estas lo&gcirc;ejoj:
      <ekz>
        la mallar&gcirc;aj stratoj, inter la multeta&gcirc;aj domoj
        en la granda lo&gcirc;<tld/>o, estis silentaj kaj senhomaj
        <fnt>
          <aut>J. Modest</aut>,
          <vrk><url ref="http://verkoj.com/verkistoj/julian-modest/murdo-en-la-parko/">
          Murdo en la parko</url></vrk>,
          <lok>2018</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_y;&c_l;&c_mol;&c_l;&c_jo;&c_v;&c_y; &c_k;&c_v;&c_a;&c_r;&c_t;&c_a;&c_l;</trd>
  <trdgrp lng="en">
    <trd>residential <ind>neighborhood</ind></trd>,
    <trd>residential <ind>district</ind></trd>,
    <trd>residential <ind>quarter</ind></trd>
  </trdgrp>
  <trd lng="es">barrio residencial</trd>
  <trd lng="fr">quartier r&eacute;sidentiel</trd>
  <trdgrp lng="ja">
    <trd>&#20303;&#23429;&#34903;  <pr>&#12376;&#12421;&#12358;&#12383;&#12367;&#12364;&#12356;</pr></trd>,
    <trd>&#20303;&#23429;&#22243;&#22320;  <pr>&#12376;&#12421;&#12358;&#12383;&#12367;&#12384;&#12435;&#12385;</pr></trd>
  </trdgrp>
  <trd lng="nl">woonwijk</trd>
  <trd lng="pl">dzielnica mieszkaniowa</trd>
  <trd lng="uk">&c_zh;&c_i;&c_t;&c_l;&c_o;&c_v;&c_i;&c_j; &c_k;&c_v;&c_a;&c_r;&c_t;&c_a;&c_l;</trd>
  <trdgrp lng="zh">
    <trd>&#20303;&#23429;&#21306; <pr>zh&ugrave;zh&aacute;iq&umacron;</pr></trd>,
    <trd>&#20303;&#23429;&#21312; <pr>zh&ugrave;zh&aacute;iq&umacron;</pr></trd>,
    <trd>&#23567;&#21306; <pr>xi&#462;oq&umacron;</pr></trd>,
    <trd>&#23567;&#21312; <pr>xi&#462;oq&umacron;</pr></trd>
  </trdgrp>
</drv>
</art>
