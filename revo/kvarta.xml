<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kvarta.xml,v 1.14 2018/06/22 12:10:13 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kvartal</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kvarta.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kvarta.0o.URB">
    <dif>
      Parto de urbo:
      <ekz>
        lo&gcirc;i en komerca <tld/>o;
      </ekz>
      <ekz>
        la latina (studenta) <tld/>o en Parizo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_v;&c_a;&c_r;&c_t;&c_a;&c_l; <klr>(&c_u; &c_g;&c_o;&c_r;&c_a;&c_d;&c_z;&c_je;)</klr></trd>
  <trd lng="ca">barri</trd>
  <trdgrp lng="de">
    <trd>Viertel</trd>,
    <trd>Stadtviertel</trd>,
    <trd>Stadtteil</trd>,
    <trd>Bezirk</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>neighborhood</trd>,
    <trd>district</trd>,
    <trd>quarter</trd>
  </trdgrp>
  <trd lng="es">barrio</trd>
  <trd lng="eu">auzo</trd>
  <trd lng="fr">quartier</trd>
  <trd lng="hu">lak&oacute;negyed</trd>
  <trd lng="it">quartiere</trd>
  <trdgrp lng="nl">
    <trd>wijk</trd>,
    <trd>buurt</trd>
  </trdgrp>
  <trd lng="pl">dzielnica</trd>
  <trd lng="pt">bairro</trd>
  <trd lng="ru">&c_k;&c_v;&c_a;&c_r;&c_t;&c_a;&c_l; <klr>(&c_g;&c_o;&c_r;&c_o;&c_d;&c_s;&c_k;&c_o;&c_j;)</klr></trd>
</drv>

<drv mrk="kvarta.logx0o">
  <kap>lo&gcirc;<tld/>o</kap>
  <snc mrk="kvarta.logx0o.URB">
    <dif>
      <tld lit="K"/>o en kiu plimulte estas lo&gcirc;ejoj:
      <ekz>
        la mallar&gcirc;aj stratoj, inter la multeta&gcirc;aj domoj
        en la granda lo&gcirc;<tld/>o, estis silentaj kaj senhomaj
        <fnt>
          <aut>J. Modest</aut>,
          <vrk><url
          ref="http://verkoj.com/verkistoj/julian-modest/murdo-en-la-parko/">
          Murdo en la parko</url></vrk>,
          <lok>2018</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">quartier r&eacute;sidentiel</trd>
  <trd lng="nl">woonwijk</trd>
</drv>
</art>
