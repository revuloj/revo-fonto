    <trd>Stadtviertel</trd>,
    <trd>Stadtteil</trd>,
    <trd>Bezirk</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>neighborhood</trd>,
    <trd>district</trd>,
    <trd>quarter</trd>
  </trdgrp>
  <trd lng="es">barrio</trd>
  <trd lng="eu">auzo</trd>
  <trd lng="fr">quartier</trd>
  <trd lng="hu">lak&oacute;negyed</trd>
  <trd lng="it">quartiere</trd>
  <trdgrp lng="ja">
    <trd>&#x5E02;&#x533A;  <pr>&#12375;&#x304F;</pr></trd>,
    <trd>&#x533A;&#x753B;  <pr>&#x304F;&#x304B;&#x304F;</pr></trd>,
    <trd>&#34903;&#x533A;  <pr>&#x304C;&#12356;&#x304F;</pr></trd>,
    <trd>&#22320;&#x533A;  <pr>&#12385;&#x304F;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
