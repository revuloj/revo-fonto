<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: otid.xml,v 1.13 2024/01/30 18:25:47 revo Exp $">
<kap>
  <rad>otid</rad>/o
  <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="otid.0o">
  <kap><tld/>o</kap>
  <snc mrk="otid.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" lst="voko:birdoj" cel="bird.0o.ZOO">Birdo</ref> sufi&ccirc;e granda kun longaj kolo kaj kruroj,
      vivanta en varmaj a&ubreve; tropikaj stepoj de E&ubreve;razio, Afriko,
      A&ubreve;stralio, <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">genro</ref>
      <trd lng="la">Otis</trd> kaj parencaj
      el familio otidedoj:
      <ekz>
        oni povas imagi ke la frua pragenerinto de la struto havis
        kutimojn kiel tiuj de <tld/>o
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro
        5a</lok></fnt>;
      </ekz>
      <ekz>
        en la varmega aero &ccirc;iam pli ofte a&ubreve;di&gcirc;adis
        la&ubreve;taj krioj de etaj <tld/>oj: &leftquot;mak-har! mak-har!&rightquot;
        <fnt><aut>I. Efremov, trad. J. Finkel</aut>, <vrk><url ref="http://www.esperanto.mv.ru/Efremov/arko.html">La Granda
        Arko</url></vrk>, <lok>2006</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Trappe</trd>
    <trd lng="en">bustard</trd>
    <trd lng="fr">outarde</trd>
    <trd lng="it">otarda</trd>
    <trd lng="hu">tuzok</trd>
    <trdgrp lng="nl">
      <trd>trap<klr tip="ind"> (trapgans)</klr></trd>,
      <trd>trapgans</trd>
    </trdgrp>
    <trd lng="pt">abetarda</trd>
    <trd lng="sv">trapp</trd>
    <trdgrp lng="zh">
    <trd>&#40199; <pr>b&#462;o</pr></trd>,
    <trd>&#40488; <pr>b&#462;o</pr></trd>
  </trdgrp>
</snc>
  <trd lng="cs">drop</trd>
  <trd lng="ja">&#12494;&#12460;&#12531;</trd>
  <trd lng="pl">drop</trd>
  <trd lng="ro">dropie</trd>
  <trd lng="sk">drop</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_r;&c_o;&c_f;&c_a;</trd>,
    <trd>&c_d;&c_r;&c_o;&c_h;&c_v;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="otid.granda0o">
  <kap>granda <tld/>o</kap>
  <snc mrk="otid.granda0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <fnt>
       <aut>Wouter F. Pilger</aut>,
       <vrk>
         <url ref="http://www.xs4all.nl/~pilger/wfp/birdeou8.htm">
           Komunlingvaj nomoj de E&ubreve;ropaj birdoj</url>
       </vrk>
    </fnt>
    <dif>
      Specio de <ref tip="super" cel="otid.0o.ZOO">otido</ref>
      (<trd lng="la">Otis tarda</trd>).
    </dif>
    <bld
      lok="&WCU;/thumb/4/4b/Drofa_(1).jpg/584px-Drofa_(1).jpg"
      prm="CC BY-SA 3.0"
      >Granda <tld/>o (masklo)
        <fnt>
          <aut>http://oblkompriroda.volganet.ru/Old_Portal/news/news/2013/11/news_00654.html</aut>
          <lok><url ref="&WCW;/File:Drofa_(1).jpg"
              >https://commons.wikimedia.org/wiki/File:Drofa_(1).jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="ca">
      <trd>avitarda</trd>,
      <trd>pioc salvatge</trd>
    </trdgrp>
    <trd lng="de">Gro&szlig;trappe</trd>
    <trd lng="en">great <ind>bustard</ind></trd>
    <trdgrp lng="fr">
      <trd>grande <ind>outarde</ind></trd>,
      <trd><ind>outarde</ind> barbue</trd>
    </trdgrp>
    <trd lng="pl">drop wielki</trd>
    <trd lng="ro">dropie</trd>
    <trd lng="sv">stortrapp</trd>
  </snc>
</drv>

<drv mrk="otid.malgranda0o">
  <kap>malgranda <tld/>o</kap>
  <snc mrk="otid.malgranda0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <fnt>
       <aut>Wouter F. Pilger</aut>,
       <vrk>
         <url ref="http://www.xs4all.nl/~pilger/wfp/birdeou8.htm">
           Komunlingvaj nomoj de E&ubreve;ropaj birdoj</url>
       </vrk>
    </fnt>
    <dif>
      Specio de <ref tip="super" cel="otid.0o.ZOO">otido</ref>
      (<trd lng="la">Tetrax tetrax</trd>).
    </dif>
    <bld
      lok="&WCU;/thumb/2/29/Tettet.jpg/640px-Tettet.jpg"
      prm="CC BY-SA 3.0"
      >Malgranda <tld/>o
        <fnt>
          <aut>Pierre Dalous, propra verko</aut>
          <lok><url ref="&WCW;/File:Tettet.jpg"
              >https://commons.wikimedia.org/wiki/File:Tettet.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="ca">sis&oacute;</trd>
    <trd lng="en">little <ind>bustard</ind></trd>
    <trd lng="fr"><ind>outarde</ind> canepeti&egrave;re</trd>
    <trd lng="pl">strepet</trd>
    <trd lng="ro">sp&acirc;rcaci</trd>
    <trd lng="sv">sm&aring;trapp</trd>
  </snc>
  <trd lng="de">Zwergtrappe</trd>
  <trdgrp lng="zh">
    <trd>&#23567;&#40199; <pr>xi&#462;ob&#462;o</pr></trd>,
    <trd>&#23567;&#40488; <pr>xi&#462;ob&#462;o</pr></trd>
  </trdgrp>
</drv>
</art>

