<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>distanc</rad>/o
</kap>
<drv mrk="distan.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Longo de interspaco:
      <ekz>
        <tld/>o de la tero al la luno;
      </ekz>
      <ekz>
        en <tld/>o de kelkaj pa&scirc;oj de li.
      </ekz>
    </dif>
    <trd lng="cs">vzd&aacute;lenost</trd>
    <trdgrp lng="de">
      <trd>Abstand</trd>,
      <trd>Entfernung</trd>,
      <trd>Distanz</trd>
    </trdgrp>
    <trd lng="fr">distance</trd>
    <trd lng="nl">afstand</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_s;&c_s;&c_t;&c_o;&c_ja;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_d;&c_i;&c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc mrk="distan.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <dif>
      Valoro de <ref cel="metrik.0o.MAT">metriko</ref> <frm><k>d</k></frm>,
      kaj pli precize:
    </dif>
    <subsnc mrk="distan.0o.inter_punktoj">
      <fnt><bib>MatStokTerm</bib><lok>p. 12</lok></fnt>
      <dif>
        <klr>(inter punktoj <frm><k>a</k></frm> kaj <frm><k>b</k></frm>)</klr>
        La valoro <frm><k>d</k>(<k>a</k>,<k>b</k>)</frm>.
      </dif>
    </subsnc>
    <subsnc mrk="distan.0o.de_punkto_al_aro">
      <fnt><bib>MatStokTerm</bib><lok>p. 12</lok></fnt>
      <dif>
        <klr>(de punkto <frm><k>a</k></frm> al aro <frm><g>A</g></frm>)</klr>
        La <ref cel="infim.0o">infimo</ref> de la <tld/>oj
        <sncref ref="distan.0o.inter_punktoj"/>
        de <frm><k>a</k></frm> al &ccirc;iu punkto de <frm><g>A</g></frm>.
      </dif>
    </subsnc>
    <subsnc mrk="distan.0o.inter_aroj">
      <fnt><bib>EKV</bib><lok>&para;87</lok></fnt>
      <dif>
        <klr>(inter aroj <frm><g>A</g></frm> kaj <frm><g>B</g></frm>)</klr>
        La <ref cel="infim.0o">infimo</ref> de la <tld/>oj
        <sncref ref="distan.0o.inter_punktoj"/>
        de &ccirc;iu punkto de <frm><g>A</g></frm> 
        al &ccirc;iu punkto de <frm><g>B</g></frm>.
      </dif>
    </subsnc>
    <trd lng="de">Abstand</trd>
    <trd lng="en">distance</trd>
    <trd lng="fr">distance<klr tip="ind"> (math.)</klr></trd>
    <trd lng="ru">&c_r;&c_a;&c_s;&c_s;&c_t;&c_o;&c_ja;&c_n;&c_i;&c_je;</trd>
  </snc>
  <trd lng="es">distancia</trd>
</drv>

<drv mrk="distan.paf0otraf0o">
  <kap>paf<tld/>o, traf<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="D"/>o &gcirc;is kiu oni povas trafi per pafo.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Reichweite</trd>
  </trdgrp>
  <trd lng="fr">port&eacute;e</trd>
  <trd lng="nl">draagwijdte</trd>
  <trd lng="ru">&c_d;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol; &c_s;&c_t;&c_r;&c_je;&c_l;&c_mol;&c_b;&c_y;</trd>
</drv>

<drv mrk="distan.angula0o">
  <kap>angula <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc mrk="distan.angula0o.MAT">
    <dif>
      <klr>(inter du punktoj de <ref cel="cirkl.0o.linio">cirklo</ref>)</klr>
      La mezuro de la <ref cel="angul.centra0o">centra angulo</ref>, kies
      lateroj trairas la punktojn:
      <ekz>
        la angula <tld/>o inter du punktoj de la sama tera meridiano egalas
        al la diferenco de iliaj respektivaj latitudoj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Winkelabstand</trd>,
    <trd>Winkelentfernung</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>angular <ind>distance</ind></trd>,
    <trd>angular <ind>separation</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>distance angulaire</trd>,
    <trd>&eacute;cart angulaire</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_g;&c_l;&c_o;&c_v;&c_o;&c_je; <ind>&c_r;&c_a;&c_s;&c_s;&c_t;&c_o;&c_ja;&c_n;&c_i;&c_je;</ind></trd>
</drv>
</art>
