<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>homogen</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="homoge.0a">
  <kap><tld/>a</kap>
  <snc num="1">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Kies &ccirc;iuj partoj havas saman naturon, konsiston:
      <ekz><tld/>a fanda&jcirc;o.</ekz>
    </dif>
    <trdgrp lng="de">
       <trd>homogen</trd>,
       <trd>gleichm&auml;&szlig;ig</trd>
    </trdgrp>
    <trd lng="sv">homogen</trd>
  </snc>
  <snc num="2">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="klr">(pri polinomo)</uzo>
    <dif>
      Kies &ccirc;iuj termoj estas samgradaj. 
    </dif>
    <refgrp tip="vid">
      <ref cel="hetero.0a">heterogena</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_o;&c_d;&c_n;&c_o;&c_r;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_g;&c_o;&c_m;&c_o;&c_g;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
