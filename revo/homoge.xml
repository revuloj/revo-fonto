<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>homogen</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="homoge.0a">
  <kap><tld/>a</kap>
  <snc num="1">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Kies &ccirc;iuj partoj havas saman naturon, konsiston:
      <ekz><tld/>a fanda&jcirc;o.</ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="hetero.0a">heterogena</ref>.
    </refgrp>
    <trdgrp lng="de">
       <trd>homogen</trd>,
       <trd>gleichm&auml;&szlig;ig</trd>
    </trdgrp>
    <trdgrp lng="nl">
       <trd>homogeen</trd>,
       <trd>gelijkmatig</trd>
    </trdgrp>
    <trd lng="sv">homogen</trd>
  </snc>
  <snc mrk="homoge.0a.polinomo">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <klr>(p.p. <ref cel="polino.0o">polinomo</ref>)</klr>
      Kies &ccirc;iuj termoj estas samgradaj. 
    </dif>
  </snc>
  <snc mrk="homoge.0a.ekvacio">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <dif>
      <klr>(p.p. <ref cel="ekvaci.0o">ekvacio</ref>)</klr>
      Kies desktra <ref cel="flank.0o.ekvacio">flanko</ref> estas 0:
      <ekz>
        por solvi ne<tld/>an diferencialan ekvacion, ofte utilas solvi
        la respondan <tld/>an ekvacion.
      </ekz>
    </dif>
  </snc>
  <snc mrk="homoge.0a.funkcio">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <klr>(p.p. <ref cel="bild.0igo">bildigo</ref> f inter du
      <ref cel="spac.vektora0o">vektoraj spacoj</ref>)</klr>
      Tia, ke por certa nombro &alfa; &ccirc;iam veras, ke 
      f(&lambda;*x)&#xa0;=&#xa0;&lambda;<sup>&alfa;</sup>*f(x).
    </dif>
  </snc>
  <trd lng="fr">homog&egrave;ne</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_d;&c_n;&c_o;&c_r;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_g;&c_o;&c_m;&c_o;&c_g;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
