<art mrk="$Id: karbol.xml,v 1.17 2023/10/18 14:48:10 revo Exp $">
<kap>
  <rad>karbol</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="karbol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="dif" cel="fenol.0o">Fenolo</ref>:
      <ekz>
        en 1834, <nom>Friedlieb Ferdinand Runge</nom> malkovris la
        fenolon tiam konata kiel
        <tld/>o, <tld/>ata acido, a&ubreve; fenata acido, kiun li elprenis el
        nepura formo
        de karbogudro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=581204">Joseph Lister</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_r;&c_b;&c_o;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_f;&c_e;&c_n;&c_o;&c_l;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>&agrave;cid carb&ograve;lic</trd>,
    <trd>fenol</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>karbol</trd>,
    <trd>karbolka</trd>,
    <trd>karbolov&aacute; voda</trd>
  </trdgrp>
  <trd lng="de">Karbol</trd>
  <trd lng="hu">karbol</trd>
  <trd lng="ja">&#x77F3;&#x70AD;&#x9178;  <pr>&#x305B;&#x304D;&#x305F;&#x3093;&#x3055;&#x3093;</pr></trd>
  <trd lng="nl">carbol</trd>
  <trdgrp lng="pl">
    <trd>fenol</trd>,
    <trd>karbol</trd>
  </trdgrp> 
  <trd lng="ro">fenol</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_r;&c_b;&c_o;&c_l;&c_o;&c_v;&c_a;&c_ja;
    &c_k;&c_i;&c_s;&c_l;&c_o;&c_t;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_b;&c_o;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_f;&c_je;&c_n;&c_o;&c_l;</trd>
  </trdgrp>
  <trd lng="sk">karbol</trd>
  <trd lng="uk">&#x43A;&#x430;&#x440;&#x431;&#x43E;&#x43A;&#x441;&#x438;&#x43B;</trd>
</drv>
</art>
