<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vek.xml,v 1.37 2024/03/11 12:39:07 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vek</rad>/i
</kap>
<drv mrk="vek.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vek.0i.eldormigi">
    <dif>
      <ref tip="super" cel="cxes.0igi">&Ccirc;esigi</ref> ies dormon:
      <ekz>
        kiam mi venis al li, li dormis; sed mi lin <tld/>is
        <fnt><bib>F</bib>, <lok>&FE; 20</lok></fnt>;
      </ekz>
      <ekz>
        se birdo tro bekas, la katon &gcirc;i <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        senplumigi kokinon, ne <tld/>ante mastrinon
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la an&gcirc;elo <klr>[&#8230;]</klr> <tld/>is min, kiel oni <tld/>as iun el lia dormo
        <fnt>
          <bib>MT</bib>
          <lok>Ze&hcirc;arja 4:1</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="pt">acordar</trd>
    <trdgrp lng="ru">
      <trd>&c_b;&c_u;&c_d;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_b;&c_u;&c_d;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="vek.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      &Ccirc;esigi la kvieton de io; vigligi, stimuli:
      <ekz>
        <ind>ne <tld/>u malfeli&ccirc;on, kiam &gcirc;i dormas</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">ne h&iacute;vd ki a sorsot</trd>
      </ekz>
      <ekz>
        <tld/>i la atenton de la a&ubreve;skultantoj;
      </ekz>
      <ekz>
        <tld/>u, ho <tld/>u, <tld/>u konstante, ne timu ridon,
        insulton
        <fnt>
          <aut>L. L. Zamenhof</aut>
          <vrk>Al la Fratoj</vrk>
        </fnt>!
      </ekz>
    </dif>
    <ref tip="vid" cel="incit.0i">inciti</ref>
    <trd lng="pt">acordar</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_b;&c_u;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_b;&c_u;&c_d;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="vek.0i.naski">
    <dif>
      <ref tip="sin" cel="nask.0i.estigi">Naski</ref>,
      <ref tip="super" cel="est.0igi">estigi</ref>:
      <ekz>
        Tamara <klr>[&#8230;]</klr> ne povas plu <tld/>i dezirojn en mi
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la glavo de la malamiko <tld/>as teruron &ccirc;irka&ubreve;e
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 6:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        mallerta, nigre-griza birdo, malbela kaj <tld/>anta abomenon
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu amo, kiu <klr>[&#8230;]</klr> en la kalikoj de la kreska&jcirc;oj <tld/>as la sopiron
        turni sin al la lumo de la suno
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        volo, kiu <tld/>as esperon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        raportoj liaj <tld/>is en Hamleto envion tiel
        grandan
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        alta revo <klr>[&#8230;]</klr> vekus pli da simpatio, pli fortan kunsenton
        <fnt>
          <bib>Marta</bib>
        </fnt>!
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ag">agigi</ref>,
      <ref cel="inicia.0i">iniciati</ref>,
      <ref cel="komenc.0i">komenci</ref>
    </refgrp>
    <trd lng="id"><baz>bangkit</baz>membangkitkan</trd>
    <trd lng="pt">incitar</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_b;&c_u;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_b;&c_u;&c_d;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_u;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_u;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_malmol;&c_b;&c_u;&c_zh;&c_d;&c_a;&c_m; &c_s;&c_je;</trd>
  <trd lng="bo">&#xF63;&#xF44;&#xF66;&#xF0B;</trd>
  <trd lng="ca">despertar</trd>
  <trdgrp lng="cs">
    <trd>budit</trd>,
    <trd>probouzet</trd>,
    <trd>probudit</trd>,
    <trd>vzbudit</trd>,
    <trd>vzbuzovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wecken</trd>,
    <trd>aufwecken</trd>,
    <trd><ind>munter</ind> machen</trd>
  </trdgrp>
  <trd lng="en">awaken</trd>
  <trd lng="es">despertar</trd>
  <trdgrp lng="fr">
    <trd>&eacute;veiller</trd>,
    <trd>r&eacute;veiller</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>felkelt</trd>,
    <trd>fel&eacute;breszt</trd>
  </trdgrp>
  <trd lng="id"><baz>bangun</baz>membangunkan</trd>
  <trdgrp lng="ja">
    <trd>&#x76EE;&#x899A;&#12417;&#12373;&#x305B;&#x308B;  <pr>&#12417;&#12374;&#12417;&#12373;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x547C;&#12403;&#x899A;&#x307E;&#12377;  <pr>&#12424;&#12403;&#12373;&#x307E;&#12377;</pr></trd>,
    <trd>&#x547C;&#12403;&#x8D77;&#12371;&#12377;  <pr>&#12424;&#12403;&#x304A;&#12371;&#12377;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>wekken</trd>,
    <trd>wakker maken</trd>
  </trdgrp>
  <trd lng="sk">zobudi&tcaron;</trd>
  <trd lng="tr">uyand&inodot;rmak</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_d;&c_i;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_r;&#x43E;&c_b;&c_u;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="vek.0a">
  <kap>
    <tld/>a
  </kap>
  <snc mrk="vek.0a.rilata">
    <dif>
      Rilata al <tld/>ado; <tld/>anta:
      <ekz>
        eksonis la unuaj <tld/>aj trumpetoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a horo, krio, sonoro.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_b;&c_u;&c_d;&c_zh;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
      <trd>&c_a;&c_b;&c_u;&c_d;&c_zh;&c_a;&c_ju;&c_ch;&c_y;</trd>
    </trdgrp>
    <trd lng="de">weckend</trd>
  <trd lng="id">hidup<klr> (adj.)</klr></trd>
  </snc>
  <snc mrk="vek.0a.maldorma">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="dorm.mal0a">Maldorma</ref>:
      <ekz>
        <tld/>a stato: se en son&gcirc;o vi vidas ke vi estas <tld/>a kaj konscia ke vi estas
        son&gcirc;anta, tio signifas ke vi estas gajnanta pli altan nivelon pri kontrolo de
        via son&gcirc;a stato
        <fnt>
          <aut>Reza Kheir-Khah</aut>
            <vrk><url ref="&Monato;/2000/007087.php">Mirinda dimensio de nia vivo</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 16</lok>
          </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="vigl.0a.fresxa">vigla<sncref/></ref>
    </refgrp>
    <rim>
      Tiu &ccirc;i uzo ignoras la transitivecon de la verbo &leftquot;<tld/>i&rightquot;. Kp
      <ref cel="kompli.0a.EVI">komplika<sncref/></ref>, <ref
      cel="korekt.0a.EVI">korekta<sncref/></ref>.
    </rim>
  <trdgrp lng="id">
    <trd><baz>bangun</baz>terbangun</trd>,
    <trd><baz>jaga</baz>terjaga</trd>
  </trdgrp>
  </snc>
  <trd lng="ja">&#x76EE;&#x899A;&#12417;&#12373;&#x305B;&#x308B;  <pr>&#12417;&#12374;&#12417;&#12373;&#x305B;&#x308B;</pr></trd>
</drv>

<drv mrk="vek.0o">
  <kap><tld/>o</kap>
  <snc mrk="vek.0o.ago">
    <dif>
      Ago <tld/>i:
      <ekz>
        trumpetadi por la <tld/>o;
      </ekz>
      <ekz>
        malfeli&ccirc;o kaj peko levi&gcirc;as sen <tld/>o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_b;&c_u;&c_d;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="ca">despertament</trd>
  <trd lng="cs">probuzen&iacute;</trd>
  <trd lng="de">Wecken</trd>
  <trdgrp lng="es">
    <trd>despertar</trd>,
    <trd>despertamiento</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>r&eacute;veil</trd>
  </trdgrp>
  <trd lng="hu">&eacute;breszt&odblac;</trd>
  <trd lng="id">bangun</trd>
  <trd lng="ja">&#x76EE;&#x899A;&#12417;&#12373;&#x305B;&#x308B;&#12371;&#12392;  <pr>&#12417;&#12374;&#12417;&#12373;&#x305B;&#x308B;&#12371;&#12392;</pr></trd>
  <trd lng="nl">het <ind>wekken</ind></trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_b;&c_u;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">prebudenie</trd>
</drv>

<drv mrk="vek.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="vek.0igxi.eldormigxi">
    <dif>
      Rekonscii&gcirc;i el dormo:
      <ekz>
        <nom>Jakob</nom> <tld/>i&gcirc;is el sia dormo
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 28:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Noa</nom> <tld/>i&gcirc;is de sia ebrieco
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 9:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iumatene mi <tld/>i&gcirc;as kun &ccirc;agreno
        <fnt><bib>ElektFab</bib>, <lok>pa&scirc;tistino de anseroj apud
        puto</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_s;&c_malmol;&c_b;&c_u;&c_zh;&c_d;&c_a;&c_m; &c_s;&c_je;</trd>
    <trd lng="de">aufwachen</trd>
    <trd lng="en">wake up</trd>
    <trdgrp lng="id">
      <trd>bangun</trd>,
      <trd><baz>bangun</baz>terbangun</trd>
    </trdgrp>
    <trd lng="pt">acordar</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_s;&c_y;&c_p;&c_a;&c_t;&c_mol;&c_s;&c_ja;<klr tip="ind">
      (&c_o;&c_t;&c_o;
      &c_s;&c_n;&c_a;)</klr></trd>,
      <trd>&c_p;&c_r;&c_o;&c_s;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_b;&c_u;&c_zh;&c_d;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_b;&c_u;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc mrk="vek.0igxi.plivigligxi">
    <dif>
      Plivigli&gcirc;i:
      <ekz>
        lia energio denove <tld/>i&gcirc;as en sia tuta forto
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;u, ho norda vento
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 4:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        nova, kvankam apena&ubreve; <tld/>i&gcirc;anta pasio
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        en lia koro <tld/>i&gcirc;is sova&gcirc;aj instinktoj, sopiroj kaj
        beda&ubreve;ro;
      </ekz>
      <ekz>
        <tld/>i&gcirc;is en li la vo&ccirc;o de la malamo
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 7a</lok></fnt>.
      </ekz>
    </dif>

    <trd lng="pt">acordar</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_b;&c_u;&c_zh;&c_d;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_b;&c_u;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc mrk="vek.0igxi.naskigxi">
    <dif>
      Naski&gcirc;i, esti&gcirc;i:
      <ekz>
        venas la fino, &gcirc;i <tld/>i&gcirc;as kontra&ubreve; vin, jen &gcirc;i venas
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 7:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la fundo de lia timo <tld/>i&gcirc;is fajrero de naiva espero
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        fiereco <klr>[&#8230;]</klr> denove en &scirc;i <tld/>i&gcirc;is
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="burgxo.0i">bur&gcirc;oni</ref>,
      <ref cel="gxerm.0i">&gcirc;ermi</ref>
    </refgrp>
    <trd lng="id">bangkit</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_b;&c_u;&c_zh;&c_d;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_b;&c_u;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_ch;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_a;&c_b;&c_u;&c_d;&c_z;&c_ib;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">despertar-se</trd>
  <trd lng="de">erwachen</trd>
  <trd lng="es">despertarse</trd>
  <trdgrp lng="fr">
    <trd>s'<ind>&eacute;veiller</ind></trd>,
    <trd>se r&eacute;veiller</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>felkel <klr tip="ind">(&aacute;lm&aacute;b&oacute;l)</klr></trd>,
    <trd>fel&eacute;bred</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x76EE;&#x899A;&#12417;&#x308B;  <pr>&#12417;&#12374;&#12417;&#x308B;</pr></trd>,
    <trd>&#x547C;&#12403;&#x899A;&#x307E;&#12373;&#x308C;&#x308B;  <pr>&#12424;&#12403;&#12373;&#x307E;&#12373;&#x308C;&#x308B;</pr></trd>,
    <trd>&#x8D77;&#x304D;&#x308B;  <pr>&#x304A;&#x304D;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>wakker worden</trd>,
    <trd>ontwaken</trd>
  </trdgrp>
  <trd lng="tr">uyanmak</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&#x43E;&c_b;&c_u;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&#x43F;&c_r;&#x43E;&#x43A;&c_i;&c_d;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&#x43F;&c_r;&#x43E;&c_s;&c_i;&#x43F;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
