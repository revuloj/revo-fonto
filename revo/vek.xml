<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vek</rad>/i
</kap>
<drv mrk="vek.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vek.0i.eldormigi">
    <dif>
      &Ccirc;esigi ies dormon:
      <ekz>
        kiam mi venis al li, li dormis; sed mi lin <tld/>is
        <fnt><bib>F</bib>, <lok>&FE; 20</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i iun el profunda dormo<fnt>B</fnt>.
      </ekz>
    </dif>
    <trd lng="pt">acordar</trd>
    <trdgrp lng="ru">
      <trd>&c_b;&c_u;&c_d;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_b;&c_u;&c_d;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      &Ccirc;esigi la kvieton de io; vigligi, stimuli:
      <ekz>
        <ind>ne <tld/>u malfeli&ccirc;on, kiam &gcirc;i dormas</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">ne h&iacute;vd ki a sorsot</trd>
      </ekz>
      <ekz>
        por kio <tld/>i senbezone la dan&gcirc;eron?<fnt>B</fnt>
      </ekz>
      <ekz>
        la kantoj <tld/>as la e&hcirc;ojn de la montoj<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i la atenton de la a&ubreve;skultantoj<fnt>B</fnt>,
      </ekz>
      <ekz>
        la kura&gcirc;on de la soldatoj;
      </ekz>
      <ekz>
        <tld/>i kaj venki la apation;
      </ekz>
      <ekz>
        <tld/>u, ho <tld/>u, <tld/>u konstante, ne timu ridon,
        insulton!<fnt>Z</fnt>
      </ekz>
      <ekz>
        <tld/>i dormantajn energiojn.
      </ekz>
    </dif>
    <trd lng="pt">acordar</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_b;&c_u;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_b;&c_u;&c_d;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Naski, estigi:
      <ekz>
        <tld/>i deziron al io, apetiton<fnt>Z</fnt>,
      </ekz>
      <ekz>
        entuziasmon, esperon, iluzion, dubon, suspekton, publikan
        intereson por, simpatian e&hcirc;on &ccirc;e la legantoj, da&ubreve;ran impreson,
        meditojn, apla&ubreve;don, ridon, tumulton<fnt>Z</fnt>,
      </ekz>
      <ekz>
        ribelon;
      </ekz>
      <ekz>
        raportoj liaj <tld/>is en Hamleto envion tiel grandan<fnt>Z</fnt>;
      </ekz>
      <ekz>
        estas facile <tld/>i malamon, sed malfacile &gcirc;in estingi;
      </ekz>
      <ekz>
        la glavo de la malamiko <tld/>as teruron &ccirc;irka&ubreve;e<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ravan senton &ccirc;iam <tld/>is en mi &scirc;iaj grandaj lumaj okuloj;
      </ekz>
      <ekz>
        tiu demando <tld/>is multe da disputoj kaj malpaco<fnt>Z</fnt>;
      </ekz>
      <ekz>
        teruro<tld/>antaj legendoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ag">agigi</ref>,
      <ref cel="inicia.0i">iniciati</ref>,
      <ref cel="komenc.0i">komenci</ref>
    </refgrp>
    <trd lng="pt">incitar</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_b;&c_u;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_b;&c_u;&c_d;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_u;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_u;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">budit</trd>
  <trd lng="de">wecken</trd>
  <trd lng="en">awaken</trd>
  <trd lng="es">despertar</trd>
  <trdgrp lng="fr">
    <trd>&eacute;veiller</trd>,
    <trd>r&eacute;veiller</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>felkelt</trd>,
    <trd>fel&eacute;breszt</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>wekken</trd>,
    <trd>wakker maken</trd>
  </trdgrp>
</drv>

<drv mrk="vek.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        trumpetadi por la <tld/>o;
      </ekz>
      <ekz>
        malfeli&ccirc;o kaj peko levi&gcirc;as sen <tld/>o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_b;&c_u;&c_d;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="de">Wecken</trd>
  <trdgrp lng="es">
    <trd>despertar</trd>,
    <trd>despertamiento</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>r&eacute;veil</trd>
  </trdgrp>
  <trd lng="hu">&eacute;breszt&odblac;</trd>  
  <trd lng="nl">het <ind>wekken</ind></trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_b;&c_u;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="vek.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="vek.0igxi.eldormigxi">
    <dif>
      Rekonscii&gcirc;i el dormo:
      <ekz>
        &ccirc;iumatene mi <tld/>i&gcirc;as kun &ccirc;agreno
        <fnt><bib>ElektFab</bib>, <lok>pa&scirc;tistino de anseroj apud
        puto</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">aufwachen</trd>
    <trd lng="es">despertarse</trd>
    <trd lng="pt">acordar</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_s;&c_y;&c_p;&c_a;&c_t;&c_mol;&c_s;&c_ja;<klr tip="ind"> (&c_o;&c_t;&c_o; &c_s;&c_n;&c_a;)</klr></trd>,
      <trd>&c_p;&c_r;&c_o;&c_s;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_b;&c_u;&c_zh;&c_d;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_b;&c_u;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Plivigli&gcirc;i:
      <ekz>
        <tld/>i&gcirc;as la vento<fnt>Z</fnt>;
      </ekz>
      <ekz>
        en lia koro <tld/>i&gcirc;is sova&gcirc;aj instinktoj, sopiroj kaj beda&ubreve;ro;
      </ekz>
      <ekz>
        <tld/>i&gcirc;is en li la vo&ccirc;o de la malamo
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 7a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">erwachen</trd>
    <trd lng="es">despertarse</trd>
    <trd lng="pt">acordar</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_b;&c_u;&c_zh;&c_d;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_b;&c_u;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Naski&gcirc;i, esti&gcirc;i:
      <ekz>
        sur la fundo de lia timo <tld/>i&gcirc;is fajrero de naiva
        espero<fnt>B</fnt>;
      </ekz>
      <ekz>
        la Esperanta movado &jcirc;us <tld/>i&gcirc;is en la lando.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="burgxo.0i">bur&gcirc;oni</ref>,
      <ref cel="gxerm.0i">&gcirc;ermi</ref>
    </refgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_b;&c_u;&c_zh;&c_d;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_b;&c_u;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_ch;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_a;&c_b;&c_u;&c_d;&c_z;&c_ib;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
    <trd lng="es">despertarse</trd>
  <trdgrp lng="fr">
    <trd>s'<ind>&eacute;veiller</ind></trd>,
    <trd>se r&eacute;veiller</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>felkel <klr tip="ind">(&aacute;lm&aacute;b&oacute;l)</klr></trd>,
    <trd>fel&eacute;bred</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>wakker worden</trd>,
    <trd>ontwaken</trd>
  </trdgrp>
</drv>
</art>
