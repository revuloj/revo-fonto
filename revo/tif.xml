<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tif.xml,v 1.23 2022/03/30 18:34:23 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>tif</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tif.0o">
  <kap><tld/>o</kap>
  <snc mrk="tif.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Infekta febra intesta 
      <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>:
      <ekz>
        oni alveturigis la kamparanon, malsanan je <tld/>o
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XXVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la mallibereja <tld/>o, kiu disvasti&gcirc;is en la urbo, pligrandigis la &gcirc;eneralan
        timon
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        pedikoj disvastigis <tld/>on
        <fnt><aut>J. Baghy</aut>, <vrk>Sur Sanga Tero</vrk>, 
        <lok>1933</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_y;&c_f;&c_u;&c_s;</trd>
  <trd lng="ca">tifus</trd>
  <trd lng="cs">tyfus</trd>
  <trd lng="de">Typhus</trd>
  <trd lng="en">typhus</trd>
  <trd lng="fa">&a_t;&f_y;&a_f;&a_w;&a_s;</trd>
  <trd lng="fr">typhus</trd>
  <trd lng="hu">t&iacute;fusz</trd>
  <trd lng="id">tifus</trd>
  <trd lng="nl">tyfus</trd>
  <trdgrp lng="pl">
    <trd>dur</trd>,
    <trd>tyfus</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>tifo</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_i;&c_f;</trd>
  <trd lng="sk">t&yacute;fus</trd>
  <trd lng="uk">&c_t;&c_i;&c_f;</trd>
</drv>
</art>
