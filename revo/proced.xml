<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>proced</rad>/i
</kap>
<drv mrk="proced.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Uzi difinitajn rimedojn por efektivigi ion:
      <ekz>
        metode, senorde, energie <tld/>i.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_je;&c_j;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">procedir</trd>
  <trdgrp lng="de">
    <trd>vorgehen</trd>,
    <trd>verfahren</trd>
  </trdgrp>
  <trd lng="en">proceed</trd>
  <trd lng="es">proceder</trd>
  <trd lng="hu">elj&aacute;r</trd>
  <trd lng="nl">te <ind>werk</ind> gaan</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_s;&c_t;&c_u;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="proced.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Intenca agmaniero difinita por atingi iun celon:
      <ekz>
        la novaj <tld/>oj pri fabrikado, por lumigado;
      </ekz>
      <ekz>
        &hcirc;irurgia <tld/>o;
      </ekz>
      <ekz>
        la vivo tro frue komencis sur &scirc;i la teruran <tld/>on de
        detruado<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_je;&c_ja;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_c;&c_e;&c_s;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_c;&c_e;&c_d;&c_u;&c_r;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Vorgehen</trd>,
    <trd>Verfahren</trd>
  </trdgrp>
  <trd lng="hu">elj&aacute;r&aacute;s</trd>
  <trdgrp lng="nl">
    <trd>proc&eacute;d&eacute;</trd>,
    <trd>werkwijze</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_c;&c_je;&c_s;&c_s;</trd>
  </trdgrp>
</drv>
</art>
