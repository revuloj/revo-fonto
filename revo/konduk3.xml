<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: konduk3.xml,v 1.14 2022/11/09 21:32:45 revo Exp $">
<kap>
  <rad>kondukt</rad>/i
</kap>
<drv mrk="konduk3.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">ELE</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="dif" cel="konduk.0i.FIZ_ELE">Konduki<sncref/></ref>:
      <ekz>
        ni vidis rostitan turfalkon en masto de alttensia <tld/>lineo
        <fnt>
          <bib>Monato</bib>
          <aut>Walter, Renate, Irene, Stefan Klag</aut>
          <vrk><url ref="&Monato;/2007/009167.php">Birdo de la jaro</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de TAP, trapasos Albanion, ebligante provizadon per natura gaso de
        Montenegro, Bosnio-Hercegovino, Makedonio, Kroatio kaj eventuale Serbio
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2013/010879.php">TAP: projekto por sud-e&ubreve;ropa
          gaskoridoro</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>b&yacute;t vodiv&yacute;</trd>,
    <trd>vodit <klr>(el., fyz.)</klr></trd>,
    <trd>v&eacute;st</trd>
  </trdgrp>
  <trd lng="de">leiten <klr>(Phys.)</klr></trd>
  <trd lng="en">to conduct</trd>
  <trdgrp lng="fr">
    <trd>conduire<klr tip="ind"> (phys.)</klr></trd>,
    <trd>&ecirc;tre <ind>conducteur</ind> (de)</trd>,
  </trdgrp>
  <trd lng="nl">geleiden<klr tip="ind"> (fys.)</klr></trd>
  <trd lng="pl">przewodzi&cacute;</trd>     
  <trd lng="ro">conduce</trd>
  <trdgrp lng="sk">
    <trd>by&tcaron; vodiv&yacute;</trd>,
    <trd>vies&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="konduk3.0anto">
  <kap><tld/>anto</kap>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">ELE</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="dif" cel="konduk.0anto.FIZ_ELE">Kondukanto</ref>:
      <ekz>
        <tld/>anto <klr>[&#8230;]</klr> povas akcepti la alvenantajn elektronojn
        <fnt>
          <vrk><url ref="&STEB;vortaroj/elektrofaka-terminaro.pdf">Elektrofaka Klariga Terminaro -
          Eventoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">vodi&ccaron;</trd>
  <trd lng="de">Leiter <klr>(Phys.)</klr></trd>
  <trd lng="en">conductor</trd>
  <trd lng="fr">conducteur<klr tip="ind"> (subst., phys.)</klr></trd>
  <trd lng="nl">geleider<klr tip="ind"> (fys.)</klr></trd>
  <trd lng="pl">przewodz&aogonek;cy</trd>     
  <trd lng="ro">conducere</trd>
  <trd lng="sk">vodi&ccaron;</trd>
</drv>

<drv mrk="konduk3.0eco">
  <kap><tld/>eco</kap>
  <uzo tip="fak">ELE</uzo>
  <uzo tip="fak">FIZ</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="konduk4.0o.grando">Konduktivo</ref>:
      <ekz>
        la termika <tld/>eco de betonbloko valoras 0,9 W/m&middot;K
        <fnt>
          <vrk><url ref="&Viki;/Betonbloko">Betonbloko - Vikipedio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">vodivost</trd>
  <trd lng="de">Leitf&auml;higkeit</trd>
  <trd lng="en">conduction</trd>
  <trd lng="nl">geleidbaarheid<klr tip="ind"> (arch.)</klr></trd>
  <trdgrp lng="pl">
    <trd>przewodno&sacute;&cacute;</trd>,
    <trd>konduktywno&sacute;&cacute;</trd>
  </trdgrp>
  <trd lng="ro">conductivitate</trd>
  <trd lng="sk">vodivos&tcaron;</trd>
</drv>

<drv mrk="konduk3.0ema">
  <kap><tld/>ema</kap>
  <uzo tip="fak">ELE</uzo>
  <uzo tip="stl">ARK</uzo>
  <fnt><bib>ScTerm</bib><lok>p. 33</lok></fnt>
  <snc>
    <dif>
      <ref cel="konduk.0i.FIZ_ELE">Kondukanta</ref> elektron.
    </dif>
  </snc>
  <trd lng="de">leitend <klr>(Phys.)</klr></trd>
  <trd lng="en">conducting</trd>
  <trd lng="nl">geleidbaar<klr tip="ind"> (arch.)</klr></trd>
</drv>

<drv mrk="konduk3.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">ELE</uzo>
  <fnt><bib>ScTerm</bib><lok>p. 34</lok></fnt>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="dif" cel="konduk.0ilo.ELE">Kondukilo</ref>:
      <ekz>
        kiam la lampo varmi&gcirc;as, la filamentoj i&gcirc;as pli fortaj <tld/>iloj male al la
        aktualaj, kiuj perdas potencon kiam ili varmi&gcirc;as
        <fnt>
          <vrk><url ref="https://tekstaro.com/t?nomo=kontakto-2011-2019">La centjara
          lampo</url></vrk>
          <lok>Kontakto 2011-2019</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">vodi&ccaron;</trd>
  <trdgrp lng="de">
    <trd>Leitung</trd>,
    <trd>Stromleitung</trd>,
    <trd>Kabel</trd>,
    <trd>Stromkabel</trd>,
    <trd>Leiter</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>conductor cable</trd>,
    <trd>conductor</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>conducteur<klr tip="ind"> (fil &eacute;lectrique)</klr></trd>,
    <trd>fil &eacute;lectrique</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kabel</trd>,
    <trd>snoer</trd>
  </trdgrp>
  <trd lng="pl">przewodnik <klr>(elektryczny)</klr></trd>     
  <trd lng="ro">conductor</trd>
  <trd lng="sk">vodi&ccaron;</trd>
</drv>

<drv mrk="konduk3.duon0anto">
  <kap>duon<tld/>anto</kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">ELE</uzo>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <dif>
      <ref tip="dif" cel="konduk.duon0anto">duonkondukanto</ref>:
      <ekz>
        duon<tld/>anto GaAs
        <fnt>
          <vrk><url ref="&Viki;/Arseno">Arseno - Vikipedio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        duon<tld/>anto konvertas la signalon al kontinua kurento, kiu
        <klr>[&#8230;]</klr> povas &scirc;argi bateriojn
        <fnt>
          <vrk><url
          ref="https://www.libe.slikom.info/Esperanta%20ligilo/2019/Esperanta%20Ligilo,%20N-ro%204,%202019.htm">www.libe.slikom.info</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Halbleiter</trd>
  <trd lng="en">semiconductor</trd>
  <trdgrp lng="fr">
    <trd>semi-conducteur<klr tip="ind"> (subst.)</klr></trd>,
    <trd>semiconducteur<klr tip="ind"> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="nl">halfgeleider</trd>
  <trd lng="pl">p&oacute;&lstroke;przewodnik</trd>     
  <trd lng="ro">semiconductor</trd>
</drv>

<drv mrk="konduk3.super0anto">
  <kap>super<tld/>anto</kap>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">ELE</uzo>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <dif>
      <ref tip="dif" cel="konduk.super0anto">Superkondukanto</ref>:
      <ekz>
        tiuj magnete levitaj veturiloj (t.n. maglevo) uzos altiran kaj forpu&scirc;an fortojn
        kreatajn de super-<tld/>anta magneto en la vagonoj kun induktaj bobenoj sur
        la gvidaj vojmuroj
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2014/011390.php">E&ccirc; pli rapide</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">supervodi&ccaron;</trd>
  <trd lng="de">Supraleiter</trd>
  <trd lng="en">superconductor</trd>
  <trdgrp lng="fr">
    <trd>supraconducteur<klr tip="ind"> (subst.)</klr></trd>,
    <trd>superconducteur<klr tip="ind"> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="nl">supergeleider</trd>
  <trd lng="pl">nadprzewodnik</trd>     
  <trd lng="ro">supraconductor</trd>
  <trd lng="sk">supravodi&ccaron;</trd>
</drv>
</art>

