<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>neniel</rad>
</kap>
<drv mrk="neniel.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Adv. signifanta: &leftquot;la&ubreve; nenia maniero&rightquot;:
      <ekz>
        ni <tld/> interesi&gcirc;as pri tio;
      </ekz>
      <ekz>
        ili <tld/> lasis sin influi;
      </ekz>
      <ekz>
        mi <tld/> povas kompreni, kion vi parolas.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>auf keine <ind>Weise</ind></trd>,
    <trd>in keiner <ind>Weise</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>op geen enkele <ind>wijze</ind></trd>,
    <trd>op geen enkele <ind>manier</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_i;&c_k;&c_a;&c_k;</trd>,
    <trd>&c_n;&c_i;&c_k;&c_o;&c_i;&c_m; &c_o;&c_b;&c_r;&c_a;&c_z;&c_o;&c_m;</trd>
  </trdgrp>
</drv>
</art>
