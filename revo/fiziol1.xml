<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>fiziologi</rad>/o
</kap>
<drv mrk="fiziol1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SCI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="scienc.0o.fako">Scienco</ref> pri la organoj k
      funkcioj de la vivantaj esta&jcirc;oj.  
    </dif>
  </snc>
  <trd lng="de">Physiologie</trd>
  <trd lng="es">fisiolog&iacute;a</trd>
  <trd lng="fr">physiologie</trd>
  <trdgrp lng="hu">
    <trd>fiziol&oacute;gia</trd>,
    <trd>&eacute;lettan</trd>
  </trdgrp>
  <trd lng="nl">fysiologie</trd>
  <trd lng="ru">&c_f;&c_i;&c_z;&c_i;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
</drv>
<drv mrk="fiziol1.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Specialisto pri <tld/>o.
    </dif>
    <ref tip="sin" cel="fiziol.0o">fiziologo..</ref>
  </snc>
  <trd lng="de">Physiologe</trd>
  <trd lng="es">fisi&oacute;logo</trd>
  <trdgrp lng="fr">
    <trd>physiologue</trd>,
    <trd>physiologiste</trd></trdgrp> 
  <trd lng="hu">fiziol&oacute;gus</trd>
  <trd lng="nl">fysioloog</trd>
  <trd lng="ru">&c_f;&c_i;&c_z;&c_i;&c_o;&c_l;&c_o;&c_g;</trd>
</drv>
</art>
