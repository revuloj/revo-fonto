<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fiziol1.xml,v 1.24 2023/11/09 20:48:33 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>fiziologi</rad>/o
</kap>
<drv mrk="fiziol1.0o">
  <kap><tld/>o</kap>
  <snc mrk="fiziol1.0o.SCI">
    <uzo tip="fak">FIZL</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="super" cel="scienc.0o.fako">Scienco</ref> pri la
      organoj kaj funkcioj de vivuloj:
      <ekz>
        en 1976 <nom>Gajdusek</nom> ricevis la Nobel-premion pri <tld/>o kaj medicino
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2006/008861.php">Nova etapo en biologio kaj medicino</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        evolua <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="&Monato;/2009/009858.php">La mondrevolucia verko finfine en Esperanto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        metabolo kaj <tld/>o varias depende de la a&gcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2014/011193.php">Malsama a&gcirc;o, malsamaj riskoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ili parolis pri la <tld/>o kaj historio de l' lingvoj
        <fnt>
          <bib>DL</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_z;&c_ib;&c_ja;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="cs">fyziologie</trd>
  <trd lng="de">Physiologie</trd>
  <trd lng="en">physiology</trd>
  <trd lng="es">fisiolog&iacute;a</trd>
  <trd lng="fr">physiologie</trd>
  <trdgrp lng="hu">
    <trd>fiziol&oacute;gia</trd>,
    <trd>&eacute;lettan</trd>
  </trdgrp>
  <trd lng="ja">&#x751F;&#x7406;&#x5B66;  <pr>&#x305B;&#x3044;&#x308A;&#x304C;&#x304F;</pr></trd>
  <trd lng="nl">fysiologie</trd>
  <trd lng="pt">fisiologia</trd>
  <trd lng="ru">&c_f;&c_i;&c_z;&c_i;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="sk">fyziol&oacute;gia</trd>
  <trd lng="uk">&#x444;&#x456;&#x437;&#x456;&#x43E;&#x43B;&#x43E;&#x433;&#x456;&#x44F;</trd>
  <trd lng="zh">&#x751F;&#x7406;&#x5B66; <pr>sh&#x113;ngl&#x1D0;xu&#xE9;</pr></trd>
</drv>

<drv mrk="fiziol1.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="fiziol1.0isto.SCI">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Specialisto pri <tld/>o:
      <ekz>
        la rusa <tld/>isto kaj ne&ubreve;rologo <nom>Ivan Mi&hcirc;ajlovi&ccirc; Se&ccirc;enov</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=402428">5234 Sechenov</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Erasmus Darwin</nom> <klr>[&#x2026;]</klr> estis angla kuracisto, naturfilozofo, <tld/>isto,
        inventisto kaj poeto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=230684">Erasmus Darwin</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="fiziol.0o">fiziologo</ref>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_z;&c_ib;&c_jo;&c_l;&c_ja;&c_g;</trd>
  <trd lng="de">Physiologe</trd>
  <trd lng="en">physiologist</trd>
  <trd lng="es">fisi&oacute;logo</trd>
  <trdgrp lng="fr">
    <trd>physiologue</trd>,
    <trd>physiologiste</trd>
  </trdgrp>
  <trd lng="hu">fiziol&oacute;gus</trd>
  <trd lng="nl">fysioloog</trd>
  <trdgrp lng="pt">
    <trd>fisiologista</trd>,
    <trd>fisi&oacute;logo</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_i;&c_z;&c_i;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="zh">&#x751F;&#x7406;&#x5B66;&#x5BB6; <pr>sh&#x113;ngl&#x1D0;xu&#xE9;ji&#x101;</pr></trd>
</drv>
</art>
