<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kale&scirc;</rad>/o
</kap>
<drv mrk="kalesx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Kvarrada, kunrisorta, &gcirc;enerale kovrita
      <ref tip="super" cel="vetur.0ilo">veturilo</ref> por personoj:
      <ekz>vojo neirebla por <tld/>oj.</ekz>
    </dif>
    <trd lng="en">carriage</trd>
  </snc>
  <trd lng="fr">cal&egrave;che</trd>
  <trd lng="nl">koets</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_l;&c_ja;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_e;&c_k;&c_i;&c_p;&c_a;&c_zh;</trd>
  </trdgrp>
</drv>
<drv mrk="kalesx.0ego">
  <kap><tld/>ego<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Granda luksa <tld/>o. 
    </dif>
  </snc>
  <trd lng="fr">carrosse</trd>
  <trd lng="nl">statiekoets</trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_je;&c_t;&c_a;</trd>
</drv>
<drv mrk="kalesx.0isto">
  <kap><tld/>isto</kap>
  <snc num="1">
    <dif>
      Kondukanto de <tld/>o. 
    </dif>
    <trd lng="en">coachman</trd>
    <trd lng="nl">koetsier</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_u;&c_ch;&c_je;&c_r;</trd>,
      <trd>&c_i;&c_z;&c_v;&c_o;&c_z;&c_ch;&c_i;&c_k;</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <dif>
      Konstruisto de <tld/>oj.  
    </dif>
    <trd lng="en">carriage maker</trd>
    <trd lng="nl">koetsenmaker</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_a;&c_r;&c_je;&c_t;&c_n;&c_i;&c_k;</trd>,
      <trd>&c_e;&c_k;&c_i;&c_p;&c_a;&c_zh;&c_n;&c_y;&c_j; &c_m;&c_a;&c_s;&c_t;&c_je;&c_r;</trd>
    </trdgrp>
  </snc>
</drv>
<drv mrk="kalesx.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      &Ccirc;io en veturilo, kio ne estas la radaro kaj la motoro.  
    </dif>
  </snc>
  <trd lng="en">chassis</trd>
  <trd lng="fr">carrosserie</trd>
  <trd lng="nl">carosserie</trd>
</drv>
</art>
