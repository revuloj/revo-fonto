<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Tales</rad>/o
</kap>
<drv mrk="tales.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Greklingve:
      <trd lng="grc">&Theta;&alfa;&lambda;&eta_circ;&sigma_fina;</trd>.
      Greka filozofo kaj matematikisto, inter la 7a kaj 6a jc a.K.
    </dif>
  </snc>
  <trd lng="de">Thales</trd>
  <trd lng="en">Thales</trd>
  <trd lng="fr">Thal&egrave;s</trd>
  <trd lng="nl">Thales</trd>
  <trd lng="ru">&c_F;&c_a;&c_l;&c_je;&c_s;</trd>
</drv>

<drv mrk="tales.t0a">
  <kap><tld lit="t"/>a</kap>
  <snc>
    <dif>
      Iel rilatanta al la verko de <tld/>o:
      <ekz>
        <uzo tip="fak">MAT</uzo>
        <ind><tld lit="t"/>a teoremo</ind>
        <klr>(vd <ref cel="homote.0a.figuroj">homotetia</ref>
        figuro)</klr>.
        <trd lng="fr">th&eacute;or&egrave;me de Thal&egrave;s</trd>
        <trd lng="nl">stelling van <ind>Thales</ind></trd>
        <trd lng="ru">&c_t;&c_je;&c_o;&c_r;&c_je;&c_m;&c_a; <ind>&c_F;&c_a;&c_l;&c_je;&c_s;&c_a;</ind></trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Thales-</trd>,
    <trd>von <ind>Thales</ind></trd></trdgrp> 
  <trd lng="en">Thales['s]</trd>
  <trd lng="fr">de <ind>Thal&egrave;s</ind></trd>
  <trd lng="nl">van <ind>Thales</ind></trd>
  <trd lng="ru">&c_F;&c_a;&c_l;&c_je;&c_s;&c_a;</trd>
</drv>
</art>
