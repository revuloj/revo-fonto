<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tales.xml,v 1.22 2023/09/04 17:49:39 revo Exp $">
<kap>
  <rad>Tales</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tales.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Greklingve:
      <trd lng="grc">&Theta;&alfa;&lambda;&eta_circ;&sigma_fina;</trd>,
      greka <ref tip="lst" lst="voko:historiaj_personoj" cel="filozo.0o">filozofo</ref> kaj 
      matematikisto, inter la 7a kaj 6a jc a.K.:
      <ekz>
        la&ubreve; la teoremo de <tld/>o, triangulo enskribita en duoncirklo kaj kies longa
        flanko koincidas kun ties diametro estas ortangula;
      </ekz>
      <ekz>
        &ccirc;u li estas fama pro siaj lertaj inventa&jcirc;oj, simile al <tld/>o en Mileto
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Dek</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_T;&c_a;&c_l;&c_je;&c_s; <klr>(&c_M;&c_ib;&c_l;&c_je;&c_c;&c_k;&c_ib;)</klr></trd>
  <trd lng="bg">&c_T;&c_a;&c_l;&c_je;&c_s;</trd>
  <trd lng="br">Thales</trd>
  <trdgrp lng="ca">
    <trd>Tales</trd>,
    <trd>Tales de Milet</trd>
  </trdgrp>
  <trd lng="cs">Thales</trd>
  <trd lng="de">Thales</trd>
  <trd lng="en">Thales</trd>
  <trd lng="fa">&a_T;&a_A;&a_l;&a_s;</trd>
  <trd lng="fr">Thal&egrave;s</trd>
  <trd lng="hu">Thal&eacute;sz</trd>
  <trd lng="it">Talete</trd>
  <trd lng="ja">&#x30BF;&#x30EC;&#x30B9;</trd>
  <trd lng="nl">Thales</trd>
  <trd lng="pl">Tales</trd>
  <trdgrp lng="pt">
   <trd>Tales</trd>
  </trdgrp>
  <trd lng="ru">&c_F;&c_a;&c_l;&c_je;&c_s;</trd>
  <trd lng="sk">Thales</trd>
  <trd lng="uk">&c_F;&c_a;&c_l;&c_je;&c_s; (&c_m;&c_a;&c_t;&c_je;&c_m;&c_a;&c_t;&c_i;&c_k;)</trd>
</drv>

<drv mrk="tales.t0a">
  <kap><tld lit="t"/>a</kap>
  <snc>
    <dif>
      Iel rilatanta al la verko de <tld/>o:
      <ekz>
        <uzo tip="fak">MAT</uzo>
        <ind><tld lit="t"/>a teoremo</ind>
        <klr>(vd <ref cel="homote.0a.figuroj">homotetia</ref>
        figuro)</klr>.
        <trd lng="be">&c_t;&c_e;&c_a;&c_r;&c_e;&c_m;&c_a; &c_T;&c_a;&c_l;&c_je;&c_s;&c_a;</trd>        
        <trd lng="bg">&c_T;&c_je;&c_o;&c_r;&c_je;&c_m;&c_a; &c_n;&c_a; &c_T;&c_a;&c_l;&c_je;&c_s;</trd>              
         
        <trd lng="br">teorem Thales</trd>
        <trd lng="ca">teorema de <ind>Tales</ind></trd>
        <trd lng="de">Satz von Thales</trd>
        <trd lng="en">Thales['s] theorem</trd>
        <trd lng="fa">&a_q;&a_D;&f_y;&a_h;&a_hamza_sure; &a_T;&a_A;&a_l;&a_s;</trd>
        <trd lng="fr">th&eacute;or&egrave;me de Thal&egrave;s</trd>
        <trd lng="hu">Thal&eacute;sz-t&eacute;tel</trd>
        <trd lng="it">teorema di Talete</trd>
        <trd lng="nl">stelling van <ind>Thales</ind></trd>
        <trd lng="pl">twierdzenie Talesa</trd>
        <trd lng="ru">&c_t;&c_je;&c_o;&c_r;&c_je;&c_m;&c_a; <ind>&c_F;&c_a;&c_l;&c_je;&c_s;&c_a;</ind></trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_T;&c_a;&c_l;&c_je;&c_s;&c_a;</trd>,
    <trd>&c_t;&c_a;&c_l;&c_je;&c_s;&c_a;&c_w;</trd>
  </trdgrp>
  <trd lng="br">-<ind>Thales</ind></trd>
  <trd lng="ca">de <ind>Tales</ind></trd>
  <trdgrp lng="de">
    <trd>Thales-</trd>,
    <trd>des <ind>Thales</ind></trd>,
    <trd>von <ind>Thales</ind></trd>
  </trdgrp> 
  <trd lng="en">Thales['s]</trd>
  <trd lng="fr">de <ind>Thal&egrave;s</ind></trd>
  <trdgrp lng="hu">
    <trd>Thal&eacute;sz-</trd>,
    <trd>thal&eacute;szi</trd></trdgrp>
  <trd lng="it">di <ind>Talete</ind></trd>
  <trd lng="nl">van <ind>Thales</ind></trd>
  <trd lng="pl">Talesa</trd>
  <trd lng="ru">&c_F;&c_a;&c_l;&c_je;&c_s;&c_a;</trd>
</drv>
</art>
