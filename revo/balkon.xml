<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>balkon</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="balkon.0o">
  <kap><tld/>o</kap>
  <snc mrk="balkon.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Elstara plato &ccirc;e fasado de domo,
      &ccirc;irka&ubreve;ita per balustrado kaj alirebla
      de la interno.
    </dif>
    <refgrp tip="vid">
      <ref cel="altan.0o">altano</ref>,
      <ref cel="verand.0o">verando</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_l;&c_k;&c_o;&c_n;</trd>
  <trd lng="bg">&c_b;&c_a;&c_l;&c_k;&c_o;&c_n;</trd>
  <trd lng="ca">balc&oacute;</trd>
  <trd lng="de">Balkon</trd>
  <trd lng="en">balcony</trd>
  <trd lng="es">balc&oacute;n</trd>
  <trd lng="fa">&a_b;&a_A;&a_l;&f_k;&a_n;</trd>
  <trd lng="fr">balcon</trd>
  <trdgrp lng="hu">
     <trd>erk&eacute;ly</trd>,
     <trd>balkon</trd>
  </trdgrp>
  <trd lng="nl">balkon</trd>
  <trd lng="pl">balkon</trd>
  <trd lng="ru">&c_b;&c_a;&c_l;&c_k;&c_o;&c_n;</trd>
  <trd lng="sk">balk&oacute;n</trd>
  <trd lng="tr">balkon</trd>
</drv>
</art>
