<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>balkon</rad>/o
</kap>
<drv mrk="balkon.0o">
  <kap><tld/>o</kap>
  <snc>
   <dif>
    Plata elstara&jcirc;o &ccirc;e fasado de domo,
    &ccirc;irka&ubreve;ita per balustrado k komuniki&gcirc;anta
    kun la interno.
   </dif>
   <refgrp tip="vid">
    <ref cel="altan.0o">altano</ref>,
    <ref cel="verand.0o">verando</ref>.
   </refgrp>
  </snc>
  <trd lng="de">Balkon</trd>
  <trd lng="fr">balcon</trd>
  <trd lng="nl">balkon</trd>
  <trd lng="ru">&c_b;&c_a;&c_l;&c_k;&c_o;&c_n;</trd>
  <trd lng="tr">balkon</trd>
</drv>
</art>
