<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>spindel</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="spinde.0o">
  <kap><tld/>o</kap>
  <snc mrk="spinde.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Fera vergo en la &scirc;pinma&scirc;ino, sur kiun oni enigas bobeneton.
    </dif>
    <trdgrp lng="fr">
      <trd>broche<klr> (tissage)</klr></trd>,
      <trd>fuseau<klr> (tissage)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="spinde.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Pli malpli konusforma akso, &ccirc;irka&ubreve; kiu rotacias rado a&ubreve; alia
      peco:
      <ekz>
        <tld/>o de borilo, glitvalvo, &scirc;ra&ubreve;btenilo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>krajkářská palička</trd>,
    <trd>vřeteno</trd>
  </trdgrp>
  <trd lng="de">Spindel</trd>
  <trd lng="en">spindle</trd>
  <trd lng="hu">ors&oacute;</trd>
  <trdgrp lng="nl">
    <trd>spoel</trd>,
    <trd>spil</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_p;&c_i;&c_n;&c_d;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">vreteno</trd>
</drv>

<drv mrk="spinde.0rado">
  <kap><tld/>rado</kap>
  <snc mrk="spinde.0rado.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Konusforma rado, &ccirc;irka&ubreve; kiu volvi&gcirc;as &ccirc;eno de horlo&gcirc;o.
    </dif>
  </snc>
  <trd lng="en">fusee</trd>
  <trd lng="fr">fus&eacute;e<klr> (horlogerie)</klr></trd>
</drv>
</art>
