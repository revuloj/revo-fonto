<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: jxabot.xml,v 1.18 2023/12/15 06:30:08 revo Exp $">
<kap>
  <rad>&jcirc;abot</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="jxabot.0o">
  <kap><tld/>o</kap>
  <snc mrk="jxabot.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Muslina a&ubreve; punta
      <ref tip="super" cel="galant2.0o">galanterio</ref>,
      almetita &ccirc;e la surbrusta fendo de la &ccirc;emizo a&ubreve;
      korsa&jcirc;o:
      <ekz>
        &scirc;velformaj <tld/>oj kaj dikaj broda&jcirc;oj vidi&gcirc;adis
        sur iliaj brustoj
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tuk.kol0o">koltuko</ref>,
      <ref cel="kravat.0o">kravato</ref>
    </refgrp>
    <bld
      lok="&WCU;/thumb/2/2d/Portrait_of_a_gentleman_wearing_a_dark_jacket_and_lace_jabot_with_a_blue_ribbon_(by_Jacob_Ferdinand_Voet).jpg/361px-Portrait_of_a_gentleman_wearing_a_dark_jacket_and_lace_jabot_with_a_blue_ribbon_(by_Jacob_Ferdinand_Voet).jpg"
      prm="PD"
      >Jacob Ferdinand Voet, portreto de &gcirc;entlemano kun <tld/>o
        <fnt>
          <aut>Jacob Ferdinand Voet, https://www.sothebys.com/en/buy/auction/2021/the-rafael-valls-sale-part-ii/portrait-of-a-gentleman-wearing-a-dark-jacket-and</aut>
          <lok><url ref="&WCW;/File:Portrait_of_a_gentleman_wearing_a_dark_jacket_and_lace_jabot_with_a_blue_ribbon_(by_Jacob_Ferdinand_Voet).jpg"
              >https://commons.wikimedia.org/wiki/File:Portrait_of_a_gentleman_wearing_a_dark_jacket_and_lace_jabot_with_a_blue_ribbon_(by_Jacob_Ferdinand_Voet).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_b;&c_o;</trd>
  <trdgrp lng="de">
    <trd>Jabot</trd>,
    <trd>R&uuml;schenhemdkrause</trd>
  </trdgrp>
  <trd lng="en">jabot</trd>
  <trd lng="fr">jabot<klr tip="ind"> (de v&ecirc;tement)</klr></trd>
  <trdgrp lng="hu">
    <trd>mellfodor</trd>,
    <trd>zsab&oacute;</trd>
  </trdgrp>
  <trd lng="ja">&#12472;&#12515;&#12508;</trd>
  <trd lng="nl">jabot</trd>
  <trd lng="pl">&zdot;abot</trd>
  <trd lng="pt">jab&ocirc;</trd>
  <trd lng="ru">&c_zh;&c_a;&c_b;&c_o;</trd>
  <trdgrp lng="it">
    <trd>gala<klr> (della camicia)</klr></trd>,
    <trd>jabot</trd>
  </trdgrp>
  <trd lng="uk">&c_zh;&c_a;&c_b;&c_o;</trd>
</drv>
</art>
