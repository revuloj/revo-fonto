<kap>
<rad>tipometr</rad>/o
</kap>
<drv mrk="tipome.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref tip="super" cel="lini.0ilo">Liniilo</ref> kun skalo por mezuri la
      tipografiajn punktojn, uzata &ccirc;e la kompostado:
      <ekz>
        kompostisto <klr>[&#8230;]</klr> per la sola rimedo de simpla <tld/>o &ccirc;e la manoj
        <fnt>
          <vrk><url
          ref="http://www.esperanto.es:8080/jspui/bitstream/11013/127/4/bol029.pdf">Boletin, n-ro 5,
          Majo 1951</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_y;&c_p;&c_o;&c_m;&c_e;&c_t;&c_a;&c_r;</trd>
  <trd lng="cs">typometr</trd>
  <trd lng="de">Typometer</trd>
  <trd lng="fr">typom&egrave;tre</trd>
  <trdgrp lng="hu">
  <trd>bet&udblac;nagys&aacute;g-m&eacute;r&odblac;</trd>,
  <trd>tipom&eacute;ter</trd>
</trdgrp>
  <trd lng="ja">&#x6D3B;&#x5B57;&#x5C3A;  <pr>&#x304B;&#12388;&#12376;&#12375;&#12419;&#x304F;</pr></trd>
  <trd lng="nl">typometer</trd>
  <trdgrp lng="pl">
    <trd>typometr</trd>,
    <trd>miarka drukarska</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>tip&ocirc;metro</trd>,
    <trd>prot&oacute;tipo</trd>
  </trdgrp>
  <trd lng="sk">typometer</trd>
  <trd lng="uk">&c_t;&c_i;&#x43F;&#x43E;&#x43C;&c_je;&c_t;&c_r;</trd>
</drv>
</art>
