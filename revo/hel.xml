<drv mrk="hel.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="hel.0eco.KOMUNE">
    <dif>
      Eco de io <tld/>a:
      <ekz>
        <tld/>eco de fulmo
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la festo brilis per &gcirc;ia tuta <tld/>eco
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam la lumantan dion sekvas de malproksime la luno, ni havas la <tld/>econ tage
        kaj la <tld/>econ nokte
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_r;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>jasnost</trd>,
    <trd>sv&ecaron;tlost</trd>
  </trdgrp>
  <trd lng="de">Helligkeit</trd>
  <trd lng="en">brightness</trd>
  <trdgrp lng="es">
    <trd>claridad</trd>,
    <trd>luminosidad</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>luminosit&eacute;</trd>,
    <trd>clart&eacute;<klr tip="ind"> (luminosit&eacute;)</klr></trd>,
    <trd>lumi&egrave;re<klr tip="ind"> (luminosit&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="id"><baz>cerah</baz>kecerahan</trd>
  <trdgrp lng="it">
    <trd>luminosit&agrave;</trd>,
    <trd>chiarezza<klr> (luce)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x660E;&#x308B;&#x3055;  <pr>&#x3042;&#x304B;&#x308B;&#x3055;</pr></trd>,
    <trd>&#x660E;&#x5EA6;  <pr>&#x3081;&#x3044;&#x3069;</pr></trd>
  </trdgrp>
  <trd lng="nl">helderheid</trd>
  <trd lng="pt">claridade</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_v;&c_je;&c_t;</trd>,
    <trd>&c_s;&c_v;&c_je;&c_t;&c_l;&c_o;&c_t;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>jasnos&tcaron;</trd>,
    <trd>svetlos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ja;&c_s;&c_k;&c_r;&c_a;&c_v;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_ja;&c_s;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4EAE;&#x5EA6; <pr>li&agrave;ngd&ugrave;</pr></trd>,
    <trd>&#x5149;&#x5EA6; <pr>gu&amacron;ngd&ugrave;</pr></trd>,
    <trd>&#x8F1D;&#x714C; <pr>hu&imacron;hu&aacute;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hel.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="hel.0igi.KOMUNE">
    <dif>
      Igi ion <tld/>a:
      <ekz>
        la suno <tld/>igis la montpintojn
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XLII</lok>
        </fnt>;
      </ekz>
      <ekz>
        vintra suno <klr>[&#x2026;]</klr> <tld/>igis &scirc;ian &ccirc;ambron
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[mi drinkas]</klr> por <tld/>igi la grizecon de vivo
        <fnt>
          <bib>KrM</bib>
          <lok>22. Per lerta admono alfluas la mono.</lok>
        </fnt>!
      </ekz>
      <ekz>
        esprimo de mal&scirc;ar&gcirc;o <tld/>igis la viza&gcirc;ojn
        <fnt>
          <bib>KrM</bib>
          <lok>33. Plej bone ridas kiu laste ridas.</lok>
        </fnt>;
      </ekz>
      <ekz>
        dis<tld/>igu la fajron kaj &jcirc;etu sur &gcirc;in ankora&ubreve; unu pecon da ligno
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_s;&c_mol;&c_v;&c_ja;&c_t;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_ja;&c_s;&c_mol;&c_n;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>rozjasnit</trd>,
    <trd>zesv&ecaron;tlit</trd>,
    <trd>zjasnit</trd>
  </trdgrp>
  <trd lng="de">erhellen</trd>
  <trd lng="en">brighten</trd>
  <trd lng="es">aclarar</trd>
  <trdgrp lng="fr">
    <trd>&eacute;clairer</trd>,
    <trd>&eacute;claircir</trd>,
    <trd>illuminer</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>cerah</baz>mencerahkan</trd>,
    <trd><baz>terang</baz>menerangi</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>rischiarare</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x660E;&#x308B;&#x304F;&#x3059;&#x308B;  <pr>&#x3042;&#x304B;&#x308B;&#x304F;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x8584;&#x304F;&#x3059;&#x308B;  <pr>&#x3046;&#x3059;&#x304F;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">verlichten</trd>
  <trd lng="pt">clarear</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_s;&c_v;&c_je;&c_t;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_ja;&c_s;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>osvetli&tcaron;</trd>,
    <trd>zjasni&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_o;&c_s;&c_v;&c_ib;&c_t;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_s;&c_v;&c_ib;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_b;&c_i;&c_t;&c_i; &c_ja;&c_s;&c_k;&c_r;&c_a;&c_v;&c_i;&c_m;</trd>,
    <trd>&c_s;&c_v;&c_ib;&c_t;&c_l;&c_i;&c_m;</trd>,
    <trd>&c_ja;&c_s;&c_n;&c_i;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7167;&#x5C04; <pr>zh&agrave;osh&egrave;</pr></trd>,
    <trd>&#x7167;&#x4EAE; <pr>zh&agrave;oli&agrave;ng</pr></trd>,
    <trd>&#x8FEA; <pr>d&iacute;</pr></trd>,
    <trd>&#x70B9;&#x4EAE; <pr>di&#x1CE;nli&agrave;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hel.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="hel.0igxi.KOMUNE">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      I&gcirc;i <tld/>a:
      <ekz>
        la nokta &ccirc;ielo <tld/>i&gcirc;is pro la <klr>[&#x2026;]</klr> brulanta domaro
        <fnt>
          <bib>StU</bib>
          <lok>Britujo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;iaj trajtoj <tld/>i&gcirc;is, kaj ridante &scirc;i kaptis liajn brakojn
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_a;&c_s;&c_mol;&c_v;&c_ja;&c_t;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_ja;&c_s;&c_mol;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>
    </trdgrp>
    <trd lng="es">aclararse</trd>
    <trdgrp lng="fr">
      <trd>s'<ind>&eacute;clairer</ind></trd>,
      <trd>s'<ind>&eacute;claircir</ind></trd>,
      <trd>s'<ind>illuminer</ind></trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>rischiararsi</trd>
    </trdgrp>
    <trd lng="nl">opklaren</trd>
    <trd lng="pt">clarear-se</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_v;&c_je;&c_t;&c_l;&c_je;&c_t;&c_mol;</trd>,
      <trd>&c_ja;&c_s;&c_n;&c_je;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_ja;&c_s;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc mrk="hel.0igxi.MET">
    <gra>sensubjekta</gra>
    <uzo tip="fak">MET</uzo>
    <dif>
      I&gcirc;i <tld/>e:
      <ekz>
        <tld/>i&gcirc;is iom post iom, la nokto malaperas
        <fnt>
          <aut>H. Laxness, trad. B. Ragnarsson</aut>
          <vrk>Sendependaj homoj</vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_d;&c_n;&c_je;&c_c;&c_mol;</trd>
  </snc>
  <trdgrp lng="de">
    <trd><ind>heller</ind> werden</trd>,
    <trd>sich <ind>lichten</ind></trd>
  </trdgrp>
  <trd lng="en">brighten</trd>
  <trdgrp lng="id">
    <trd><baz>cerah</baz>mencerah</trd>,
    <trd><baz>terang</baz>menerang</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x660E;&#x308B;&#x304F;&#x306A;&#x308B;
    <pr>&#x3042;&#x304B;&#x308B;&#x304F;&#x306A;&#x308B;</pr></trd>,
    <trd>&#x8584;&#x304F;&#x306A;&#x308B;  <pr>&#x3046;&#x3059;&#x304F;&#x306A;&#x308B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hel.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="hel.mal0a.senluma">
    <dif>
      En kiu mankas lumo:
      <ekz>
        mal<tld/>a kelo
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2015/011613.php"
              >Terpomoj gravedaj</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>a kristalo
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>a arbareto de akacioj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        li klopodas trabori per la miopaj okuloj la mal<tld/>an parton de la koridoro
        <fnt>
          <bib>IdO</bib>
          <lok>Kvina Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">dunkel</trd>
    <trdgrp lng="en">
      <trd>dim</trd>,
      <trd>dark</trd>
    </trdgrp>
    <trdgrp lng="es"><trd>oscuro</trd></trdgrp>
    <trd lng="id">gelap</trd>
    <trdgrp lng="it"><trd>oscuro</trd>, <trd>cupo</trd></trdgrp>
    <trd lng="vo">dagik</trd>
  </snc>
  <snc mrk="hel.mal0a.nigreca">
    <dif>
      Kolore proksima al nigro:
      <ekz>
        mal<tld/>a purpuro
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>a mantelo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>aj okuloj
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>a viza&gcirc;koloro
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        du vicoj de mal<tld/>aj truoj en la ne&gcirc;o
        <fnt>
          <bib>IdO</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">dark</trd>
    <trd lng="id">gelap</trd>
  </snc>
  <snc mrk="hel.mal0a.SON">
    <dif>
      Sone obtuza, malalta kaj malfacile distingebla:
      <ekz>
        mal<tld/>a tono
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">muted</trd>
  </snc>
  <snc mrk="hel.mal0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malgaja, preme mal&gcirc;ojiga, eventuale e&ccirc; minaca:
      <ekz>
        la efektiva&jcirc;o ja &ccirc;iam estas mal<tld/>a kaj malbela la&ubreve; la
        pesimistoj
        <fnt><aut>Valdemar Langlet</aut>: <vrk>Voja&gcirc;impresoj</vrk>.
          <lok>Lingvo Internacia, septembro 1896.</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiuhore &gcirc;i <klr>(Moskvo)</klr> estas antikva maljunulino,
        silente atestanta la mal<tld/>an historion de la rusa popolo
        <fnt><bib>Metrop</bib></fnt>;
      </ekz><ekz>
        kampulo, kiu atendis humile, sed kun mal<tld/>a mieno
        <fnt><aut>Ferenc Szil&aacute;gyi</aut>:
          <vrk>La sekreto de la banko</vrk>.
          <lok>En: Koko krias jam!, La Laguna 1955</lok></fnt>;
      </ekz>
      <ekz>
        la perspektivoj estas des pli mal<tld/>aj, ke Britujo
        <klr>[&#x2026;]</klr> forlasas la Union
        <fnt><lok>Le Monde diplomatique en Esperanto 2017-2019</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="en">gloomy</trd>
    <trd lng="id">suram</trd>
    <trd lng="ru">&c_m;&c_r;&c_a;&c_ch;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_jo;&c_m;&c_n;&c_y;</trd>,
    <trd>&c_c;&c_mol;&c_m;&c_ja;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_h;&c_m;&c_u;&c_r;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kaln&yacute;</trd>,
    <trd>tmav&yacute;</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>sombr&iacute;o</trd>,
    <trd>oscuro</trd>,
    <trd>siniestro</trd>,
    <trd>funesto</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>sombre</trd>,
    <trd>fonc&eacute;</trd>,
    <trd>obscur</trd>,
    <trd>sinistre</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8584;&#x6697;&#x3044;  <pr>&#x3046;&#x3059;&#x3050;&#x3089;&#x3044;</pr></trd>,
    <trd>&#x66C7;&#x3063;&#x305F;  <pr>&#x304F;&#x3082;&#x3063;&#x305F;</pr></trd>,
    <trd>&#x6697;&#x3044;  <pr>&#x304F;&#x3089;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_jo;&c_m;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">tmav&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_je;&c_m;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_t;&c_mol;&c_m;&c_ja;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_t;&c_mol;&c_m;&c_a;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_h;&c_m;&c_u;&c_r;&c_i;&c_j;</trd>,
    <trd>&c_m;&c_u;&c_t;&c_n;&c_i;&c_j;</trd>,
    <trd>(&c_p;&c_r;&c_o; &c_g;&c_o;&c_l;&c_o;&c_s;) &c_n;&c_i;&c_z;&c_mol;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x51A5; <pr>m&iacute;ng</pr></trd>,
    <trd>&#x660F;&#x6697; <pr>h&umacron;n&agrave;n</pr></trd>,
    <trd>&#x4E4C; <pr>w&umacron;</pr></trd>,
    <trd>&#x609C; <pr>ch&ecaron;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hel.mal0igi">
