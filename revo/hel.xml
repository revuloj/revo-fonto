    <trd>&#34180;&#x304F;&#12377;&#x308B;  <pr>&#12358;&#12377;&#x304F;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">verlichten</trd>
  <trd lng="pt">clarear</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_s;&c_v;&c_je;&c_t;&c_l;&c_ja;&c_t;&c_mol;</trd>,
