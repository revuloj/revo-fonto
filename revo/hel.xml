<drv mrk="hel.mal0igxi">
  <kap>mal<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="hel.mal0igxi.KOMUNE">
    <dif>
      I&gcirc;i mal<tld/>a:
      <ekz>
        la vetero mal<tld/>i&gcirc;is
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;ia bruna rigardo mal<tld/>i&gcirc;as
        <fnt>
          <bib>LGA</bib>
          <lok>II.</lok>
        </fnt>;
      </ekz>
      <ekz>
        koloro, kiu dum la da&ubreve;ro de la vivo pli kaj mal<tld/>i&gcirc;as
        <fnt>
          <bib>VaK</bib>
          <lok>Dekdua &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        venas nigraj nuboj, mal<tld/>i&gcirc;as mondo
        <fnt>
          <aut>S. Pet&odblac;fi, trad. K. Kalocsay</aut>,
          <vrk><url ref="http://www.egalite.hu/kalocsay/petofi/johano%20la%20brava.htm">
          Johano la brava</url></vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_ja;&c_m;&c_n;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_c;&c_ja;&c_m;&c_n;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_c;&c_mol;&c_m;&c_ja;&c_n;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_z;&c_m;&c_r;&c_o;&c_ch;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sich <ind>verdunkeln</ind></trd>,
    <trd><ind>dunkler</ind> werden</trd>
  </trdgrp>
  <trd lng="en">darken</trd>
  <trdgrp lng="es">
    <trd><ind>ensombrecer</ind>se</trd>,
    <trd><ind>oscurecer</ind>se</trd>
  </trdgrp>
  <trd lng="fr">s'<ind>assombrir</ind></trd>
  <trd lng="id"><baz>gelap</baz>menggelap</trd>
  <trdgrp lng="ja">
    <trd>&#x8584;&#x6697;&#x304F;&#x306A;&#x308B;  <pr>&#x3046;&#x3059;&#x3050;&#x3089;&#x304F;&#x306A;&#x308B;</pr></trd>,
    <trd>&#x304B;&#x3052;&#x308B;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_t;&c_je;&c_m;&c_n;&c_je;&c_t;&c_mol;</trd>
</drv>
