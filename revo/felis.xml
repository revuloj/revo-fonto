<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>felis</rad>/o
</kap>
<drv mrk="felis.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="felis.0edoj">felisedoj</ref>
      (<trd lng="la">Felis</trd>), al kiu apartenas ekzemple
      <refgrp tip="sub">
        <ref cel="jaguar.0o">jaguaro</ref>,
        <ref cel="panter.0o">pantero</ref>,
        <ref cel="leopar.0o">leopardo</ref>,
        <ref cel="tigr.0o">tigro</ref>
      </refgrp>.
    </dif>
  </snc>
  <trd lng="de">Katze</trd>
</drv>
<drv mrk="felis.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="best.rabo0oj">rabobestoj</ref> 
      (<trd lng="la">Felidae</trd>).
     </dif>
  </snc>
  <trd lng="de">Katzen</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>