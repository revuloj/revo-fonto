<art mrk="$Id: felis.xml,v 1.18 2023/10/14 18:06:40 revo Exp $">
<kap>
  <rad>felis</rad>/o
</kap>
<drv mrk="felis.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref>
      el familio <ref tip="malprt" cel="felise.0oj">felisedoj</ref>
      (<trd lng="la">Felis</trd>), al kiu apartenas ekzemple la
      <ref tip="sub" cel="kat.0o">kato</ref>:
      <ekz>
        la bestaro rilatantaj al tiu &ccirc;i ekosistemo estas la birdoj, la amfibioj kaj la
        grandaj <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2233">Kambo&gcirc;o</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_t; <klr tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>
  <trd lng="cs">ko&ccaron;ka</trd>
  <trd lng="de">Katze <klr>(Gattung)</klr></trd>
  <trd lng="es">felino</trd>
  <trd lng="fr">chat<klr tip="ind"> (genre)</klr></trd>
  <trd lng="hu">macska<klr tip="ind"> (nem)</klr></trd>
  <trd lng="ja">&#x30CD;&#x30B3;&#x5C5E;  <pr>&#x30CD;&#x30B3;&#x305E;&#x304F;</pr></trd>
  <trd lng="pt">f&eacute;lis</trd>
  <trd lng="ru">&c_k;&c_o;&c_sh;&c_k;&c_a; <klr tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>
  <trd lng="sk">ma&ccaron;ka</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x456;&#x448;&#x43A;&#x430;</trd>,
    <trd>&#x442;&#x432;&#x430;&#x440;&#x438;&#x43D;&#x430; &#x437; &#x440;&#x43E;&#x434;&#x443; &#x43A;&#x43E;&#x442;&#x44F;&#x447;&#x438;&#x445;</trd>
  </trdgrp>
</drv>
</art>
