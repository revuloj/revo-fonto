<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>felis</rad>/o
</kap>
<drv mrk="felis.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="felise.0oj">felisedoj</ref>
      (<trd lng="la">Felis</trd>), al kiu apartenas ekzemple la
      <ref tip="sub" cel="kat.0o">kato</ref>.
    </dif>
  </snc>
  <trd lng="de">Katze</trd>
  <trd lng="fr">chat<klr tip="ind"> (genre)</klr></trd>
  <trd lng="ru">&c_k;&c_o;&c_sh;&c_k;&c_a; <klr tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>
</drv>
</art>
