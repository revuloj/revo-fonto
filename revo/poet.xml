<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>poet</rad>/o
</kap>
<drv mrk="poet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      Verkisto de <ref tip="vid" cel="poezi.0o.arto">poezio</ref>
      <ekz>
        a&ccirc;', mi revis esti <tld/>o, kaj nun vidu ...
        reklamisto<fnt><aut>Alberto Garc&iacute;a Fumero</aut>:
        <vrk>Kura&gcirc;o</vrk>, <bib>Monato</bib><lok>jaro 2002a, numero 2a, p.
        23a</lok></fnt>!
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_e;&c_t;</trd>,
    <trd>&c_p;&c_a;&c_e;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">poeta</trd>
  <trdgrp lng="de">
    <trd>Poet</trd>,
    <trd>Dichter</trd>
  </trdgrp>
  <trd lng="en">poet</trd>
  <trd lng="es">poeta</trd>
  <trd lng="fr">po&egrave;te</trd>
  <trdgrp lng="hu">
    <trd>k&ouml;lt&odblac;</trd>,
    <trd>po&eacute;ta</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>dichter</trd>,
    <trd>po&euml;et</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_e;&c_t;</trd>
  <trd lng="pt">poeta</trd>
</drv>
</art>
