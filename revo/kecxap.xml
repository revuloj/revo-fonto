<vortaro>
<art mrk="$Id: kecxap.xml,v 1.14 2023/12/03 09:58:48 revo Exp $">
<kap>
  <rad>ke&ccirc;up</rad>/o <fnt><bib>PIV3</bib></fnt>,
  <var><kap><rad var="a">ke&ccirc;ap</rad>/o
  <fnt><bib>HejmVort</bib></fnt></kap></var>
</kap>
<drv mrk="kecxap.0o">
  <kap><tld/>o, <var><kap><tld var="a"/>o</kap></var></kap>
  <snc mrk="kecxap.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Densa <ref tip="super" cel="sauxc.0o.KUI">sa&ubreve;co</ref> el <ref tip="prt" cel="tomat.0o.KUI">tomatoj</ref> kaj spicoj:
      <ekz>
        la densa tomato-sa&ubreve;co multe similas al la fama <tld/>o
        <fnt>
          <vrk><url ref="&Monato;/1989/monato198908.pdf#page=23">Tri someraj
          sa&ubreve;coj</url></vrk>
          <bib>Monato</bib>
          <lok>1989/08, p. 24</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fingropremo de Biggs estis trovita sur <tld var="a"/>a botelo
        <fnt>
          <bib>Viki</bib>,
          <lok>artikolo &leftquot;Ronnie Biggs&rightquot;, 2005-08-29</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_je;&c_t;&c_ch;&c_u;&c_p;</trd>
  <trd lng="ca">qu&egrave;txup</trd>
  <trd lng="cs">ke&ccaron;up</trd>
  <trd lng="de">Ketchup</trd>
  <trd lng="en">ketchup</trd>
  <trd lng="es">k&eacute;tchup</trd>
  <trd lng="fa">&f_k;&f_ch;&a_A;&f_p;</trd>
  <trd lng="fr">ketchup</trd>
  <trd lng="hu">ketchup</trd>
  <trd lng="it">ketchup</trd>
  <trd lng="ja">&#x30B1;&#x30C1;&#x30E3;&#x30C3;&#x30D7;  <pr>&#x3051;&#x3061;&#x3083;&#x3063;&#x3077;</pr></trd>
  <trd lng="nl">ketchup</trd>
  <trd lng="pl">keczup</trd>
  <trd lng="pt">ketchup</trd>
  <trd lng="ro">ketchup</trd>
  <trd lng="ru">&c_k;&c_je;&c_t;&c_ch;&c_u;&c_p;</trd>
  <trd lng="sk">ke&ccaron;up</trd>
  <trd lng="uk">&c_k;&c_je;&c_t;&c_ch;&c_u;&c_p;</trd>
  <trd lng="zh">&#x756A;&#x8304;&#x9171; <pr>f&#x101;nqi&#xE9;ji&#xE0;ng</pr></trd>
</drv>
