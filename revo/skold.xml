<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>skold</rad>/i
</kap>
<drv mrk="skold.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">POE</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="riproc.0i">ripro&ccirc;i</ref>
      <ekz>
        pli a&gcirc;aj knabinoj povas puni a&ubreve; <tld/>i &scirc;in
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 58a, p. 285a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    Gramatike oni ripro&ccirc;as iun (pri a&ubreve; pro io),
    a&ubreve; ripro&ccirc;as ion (al iu).
    Oni skoldas iun (pri a&ubreve; pro io).
  </rim>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_ja;&c_c;&c_mol;</trd>, 
    <trd>&c_r;&c_a;&c_s;&c_mol;&c_p;&c_ja;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>vorwerfen</trd>,
    <trd><klr>einen </klr>Vorwurf machen</trd>,
    <trd>tadeln</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bl&acirc;mer</trd>,
    <trd>disputer</trd>,
    <trd>gronder</trd>,
    <trd>r&eacute;primander</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megszid</trd>,
    <trd>megfedd</trd>
  </trdgrp>
</drv>
</art>

