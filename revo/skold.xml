<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>skold</rad>/i
</kap>
<drv mrk="skold.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">POE</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="riproc.0i">ripro&ccirc;i</ref>
      <ekz>
        pli a&gcirc;aj knabinoj povas puni a&ubreve; <tld/>i &scirc;in
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 58a, p. 285a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    Gramatike oni ripro&ccirc;as iun (pri a&ubreve; pro io),
    a&ubreve; ripro&ccirc;as ion (al iu).
    Oni skoldas iun (pri a&ubreve; pro io).
  </rim>
  <trdgrp lng="fr">
    <trd>bl&acirc;mer</trd>,
    <trd>disputer</trd>,
    <trd>gronder</trd>,
    <trd>r&eacute;primander</trd>
  </trdgrp>
</drv>
</art>

