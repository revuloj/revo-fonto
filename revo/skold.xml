<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>skold</rad>/i
</kap>
<drv mrk="skold.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malmilde a&ubreve; kolere 
      <ref tip="super" cel="riproc.0i">ripro&ccirc;i</ref>
      a&ubreve; <ref tip="sin" cel="lauxd.mal0i">malla&ubreve;di</ref>:
      <ekz>
        pli a&gcirc;aj knabinoj povas puni a&ubreve; <tld/>i &scirc;in
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 58a, p. 285a</lok></fnt>;
      </ekz>
      <ekz>
        kria&ccirc;a vo&ccirc;o de l' princino sonis en la mansardo: &scirc;i skoldis sian
        &ccirc;ambristinon
        <fnt>
        <aut>I. Turgenev</aut>: <vrk>La unua amo</vrk>.
        <lok>Jekaterinburg: Sezonoj, 1993.</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="admon.0i.riprocxi">admoni <sncref/>.</ref>
  </snc>
  <rim>
    Gramatike oni ripro&ccirc;as iun (pri a&ubreve; pro io), a&ubreve; ripro&ccirc;as ion (al iu).
    Oni <tld/>as iun (pri a&ubreve; pro io).
  </rim>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_mol;&c_p;&c_ja;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>vorwerfen</trd>,
    <trd><klr>einen </klr>Vorwurf machen</trd>,
    <trd>tadeln</trd>
  </trdgrp>
  <trd lng="en">scold</trd>
  <trdgrp lng="fr">
    <trd>bl&acirc;mer<klr tip="ind"> (r&eacute;primander)</klr></trd>,
    <trd>disputer<klr tip="ind"> (r&eacute;primander)</klr></trd>,
    <trd>gronder<klr tip="ind"> (r&eacute;primander)</klr></trd>,
    <trd>r&eacute;primander</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megszid</trd>,
    <trd>megfedd</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_a;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_u;&c_g;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_je;&c_k;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
