<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>garb</rad>/o
</kap>
<drv mrk="garb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
  <uzo tip="fak">AGR</uzo>
    <dif>
      Fasko da fal&ccirc;ita greno. 
    </dif>
    <trd lng="de">Garbe</trd>
    <trd lng="de">k&eacute;ve</trd>
    <trd lng="sv">k&auml;rve</trd>
  </snc>
  <snc num="2">
    <dif>
      Simila dika kunliga&jcirc;o da detran&ccirc;itaj vegeta&jcirc;aj
trunketoj:
      <ekz><tld/>o da pajlo, fojno, floroj.</ekz>
    </dif>
    <trd lng="de">Strau&szlig;</trd>
    <trd lng="hu">csom&oacute;</trd>
    <trd lng="sv">k&auml;rve</trd>
  </snc>
  <snc num="3">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Aro da longaj a&jcirc;oj:
      <ekz>
        tutaj <tld/>oj da rapidaj, oraj, blindigantaj radioj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiuj pensoj, kvaza&ubreve; <tld/>o da fulmoj, trakuris la kapon de
        Marta<fnt>Z</fnt>.
      </ekz> 
    </dif>
    <trd lng="de">B&uuml;ndel</trd>
    <trd lng="hu">k&ouml;teg</trd>
    <trd lng="sv">knippe</trd>
  </snc>
  <refgrp tip="vid">
    <ref cel="fask.0o">fasko</ref>.
  </refgrp>
  <trdgrp lng="fr">
    <trd>gerbe</trd>,
    <trd>botte<klr tip="ind"> (gerbe)</klr></trd></trdgrp> 
  <trd lng="ru">&c_s;&c_n;&c_o;&c_p;</trd>
</drv>
<drv mrk="garb.0ejo">
  <kap><tld/>ejo</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Loko por konservi <tld/>ojn.
    </dif>
    <refgrp tip="vid">
      <ref cel="gren.0ejo">grenejo</ref>.
    </refgrp>
  </snc>
  <trd lng="fr">grange</trd>
  <trd lng="hu">cs&udblac;r</trd>
  <trd lng="ru">&c_r;&c_i;&c_g;&c_a;</trd>
</drv>
<drv mrk="garb.0igi">
  <kap><tld/>igi<fnt>Z</fnt></kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Aran&gcirc;i kaj ligi <tld/>e.  
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>gerber<klr tip="ind"> (mettre en gerbes)</klr></trd>,
    <trd>mettre en <ind>gerbes</ind></trd></trdgrp> 
  <trdgrp lng="hu">
    <trd>k&eacute;v&eacute;t k&ouml;t</trd>,
    <trd>k&eacute;v&eacute;be k&ouml;t</trd></trdgrp>
  <trd lng="ru">&c_v;&c_ja;&c_z;&c_a;&c_t;&c_mol;
&c_s;&c_n;&c_o;&c_p;&c_y;</trd>
</drv>
</art>
