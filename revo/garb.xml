<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: garb.xml,v 1.32 2025/04/13 18:35:24 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>garb</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="garb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="garb.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="fask.0o.KOMUNE">Fasko</ref> da
      vegeta&jcirc;oj:
    </dif>
    <subsnc mrk="garb.0o.AGR">
      <uzo tip="fak">AGR</uzo>
      <dif>
        <ref tip="super" cel="lig.kun0ajxo.KOMUNE">Kunliga&jcirc;o</ref> da fal&ccirc;ita greno:
        <ekz>
         ni ligis <tld/>ojn meze de la kampo
         <fnt>
           <bib>MT</bib>
           <lok>Genezo 37:7</lok>
         </fnt>;
       </ekz>
       <ekz>
         vila&gcirc;ano tute apude de la muro starigis stangon kaj alligis al &gcirc;i nedra&scirc;itan
         avenan <tld/>on
         <fnt>
           <bib>Fab4</bib>
           <lok>Knabino, kiu pa&scirc;is sur panon</lok>
         </fnt>;
       </ekz>
      </dif>
      <trd lng="ca">garba</trd>
      <trd lng="de">Garbe</trd>
      <trd lng="hu">k&eacute;ve</trd>
      <trdgrp lng="nl">
        <trd>schoof</trd>,
        <trd>garve</trd>
      </trdgrp>
      <trd lng="sv">k&auml;rve</trd>
    </subsnc>
    <subsnc mrk="garb.0o.vegetajxa">
      <dif>
        Simila dika kunliga&jcirc;o da detran&ccirc;itaj vegeta&jcirc;aj
        trunketoj:
        <ekz>
            <tld/>o da pajlo, fojno, floroj;
          </ekz>
          <ekz>
          &scirc;i rikoltis nov-<tld/>on da flora herbaro
           <fnt>
             <bib>SrT</bib>
             <lok>Libro XI.</lok>
           </fnt>;
         </ekz>
         <ekz>
          ni portis eksteren <tld/>ojn da pajlo kaj kovris nin per feloj
          <fnt>
            <bib>StU</bib>
            <lok>Britujo</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="ca">feix</trd>
      <trdgrp lng="de">
        <trd>B&uuml;ndel</trd>,
        <trd>Strau&szlig;</trd>
      </trdgrp>
      <trd lng="hu">csom&oacute;</trd>
      <trdgrp lng="nl">
        <trd>garve</trd>,
        <trd>struik<klr tip="ind"> (bloemen)</klr></trd>
      </trdgrp>
      <trd lng="sv">k&auml;rve</trd>
    </subsnc>
    <bld
      lok="&WCU;/thumb/3/33/Gierymski_A_boy_carrying_a_sheaf.jpg/387px-Gierymski_A_boy_carrying_a_sheaf.jpg"
      prm="PD"
      >Gierymski: knabo portanta <tld/>on
        <fnt>
          <aut>Aleksander Gierymski, www.pinakoteka.zascianek.pl</aut>
          <lok><url ref="&WCW;/File:Gierymski_A_boy_carrying_a_sheaf.jpg"
              >https://commons.wikimedia.org/wiki/File:Gierymski_A_boy_carrying_a_sheaf.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="pl">
      <trd>snop</trd>,
      <trd>wi&aogonek;zanka</trd>,
      <trd>wi&aogonek;zka</trd>,
      <trd>p&eogonek;czek</trd>
    </trdgrp>
  </snc>
  <snc mrk="garb.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Aro da longaj a&jcirc;oj:
      <ekz>
        tutaj <tld/>oj da rapidaj, oraj, blindigantaj radioj
        <fnt><bib>PV</bib></fnt>;
      </ekz>
      <ekz>
        gaje &scirc;prucis la fajrera <tld/>o de la lignofajro
        <fnt>
          <bib>MkM</bib>
          <lok>21</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i tiuj pensoj kvaza&ubreve; <tld/>o da fulmoj trakuris la
        kapon de Marta
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        la tegmento disfalis meze de supermezura <tld/>o de fajreroj
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">manat</trd>
    <trd lng="de">B&uuml;ndel</trd>
    <trd lng="nl">bundel</trd>
    <trd lng="hu">k&ouml;teg</trd>
    <trdgrp lng="pl">
      <trd>snop</trd>,
      <trd>wi&aogonek;zka</trd>,
      <trd>p&eogonek;czek</trd>
    </trdgrp>
    <trd lng="sv">knippe</trd>
    <trd lng="zh">&#25226; <pr>b&#462;</pr></trd>
  </snc>
  <refgrp tip="vid">
    <ref cel="penik.0o">peniko</ref>,
    <ref cel="tuf.0o">tufo</ref>
  </refgrp>
  <trd lng="be">&c_s;&c_n;&c_o;&c_p;</trd>
  <trdgrp lng="en">
    <trd>sheaf</trd>,
    <trd>shock</trd>,
    <trd>bundle</trd>,
    <trd>cluster</trd>,
    <trd>bunch</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>botte<klr tip="ind"> (gerbe)</klr></trd>,
    <trd>gerbe</trd>
  </trdgrp>
  <trd lng="ja">&#26463;  <pr>&#12381;&#12367;</pr></trd>
  <trdgrp lng="pt">
    <trd>feixe</trd>,
    <trd>paveia</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_n;&c_o;&c_p;</trd>
  <trd lng="sk">zv&auml;zok</trd>
  <trdgrp lng="cs">
    <trd>svazek <klr>(p&rcaron;&iacute;mek, rovin)</klr></trd>,
    <trd>trs</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_n;&c_ib;&c_p;</trd>,
    <trd>&c_p;&c_u;&c_k;</trd>,
    <trd>&c_p;&c_u;&c_ch;&c_o;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="garb.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="garb.0ejo.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Loko por konservi <tld/>ojn:
      <ekz>
        havigu al li iom el viaj brutoj kaj el via <tld/>ejo
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 15:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i iris <klr>[&#8230;]</klr> en la <tld/>ejon, prenis fojnon
        <fnt><bib>ElektFab</bib>, <lok>Fingreto</lok></fnt>;
      </ekz>
      <ekz>
        kio restis en la <tld/>ejo, falis en la manojn de l' &scirc;telisto
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gren.0ejo">grenejo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_m;&c_n;&c_o;</trd>
  <trd lng="ca">pallissa</trd>
  <trd lng="cs">stodola</trd>
  <trdgrp lng="de">
    <trd>Banse</trd>,
    <trd>Scheune</trd>,
    <trd>Scheuer</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>barn</trd>,
    <trd>granary</trd>
  </trdgrp>
  <trd lng="fr">grange</trd>
  <trd lng="hu">cs&udblac;r</trd>
  <trdgrp lng="ja">
    <trd>&#31296;&#29289;&#20489;  <pr>&#12371;&#12367;&#12418;&#12388;&#12367;&#12425;</pr></trd>,
    <trd>&#32013;&#23627;  <pr>&#12394;&#12420;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schuur</trd>,
    <trd>hooischuur</trd>,
    <trd>korenschuur</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>stodo&lstroke;a</trd>,
    <trd>gumno<klr> (przest.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>celeiro</trd>,
    <trd>paiol</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_i;&c_g;&c_a;</trd>
  <trd lng="sk">stodola</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_l;&c_u;&c_n;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_d;&c_o;&c_l;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#24293; <pr>ku&agrave;i</pr></trd>
</drv>

<drv mrk="garb.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="garb.0igi.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Aran&gcirc;i kaj ligi <tld/>e:
      <ekz>
        rikoltanto <klr>[&#8230;]</klr> <tld/>iganto
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 129:7</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ja;&c_z;&c_a;&c_c;&c_mol; &c_s;&c_n;&c_a;&c_p;&c_y;</trd>,
    <trd>&c_z;&c_mol;&c_v;&c_ja;&c_z;&c_a;&c_c;&c_mol; &c_u; &c_s;&c_n;&c_o;&c_p;</trd>
  </trdgrp>
  <trd lng="ca">agarbar</trd>
  <trdgrp lng="cs">
    <trd>sv&aacute;zat do snop&uring;</trd>,
    <trd>v&aacute;zat snopy</trd>
  </trdgrp>
  <trd lng="de">zu <ind>Garben</ind> b&uuml;ndeln</trd>
  <trd lng="en">bind into sheaves</trd>
  <trdgrp lng="fr">
    <trd>gerber<klr tip="ind"> (mettre en gerbes)</klr></trd>,
    <trd>mettre en <ind>gerbes</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&eacute;v&eacute;t k&ouml;t</trd>,
    <trd>k&eacute;v&eacute;be k&ouml;t</trd>
  </trdgrp>
  <trd lng="ja">&#26463;&#12397;&#12427;  <pr>&#12383;&#12400;&#12397;&#12427;</pr></trd>
  <trdgrp lng="nl">
    <trd>garven</trd>,
    <trd>schoven</trd>,
    <trd>in <ind>schoven</ind> binden</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wi&aogonek;za&cacute; w <ind>snopki</ind></trd>,
    <trd>wi&aogonek;za&cacute; w <ind>p&eogonek;czki</ind></trd>
  </trdgrp>
  <trd lng="pt">enfeixar</trd>
  <trd lng="ru">
  &c_v;&c_ja;&c_z;&c_a;&c_t;&c_mol; &c_s;&c_n;&c_o;&c_p;&c_y;</trd>
  <trd lng="sk">zv&auml;zova&tcaron; do snopov</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&#8217;&c_ja;&c_z;&c_a;&c_t;&c_i; &c_s;&c_n;&c_o;&c_p;&c_i;</trd>,
    <trd>&c_v;&#8217;&c_ja;&c_z;&c_a;&c_t;&c_i; &c_v; &c_s;&c_n;&c_o;&c_p;&c_i;</trd>
  </trdgrp>
</drv>
</art>
