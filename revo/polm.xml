<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>polm</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="polm.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">ANA</uzo>
    <ref tip="dif" cel="plat.man0o">manplato</ref>
    <ekz>
      kuniginte la <tld/>ojn, li kriis &leftquot;...&rightquot;
      <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 67a, volumo 2a, p.
      429a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_l;&c_o;&c_n;&c_mol;</trd>
  <trd lng="ca">palma</trd>
  <trd lng="es">palma<klr> (de la mano)</klr></trd>
  <trdgrp lng="fr">
    <trd>paume</trd>,
    <trd>plat de la main</trd>
  </trdgrp>
  <trd lng="hu">teny&eacute;r</trd>
  <trd lng="nl">handpalm</trd>
  <trd lng="pl">d&lstroke;o&nacute;</trd>
  <trd lng="pt">palma <klr>(da m&atilde;o)</klr></trd>
  <trd lng="ru">&c_l;&c_a;&c_d;&c_o;&c_n;&c_mol;</trd>
</drv>
</art>
