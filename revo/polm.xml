<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: polm.xml,v 1.16 2022/06/30 12:59:49 revo Exp $">
<kap>
  <rad>polm</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="polm.0o">
  <kap><tld/>o</kap>
  <snc mrk="polm.0o.ANA">
  <uzo tip="fak">ANA</uzo>
    <dif>
      <ref tip="dif" cel="plat.man0o">Manplato</ref>:
      <ekz>
        kuniginte la <tld/>ojn, li kriis &leftquot;...&rightquot;
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 67a, volumo 2a, p.
        429a</lok></fnt>;
      </ekz>
      <ekz>
        liaj fingroj, <tld/>oj, lipoj frenezigis &scirc;in
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/007215.html">La peko de Ros</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_l;&c_o;&c_n;&c_mol;</trd>
  <trd lng="ca">palma</trd>
  <trd lng="cs">dla&ncaron;</trd>
  <trdgrp lng="de">
    <trd>Handfl&auml;che</trd>,
    <trd>Handteller</trd>
  </trdgrp>
  <trd lng="en">palm</trd>
  <trd lng="es">palma<klr> (de la mano)</klr></trd>
  <trdgrp lng="fr">
    <trd>paume</trd>,
    <trd>plat de la main</trd>
  </trdgrp>
  <trd lng="hu">teny&eacute;r</trd>
  <trd lng="id"><ind>telapak</ind> tangan</trd>
  <trdgrp lng="it">
    <trd>palma<klr> (della mano)</klr></trd>,
    <trd>palmo</trd>
  </trdgrp>
  <trd lng="nl">handpalm</trd>
  <trd lng="pl">d&lstroke;o&nacute;</trd>
  <trd lng="pt">palma <klr>(da m&atilde;o)</klr></trd>
  <trd lng="ru">&c_l;&c_a;&c_d;&c_o;&c_n;&c_mol;</trd>
  <trd lng="sk">dla&ncaron;</trd>
  <trd lng="uk">&c_d;&c_o;&c_l;&c_o;&c_n;&c_ja;</trd>
</drv>
</art>
