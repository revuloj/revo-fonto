<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>rukul</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="rukul.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <ref tip="dif" cel="kolomb.0umi">kolombumi</ref>
    <ref tip="dif" cel="kver.0i">kveri</ref>
    <ekz>
      grupo da blankaj kolomboj pare <tld/>as sur anta&ubreve;tegmento
        <fnt>
          <aut>J. Ribillard</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr36/majorkaj.html">
          Majorkaj azule&hcirc;oj</url></vrk>,
          <lok>La Nica Literatura Revuo, 1958-07 &gcirc;is 08 (3/6),
          p. 211a-217a</lok>
        </fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_r;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>cukrovat <klr>(hrdlička)</klr></trd>,
    <trd>vrkat</trd>
  </trdgrp>
  <trd lng="de">gurren</trd>
  <trd lng="fr">roucouler</trd>
  <trd lng="hu">turb&eacute;kol</trd>
  <trd lng="nl">koeren</trd>
  <trd lng="pt">arrulhar</trd>
  <trd lng="ru">&c_v;&c_o;&c_r;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">hrkútať</trd>
</drv>
</art>
