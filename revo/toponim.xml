<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>toponim</rad>/io <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="toponim.0io">
  <kap><tld/>io</kap>
  <snc mrk="toponim.0io.SCI">
    <uzo tip="fak">LIN</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Studo pri loknomoj.
      <ekz>
        E&ubreve;ska <tld/>io
        <fnt><vrk><url
        ref="http://eo.wikipedia.org/wiki/E%C5%ADska_toponimio">E&ubreve;ska
        toponimio</url></vrk></fnt>
      </ekz>
    </dif>
  </snc>
<trd lng="en">toponymy</trd>  
<trd lng="es">Toponimia</trd>
<trd lng="fr">toponymie</trd>
<trd lng="pl">Toponimia</trd>
</drv>

<drv mrk="toponim.0o">
  <kap><tld/>o</kap>
    <snc mrk="toponim.0o.LOKNOMO">
    <uzo tip="fak">LIN</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="super" cel="nom.lok0o">Loknomo</ref> studata en
      toponimio
        <ekz>
        La konjako estas ekzemplo por tio, kiel <tld/>oj donas nomon al
        produktoj:
        <fnt><vrk><url
        ref="http://www.esperanto.org/Ondo/L-162rh.htm">Aliaj landoj,
        aliaj brandoj
        </url></vrk></fnt>.
      </ekz>
     </dif>
  </snc>
  <trd lng="fr">toponyme</trd>
  </drv>
</art>

<!--
$Log$
-->
</vortaro>