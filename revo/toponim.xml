<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: toponim.xml,v 1.5 2019/10/17 20:10:32 revo Exp $">


<kap>
  <rad>toponim</rad>/o
</kap>
<drv mrk="toponim.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="toponim.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="dif" cel="nom.lok0o">Loknomo</ref>:
      <ekz>
        la konjako estas ekzemplo por tio,
        kiel <tld/>oj donas nomon al produktoj
        <fnt>
          <aut>R. Haupenthal</aut>,
          <vrk><url ref="http://esperanto-ondo.ru/Ondo/162-lode.htm#162-08">
          Aliaj landoj, aliaj brandoj</url></vrk>,
          <lok>La Ondo de Esperanto, 2008:4 (162)</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o Dnipro estu por ni kara kaj valora same kiel la rivero mem
        <fnt>
          <aut>Ihor Drul</aut>
          <vrk><url ref="https://revuoesperanto.org/toponimoj"
              >Al traduko de la toponimoj (Kelkaj amatoraj pensoj pri la problemo)</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">top&ograve;nim</trd>
  <trd lng="de">Toponym</trd>
  <trd lng="en">toponym</trd>
  <trd lng="es">top&oacute;nimo</trd>
  <trd lng="fr">toponyme</trd>
</drv>
</art>

