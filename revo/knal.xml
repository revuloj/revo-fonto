<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>knal</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="knal.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="knal.0i.SON">
    <dif>
      <ref tip="super" cel="bru.0i.soni">Brui</ref> kiel eksplodo a&ubreve;
      subita, sonora bato:
      <ekz>
        flamo fulmis, eksplodo <tld/>is, kaj la besto respondis per
        mu&gcirc;o
        de doloro kaj kolero
        <fnt>
        <aut>Edgar Rice Burroughs, trad. Donald J. Harlow</aut>,
        <vrk>Tarzan de la simioj</vrk>,
        <lok><url
        ref="http://donh.best.vwh.net/Esperanto/Literaturo/Tarzan/tarzanu14.html">&Ccirc;apitro
        14, La &gcirc;angalo kompatu!</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">knallen</trd>
  <trdgrp lng="fr">
    <trd>craquer</trd>,
    <trd>d&eacute;toner<klr> (explosion)</klr></trd>,
    <trd>p&eacute;ter<klr> (explosion)</klr></trd>,
  </trdgrp>
   <trdgrp lng="nl">
    <trd>knallen</trd>,
    <trd>ploffen</trd>,
    <trd>met een <ind>knal</ind> exploderen</trd>
  </trdgrp>
  <trd lng="pt">explodir <klr>(estrepitosamente)</klr></trd>
</drv>

<drv mrk="knal.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="knal.0o.SON">
    <dif>
      Sono de eksplodo, de ekbato:
      <ekz>
        mi a&ubreve;dis teruran <tld/>on kaj krakadon
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>tria libro, &ccirc;apitro
        46a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Knall</trd>
  <trdgrp lng="fr">
    <trd>claquement<klr tip="amb"> (d&eacute;tonation)</klr></trd>,
    <trd>d&eacute;tonation</trd>
  </trdgrp>
  <trd lng="nl">knal</trd>
</drv>

<drv mrk="knal.pra0o">
  <kap>pra<tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="knal.pra0o.AST">
    <uzo tip="fak">AST</uzo>
    <ref tip="dif" cel="eksplo.pra0o.AST">praeksplodo</ref>
    <ekz>
      &ccirc;iuj steloj kaj la tuta materio samtempe ekestis en la pra<tld/>o
      
      <fnt>
        <aut>K. P. Beisse, trad. Donjo, Frank kaj Cezar</aut>,
        <vrk><url
        ref="http://karapacopanoramo.blogspot.com/2008/10/fuo-de-la-galaksioj.html">
        Fu&gcirc;o de la galaksioj</url></vrk>,
        <lok>Karapaco-panoramo, 2008-10-29</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_V;&c_ja;&c_l;&c_ib;&c_k;&c_ib; <ind>&c_V;&c_y;&c_b;&c_u;&c_h;</ind></trd>
  <trd lng="de">Urknall</trd>
  <trd lng="en">Big Bang</trd>
  <trd lng="fi">Alkur&auml;j&auml;hdys</trd>
  <trd lng="fr">Big bang</trd>
  <trd lng="hu">&odblac;srobban&aacute;s</trd>
  <trd lng="nl">oerknal</trd>
  <trd lng="pl">Wielki Wybuch</trd>
  <trd lng="pt">Grande Explos&atilde;o</trd>
  <trd lng="ru">&c_B;&c_o;&c_l;&c_mol;&c_sh;&c_o;&c_j; &c_V;&c_z;&c_r;&c_y;&c_v;</trd>
  <trd lng="tr">B&uuml;y&uuml;k Patlama</trd>
</drv></art>

