<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kokain</rad>/o
</kap>
<drv mrk="kokain.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="alkalo.0o">Alkaloido</ref>
      ekstraktata el la folioj de 
      <ref cel="koka.0o">kokao</ref>: 
      <ekz>la <tld/>o estas tre &scirc;atata anestezilo. </ekz>
    </dif>
  </snc>
  <trd lng="de">Kokain</trd>
  <trd lng="hu">kokain</trd>
  <trd lng="nl">coca&iuml;ne</trd>
  <trd lng="ru">&c_k;&c_o;&c_k;&c_a;&c_i;&c_n;</trd>
</drv>
<drv mrk="kokain.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      Malsana stato ka&ubreve;zata de trouzo de la <tld/>o.  
    </dif>
  </snc>
  <trd lng="hu">kokainizmus</trd>
  <trd lng="nl">coca&iuml;nisme</trd>
  <trd lng="ru">&c_k;&c_o;&c_k;&c_a;&c_i;&c_n;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
