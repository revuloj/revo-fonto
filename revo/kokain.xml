<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kokain.xml,v 1.22 2023/12/03 09:58:52 revo Exp $">
<kap>
  <rad>kokain</rad>/o
</kap>
<drv mrk="kokain.0o">
  <kap><tld/>o</kap>
  <snc mrk="kokain.0o.MED">
    <uzo tip="fak">KEM</uzo>
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="alkalo.0o">Alkaloido</ref>,
      ekstraktata el la folioj de <ref cel="koka.0o">kokao</ref>:
      <ekz>
        la <tld/>o estas tre &scirc;atata anestezilo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_k;&c_a;&c_ib;&c_n;</trd>
  <trd lng="ca">coca&iuml;na</trd>
  <trd lng="cs">kokain</trd>
  <trd lng="de">Kokain</trd>
  <trdgrp lng="fr">
    <trd>coca&iuml;ne</trd>,
    <trd>coco<klr tip="ind"> (coca&iuml;ne)</klr></trd>
  </trdgrp>
  <trd lng="hu">kokain</trd>
  <trd lng="id">kokaina</trd>
  <trd lng="ja">&#x30B3;&#x30AB;&#x30A4;&#x30F3;  <pr>&#x3053;&#x304B;&#x3044;&#x3093;</pr></trd>
  <trd lng="nl">coca&iuml;ne</trd>
  <trd lng="pl">kokaina</trd>     
  <trd lng="ro">cocain&abreve;</trd>
  <trd lng="ru">&c_k;&c_o;&c_k;&c_a;&c_i;&c_n;</trd>
  <trd lng="sk">koka&iacute;n</trd>
  <trd lng="uk">&#x43A;&#x43E;&#x43A;&#x430;&#x457;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x53EF;&#x5361;&#x56E0; <pr>k&#x11B;k&#x1CE;y&#x12B;n</pr></trd>,
    <trd>&#x53E4;&#x67EF;&#x78B1; <pr>g&#x1D4;k&#x113;ji&#x1CE;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kokain.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="kokain.0ismo.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">Malsana</ref> stato, ka&ubreve;zita de veneni&gcirc;o
      per <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_k;&c_a;&c_ib;&c_n;&c_ib;&c_z;&c_m;</trd>
  <trd lng="ca">coca&iuml;nisme</trd>
  <trdgrp lng="cs">
    <trd>kokainizmus</trd>,
    <trd>z&aacute;vislost na kokainu</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>coca&iuml;nisme</trd>,
    <trd>coca&iuml;sme</trd>
  </trdgrp>
  <trd lng="hu">kokainizmus</trd>
  <trd lng="id">kokainisme</trd>
  <trd lng="ja">&#x30B3;&#x30AB;&#x30A4;&#x30F3;&#x4E2D;&#x6BD2;  <pr>&#x3053;&#x304B;&#x3044;&#x3093;&#x3061;&#x3085;&#x3046;&#x3069;&#x304F;</pr></trd>
  <trd lng="nl">coca&iuml;nisme</trd>
  <trd lng="pl">kokainizm</trd>
  <trd lng="ru">&c_k;&c_o;&c_k;&c_a;&c_i;&c_n;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">z&aacute;vislos&tcaron; na koka&iacute;ne</trd>
  <trd lng="uk">&#x43A;&#x43E;&#x43A;&#x430;&#x457;&#x43D;&#x456;&#x437;&#x43C;</trd>
</drv>
</art>
