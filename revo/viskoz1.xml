<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: viskoz1.xml,v 1.9 2023/10/21 16:50:36 afido Exp $">
    <kap>
      <rad>viskoz</rad>/a
    </kap>

<drv mrk="viskoz1.0a">
  <kap><tld/>a</kap>
  <snc mrk="viskoz1.0a.neflua">
    <dif>
      Malfacile fluanta kaj adhera kiel <ref cel="sirop.0o">siropo</ref>:
      <ekz>
        ili pentris siajn nazojn blanka per kalko, siajn sidvangojn bluigis per <tld/>a
        kaj fetora farbo
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
        </fnt>;
      </ekz>
      <ekz>
        li permane kolektis centojn kaj centojn <klr>[da helikoj]</klr> en sitelojn,
        kiujn li poste plenigis per varmega akvo <klr>[&#8230;]</klr> en la siteloj restis
        nur <tld/>a maso
        <fnt><aut>Jouko Lindstedt</aut>,
        <vrk><url ref="&Monato;/2000/006918.php">La helikoj</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/07, p. 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tableton <klr>[&scirc;i]</klr> estis kovrinta per <klr>[&#8230;]</klr> <ind><tld/>igita</ind>j sukoj
        el beroj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La tri Magiaj Pordegoj</lok>
        </fnt>;
        <trd lng="de">eingedickt</trd>
      </ekz>
      <ekz>
        &gcirc;i enhavis klaran, iomete flavan likva&jcirc;on, kiu fluis <tld/>e, kiel mielo
        <fnt>
          <bib>Monato</bib>
          <aut>Steven K. Smith</aut>
          <vrk><url ref="&Monato;/2009/009659.php">La Helpanto</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>eco de la nafto helpis kvietigi la marsurfacon
        <fnt>
          <bib>Monato</bib>
          <aut>Christian Bertin</aut>
          <vrk><url ref="&Monato;/2003/008085.html">&Ccirc;u revo iam fari&gcirc;os realo?</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>z&auml;h <klr>(flie&szlig;end)</klr></trd>,
      <trd>dickfl&uuml;ssig</trd>,
      <trd>visk&ouml;s</trd>
    </trdgrp>
    <trd lng="en">viscous</trd>
    <trd lng="fr">visqueux</trd>
  </snc>
  <ref tip="hom" cel="viskoz.0o">viskozo (celuloza fibro)</ref>
  <trdgrp lng="be">
    <trd>&c_v;&c_ja;&c_z;&c_k;&c_ib;</trd>,
    <trd>&c_g;&c_r;&c_u;&c_z;&c_k;&c_ib;</trd>,
    <trd>&c_g;&c_l;&c_je;&c_ib;&c_s;&c_t;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7C98;&#x308A;&#x6C17;&#x306E;&#12354;&#x308B; 
    <pr>&#x306D;&#12400;&#x308A;&#12369;&#x306E;&#12354;&#x308B;</pr></trd>,
    <trd>&#x7C98;&#24615;&#x306E; <pr>&#12397;&#12435;&#12379;&#12356;&#12398;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43B;&c_i;&#x43F;&#x43A;&c_i;&c_j;</trd>,
    <trd>&c_v;&#8217;&#x44F;&c_z;&#x43A;&c_i;&c_j;</trd>,
    <trd>&c_t;&#x44F;&c_g;&c_u;&c_ch;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

  </art>

