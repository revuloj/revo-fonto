<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>dir</rad>/i
</kap>
<drv mrk="dir.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Konigi sian penson per vortoj, esprimi:
      <ekz>
        <tld/>i libere sian opinion<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i koran dankon al iu<fnt>Z</fnt>;
      </ekz>
      <ekz>
        el la bu&scirc;o multaj vortoj eliras, sed ne &ccirc;iuj ion
        <tld/>as<fnt>Z</fnt>
        <klr>(estas signifoplenaj, gravaj)</klr>;
      </ekz>
      <ekz>
        se oni povas tiel <tld/>i<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi <tld/>is al mi, ke popolo povas erari<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i al si en la pensoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i al si en sia koro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        oni rajtas <tld/>i tiun verkon
        tiel perfekta kiel povas esti homa fara&jcirc;o;
       </ekz>
       <ekz>
         <klr>(f)</klr>
         la senco <tld/>as al ni, ke la sufikso estas
         necesa<fnt>Z</fnt>;
       </ekz>
       <ekz>
         la nerefuteblaj le&gcirc;oj de la
         logiko <tld/>as al mi, ke tio estas la sola vojo<fnt>Z</fnt>,
       </ekz> 
     </dif>
     <refgrp tip="sin">
       <ref cel="sci.0igi">sciigi</ref>,
       <ref cel="asert.0i">aserti</ref>,
       <ref cel="parol.0i">paroli</ref>,
       <ref cel="rakont.0i">rakonti</ref>,
       <ref cel="rimark.0igi">rimarkigi</ref>.
     </refgrp>
   </snc>
   <trd lng="cs">&rcaron;&iacute;ci</trd>
   <trd lng="de">sagen</trd>
   <trd lng="en">say</trd>
</drv>
<drv mrk="dir.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Tio, kion oni <tld/>as. 
    </dif>
  </snc>
  <trd lng="de">Worte</trd>
</drv>
<drv mrk="dir.al0i">
  <kap>al<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld/>ante aldoni. 
    </dif>
  </snc>
  <trd lng="de">hinzuf&uuml;gen</trd>
</drv>
<drv mrk="dir.antaux0i">
  <kap>anta&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Anonci okazonta&jcirc;ojn. 
    </dif>
  </snc>
  <trd lng="de">vorhersagen</trd>
</drv>
<drv mrk="dir.dis0i">
  <kap>dis<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld/>ante divastigi. 
    </dif>
    <refgrp tip="sin">
      <ref cel="babil.0i">babili</ref>,
      <ref cel="perfid.0i">perfidi</ref>,
      <ref cel="port.dis0i">disporti</ref>,
      <ref cel="anonc">disanonci</ref>,
      <ref cel="trumpe.dis0i">distrumpeti</ref>.
    </refgrp>
  </snc>
</drv>
<drv mrk="dir.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Vortigi, formuli, esprimi. 
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>ausdr&uuml;cken</trd>,
    <trd>aussprechen</trd>,
    <trd>formulieren</trd>
  </trdgrp>
</drv>
<drv mrk="dir.kontraux0i">
  <kap>kontra&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Esprimi kontra&ubreve;an opinion, kontra&ubreve;diskuti.  
    </dif>
  </snc>
  <trd lng="de">widersprechen</trd>
</drv>
<drv mrk="dir.kvazaux0iduon0i">
  <kap>kvaza&ubreve;<tld/>i, duon<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Alude diri. 
    </dif>
  </snc>
  <trd lng="de">andeuten</trd>
</drv>
<drv mrk="dir.laux0e">
  <kap>la&ubreve;<tld/>e</kap>
  <snc>
    <dif>
      Konforme al la komuna <tld/>o. 
    </dif>
  </snc>
</drv>
<drv mrk="dir.oni0o">
  <kap>oni<tld/>o</kap>
  <snc>
    <dif>
      Komuna <tld/>o, famo. 
    </dif>
    <refgrp tip="sin">
      <ref cel="klacx.0o">kla&ccirc;o</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Ger&uuml;cht</trd>
</drv>
<drv mrk="dir.popol0o">
  <kap>popol<tld/>o</kap>
  <snc>
    <dif>
      Proverbo.  
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Volksmund</trd>,
    <trd>Sprichwort</trd>
  </trdgrp>
</drv>
</art>
<!--
$Log$
-->
</vortaro>