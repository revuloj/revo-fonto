<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: jeralasx.xml,v 1.7 2007/04/19 16:31:00 revo Exp $">
<kap>
  <rad>jerala&scirc;</rad>/o
</kap>
<drv mrk="jeralasx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      Malnova <ref tip="super" cel="kart.0ludo">kartludo</ref> rusa,
      simila al visto:
      <ekz>
        post la tagman&gcirc;o ni sidi&gcirc;os por ludi <em><tld/>on</em>
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 21a, p. 136a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_je;&c_r;&c_a;&c_l;&c_a;&c_sh; <klr tip="amb">(&c_k;&c_a;&c_r;&c_t;&c_a;&c_ch;&c_n;&c_a;&c_ja; &c_g;&c_u;&c_l;&c_mol;&c_n;&c_ja;)</klr></trd>
  <trd lng="fr">i&eacute;ralache</trd>
  <trd lng="hu">orosz <ind>whist</ind></trd>
  <trdgrp lng="nl">
    <trd>jeralasj</trd>,
    <trd>Russische <ind>whist</ind></trd>
  </trdgrp>
  <trd lng="de">Bridge</trd>
  <trd lng="pt">u&#237;ste</trd>
  <trd lng="ru">&c_je;&c_r;&c_a;&c_l;&c_a;&c_sh;<klr tip="amb"> (&c_k;&c_a;&c_r;&c_t;&c_o;&c_ch;&c_n;&c_a;&c_ja; &c_i;&c_g;&c_r;&c_a;)</klr></trd>
</drv>
</art>

