<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: buro.xml,v 1.10 2023/10/29 19:11:04 revo Exp $">
<kap>
  <rad>buro</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="buro.0o">
  <kap><tld/>o</kap>
  <snc mrk="buro.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Iom granda, sufi&ccirc;e memstara <ref tip="super" cel="admini.0ejo.POL">administrejo</ref> 
      dedi&ccirc;ita al difinita temo:
      <ekz>
        Novjorka <tld lit="B"/>o por Kongresoj kaj Vizitantoj
        <fnt>
          <bib>EeP</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Konsilantaro de la Internacia Paca <tld lit="B"/>o
        <fnt>
          <bib>EeP</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ricevis la tekston de la <tld/>o de AI kaj tradukis &gcirc;in al Esperanto
        <fnt>
          <bib>Kon11</bib>
          <lok>Kontakto 50-jara</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ofic.centra_0ejo">centra oficejo</ref>,
      <ref cel="instit.0o">institucio</ref>,
      <ref cel="kontor.0o">kontoro</ref>,
      <ref cel="reprezent.0ejo">reprezentejo</ref>,
      <ref cel="sid.0ejo">sidejo</ref>
    </refgrp>
    <trd lng="zh">&#x7BA1;&#x7406;&#x5904; <pr>gu&#x1CE;nl&#x1D0;ch&#xF9;</pr></trd>
</snc>
  <trd lng="be">&c_b;&c_ju;&c_r;&c_o; <klr tip="amb">(&c_u;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_a;)</klr></trd>
  <trdgrp lng="cs">
    <trd>byro</trd>,
    <trd>&uacute;&rcaron;ad</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>B&uuml;ro</trd>,
    <trd>Verwaltungsstelle</trd>
  </trdgrp>
  <trd lng="fr">bureau<klr> (administration)</klr></trd>
  <trd lng="he">&mem;&#x5B4;&shin;&#x5C2;&resh;&#x5B8;&dalet;</trd>
  <trdgrp lng="sk">
    <trd>byro</trd>,
    <trd>&uacute;rad</trd>
  </trdgrp>
  <trd lng="uk">&c_b;&c_ju;&c_r;&c_o;</trd>
</drv>
</art>

