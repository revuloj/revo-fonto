    <trd lng="be">&c_p;&c_a;&c_s;&c_mol;&c_v;&c_je;&c_d;&c_ch;&c_a;&c_n;&c_mol;&c_n;&c_je; &c_a;&c_s;&c_o;&c_b;&c_y;</trd>
