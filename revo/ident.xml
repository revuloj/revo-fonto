<drv mrk="ident.0igilo">
  <kap><tld/>igilo</kap>
  <snc mrk="ident.0igilo.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      <ref cel="legiti.0ajxo.JUR">Legitimilo</ref>
      kiu atestas la <tld/>econ<sncref ref="ident.0eco.JUR"/> de iu a&ubreve; io:
      <ekz>
        &leftquot;bonvolu montri <tld/>igilon&rightquot;, li petis
        <fnt><bib>ChB</bib> <lok>15</lok></fnt>;
      </ekz><ekz>
        neniu povas a&ccirc;eti bileton por longdistancaj
        busoj ne montrinte anta&ubreve;e validan <tld/>igilon
        <fnt>
          <aut>K. Kniivil&auml;</aut>: <vrk>Homoj de Putin</vrk>
          <lok>2016</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="indik.0ilo">indikilo</ref>,
      <ref cel="mark.0o.signo">marko<sncref/></ref>,
      <ref cel="signatur.0o">signaturo</ref>
