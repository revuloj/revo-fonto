<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>golf</rad>/o
</kap>
<drv mrk="golf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Vastega parto de maro eni&gcirc;anta en la teron:
      <ekz>
        <tld/>o de Botnio, de Finlando.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Golf</trd>,
    <trd>Meerbusen</trd>
  </trdgrp>
  <trd lng="cs">z&aacute;toka</trd>
  <trd lng="es">golfo</trd>
  <trd lng="fr">golfe</trd>
  <trd lng="nl">golf</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_l;&c_i;&c_v;</trd>,
    <trd>&c_b;&c_u;&c_h;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="pt">golfo</trd>
  <trd lng="tr">k&ouml;rfez</trd>
</drv>

<drv mrk="golf.0eto">
  <kap><tld/>eto</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Parto de maro eni&gcirc;anta en la teron, pli malvasta ol <tld/>o
      <klr>(longa &gcirc;is cent kilometroj, proksimume)</klr>:
      <ekz>
        la <tld/>eto de Napolo.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">baie</trd>
  <trd lng="es">bah&iacute;a</trd>
  <trdgrp lng="pt">
     <trd>ba&iacute;a</trd>,
     <trd>enseada</trd>
  </trdgrp>
</drv>
</art>
