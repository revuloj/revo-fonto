<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lake</rad>/o
</kap>
<drv mrk="lake.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Hejma servisto &ccirc;e ri&ccirc;uloj, ordinare portanta livreon.
    </dif>
    <trdgrp lng="en">
      <trd>servant</trd>,
      <trd>footman</trd>,
      <trd>flunkey</trd>,
      <trd>lackey</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Humila&ccirc;a homo sen memestimo.
    </dif>
  </snc>
  <trd lng="nl">lakei</trd>
  <trd lng="ru">&c_l;&c_a;&c_k;&c_je;&c_j;</trd>
</drv>
</art>
