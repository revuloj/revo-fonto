<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lake</rad>/o
</kap>
<drv mrk="lake.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lake.0o.servisto">
    <dif>
      Hejma <ref tip="super" cel="serv.0isto">servisto</ref> &ccirc;e ri&ccirc;uloj,
      ordinare portanta livreon.
    </dif>
    <trd lng="be">&c_l;&c_jo;&c_k;&c_a;&c_j;</trd>
    <trd lng="br">lakez</trd>
    <trdgrp lng="de">
      <trd>Lakai</trd>,
      <trd>Diener in Livree</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>servant</trd>,
      <trd>footman</trd>,
      <trd>flunkey</trd>,
      <trd>lackey</trd>
    </trdgrp>
  </snc>
  <snc mrk="lake.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Humila&ccirc;a homo sen memestimo.
    </dif>
    <trdgrp lng="be">
      <trd>&c_l;&c_jo;&c_k;&c_a;&c_j;</trd>,
      <trd>&c_h;&c_a;&c_l;&c_u;&c_j;</trd>,
      <trd>&c_p;&c_a;&c_s;&c_l;&c_u;&c_g;&c_a;&c_ch;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Kriecher</trd>,
      <trd>unterw&uuml;rfiger Mensch</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">laquais</trd>
  <trd lng="hu">lak&aacute;ly</trd>
  <trd lng="nl">lakei</trd>
  <trd lng="pt">lacaio</trd>
  <trd lng="ru">&c_l;&c_a;&c_k;&c_je;&c_j;</trd>
</drv>
</art>
