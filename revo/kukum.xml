<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>kukum</rad>/o
</kap>
<uzo tip="fak">BOT</uzo>
<drv mrk="kukum.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kukum.0o.genro">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="kukurb.0acoj">kukurbacoj</ref>
      (<trd lng="la">Cucumis</trd>), al kiu apartenas 
      <tld/>o <sncref ref="kukum.0o.specio"/> kaj <ref tip="sub"
      cel="melon.0o">melono</ref>.
    </dif>
  </snc>
  <snc mrk="kukum.0o.specio">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Specio de <ref tip="super" cel="kukum.0o.genro"><tld/>o
      <sncref ref="kukum.0o.genro"/></ref>
      (<trd lng="la">Cucumis sativus</trd>), kun longformaj kaj akvoplenaj
      fruktoj.
    </dif>
  </snc>
  <snc mrk="kukum.0o.frukto">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Frukto de <ref tip="malprt" cel="kukum.0o.specio"><tld/>o
      <sncref ref="kukum.0o.specio"/></ref>, man&gcirc;ata kruda,
      en salato a&ubreve; konservita en vinagro.
    </dif>
  </snc>
  <trd lng="de">Gurke</trd>
  <trd lng="fr">concombre</trd>
</drv>
<drv mrk="kukum.0eto">
  <kap><tld/>eto</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <tld lit="K"/>o, kiun oni rikoltas anta&ubreve; maturi&gcirc;o, kaj kiun oni
      konfitas en vinagro por servi kiel spica&jcirc;o.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd><ind>saure</ind> Gurke</trd>,
    <trd>Gew&uuml;rzgurke</trd>
  </trdgrp>
  <trd lng="fr">cornichon</trd>
  <trd lng="nl">augurk</trd>
</drv>
</art>
