<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kukum.xml,v 1.38 2024/07/24 06:33:38 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kukum</rad>/o <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="kukum.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="kukum.0o.frukto">
    <uzo tip="fak">KUI</uzo>
    <dif>
      La <ref tip="super" cel="ber.0o.BOT">bero</ref> de
      <ref tip="malprt" cel="kukum.0o.specio"><tld/>o<sncref ref="kukum.0o.specio"/></ref>,
      man&gcirc;ata kruda, en salato a&ubreve; konservita en vinagro:
      <ekz>
        ni man&gcirc;is <klr>[&#8230;]</klr> senpage la <tld/>ojn kaj la melonojn
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 11:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        por du salitaj <tld/>oj aparte kaj duonporcio da kaviaro unu rublo
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        pano kun acidaj <tld/>oj kaj grasega &scirc;mira&jcirc;o
        <fnt>
          <bib>Kon11</bib>
          <lok>Froste kaj toste, sed anta&ubreve; &ccirc;io lingvistike en Pozna&nacute;</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni trinkas <tld/>an pekl-akvon por forigi postebrion
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470; 10</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">uborka<klr tip="amb"> (&eacute;tel)</klr></trd>
    <trd lng="sv">gurka</trd>
  </snc>
  <snc mrk="kukum.0o.specio">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Specio de <ref tip="super" cel="kukum.0o.genro"
      ><tld/>o<sncref ref="kukum.0o.genro"/></ref>
      (<trd lng="la">Cucumis sativus</trd>), kun longformaj kaj
      akvoplenaj fruktoj:
      <ekz>
        sur la kampoj oni semis la linon, hordeon, fabon, fazeolon, <tld/>ojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        en kestetoj <klr>[&#8230;]</klr> staris malgrandaj <tld/>oj kaj aliaj plantoj
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/9/96/ARS_cucumber.jpg/334px-ARS_cucumber.jpg"
      prm="PD"
      ><tld/>o kun fruktoj
        <fnt>
          <aut>Stephen Ausmus, USDA ARS, ID D730-30</aut>
          <lok><url ref="&WCW;/File:ARS_cucumber.jpg"
              >https://commons.wikimedia.org/wiki/File:ARS_cucumber.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="ca">cogombrera</trd>
    <trd lng="en">cucumber</trd>
    <trd lng="hu">uborka<klr tip="amb"> (n&ouml;v&eacute;ny)</klr></trd>
  </snc>
  <snc mrk="kukum.0o.genro">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="kukurb1.0oj">kukurbacoj</ref>
      (<trd lng="la">Cucumis</trd>), al kiu apartenas
      <tld/>o<sncref ref="kukum.0o.specio"/> kaj <ref tip="sub" cel="melon.0o">melono</ref>.
    </dif>
    <trd lng="ca">g&egrave;nere Cucumis</trd>
    <trd lng="hu">Cucumis<klr tip="amb"> (nemzets&eacute;g)</klr></trd>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_r;&c_o;&c_k;</trd>
  <trd lng="bg">&c_k;&c_r;&c_a;&c_s;&c_t;&c_a;&c_v;&c_i;&c_c;&c_a;</trd>
  <trd lng="ca">cogombre</trd>
  <trd lng="de">Gurke</trd>
  <trd lng="es">pepino</trd>
  <trd lng="fa">&a_kh;&f_y;&a_A;&a_r;</trd>
  <trd lng="fr">concombre</trd>
  <trdgrp lng="id">
    <trd>mentimun</trd>,
    <trd>timun</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12461;&#12517;&#12454;&#12522;</trd>,
    <trd>&#12461;&#12517;&#12454;&#12522;&#12398;&#26524;&#23455;
    <pr>&#12365;&#12421;&#12358;&#12426;&#12398;&#12363;&#12376;&#12388;</pr></trd>
  </trdgrp>
  <trd lng="nl">komkommer</trd>
  <trd lng="pl">og&oacute;rek</trd>
  <trd lng="pt">pepino</trd>
  <trd lng="ro">castravete</trd>
  <trd lng="ru">&c_o;&c_g;&c_u;&c_r;&c_je;&c_c;</trd>
  <trd lng="uk">&c_o;&c_g;&c_ib;&c_r;&c_o;&c_k; (&c_r;&c_o;&c_s;&c_l;&c_i;&c_n;&c_a; &c_ib; &c_p;&c_l;&c_ib;&c_d;)</trd>
  <trdgrp lng="zh">
    <trd>&#40643;&#29916; <pr>hu&aacute;nggu&amacron;</pr></trd>,
    <trd>&#40644;&#29916; <pr>hu&aacute;nggu&amacron;</pr></trd>,
    <trd>&#32993;&#29916; <pr>h&uacute;gu&amacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kukum.0eto">
  <kap>
    <tld/>eto
  </kap>
  <snc mrk="kukum.0eto.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="K"/>o, kiun oni rikoltas anta&ubreve; maturi&gcirc;o,
      ordinare por marinado kaj konservado en vinagro kiu igos &gcirc;in
      spica&jcirc;o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_u;&c_r;&c_o;&c_ch;&c_a;&c_k;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_n;&c_ib;&c_sh;&c_o;&c_n;</trd>
  </trdgrp>
  <trd lng="bg">&c_k;&c_i;&c_s;&c_je;&c_l;&c_a; <ind>&c_k;&c_r;&c_a;&c_s;&c_t;&c_a;&c_v;&c_i;&c_ch;&c_k;&c_a;</ind></trd>
  <trdgrp lng="ca">
    <trd>cogombre de mar</trd>,
    <trd>pardal de moro</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>saure <ind>Gurke</ind></trd>,
    <trd>Gew&uuml;rzgurke</trd>,
    <trd>Essiggurke</trd>,
    <trd>Cornichon</trd>,
    <trd>G&uuml;rkchen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>gherkin</trd>,
    <trd>pickle <klr>(cucumber)</klr></trd>
  </trdgrp>
  <trd lng="fa">&a_kh;&f_y;&a_A;&a_r; &a_t;&a_r;&a_sh;&f_y;</trd>
  <trd lng="fr">cornichon</trd>
  <trdgrp lng="hu">
    <trd>fiatal <ind>uborka</ind></trd>,
    <trd>f&udblac;szeruborka</trd>
  </trdgrp>
  <trd lng="id"><ind>timun</ind> acar</trd>
  <trd lng="ja">&#33509;&#12356;&#23567;&#12373;&#12394;&#12461;&#12517;&#12454;&#12522;
  <pr>&#12431;&#12363;&#12356;&#12385;&#12356;&#12373;&#12394;&#12461;&#12517;&#12454;&#12522;</pr></trd>
  <trd lng="nl">augurk</trd>
  <trd lng="pl">korniszon</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_r;&c_n;&c_i;&c_sh;&c_o;&c_n;</trd>,
    <trd>&c_p;&c_i;&c_k;&c_u;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_o;&c_g;&c_ib;&c_r;&c_o;&c_ch;&c_o;&c_k;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_n;&c_ib;&c_sh;&c_o;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#23567;&#40643;&#29916; <pr>xi&#462;ohu&aacute;nggu&amacron;</pr></trd>,
    <trd>&#23567;&#40644;&#29916; <pr>xi&#462;ohu&aacute;nggu&amacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kukum.mar0o">
  <kap>mar<tld/>o</kap>
  <snc mrk="kukum.mar0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Longa, mola besto rampanta en sablo, trovebla en varmaj
      maroj &gcirc;is tre profunde, klaso el <ref tip="super" cel="ehxinoderm.0oj.ZOO">e&hcirc;inodermoj</ref>:
    <ekz>
      <ctl>Maturi&gcirc;o kaj Fekundi&gcirc;o &ccirc;e la Ovo de l' Mar<tld/>oj</ctl>
      <fnt>
        <aut>H. Ohshima</aut>,
        <vrk>Maturi&gcirc;o...</vrk>,
        <lok>Bulteno Scienca de la Fakultato Terkultura, 1925/2,
        p.70a-102a</lok>
      </fnt>;
    </ekz>
    <ekz>
      &leftquot;kaj kiajn florojn tie vi kultivas?&rightquot;  &leftquot;La maranemonojn,&rightquot;
      diris nia
      amiko, &leftquot;en kelkaj raraj specioj. Anka&ubreve; la marstelojn kaj
      mar<tld/>ojn&rightquot;
      <fnt><bib>Salamandroj</bib>, <lok>libro 2a, &ccirc;apitro
      3a</lok></fnt>.
    </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_l;&c_ja;&c_t;&c_u;&c_r;&c_y;&c_ja;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_s;&c_k;&c_ib; <ind>&c_g;&c_u;&c_r;&c_o;&c_k;</ind></trd>,
    <trd>&c_t;&c_r;&c_e;&c_p;&c_a;&c_n;&c_g;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Seegurke</trd>,
    <trd>Seewalze</trd>
  </trdgrp>
  <trd lng="en">sea cucumber</trd>
  <trdgrp lng="fr">
    <trd>concombre de mer</trd>,
    <trd>holothurie</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>teripang</trd>,
    <trd><ind>timun</ind> laut</trd>
  </trdgrp>
  <trd lng="it">oloturia</trd>
  <trdgrp lng="nl">
    <trd>zeekomkommer</trd>,
    <trd>holothurie</trd>
  </trdgrp>
  <trd lng="pl">strzykwa</trd>
  <trdgrp lng="zh">
    <trd>&#28023;&#21442; <pr>h&#462;ish&emacron;n</pr></trd>,
    <trd>&#28023;&#21443; <pr>h&#462;ish&emacron;n</pr></trd>
  </trdgrp>
</drv>
</art>
