<art mrk="$Id$">
<kap>
 <ofc>*</ofc>
 <rad>ci</rad>
</kap>
<drv mrk="ci.0">
 <kap><ofc>*</ofc><tld/></kap>
 <uzo tip="klr">Ev.</uzo>
 <snc num="1">
  <dif>
   Pronomo de la dua persono en ununombro, uzata por alparoli iun senceremonie.  
  </dif>
 </snc>
 <snc num="2">
  <dif>
   Kelkfoje alparolante al Dio.  
  </dif>
 </snc>
 <rim>
  Apena&ubreve; uzata.
 </rim>
 <trd lng="be">&c_t;&c_y;</trd>
 <trd lng="de">du</trd>
 <trd lng="en">thou<klr> (ne plu uzata krom en pregxoj)</klr></trd>
 <trd lng="es">t&uacute;</trd>
 <trd lng="fr">tu</trd>
 <trd lng="nl">jij</trd>
 <trd lng="ru">&c_t;&c_y;</trd>
 <trd lng="sv">du</trd>
</drv>
</art>
