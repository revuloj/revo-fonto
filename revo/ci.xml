<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
  <ofc>*</ofc>
  <rad>ci</rad>
  </kap>
  <drv mrk="ci.0">
    <kap><ofc>*</ofc><tld/></kap>
    <uzo tip="stl">EVI</uzo>
    <uzo tip="stl">RAR</uzo>
    <snc num="1">
      <dif>
        Pronomo de la dua persono en ununombro, uzata por alparoli iun
        senceremonie.
      </dif>
      <trd lng="es">usted</trd>
      <trd lng="pl">ty</trd>
    </snc>
    <snc num="2">
      <dif>
        Kelkfoje alparolante al Dio.
      </dif>
      <trd lng="pl">Ty</trd>
    </snc>
    <trd lng="be">&c_t;&c_y;</trd>
    <trd lng="br">te</trd>
    <trd lng="de">du</trd>
    <trd lng="el">(&epsilon;)&sigma;&ypsilon_ton;</trd>
    <trd lng="en">thou<klr> (ne plu uzata krom en
    pre&gcirc;oj)</klr></trd>
    <trd lng="fr">tu</trd>
    <trd lng="hu">te</trd>
    <trd lng="it">tu</trd>
    <trd lng="nl">jij</trd>
    <trd lng="pt">tu</trd>
    <trd lng="ru">&c_t;&c_y;</trd>
    <trd lng="sk">ty</trd>
    <trd lng="sv">du</trd>
    <trd lng="vo">ol</trd>
  </drv>

</art>

