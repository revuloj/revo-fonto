<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ci</rad> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ci.0">
  <kap><tld/></kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="ci.0.MOR">
    <dif>
      Pronomo de la dua persono en ununombro, uzata por alparoli iun
      senceremonie.
    </dif>
    <trd lng="es">t&uacute;<klr tip="amb"> (2&#170; persona singular tratamiento de
    t&uacute;)</klr></trd>
    <trd lng="pl">ty</trd>
  </snc>
  <snc mrk="ci.0.REL">
    <dif>
      Kelkfoje alparolante al Dio.
    </dif>
    <trd lng="pl">Ty</trd>
  </snc>
  <trd lng="be">&c_t;&c_y;</trd>
  <trd lng="br">te</trd>
  <trd lng="de">du</trd>
  <trd lng="el">(&epsilon;)&sigma;&ypsilon_ton;</trd>
  <trd lng="en">thou<klr> (ne plu uzata krom en
  pre&gcirc;oj)</klr></trd>
  <trd lng="fa">&a_t;&a_w;</trd>
  <trd lng="fr">tu</trd>
  <trd lng="hu">te</trd>
  <trd lng="it">tu</trd>
  <trd lng="nl">jij</trd>
  <trd lng="pt">tu</trd>
  <trd lng="ru">&c_t;&c_y;</trd>
  <trd lng="sk">ty</trd>
  <trd lng="sv">du</trd>
  <trd lng="vo">ol</trd>
</drv>
</art>

