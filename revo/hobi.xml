<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hobi.xml,v 1.16 2019/10/09 20:10:21 revo Exp $">
<kap>
  <rad>hobi</rad>/o
</kap>
<drv mrk="hobi.0o">
  <kap><tld/>o <fnt><bib>SPIV</bib></fnt></kap>
  <uzo tip="stl">NEO</uzo>
  <snc mrk="hobi.0o.KOMUNE">
    <dif>
      <ref tip="dif" cel="sxat.0okupo">&Scirc;atokupo</ref>:
      <ekz>
        inter niaj <tld/>oj estas klasika muziko
        <fnt>
          <aut>A. kaj M. Pentus</aut>:
          <vrk>[memprezento]</vrk>,
          <bib>Monato</bib>,
          <lok>2002:6, p. 6a</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;ardenumado estas tre absorba <tld/>o
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006878.html">Dommastrumo: &Gcirc;ardenumi</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        tiutempe ekestis anka&ubreve; nova <tld/>o: Filatelo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007065.html">Filatelo: VIPE 2000, a&ubreve; &ccirc;u la po&scirc;tmarkoj malaperos?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        beda&ubreve;rinde, praktikadon de miaj preferataj <tld/>oj &ndash; &ccirc;arpentado, pentrado kaj
        biciklado &ndash; iom malhelpas sekvoj de ses operacioj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="https://www.monato.be/2003/008009.html">Stefan Maul</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        preska&ubreve; tutan tempon mi dedi&ccirc;as al mia plej nova, poreterna <tld/>o: eduko de
        nia fileto
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="https://www.monato.be/2003/008010.html">Katalin Kov&aacute;ts</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_o;&c_b;&c_ib;</trd>,
    <trd>&c_z;&c_a;&c_h;&c_a;&c_p;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="bg">&c_h;&c_o;&c_b;&c_i;</trd>
  <trd lng="ca">passatemps</trd>
  <trdgrp lng="de">
    <trd>Hobby</trd>,
    <trd>Steckenpferd</trd>,
    <trd>Freizeitbesch&auml;ftigung</trd>
  </trdgrp>
  <trd lng="el">&chi;&omikron_ton;&my;&pi;&jota;</trd>
  <trd lng="en">hobby</trd>
  <trdgrp lng="es">
    <trd>hobby</trd>,
    <trd>pasatiempo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>hobby</trd>,
    <trd>dada<klr tip="ind"> (passe-temps)</klr></trd>,
    <trd>marotte</trd>,
    <trd>passe-temps</trd>,
    <trd>violon d'Ingres</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hobbi</trd>,
    <trd>kedvtel&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>hobby</trd>,
    <trd>passatempo</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>hobby</trd>,
    <trd>stokpaardje</trd>,
    <trd>vrijetijdsbesteding</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>hobby</trd>,
    <trd>passatempo</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_v;&c_l;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_h;&c_o;&c_b;&c_b;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="hobi.0a">
  <kap><tld/>a</kap>
  <snc mrk="hobi.0a.KOMUNE">
    <dif>
      Rilata al <tld/>o; praktikanta <tld/>on:
      <ekz>
        ne &ccirc;iu povas a&ccirc;eti komputilon nur por hejma <tld/>a uzo
        <fnt>
          <aut>Pejno S.</aut>,
          <vrk>Sidi&gcirc;u kaj tajpu!</vrk>,
          <bib>Monato</bib>,
          <lok>2001:4, p. 18a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj &gcirc;ardenistoj inter&scirc;an&gcirc;as siajn plantojn kaj spertojn
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="https://www.monato.be/2008/009355.php">Nordiaj rozoj</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        bastonmar&scirc;ado estas la plej rapide evoluanta <tld/>a sporto en la mondo
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="https://www.monato.be/2005/008790.php">Praktiki bastonmar&scirc;adon</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">de lleure</trd>
  <trd lng="it">hobbistico</trd>
  <trd lng="ru">&c_l;&c_ju;&c_b;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="hobi.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="hobi.0ismo.KOMUNE">
    <dif>
      <tld lit="H"/>a praktiko de iu agado, ne profesi-nivela,
      eventuale ne serioza; amatoreco, diletantismo:
      <ekz>
        tro ofte <klr>[&#8230;]</klr> en niaj kluboj regas la <tld/>ismo
        <fnt>
          <aut>I. F. Bociort</aut>,
          <vrk>en: <url ref="http://esperanto-ondo.ru/Historio/247bocrt.htm">
          Profesoro Bociort respondas</url></vrk>,
          <lok>La Ondo de Esperanto, 2015:4-5 (246-247)</lok>
        </fnt>;
      </ekz>
      <ekz>
        nia movado finfine transpas<klr>[&#8230;]</klr>is la stadion de nura lingvo<tld/>ismo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="https://www.monato.be/2004/008242.html">Gravaj opinioj</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Hobbyismus</trd>
  <trd lng="fr">amateurisme</trd>
</drv>

<drv mrk="hobi.0ulo">
  <kap>
    <tld/>ulo
  </kap>
  <snc mrk="hobi.0ulo.praktikanto">
    <dif>
      Praktikanto de <tld/>o:
      <ekz>
        klubo de <tld/>uloj
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="https://www.monato.be/2009/009629.php">Vintra banado</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        en sia historia evoluo arto kaj scienco &scirc;uldas multon al <tld/>uloj, kiuj
        kelkfoje atingas nekredeblajn rezultojn
        <fnt>
          <bib>Monato</bib>
          <aut>Dimitar Ha&gcirc;iev</aut>
          <vrk><url ref="https://www.monato.be/2009/009670.php">Pri la nobla pasio hobio</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        ornitologoj kaj <tld/>uloj sur la marbordo de <nom>Tammisaari</nom> binokle observas la
        migrobirdojn tien flugantajn
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="https://www.monato.be/2014/011095.php">Tammisaari, urbeto &ccirc;e Balta Maro</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
       <ref cel="amator.0o">amatoro</ref>,
       <ref cel="laik.0o.FIG">laiko<sncref/></ref>
    </refgrp>
    <refgrp tip="ant">
      <ref cel="profes.0isto">profesiisto, profesiulo</ref>,
      <ref cel="eksper1.0o">eksperto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Laie</trd>,
    <trd>Amateur</trd>
  </trdgrp>
</drv>


</art>
