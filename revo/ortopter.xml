<art mrk="$Id: ortopter.xml,v 1.15 2024/01/30 16:09:26 revo Exp $">
<kap><rad>ortopter</rad>/oj</kap>
<drv mrk="ortopter.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_ordoj" cel="ord.0o.BIO">Ordo</ref>
      de <ref tip="malprt" cel="insekt.0oj">insektoj</ref>
      (<trd lng="la">Orthoptera</trd>):
      <ekz>
        akrido estas familio de saltanta insekto el la ordo de <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=23035">Akrido</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_m;&c_a;&c_k;&c_r;&c_y;&c_l;&c_y;&c_ja; <klr tip="amb">(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="cs">rovnok&rcaron;&iacute;dl&iacute;</trd>
  <trd lng="de">Geradfl&uuml;gler</trd>
  <trd lng="en">orthoptera</trd>
  <trd lng="hu">egyenessz&aacute;rny&uacute;ak<klr tip="amb"> (rend)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x76F4;&#x7FC5;&#x985E;  <pr>&#x3061;&#x3087;&#x304F;&#x3057;&#x308B;&#x3044;</pr></trd>,
    <trd>&#x30D0;&#x30C3;&#x30BF;&#x76EE;  <pr>&#x30D0;&#x30C3;&#x30BF;&#x3082;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">rechtvleugeligen</trd>
  <trd lng="pl">prostoskrzyd&lstroke;e</trd>
  <trd lng="pt">ort&oacute;pteros</trd>
  <trd lng="ro">ortoptera</trd>
  <trd lng="ru">&c_p;&c_r;&c_ja;&c_m;&c_o;&c_k;&c_r;&c_y;&c_l;&c_y;&c_je; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d; &c_n;&c_a;&c_s;&c_je;&c_k;&c_o;&c_m;&c_y;&c_h;)</klr></trd>
  <trd lng="sk">rovnokr&iacute;dly hmyz</trd>
  <trd lng="uk">&#x43F;&#x440;&#x44F;&#x43C;&#x43E;&#x43A;&#x440;&#x438;&#x43B;&#x456;</trd>
  <trd lng="zh">&#x76F4;&#x7FC5;&#x76EE; <pr>zh&#xED;ch&#xEC;m&#xF9;</pr></trd>
</drv>
