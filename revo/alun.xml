<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>alun</rad>/o
</kap>
<drv mrk="alun.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Duobla sulfato de aluminio kaj de alia metalo (kalio, amonio, fero,
      kromo):
      <ekz>
        la kalian <tld/>on oni nomas ofte simple <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">alum</trd>
  <trd lng="fr">alun</trd>
  <trd lng="nl">aluin</trd>
  <trd lng="ru">&c_k;&c_v;&c_a;&c_s;&c_c;&c_y;</trd>
</drv>
</art>
