<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: alun.xml,v 1.21 2023/08/30 18:23:33 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>alun</rad>/o
</kap>
<drv mrk="alun.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Duobla sulfato de aluminio kaj de alia metalo (kalio, amonio,
      fero,
      kromo):
      <ekz>
        la kalian <tld/>on oni nomas ofte simple <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_l;&c_y;&c_n;</trd>
  <trd lng="bg">&c_s;&c_t;&c_i;&c_p;&c_c;&c_a;</trd>
  <trd lng="ca">alum</trd>
  <trd lng="cs">kamenec</trd>
  <trd lng="de">Alaun</trd>
  <trd lng="en">alum</trd>
  <trd lng="es">alumbre</trd>
  <trdgrp lng="fa">
    <trd>&a_z;&a_A;&a_j;</trd>,
    <trd>&a_A_madda;&a_l;&a_w;&a_m;</trd>
  </trdgrp>
  <trd lng="fr">alun</trd>
  <trd lng="he">&#x5D0;&#x5DC;&#x5D5;&#x5DD;</trd>
  <trd lng="ja">&#x30DF;&#x30E7;&#x30A6;&#x30D0;&#x30F3;</trd>
  <trd lng="nl">aluin</trd>
  <trdgrp lng="pt">
    <trd>alume</trd>,
    <trd>pedra-ume</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_v;&c_a;&c_s;&c_c;&c_y;</trd>
  <trdgrp lng="sk">
    <trd>kamenec</trd>,
    <trd>kamenec <klr>(hlinito draseln&yacute;)</klr></trd>
  </trdgrp>
  <trd lng="uk">&#x433;&#x430;&#x43B;&#x443;&#x43D;</trd>
</drv>
</art>
