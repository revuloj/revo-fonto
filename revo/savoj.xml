<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: savoj.xml,v 1.8 2023/10/21 06:31:28 revo Exp $">
    <kap>
      <rad>Savoj</rad>/o
    </kap>

<drv mrk="savoj.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">HIS</uzo>
  <snc mrk="savoj.0o.regiono">
    <dif>
      Plej alta <ref tip="lst" lst="voko:regionoj" cel="region.0o.land">regiono</ref>
      de E&ubreve;ropo sude de la &Gcirc;eneva lago, iama <ref tip="super" cel="duk.0ujo">dukujo</ref>,
      nun parto de <ref tip="malprt" cel="franc.F0ujo">Francujo</ref>:
      <ekz>
        la klimato kaj vetero en <tld/>o dependas de la altitudo
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2005/008395.php">Vizito al la nacia parko Vanoise</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        en 1729 Vittorio Amadeo 2a de <tld/>o a&ccirc;etis la bienon
       <fnt>
       <aut>Thomas Eccardt</aut>
          <vrk><url ref="&Monato;/2000/006859.php">Urbeto a&ubreve; sendependa lando?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/05, p. 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        perditaj estis 800 maristoj, 80 kanonoj kaj sufi&ccirc;e da oraj moneroj por suba&ccirc;eti
        la fidelecon de la Duko de <tld/>o en disputo inter Britio kaj Francio
        <fnt>
          <bib>Monato</bib>
          <aut>Elson B. Snow</aut>
          <vrk><url ref="&Monato;/2004/008155.html">Trez-oro submara</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_S;&c_a;&c_v;&c_o;&c_j;</trd>
    <trd lng="de">Savoyen</trd>
    <trd lng="en">Savoy</trd>
    <trd lng="fr">Savoie</trd>
    <trd lng="pl">Sabaudia</trd>
  </snc>
  <trd lng="ja">&#x30B5;&#x30F4;&#x30A9;&#x30A2;&#x5730;&#x65B9;  <pr>&#x30B5;&#x30F4;&#x30A9;&#x30A2;&#x3061;&#x307B;&#x3046;</pr></trd>
  <trd lng="uk">&c_S;&c_a;&c_v;&#x43E;&c_j;&#x44F;</trd>
</drv>

  </art>

