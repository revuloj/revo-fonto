<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: percxe.xml,v 1.5 2022/03/25 12:53:16 revo Exp $">
<kap>
  <rad>per&ccirc;e</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="percxe.0o">
  <kap><tld/>o</kap>
  <snc mrk="percxe.0o.MEZ">
    <dif>
      Malnova e&ubreve;ropa
      <ref tip="lst" cel="unu.mezur0o" lst="voko:longoj">mezurunuo</ref>
      de longo inter 2,5 kaj 7 metrojn; kvadrata <tld/>o respondas al 160-ono de
      <ref cel="akre.0o">akreo</ref>:  
      <ekz>
        vasta na&ubreve; a&ubreve; dek <tld/>ojn
        <fnt><aut>A. Manzoni, trad. B. Cadei</aut>, <vrk>La 
        Gefian&ccirc;oj</vrk>, <lok>2006</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Perch</trd>,
    <trd>Pole</trd>,
    <trd>Rod</trd>
  </trdgrp>  
  <trd lng="en">perch <klr>(unit)</klr></trd>
  <trd lng="fr">perche<klr> (unit&eacute; de longueur)</klr></trd>
  <trd lng="pl">pr&eogonek;t  <klr>(miara)</klr></trd>
  <trd lng="ro">rod  <klr>(unitate de m&abreve;sur&abreve;)</klr></trd>
  <trd lng="uk">&#x43F;&#x435;&#x440;&#x448; (&#x441;&#x442;&#x430;&#x440;&#x43E;&#x432;&#x438;&#x43D;&#x43D;&#x430; &#x43C;&#x456;&#x440;&#x430; &#x43F;&#x43B;&#x43E;&#x449;&#x456;)</trd>
</drv>
</art>

