<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fez.xml,v 1.18 2022/04/27 17:19:11 revo Exp $">
<kap>
  <rad>fez</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="fez.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Turkdevena konustrunka <ref tip="super" cel="cxap.0o">&ccirc;apo</ref>
      el ordinare ru&gcirc;a a&ubreve; blanka drapo, kun pendanta nigra peniko a&ubreve;
      kvasto, nun malofte plu portata en islamaj landoj:
      <ekz>
        la kvasto sur mia <tld/>o estis ornamita per ar&gcirc;ento
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        sub sia <klr>[&#x2026;]</klr> nigra <tld/>&ccirc;apo la maljunulo 
        ridetis fiere <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro 36a, p. 
        86a</lok></fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/f/fc/Bedouin_man_with_Fez.jpg/315px-Bedouin_man_with_Fez.jpg" prm="CC BY 2.0">bedueno kun <tld/>o, Israelo 2005
        <fnt>
          <aut>Steve Evans aka &leftquot;babasteve&rightquot;</aut>
          <lok><url ref="&WCW;/File:Bedouin_man_with_Fez.jpg">&WCW;/File:Bedouin_man_with_Fez.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_f;&c_e;&c_s;&c_k;&c_a;</trd>
  <trd lng="cs">fez</trd>
  <trd lng="de">Fes</trd>
  <trd lng="en">fez</trd>
  <trd lng="fa">&a_f;&f_y;&a_n;&a_h;</trd>
  <trd lng="fr">fez</trd>
  <trd lng="hu">fez</trd>
  <trd lng="nl">fez</trd>
  <trdgrp lng="pt">
    <trd>fez</trd>,
    <trd>barrete turco</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_je;&c_s;&c_k;&c_a;</trd>
  <trd lng="sk">fez</trd>
  <trd lng="uk">&#x444;&#x435;&#x441;&#x43A;&#x430; (&#x448;&#x430;&#x43F;&#x43E;&#x447;&#x43A;&#x430; &#x437; &#x43A;&#x438;&#x442;&#x438;&#x446;&#x435;&#x44E;)</trd>
</drv>
</art>
