<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fez.xml,v 1.22 2025/04/15 18:38:05 revo Exp $">
<kap>
  <rad>fez</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="fez.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Turkdevena konustrunka <ref tip="super" cel="cxap.0o">&ccirc;apo</ref>
      el ordinare ru&gcirc;a a&ubreve; blanka drapo, kun pendanta nigra peniko a&ubreve;
      kvasto, nun malofte plu portata en islamaj landoj:
      <ekz>
        la kvasto sur mia <tld/>o estis ornamita per ar&gcirc;ento
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        sub sia <klr>[&#8230;]</klr> nigra <tld/>&ccirc;apo la maljunulo
        ridetis fiere <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro 36a, p.
        86a</lok></fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/f/fc/Bedouin_man_with_Fez.jpg/315px-Bedouin_man_with_Fez.jpg" prm="CC BY
    2.0">bedueno kun <tld/>o, Israelo 2005
        <fnt>
          <aut>Steve Evans aka &leftquot;babasteve&rightquot;</aut>
          <lok><url
          ref="&WCW;/File:Bedouin_man_with_Fez.jpg">&WCW;/File:Bedouin_man_with_Fez.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_f;&c_e;&c_s;&c_k;&c_a;</trd>
  <trd lng="cs">fez</trd>
  <trd lng="de">Fes</trd>
  <trd lng="en">fez</trd>
  <trd lng="fa">&a_f;&f_y;&a_n;&a_h;</trd>
  <trd lng="fr">fez</trd>
  <trd lng="hu">fez</trd>
  <trdgrp lng="ja">
    <trd>&#x30D5;&#x30A7;&#x30BA;</trd>,
    <trd>&#x30C8;&#x30EB;&#x30B3;&#x5E3D;  <pr>&#x30C8;&#x30EB;&#x30B3;&#x307C;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">fez</trd>
  <trd lng="pl">fez</trd>
  <trdgrp lng="pt">
    <trd>fez</trd>,
    <trd>barrete turco</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_je;&c_s;&c_k;&c_a;</trd>
  <trd lng="sk">fez</trd>
  <trd lng="uk">&c_f;&c_je;&c_s;&#x43A;&c_a; (&c_sh;&c_a;&#x43F;&#x43E;&c_ch;&#x43A;&c_a; &c_z; &#x43A;&c_i;&c_t;&c_i;&c_c;&c_je;&#x44E;)</trd>
</drv>
</art>
