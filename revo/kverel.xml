<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>4</ofc>
   <rad>kverel</rad>/i
</kap>
<drv mrk="kverel.0i">
   <kap>
     <tld/>i
   </kap>
   <gra><vspec>ntr</vspec></gra>
   <snc>
     <dif>
       Kolere kaj brue interdisputi:
       <ekz>
         <tld/>i kun iu<fnt>Z</fnt>,
       </ekz>
       <ekz>
         kontra&ubreve; iu<fnt>Z</fnt>;
       </ekz>
       <ekz>
         kontra&ubreve; viaj <tld/>antoj mi <tld/>os<fnt>Z</fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_s;&c_v;&c_a;&c_r;&c_y;&c_c;&c_c;&c_a;</trd>,
     <trd>&c_l;&c_a;&c_ja;&c_c;&c_c;&c_a;</trd>
   </trdgrp>
   <trdgrp lng="de">
       <trd>streiten</trd>,
       <trd>zanken</trd>
   </trdgrp>
   <trd lng="hu">veszekszik</trd>
   <trd lng="nl">ruzie maken</trd>
   <trd lng="fr">se <ind>quereller</ind></trd>
   <trdgrp lng="ru">
     <trd>&c_s;&c_s;&c_o;&c_r;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
     <trd>&c_b;&c_r;&c_a;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
   </trdgrp>
   <trd lng="sv">gr&auml;la</trd>
</drv>
</art>
