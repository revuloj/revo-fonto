<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: jasp.xml,v 1.25 2020/09/09 06:10:37 revo Exp $">
<kap>
  <rad>jasp</rad>/o <fnt><bib>PV</bib></fnt>,
  <var><kap><rad var="is">jaspis</rad>/o <fnt><bib>MT</bib></fnt></kap></var>
</kap>
<drv mrk="jasp.0o">
  <kap><tld/>o, <var><kap><tld var="is"/>o</kap></var></kap>
  <uzo tip="fak">MIN</uzo>
  <snc mrk="jasp.0o.sxtono">
    <dif>
      Malmola kaj nediafana &scirc;tono, simila al agato, havanta 
      multkolorajn striojn kaj makulojn:
      <ekz>
        multekostaj &scirc;tonoj vin kovris: rubeno, topazo, diamanto, sukceno,
        berilo, <tld var="is"/>o
        <fnt><bib>MT</bib>, <lok>&Jeh; 28:13</lok></fnt>.
      </ekz>
      <ekz>
        en aspekto simila al &scirc;tono <tld var="is"/>a kaj sardia
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 4:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        uzu oron, ar&gcirc;enton, malakiton, turkisojn, <tld var="is"/>ojn, smeraldojn
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        iliajn <tld var="is"/>ajn korpojn ru&gcirc;igis la sango
        <fnt>
          <bib>FdO</bib>
        </fnt>;
      </ekz>
      <ekz>
        la Sankta Kapelo <klr>[estas]</klr> realigita per noblaj materialoj: bronzo,
        <tld/>o kaj marmoro
        <fnt>
          <vrk><url
          ref="https://sites.google.com/site/fratecoesperanto/1970-1985---6/1983---baziliko-n-s-pilar"
              >1983 - Baziliko NiaSinjorino Pilar - Frateco</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la pre&gcirc;ejan fasadon kaj internon ornamas pli ol 500 antikvaj kolonoj el marmoro,
        porfiro, <tld/>o, serpenteno kaj alabasto
        <fnt>
          <vrk><url ref="http://www.literatura.bucek.name/brno/trzosova-venecio.pdf"
              >Esperanto-Brno</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">jasper</trd>
  <trd lng="be">&c_ja;&c_sh;&c_m;&c_a;</trd>
  <trd lng="bg">&c_ja;&c_s;&c_p;&c_i;&c_s;</trd>
  <trd lng="ca">jaspi</trd>
  <trd lng="cs">jaspis</trd>
  <trd lng="de">Jaspis</trd>
  <trd lng="el">&jota_ton;&alfa;&sigma;&pi;&jota;&sigma_fina;</trd>
  <trd lng="es">jaspe</trd>
  <trd lng="fr">jaspe</trd>
  <trd lng="he">&yod;&shin;&pe;&he;</trd>
  <trd lng="hu">j&aacute;spis</trd>
  <trd lng="it">diaspro</trd>
  <trd lng="nl">jaspis</trd>
  <trd lng="pt">jaspe</trd>
  <trd lng="ru">&c_ja;&c_sh;&c_m;&c_a;</trd>
  <trd lng="sk">jaspis</trd>
  <trd lng="sv">jaspis</trd>
</drv>

<drv mrk="jasp.0i">
<kap><tld/>i</kap>
  <snc>
   <dif>
    Farbi multkolore imitante la kolorojn de <tld/>o:
    <ekz><tld/>i la randojn de libro.</ekz>
   </dif>
  </snc>
  <trd lng="ca">jaspiar</trd>
   <trd lng="fr">jasper</trd>
   <trd lng="hu">m&aacute;rv&aacute;nyoz</trd>
   <trd lng="it">marmorizzare</trd>
</drv>
</art>

