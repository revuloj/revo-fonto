<vortaro>
<art mrk="$Id$">
<kap>
  <rad>dur</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="dur.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      Malnova <ref tip="super" cel="unu.mon0o">monunuo</ref> de
      Hispanujo kaj Portugalujo. Ar&gcirc;enta monero.
    </dif>
  </snc>
  <trd lng="de">Duro</trd>
  <trd lng="es">duro <klr tip="amb">(moneda)</klr></trd>
  <trd lng="fr">douro</trd>
  <trd lng="hu">duro <klr tip="amb">(p&eacute;nz)</klr></trd>
  <trdgrp lng="pt">
    <trd>duro</trd>,
    <trd>peso duro</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_u;&c_r;&c_o; <klr tip="amb">(&c_m;&c_o;&c_n;&c_je;&c_t;&c_a;)</klr></trd>
</drv>
</art>
