<art mrk="$Id: frenol.xml,v 1.19 2023/11/10 06:08:01 revo Exp $">
<kap>
  <rad>frenologi</rad>/o
</kap>
<drv mrk="frenol.0o">
  <kap><tld/>o</kap>
  <snc mrk="frenol.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Scienco populara dum la 19a jarcento, kiu la&ubreve; la ekstera formo de
      homa kranio konkludis pri cerbo, intelektaj kapabloj,
      temperamento:
      <ekz>
        sed al ni, ekzemple, anka&ubreve; la <tld/>o ne estas fremda
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 20a, p.
        120a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_r;&c_e;&c_n;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="ca">frenologia</trd>
  <trdgrp lng="cs">
    <trd>frenologie</trd>,
    <trd>lebozpyt</trd>
  </trdgrp>
  <trd lng="de">Phrenologie</trd>
  <trd lng="es">frenolog&iacute;a</trd>
  <trd lng="fr">phr&eacute;nologie</trd>
  <trdgrp lng="hu">
    <trd>frenol&oacute;gia</trd>,
    <trd>koponyatan</trd>
  </trdgrp>
  <trd lng="nl">frenologie</trd>
  <trdgrp lng="pt">
    <trd>frenologia</trd>,
    <trd>frenologismo</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_r;&c_je;&c_n;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="sk">frenol&oacute;gia</trd>
  <trd lng="uk">&#x444;&#x440;&#x435;&#x43D;&#x43E;&#x43B;&#x43E;&#x433;&#x456;&#x44F;</trd>
  <trd lng="zh">&#x9871;&#x76F8;&#x5B78; <pr>l&#xFA;xi&#x101;ngxu&#xE9;</pr></trd>
</drv>

<drv mrk="frenol.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="frenol.0isto.SCI">
    <dif>
      Fakulo pri <ref tip="vid" cel="frenol.0o.SCI"><tld/>o</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_r;&c_e;&c_n;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="ca">fren&ograve;leg</trd>
  <trd lng="fr">phr&eacute;nologue</trd>
</drv>
</art>
