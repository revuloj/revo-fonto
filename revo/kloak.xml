<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kloak</rad>/o
</kap>
<drv mrk="kloak.0o">
  <kap><tld/>o</kap>
  <snc mrk="kloak.0o.fosajxo">
    <dif>
      Fosa&jcirc;o, en kiu kolekti&gcirc;as la hejmaj malpura&jcirc;oj kaj ekskrementoj.
    </dif>
    <trdgrp lng="de">
      <trd>Pfuhl</trd>,
      <trd>Jauchegrube</trd>,
      <trd>Senkgrube</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>cesspool</trd>,
      <trd>cesspit</trd>,
      <trd>latrine</trd>,
      <trd>septic tank</trd>
    </trdgrp>
    <trd lng="fr">fosse septique</trd>
    <trd lng="nl">beerput</trd>
    <trd lng="ru">&c_v;&c_y;&c_g;&c_r;&c_je;&c_b;&c_n;&c_a;&c_ja; &c_ja;&c_m;&c_a;</trd>
  </snc>
  <snc mrk="kloak.0o.kanalo">
    <dif>
      Kanalo, ordinare subtera, tra kiu elfluas la malpura&jcirc;oj kaj
      ekskrementoj de urbo.
    </dif>
    <trdgrp lng="de">
      <trd>Abwasserkanal</trd>,
      <trd>Unratkanal</trd>
    </trdgrp>
    <trd lng="en">sewer</trd>
    <trd lng="fr">&eacute;gout<klr tip="ind"> (collecteur d'eaux us&eacute;es)</klr></trd>
    <trd lng="nl">riool</trd>
    <trd lng="ru">&c_k;&c_a;&c_n;&c_a;&c_l;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;</trd>
  </snc>
  <snc mrk="kloak.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Loko, kie multi&gcirc;as plej malla&ubreve;dindaj faroj:
      <ekz>
        tiu urbo, <tld/>o de &ccirc;iuj malvirtoj.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Pfuhl</trd>,
      <trd>S&uuml;ndenpfuhl</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>cesspit</trd>,
      <trd>cesspool</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>bas-fond<klr tip="ind"> (fig.)</klr></trd>,
      <trd>quartier malfam&eacute;</trd>
    </trdgrp>
    <trd lng="nl">beerput</trd>
  </snc>
  <snc mrk="kloak.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      &Ccirc;e rampuloj kaj birdoj, la ekstrema&jcirc;o de la <ref tip="malprt"
      cel="intest.0o">intesto</ref>, formanta po&scirc;on.
    </dif>
    <trd lng="de">Kloake</trd>
    <trd lng="en">cloaca</trd>
    <trd lng="fr">cloaque<klr tip="ind"> (anat.)</klr></trd>
    <trd lng="nl">cloaca</trd>
  </snc>
  <trd lng="ru">&c_k;&c_l;&c_o;&c_a;&c_k;&c_a;</trd>
</drv>
</art>
