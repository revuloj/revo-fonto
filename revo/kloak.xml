<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kloak</rad>/o
</kap>
<drv mrk="kloak.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Fosa&jcirc;o, en kiu kolekti&gcirc;as la hejmaj malpura&jcirc;oj kaj ekskrementoj.
    </dif>
    <trdgrp lng="en">
      <trd>cesspool</trd>,
      <trd>latrine</trd>,
      <trd>septic tank</trd>
    </trdgrp>
    <trd lng="fr">fosse septique</trd>
    <trd lng="nl">beerput</trd>
    <trd lng="ru">&c_v;&c_y;&c_g;&c_r;&c_je;&c_b;&c_n;&c_a;&c_ja; &c_ja;&c_m;&c_a;</trd>
  </snc>
  <snc num="2">
    <dif>
      Kanalo, ordinare subtera, tra kiu elfluas la malpura&jcirc;oj kaj
      ekskrementoj de urbo. 
    </dif>
    <trd lng="en">sewer</trd>
    <trd lng="fr">egout</trd>
    <trd lng="nl">riool</trd>
    <trd lng="ru">&c_k;&c_a;&c_n;&c_a;&c_l;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;</trd>
  </snc>
  <snc num="3">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Loko, kie multi&gcirc;as plej malla&ubreve;dindaj faroj:
      <ekz>tiu urbo, <tld/>o de &ccirc;iuj malvirtoj.</ekz>
    </dif>
    <trd lng="fr">bas-fond<klr> (fig.)</klr></trd>
    <trd lng="nl">beerput</trd>
  </snc>
  <snc num="4">
    <uzo tip="fak">ANA</uzo>
    <dif>
      &Ccirc;e rampuloj kaj birdoj, la ekstrema&jcirc;o de la intesta kanalo,
      formanta po&scirc;on.  
    </dif>
  </snc>
  <trd lng="en">cloaca</trd>
  <trd lng="fr">cloaque<klr> (anatomie)</klr></trd>
  <trd lng="nl">cloaca</trd>
  <trd lng="ru">&c_k;&c_l;&c_o;&c_a;&c_k;&c_a;</trd>
</drv>
</art>
