<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>Afrodit</rad>/o</kap>

<drv mrk="afrodit.0o">
  <kap><tld/>o, <tld/>a</kap>
  <snc>
    <uzo tip="fak">MIT</uzo>
    <dif>
      Helena <ref tip="super" cel="di.0o.politeisma">diino</ref> de amo
      kaj beleco, naski&gcirc;inta el la mara &scirc;a&ubreve;mo.
    </dif>
    <ref tip="vid" cel="venus.0o.mita">Venuso</ref>
  </snc>
  <trd lng="be">&c_A;&c_f;&c_r;&c_a;&c_d;&c_y;&c_t;&c_a;</trd>
  <trd lng="ca">Afrodita</trd>
  <trd lng="de">Aphrodite</trd>
  <trd lng="el">&Alfa;&phi;&rho;&omikron;&delta;&jota_acute;&tau;&eta;
  </trd>
  <trd lng="en">Aphrodite</trd>
  <trd lng="es">Afrodita</trd>
  <trd lng="it">Afrodite</trd>
  <trd lng="pl">Afrodyta</trd>
  <trd lng="pt">Afrodite</trd>
  <trd lng="fr">Aphrodite</trd>
  <trd lng="ru">&c_A;&c_f;&c_r;&c_o;&c_d;&c_i;&c_t;&c_a;</trd>
  <trd lng="sv">Afrodite</trd>
  <adm>
    Indas kontroli la grekan tradukon kaj aldoni la klasikan grekan
    . [JF]
  </adm>
</drv>
</art>

