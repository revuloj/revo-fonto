<art mrk="$Id: topaz.xml,v 1.21 2023/10/21 14:37:42 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>topaz</rad>/o
</kap>
<drv mrk="topaz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="topaz.0o.MIN">
    <dif>
      Flava diafana <ref tip="super" cel="sxton.juvel0o.MIN">juvel&scirc;tono</ref>:
      <ekz>
        la unua vico estis: rubeno, <tld/>o, kaj smeraldo
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 39:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        oraj ringoj, kadritaj per <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 5:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        en ni&ccirc;oj kaj sur &scirc;tonaj tabloj ku&scirc;is multekostaj &scirc;tonoj de &ccirc;iuj koloroj:
        rubenoj, <tld/>oj, smeraldoj, safiroj, diamantoj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/0/03/Topaze1.jpg/311px-Topaze1.jpg" prm="CC BY-SA 3.0"><tld/>o, Vermelh&atilde;o Minejo, Saramenha, Brazilo (3&#x202F;x&#x202F;1.5&#x202F;cm)
        <fnt>
          <aut>Archaeodontosaurus, propra verko</aut>
          <lok><url ref="&WCW;/File:Topaze1.jpg">&WCW;/File:Topaze1.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_p;&c_a;&c_z;</trd>
  <trd lng="ca">topazi</trd>
  <trdgrp lng="cs">
    <trd>topas</trd>,
    <trd>topaz</trd>
  </trdgrp>
  <trd lng="de">Topas</trd>
  <trd lng="en">topaz</trd>
  <trd lng="es">topacio</trd>
  <trdgrp lng="fa">
    <trd>&f_y;&a_A;&a_q;&a_w;&a_t; &a_z;&a_r;&a_d;</trd>,
    <trd>&a_z;&a_b;&a_r;&a_j;&a_d; &a_h;&a_n;&a_d;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">topaze</trd>
  <trd lng="hu">top&aacute;z</trd>
  <trdgrp lng="ja">
    <trd>&#x30C8;&#x30D1;&#x30FC;&#x30BA;</trd>,
    <trd>&#x9EC4;&#x7389;  <pr>&#x304A;&#x3046;&#x304E;&#x3087;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">topaas</trd>
  <trd lng="pl">topaz</trd>
  <trd lng="pt">top&aacute;zio</trd>
  <trd lng="ru">&c_t;&c_o;&c_p;&c_a;&c_z;</trd>
  <trd lng="sk">top&aacute;s</trd>
  <trd lng="uk">&#x442;&#x43E;&#x43F;&#x430;&#x437;</trd>
</drv>
