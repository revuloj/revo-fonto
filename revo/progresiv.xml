<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: progresiv.xml,v 1.10 2024/04/08 08:28:01 revo Exp $">
<kap>
  <rad>progresiv</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="progresiv.0a">
  <kap><tld/>a</kap>
  <snc mrk="progresiv.0a.KOMUNE">
    <uzo tip="stl">RAR</uzo>
    <dif>
     <ref tip="dif" cel="iom.0post0a.KOMUNE">Iompostioma</ref>:
      <ekz>
        <tld/>a muskol-malstre&ccirc;ado
        <fnt>
          <aut>[J. Meyer]</aut>,
          <vrk><url ref="http://www.learnlangs.com/sites/default/files/plibonigado           de
          vivo.doc">
          Plibonigado de vivo</url></vrk>,
          <lok>[vidita en 2014]</lok>
        </fnt>;
      </ekz>
      <ekz>
        la maturi&gcirc;o de la cerbo kaj de la nerva sistemo nur <tld/>e disvolvi&gcirc;as post la
        naski&gcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Lenio Marobin</aut>
          <vrk><url ref="&Monato;/2013/010927.php">Profitinda infana&gcirc;o</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_s;&c_t;&c_u;&c_p;&c_o;&c_v;&c_y;</trd>
    <trdgrp lng="de">
      <trd>progressiv</trd>,
      <trd>allm&auml;hlich</trd>
    </trdgrp>
    <trd lng="fr">insensible<klr> (progressif)</klr></trd>
    <trdgrp lng="zh">
    <trd>&#x7D2F;&#x8FDB; <pr>l&#x11B;ij&#xEC;n</pr></trd>,
    <trd>&#x7D2F;&#36914; <pr>l&#x11B;ij&#xEC;n</pr></trd>,
    <trd>&#20873;&#20873; <pr>r&#x1CE;nr&#x1CE;n</pr></trd>,
    <trd>&#x65E5;&#x6E10; <pr>r&#xEC;ji&#xE0;n</pr></trd>,
    <trd>&#x65E5;&#x6F38; <pr>r&#xEC;ji&#xE0;n</pr></trd>,
    <trd>&#x65E5;&#x76CA;&#x589E;&#x591A;&#22320; <pr>r&#xEC;y&#xEC;z&emacron;ngdu&#x14D;de</pr></trd>,
    <trd>&#x65E5;&#x8D8B; <pr>r&#xEC;q&#x16B;</pr></trd>,
    <trd>&#x65E5;&#x8DA8; <pr>r&#xEC;q&#x16B;</pr></trd>,
    <trd>&#x6E10; <pr>ji&#xE0;n</pr></trd>,
    <trd>&#x6F38; <pr>ji&#xE0;n</pr></trd>,
    <trd>&#x6E10;&#x6B21; <pr>ji&#xE0;nc&#xEC;</pr></trd>,
    <trd>&#x6F38;&#x6B21; <pr>ji&#xE0;nc&#xEC;</pr></trd>,
    <trd>&#36880;&#x6B21; <pr>zh&#xFA;c&#xEC;</pr></trd>,
    <trd>&#36880;&#x6B65; <pr>zh&#xFA;b&#xF9;</pr></trd>,
    <trd>&#36880;&#x6E10; <pr>zh&#xFA;ji&#xE0;n</pr></trd>,
    <trd>&#36880;&#x6F38; <pr>zh&#xFA;ji&#xE0;n</pr></trd>,
    <trd>&#x70B9;&#x70B9;&#x6EF4;&#x6EF4; <pr>di&#x1CE;ndi&#x1CE;nd&#x12B;d&#x12B;</pr></trd>,
    <trd>&#x9EDE;&#x9EDE;&#x6EF4;&#x6EF4; <pr>di&#x1CE;ndi&#x1CE;nd&#x12B;d&#x12B;</pr></trd>,
    <trd>&#38470;&#x7EED; <pr>l&#xF9;x&#xF9;</pr></trd>,
    <trd>&#38520;&#x7E8C; <pr>l&#xF9;x&#xF9;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="progresiv.0a.progresema">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="progre.0ema.POL">Progresema</ref>:
      <ekz>
        interreto <klr>[&#8230;]</klr> ebligas al la
        <tld/>a junularo komuniki pli rapide
        <fnt>
          <aut>&Ccirc;e&hcirc;a Esperanto-Junularo</aut>,
          <vrk><url
          ref="http://esperanto.blahus.cz/cxej/vikio/index.php/Jarraporto_de_%C4%88EJ/20050618">
          Jarraporto...</url></vrk>,
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malgranda <tld lit="P"/>a Demokrata Partio
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2008/009515.php">Nova &ccirc;efo, nova a&ubreve;roro</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <!-- sencon evitindan prefere ne traduku -->
  <trd lng="be">&c_p;&c_r;&c_a;&c_g;&c_r;&c_e;&c_s;&c_y;&c_w;&c_n;&c_y;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>postupuj&iacute;c&iacute;</trd>,
    <trd>progresivn&iacute;</trd>,
    <trd>vyv&iacute;jej&iacute;c&iacute; se</trd>
  </trdgrp>
  <trd lng="ja">&#x7D2F;&#36914;&#30340;&#x306A;  <pr>&#x308B;&#12356;&#12375;&#12435;&#12390;&#x304D;&#x306A;</pr></trd>
  <trd lng="sk">progres&iacute;vny</trd>
  <trd lng="uk">&#x43F;&c_r;&#x43E;&c_gu;&c_r;&c_je;&c_s;&c_i;&c_v;&#x43D;&c_i;&c_j;</trd>
</drv>
</art>

