<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kegl</rad>/o
</kap>
<drv mrk="kegl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;iu el la lignaj kolonetoj, kiujn oni penas vete renversi
      &jcirc;etante inter ili lignan globon. 
    </dif>
  </snc>
  <trd lng="de">Kegel</trd>
  <trdgrp lng="en">
    <trd>skittle</trd>,
    <trd>ninepin</trd>,
    <trd>bowling pin</trd>
  </trdgrp>
  <trd lng="nl">kegel</trd>
  <trd lng="ru">&c_k;&c_je;&c_g;&c_l;&c_ja;</trd>
</drv>
<drv mrk="kegl.0aro">
  <kap><tld/>aro</kap>
  <snc>
    <dif>
      Ludo per <tld/>oj.  
    </dif>
  </snc>
  <trd lng="de">Kegelspiel</trd>
  <trdgrp lng="en">
    <trd>skittles</trd>,
    <trd>ninepins</trd>
    <trd>bowling pin</trd>,
    </trdgrp>
  <trd lng="nl">kegelspel</trd>
  <trd lng="ru">&c_k;&c_je;&c_g;&c_l;&c_i; <klr>(&c_i;&c_g;&c_r;&c_a;)</klr></trd>
</drv>
</art>
