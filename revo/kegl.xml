<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kegl</rad>/o
</kap>
<drv mrk="kegl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      &Ccirc;iu el la lignaj kolonetoj, kiujn oni penas vete renversi
      &jcirc;etante inter ili lignan globon.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_je;&c_g;&c_l;&c_ja;</trd>
  <trd lng="de">Kegel</trd>
  <trdgrp lng="en">
    <trd>skittle</trd>,
    <trd>ninepin</trd>,
    <trd>bowling pin</trd>
  </trdgrp>
  <trd lng="fr">quille<klr tip="ind"> (pour jouer)</klr></trd>
  <trd lng="hu">tekeb&aacute;bu</trd>
  <trd lng="nl">kegel</trd>
  <trd lng="ru">&c_k;&c_je;&c_g;&c_l;&c_ja;</trd>
  <trd lng="sv">k&auml;gla</trd>
</drv>

<drv mrk="kegl.0aro">
  <kap><tld/>aro</kap>
  <snc>
    <dif>
      Tuto de la <tld/>oj kaj globoj, necesaj por ludi.
    </dif>
  </snc>
  <adm>
    La antauxa difino tekstis "ludo per kegloj", kio eble
    influis kelkajn tradukojn.
    <aut>MB</aut>
  </adm>
  <trd lng="be">&c_k;&c_je;&c_g;&c_l;&c_ib;
  <klr>(&c_g;&c_u;&c_l;&c_mol;&c_n;&c_ja;)</klr></trd>
  <trd lng="de">Kegelspiel</trd>
  <trdgrp lng="en">
    <trd>skittles</trd>,
    <trd>ninepins</trd>
    <trd>bowling pin</trd>,
  </trdgrp>
  <trd lng="fr">jeu de quilles</trd>
  <trd lng="hu">tekej&aacute;t&eacute;k</trd>
  <trd lng="nl">kegelspel</trd>
  <trd lng="ru">&c_k;&c_je;&c_g;&c_l;&c_i;
  <klr>(&c_i;&c_g;&c_r;&c_a;)</klr></trd>
</drv>
</art>
