<art mrk="$Id: sistol.xml,v 1.20 2024/03/24 20:21:58 revo Exp $">
<kap>
  <rad>sistol</rad>/o
</kap>
<drv mrk="sistol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Kuntiri&gcirc;a movo de la koro:
      <ekz>
        20 malsanaj viroj ludis golfon, kaj dum ilia ludo frekvenco de
        la
        <tld/>oj &ccirc;e ili estis sama kiel &ccirc;e aliaj tute sanaj
        personoj<fnt><aut>Stanis&lstroke;aw &Sacute;migielski</aut>: <vrk>Vivi por
        golfludi?</vrk>, <bib>Monato</bib><lok>jaro 2002a, numero 2a, p.
        22a</lok></fnt>;
      </ekz>
      <ekz>
        la refleksaj simptomoj cirkulaj &mdash; palpitacio, ta&hcirc;ikardio,
        <ind>ekstra<tld/>o</ind> <klr>(tro frua, eksterritma <tld/>o)</klr>
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Uterito">
          Uterito</url>, 2008-07-09</lok>
        </fnt>.
        <trd lng="de">Extrasystole</trd>
        <trdgrp lng="en">
          <trd>premature heart beat</trd>,
          <trd>extrasystole</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <ref tip="ant" cel="diasto.0o">diastolo</ref>
  <trd lng="be">&c_s;&c_y;&c_s;&c_t;&c_a;&c_l;&c_a;</trd>
  <trd lng="cs">srde&ccaron;n&iacute; systola</trd>
  <trd lng="de">Systole</trd>
  <trd lng="en">systole</trd>
  <trdgrp lng="fr">
    <trd>systole</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&iacute;v&ouml;sszeh&uacute;z&oacute;d&aacute;s</trd>,
    <trd>szisztol&eacute;</trd>
  </trdgrp>
  <trd lng="ja">&#x5FC3;&#x53CE;&#x7E2E;&#x671F;  <pr>&#x3053;&#x3053;&#x308D;&#x3057;&#x3085;&#x3046;&#x3057;&#x3085;&#x304F;&#x304D;</pr></trd>
  <trd lng="nl">systole</trd>
  <trdgrp lng="pl">
    <trd>skurcz  <ind>serca</ind></trd>,
    <trd>systola</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_i;&c_s;&c_t;&c_o;&c_l;&c_a;</trd>,
    <trd>&c_s;&c_o;&c_k;&c_r;&c_a;&c_shch;&c_je;&c_n;&c_i;&c_je; &c_s;&c_je;&c_r;&c_d;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>stiahnutie srdca</trd>,
    <trd>systola</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&c_s;&c_t;&c_o;&c_l;&c_a;</trd>,
    <trd>&c_s;&c_k;&c_o;&c_r;&c_o;&c_ch;&c_je;&c_n;&c_n;&c_ja; &c_s;&c_je;&c_r;&c_c;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5FC3;&#x810F;&#x6536;&#x7F29; <pr>x&#x12B;nz&#xE0;ngsh&#x14D;usu&#x14D;</pr></trd>,
    <trd>&#x5FC3;&#x81DF;&#x6536;&#x7E2E; <pr>x&#x12B;nz&#xE0;ngsh&#x14D;usu&#x14D;</pr></trd>
  </trdgrp>
</drv>
</art>
