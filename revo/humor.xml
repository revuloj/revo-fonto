<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>humor</rad>/o
</kap>
<drv mrk="humor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1" mrk="humor.0o.animstato">
    <dif>
      Kutima a&ubreve; okaza stato de la temperamento a&ubreve; de la spirito;
      animstato:
      <ekz>
        melankolia, milda, akra, acida<fnt>Z</fnt>,
      </ekz>
      <ekz>
        trankvila, gaja <tld/>o;
      </ekz>
      <ekz>
        krei&gcirc;as iom post iom internacia <tld/>o malfavora al la mitito;
      </ekz>
      <ekz>
        bon<tld/>ulo;
      </ekz>
      <ekz>
        nun li ne &scirc;erceme <tld/>as;
      </ekz>
      <ekz>
        malbon<tld/>igi iun;
      </ekz>
      <ekz>
        kontra&ubreve; doloro helpas bona <tld/>o
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Laune</trd>,
      <trd>Stimmung</trd>
    </trdgrp>
    <trd lng="nl">humeur</trd>
    <trd lng="ru">&c_n;&c_a;&c_s;&c_t;&c_r;&c_o;&c_je;&c_n;&c_i;&c_je;</trd>
    <trd lng="sv">hum&ouml;r</trd>
  </snc>
  <snc num="2" mrk="humor.0o.sxerco">
    <dif>
      Originala kaj milda gajemeco, speco de &scirc;erco, precipe &ccirc;e
      anglaj a&ubreve;toroj, la&ubreve; kiu verkisto elmetas kun seriozeco kaj
      scienca &scirc;ajno satirajn burleska&jcirc;ojn:
      <ekz>
        <tld/>a parolo, rakonto;
      </ekz>
      <ekz>
        <tld/>a&jcirc;o;
      </ekz>
      <ekz>
        <tld/>isto;
      </ekz>
      <ekz>
        <tld/>plena libro.
      </ekz>
    </dif>
    <ref tip="sin" cel="humur.0o">humuro</ref>
    <trd lng="de">Humor</trd>
    <trd lng="nl">humor</trd>
    <trd lng="ru">&c_ju;&c_m;&c_o;&c_r;</trd>
    <trd lng="sv">humor</trd>
  </snc>
  <trd lng="cs">n&aacute;lada</trd>
</drv>
</art>

