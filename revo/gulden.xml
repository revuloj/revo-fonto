<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>gulden</rad>/o <fnt><bib>PV</bib></fnt> 
</kap>
<drv mrk="gulden.0o">
  <kap><tld/>o</kap>
  <snc mrk="gulden.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Malnova <ref tip="lst" cel="unu.mon0o"
      lst="voko:monunuoj">monunuo</ref>,
      interalie de Nederlando:
      <ekz>
        ni diris tri <tld/>ojn, mi certas pri tio
        <fnt>
          <aut>trad. C.M.C.</aut>,
          <vrk><url ref="http://www.senlime.be/Kino/Perle.htm">
          La junulino kun la perlo</url></vrk>,
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="cend.0o">cendo</ref>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_l;&c_mol;&c_d;&c_e;&c_n;</trd>
  <trd lng="ca">flor&iacute;</trd>
  <trd lng="de">Gulden</trd>
  <trd lng="en">gulden</trd>
  <trd lng="fr">florin<klr tip="ind"> (n&eacute;erlandais)</klr></trd>
  <trdgrp lng="hu">
    <trd>gulden</trd>,
    <trd>holland forint</trd>
  </trdgrp>
  <trd lng="nl">gulden</trd>
  <trd lng="pl">gulden</trd>
  <trd lng="pt">florim</trd>
  <trd lng="ru">&c_g;&c_u;&c_l;&c_mol;&c_d;&c_je;&c_n;</trd>
</drv>
</art>
