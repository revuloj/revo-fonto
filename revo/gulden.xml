  <trd lng="id">gulden</trd>
  <trd lng="ja">&#x30AE;&#x30EB;&#x30C0;&#x30FC;</trd>
  <trd lng="nl">gulden</trd>
  <trd lng="pl">gulden</trd>
  <trd lng="pt">florim</trd>
  <trd lng="ru">&c_g;&c_u;&c_l;&c_mol;&c_d;&c_je;&c_n;</trd>
  <trd lng="sk">zlatka</trd>
  <trd lng="uk">&#x491;&#x443;&#x43B;&#x44C;&#x434;&#x435;&#x43D;&#xA0;(&#x433;&#x440;&#x43E;&#x448;&#x43E;&#x432;&#x430; &#x43E;&#x434;&#x438;&#x43D;&#x438;&#x446;&#x44F;)</trd>
