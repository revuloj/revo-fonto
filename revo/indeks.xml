<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>indeks</rad>/o
</kap>
<drv mrk="indeks.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Indika tabelo, ordigita la&ubreve; alfabeta ordo:
      <ekz><tld/>o de propraj nomoj;</ekz>
      <ekz>historia <tld/>o.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konkor1.0o">konkordanco</ref>,
      <ref cel="repert.0o">repertuaro</ref>,
      <ref cel="list.0o">listo</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_ib;&c_n;&c_d;&c_e;&c_k;&c_s;</trd>,
      <trd>&c_u;&c_k;&c_a;&c_z;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
    </trdgrp>
    <trd lng="de">Index</trd>
    <trdgrp lng="fr">
      <trd>index<klr tip="ind"> (liste alphab&eacute;tique)</klr></trd>,
      <trd>r&eacute;pertoire</trd></trdgrp> 
    <trdgrp lng="ru">
      <trd>&c_i;&c_n;&c_d;&c_je;&c_k;&c_s;</trd>,
      <trd>&c_u;&c_k;&c_a;&c_z;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
      <trd>&c_s;&c_p;&c_r;&c_a;&c_v;&c_o;&c_ch;&c_n;&c_a;&c_ja; &c_t;&c_a;&c_b;&c_l;&c_i;&c_c;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <uzo tip="fak">KAT</uzo>
    <dif>
      Listo de la libroj, kies legadon malpermesas la katolika eklezio.  
    </dif>
    <trd lng="be">&c_ib;&c_n;&c_d;&c_e;&c_k;&c_s;</trd>
    <trd lng="de">Index</trd>
    <trd lng="fr">index<klr tip="ind"> (cath.)</klr></trd>
  </snc>
  <trd lng="es">&iacute;ndice</trd>
  <trd lng="hu">index</trd>
  <trd lng="nl">index</trd>
</drv>
</art>
