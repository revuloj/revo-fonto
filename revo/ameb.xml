<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>9</ofc>
  <rad>ameb</rad>/o
  <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ameb.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="super" cel="protoz.0o">Protozoo</ref>, sin movanta per
      <ref tip="vid" cel="pied.sxajn0o">&scirc;ajnpiedoj</ref>,
      vivanta en akvo, malseka tero a&ubreve; parazite en bestoj a&ubreve; homoj,
      ofte &scirc;an&gcirc;anta sian formon.
      <!-- Malgranda protozoo simila al gelatineca ero, kiu vivas en
      akvoj maraj
      a&ubreve; riveraj, kapabla movi&gcirc;i kaj &scirc;an&gcirc;i sian
      formon.
      -->
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_jo;&c_b;&c_a;</trd>
  <trd lng="de">Am&ouml;be</trd>
  <trd lng="en">amoeba</trd>
  <trd lng="es">amiba</trd>
  <trd lng="fr">amibe</trd>
  <trdgrp lng="he">
    <trd>&alef;&mem;&bet;&he;</trd>,
    <trd>&het;&yod;&lamed;&vav;&pe;&yod;&tav;</trd>
  </trdgrp>
  <trd lng="hu">am&odblac;ba</trd>
  <trd lng="nl">amoebe</trd>
  <trd lng="pl">ameba</trd>
  <trd lng="ru">&c_a;&c_m;&c_jo;&c_b;&c_a;</trd>
  <trd lng="sk">am&eacute;ba</trd>
</drv>
</art>
