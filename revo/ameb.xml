<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ameb.xml,v 1.17 2018/08/19 13:10:14 revo Exp $">
<kap>
  <ofc>9</ofc>
  <rad>ameb</rad>/o
  <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ameb.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="super" cel="protoz.0o">Protozoo</ref>, sin movanta per
      <ref tip="vid" cel="pied.sxajn0o">&scirc;ajnpiedoj</ref>,
      vivanta en akvo, malseka tero a&ubreve; parazite en bestoj a&ubreve; homoj,
      ofte &scirc;an&gcirc;anta sian formon:
      <ekz>
        &ccirc;iuj &ccirc;elhavaj organismoj (bakterioj, <tld/>oj, fungoj, gistoj, 
        plantoj kaj animaloj) uzas DNA-on por konservi la genetikan informon
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007157.html">Vivo: Riboj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        disenterio <klr>[&#8230;]</klr> povas esti ka&ubreve;zata de <tld/>oj a&ubreve; de baciloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=241093">Disenterio</url></lok>
        </fnt>.
      </ekz>   
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_jo;&c_b;&c_a;</trd>
  <trd lng="de">Am&ouml;be</trd>
  <trd lng="en">amoeba</trd>
  <trd lng="es">amiba</trd>
  <trd lng="fr">amibe</trd>
  <trdgrp lng="he">
    <trd>&alef;&mem;&bet;&he;</trd>,
    <trd>&het;&yod;&lamed;&vav;&pe;&yod;&tav;</trd>
  </trdgrp>
  <trd lng="hu">am&odblac;ba</trd>
  <trd lng="nl">amoebe</trd>
  <trd lng="pl">ameba</trd>
  <trd lng="ru">&c_a;&c_m;&c_jo;&c_b;&c_a;</trd>
  <trd lng="sk">am&eacute;ba</trd>
</drv>
</art>
