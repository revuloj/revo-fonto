<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>rokok</rad>/o
</kap>
<drv mrk="rokok.0o">
  <kap><tld/>o</kap>
  <snc mrk="rokok.0o.BELA">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">BELA</uzo>
    <dif>
      Arta stilo franc-devena, prospera en okcidenta, centra kaj suda
      E&ubreve;ropo inter 1700 kaj 1750, ido de <ref tip="vid"
      cel="barok.0o">baroko</ref>, produktinta delikatajn verkojn kun
      kapricaj volutoj kaj densaj ornama&jcirc;oj &scirc;atataj en tiamaj kortegoj kaj
      nobelaj rondoj:
      <ekz>
        la ekipa&jcirc;aro de la pre&gcirc;ejo devenas el la periodo de baroko kaj 
        <tld/>o
        <fnt>
          <aut>-</aut>,
          <vrk>elsendo</vrk>,
          <lok><url ref="http://www.polskieradio.pl/eo/">
          Pola Radio</url>, 2007-07-07</lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      Kelkaj artfakuloj distingas anka&ubreve; en beletro kaj muziko <tld/>ajn
      verkojn.
      <aut>&jcirc;v</aut>
    </rim>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_k;&c_a;&c_k;&c_o;</trd>
  <trd lng="ca">rococ&oacute;</trd>
  <trd lng="de">Rokoko</trd>
  <trd lng="en">rococo</trd>
  <trd lng="es">rococ&oacute;</trd>
  <trdgrp lng="fr">
    <trd>style <ind>rocaille</ind></trd>,
    <trd>style <ind>rococo</ind></trd>
  </trdgrp>
  <trd lng="hu">rokok&oacute;</trd>
  <trd lng="nl">rococo</trd>
  <trd lng="pt">rococ&oacute;</trd>
  <trd lng="ru">&c_r;&c_o;&c_k;&c_o;&c_k;&c_o;</trd>
</drv>

<drv mrk="rokok.0a">
  <kap><tld/>a</kap>
  <snc mrk="rokok.0a.BELA">
    <dif>
      Konforma al <ref tip="vid" cel="rokok.0o">rokoko</ref>.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>rococo<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>rokok&oacute;</trd>,
    <trd>rokok&oacute; st&iacute;lus&uacute;</trd>
  </trdgrp>
</drv>
</art>
