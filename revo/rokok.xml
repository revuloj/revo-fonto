<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rokok.xml,v 1.28 2024/04/14 05:47:06 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>rokok</rad>/o
</kap>
<drv mrk="rokok.0o">
  <kap><tld/>o</kap>
  <snc mrk="rokok.0o.BELA">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">BELA</uzo>
    <dif>
      Arta stilo franc-devena, prospera en okcidenta, centra kaj suda
      E&ubreve;ropo inter 1700 kaj 1750, ido de <ref tip="vid" cel="barok.0o">baroko</ref>, produktinta
      delikatajn verkojn kun
      kapricaj volutoj kaj densaj ornama&jcirc;oj &scirc;atataj en tiamaj kortegoj
      kaj
      nobelaj rondoj:
      <ekz>
        la ekipa&jcirc;aro de la pre&gcirc;ejo devenas el la periodo de baroko kaj

        <tld/>o
        <fnt>
          <aut>-</aut>,
          <vrk>elsendo</vrk>,
          <lok><url ref="http://www.polskieradio.pl/eo/">
          Pola Radio</url>, 2007-07-07</lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      Kelkaj artfakuloj distingas anka&ubreve; en beletro kaj muziko <tld/>ajn
      verkojn.
      <aut>&jcirc;v</aut>
    </rim>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_k;&c_a;&c_k;&c_o;</trd>
  <trd lng="bg">&c_r;&c_o;&c_k;&c_o;&c_k;&c_o;</trd>
  <trd lng="ca">rococ&oacute;</trd>
  <trd lng="cs">rokoko</trd>
  <trd lng="de">Rokoko</trd>
  <trd lng="en">rococo</trd>
  <trd lng="es">rococ&oacute;</trd>
  <trdgrp lng="fr">
    <trd>style <ind>rocaille</ind></trd>,
    <trd>style <ind>rococo</ind></trd>
  </trdgrp>
  <trd lng="hu">rokok&oacute;</trd>
  <trd lng="ja">&#x30ED;&#x30B3;&#x30B3;&#x5F0F;  <pr>&#x30ED;&#x30B3;&#x30B3;&#x3057;&#x304D;</pr></trd>
  <trd lng="nl">rococo</trd>
  <trd lng="pl">rokoko</trd>
  <trd lng="pt">rococ&oacute;</trd>
  <trd lng="ro">rococo</trd>
  <trd lng="ru">&c_r;&c_o;&c_k;&c_o;&c_k;&c_o;</trd>
  <trd lng="sk">rokoko</trd>
  <trd lng="uk">&c_r;&#x43E;&#x43A;&#x43E;&#x43A;&#x43E;</trd>
  <trd lng="zh">&#x6D1B;&#x53EF;&#x53EF; <pr>lu&#xF2;k&#x11B;k&#x11B;</pr></trd>
</drv>

<drv mrk="rokok.0a">
  <kap><tld/>a</kap>
  <snc mrk="rokok.0a.BELA">
    <dif>
      Konforma al <ref tip="vid" cel="rokok.0o">rokoko</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_u; &c_s;&c_t;&c_y;&c_l;&c_ju; <ind>&c_r;&c_a;&c_k;&c_a;&c_k;&c_o;</ind></trd>
  <trd lng="en">rococo</trd>
  <trdgrp lng="fr">
    <trd>rococo<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>rokok&oacute;</trd>,
    <trd>rokok&oacute; st&iacute;lus&uacute;</trd>
  </trdgrp>
  <trd lng="nl">rococo-</trd>
  <trd lng="pl">rokokowy</trd>
  <trd lng="uk">&c_u; &c_s;&c_t;&c_i;&#x43B;&c_ib; &c_r;&#x43E;&#x43A;&#x43E;&#x43A;&#x43E;</trd>
</drv>
</art>
