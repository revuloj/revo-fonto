<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>8</ofc>
<rad>injekt</rad>/i
<fnt>Z</fnt>
</kap>
<drv mrk="injekt.0i">
  <kap><tld/>i</kap>
  <snc>
    <gra><vspec>tr</vspec></gra>
    <uzo tip="fak">MED</uzo>
    <dif>
      En&scirc;prucigi per speciala ilo likva&jcirc;on sub la ha&ubreve;ton
      a&ubreve; en kavon de la korpo:
      <ekz>
        <tld/>i ion al iu;
      </ekz>
      <ekz>
        <tld/>i morfinon al malsanulo.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="fak">TEK</uzo>
    <dif>
      En&scirc;prucigi likva&jcirc;on.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd><klr tip="ind">(jmd. etw.)</klr> spritzen</trd>,
    <trd>injizieren</trd>
  </trdgrp>
</drv>
<drv mrk="injekt.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago, procedo <tld/>i a&ubreve; la rezulto de tiu ago.
    </dif>
  </snc>
  <trd lng="de">Injektion</trd>
</drv>
<drv mrk="injekt.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      <tld/>ata likva&jcirc;o.
    </dif>
  </snc>
</drv>
<drv mrk="injekt.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <uzo tip="fak">MED</uzo>
    <dif>
      Instrumento  kun akrepinta, tre maldika fintubo, por <tld/>i.
    </dif>
  </snc>
  <snc>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Aparato por <tld/>i:
      <ekz>
        <tld/>opumpiloj;
      </ekz>
      <ekz>
        peraera, perakva, pervapora <tld/>ilo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Spritze</trd>
</drv>
</art>
