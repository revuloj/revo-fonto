<art mrk="$Id$">
<kap><rad>bilion</rad>/o
<fnt>Z</fnt></kap>
<drv mrk="bilion.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      miliono da milionoj, mil miliardoj (= duiliono), 1 000 000 000 000.
    </dif>
    <trd lng="de">Billion</trd>
    <trdgrp lng="en">
      <trd>trillion<klr> [US]</klr></trd>,
      <trd>billion<klr> [GB]</klr></trd>
    </trdgrp>
    <trd lng="es">bill&oacute;n</trd>
    <trd lng="hu">billi&oacute;</trd>
    <trd lng="nl">biljoen</trd>
    <trd lng="sk">bili&oacute;n</trd>
    <trd lng="vo">telion</trd>
  </snc>
  <snc>
    <uzo tip="stl">EVI</uzo>
    <uzo tip="klr">(la&ubreve; Usona kaj Rusa sistemo)</uzo>
    <dif>
      miliardo, 1 000 000 000.
    </dif>
  </snc>
</drv>
</art>
