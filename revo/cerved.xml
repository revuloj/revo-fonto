<kap>
  <rad>cerved</rad>/oj
</kap>
<drv mrk="cerved.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt" cel="huf.par0uloj">parhufuloj</ref>
      (<trd lng="la">Cervidae</trd>):
      <ekz>
        el la <tld/>oj nur la boacoj amba&ubreve;sekse portas kornaron
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=6790">Boaco</url></lok>
        </fnt>;
      </ekz>      
      <ekz>
        la parhufulajn bovedojn karakterizas unubran&ccirc;a kavkorno, kiu &#8208; malkiel &ccirc;e
        cervedoj <klr>[&#8230;]</klr> &#8208; neniam defalas
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16110">Bovedoj</url></lok>
        </fnt>;
      </ekz>      
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_je;&c_n;&c_ib; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">c&egrave;rvids</trd>
  <trd lng="de">Hirsche</trd>
  <trd lng="el">&epsilon;&lambda;&alfa;&phi;&omikron;&epsilon;&jota;&delta;&eta_ton;</trd>
  <trd lng="es">c&eacute;rvidos</trd>
  <trd lng="fr">cervid&eacute;s</trd>
  <trd lng="hu">szarvasf&eacute;l&eacute;k</trd>
  <trd lng="ja">&#12471;&#12459;&#31185;  <pr>&#12471;&#12459;&#12363;</pr></trd>
  <trd lng="pl">jeleniowate</trd>
  <trd lng="pt">cerv&iacute;deos</trd>
  <trd lng="ru">&c_o;&c_l;&c_je;&c_n;&c_i; <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;
  &c_p;&c_a;&c_r;&c_n;&c_o;&c_k;&c_o;&c_p;&c_y;&c_t;&c_n;&c_y;&c_h;)</klr></trd>
  <trd lng="uk">&c_o;&c_l;&c_je;&c_n;&c_je;&c_v;&c_ib;</trd>
  <trd lng="zh">&#40575;&#31185; <pr>l&ugrave;k&#275;</pr></trd>
</drv>
