<art mrk="$Id: cerved.xml,v 1.14 2023/10/31 13:37:03 revo Exp $">
<kap>
  <rad>cerved</rad>/oj
</kap>
<drv mrk="cerved.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref tip="malprt" cel="huf.par0uloj">parhufuloj</ref>
      (<trd lng="la">Cervidae</trd>):
      <ekz>
        el la <tld/>oj nur la boacoj amba&ubreve;sekse portas kornaron
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=6790">Boaco</url></lok>
        </fnt>;
      </ekz>      
      <ekz>
        la parhufulajn bovedojn karakterizas unubran&ccirc;a kavkorno, kiu &dash; malkiel &ccirc;e
        cervedoj <klr>[&#x2026;]</klr> &dash; neniam defalas
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16110">Bovedoj</url></lok>
        </fnt>;
      </ekz>      
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_je;&c_n;&c_ib; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Hirsche</trd>
  <trd lng="el">&epsilon;&lambda;&alfa;&phi;&omikron;&epsilon;&jota;&delta;&eta_ton;</trd>
  <trd lng="es">c&eacute;rvidos</trd>
  <trd lng="fr">cervid&eacute;s</trd>
  <trd lng="hu">szarvasf&eacute;l&eacute;k</trd>
  <trd lng="ja">&#x30B7;&#x30AB;&#x79D1;  <pr>&#x30B7;&#x30AB;&#x304B;</pr></trd>
  <trd lng="pl">jeleniowate</trd>
  <trd lng="pt">cerv&iacute;deos</trd>
  <trd lng="ru">&c_o;&c_l;&c_je;&c_n;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;
  &c_p;&c_a;&c_r;&c_n;&c_o;&c_k;&c_o;&c_p;&c_y;&c_t;&c_n;&c_y;&c_h;)</klr></trd>
  <trd lng="uk">&#x43E;&#x43B;&#x435;&#x43D;&#x435;&#x432;&#x456;</trd>
  <trd lng="zh">&#x9E7F;&#x79D1; <pr>l&#xF9;k&#x113;</pr></trd>
</drv>
</art>

