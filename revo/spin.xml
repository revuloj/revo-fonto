<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>spin</rad>/o
</kap>
<drv mrk="spin.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc num="1">
    <dif>
      Tuto de la sinsekvaj <ref tip="prt" cel="verteb.0o">vertebroj</ref>
      formantaj la dorsan parton de
      la <ref tip="malprt" cel="skelet.0o.ANA">skeleto</ref>
      &ccirc;e la vertebruloj = vertebraro, vertebra kolono. 
    </dif>
    <trd lng="de">R&uuml;ckgrat</trd>
    <trdgrp lng="en">
      <trd>spine</trd>,
      <trd>spinal column</trd>,
      <trd>backbone</trd>
    </trdgrp>
    <trd lng="nl">ruggegraat</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_z;&c_v;&c_o;&c_n;&c_o;&c_ch;&c_n;&c_i;&c_k;</trd>,
      <trd>&c_h;&c_r;&c_je;&c_b;&c_je;&c_t;</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Precipa akso, plej grava linio de io:
      <ekz>alta mont<tld/>o (suprolinio, vertolinio).</ekz>
    </dif>
    <trd lng="ru">&c_h;&c_r;&c_je;&c_b;&c_je;&c_t;</trd>
  </snc>
</drv>
</art>
