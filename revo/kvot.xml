<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kvot</rad>/o
</kap>
<drv mrk="kvot.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Proporcia parto de tuto, kiun oni devas doni, pagi a&ubreve; kiun oni
      rajtas preni, ricevi.
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_v;&c_o;&c_t;&c_a;</trd>,
      <trd>&c_d;&c_o;&c_l;&c_ja;</trd>,
      <trd>&c_p;&c_a;&c_j;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Quote</trd>,
      <trd>Anteil</trd>,
      <trd>Kontingent</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>quota</trd>,
      <trd>share</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>h&aacute;nyad</trd>,
      <trd>kv&oacute;ta</trd>,
      <trd>r&eacute;szesed&eacute;s</trd>
    </trdgrp>
    <trd lng="nl">kwotum</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_v;&c_o;&c_t;&c_a;</trd>,
      <trd>&c_d;&c_o;&c_l;&c_ja;</trd>,
      <trd>&c_p;&c_a;&c_j;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="kvot.0igo">
  <kap>
    <tld/>igo
  </kap>
  <snc>
    <dif>
      Proporcia alju&gcirc;o en a&ccirc;eto de akcioj, valutoj, valorpaperoj
      ktp en la borso, kiu influas la fiksadon de la taga kurzo.
    </dif>
    <refgrp tip="sin">
      <ref cel="kurz.0o">kurzo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_y;&c_r;&c_o;&c_w;&c_k;&c_a;</trd>
  <trd lng="nl">toewijzing</trd>
  <trd lng="ru">&c_k;&c_o;&c_t;&c_i;&c_r;&c_o;&c_v;&c_k;&c_a;</trd>
</drv>

<drv mrk="kvot.profit0o">
  <kap>
    profit<tld/>o
  </kap>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Parto de farita profito.
    </dif>
    <trd lng="be">&c_d;&c_o;&c_l;&c_ja; <ind>&c_p;&c_r;&c_y;&c_b;&c_y;&c_t;&c_k;&c_u;</ind></trd>
    <trd lng="de">Gewinnanteil</trd>
    <trdgrp lng="hu">
      <trd>haszonr&eacute;szesed&eacute;s</trd>,
      <trd>nyeres&eacute;gr&eacute;szesed&eacute;s</trd>
    </trdgrp>
    <trd lng="ru">&c_d;&c_o;&c_l;&c_ja; &c_p;&c_r;&c_i;&c_b;&c_y;&c_l;&c_i;</trd>
  </snc>
  <snc>
    <uzo tip="fak">MAH</uzo>
    <dif>
      Rilato de la plusvaloro al la tuta kapitalo.
    </dif>
    <trd lng="be">&c_n;&c_o;&c_r;&c_m;&c_a; <ind>&c_p;&c_r;&c_y;&c_b;&c_y;&c_t;&c_k;&c_u;</ind></trd>
    <trd lng="de">Rendite</trd>
    <trd lng="hu">profitr&aacute;ta</trd>
    <trd lng="ru">&c_n;&c_o;&c_r;&c_m;&c_a; &c_p;&c_r;&c_i;&c_b;&c_y;&c_l;&c_i;</trd>
  </snc>
</drv>

<drv mrk="kvot.0odeplusvaloro">
  <kap>
    <tld/>o de plusvaloro
  </kap>
  <uzo tip="fak">MAH</uzo>
  <snc>
    <dif>
      Ekspluatgrado t. e. rilato de la plusvaloro al la salajrosumo
      (variebla kapitalo).
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_o;&c_r;&c_m;&c_a; &c_d;&c_a;&c_d;&c_a;&c_n;&c_a;&c_j; <ind>&c_v;&c_a;&c_r;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_ib;</ind></trd>
  <trd lng="hu">&eacute;rt&eacute;kt&ouml;bbletr&aacute;ta</trd>
  <trd lng="ru">&c_n;&c_o;&c_r;&c_m;&c_a; &c_p;&c_r;&c_i;&c_b;&c_a;&c_v;&c_o;&c_ch;&c_n;&c_o;&c_j; &c_s;&c_t;&c_o;&c_i;&c_m;&c_o;&c_s;&c_t;&c_i;</trd>
</drv>

<drv mrk="kvot.0odeinterezoj">
  <kap>
    <tld/>o de interezoj
  </kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">MAH</uzo>
  <snc>
    <dif>
      Jara proporcio inter la interezoj kaj la &gcirc;in donanta kapitalo.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_o;&c_r;&c_m;&c_a; <ind>&c_p;&c_r;&c_y;&c_b;&c_y;&c_t;&c_k;&c_u;</ind></trd>
  <trd lng="de">Zinssatz</trd>
  <trd lng="ru">&c_n;&c_o;&c_r;&c_m;&c_a; &c_p;&c_r;&c_i;&c_b;&c_y;&c_l;&c_i;</trd>
</drv>
</art>
