<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>androlog</rad>/o
</kap>
<drv mrk="androl.0o">
  <kap><tld/>o</kap>
  <snc mrk="androl.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="kurac.0isto">Kuracisto</ref>, kies fako
      estas <tld/>io.
    </dif>
    <rim>
      <ekz>Andrologo</ekz> kaj <ekz><tld/>iisto</ekz> (kiu ne trovi&gcirc;as
      en PIV) estas sinonimoj. Sed, pro la longeco de tiu lasta vorto,
      kaj pro la malagrabla duobla &rightquot;i&#8221;, <ekz><tld/>o</ekz> normale estas
      preferinda.
      <fnt>
        <aut>PMEG</aut>,
        <vrk><url
        ref="http://bertilow.com/pmeg/vortfarado/neoficialaj_afiksoj/sufiksoj/i.html">39.1.12.
        Neoficiala sufikso I</url></vrk>
      </fnt>.
    </rim>
  </snc>
  <trdgrp lng="de">
    <trd>Androloge</trd>,
    <trd>M&auml;nnerarzt</trd>
  </trdgrp>
  <trd lng="en">andrologist</trd>
  <trd lng="fi">andrologi</trd>
  <trd lng="fr">andrologue</trd>
  <trd lng="io">andrologo</trd>
  <trdgrp lng="it">
    <trd>andrologo</trd>,
    <trd>androloga</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>androloog</trd>,
    <trd>mannenarts</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>andrologista</trd>,
    <trd>andr&oacute;logo</trd>
  </trdgrp>
  <trd lng="vo">manasanan</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>