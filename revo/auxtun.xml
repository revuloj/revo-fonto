<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>a&ubreve;tun</rad>/o
</kap>
<drv mrk="auxtun.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="auxtun.0o.MET">
    <uzo tip="fak">KAL</uzo>
    <uzo tip="fak">MET</uzo>
    <dif>
      <ref tip="super" cel="sezon.0o">Sezono</ref> de malvarmi&gcirc;o inter
      somero kaj vintro (en la norda hemisfero de proksimume la 23a de
      septembro &gcirc;is la 21a de decembro; en la suda de la 21a de marto &gcirc;is
      la 21a de junio):
      <ekz>
        ankora&ubreve; estis agrable varme, sed jam senteblis la friska spiro de
        <tld/>o<fnt><aut>Julian Modest</aut>: <vrk>La ringo</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 3a, p. 32a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="he">&samekh;&tav;&yod;&vav;</trd>
    <trd lng="tr">sonbahar</trd>
    <trd lng="vo">flukatim</trd>
  </snc>
  <snc mrk="auxtun.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ekkaduki&gcirc;o.
    </dif>
    <refgrp tip="vid">
      <ref cel="klin.0igxi">klini&gcirc;i</ref>,
      <ref cel="krepus.0o">krepusko</ref>,
      <ref cel="velk.0i">velki</ref>,
      <ref cel="vesper.0o">vespero</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_o;&c_s;&c_je;&c_n;&c_mol;</trd>
  <trd lng="br">diskar-amzer</trd>
  <trd lng="cs">podzim</trd>
  <trdgrp lng="de">
    <trd>Herbst</trd>,
    <trd>Sp&auml;tjahr [sudgermane]</trd>
  </trdgrp>
  <trd lng="ca">tardor</trd>
  <trd lng="en">autumn</trd>
  <trd lng="es">oto&ntilde;o</trd>
  <trd lng="fr">automne</trd>
  <trd lng="hu">&odblac;sz</trd>
  <trd lng="nl">herfst</trd>
  <trd lng="pl">jesie&nacute;</trd>
  <trd lng="ru">&c_o;&c_s;&c_je;&c_n;&c_mol;</trd>
  <trd lng="sk">jese&ncaron;</trd>
  <trd lng="sv">h&ouml;st</trd>
  <trd lng="pt">outono</trd>
</drv>

<drv mrk="auxtun.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Okazanta en <tld/>o, simila al la etoso de <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>herbstlich</trd>,
    <trd>Herbst-</trd>,
    <trd>im Herbst</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>automnal</trd>
  </trdgrp>
  <trd lng="nl">herfst-</trd>
  <trd lng="vo">flukatimik</trd>
  <trd lng="ru">&c_o;&c_s;&c_je;&c_n;&c_n;&c_i;&c_j;</trd>
  <trd lng="pt">outonal</trd>
</drv>
</art>
