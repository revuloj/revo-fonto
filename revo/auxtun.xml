<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>a&ubreve;tun</rad>/o
</kap>
<drv mrk="auxtun.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
   <dif>
    Sezono inter somero kaj vintro (en la norda hemisfero de proks. la 23a de sep. &gcirc; la 21a de dec; en la suda de la 21a de mar. &gcirc; la 21a de jun.).
   </dif>
  <trd lng="tr">sonbahar</trd>
  </snc>
  <snc num="2">
   <uzo tip="stl">FIG</uzo>
   <dif>
    Ekkaduki&gcirc;o.
   </dif>
   <refgrp tip="sin">
    <ref cel="krepus.0o">krepusko</ref>,
    <ref cel="vesper.0o">vespero</ref>,
    <ref cel="velk.0i">velki</ref>,
    <ref cel="klin.0igxi">klini&gcirc;i</ref>.
   </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_o;&c_s;&c_je;&c_n;&c_mol;</trd>
  <trd lng="cs">podzim</trd>
  <trdgrp lng="de">
     <trd>Herbst</trd>,
     <trd>Sp&auml;tjahr [sudgermane]</trd></trdgrp>
  <trd lng="en">autumn</trd>
  <trd lng="fr">automne</trd>
  <trd lng="nl">herfst</trd>
  <trd lng="pl">jesie&nacute;</trd>
  <trd lng="ru">&#1086;&#1089;&#1077;&#1085;&#1100;</trd>
  <trd lng="sv">h&ouml;st</trd>
</drv>
</art>
