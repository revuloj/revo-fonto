<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>pore</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="pore.0o">
  <kap><tld/>o</kap>
  <snc mrk="pore.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="fak">KUI</uzo>
    <dif>
      Specio de granda <ref tip="super" cel="ajl.0o.genro">ajlo</ref>
      kultivata kiel <ref tip="super" cel="legom.0o.KUI">legomo</ref>
      (<trd lng="la">Allium porrum</trd>):
      <ekz>
        &gcirc;i estis iu platkuka pasta&jcirc;o <klr>...</klr>, en kiu kuni&gcirc;is,
        inter aliaj, precipe raspitaj terpomoj kaj fajne hakitaj
        <tld/>oj
        <fnt><bib>KielAkvo</bib>, <lok>1:3</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_e;&c_j;</trd>,
    <trd>&c_c;&c_y;&c_b;&c_u;&c_l;&c_ja;-&c_p;&c_a;&c_r;&c_e;&c_j;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_r;&c_a;&c_z;</trd>
  <trd lng="de">Porree</trd>
  <trd lng="en">leek</trd>
  <trd lng="fr">poireau</trd>
  <trdgrp lng="hu">
    <trd>p&oacute;r&eacute;hagyma</trd>,
    <trd>p&aacute;rhagyma</trd>
  </trdgrp>
  <trd lng="nl">prei</trd>
  <trd lng="pl">por</trd>
  <trdgrp lng="pt">
    <trd>porro</trd>,
    <trd>alho-porro</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_u;&c_k;-&c_p;&c_o;&c_r;&c_je;&c_j;</trd>
</drv>
</art>
