<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>pore</rad>/o
</kap>
<drv mrk="pore.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="ajl.0o.genro">ajlo 1</ref>
      (<trd lng="la">Allium porrum</trd>), kulturata kiel legomo.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_e;&c_j;</trd>,
    <trd>&c_c;&c_y;&c_b;&c_u;&c_l;&c_ja;-&c_p;&c_a;&c_r;&c_e;&c_j;</trd>
  </trdgrp>
  <trd lng="de">Porree</trd>
  <trd lng="fr">poireau</trd>
  <trdgrp lng="hu">
    <trd>p&oacute;r&eacute;hagyma</trd>,
    <trd>p&aacute;rhagyma</trd></trdgrp>
  <trd lng="nl">prei</trd>
  <trdgrp lng="pt">
    <trd>porro</trd>,
    <trd>alho-porro</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_u;&c_k;-&c_p;&c_o;&c_r;&c_je;&c_j;</trd>
</drv>
</art>
