<vortaro>
<art mrk="$Id: husejn.xml,v 1.5 2023/01/12 08:35:53 revo Exp $">
<kap>
  <rad>Husejn</rad>/o
</kap>
<drv mrk="husejn.0o">
  <kap><tld/>o</kap>
  <snc mrk="husejn.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> arabdevena:
      <ekz>
        mi tute ne rekonas pla&ccirc;an kaj bonkoran <tld/>on en
        &ccirc;iuj tiuj 
        senda&jcirc;oj
        <fnt>
          <aut>O. Kerziouk</aut>,
          <vrk><url
          ref="http://groups.yahoo.com/group/per-esperanto-politiko/message/6959">
          Averto kaj demando</url></vrk>,
          <lok>2009-01-19</lok>
        </fnt>;
      </ekz>
      <ekz>
        Mohamedo <tld/>o Fadlalaho <klr>...</klr> estis ulemo kaj 
        granda ajatolo libana
        <fnt>
          <bib>Viki</bib>,
          <lok><url
          ref="&Viki;/Mohamedo_Husejno_Fadlalaho">
          Mohammad Hussejn Fadlallah</url>, 2010-07-24</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_G;&c_u;&c_s;&c_e;&c_j;&c_n;</trd>,
      <trd>&c_H;&c_u;&c_s;&c_e;&c_j;&c_n;</trd>
    </trdgrp>
    <trd lng="fr">Hussein</trd>
    <trdgrp lng="pl">
      <trd>Husajn</trd>,
      <trd>Husejn</trd>
    </trdgrp>
  </snc>
</drv>
</art>

