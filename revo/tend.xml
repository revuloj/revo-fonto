    <trd>&c_t;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_sh;&c_a;&c_t;&c_jo;&c_r;</trd>
  </trdgrp>
  <trd lng="sgn" kod="M525x563S11511500x536S20500495x527S11519474x536S2ff00482x483">u@07*</trd>
