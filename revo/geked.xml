<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: geked.xml,v 1.13 2023/11/11 10:00:39 revo Exp $">
<kap>
  <rad>geked</rad>/oj
</kap>
<drv mrk="geked.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref tip="malprt" cel="skvam.0uloj">skvamuloj</ref>
      (<trd lng="la">Gekkonidae</trd>), el kiu pluraj specioj povas
      rampi sur vertikalaj surfacoj per elektrostataj teniloj de la fingroj:
      <ekz>
        <ref cel="gek.0o.genro">geko</ref>
        estas genro de lacerto el la familio de <tld/>oj, kiu trovi&gcirc;as en varmaj
        klimatoj tramonde, ili estas unikaj el la lacertoj vo&ccirc;e, &ccirc;ar nur ili kapablas
        fari sonon klakantan, kiam ili interreagas socie kun aliaj gekoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19445">Geko</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_k;&c_o;&c_n;&c_y;<klr tip="amb"> (&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_g;&c_je;&c_k;&c_o;&c_n;&c_o;&c_v;&c_i;<klr tip="amb"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="ca">gec&ograve;nids</trd>
  <trd lng="de">Geckos<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="en">geckos</trd>
  <trd lng="fr">geckonid&eacute;s</trd>
  <trd lng="hu">tapad&oacute;gy&iacute;kf&eacute;l&eacute;k</trd>
  <trdgrp lng="nl">
    <trd>gekko's<klr tip="ind"> (fam.)</klr></trd>,
    <trd>nachthagedissen<klr tip="ind"> (fam.)</klr></trd>
  </trdgrp>
  <trd lng="pt">gecon&iacute;deos</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_je;&c_k;&c_k;&c_o;&c_n;&c_y;<klr tip="ind"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_c;&c_je;&c_p;&c_k;&c_o;&c_p;&c_a;&c_l;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="zh">&#x58C1;&#x864E;&#x79D1; <pr>b&#xEC;h&#x1D4;k&#x113;</pr></trd>
</drv>
</art>
