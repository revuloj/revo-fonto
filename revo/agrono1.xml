<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: agrono1.xml,v 1.29 2023/10/28 08:32:44 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>agronomi</rad>/o
</kap>
<drv mrk="agrono1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SCI</uzo><uzo tip="fak">AGR</uzo>
  <snc mrk="agrono1.0o.scienco">
    <dif>
      <ref tip="lst" lst="voko:sciencoj" cel="scienc.0o.fako">Scienco</ref>
      pri <ref tip="prt" cel="agriku.0o">agrikulturo</ref>:
      <ekz>
        en la jaro 1887 oni malfermis <klr>[&#x2026;]</klr> lernejon de <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la fabrikoj kaj la <tld/>o produktas la varojn, sed neniu ricevas salajron
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj kaj metiaj iloj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        la terkulturaj aferoj ne estis por li antipatiaj, li e&ccirc; kun plezuro revis pri
        <tld/>a agado
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>;
      </ekz>
      <ekz>
        demandoj rilatantaj al agrikulturo kaj <tld/>o
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiuj junuloj speciale trejni&gcirc;as kun profesoroj pri <tld/>o, mediprotektado,
        nutra sekureco, familia legomproduktado, administrado de akvofontoj ktp
        <fnt>
          <bib>Ko11</bib>
          <vrk>Intervjuo</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_g;&c_r;&c_a;&c_n;&c_o;&c_m;&c_ib;&c_ja;</trd>
  <trd lng="cs">agronomie <klr>(v&ecaron;d.)</klr></trd>
  <trdgrp lng="de">
    <trd>Agronomie</trd>,
    <trd>Landwirtschaftswissenschaft</trd>
  </trdgrp>
  <trd lng="en">agronomy</trd>
  <trd lng="es">agronom&iacute;a</trd>
  <trd lng="fr">agronomie</trd>
  <trd lng="he">&alef;&gimel;&resh;&vav;&nun;&vav;&mem;&yod;&he;</trd>
  <trdgrp lng="hu">
    <trd>mez&odblac;gazdas&aacute;gtan</trd>,
    <trd>agron&oacute;mia</trd></trdgrp>
  <trd lng="it">agronomia</trd>
  <trdgrp lng="ja">
    <trd>&#x8FB2;&#x5B66;  <pr>&#x306E;&#x3046;&#x304C;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">agronomie</trd>
  <trd lng="pl">agronomia</trd>
  <trd lng="pt">agronomia</trd>
  <trd lng="ru">&c_a;&c_g;&c_r;&c_o;&c_n;&c_o;&c_m;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>agron&oacute;mia</trd>,
    <trd>agron&oacute;mia</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>tar&inodot;m bilim</trd>,
    <trd>agronomi</trd>
  </trdgrp>
  <trd lng="uk">&c_a;&c_gu;&c_r;&c_o;&c_n;&c_o;&c_m;&c_ib;&c_ja;</trd>
</drv>

<drv mrk="agrono1.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="agrono1.0isto.fakulo">
    <dif>
      <ref tip="super" cel="fak.0ulo">Fakulo</ref>
      pri <ref tip="vid" cel="agrono1.0o"><tld/>o</ref>:
      <ekz>
        la&ubreve; la <tld/>isto <nom>Marc Dufumier</nom>, malsatego povas ekesti kiam ajn,
        okaze de la plej eta klimata incidento
        <fnt>
          <bib>MD08</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="agrono.0o">agronomo</ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#x519C;&#x5B66;&#x5BB6; <pr>n&#xF3;ngxu&#xE9;ji&#x101;</pr></trd>,
    <trd>&#x8FB2;&#x5B78;&#x5BB6; <pr>n&#xF3;ngxu&#xE9;ji&#x101;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_g;&c_r;&c_a;&c_n;&c_o;&c_m;</trd>
  <trd lng="cs">agronom</trd>
  <trdgrp lng="de">
     <trd>Agronom</trd>,
     <trd>studierter <ind>Landwirt</ind></trd>
  </trdgrp>
  <trd lng="en">agronomist</trd>
  <trd lng="fr">agronome</trd>
  <trd lng="he">&alef;&#x5B7;&gimel;&resh;&vav;&#x5B9;&nun;&vav;&#x5B9;&fmem;</trd>
  <trdgrp lng="hu">
    <trd>mez&odblac;gazd&aacute;sz</trd>,
    <trd>agron&oacute;mus</trd></trdgrp>
  <trd lng="it">agronomo</trd>
  <trd lng="ja">&#x8FB2;&#x5B66;&#x8005;  <pr>&#x306E;&#x3046;&#x304C;&#x304F;&#x3057;&#x3083;</pr></trd>
  <trd lng="nl">agronoom</trd>
  <trd lng="pl">agronom</trd>
  <trd lng="pt">agr&ocirc;nomo</trd>
  <trd lng="ru">&c_a;&c_g;&c_r;&c_o;&c_n;&c_o;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>agron&oacute;m</trd>,
    <trd>agron&oacute;m</trd>
  </trdgrp>
  <trd lng="sv">agronom</trd>
</drv>
</art>
