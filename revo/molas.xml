<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: molas.xml,v 1.6 2022/03/23 14:02:26 revo Exp $">
    <kap>
      <rad>molas</rad>/o <fnt><bib>PIV2</bib></fnt>
    </kap>

    <drv mrk="molas.0o">
      <kap><tld/>o</kap>
      <snc mrk="molas.0o.MIN">
        <uzo tip="fak">MIN</uzo>
        <dif>
          &Scirc;tonospeco ofte sabla, kalka, argila, sedimenti&gcirc;inta
          apud montaro:
          <ekz>
            la <tld/>o en suda Germanio, anta&ubreve; jarmilionoj
            &scirc;uti&gcirc;inta de la alpoj, konservis multajn interesajn
            bestojn kaj plantojn
            <fnt>
              <aut>J. Ipfelkofer</aut>,
              <vrk>Fosilioj en suda Germanio</vrk>,
              <bib>Monato</bib>,
              <lok>jaro 2008a, numero 5a, p. 18a-20a</lok>
            </fnt>;
          </ekz><ekz>
            la supera geologia tavolo en la Svisa Mezlando estas la 
            <tld/>o, kiun postlasis kaj formis la gigantaj gla&ccirc;eroj
            dum  lasta glaciepoko
            <fnt><bib>Viki</bib>,
              <lok>artikolo &#xAB;Svisa Mezlando&#xBB;, 
                2008-04-23</lok></fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="cs">molasa</trd>
  <trd lng="fr">molasse</trd>
  <trdgrp lng="pl">
        <trd>molasa</trd>,
        <trd>szlir<klr> (molasa morska)</klr></trd>
      </trdgrp>
  <trdgrp lng="ru">
        <trd>&c_o;&c_s;&c_a;&c_d;&c_o;&c_ch;&c_n;&c_a;&c_ja; &c_g;&c_o;&c_r;&c_n;&c_a;&c_ja; &c_p;&c_o;&c_r;&c_o;&c_d;&c_a;</trd>,
        <trd>&c_m;&c_o;&c_l;&c_ja;&c_s;&c_s;&c_a;</trd>,
        <trd>&c_m;&c_o;&c_l;&c_a;&c_s;&c_s;&c_a;</trd>
      </trdgrp>
  <trd lng="sk">molasa</trd>
  <trd lng="uk">&#x43C;&#x43E;&#x43B;&#x430;&#x441;&#x430;</trd>
</drv>
  </art>

