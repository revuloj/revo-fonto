<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>rabi</rad>/o
</kap>
<drv mrk="rabi.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MED</uzo>
    <dif>
      Mortiga infekta malsano, inokulebla al homoj a&ubreve; bestoj per
      la salivo kaj karakterizata per furiozego kaj deziro mordi, kiun
      sekvas paralizo:
      <ekz>
        la vakcinadon kontra&ubreve; <tld/>o eltrovis Pasteur.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hidrof.0o">hidrofobio</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_sh;&c_a;&c_l;&c_je;&c_n;&c_s;&c_t;&c_v;&c_a;</trd>,
      <trd>&c_v;&c_a;&c_d;&c_a;&c_b;&c_o;&c_ja;&c_z;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">Tollwut</trd>
    <trd lng="en">rabies</trd>
    <trd lng="hu">veszetts&eacute;g</trd>
    <trdgrp lng="nl">
      <trd>hondsdolheid</trd>,
      <trd>rabies</trd>
    </trdgrp>
    <trd lng="pl">w&sacute;cieklizna</trd>
    <trdgrp lng="pt">
     <trd>raiva</trd>,
     <trd>dana&ccedil;&atilde;o</trd>,
     <trd>hidrofobia</trd>,
     <trd>lissa</trd>,
     <trd>r&aacute;bia</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_b;&c_je;&c_sh;&c_je;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>,
      <trd>&c_v;&c_o;&c_d;&c_o;&c_b;&c_o;&c_ja;&c_z;&c_n;&c_mol;</trd>
    </trdgrp>
    <trd lng="sv">rabies</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Furioza pasia ekscitegeco.
    </dif>
    <trdgrp lng="be">
      <trd>&c_sh;&c_a;&c_l;&c_je;&c_n;&c_s;&c_t;&c_v;&c_a;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_mol;&c_ju;&c_sh;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">Raserei</trd>
    <trdgrp lng="en">
      <trd>frenzy</trd>,
      <trd>rage</trd>
    </trdgrp>
    <trd lng="hu">&odblac;rj&ouml;ng&eacute;s</trd>
    <trd lng="nl">razernij</trd>
    <trdgrp lng="pl">
      <trd>w&sacute;ciek&lstroke;o&sacute;&cacute;</trd>,
      <trd>sza&lstroke;</trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_je;&c_sh;&c_je;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>
    <trdgrp lng="sv">
      <trd>raseri</trd>,
      <trd>ursinne</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">rage<klr tip="ind"> (maladie, excitation)</klr></trd>
  <trd lng="lat">rabies, em, e</trd>
</drv>

<drv mrk="rabi.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o 1.
    </dif>
    <trd lng="en">rabies</trd>
    <trd lng="hu">veszetts&eacute;ggel kapcsolatos</trd>
  </snc>
  <snc>
    <dif>
      Malsana je <tld/>o 1:
      <ekz>por hundon dronigi oni nomas &gcirc;in <tld/>a
      <fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
    <trd lng="be">&c_sh;&c_a;&c_l;&c_jo;&c_n;&c_y;</trd>
    <trd lng="de">tollw&uuml;tig</trd>
    <trd lng="en">rabid</trd>
    <trd lng="hu">veszett</trd>
    <trd lng="pl">chory na <ind>w&sacute;cieklizn&eogonek;</ind></trd>
    <trd lng="sv">rabiessmittad</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Furioze ekscitita, ne plu sin reganta:
      <ekz>
        <tld/>a pro, de kolero<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_sh;&c_a;&c_l;&c_jo;&c_n;&c_y;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_mol;&c_ju;&c_sh;&c_a;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>rabiat</trd>,
      <trd>w&uuml;tend</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>raging</trd>,
      <trd>frenzied</trd>,
      <trd>furious</trd>
    </trdgrp>
    <trd lng="hu">&odblac;rj&ouml;ng&odblac;</trd>
    <trdgrp lng="nl">
      <trd>dol</trd>,
      <trd>razend</trd>,
      <trd>rabiaat</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>rasande</trd>,
      <trd>ursinnig</trd>,
      <trd>rabiat</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>enrag&eacute;</trd>,
    <trd>rabique</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>w&sacute;ciek&lstroke;y</trd>,
    <trd>oszala&lstroke;y</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_sh;&c_je;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
