<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>rabi</rad>/o
</kap>
<drv mrk="rabi.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MED</uzo>
    <dif>
      Mortiga infekta malsano, inokulebla al homoj a&ubreve; bestoj per la salivo
      kaj karakterizata per furiozego kaj deziro mordi, kiun sekvas
      paralizo:
      <ekz>
        la vakcinadon kontra&ubreve; <tld/>o eltrovis Pasteur.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hidrof.0o">hidrofobio</ref>.
    </refgrp>
    <trd lng="de">Tollwut</trd>
    <trdgrp lng="nl">
      <trd>hondsdolheid</trd>,
      <trd>rabies</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_b;&c_je;&c_sh;&c_je;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>,
      <trd>&c_v;&c_o;&c_d;&c_o;&c_b;&c_o;&c_ja;&c_z;&c_n;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Furioza pasia ekscitegeco.
    </dif>
    <trd lng="de">Raserei</trd>
    <trd lng="nl">razernij</trd>
    <trd lng="ru">&c_b;&c_je;&c_sh;&c_je;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>
  </snc>
  <trd lng="fr">rage<klr tip="ind"> (maladie, excitation)</klr></trd>
</drv>

<drv mrk="rabi.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o 1.
    </dif>
  </snc>
  <snc>
    <dif>
      Malsana je <tld/>o 1:
      <ekz>
        por hundon dronigi oni nomas &gcirc;in <tld/>a<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">tollw&uuml;tig</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Furioze ekscitita, ne plu sin reganta:
      <ekz>
        <tld/>a pro, de kolero<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>rabiat</trd>,
      <trd>w&uuml;tend</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>dol</trd>,
      <trd>razend</trd>,
      <trd>rabiaat</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>enrag&eacute;</trd>,
    <trd>rabique</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_sh;&c_je;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
