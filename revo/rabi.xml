<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rabi.xml,v 1.35 2024/04/13 20:15:51 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>rabi</rad>/o
</kap>
<drv mrk="rabi.0o">
  <kap><tld/>o</kap>
  <snc mrk="rabi.0o.malsano">
    <uzo tip="fak">MED</uzo>
    <dif>
      Mortiga infekta <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>,
      inokulebla al homoj a&ubreve;
      bestoj
      per
      la salivo kaj karakterizata per furiozego kaj deziro mordi, kiun
      sekvas paralizo:
      <ekz>
        la vakcinadon kontra&ubreve; <tld/>o eltrovis Pasteur;
      </ekz>
      <ekz>
        Mi frapos &ccirc;iun &ccirc;evalon per <tld/>o kaj &gcirc;ian
        rajdanton per
        frenezeco
        <fnt>
          <bib>MT</bib>
          <lok>Ze&hcirc;arja 12:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        vakcinadi vulpojn kontra&ubreve; <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2008/009614.php">Historia
          histerio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estis bone konata al filozofoj kaj kuracistoj de
        antikvaj Grekio kaj Romo,
        kiuj prave kunligis la malsanon kun la mordo de <tld/>aj bestoj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2008/009550.php">Batalo
          kontra&ubreve; mortiga malsano</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la vakcino posedis rimarkindan propra&jcirc;on: &gcirc;i
        sekvigis la kreadon
        de antikorpoj,
        kiuj plejparte neniigis la virusojn de <tld/>o ankora&ubreve; en
        la
        inkubacia (latenta) stadio de la malsano
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2008/009550.php">Batalo
          kontra&ubreve; mortiga malsano</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hidrof.0o">hidrofobio</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_sh;&c_a;&c_l;&c_je;&c_n;&c_s;&c_t;&c_v;&c_a;</trd>,
      <trd>&c_v;&c_a;&c_d;&c_a;&c_b;&c_o;&c_ja;&c_z;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">Tollwut</trd>
    <trd lng="en">rabies</trd>
    <trd lng="hu">veszetts&eacute;g</trd>
    <trd lng="id">rabies</trd>
    <trdgrp lng="nl">
      <trd>hondsdolheid</trd>,
      <trd>rabies</trd>
    </trdgrp>
    <trd lng="pl">w&sacute;cieklizna</trd>
    <trdgrp lng="pt">
     <trd>raiva</trd>,
     <trd>dana&ccedil;&atilde;o</trd>,
     <trd>hidrofobia</trd>,
     <trd>lissa</trd>,
     <trd>r&aacute;bia</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_b;&c_je;&c_sh;&c_je;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>,
      <trd>&c_v;&c_o;&c_d;&c_o;&c_b;&c_o;&c_ja;&c_z;&c_n;&c_mol;</trd>
    </trdgrp>
    <trd lng="sv">rabies</trd>
    <trd lng="zh">&#x72C2;&#x72AC;&#x75C5; <pr>ku&#xE1;ngqu&#x1CE;nb&#xEC;ng</pr></trd>
</snc>
  <snc mrk="rabi.0o.furiozo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Furioza pasia ekscitegeco:
      <ekz>
        se al hundo mankas nenio, &gcirc;in atakas <tld/>o
        (ekfuriozas pro senagado)
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> tuj devis repreni la proponon pro universala
        malaprobo,
        nun la iama <tld/>o forpasis, kaj oni povas kun malvarmeta kapo
        mediti pri la afero
        <fnt>
          <bib>Monato</bib>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url ref="&Monato;/2003/007978.html">Landnomoj:
          homaj frenezigiloj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_sh;&c_a;&c_l;&c_je;&c_n;&c_s;&c_t;&c_v;&c_a;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_mol;&c_ju;&c_sh;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">Raserei</trd>
    <trdgrp lng="en">
      <trd>frenzy</trd>,
      <trd>rage</trd>
    </trdgrp>
    <trd lng="hu">&odblac;rj&ouml;ng&eacute;s</trd>
    <trd lng="nl">razernij</trd>
    <trdgrp lng="pl">
      <trd>w&sacute;ciek&lstroke;o&sacute;&cacute;</trd>,
      <trd>sza&lstroke;</trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_je;&c_sh;&c_je;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>
    <trdgrp lng="sv">
      <trd>raseri</trd>,
      <trd>ursinne</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x605A; <pr>hu&#xEC;</pr></trd>,
    <trd>&#x5927;&#x53D1;&#x813E;&#x6C14; <pr>d&#xE0;f&#x101;p&#xED;qi</pr></trd>,
    <trd>&#x5927;&#x767C;&#x813E;&#x6C23; <pr>d&#xE0;f&#x101;p&#xED;qi</pr></trd>
  </trdgrp>
</snc>
  <trd lng="es">rabia</trd>
  <trdgrp lng="ja">
    <trd>&#x72C2;&#x72AC;&#x75C5;  <pr>&#x304D;&#x3087;&#x3046;&#x3051;&#x3093;&#x3073;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x6FC0;&#x6012;  <pr>&#x3052;&#x304D;&#x3069;</pr></trd>,
    <trd>&#x72C2;&#x66B4;  <pr>&#x304D;&#x3087;&#x3046;&#x307C;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="lat">rabies, em, e</trd>
  <trd lng="ca">r&agrave;bia</trd>
  <trdgrp lng="cs">
    <trd>lysa</trd>,
    <trd>rabies</trd>,
    <trd>vzteklina</trd>
  </trdgrp>
  <trd lng="fr">rage<klr tip="ind"> (maladie, excitation)</klr></trd>
  <trd lng="sk">besnota</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43A;&c_a;&c_z;</trd>,
    <trd>&c_v;&#x43E;&c_d;&#x43E;&c_b;&#x43E;&#x44F;&c_z;&#x43D;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="rabi.0a">
  <kap><tld/>a</kap>
  <snc mrk="rabi.0a.rilata">
    <dif>
      Rilata al <ref cel="rabi.0o.malsano"><tld/>o<sncref/></ref>:
      <ekz>
        <tld/>a viruso;
      </ekz>
      <ekz>
        kontra&ubreve;<tld/>a vakcinado
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2008/009550.php">Batalo
          kontra&ubreve; mortiga malsano</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_ch;&c_n;&c_y;</trd>
    <trd lng="en">rabies</trd>
    <trd lng="hu">veszetts&eacute;ggel kapcsolatos</trd>
  </snc>
  <snc mrk="rabi.0a.malsana">
    <dif>
      Malsana je <ref cel="rabi.0o.malsano"><tld/>o<sncref/></ref>:
      <ekz>
        por hundon dronigi oni nomas &gcirc;in <tld/>a
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        morditaj de <tld/>aj hundoj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2008/009550.php">Batalo
          kontra&ubreve; mortiga malsano</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        malsani&gcirc;is &ccirc;iu tria mordita de <tld/>a besto
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2008/009550.php">Batalo
          kontra&ubreve; mortiga malsano</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_sh;&c_a;&c_l;&c_jo;&c_n;&c_y;</trd>
    <trd lng="de">tollw&uuml;tig</trd>
    <trd lng="en">rabid</trd>
    <trd lng="hu">veszett</trd>
    <trd lng="pl">chory na <ind>w&sacute;cieklizn&eogonek;</ind></trd>
    <trd lng="sv">rabiessmittad</trd>
  </snc>
  <snc mrk="rabi.0a.furioza">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Furioze ekscitita, ne plu sin reganta:
      <ekz>
        <tld/>a pro, de kolero;
      </ekz>
      <ekz>
        li estas <klr>[&#x2026;]</klr> <tld/>a leono, kiu e&ccirc;
        karesante de&scirc;iras
        la ha&ubreve;ton
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lastajn frazojn Eunana diris kun tiel rabia kolero, ke
        <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i kuris kiel <tld/>a, nenion vidante anta&ubreve; si, mi
        pensis, ke &scirc;i
        estas freneza
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        li preferis atendi en sia ka&scirc;ejo &gcirc;is la <tld/>a
        procesio estis
        preteririnta
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>En la Lando de la Noktuloj</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_sh;&c_a;&c_l;&c_jo;&c_n;&c_y;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_mol;&c_ju;&c_sh;&c_a;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="ca">furi&oacute;s</trd>
    <trdgrp lng="de">
      <trd>rabiat</trd>,
      <trd>w&uuml;tend</trd>,
      <trd>rasend</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>raging</trd>,
      <trd>frenzied</trd>,
      <trd>furious</trd>
    </trdgrp>
    <trd lng="hu">&odblac;rj&ouml;ng&odblac;</trd>
    <trdgrp lng="nl">
      <trd>dol</trd>,
      <trd>razend</trd>,
      <trd>rabiaat</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>rasande</trd>,
      <trd>ursinnig</trd>,
      <trd>rabiat</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x6C14;&#x6124; <pr>q&#xEC;f&#xE8;n</pr></trd>,
    <trd>&#x6C23;&#x61A4; <pr>q&#xEC;f&#xE8;n</pr></trd>,
    <trd>&#x7608; <pr>j&#xEC;</pr></trd>,
    <trd>&#x7316;&#x72C2; <pr>ch&#x101;ngku&#xE1;ng</pr></trd>
  </trdgrp>
</snc>
  <trd lng="ca">rabi&oacute;s</trd>
  <trdgrp lng="cs">
    <trd>maj&iacute;c&iacute; vzteklinu  <klr>(infikovan&eacute;
    zv&iacute;&rcaron;e)</klr></trd>,
    <trd>vztekl&yacute;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>enrag&eacute;</trd>,
    <trd>rabique</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x72C2;&#x72AC;&#x75C5;&#x306E;  <pr>&#x304D;&#x3087;&#x3046;&#x3051;&#x3093;&#x3073;&#x3087;&#x3046;&#x306E;</pr></trd>,
    <trd>&#x72C2;&#x72AC;&#x75C5;&#x306B;&#x304B;&#x304B;&#x3063;&#x305F; 
    <pr>&#x304D;&#x3087;&#x3046;&#x3051;&#x3093;&#x3073;&#x3087;&#x3046;&#x306B;&#x304B;&#x304B;&#x3063;&#x305F;</pr></trd>,
    <trd>&#x6FC0;&#x9AD8;&#x3057;&#x305F;  <pr>&#x3052;&#x304D;&#x3053;&#x3046;&#x3057;&#x305F;</pr></trd>,
    <trd>&#x72C2;&#x66B4;&#x306A;  <pr>&#x304D;&#x3087;&#x3046;&#x307C;&#x3046;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>w&sacute;ciek&lstroke;y</trd>,
    <trd>oszala&lstroke;y</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_sh;&c_je;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>besn&yacute;</trd>,
    <trd>maj&uacute;ci besnotu</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43A;&c_a;&c_zh;&c_je;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43B;&#x44E;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_sh;&c_a;&#x43B;&c_je;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6C14;&#x51B2;&#x51B2; <pr>q&#xEC;ch&#x14D;ngch&#x14D;ng</pr></trd>,
    <trd>&#x6C23;&#x6C96;&#x6C96; <pr>q&#xEC;ch&#x14D;ngch&#x14D;ng</pr></trd>
  </trdgrp>
</drv>
</art>
