<vortaro>
<art mrk="$Id: drog.xml,v 1.33 2023/11/04 18:50:52 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>drog</rad>/o
</kap>
<drv mrk="drog.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="drog.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <uzo tip="fak">MED</uzo>
    <dif>
      Kuraca, venena, narkota substanco uzata en farmacio a&ubreve;
      magio:
      <ekz>
        observu tiun junulon, <klr>[&#x2026;]</klr> kiu eble metis <tld/>on en
        &scirc;ian kafon
        <fnt><bib>Gerda</bib>, <lok>&ccirc;apitro 6a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_l;&c_je;&c_k;&c_ib;</trd>,
      <trd>&c_l;&c_je;&c_ch;&c_y;&c_v;&c_a;</trd>,
      <trd>&c_m;&c_e;&c_d;&c_y;&c_k;&c_a;&c_m;&c_e;&c_n;&c_t;</trd>,
      <trd>&c_l;&c_ja;&c_k;&c_a;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>,
      <trd>&c_p;&c_r;&c_e;&c_p;&c_a;&c_r;&c_a;&c_t; <klr>(&c_l;&c_je;&c_k;&c_a;&c_v;&c_y;)</klr></trd>,
      <trd>&c_z;&c_je;&c_l;&c_mol;&c_l;&c_je; <klr>(&c_l;&c_je;&c_k;&c_a;&c_v;&c_a;&c_je;)</klr></trd>,
      <trd>&c_z;&c_jo;&c_l;&c_k;&c_ib;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Arznei</trd>,
      <trd>Medikament</trd>,
      <trd>Droge<klr> [fak]</klr></trd>,
      <trd>Heildroge</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>droga</trd>,
      <trd>medicamento</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>drog</trd>,
      <trd>gy&oacute;gyszer</trd>,
      <trd>gy&oacute;gyanyag</trd>,
      <trd>szer<klr tip="amb">
      (gy&oacute;gy&iacute;t&oacute;)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>medykament</trd>,
      <trd>&sacute;rodek<klr> (np. na co&sacute;)</klr></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>droga</trd>,
      <trd>medicamento</trd>,
      <trd>medicina</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_je;&c_l;&c_mol;&c_je;</trd>,
      <trd>&c_s;&c_n;&c_a;&c_d;&c_o;&c_b;&c_mol;&c_je;</trd>,
      <trd>&c_p;&c_r;&c_je;&c_p;&c_a;&c_r;&c_a;&c_t;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x836F;&#x54C1; <pr>y&#xE0;op&#x1D0;n</pr></trd>,
    <trd>&#x539F;&#x6599;&#x836F; <pr>yu&#xE1;nli&#xE0;oy&#xE0;o</pr></trd>,
    <trd>&#x836F;&#x6750; <pr>y&#xE0;oc&#xE1;i</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="drog.0o.drogacxo">
    <dif>
      <ref tip="dif" cel="drog.0acxo">Droga&ccirc;o</ref>:
      <ekz>
        <tld/>konsumantoj
        <fnt>
          
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2013/010990.php">Birdoj kriantaj kaj
          kantantaj</url></vrk>
          <bib>Monato</bib>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>komercistoj</ind>
        <fnt>
          
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2013/011125.php">Historie senekzempla
          katastrofo</url></vrk>
          <bib>Monato</bib>
          <lok>2013</lok>
        </fnt>;
        <trd lng="de">Drogendealer</trd>
        <trd lng="nl">drugsdealer</trd>
      </ekz>
      <ekz>
        ni a&ubreve;dis pri neniu kri&scirc;nano, kiu uzas a&ubreve;
        vendas <tld/>ojn, fumas, drinkas alkoholon
        <fnt>
          <aut>Bob Felby</aut>,
          <vrk>Ne tiom dan&gcirc;era</vrk>,
          <bib>Monato</bib>,
          <lok>1999/12, p. 4</lok>
        </fnt>;
      </ekz>
      <ekz>
        falis &scirc;teloj sed kreskis krimoj rilataj al <tld/>oj
        <fnt>
       <aut>LAST/pg</aut>
          <vrk><url ref="&Monato;/2000/006850.php">Krim-portreto</url></vrk>
          <bib>Monato</bib>
          <lok>2000/11, p. 14</lok>
        </fnt>;
      </ekz>
      <ekz>
        per anonimaj &scirc;parlibretoj <tld/>&scirc;akristoj kaj aliaj krimuloj legalizas sian
        fimonon
        <fnt><aut>Walter Klag</aut>,
        <vrk><url ref="&Monato;/2000/006922.php">&Scirc;parlibretoj: fino por la anonimeco</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/06, p. 13</lok>
        </fnt>;
      </ekz>
      <ekz>
        profesiuloj ne lasus sin kapti per <tld/>kontrolo
        <fnt>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2000/007252.php">Du mil</url></vrk>
          <bib>Monato</bib>
          <lok>2000/12, p. 5</lok>
        </fnt>;
      </ekz>
      <ekz>
        kanabo &ndash; populara <tld/>a vegeta&jcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Antanas Grincevi&ccaron;ius</aut>
          <vrk><url ref="&Monato;/2003/007879.html">&Ccirc;okolada konflikto</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni spertis dum la Olimpikoj en Ateno: neniam anta&ubreve;e tiom da atletoj estis tro
        <tld/>itaj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2004/008492.html">Du mil kvar</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        milito inter <tld/>karteloj
        <fnt>
          <bib>Monato</bib>
          <aut>AMik</aut>
          <vrk><url ref="&Monato;/2011/010441.php">Rio-granda stimulado</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_n;&c_a;&c_r;&c_k;&c_o;&c_t;&c_y;&c_k;</trd>
    <trdgrp lng="de">
      <trd>Droge</trd>,
      <trd>Rauschgift</trd>,
      <trd>Dopingmittel</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>droga</trd>,
      <trd>narc&oacute;tico</trd>,
      <trd>estupefaciente</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>drog<klr tip="ind">
      (k&aacute;b&iacute;t&oacute;szer)</klr></trd>,
      <trd>szer<klr tip="ind">
      (k&aacute;b&iacute;t&oacute;szer)</klr></trd>,
      <trd>k&aacute;b&iacute;t&oacute;szer</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>drag<klr> (m&lstroke;odzie&zdot;.)</klr></trd>,
      <trd>narkotyk</trd>,
      <trd>&sacute;rodek odurzaj&aogonek;cy</trd>,
      <trd>doping<klr> (&sacute;rodek wspomagaj&aogonek;cy)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="cs">droga</trd>
  <trd lng="en">drug</trd>
  <trd lng="fr">drogue</trd>
  <trdgrp lng="ja">
    <trd>&#x85AC;&#x7A2E;  <pr>&#x3084;&#x304F;&#x3057;&#x3085;</pr></trd>,
    <trd>&#x751F;&#x85AC;  <pr>&#x3057;&#x3087;&#x3046;&#x3084;&#x304F;</pr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_n;&c_a;&c_d;&c_o;&c_b;&c_mol;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_p;&c_a;&c_r;&c_a;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">droga</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&c_ib;&#x43A;&c_i;</trd>,
    <trd>&#x43C;&c_je;&c_d;&c_i;&#x43A;&c_a;&#x43C;&c_je;&#x43D;&c_t;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_ib;&c_b;</trd>,
    <trd>&c_z;&c_ib;&#x43B;&#x43B;&#x44F;</trd>,
    <trd>&#x43D;&c_a;&c_r;&#x43A;&#x43E;&c_t;&c_i;&#x43A;</trd>
  </trdgrp>
</drv>

<drv mrk="drog.0acxo">
  <kap><tld/>a&ccirc;o</kap>
  <snc mrk="drog.0acxo.KOMUNE">
    <dif>
      <ref tip="super" cel="narkot.0ajxo">Narkota&jcirc;o</ref> uzata
      kiel stimulilo a&ubreve; por plezuro, ofte kun misaj sekvoj al la
      sano:
      <ekz>
        la kulpigo tekstas: legaligo de <tld/>a&ccirc;-mono kaj
        organizado de infan- kaj virin-komercado
        <fnt>
          <aut>Guido Van Damme</aut>,
          <vrk>Kiu faris kion?</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 1996a, numero 11a, p. 8a</lok>
        </fnt>;
      </ekz>
      <ekz>
        malnova balkana <tld/>a&ccirc;o-vojo, kiu kunligis Turkion, Bulgarion, Jugoslavion kaj
        Okcidenton
        <fnt>
          <aut>div.</aut>
          <vrk><url ref="&Monato;/1993/A931002.php">Noticoj</url></vrk>
          <bib>Monato</bib>
          <lok>1993/10, p. 4</lok>
         </fnt>;
      </ekz>
      <ekz>
        la <tld/>a&ccirc;o trovi&gcirc;is en kvar statuetoj importitaj el Venezuelo
        <fnt>
          <aut>div.</aut>
          <vrk><url ref="&Monato;/1993/A931002.php">Noticoj</url></vrk>
          <bib>Monato</bib>
          <lok>1993/10, p. 4</lok>
         </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_n;&c_a;&c_r;&c_k;&c_o;&c_t;&c_y;&c_k;</trd>
    <trdgrp lng="de">
      <trd>Droge</trd>,
      <trd>Rauschgift</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>abused substance</trd>,
      <trd>dangerous drug</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>droga</trd>,
      <trd>estupefaciente</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>drog<klr tip="ind">
      (k&aacute;b&iacute;t&oacute;szer)</klr></trd>,
      <trd>szer<klr tip="ind">
      (k&aacute;b&iacute;t&oacute;szer)</klr></trd>,
      <trd>k&aacute;b&iacute;t&oacute;szer</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>drag<klr> (m&lstroke;odzie&zdot;.)</klr></trd>,
      <trd>narkotyk</trd>,
      <trd>&sacute;rodek odurzaj&aogonek;cy</trd>,
      <trd>doping<klr> (&sacute;rodek wspomagaj&aogonek;cy)</klr></trd>
    </trdgrp>
  <trdgrp lng="pt">
    <trd>droga</trd>,
    <trd>entorpecente</trd>,
    <trd>t&oacute;xico</trd>
  </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_u;&c_r;&c_m;&c_a;&c_n;</trd>,
      <trd>&c_n;&c_a;&c_r;&c_k;&c_o;&c_t;&c_i;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x9EBB;&#x9189;&#x54C1; <pr>m&#xE1;zu&#xEC;p&#x1D0;n</pr></trd>,
    <trd>&#x8FF7;&#x5E7B;&#x5242; <pr>m&#xED;hu&#xE0;nj&#xEC;</pr></trd>
  </trdgrp>
</snc>
  <rim>
    La vorto estas misfarita: <em>-a&ccirc;</em> &scirc;ajnas aludi
    miskvaliton de la drogo, dum fakte temas pri fieco de &gcirc;ia
    konsumo. Krome, formale imageblas uzo de &leftquot;fi-<tld/>o&rightquot; por
    malpermesita uzo de stimuliloj fare de malhonestaj sportistoj, sed tia vortuzo
    &scirc;ajne ne ekzistas.
    <aut>Sergio Pokrovskij</aut>
  </rim>
</drv>

<drv mrk="drog.0emulo">
  <kap><tld/>emulo, <var><kap><tld/>ulo</kap></var></kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="drog.0emulo.narkotajxemulo">
    <dif>
      <ref tip="dif" cel="narkot.0ulo">Narkota&jcirc;emulo</ref>:
      <ekz>
        aidosuloj plus <tld/>emuloj
        <fnt>
          <aut>Vladimir Lemelev</aut>,
          <vrk>Novaj strategioj en Rusio</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2001a, numero 5a, p. 14a</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estas a&ubreve; senhejmulo a&ubreve; <tld/>ulo
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2005/008463.php">Strangulo en la naturo</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_r;&c_k;&c_a;&c_m;&c_a;&c_n;</trd>
  <trdgrp lng="de">
    <trd>Drogens&uuml;chtige(r)</trd>,
    <trd>Rauschgifts&uuml;chtige(r)</trd>
  </trdgrp>
  <trd lng="en">drug addict</trd>
  <trdgrp lng="es">
    <trd>drogadicto</trd>,
    <trd>toxic&oacute;mano</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>drogu&eacute;<klr> (subst.)</klr></trd>,
    <trd>toxicomane</trd>
  </trdgrp>
  <trd lng="he">&#x5DE;&#x5DB;&#x5D5;&#x5E8; &#x5DC;&#x5E1;&#x5DE;&#x5D9;&#x5DD;</trd>
  <trdgrp lng="pl">
    <trd>narkoman</trd>,
    <trd>lekoman</trd>,
    <trd>&cacute;pun</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_r;&c_k;&c_o;&c_m;&c_a;&c_n;</trd>,
    <trd>&c_t;&c_o;&c_k;&c_s;&c_i;&c_k;&c_o;&c_m;&c_a;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5438;&#x6BD2;&#x8005; <pr>x&#x12B;d&#xFA;zh&#x11B;</pr></trd>,
    <trd>&#x5438;&#x6BD2;&#x4EBA;&#x5458; <pr>x&#x12B;d&#xFA;r&#xE9;nyu&#xE1;n</pr></trd>,
    <trd>&#x763E;&#x541B;&#x5B50; <pr>y&#x1D0;nj&#x16B;nz&#x1D0;</pr></trd>,
    <trd>&#x55DC;&#x6BD2;&#x8005; <pr>sh&#xEC;d&#xFA;zh&#x11B;</pr></trd>
  </trdgrp>
</drv>
</art>
