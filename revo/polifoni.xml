<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: polifoni.xml,v 1.9 2023/10/20 16:38:17 revo Exp $">
<kap>
  <rad>polifoni</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="polifoni.0o">
  <kap><tld/>o</kap>
  <snc mrk="polifoni.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="dif" cel="vocx.plur0e.MUZ">Plurvo&ccirc;eco</ref>:
      <ekz>
        la muziko de <klr>[Fernando <nom>Sor</nom>]</klr> estis pli
        matura,
        <tld/>a kaj artisma
        <fnt>
          <aut>J. de las Heras</aut>,
          <vrk><url ref="http://www.gazetoteko.com/sor/sor1.htm">
          Vivo de Fernando Sor</url></vrk>,
          <lok>1997</lok>
        </fnt>;
      </ekz>
      <ekz>
        baroka muziko dividas kun renesanca muziko sian grandan uzadon
        de <tld/>o kaj
        kontrapunkto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=480">Baroko</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>mnohohlasost</trd>,
    <trd>polyfonie</trd>
  </trdgrp>
  <trd lng="de">Polyphonie</trd>
  <trd lng="en">polyphony</trd>
  <trd lng="fr">polyphonie</trd>
  <trdgrp lng="ja">
    <trd>&#x30DD;&#x30EA;&#x30D5;&#x30A9;&#x30CB;&#x30FC;</trd>,
    <trd>&#x591A;&#x58F0;&#x97F3;&#x697D;  <pr>&#x305F;&#x3053;&#x3048;&#x304A;&#x3093;&#x304C;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="pl">polifonia</trd>
  <trd lng="ro">polifonie</trd>
  <trd lng="sk">mnohohlasovos&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43B;&c_ib;&c_f;&#x43E;&#x43D;&c_ib;&#x44F;</trd>,
    <trd>&c_b;&c_a;&c_g;&c_a;&c_t;&#x43E;&c_g;&#x43E;&#x43B;&#x43E;&c_s;&c_s;&#x44F;</trd>
  </trdgrp>
</drv>
</art>

