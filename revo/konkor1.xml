<art mrk="$Id: konkor1.xml,v 1.16 2023/10/18 17:23:24 revo Exp $">
<kap>
  <rad>konkordanc</rad>/o
</kap>
<drv mrk="konkor1.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">LIN</uzo>
    <dif>
      Indekso de &ccirc;iuj vortoj de iu verko (speciale pri Bb.)
      kun cito de &ccirc;iuj lokoj, kie la vorto estas uzata:
      <ekz>
        <tld/>o de l' Ekzercaro.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="refere.0o">referenco</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_k;&c_a;&c_r;&c_d;&c_a;&c_n;&c_s;</trd>
  <trd lng="de">Konkordanz</trd>
  <trd lng="en">concordance</trd>
  <trdgrp lng="fa">
    <trd>&f_k;&a_sh;&a_f; &a_A;&a_l;&a_A_madda;&f_y;&a_A;&a_t;</trd>,
    <trd>&a_f;&a_h;&a_r;&a_s;&a_t;</trd>
  </trdgrp>
  <trd lng="fr">concordance</trd>
  <trdgrp lng="ja">
    <trd>&#x7528;&#x8A9E;&#x7D22;&#x5F15;  <pr>&#x3088;&#x3046;&#x3054;&#x3055;&#x304F;&#x3044;&#x3093;</pr></trd>,
    <trd>&#x30B3;&#x30F3;&#x30B3;&#x30FC;&#x30C0;&#x30F3;&#x30B9;</trd>
  </trdgrp>
  <trd lng="nl">concordantie</trd>
  <trd lng="pl">konkordancja</trd>
  <trd lng="pt">concord&acirc;ncia</trd>
  <trd lng="ro">concordan&#x21B;&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_n;&c_k;&c_o;&c_r;&c_d;&c_a;&c_n;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_k;&c_o;&c_r;&c_d;&c_a;&c_n;&c_s;</trd>
  </trdgrp>
</drv>
</art>
