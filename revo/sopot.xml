<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: sopot.xml,v 1.3 2023/09/28 20:23:53 afido Exp $">
    <kap>
      <rad>Sopot</rad>/o
    </kap>

<drv mrk="sopot.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="sopot.0o.banloko">
    <dif>
      Ban<ref tip="super" cel="lok.0o.landa">loko</ref>
      en la nordo de <ref cel="gdansk.0o">Gdansko</ref>, <ref tip="malprt"
      cel="pol.P0lando">Pollando</ref>:
      <ekz>
        vizitado de Gdansko kaj <tld/>o
        <fnt>
          <vrk><url ref="http://pej.pl/eo/2020/02/29/pej-semajnfino-triurbo2020/"
              >PEJ-semajnfino en Triurbo - 27-29 marto 2020</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        survoje al <tld/>o per boato
        <fnt>
          <vrk><url ref="http://esperanto-gbg.org/oraklubo/Ora_Jxurnalo_n-ro_11.pdf"
              >Al la tuta mondo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_S;&c_o;&c_p;&c_a;&c_t;</trd>
    <trd lng="de">Sopot</trd>
    <trd lng="en">Sopot</trd>
    <trd lng="fr">Sopot</trd>
    <trd lng="pl">Sopot</trd>
  </snc>
</drv>
  </art>

