<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kornic.xml,v 1.20 2023/10/18 17:23:25 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kornic</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kornic.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="kornic.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      La&ubreve;longa, mezgranda, &gcirc;enerale alta elstara&jcirc;o de
      konstrua&jcirc;o:
      <ekz>
        li faris por la altaro kradon en formo de kupra reto, sub &gcirc;ia <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 38:4</lok>
        </fnt>.
      </ekz>;
      <ekz>
        &ccirc;iu konstrua&jcirc;o videblas <klr>[&#x2026;]</klr> kiel desegneto kun siaj detalaj pordoj,
        fenestroj, e&ccirc; <tld/>oj, korbeloj kaj konzoloj
        <fnt>
          <bib>Monato</bib>
          <aut>Istv&aacute;n Ertl</aut>
          <vrk><url ref="&Monato;/2011/010457.php">Perspektivoj el rubujo</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <subsnc mrk="kornic.0o.ARKI_kolona">
      <dif>
        Elstara, supra, kvaza&ubreve;krona parto de surkolona&jcirc;o
        (entablemento)
        a&ubreve; de piedestalo de kolono a&ubreve; meblo.
      </dif>
      <trd lng="pt">cornija</trd>
    </subsnc>
    <subsnc mrk="kornic.0o.ARKI_cxambra">
      <dif>
        Ornama elstara&jcirc;o inter muro kaj plafono.
      </dif>
    </subsnc>
    <subsnc mrk="kornic.0o.ARKI_fenestra">
      <dif>
        Randelstara&jcirc;o de fenestro:
        <ekz>
          alumetoj ku&scirc;is sur <tld/>o inter fajrofarilo kaj malnova fera poto
          <fnt>
            <bib>Fab1</bib>
            <lok>Fluganta kofro</lok>
          </fnt>;
        </ekz>
        <ekz>
          sur la <tld/>oj de la fenestroj staris floroj
          <fnt>
            <bib>BdV</bib>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="korbel.0o.ARKI">korbelo</ref>,
      <ref cel="listel.0o.ARKI">listelo</ref>,
      <ref cel="modlur.0o.ARKI">modluro</ref>
    </refgrp>
    <trd lng="en">cornice</trd>
  </snc>
  <snc mrk="kornic.0o.GEOG">
    <uzo tip="stl">FIG</uzo>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Mallar&gcirc;a vojeto, kiu borderas profundega&jcirc;on flanke de
      monto:
      <ekz>
        estis neniu vojeto, sed apena&ubreve; rimarkebla <tld/>o, tute
        apude de la abismo
        <fnt><bib>PV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_z;&c_y;&c_m;&c_s;</trd>
  <trd lng="de">Sims</trd>
  <trd lng="es">cornisa</trd>
  <trd lng="fr">corniche</trd>
  <trdgrp lng="hu">
    <trd>p&aacute;rk&aacute;ny</trd>,  
    <trd>p&aacute;rk&aacute;nyzat</trd>,    
    <trd>p&aacute;rk&aacute;ny&uacute;t</trd>,    
  </trdgrp> 
  <trdgrp lng="ja">
    <trd>&#x30B3;&#x30FC;&#x30CB;&#x30B9;</trd>,
    <trd>&#x8ED2;&#x86C7;&#x8179;  <pr>&#x306E;&#x304D;&#x3078;&#x3073;&#x306F;&#x3089;</pr></trd>,
    <trd>&#x5CA9;&#x68DA;  <pr>&#x3044;&#x308F;&#x305F;&#x306A;</pr></trd>,
    <trd>&#x96EA;&#x5E87;  <pr>&#x305B;&#x3063;&#x3074;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>lijst</trd>,
    <trd>kroonlijst</trd>
  </trdgrp> 
  <trd lng="pl">gzyms</trd>     
  <trd lng="ro">corni&#x219;&abreve;</trd>  
  <trd lng="ru">&c_k;&c_a;&c_r;&c_n;&c_i;&c_z;</trd>
</drv>
</art>

