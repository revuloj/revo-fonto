<art mrk="$Id: turkmeni.xml,v 1.10 2019/06/30 18:21:50 revo Exp $">
<kap>
  <rad>Turkmeni</rad>/o
</kap>

<drv mrk="turkmeni.T0o">
  <kap>
    <tld lit="T"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <mlg kod="ISO-3166">TM</mlg>
  <snc mrk="turkmeni.T0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="turkmen.T0ujo.GEOG">Turkmenujo</ref>:
      <ekz>
        Usono volis Afganion kiel sekuran vojon por transporti petrolon de <tld/>o al
        Afganio kaj Pakistano
        <fnt>
          <bib>Monato</bib>
          <aut>Zahra Karimi</aut>
          <vrk><url ref="&Monato;/2003/008174.html"
              >Post la milito ... malstabileco</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en decembro mortis <klr>[&#8230;]</klr> la diktatoro de <tld/>o, unu el la lastaj
        diktatoroj inter la post-sovetiaj regantoj
        <fnt>
          <bib>Monato</bib>
          <aut>Sergeo Yurjich</aut>
          <vrk><url ref="&Monato;/2007/009171.php"
              >Ni esperu pri demokratiaj reformoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la partoprenantaj entreprenoj deklaris, ke ili ricevis promesojn pri gasliveroj
        el Azerbaj&gcirc;ano, norda Irako kaj <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2009/009895.php"
              >Pa&scirc;o al energia sendependeco</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_T;&c_u;&c_r;&c_k;&c_m;&c_je;&c_n;&c_i;&c_s;&c_t;&c_a;&c_n;</trd>
  <trdgrp lng="ca">
    <trd>el <ind>Turkmenistan</ind></trd>,
    <trd>Rep&uacute;blica del <ind>Turkmenistan</ind></trd>
  </trdgrp>
  <trd lng="de">Turkmenistan</trd>
  <trdgrp lng="fr">
    <trd>Turkm&eacute;nistan</trd>,
    <trd>Turkm&eacute;nie</trd>,
    <trd>R&eacute;publique du Turkm&eacute;nistan</trd>
  </trdgrp>
  <trd lng="hu">T&uuml;rkmeniszt&aacute;n</trd>
  <trd lng="id">Turkmenistan</trd>
  <trd lng="it">Turkmenistan</trd>
  <trd lng="pl">Turkmenistan</trd>
</drv>
</art>

