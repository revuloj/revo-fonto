<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>&ccirc;ek</rad>/o
</kap>
<drv mrk="cxek.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Mandato, kiu rajtigas iun ricevi monon el la banka konto de la
      subskribinto:
      <ekz>
        la sinjorino vestita nigre subskribas mian nomon sur la
        <tld/>on<fnt><aut>Spomenka &Scirc;timec</aut>: <vrk>Hejmo en
        Meze&ubreve;ropo</vrk>, <bib>Monato</bib><lok>jaro 1996a, numero 
1a, p.
        20a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kambi.0o">kambio</ref>,
      <ref cel="trat.0o">trato</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_ch;&c_e;&c_k;</trd>
  <trd lng="de">Scheck</trd>
  <trd lng="el">&epsilon;&pi;&jota;&tau;&alfa;&gamma;&eta_ton;</trd>
  <trdgrp lng="en">
    <trd>cheque</trd>,
    <trd>check</trd>
  </trdgrp>
  <trd lng="es">cheque</trd>
  <trd lng="fr">ch&egrave;que</trd>
  <trdgrp lng="he">
    <trd>&he;&mem;&het;&alef;&he;</trd>,
    <trd>&shin;&yod;&qof;<klr tip="ind"> 
(&he;&mem;&het;&alef;&he;)</klr></trd>,
    <trd>&tsadi;'&qof;</trd>
  </trdgrp>
  <trd lng="hu">csekk</trd>
  <trd lng="nl">cheque</trd>
  <trd lng="pl">czek</trd>
  <trd lng="pt">cheque</trd>
  <trd lng="ro">cec</trd>
  <trd lng="ru">&c_ch;&c_je;&c_k;</trd>
  <trd lng="sk">&scaron;ek</trd>
  <trd lng="tr">&ccedil;ek</trd>
</drv>

<drv mrk="cxek.0aro">
  <kap><tld/>aro</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Kajereto de de&scirc;ireblaj &ccirc;ekoj pretaj por alskribo de 
monsumo kaj
      subskribo. &Ccirc;eklibro.
    </dif>
  </snc>
  <trd lng="de">Scheckheft</trd>
  <trdgrp lng="fr">
    <trd>carnet de ch&egrave;ques</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&pe;&nun;&qof;&samekh; &shin;&yod;&qof;&yod;&fmem;</trd>,
    <trd>&pe;&nun;&qof;&samekh; &tsadi;'&qof;&yod;&fmem;</trd>
  </trdgrp>
  <trd lng="hu">csekkf&uuml;zet</trd>
  <trd lng="nl">chequeboek</trd>
  <trd lng="pl">ksi&aogonek;&zdot;eczka <ind>czekowa</ind></trd>
  <trd lng="ru">&c_ch;&c_je;&c_k;&c_o;&c_v;&c_a;&c_ja; 
&c_k;&c_n;&c_i;&c_zh;&c_k;&c_a;</trd>
</drv>
</art>
