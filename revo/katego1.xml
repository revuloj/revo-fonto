<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: katego1.xml,v 1.2 2022/05/22 12:50:23 afido Exp $">
<kap>
    <rad>kategori</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="katego1.0a">
  <kap><tld/>a</kap>
  <snc mrk="katego1.0a.senkondicxa">
    <dif>
      <ref tip="sin" cel="kondic1.sen0a">Senkondi&ccirc;a</ref>, <ref cel="firm.0a.fiksita">firma</ref>, 
      <ref cel="nepr.0a">nepra</ref>, postula, tute <ref cel="klar.0a.komprenebla">klara</ref>:
      <ekz>
        <tld/>a tono
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la generaloj ne kura&gcirc;is ion diri post la <tld/>aj vortoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne formulante <tld/>an respondon, mi diros al vi, ke ver&scirc;ajne <klr>[&#8230;]</klr>
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        malgra&ubreve; la firma kaj <tld/>a aserto de &ccirc;iuj oficialuloj responsaj <klr>[&#8230;]</klr>
        absolute nenie estis registrita lia alveno
        <fnt>
          <bib>MkM</bib>
          <lok>27</lok>
        </fnt>;
      </ekz>
      <ekz>
        doni jam nun <tld/>an, difinitan kaj &ccirc;iam-devigan regulon por la skribado de
        nomoj ne estas ankora&ubreve; tempo
        <fnt>
          <bib>LR</bib>
          <lok>Dua Parto &ndash; Gramatiko</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="absolu.0a">absoluta</ref>
    <trd lng="be">&c_k;&c_a;&c_t;&c_e;&c_g;&c_a;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
    <trdgrp lng="de">
      <trd>bestimmt</trd>,
      <trd>entschieden</trd>,
      <trd>v&ouml;llig</trd>,
      <trd>absolut</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>categ&oacute;rico<klr tip="ind"> (sin equivocaci&oacute;n)</klr></trd>,
      <trd>sin <ind>r&eacute;plica</ind></trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_b;&f_y; &a_q;&f_y;&a_d; &a_w; &a_sh;&a_r;&a_T;</trd>,
      <trd>&a_m;&a_T;&a_l;&a_q;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>cat&eacute;gorique<klr tip="ind"> (sans &eacute;quivoque)</klr></trd>,
      <trd>sans <ind>&eacute;quivoque</ind></trd>,
      <trd>sans <ind>r&eacute;plique</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kategorikus</trd>,
      <trd>hat&aacute;rozott</trd>,
      <trd>felt&eacute;tlen</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_a;&c_t;&c_je;&c_g;&c_o;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_k;&c_a;&c_t;&c_je;&c_g;&c_o;&c_r;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_b;&c_je;&c_z;&c_a;&c_p;&c_je;&c_l;&c_l;&c_ja;&c_c;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <ref tip="hom" cel="katego.0a.de_kategorio">kategoria (speca)</ref>
</drv>
</art>

