<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>trateratra</rad> <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="trateratra.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="trateratra.0.SON">
    <dif>
      <ref tip="lst" cel="son.0o" lst="voko:sonoj">Sono</ref>
      de latuna muzikilo, trumpeto a&ubreve; simila, la&ubreve;te vokanta:
      <ekz>
        <ctl><tld/>!</ctl> la po&scirc;to alvenis
        <fnt>
          <bib>Fab4</bib>,
          <vrk>Dekdu per la po&scirc;to</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">taratata !</trd>
</drv>
</art>

