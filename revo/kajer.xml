<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kajer.xml,v 1.28 2022/03/21 17:51:56 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>kajer</rad>/o
</kap>

<drv mrk="kajer.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="kun.0igajxo">Kuniga&jcirc;o</ref> de kelkaj 
      paperfolioj, falditaj a&ubreve; ne:
      <ekz>
        jen estas la <tld/>o de la juna fra&ubreve;lino
        <fnt>
          <bib>F</bib>
          <lok>&FE; 10</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i amasigis en angulon muzikajn <tld/>ojn
        <fnt>
          <bib>KPr</bib>
        </fnt>.
      </ekz>
    </dif>
    <subsnc mrk="kajer.0o.porskribi">
       <dif>
         uzataj por skribi:
        <ekz>
          la <ind>skrib<tld/>o</ind> de <nom>Hjalmar</nom>
          <fnt>
            <bib>Fab2</bib>
            <lok>Dio de dormo</lok>
          </fnt>;
          <trd lng="de">Schreibheft</trd>
        </ekz>
        <ekz>
          alportu la <tld/>on, ni ripetos vian aritmetikan lecionon
          <fnt>
            <bib>IK</bib>
          </fnt>;
        </ekz>
        <ekz>
          la profunda lingva sciado, kiun posedis la anta&ubreve;a instruistino de la knabineto,
          atestis la diversloke en la <tld/>oj trovi&gcirc;antaj notoj
          <fnt>
            <bib>Marta</bib>
          </fnt>;
        </ekz>
        <ekz>
          &ccirc;iujn tekstojn, kiujn s-ro <nom>Kertsch</nom> diktas al mi, 
          mi konservas <klr>[&#x2026;]</klr> en miaj <ind>steno<tld/>o</ind>j
          <fnt>
            <bib>CKv</bib>
          </fnt>.
          <trd lng="de">Stenoheft</trd>
        </ekz>
      </dif>
      <trdgrp lng="nl">
         <trd>schrift<klr tip="ind"> (schrijfboek)</klr></trd>,
         <trd>schrijfboek</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="kajer.0o.de_libro">
      <uzo tip="fak">TIP</uzo>
      <dif>
        de <ref tip="malprt" cel="libr.0o.presajxo">libro</ref> a&ubreve; <ref tip="malprt" cel="revu.0o.INF_gazeto">revuo</ref>:
        <ekz>
          tiu libro enhavas dek <tld/>ojn kune binditajn;
        </ekz>
        <ekz>
          mi decidis eldoni la libron kun la respondoj &mdash; per apartaj <tld/>oj
          <fnt>
            <bib>DL</bib>
          </fnt>;
        </ekz>
        <ekz>
          la tuta libro havos 5&#x202F;&ndash;&#x202F;6 <tld/>ojn
          <fnt>
            <bib>DL</bib>
          </fnt>;
        </ekz>
        <ekz>
          tre &scirc;atataj estis liaj &ccirc;iujaraj kontribua&jcirc;oj al 
          la <ind>jar<tld/>o</ind> &leftquot;Sennacieca Revuo&rightquot;
          <fnt>
            <bib>EeP</bib>
          </fnt>;
          <trd lng="de">Jahresausgabe</trd>
        </ekz>
        <ekz>
          <klr>[la]</klr> proverboj en la lingvoj rusa, pola, franca, germana
          <klr>[&#x2026;]</klr> komencis aperi <ind>po<tld/>e</ind> en 1905
          <fnt>
            <bib>Monato</bib>
            <vrk><url ref="https://tekstaro.com/t?nomo=artikoloj-el-monato">Venas proverbo el popola la cerbo</url></vrk>
          </fnt>;
          <trd lng="de">heftweise</trd>
        </ekz>
        <ekz>
          <nom>GIL</nom> eldonas la bibliografie orientitan bultenon &leftquot;<nac>Interlinguistische
          Informationen</nac>&rightquot; (kvar <tld/>oj jare) kaj publikigas la prelegojn de la
          fakprogramoj en <ind>krom<tld/>o</ind>j
          <fnt>
            <bib>LOdE</bib>
            <vrk><url ref="https://tekstaro.com/t?nomo=ondo-de-esperanto">10a konferenco de GIL</url></vrk>
          </fnt>.
          <trd lng="de">Beilage <klr>(Heft)</klr></trd>
        </ekz>
      </dif>
      <ref tip="sin" cel="brosxu.0o">bro&scirc;uro</ref>
      <trdgrp lng="nl">
        <trd>katern</trd>
      </trdgrp>
    </subsnc>
    <refgrp tip="sub">
       <ref cel="oktav.0o.TIP">oktavo<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_sh;&c_y;&c_t;&c_a;&c_k;</trd>
  <trd lng="bg">&c_t;&c_je;&c_t;&c_r;&c_a;&c_d;&c_k;&c_a;</trd>
  <trd lng="br">kaier</trd>
  <trd lng="cs">se&scaron;it</trd>
  <trd lng="de">Heft</trd>
  <trdgrp lng="en">
   <trd>notebook</trd>,
   <trd>pamphlet</trd>
 </trdgrp>
  <trd lng="es">cuaderno</trd>
  <trd lng="fi">vihko</trd>
  <trd lng="fr">cahier</trd>
  <trd lng="he">&het;&vav;&bet;&resh;&tav;</trd>
  <trd lng="hu">f&udblac;zet</trd>
  <trdgrp lng="pl">
    <trd>zeszyt</trd>,
    <trd>kajet</trd>
  </trdgrp>
  <trd lng="pt">caderno</trd>
  <trdgrp lng="ro">
    <trd>carne&tcedil;el</trd>,
     <trd>caiet</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_t;&c_r;&c_a;&c_d;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>p&iacute;sanka</trd>,
    <trd>zo&scaron;it</trd>
  </trdgrp>
  <trd lng="sv">h&auml;fte</trd>
  <trd lng="tr">defter</trd>
  <trdgrp lng="uk">
    <trd>&#x437;&#x43E;&#x448;&#x438;&#x442;</trd>,
    <trd>&#x432;&#x438;&#x43F;&#x443;&#x441;&#x43A; (&#x447;&#x430;&#x441;&#x442;&#x438;&#x43D;&#x430; &#x442;&#x432;&#x43E;&#x440;&#x443;)</trd>
  </trdgrp>
</drv>
</art>
