<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kajer</rad>/o
</kap>
<drv mrk="kajer.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Kuniga&jcirc;o da kelkaj paperfolioj, falditaj a&ubreve; ne:
      <ekz><tld/>o por skriblernado;</ekz>
      <ekz>tiu libro enhavas dek <tld/>ojn kune binditajn;</ekz>
      <ekz>eldoni libron per apartaj <tld/>oj<fnt>Z</fnt>.</ekz>  
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_sh;&c_y;&c_t;&c_a;&c_k;</trd>
  <trd lng="cs">se&scaron;it</trd>
  <trd lng="de">Heft</trd>
  <trdgrp lng="en">
     <trd>notebook</trd>,
     <trd>pamphlet</trd>
  </trdgrp>
  <trd lng="fr">cahier</trd>
  <trd lng="he">&het;&vav;&bet;&resh;&tav;</trd>
  <trd lng="hu">f&udblac;zet</trd>
  <trdgrp lng="nl">
     <trd>schrift</trd>,
           <trd>katern</trd>
           </trdgrp>
           <trd lng="ru">&c_t;&c_je;&c_t;&c_r;&c_a;&c_d;&c_mol;</trd>
  <trd lng="sv">h&auml;fte</trd>
</drv>
</art>
