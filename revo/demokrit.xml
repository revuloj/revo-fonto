<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: demokrit.xml,v 1.14 2023/11/04 13:16:27 revo Exp $">
<kap>
  <rad>Demokrit</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="demokrit.0o">
  <kap><tld/>o</kap>
  <snc mrk="demokrit.0o.PERS">
    <uzo tip="fak">FIL</uzo>
    <uzo tip="fak">PERS</uzo>
    <dif>
      Antikva greka <ref tip="lst" lst="voko:historiaj_personoj" cel="filozo.0o.scienculo">filozofo</ref>,
      imaginto de atoma konsisto de materio (proksimume 460 a.K. &ndash; proksimume 370 a.K.):
      <ekz>
        mi ridos kiel <tld/>o
        <fnt>
          <aut>H. Sienkiewicz, trad. Lydia Zamenhof</aut>,
          <vrk>Quo vadis?</vrk>,
          <lok>1933</lok>
        </fnt>;
      </ekz>
      <ekz>
        li komentis <klr>[&#x2026;]</klr> la tekstojn de Platono, Aristotelo
        kaj <tld/>o
        <fnt>
          <aut>E. Kusters</aut>,
          <vrk>Alfred Nobel...</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2006a, numero 12a, p. 20a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_D;&c_e;&c_m;&c_a;&c_k;&c_r;&c_y;&c_t;</trd>
  <trd lng="bg">&c_D;&c_je;&c_m;&c_o;&c_k;&c_r;&c_i;&c_t;</trd>
  <trd lng="ca">Dem&ograve;crit</trd>
  <trd lng="cs">Demokritos</trd>
  <trdgrp lng="de">
    <trd>Demokrit</trd>,
    <trd>Demokritos</trd>
  </trdgrp>
  <trd lng="fr">D&eacute;mocrite</trd>
  <trd lng="hu">Demokritosz</trd>
  <trd lng="it">Democrito</trd>
  <trd lng="ja">&#x30C7;&#x30E2;&#x30AF;&#x30EA;&#x30C8;&#x30B9;</trd>
  <trd lng="pl">Demokryt</trd>
  <trd lng="pt">Dem&oacute;crito</trd>
  <trd lng="sk">Demokritos</trd>
  <trd lng="uk">&c_D;&c_je;&c_m;&c_o;&c_k;&c_r;&c_ib;&c_t;</trd>
  <trd lng="zh">&#x5FB7;&#x8B28;&#x514B;&#x5229;&#x7279; <pr>d&#xE9;m&#xF3;k&#xE8;l&#xEC;t&#xE8;</pr></trd>
</drv>
</art>

