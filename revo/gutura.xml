<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gutura.xml,v 1.24 2024/12/13 08:36:50 revo Exp $">
<kap>
  <rad>gutural</rad>/o
</kap>
<drv mrk="gutura.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FON</uzo>
  <snc>
    <dif>
      Nomo, kiun kelkaj fonetikistoj donas al la
      <ref tip="sin" cel="velar.0o">velaroj</ref>:
      <ekz>
      distingas &gcirc;in <klr>(la lingvon)</klr> multaj <tld/>oj (velaroj)
      kaj manko de klaraj vokaloj
      <fnt>
       <aut>Stefan Maul</aut>,
       <vrk>El verva vivo jurnalista</vrk>,
      <lok>p. 34</lok>
      </fnt>.
      </ekz>
    </dif>
  </snc>
  <adm>
  Laux mi temas pri iom pli vasta kategorio ol la nuraj
  velaroj. <aut>MB</aut>
  </adm>
  <trd lng="be">&c_g;&c_u;&c_t;&c_u;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y; &c_z;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="bg">&c_g;&c_u;&c_t;&c_u;&c_r;&c_a;&c_l;&c_je;&c_n; &c_z;&c_v;&c_u;&c_k;</trd>
  <trd lng="ca">gutural</trd>
  <trdgrp lng="de">
     <trd>Guttural</trd>,
     <trd>Kehllaut</trd></trdgrp>
  <trd lng="en">guttural</trd>
  <trd lng="es">gutural</trd>
  <trd lng="fr">gutturale</trd>
  <trdgrp lng="hu">
     <trd>torokhang</trd>,
     <trd>guttur&aacute;lis</trd></trdgrp>
  <trd lng="ja">&#21897;&#x97F3;  <pr>&#x306E;&#12393;&#x304A;&#12392;</pr></trd>
  <trd lng="nl">keelklank</trd>
  <trd lng="pl">g&lstroke;oska gard&lstroke;owa</trd>
  <trdgrp lng="pt">
    <trd>gutural</trd>,
    <trd>consoante <ind>gutural</ind></trd>
  </trdgrp>
  <trd lng="ro">sunetel&abreve; gutural&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_u;&c_t;&c_t;&c_u;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_n;&c_je;&c_ja;&c_z;&c_y;&c_ch;&c_n;&c_y;&c_j; &c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_n;&c_je;&c_n;&c_jo;&c_b;&c_n;&c_y;&c_j; &c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">hrtanov&aacute; spoluhl&aacute;ska <klr>(k| g)</klr></trd>
  <trd lng="sk">hrtanov&aacute; spoluhl&aacute;ska <klr>(k| g)</klr></trd>
  <trd lng="cs">hrtanov&aacute; souhl&aacute;ska <klr>(k| g)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_g;&#x43E;&c_r;&c_t;&c_a;&#x43D;&#x43D;&c_i;&c_j;
    (&c_a;&c_b;&#x43E;&#xA0;&c_z;&c_a;&c_d;&#x43D;&#x44C;&#x43E;&#x44F;&c_z;&c_i;&c_ch;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_d;&#x43D;&#x44C;&#x43E;&#x43F;&c_ib;&c_d;&#x43D;&c_je;&c_b;&c_ib;&#x43D;&#x43D;&c_i;&c_j;)&#xA0;&#x43F;&c_r;&c_i;&c_g;&#x43E;&#x43B;&#x43E;&c_s;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x984E;&#x97F3; <pr>&#xE8;y&#x12B;n</pr></trd>,
    <trd>&#x5C0F;&#x820C;&#x97F3;&#x5B57;&#x6BCD;
    <pr>xi&#x1CE;osh&#xE9;y&#x12B;nz&#xEC;m&#x1D4;</pr></trd>,
    <trd>&#21897;&#x97F3; <pr>h&#xF3;uy&#x12B;n</pr></trd>
  </trdgrp>
</drv>
</art>
