<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: krizan.xml,v 1.25 2023/12/03 09:59:01 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>krizantem</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="krizan.0o">
  <kap><tld/>o</kap>
  <snc mrk="krizan.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="astera.0oj">asteracoj</ref>,
      &gcirc;ardene kultivata (<trd lng="la">Chrysanthemum</trd>):
      <ekz>
        jen estas <tld/>o, &mdash; mi volis doni al vi kelke da violoj, 
        sed ili &ccirc;iuj velkis de la tempo, kiam mia patro mortis
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la 19a jarcento en Japanio oni komencis ekspozicii pupojn, vestitajn per <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Hayasi Kaneyuki</aut>
          <vrk><url ref="&Monato;/2006/008846.php">Priploru la krizan temon</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        aromas <tld/>aj floroj blanke kaj flave sur la verda razeno
        <fnt>
          <aut>Hasegawa T.</aut>,
          <vrk><url ref="http://wikilivres.ca/wiki/En_%C4%88inio_Batalanta">
          En &Ccirc;inio Batalanta</url></vrk>, 
          <lok>1954</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="&WCU;/thumb/d/d9/Chrysanthemum_x_morifolium_Dompierre_3.jpg/640px-Chrysanthemum_x_morifolium_Dompierre_3.jpg" prm="CC0">&gcirc;ardenaj <tld/>oj (Chrysanthemum x morifolium)
       <fnt>
         <aut>Jebulon, propra verko</aut>
         <lok><url ref="&WCW;/File:Chrysanthemum_x_morifolium_Dompierre_3.jpg">&WCW;/File:Chrysanthemum_x_morifolium_Dompierre_3.jpg</url></lok>
       </fnt>
  </bld>
  <bld lok="&WCU;/thumb/a/a0/Tanacetum_Vulgare.jpg/640px-Tanacetum_Vulgare.jpg" prm="CC BY-SA 3.0"><ind>ordinara <tld/>o</ind> (Tanacetum vulgare a&ubreve; Chrysanthemum vulgare) 
       <fnt>
         <aut>Brigitte Alliot</aut>
         <lok><url ref="&WCW;/File:Tanacetum_Vulgare.jpg">&WCW;/File:Tanacetum_Vulgare.jpg</url></lok>
       </fnt>
    <trd lng="de">Rainfarn</trd>
    <trd lng="hu">var&aacute;dics</trd>
    <trd lng="la">Chrysanthemum vulgare</trd>
  </bld>
  <trd lng="be">&c_h;&c_r;&c_y;&c_z;&c_a;&c_n;&c_t;&c_e;&c_m;&c_a;</trd>
  <trd lng="bg">&c_h;&c_r;&c_i;&c_z;&c_a;&c_n;&c_t;&c_je;&c_m;&c_a;</trd>
  <trd lng="de">Chrysantheme</trd>
  <trd lng="en">chrysanthemum</trd>
  <trd lng="fa">&a_d;&a_A;&a_w;&a_d;&f_y;</trd>
  <trd lng="fr">chrysanth&egrave;me</trd>
  <trdgrp lng="hu">
    <trd>margitvir&aacute;g</trd>,
    <trd>margar&eacute;ta</trd>,
    <trd>aranyvir&aacute;g</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30AD;&#x30AF;  <pr>&#x304D;&#x304F;</pr></trd>,
    <trd>&#x83CA;  <pr>&#x304D;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">chrysant</trd>
  <trdgrp lng="pl">
    <trd>z&lstroke;ocie&nacute;</trd>,
    <trd>chryzantema</trd>
  </trdgrp> 
  <trd lng="pt">cris&acirc;ntemo</trd>
  <trd lng="ro">crizantem&abreve;</trd>
  <trd lng="ru">&c_h;&c_r;&c_i;&c_z;&c_a;&c_n;&c_t;&c_je;&c_m;&c_a;</trd>
  <trd lng="sk">chryzant&eacute;ma</trd>
  <trdgrp lng="cs">
    <trd>chryzant&eacute;ma</trd>,
    <trd>kopretina</trd>
  </trdgrp>
  <trd lng="uk">&#x445;&#x440;&#x438;&#x437;&#x430;&#x43D;&#x442;&#x435;&#x43C;&#x430;</trd>
  <trdgrp lng="zh">
    <trd>&#x83CA; <pr>j&#xFA;</pr></trd>,
    <trd>&#x83CA;&#x82B1; <pr>j&#xFA;hu&#x101;</pr></trd>,
    <trd>&#x9EC3;&#x82B1; <pr>hu&#xE1;nghu&#x101;</pr></trd>,
    <trd>&#x9EC4;&#x82B1; <pr>hu&#xE1;nghu&#x101;</pr></trd>
  </trdgrp>
</drv>
</art>
