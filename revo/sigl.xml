<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sigl.xml,v 1.10 2023/05/14 13:25:53 revo Exp $">
<kap>
  <rad>sigl</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sigl.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="sigl.0o.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      <ref tip="super" cel="long.mal0igo">Mallongigo</ref> el la
      unuaj literoj de la vortoj de plena esprimo:
      <ekz>
        sur pa&gcirc;o 14 mi stumblis je la <tld/>o RGADA
        <fnt>
          <aut>Sergej Derevjanny&hcirc;</aut>
          <vrk><url ref="&Monato;/2001/pdf/monato200111.pdf#page=30">Atrakcio por
          lingvo&scirc;atantoj?</url></vrk>
          <bib>Monato</bib>
          <lok>2001/11, p. 30</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">sigla</trd>
  <trd lng="cs">sigla</trd>
  <trd lng="es">sigla</trd>
  <trd lng="fr">sigle</trd>
  <trd lng="hu">bet&udblac;sz&oacute;</trd>
  <trdgrp lng="nl">
    <trd>letterwoord</trd>,
    <trd>acroniem</trd>
  </trdgrp>
  <trd lng="pl">skr&oacute;towiec</trd>
  <trd lng="ru">&c_a;&c_b;&c_b;&c_r;&c_je;&c_v;&c_i;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>samoznak</trd>,
    <trd>skratka <klr>(p&#xED;smenov&#xE1;)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43A;&#x43E;&c_r;&#x43E;&c_ch;&c_je;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_a;&c_b;&c_r;&c_je;&c_v;&c_ib;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
  </trdgrp>
</drv>
</art>

