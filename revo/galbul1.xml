<art mrk="$Id: galbul1.xml,v 1.11 2023/07/10 06:36:05 revo Exp $">
<kap>
  <rad>galbuled</rad>/oj
</kap>
<drv mrk="galbul1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="peg.0oformaj">pegoformaj birdoj</ref>
      (<trd lng="la">Galbulidae</trd>), vivantaj en tropika
      Ameriko kun longa beko por kapti insektojn, precipe papiliojn,
      pluraj kun orverde brilanta plumaro:
      <ekz>
        <tld/>oj a&ubreve; jakamaraoj estas familio <klr>[&#8230;]</klr> el tropika Suda kaj
        Centrameriko, &gcirc;is Meksiko, la grupo enhavas kvin genrojn kaj 18 speciojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=264976">Galbuledoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="galbul.0o">galbulo</ref>
  </snc>
  <trd lng="be">&c_ja;&c_k;&c_a;&c_m;&c_a;&c_r;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Glanzv&ouml;gel<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="en">jacamars</trd>
  <trd lng="fr">galbulid&eacute;s</trd>
  <trd lng="hu">jakam&aacute;rf&eacute;l&eacute;k</trd>
  <trd lng="nl">glansvogels<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pt">galbul&iacute;deos</trd>
  <trd lng="ru">&c_zh;&c_a;&c_k;&c_a;&c_m;&c_a;&c_r;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&#x44F;&#x43A;&c_a;&#x43C;&c_a;&c_r;&#x43E;&c_v;&c_ib;</trd>
</drv>
</art>
