<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dren.xml,v 1.17 2021/03/08 16:54:56 revo Exp $">
<kap>
  <rad>dren</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="dren.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="dren.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="sek.0igi.KOMUNE">Sekigi</ref>
      per forkondukado de likva&jcirc;oj.
    </dif>
    <subsnc mrk="dren.0i.AGR">
      <uzo tip="fak">AGR</uzo>
      <dif>
        Sekigi tro malsekan grundon per kanaletoj
        a&ubreve; subteraj tuboj:
        <ekz>
          mi petis <klr>[&#x2026;]</klr> fari fosa&jcirc;on &ccirc;e la herbarfino, por ke la &gcirc;ardeno de mia
          patrino estu <tld/>ita
          <fnt>
            <bib>Monato</bib>
            <vrk><url ref="http://steloj.de/esperanto/monato2/006878.html">Dommastrumo: &Gcirc;ardenumi</url></vrk>
          </fnt>;
        </ekz>
        <ekz>
          instala&jcirc;oj de irigaciado kaj <tld/>ado
          <fnt>
            <bib>Monato</bib>
            <aut>Mu Binghua</aut>
            <vrk><url ref="https://www.monato.be/2011/010266.php">Akvomastrumado postulas grandegajn investojn</url></vrk>
            <lok>2011</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="dren.0i.MED">
      <uzo tip="fak">MED</uzo>
      <dif>
        Forkonduki likva&jcirc;on el organo:
        <ekz>
          <tld/>i kavon, vundon
          <fnt>
            <aut>red. J. Hradil</aut>,
            <vrk><url ref="http://www.eventoj.hu/steb/vortaroj/medicina-vortaro-de-umea.pdf">
            Esperanta Medicina Vortaro</url></vrk>,
            <lok>[vidita en 2017]</lok>
          </fnt>;
        </ekz>
        <ekz>
          remado ebligas naturan limfan <tld/>adon kaj plifortigas muskolojn
          <fnt>
            <bib>Monato</bib>
            <aut>Christian Bertin</aut>
            <vrk><url ref="https://www.monato.be/2015/011437.php">Remado-rimedo</url></vrk>
            <lok>2015</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
  </snc>
  <ref tip="hom" cel="tren.0i">treni</ref>
  <trdgrp lng="cs">
    <trd>dren&aacute;&zcaron;ovat</trd>,
    <trd>odv&aacute;d&ecaron;t sekrety</trd>,
    <trd>vypr&aacute;zdnit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>dr&auml;nieren</trd>,
    <trd>dr&auml;nen</trd>,
    <trd>trockenlegen</trd>
  </trdgrp>
  <trd lng="en">drain</trd>
  <trd lng="es">drenar</trd>
  <trdgrp lng="fr">
    <trd>drainer</trd>,
    <trd>vidanger</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>drenowa&cacute;</trd>,
    <trd>odwadnia&cacute;</trd>,
    <trd>osusza&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>drenar</trd>,
    <trd>drainar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_s;&c_u;&c_sh;&c_a;&c_t;&c_mol;
    &c_p;&c_o;&c_ch;&c_v;&c_u;</trd>,
    <trd>&c_d;&c_r;&c_je;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dren&aacute;&zcaron;ova&tcaron;</trd>,
    <trd>vypr&aacute;zdni&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="dren.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="dren.0ilo.AGR">
    <dif>
      Kanaleto a&ubreve; tubo por elfluigi akvon el tro malseka grundo.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>dr&eacute;n</trd>,
    <trd>odvod&ncaron;ovac&iacute; potrub&iacute;</trd>,
    <trd>trativod</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Dr&auml;nrohr</trd>,
    <trd>Dr&auml;nager&ouml;hre</trd>
  </trdgrp>
  <trd lng="en">drain</trd>
  <trd lng="es">drenaje</trd>
  <trd lng="fr">drain</trd>
  <trd lng="he">&#x5E0;&#x5D9;&#x5E7;&#x5D5;&#x5D6;</trd>
  <trdgrp lng="pl">
    <trd>dren</trd>,
    <trd>rura <ind>drenarska</ind></trd>,
    <trd>drena&zdot;<klr> (ca&lstroke;y system)</klr></trd>,
    <trd>s&aogonek;czek</trd>
  </trdgrp>
  <trd lng="pt">dreno</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_r;&c_je;&c_n;&c_a;&c_zh;&c_n;&c_y;&c_j;
    &c_s;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_a;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">dr&eacute;n</trd>
</drv>
</art>
