<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>dren</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="dren.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="dren.0i.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Sekigi tro malsekan grundon per subteraj tuboj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>dr&auml;nieren</trd>,
    <trd>dr&auml;nen</trd>,
    <trd>trockenlegen</trd>
  </trdgrp>
  <trd lng="es">drenar</trd>
  <trd lng="fr">drainer</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_s;&c_u;&c_sh;&c_a;&c_t;&c_mol; &c_p;&c_o;&c_ch;&c_v;&c_u;</trd>,
    <trd>&c_d;&c_r;&c_je;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="dren.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="dren.0ilo.AGR">
    <dif>
      Subtera tubo, por elfluigi akvon el tro malseka grundo.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Dr&auml;nrohr</trd>,
    <trd>Dr&auml;nager&ouml;hre</trd>
  </trdgrp>
  <trd lng="es">drenaje</trd>
  <trd lng="fr">drain</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_r;&c_je;&c_n;&c_a;&c_zh;&c_n;&c_y;&c_j; &c_s;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_a;&c_v;&c_a;</trd>
  </trdgrp>
</drv>
</art>
