<vortaro>
<art mrk="$Id$">
<kap>
<rad>cibet</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="cibet.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Fekodora graseca substanco, sekreciata el anusa glando
      de <ref tip="malprt" cel="viver.0o">vivero</ref>.
    </dif>
    <trd lng="de">Zibet</trd>
  </snc>
</drv>
<drv mrk="cibet.0kato">
  <kap><tld/>kato</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <ref tip="dif" cel="viver.0o">Vivero</ref>
  </snc>
</drv>
</art>
