<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>cibet</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="cibet.0o">
  <kap><tld/>o</kap>
  <snc mrk="cibet.0o.substanco">
    <dif>
      Fekodora graseca substanco, sekreciata el anusa glando de 
      <tld/>o <sncref ref="cibet.0o.genro"/>.
    </dif>
    <trd lng="de">Zibet</trd>
  </snc>
  <snc mrk="cibet.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="cibet.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Viverra</trd>), vivanta en Azio kaj Afriko.
    </dif>
    <trd lng="de">Zibetkatze</trd>
  </snc>
</drv>
<drv mrk="cibet.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="best.rabo0oj">rabobestoj</ref>
      (<trd lng="la">Viverridae</trd>).
    </dif>
  </snc>
  <trd lng="de">Schleichkatzen</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
