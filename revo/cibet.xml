<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cibet.xml,v 1.26 2023/10/31 13:37:03 revo Exp $">
<kap>
<rad>cibet</rad>/o
</kap>

<drv mrk="cibet.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Fekodora graseca substanco, sekreciata el anusa glando
      de <ref tip="malprt" cel="viver.0o">vivero</ref>;
      &gcirc;i estas uzata en la parfuma industrio:
      <ekz>
        delonge viveroj estis &ccirc;ascelo pro <tld/>o, aroma substanco en iliaj &ccirc;irka&ubreve;anusaj glandoj,
        kiun oni uzis kiel parfuma&jcirc;on
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2005/008721.php">De kie venis kaj kial &ccirc;esis netipa
          pne&ubreve;monio?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="mosk.0o">mosko</ref>
    <trd lng="de">Zibet</trd>
    <trd lng="en">civet</trd>
    <trd lng="fa">&a_z;&a_fatha;&a_b;&a_A;&a_d;</trd>
    <trd lng="fr">civette<klr tip="ind"> (parfumerie)</klr></trd>
    <trd lng="hu">cibet</trd>
    <trd lng="it">zibetto<klr> (essenza odorosa)</klr></trd>
    <trd lng="pl">pi&zdot;mo</trd>
    <trd lng="pt">alm&iacute;scar</trd>
    <trd lng="ru">&c_c;&c_i;&c_b;&c_je;&c_t;</trd>
  </snc>
  <trd lng="cs">cibet</trd>
  <trd lng="ja">&#x3058;&#x3083;&#x9999;  <pr>&#x3058;&#x3083;&#x304B;&#x304A;&#x308A;</pr></trd>
  <trd lng="sk">cibet</trd>
  <trd lng="uk">&c_c;&c_i;&c_b;&c_je;&c_t;&c_i;&#x43D;</trd>
</drv>

<drv mrk="cibet.0kato">
  <kap><tld/>kato</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="viver.0o">Vivero</ref>:
      <ekz>
        <tld/>kato <klr>[&#x2026;]</klr> uzas <klr>[cibeton]</klr> por allogi alian sekson por pari&gcirc;o kaj
        marki sian areon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=10048">Cibeto</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Zibetkatze</trd>
  <trd lng="en">civet</trd>
  <trd lng="fa">&f_g;&a_r;&a_b;&a_h;&a_hamza_sure; &a_z;&a_fatha;&a_b;&a_A;&a_d;</trd>
  <trd lng="fr">civette<klr tip="ind"> (animal)</klr></trd>
  <trd lng="hu">cibetmacska</trd>
  <trdgrp lng="it">
    <trd>viverra</trd>,
    <trd>zibetto</trd>,
    <trd>genetta</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd/>,
    <trd>&#x30B8;&#x30E3;&#x30B3;&#x30A6;&#x30CD;&#x30B3;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wiwera</trd>,
    <trd>cyweta</trd>,
    <trd>cybeta</trd>
  </trdgrp>
  <trd lng="pt">almiscareiro</trd>
  <trdgrp lng="ru">
   <trd>&c_c;&c_i;&c_v;&c_je;&c_t;&c_t;&c_a;</trd>,
   <trd>&c_v;&c_i;&c_v;&c_je;&c_r;&c_r;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x679C;&#x5B50;&#x72F8; <pr>gu&#xF3;z&#x1D0;m&#x101;o</pr></trd>,
    <trd>&#x7075;&#x732B; <pr>l&#xED;ngm&#x101;o</pr></trd>,
    <trd>&#x9E9D;&#x732B; <pr>sh&#xE8;m&#x101;o</pr></trd>
  </trdgrp>
</drv>
</art>
