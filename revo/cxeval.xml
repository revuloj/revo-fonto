<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&ccirc;eval</rad>/o
</kap>
<drv mrk="cxeval.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="cxeval.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Specio (<trd lng="la">Equus caballus</trd>) de grandaj <ref
      tip="super" cel="huf.nepar0uloj">neparhufulaj</ref> <ref tip="super"
      cel="mam.0uloj">mamuloj</ref> uzata de anta&ubreve;historia epoko kiel
      rajd- kaj tir-besto:
      <ekz>
        Azenoj, zebroj, kaj <tld/>oj estas en la sama genro;
      </ekz>
      <ekz>
        <tld/>ido estas nematura <tld/>o
        <fnt><bib>F</bib>, <lok>&FE; 36</lok></fnt>;
      </ekz>
    </dif>
    <trd lng="bg">&c_k;&c_o;&c_n;</trd>
    <trd lng="cs">k&uring;&ncaron;</trd>
    <trdgrp lng="de">
      <trd>Pferd</trd>,
      <trd>Gaul<klr> [mezokcidentgermane]</klr></trd>,
      <trd>Ross<klr> [sudgermane]</klr></trd>
    </trdgrp>
    <trdgrp lng="el">
      <trd>&alfa_ton;&lambda;&omikron;&gamma;&omikron;</trd>,
      <trd>&jota_ton;&pi;&pi;&omikron;&sigma_fina;</trd>
    </trdgrp>
    <trd lng="en">horse</trd>
    <trd lng="he">&samekh;&vav;&samekh;</trd>
    <trd lng="hu">l&oacute;</trd>
    <trd lng="os">&c_b;&aelig;&c_h;</trd>
    <trd lng="pl">ko&nacute;</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_n;&c_mol;</trd>,
      <trd>&c_l;&c_o;&c_sh;&c_a;&c_d;&c_mol;</trd>
    </trdgrp>
    <trd lng="tr">at</trd>
    <trd lng="vo">jeval</trd>
  </snc>
  <snc mrk="cxeval.0o.sxaka">
    <uzo tip="fak">SPO</uzo>
    <dif>
      &Scirc;akpeco, kutime figurata per <tld/>a kapo.
    </dif>
    <trd lng="en">knight</trd>
    <trd lng="he"><ind>&pe;&resh;&shin;</ind><klr tip="amb">
    (&shin;&het;&mem;&tet;)</klr></trd>
    <trdgrp lng="hu">
      <trd>l&oacute;</trd>,
      <trd>husz&aacute;r</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_o;&c_n;&c_mol;</trd>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_n;&c_mol;</trd>
  <trd lng="es">caballo</trd>
  <trd lng="fr">cheval</trd>
  <trd lng="nl">paard</trd>
  <trd lng="de">Pferd</trd>
  <trd lng="pt">cavalo</trd>
  <trd lng="sk">k&ocirc;&ncaron;</trd>
  <trdgrp lng="zh">
    <trd>&#39532;</trd>,
    <trd><klr>tr. </klr>&#39340;<klr> [m&#462;]</klr></trd>
  </trdgrp>
</drv>

<drv mrk="cxeval.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="cxeval.0eto.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Temo, pri kiu koncernulo &ccirc;iam volonte parolas, kiu
      &ccirc;iam lin interesas:
      <ekz>
        tio estas lia amata <tld/>eto<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        li tuj ekrajdis sian amatan <tld/>eton pri la supersigna
        reformo;
      </ekz>
      <ekz>
        la facileco de Volap&uuml;k estas la plej amata <tld/>eto,
        sur kiu ili elveturas al &ccirc;iu okazo
        <fnt><aut>&Z;</aut>, <lok>citite la&ubreve; PIV, p.158</lok>.
        </fnt>
      </ekz>
    </dif>
    <ref tip="sin" cel="am.0atajxo">amata&jcirc;o</ref>
    <refgrp tip="vid">
      <ref cel="sxat.0okupo">&scirc;atokupo</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Steckenpferd</trd>,
      <trd>Lieblingsbesch&auml;ftigung</trd>,
      <trd>Hobby</trd>
    </trdgrp>
    <trd lng="en">hobbyhorse</trd>
    <trdgrp lng="fr">
      <trd>dada</trd>,
      <trd>marotte</trd>
    </trdgrp>
    <trd lng="he">&shin;&yod;&gimel;&yod;&vav;&fnun;</trd>
    <trd lng="hu">vessz&odblac;paripa</trd>
    <trdgrp lng="nl">
      <trd>stokpaardje</trd>, <trd>hobby</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_o;&c_n;&c_jo;&c_k;</trd>
    <trd lng="sk">kon&iacute;&ccaron;ek</trd>
  </snc>
  <snc mrk="cxeval.0eto.etacxevalo">
    <dif>
      Malgranda <tld/>o;
    </dif>
    <trd lng="de">Pferdchen</trd>
    <trd lng="en">pony</trd>
    <trd lng="hu">lovacska</trd>
    <trd lng="nl">paardje</trd>
    <trdgrp lng="ru">
      <trd>&c_l;&c_o;&c_sh;&c_a;&c_d;&c_k;&c_a;</trd>,
      <trd>&c_k;&c_o;&c_n;&c_jo;&c_k;</trd>
    </trdgrp>
    <trd lng="vo">jevalil</trd>
  </snc>
</drv>

<drv mrk="cxeval.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="cxeval.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Viando de <tld/>o.
    </dif>
  </snc>
  <trd lng="en">horsemeat</trd>
  <trd lng="es">carne de <ind>caballo</ind></trd>
</drv>

<drv mrk="cxeval.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="cxeval.0ejo.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Konstrua&jcirc;o en kiu oni tenas <tld/>ojn.
    </dif>
  </snc>
  <trd lng="en">stables</trd>
  <trd lng="es">caballeriza</trd>
</drv>

<drv mrk="cxeval.0ido">
  <kap><tld/>ido</kap>
  <snc mrk="cxeval.0ido.ZOO">
    <dif>
      Ido de <tld/>o:
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>foal</trd>,
    <trd>colt</trd>,
    <trd>filly</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>potro</trd>,
    <trd>potranco</trd>
  </trdgrp>
</drv>

<drv mrk="cxeval.0ino">
  <kap><tld/>ino</kap>
  <snc mrk="cxeval.0ino.ZOO">
    <dif>
      Inseksa <tld/>o:
    </dif>
  </snc>
  <trd lng="en">mare</trd>
  <trd lng="es">yegua</trd>
</drv>

<drv mrk="cxeval.0povo">
  <kap><tld/>povo</kap>
  <snc>
    <uzo tip="fak">FIZ</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Malnova komunuza
      <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de me&hcirc;anika
      povo, egala en E&ubreve;ropo al 75
      <ref cel="kilogr.0metro">kilogrammetroj</ref> en sekundo, t. e.
      735,75W,
      kaj, en Usono, al 550 futpundoj en sekundo, t.e. 745,7W; simb.
      &Ccirc;P.
    </dif>
    <adm>
      Se gravas konservi la usonan valoron, necesos enkonduki "pundo"
      kaj "futpundo" en Revo-n. Uzo de "funto-futo" estus eble pli
      adekvata, kvankam malbonsona, &ccirc;ar &leftquot;pundo&rightquot;
      estas monunuo.
      <aut>MB</aut>
    </adm>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_n;&c_s;&c_k;&c_a;&c_ja;
  &c_s;&c_ib;&c_l;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Pferdest&auml;rke</trd>,
    <trd>PS</trd>
  </trdgrp>
  <trd
  lng="el">&jota;&pi;&pi;&omikron;&delta;&ypsilon_ton;&ny;&alfa;&my;&eta;</trd>
  <trd lng="en">horsepower</trd>
  <trd lng="es"><ind>caballo</ind> de fuerza</trd>
  <trd lng="fr">cheval-vapeur</trd>
  <trd lng="he">&kaf;&vav;&het;-&samekh;&vav;&samekh;</trd>
  <trd lng="hu">l&oacute;er&odblac;</trd>
  <trd lng="nl">paardekracht</trd>
  <trd lng="pt">cavalo-vapor</trd>
  <trd lng="ru">&c_l;&c_o;&c_sh;&c_a;&c_d;&c_i;&c_n;&c_a;&c_ja;
  &c_s;&c_i;&c_l;&c_a;</trd>
  <trd lng="sk">konsk&aacute; sila</trd>
</drv>

<drv mrk="cxeval.de0igxi">
  <kap>de<tld/>i&gcirc;i</kap>
  <snc mrk="cxeval.de0igxi.RAJ">
    <dif>
      Lasi la rajdadon de <tld/>o:
      <ekz>
        de<tld/>i&gcirc;u, amiko, kaj senbridigu la bestojn
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 12a, p.
        457a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">descendre de cheval</trd>
  <trd lng="hu">lesz&aacute;ll a <ind>l&oacute;r&oacute;l</ind></trd>
  <trd lng="ru">&c_s;&c_p;&c_je;&c_sh;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="cxeval.sur0igxi">
  <kap>sur<tld/>i&gcirc;i</kap>
  <snc mrk="cxeval.sur0igxi.RAJ">
    <dif>
      Eksidi sur la dorso de <tld/>o, enseli&gcirc;i.
    </dif>
  </snc>
  <trd lng="en">mount</trd>
  <trd lng="fr">monter &agrave; cheval</trd>
  <trd lng="hu">felsz&aacute;ll a <ind>l&oacute;ra</ind></trd>
  <trd lng="ru">&c_s;&c_je;&c_s;&c_t;&c_mol; &c_n;&c_a;
  &c_k;&c_o;&c_n;&c_ja;</trd>
</drv>

<drv mrk="cxeval.troja0o">
  <kap>troja <tld/>o</kap>
  <snc mrk="cxeval.troja0o.KOMUNE">
    <dif>
      Insida enigo de malutilaj homoj a&ubreve; rimedoj inter malamikojn
      <klr>(kiel faris antikvaj grekoj kontra&ubreve; trojanoj per &ccirc;evalstatuo 
      la&ubreve; <ref tip="vid" cel="iliad.0o.BELE">Iliado</ref>)</klr>:
      <ekz>
        la popola fronto estis elpensita de la Tria Internacio kiel troja 
        <tld/>o por akiri la kontrolon de la laboristaj organizoj
        <fnt>
          <aut>P. A. Foche</aut>,
          <vrk>Re: Pri la nomo Franco</vrk>,
          <lok><url
          ref="http://groups.google.be/group/soc.culture.esperanto?hl=eo">soc.culture.esperanto</url>,
          2004-01-19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="cxeval.troja0o.KOMP"> <fnt><bib>KompLeks</bib></fnt>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      &Scirc;ajne utila, reale malutila programo, kiu ka&scirc;e difektas, rompas 
      sekurecon, malfermas konekton al eksterulo...:
      <ekz>
        pkzip.300 <klr>...</klr> estas Troja <tld lit="&Ccirc;"/>o, ne viruso, 
        kaj tre dan&gcirc;era
        <fnt>
          <aut>P. Jelenc</aut>,
          <vrk>Re: pri viruso "zip"</vrk>,
          <lok><url 
          ref="http://groups.google.be/group/soc.culture.esperanto?hl=eo">soc.culture.esperanto</url>,
          1996-10-20</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">cheval de troie</trd>
</drv>

<drv mrk="cxeval.vir0o">
  <kap>vir<tld/>o</kap>
  <snc mrk="cxeval.vir0o.ZOO">
    <dif>
      Virseksa <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">&eacute;talon<klr tip="ind"> (cheval)</klr></trd>
  <trd lng="de">Hengst</trd>
  <trd lng="en">stallion</trd>
  <trd lng="es">caballo macho</trd>
  <trdgrp lng="hu">
    <trd>cs&odblac;d&ouml;r</trd>,
    <trd>m&eacute;n</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_n;&c_mol;</trd>,
    <trd>&c_zh;&c_je;&c_r;&c_je;&c_b;&c_je;&c_c;</trd>
  </trdgrp>
</drv>
</art>
