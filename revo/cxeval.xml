<drv mrk="cxeval.mar0eto">
  <kap>mar<tld/>eto</kap>
  <snc mrk="cxeval.mar0eto.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Mara <ref tip="lst" lst="voko:fi&scirc;oj" cel="fisx.0o.ZOO">fi&scirc;</ref>eto kun arka korpo
      kaj fleksebla vosto, per kiu &gcirc;i tenas sin al objektoj;
      anka&ubreve; nomata <ref tip="sin" cel="hipokamp1.0o.ZOO">hipokampo</ref>;
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">genro</ref>
      el familio singnatedoj (<trd lng="la">Hippocampus</trd>):
      <ekz>
        plej okulfrapa estas la mar<tld/>eto <klr>[&#x2026;]</klr>, &gcirc;ia kapo
        vere similas al la <tld/>a <klr>[&#x2026;]</klr>, pro tio &ccirc;i tiu fi&scirc;o
        ricevis en &ccirc;iuj lingvoj la saman nomon &mdash; mar<tld/>eto
        <fnt>
          <aut>P. Giunio</aut>
          <vrk>Sekretoj de la maresta&jcirc;oj</vrk>
          <lok>1960</lok>
        </fnt>;
      </ekz><ekz>
        mi trovis verdan mar<tld/>eton (hipokampo), egan rozkoloran
        languston kaj nigrebrunan kankregon
        <fnt>
          <vrk><url ref="https://www.vastalto.com/blog/?p=921">Verda
          mar&ccirc;evaleto - Akvosfero</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/25/Hippocampus_hippocampus_(on_Ascophyllum_nodosum).jpg/360px-Hippocampus_hippocampus_(on_Ascophyllum_nodosum).jpg" prm="CC BY-SA 4.0">mar<tld/>eto, en Belga parto de Norda Maro
        <fnt>
          <aut>Hans Hillewaert, propra verko</aut>
          <lok><url ref="&WCW;/File:Hippocampus_hippocampus_(on_Ascophyllum_nodosum).jpg">&WCW;/File:Hippocampus_hippocampus_(on_Ascophyllum_nodosum).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_s;&c_k;&c_ib; &c_k;&c_o;&c_n;&c_ib;&c_k;</trd>
  <trd lng="de">Seepferdchen</trd>
  <trd lng="fr">cheval de mer</trd>
  <trd lng="id"><ind>kuda</ind> laut</trd>
  <trd lng="nl">zeepaardje</trd>
  <trdgrp lng="pl">
    <trd>konik morski</trd>,
    <trd>p&lstroke;awikonik</trd>
  </trdgrp>
  <trd lng="ro">c&abreve;lu&#x21B; de mare</trd>
  <trd lng="uk">&c_m;&c_o;&c_r;&c_s;&c_mol;&c_k;&c_i;&c_j; &c_k;&c_o;&c_n;&c_i;&c_k;</trd>
