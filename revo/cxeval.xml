<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>&ccirc;eval</rad>/o
</kap>
<drv mrk="cxeval.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Kvarpiedulo (<trd lng="la">Equus przewalskii</trd>), 
      uzata kiel jung- k rajdbesto.
    </dif>
  </snc>
  <trd lng="cs">k&ugrave;&ncaron;</trd>
  <trd lng="de">Pferd</trd>
  <trd lng="en">horse</trd>
  <trd lng="fr">cheval</trd>
  <trd lng="tr">at</trd>
  <trd lng="nl">paard</trd>
</drv>
<drv mrk="cxeval.0eto">
  <kap><tld/>eto</kap>
  <snc>
    <uzo tip="klr">(f)</uzo>
    <dif> 
      Amata&jcirc;o, preferata okupi&gcirc;o:
      <ekz>
        amata <tld/>eto
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>.
      </ekz> 
    </dif>
    <refgrp tip="sin">
      <ref cel="mani.0o">manio</ref>, 
      <ref cel="sxat.0okupo">&scirc;atokupo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de"><trd>Steckenpferd</trd>, <trd>Hobby</trd></trdgrp>
<trdgrp lng="nl"><trd>stokpaardje</trd>, <trd>hobby</trd></trdgrp>
</drv>
<drv mrk="cxeval.0povovapor0o">
  <kap><tld/>povo, vapor<tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      (HP a&ubreve; CV).
      Konvencia unuo de forto en mekaniko, egala je la teoria forto necesa
      por levi 75 kg &gcirc;is 1 m dum 1 sekundo.  
    </dif>
  </snc>
  <trd lng="de">Pferdest&auml;rke</trd>
  <trd lng="fr">cheval vapeur</trd>
  <trd lng="nl">paardekracht</trd>
</drv>
</art>
