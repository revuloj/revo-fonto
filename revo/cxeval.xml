<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>&ccirc;eval</rad>/o
    </kap>
    <drv mrk="cxeval.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <uzo tip="fak">ZOO</uzo>
      <snc mrk="cxeval.0o.genro">
        <dif>
          Genro el familio
          <ref tip="super" cel="cxeval.0edoj"><tld/>edoj</ref>
          (<trd lng="la">Equus</trd>) kaj aparte la hejma <tld/>o
          (devenanta de <trd lng="la">Equus przewalskii</trd>), 
          kun multaj rasoj kaj uzata kiel jung- kaj rajdbesto.
        </dif>
      </snc>
      <trd lng="cs">k&ugrave;&ncaron;</trd>
      <trd lng="de">Pferd</trd>
      <trd lng="en">horse</trd>
      <trd lng="fr">cheval</trd>
      <trd lng="nl">paard</trd>
      <trd lng="pl">ko&nacute;</trd>
      <trdgrp lng="ru"><trd>&#x43a;&#x43e;&#x43d;&#x44c;</trd>,
        <trd>&#x43b;&#x43e;&#x448;&#x430;&#x434;&#x44c;</trd></trdgrp>
      <trd lng="tr">at</trd>
    </drv>
    <drv mrk="cxeval.0edoj">
      <kap><tld/>edoj</kap>
      <uzo tip="fak">ZOO</uzo>
      <snc>
        <dif>
          Familio el la ordo
          <ref tip="super" cel="huf.nepar0uloj">neparhufuloj</ref>
          (<trd lng="la">Equidae</trd>), kun nefenditaj hufoj.
        </dif>
        <ref tip="sin" cel="huf.unu0uloj">unuhufuloj</ref>
      </snc>
      <trdgrp lng="de">
        <trd>Pferde</trd>,
        <trd>Einhufer</trd>
      </trdgrp>
    </drv>
    <drv mrk="cxeval.0eto">
      <kap><tld/>eto</kap>
      <snc>
        <uzo tip="stl">FIG</uzo>
        <dif> 
          Temo pri kiu koncernulo &ccirc;iam volonte parolas, kiu &ccirc;iam lin
          interesas:
          <ekz>
            tio estas lia amata <tld/>eto
            <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
          </ekz><ekz>
            li tuj ekrajdis sian amatan <tld/>eton pri la supersigna
            reformo;
          </ekz><ekz>
            la facileco de Volap&uuml;k estas la plej amata <tld/>eto,
            sur kiu ili elveturas al &ccirc;iu okazo
            <fnt><aut>&Z;</aut>, <lok>citite la&ubreve; PIV, p.158</lok>.
            </fnt>
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="mani.0o">manio</ref>, 
          <ref cel="obsed.0o">obsedo</ref>
        </refgrp>
        <ref tip="vid" cel="sxat.0okupo">&scirc;atokupo</ref>
      </snc>
      <trdgrp lng="de"><trd>Steckenpferd</trd>, <trd>Hobby</trd></trdgrp>
      <trd lng="en">hobbyhorse</trd>
      <trdgrp lng="nl"><trd>stokpaardje</trd>, <trd>hobby</trd></trdgrp>
      <trd lng="ru">&#x43a;&#x43e;&#x43d;&#x451;&#x43a;</trd>
    </drv>
    <drv mrk="cxeval.0povo">
      <kap><tld/>povo</kap>
      <uzo tip="fak">TEK</uzo>
      <snc>
        <dif>
          (&Ccirc;P).
          Malnova nesistema unuo de povumo en me&hcirc;aniko, 1kg&#xd7;m/s
          = 735,5W en E&ubreve;ropo, a&ubreve; 550futpundoj/s = 746W en Usono.
        </dif>
      </snc>
      <trd lng="de">Pferdest&auml;rke</trd>
      <trd lng="fr">cheval vapeur</trd>
      <trd lng="nl">paardekracht</trd>
      <trd lng="ru">&#x43b;&#x43e;&#x448;&#x430;&#x434;&#x438;&#x43d;&#x430;&#x44f; &#x441;&#x438;&#x43b;&#x430;</trd>
    </drv>
  </art>
