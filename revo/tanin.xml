<art mrk="$Id: tanin.xml,v 1.19 2023/09/04 17:49:39 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>tanin</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tanin.0o">
  <kap><tld/>o</kap>
  <snc mrk="tanin.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Organika substanco trovi&gcirc;anta en multaj vegeta&jcirc;oj kaj
      vinoj, &ccirc;efa aganto &ccirc;e tanado:
      <ekz>
        la varmega akvo eligis el la kverka&jcirc;o <tld/>on &ndash; kaj
        tiu 
        nigras kiel inko
        <fnt>
          <aut>J. Kozarac, trad. D. Klobu&ccaron;ar</aut>,
          <vrk><url ref="http://public.carnet.hr/esperanto-kel/slavonia_arbaro.pdf">
          Slavonia Arbaro</url></vrk>,
          <lok>Kroata Esperanto-Ligo, 2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_n;&c_ib;&c_n;</trd>
  <trd lng="ca">tan&iacute;</trd>
  <trdgrp lng="cs">
    <trd>tanin</trd>,
    <trd>t&#x159;&#xED;slovina</trd>
  </trdgrp>
  <trd lng="de">Tannin</trd>
  <trd lng="en">tannin</trd>
  <trd lng="es">tanino</trd>
  <trd lng="fr">tanin</trd>
  <trd lng="hu">tannin</trd>
  <trd lng="ja">&#x30BF;&#x30F3;&#x30CB;&#x30F3;</trd>
  <trdgrp lng="nl">
    <trd>tannine</trd>,
    <trd>looistof</trd>
  </trdgrp>
  <trd lng="pl">tanina<klr> (garbnik galasowy)</klr></trd>
  <trdgrp lng="pt">
    <trd>t&acirc;nico</trd>,
    <trd>tanino</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_n;&c_i;&c_n;</trd>
  <trd lng="sk">tan&#xED;n</trd>
  <trd lng="uk">&#x442;&#x430;&#x43D;&#x456;&#x43D; (&#x434;&#x443;&#x431;&#x438;&#x43B;&#x44C;&#x43D;&#x430; &#x440;&#x435;&#x447;&#x43E;&#x432;&#x438;&#x43D;&#x430;)</trd>
</drv>
</art>
