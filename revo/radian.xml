<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>radian</rad>/o
</kap>
<drv mrk="radian.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de
      <ref cel="angul.ebena0o">ebena angulo</ref> en la Internacia Sistemo de
      unuoj, egala al <ref cel="angul.centra0o">centra angulo</ref>, kiu
      <ref cel="trancx.de0i.MAT">detran&ccirc;as</ref> 
      sur la <ref cel="cirkl.0o.linio">cirklo</ref>
      <ref cel="ark.0o">arkon</ref> egalan al la radiuso; 
      simb. <frm>rad</frm> a&ubreve; <frm>rd</frm>:
      <ekz>
        la mezuro de stre&ccirc;ita angulo estas <frm>&pi; rad</frm>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_d;&c_y;&c_ja;&c_n;</trd>
  <trd lng="ca">radiant</trd>
  <trd lng="de">Radiant</trd>
  <trd lng="en">radian</trd>
  <trd lng="es">radi&aacute;n</trd>
  <trd lng="fr">radian<klr tip="ind"> (unit&eacute; de mesure)</klr></trd>
  <trd lng="hu">radi&aacute;n</trd>
  <trd lng="nl">radian</trd>
  <trd lng="pl">radian</trd>
  <trd lng="ru">&c_r;&c_a;&c_d;&c_i;&c_a;&c_n;</trd>
</drv>
</art>

