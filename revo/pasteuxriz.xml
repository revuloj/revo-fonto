<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pasteuxriz.xml,v 1.13 2022/03/25 12:53:15 revo Exp $">
<kap>
  <rad>paste&ubreve;riz</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="pasteuxriz.0o">
  <kap><tld/>o</kap>
  <snc mrk="pasteuxriz.0o.KUI">
  <uzo tip="fak">BIO</uzo>
  <uzo tip="fak">KUI</uzo>
    <dif>
      Metodo de neniigo de
      <ref tip="vid" cel="mikrob.0o">mikroboj</ref>
      en trinka&jcirc;oj kaj man&gcirc;a&jcirc;oj per
      unufoja varmigo &gcirc;is temperaturo malpli 100&#x2DA;C (plej ofte
      60-70&#x2DA;C), proponita de
      <ref tip="vid" cel="pasteuxr.0o">Paste&ubreve;ro</ref>.
      <tld lit="P"/>o estas uzata por konservado de lakto, vino, biero
      kc.
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="steril.0igi">steriligi</ref>
  </refgrp>
  <trd lng="be">&c_p;&c_a;&c_s;&c_t;&c_e;&c_r;&c_y;&c_z;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_p;&c_a;&c_s;&c_t;&c_mol;&c_o;&c_r;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trd lng="de">Pasteurisation</trd>
  <trdgrp lng="en">
    <trd>pasteurization <klr>(usone)</klr></trd>,
    <trd>pasteurisation <klr>(brite)</klr></trd>
  </trdgrp>
  <trd lng="fa">&f_p;&a_A;&a_s;&a_t;&a_w;&a_r;&f_y;&a_z;&a_A;&a_s;&f_y;&a_w;&a_n;</trd>
  <trd lng="fr">pasteurisation</trd>
  <trd lng="hu">paszt&odblac;r&ouml;z&eacute;s</trd>  
  <trd lng="nl">pasteurisatie</trd>
  <trd lng="pl">pasteryzacja</trd>
  <trd lng="pt">pasteuriza&ccedil;&atilde;o</trd>
  <trd lng="ro">pasteurizare</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_je;&c_r;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trd lng="uk">&#x43F;&#x430;&#x441;&#x442;&#x435;&#x440;&#x438;&#x437;&#x430;&#x446;&#x456;&#x44F;</trd>
</drv>

<drv mrk="pasteuxriz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pasteuxriz.0i.KUI">
  <uzo tip="fak">BIO</uzo>
  <uzo tip="fak">KUI</uzo>
    <dif>
      Trakti per varmo bieron, vinon, lakton ktp, la&ubreve; la metodo,
      proponita de Paste&ubreve;ro:
      <ekz>
        <ind><tld/>ita</ind> lakto
        <trd lng="be">&c_p;&c_a;&c_s;&c_t;&c_e;&c_r;&c_y;&c_z;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
        <trd lng="hu">paszt&odblac;r&ouml;z&ouml;tt</trd>
        <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_je;&c_r;&c_i;&c_z;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_t;&c_e;&c_r;&c_y;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">pasteurisieren</trd>
  <trdgrp lng="en"><trd>to pasteurize</trd>,
  <trd>pasteurise</trd></trdgrp>
  <trd lng="fa">&f_p;&a_A;&a_s;&a_t;&a_w;&a_r;&f_y;&a_z;&a_h; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">pasteuriser</trd>
  <trdgrp lng="hu">
    <trd>paszt&odblac;r&ouml;z</trd>,
    <trd>paszt&ouml;riz&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">pasteuriseren</trd>
  <trd lng="pl">pasteryzowa&cacute;</trd>
  <trd lng="ro">pasteuriza</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_je;&c_r;&c_i;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="uk">&#x43F;&#x430;&#x441;&#x442;&#x435;&#x440;&#x438;&#x437;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
</drv>
</art>
