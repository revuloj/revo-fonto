<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lunati.xml,v 1.15 2020/02/28 21:10:42 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lunatik</rad>/o
</kap>
<drv mrk="lunati.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      <ref tip="dif" cel="somnam.0o">Somnambulo</ref>:
       <ekz>
        tage kaj nokte li estis kiel <tld/>o kaj ne sciis, &ccirc;u li estas
        en la &ccirc;ielo a&ubreve;
        sur la tero
        <fnt>
          <bib>Monato</bib>
          <aut>Julian Modest</aut>
          <vrk><url ref="&Monato;/2013/010962.php"
              >La iluziisto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_l;&c_u;&c_n;&c_a;&c_t;&c_y;&c_k;</trd>,
      <trd>&c_s;&c_a;&c_m;&c_n;&c_a;&c_m;&c_b;&c_u;&c_l;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Schlafwandler</trd>,
      <trd>Monds&uuml;chtiger</trd>
    </trdgrp>
    <trd lng="en">sleepwalker</trd>
    <trd lng="fr">somnanbule</trd>
    <trdgrp lng="hu">
      <trd>holdk&oacute;r&oacute;s</trd>,
      <trd>alvaj&aacute;r&oacute;</trd>
    </trdgrp>
    <trd lng="nl">slaapwandelaar</trd>
    <trdgrp lng="pl">
      <trd>lunatyk</trd>,
      <trd>somnambulik</trd>
    </trdgrp> 
    <trd lng="pt">son&acirc;mbulo</trd>
    <trdgrp lng="ro">
      <trd>lunatic</trd>,
      <trd>somnambul</trd>
    </trdgrp>
    <trd lng="ru">&c_l;&c_u;&c_n;&c_a;&c_t;&c_i;&c_k;</trd>
  </snc>
  <snc>
    <dif>
      Homo ege kaprica kaj &scirc;an&gcirc;humora.
    </dif>
    <trdgrp lng="de">
      <trd>schrulliger Kauz</trd>,
      <trd>Maniker</trd>
    </trdgrp>
    <trd lng="en">lunatic</trd>
    <trd lng="fr">lunatique<klr> (subst.)</klr></trd>
    <trdgrp lng="hu">
      <trd>h&oacute;bortos alak</trd>,
      <trd>szesz&eacute;lyes ember</trd>
    </trdgrp>
    <trd lng="nl">grillig persoon</trd>
  </snc>
  </drv>
</art>
