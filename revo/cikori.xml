<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>cikori</rad>/o
</kap>
<drv mrk="cikori.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="aster.0acoj">asteracoj</ref>.
      (<trd lng="la">Cichorium</trd>).
      La ordinara <tld/>o estis kulturata pro la radiko uzata kiel kafsurogato.
    </dif>
    <ref tip="sub" cel="endivi.0o">endivio</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Chicoree</trd>,
    <trd>Wegwarte</trd>,
    <trd>Zichorie</trd>
  </trdgrp>
  <trd lng="fr">chicor&eacute;e</trd>
  <trd lng="ru">&c_c;&c_i;&c_k;&c_o;&c_r;&c_i;&c_j;</trd>
</drv>
</art>
