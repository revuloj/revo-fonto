<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>cikori</rad>/o
</kap>
<drv mrk="cikori.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="astera.0oj">asteracoj</ref>.
      (<trd lng="la">Cichorium</trd>), uzata kiel legomo.
      La ordinara <tld/>o estis kulturata pro la radiko uzata
      kiel kafsurogato.
    </dif>
    <ref tip="sub" cel="endivi.0o">endivio</ref>
  </snc>
  <bld lok="../bld/cikori.jpg">
    <ind>ordinara <tld/>o</ind>
    <trd lng="de">Gemeine Wegwarte</trd>
  <trdgrp lng="hu">
    <trd>mezei <ind>kat&aacute;ng</ind></trd>,
    <trd>kat&aacute;ngk&oacute;r&oacute;</trd>
 </trdgrp>
    <trd lng="nl">cichorei</trd>
    <trd lng="la">Cichorium intybus</trd>
  </bld>
  <trd lng="be">&c_c;&c_y;&c_k;&c_o;&c_r;&c_y;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Chicoree</trd>,
    <trd>Wegwarte</trd>,
    <trd>Zichorie</trd>
  </trdgrp>
  <trd lng="el">&rho;&alfa;&delta;&jota_ton;&kappa;&jota;</trd>
  <trd lng="fr">chicor&eacute;e</trd>
  <trdgrp lng="hu">
    <trd>cik&oacute;ria</trd>,
    <trd>kat&aacute;ng</trd>
 </trdgrp>
  <trdgrp lng="nl">
    <trd>witlof</trd>,
    <trd>witloof</trd>
 </trdgrp>
 <trd lng="pt">chic&oacute;ria</trd>
 <trd lng="ru">&c_c;&c_i;&c_k;&c_o;&c_r;&c_i;&c_j;</trd>
  <trd lng="sv">cikoria</trd>
</drv>
</art>
