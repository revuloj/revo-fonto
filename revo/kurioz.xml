<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kurioz</rad>/a
</kap>
<drv mrk="kurioz.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <ref tip="super" cel="strang.0a">Strangeta</ref>, interese nekutima;
      vidinda, rimarkinda:
      <ekz>
        <tld/>a besto, spektaklo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>seltsam</trd>,
    <trd>sonderbar</trd>,
    <trd>merkw&uuml;rdig</trd>,
    <trd>wunderlich</trd>,
    <trd>kurios</trd>
  </trdgrp>
  <trd lng="fr">curieux<klr> (digne d'int&eacute;r&ecirc;t)</klr></trd>
  <trdgrp lng="hu">
    <trd>&eacute;rdekes</trd>,
    <trd>furcsa</trd>,
    <trd>saj&aacute;tos</trd>,
    <trd>k&uuml;l&ouml;n&ouml;s</trd>,
    <trd>k&uuml;l&ouml;nleges</trd>
  </trdgrp>
  <trd lng="nl">vreemd<klr tip="ind"> (eigenaardig)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_r;&c_mol;&c_jo;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_l;&c_ju;&c_b;&c_o;&c_p;&c_y;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_n;&c_ja;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="kurioz.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Strangeta afero, kiu logas atenton:
      <ekz>
        kolekti <tld/>a&jcirc;ojn.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">curiosit&eacute;<klr> (objet int&eacute;ressant)</klr></trd>
  <trdgrp lng="hu">
    <trd>&eacute;rdekess&eacute;g</trd>,
    <trd>furcsas&aacute;g</trd>,
    <trd>saj&aacute;toss&aacute;g</trd>,
    <trd>k&uuml;l&ouml;nlegess&eacute;g</trd>,
    <trd>nevezetess&eacute;g</trd>,
    <trd>kuri&oacute;zum</trd>
  </trdgrp>
</drv>
</art>
