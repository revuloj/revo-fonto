</kap>
<drv mrk="kurioz.0a">
  <kap><tld/>a</kap>
  <snc mrk="kurioz.0a.strangeta">
    <dif>
      <ref tip="super" cel="strang.0a">Strangeta</ref>, interese
      nekutima; vidinda, rimarkinda:
      <ekz>
        <tld/>a besto, spektaklo;
      </ekz>
      <ekz>
        <tld/>a trompo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la &jcirc;iloj dancas sur la stratoj de la ankora&ubreve; dormanta urbo kaj <klr>[&#x2026;]</klr>
        surhavas <tld/>ajn tradiciajn maskojn
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/006955.html">Belgio: Fama karnavalo en Binche</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        tre <tld/>e (por ni), <nom>Dante</nom> evidente supozis, ke liaj legantoj malfacile
        komprenos la novan poezion
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2004/008327.html">Pozitivaj kaj negativaj aspektoj de Dante</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    Ne uzu la vorton en la senco <ref cel="sci.0vola">scivola</ref>.
  </rim>
  <trdgrp lng="be">
    <trd>&c_k;&c_u;&c_r;'&c_jo;&c_z;&c_n;&c_y;</trd>,
    <trd>&c_d;&c_z;&c_ib;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_c;&c_ib;&c_k;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_z;&c_a;&c_b;&c_a;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kuri&oacute;zn&iacute;</trd>,
    <trd>neobvykl&yacute;</trd>,
    <trd>podivn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>seltsam</trd>,
    <trd>sonderbar</trd>,
    <trd>merkw&uuml;rdig</trd>,
    <trd>wunderlich</trd>,
    <trd>kurios</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>curious</trd>,
    <trd>odd</trd>,
    <trd>quaint</trd>,
    <trd>strange</trd>
  </trdgrp>
  <trd lng="fr">curieux<klr> (digne d'int&eacute;r&ecirc;t)</klr></trd>
  <trdgrp lng="hu">
    <trd>&eacute;rdekes</trd>,
    <trd>furcsa</trd>,
    <trd>saj&aacute;tos</trd>,
    <trd>k&uuml;l&ouml;n&ouml;s</trd>,
    <trd>k&uuml;l&ouml;nleges</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>curieus</trd>,
    <trd>vreemdsoortig</trd>,
    <trd>vreemd<klr tip="ind"> (eigenaardig)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ciekawy</trd>,
    <trd>dziwny</trd>,
    <trd>osobliwy</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>interesant</trd>,
    <trd>ciudat</trd>,
    <trd>neobi&#x219;nuit</trd>,
    <trd>curios</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_r;&c_mol;&c_jo;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_l;&c_ju;&c_b;&c_o;&c_p;&c_y;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_n;&c_ja;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kuri&oacute;zny</trd>,
    <trd>neoby&ccaron;ajn&yacute;</trd>,
    <trd>zvl&aacute;&scaron;tny</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x443;&#x440;&#x439;&#x43E;&#x437;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x434;&#x438;&#x432;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x43D;&#x435;&#x437;&#x432;&#x438;&#x447;&#x430;&#x439;&#x43D;&#x438;&#x439;</trd>
