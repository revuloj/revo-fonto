<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kurioz</rad>/a
</kap>
<drv mrk="kurioz.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Interese nekutima; vidinda, rimarkinda:
      <ekz>
        <tld/>a besto, spektaklo;
      </ekz>
      <ekz>
        kolekti plej strangajn <tld/>a&jcirc;ojn.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>seltsam</trd>,
     <trd>sonderbar</trd>,
     <trd>merkw&uuml;rdig</trd>,
     <trd>wunderlich</trd>,
     <trd>kurios</trd>
  </trdgrp>
  <trd lng="nl">vreemd<klr tip="ind"> (eigenaardig)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_r;&c_mol;&c_jo;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_l;&c_ju;&c_b;&c_o;&c_p;&c_y;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_n;&c_ja;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
