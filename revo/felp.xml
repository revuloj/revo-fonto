  <rad>felp</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="felp.0o">
  <kap><tld/>o</kap>
  <snc mrk="felp.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref cel="velur.0a.TEKS">Velur</ref>simila <ref tip="super"
      cel="teks.0ajxo.TEKS">teksa&jcirc;o</ref>, kun pli longaj plate ku&scirc;antaj haroj,
      iam uzita ekz-e por la tega&jcirc;o de <ref cel="cxapel.cilindro0o">cilindro&ccirc;apelo</ref>j:
      <ekz>
        li estis meza&gcirc;a kampulo, malbone vestita, kun
        &scirc;rumpinta bluvelura jako kaj pantalono el flava
        velur<tld/>o
        <fnt>
          <aut>V. Catal&agrave;, trad. J. Ventura i Freixas</aut>,
          <vrk>Soleco</vrk>,
          <lok>1967</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="plusx.0o">plu&scirc;o</ref>
    <adm>
      Kia diferenco kun plu&scirc;o? PIV1 difinis ilin amba&ubreve; idente.
      <aut>MB</aut>
    </adm>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ju;&c_sh;</trd>
  <trdgrp lng="cs">
    <trd>felba</trd>,
    <trd>hedv&aacute;bn&yacute; ply&scaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Felbel</trd>,
     <trd>Halbsamt</trd></trdgrp>
  <trdgrp lng="es">
    <trd>felpa</trd>,
    <trd>peluche</trd>
  </trdgrp>
  <trd lng="fr">peluche<klr tip="ind"> (tissu)</klr></trd>
  <trdgrp lng="hu">
     <trd>f&eacute;lb&aacute;rsony</trd>,
     <trd>pl&uuml;ss</trd></trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30D5;&#x30E9;&#x30B7;&#22825;  <pr>&#x30D5;&#x30E9;&#x30B7;&#12390;&#12435;</pr></trd>,
    <trd>&#x30D7;&#x30E9;&#x30C3;&#x30B7;&#x30E5;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_ju;&c_sh;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_mol;&c_v;&c_je;&c_t;&c_i;&c_n;</trd>,
    <trd>&c_p;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="sk">hodv&aacute;bny ply&scaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43B;&#x44E;&c_sh;</trd>,
    <trd>&c_v;&c_je;&#x43B;&#x44C;&c_v;&c_je;&c_t;&c_i;&#x43D;</trd>,
    <trd>&#x43F;&c_a;&#x43D;</trd>
  </trdgrp>
</drv>
