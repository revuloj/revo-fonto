<vortaro>
<art mrk="$Id$">
<kap>
<rad>felp</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="felp.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Velursimila teksa&jcirc;o, kun pli longaj plate ku&scirc;antaj haroj.  
    </dif>
    <ref tip="vid" cel="plusx.0o">plu&scirc;o.</ref>
    <adm>
    Kia diferenco kun plusxo? PIV1 difinis ilin ambaux idente. <aut>MB</aut>
    </adm>
  </snc>
  <trdgrp lng="de">
     <trd>Felbel</trd>,
     <trd>Halbsamt</trd></trdgrp>
  <trdgrp lng="es">
    <trd>felpa</trd>,
    <trd>peluche</trd>
  </trdgrp>
  <trd lng="fr">peluche<klr tip="ind"> (tissu)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_ju;&c_sh;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_mol;&c_v;&c_je;&c_t;&c_i;&c_n;</trd>,
    <trd>&c_p;&c_a;&c_n;</trd>
  </trdgrp>
</drv>
</art>
