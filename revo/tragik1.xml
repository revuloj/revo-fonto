<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>tragikomedi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tragik1.0o">
  <kap><tld/>o</kap>
  <snc mrk="tragik1.0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="super" cel="teatr.0ajxo.TEA">Teatra&jcirc;o</ref>
      mezvoja inter tragedio kaj komedio,
