  <trd lng="be">&c_t;&c_r;&c_a;&c_g;&c_ib;&c_k;&c_a;&c_m;&c_e;&c_d;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_t;&c_r;&c_a;&c_g;&c_i;&c_k;&c_o;&c_m;&c_je;&c_d;&c_i;&c_ja;</trd>
  <trd lng="de">Tragikom&ouml;die</trd>
  <trd lng="en">tragicomedy</trd>
  <trd lng="fr">tragicom&eacute;die</trd>
  <trd lng="hu">tragikom&eacute;dia</trd>
  <trd lng="nl">tragikomedie</trd>
  <trd lng="pt">tragicom&eacute;dia</trd>
  <trd lng="ru">&c_t;&c_r;&c_a;&c_g;&c_i;&c_k;&c_o;&c_m;&c_je;&c_d;&c_i;&c_ja;</trd>
</drv>
</art>
