<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Neper</rad>/o
</kap>
<drv mrk="neper.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Anglalingve: <trd lng="en">John <ind>Neper</ind></trd> (a&ubreve; Napier),
      1550-1617. Angla matematikisto.
    </dif>
  </snc>
  <trd lng="fr">Neper</trd>
  <trd lng="nl">John <ind>Neper</ind></trd>
  <trd lng="ru">&c_D;&c_zh;&c_o;&c_n; <ind>&c_N;&c_je;&c_p;&c_je;&c_r;</ind></trd>
</drv>

<drv mrk="neper.n0o">
  <kap><tld lit="n"/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Praktika <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de
      kvociento de la elira al la enira <ref cel="pov.0o.FIZ">povo</ref>
      en fizika sistemo; simb. Np:
      <ekz>
        la mezuro en <tld lit="n"/>oj de kvociento de potencoj egalas al
        la duono de la <tld lit="n"/>a logaritmo de tiu kvociento.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Neper</trd>
  <trd lng="fr">n&eacute;per<klr tip="ind"> (unit&eacute; de mesure)</klr></trd>
  <trd lng="nl">neper</trd>
  <trd lng="ru">&c_n;&c_je;&c_p;&c_je;&c_r;</trd>
</drv>

<drv mrk="neper.n0a">
  <kap><tld lit="n"/>a</kap>
  <snc>
    <refgrp tip="vid">
      <ref cel="logari.nepera0o"><tld lit="n"/>a logaritmo</ref>.
    </refgrp>
    <rim>
      Ne konfuzu kun <ctl>ne-pera</ctl> (MatVort).
    </rim>
  </snc>
  <trd lng="de">Nepersche</trd>
  <trd lng="en">Napierian</trd>
  <trd lng="fr">n&eacute;p&eacute;rien</trd>
  <trd lng="nl">Neperiaans</trd>
  <trd lng="ru">&c_n;&c_je;&c_p;&c_je;&c_r;&c_o;&c_v;</trd>
</drv>
</art>
