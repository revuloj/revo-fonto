<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>doktor</rad>/o
</kap>
<drv mrk="doktor.0o">
   <kap><tld/>o</kap>
   <mlg>d-ro</mlg>
   <snc mrk="doktor.0o.PED">
   <uzo tip="fak">PED</uzo>
     <dif>
       Titolo de persono ricevinta la plej altan universitatan rangon.
     </dif>
     <refgrp tip="vid">
       <ref cel="abitur.0o">abituriento</ref>,
       <ref cel="licenc">licenciulo</ref>,
       <ref cel="magist.0o">magistro</ref>.
     </refgrp>
     <trd lng="be">&c_d;&c_o;&c_k;&c_t;&c_a;&c_r; <klr tip="amb">(&c_n;&c_a;&c_v;&c_u;&c_k;)</klr></trd>
     <trd lng="de">Doktor</trd>
     <trd lng="fr">docteur<klr tip="ind"> (titre)</klr></trd>
     <trd lng="hu">doktor</trd>
     <trd lng="nl">doctor</trd>
     <trd lng="ru">&c_d;&c_o;&c_k;&c_t;&c_o;&c_r; <klr>(&c_n;&c_a;&c_u;&c_k;)</klr></trd>
   </snc>
   <snc mrk="doktor.0o.REL">
     <uzo tip="fak">REL</uzo>
     <dif>
       Homo, kiu instruas doktrinon:
       <ekz>
         <ind>la <tld/>oj de la Eklezio</ind>.
         <trd lng="hu">egyh&aacute;zaty&aacute;k</trd>
       </ekz>
     </dif>
     <trd lng="de">Lehrer<klr> (einer Doktrin)</klr></trd>
     <trd lng="fr">docteur<klr tip="ind"> (de la loi)</klr></trd>
     <trdgrp lng="hu">
       <trd>tud&oacute;s</trd>,
       <trd>gondolkod&oacute;</trd>
     </trdgrp>
     <trd lng="nl">doctor</trd>
   </snc>
   <snc mrk="doktor.0o.MED">
     <uzo tip="fak">MED</uzo>
     <uzo tip="stl">EVI</uzo>
     <ref tip="dif" cel="kurac.0isto">kuracisto</ref>
     <trdgrp lng="be">
       <trd>&c_d;&c_o;&c_k;&c_t;&c_a;&c_r;</trd>,
       <trd>&c_l;&c_je;&c_k;&c_a;&c_r;</trd>
     </trdgrp>
     <trdgrp lng="de">
       <trd>Doktor</trd>,
       <trd>Arzt</trd>
     </trdgrp>
     <trdgrp lng="hu">
       <trd>orvos</trd>,
       <trd>doktor</trd>
     </trdgrp>
   </snc>
   <trd lng="de">Doktor</trd>
   <trd lng="en">doctor</trd>
   <trd lng="es">doctor</trd>
</drv>
</art>
