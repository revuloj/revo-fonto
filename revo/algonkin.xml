<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: algonkin.xml,v 1.4 2024/07/29 06:35:50 revo Exp $">
<kap>
  <rad>algonkin</rad>/o
</kap>
<drv mrk="algonkin.0o">
  <kap><tld/>o</kap>
  <snc mrk="algonkin.0o.ETN">
    <dif>
      <ref tip="super" cel="indian.0o.ETN">Indiano</ref>
      el popolo vivanta en Kebekio kaj Ontario.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_m;&c_a;&c_m;&c_ib;&c_v;&c_ib;&c_n;&c_ib;&c_n;&c_ib;</trd>,
    <trd>&c_a;&c_l;&c_g;&c_a;&c_n;&c_k;&c_ib;&c_n;</trd>
  </trdgrp>
  <trd lng="de">Algonkin</trd>
  <trd lng="en">Algonquin</trd>
  <trd lng="fr">Algonquin<klr> (subst.)</klr></trd>
</drv>

<drv mrk="algonkin.0a">
  <kap><tld/>a</kap>
  <snc mrk="algonkin.0a.ETN">
    <dif>
      Rilata al <tld/>oj:
      <ekz>
        la menomina apartenas al la <tld/>a <klr>...</klr>
        lingvofamilio
        <fnt>
        <aut>K. Miner</aut>,
        <vrk><url
        ref="http://www.sunflower.com/~miner/PRIMI_package/primi.html">
        Pri mi</url></vrk>,
        <lok>2003-08-24</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_g;&c_a;&c_n;&c_k;&c_ib;&c_n;&c_s;&c_k;&c_ib;</trd>
  <trd lng="de">Algonkin-</trd>
  <trd lng="en">Algonquin</trd>
  <trd lng="fr">algonquin<klr> (adj.)</klr></trd>
</drv>
</art>

