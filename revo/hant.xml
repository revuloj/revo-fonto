<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hant</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="hant.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="hant.0i.MAG">
    <ref tip="dif" cel="fantom.0i.MAG">fantomi</ref>
      <ekz>
        la Navarina rodo estus nur &ccirc;arma pejza&gcirc;o, sen la fantomoj,
        kiuj <tld/>as &ccirc;i tie &ccirc;iujn &scirc;tonojn
        <fnt><aut>E. de Zilah</aut>, <vrk>Vivi sur barko</vrk>, 
        <lok>1994</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la kadavro de Ma&ubreve;ma&ubreve; da&ubreve;re <tld/>as mian fantazion
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
  </snc>
  <trdgrp lng="cs">
    <trd>děsit <klr>(přízrak)</klr></trd>,
    <trd>strašit</trd>
  </trdgrp>
  <trd lng="fr">hanter</trd>
  <trdgrp lng="sk">
    <trd>desiť <klr>(prízrak)</klr></trd>,
    <trd>strašiť</trd>
  </trdgrp>
</drv>
</art>

