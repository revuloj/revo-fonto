<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hant.xml,v 1.14 2025/12/22 02:28:36 revo Exp $">
<kap>
  <rad>hant</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="hant.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="hant.0i.MAG">
    <dif>
      <ref tip="dif" cel="fantom.0i.MAG">Fantomi</ref>:
      <ekz>
        la Navarina rodo estus nur &ccirc;arma pejza&gcirc;o, sen la fantomoj,
        kiuj <tld/>as &ccirc;i tie &ccirc;iujn &scirc;tonojn
        <fnt><aut>E. de Zilah</aut>, <vrk>Vivi sur barko</vrk>,
        <lok>1994</lok></fnt>;
      </ekz>
      <ekz>
        fantoma &ccirc;ashundo, kiu <tld/>as dumnokte la anojn de la familio Baskerville
        <fnt>
          <bib>LOdE</bib>
          <aut>Wolfgang Kirschstein</aut>
          <lok>"Oni neniam tedi&gcirc;as prl la erikejo", 1999. No 3 (53)</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;iun nokton revenis por <tld/>i ilin la sama timo
        <fnt>
          <bib>Monato</bib>
          <aut>Angela Tellier</aut>
          <vrk><url ref="&Monato;/2004/008306.html">La&ubreve;proverbaj fabeloj</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la kadavro de <nom>Ma&ubreve;ma&ubreve;</nom> da&ubreve;re <tld/>as mian fantazion
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="persek.0i.turmenti">persekuti<sncref/></ref>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>teruri</ke>, <ke>fantom(ad)i</ke> (&ccirc;e iu/ie a&ubreve;
    ion/iun), <ke>vizitadi</ke> (fare de fantomoj), <ke>aperadi</ke> (&ccirc;e iu/ie), <ke>persekuti</ke>,
    <ke>turment(ad)i</ke>.
  </rim>
  <trdgrp lng="en">
    <trd>haunt <klr>(as a ghost)</klr></trd>,
    <trd>haunt <klr>(make uneasy)</klr></trd>
  </trdgrp>
  </snc>

  <snc mrk="hant.0i.FIG">
    <dif>
      Viziti ofte:
      <ekz>
        mi <tld/>as laboristajn drinkejojn
        <fnt>
          <bib>Monato</bib>
          <aut>Higinio Garc&iacute;a</aut>
          <lok>"El la atesto de Mustafao Vanesavir&#x2019;", 2001/03, p. 25</lok>
        </fnt>;
      </ekz>
      <ekz>
        li kaj mi kun<tld/>as la barcelonan prekongresan forumon
        <fnt>
          <bib>MMa</bib>
          <aut>Jorge Camacho</aut>
          <lok>&Ccirc;apitro 9</lok>
        </fnt>;
      </ekz>
    </dif>
    <trd lng="en">haunt <klr>(frequent a location)</klr></trd>
  </snc>

  <trd lng="be">&c_z;&c_mol;&c_ja;&c_w;&c_l;&c_ja;&c_c;&c_c;&c_a; &c_ja;&c_k; &c_p;&c_r;&c_y;&c_v;&c_ib;&c_d;</trd>
  <trdgrp lng="cs">
    <trd>d&ecaron;sit <klr>(p&rcaron;&iacute;zrak)</klr></trd>,
    <trd>stra&scaron;it</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>heimsuchen</trd>,
    <trd>spuken</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>aparecerse<klr> (un fantasma)</klr></trd>,
    <trd>atormentar</trd>
  </trdgrp>
  <trd lng="fr">hanter</trd>
  <trd lng="pl">nawiedzi&cacute; jako <ind>duch</ind></trd>
  <trdgrp lng="sk">
    <trd>desi&tcaron; <klr>(pr&iacute;zrak)</klr></trd>,
    <trd>stra&scaron;i&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&#x2019;&c_ja;&c_v;&c_l;&c_ja;&c_t;&c_i;&c_s;&c_ja; (&c_ja;&c_k; &c_p;&c_r;&c_i;&c_v;&c_i;&c_d;)</trd>,
    <trd>&c_p;&c_r;&c_i;&c_v;&c_i;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_s;&c_l;&c_ib;&c_d;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x4FB5;&#x88AD; <pr>q&imacron;nx&iacute;</pr></trd>
</drv>
</art>

