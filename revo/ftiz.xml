<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ftiz</rad>/o
</kap>
<drv mrk="ftiz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="sin" cel="tuberk.0o">Tuberkulozo</ref>, precipe de la pulmoj.  
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Schwindsucht</trd>,
     <trd>Tuberkulose</trd></trdgrp>
  <trdgrp lng="fr">
    <trd>phtisie</trd>,
    <trd>tuberculose</trd></trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_t;&c_u;&c_b;&c_je;&c_r;&c_k;&c_u;&c_l;&c_jo;&c_z;</trd>,
    <trd>&c_ch;&c_a;&c_h;&c_o;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
