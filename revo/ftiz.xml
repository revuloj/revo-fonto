<kap><ofc>*</ofc>
<rad>
  ftiz</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="ftiz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="ftiz.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="sin" cel="tuberk.0o">Tuberkulozo</ref>,
      precipe de la pulmoj:
      <ekz>
        se mi estus hejme neniofara, ili murmurus,
        ke mi estas <tld/>a
        <fnt>
          <aut>Hasegawa T.</aut>,
          <vrk><url ref="http://wikilivres.ca/wiki/En_%C4%88inio_Batalanta">
          En &Ccirc;inio Batalanta</url></vrk>, 
          <lok>1954</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_h;&c_o;&c_t;&c_y;</trd>,
    <trd>&c_t;&c_u;&c_b;&c_e;&c_r;&c_k;&c_u;&c_l;&c_jo;&c_z;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>tisi</trd>,
    <trd><ind>tuberculosi</ind> pulmonar</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schwindsucht</trd>,
    <trd>Tuberkulose</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>phthisis</trd>,
    <trd>consumption</trd>,
    <trd>pulmonary tuberculosis</trd>
  </trdgrp>
  <trd lng="es">tisis</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_kasra;&a_l;&a_shadda;</trd>,
    <trd>&a_t;&a_b; &a_l;&a_A;&a_z;&a_m;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>phtisie</trd>,
    <trd>tuberculose</trd>
  </trdgrp>
  <trdgrp lng="hu">
  <trd>g&uuml;m&odblac;k&oacute;r</trd>,
    <trd>tbc</trd>,
  <trd>turberkul&oacute;zis</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>ftisis</trd>,
    <trd>TBC</trd>,
    <trd>tuberkulosis</trd>
  </trdgrp>
  <trd lng="it">tisi</trd>
  <trd lng="ja">&#x80BA;&#x7D50;&#x6838;  <pr>&#x306F;&#x3044;&#x3051;&#x3063;&#x304B;&#x304F;</pr></trd>
  <trdgrp lng="nl">
    <trd>ftisis</trd>,
    <trd>tering</trd>,
    <trd>longtering</trd>,
    <trd>turberculose</trd>,
    <trd>longtuberculose</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>gru&zacute;lica</trd>,
    <trd>suchoty</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>tuberculose pulmonar</trd>,
    <trd>pulmotuberculose</trd>,
    <trd>t&iacute;sica</trd>
  </trdgrp>
  <trd lng="ro">tuberculoz&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_u;&c_b;&c_je;&c_r;&c_k;&c_u;&c_l;&c_jo;&c_z;</trd>,
    <trd>&c_ch;&c_a;&c_h;&c_o;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x441;&#x443;&#x445;&#x43E;&#x442;&#x438;</trd>,
    <trd>&#x442;&#x443;&#x431;&#x435;&#x440;&#x43A;&#x443;&#x43B;&#x44C;&#x43E;&#x437; &#x43B;&#x435;&#x433;&#x435;&#x43D;&#x44C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7635; <pr>zh&#xE0;i</pr></trd>,
    <trd>&#x7D50;&#x6838; <pr>ji&#xE9;h&#xE9;</pr></trd>
  </trdgrp>
</drv>
