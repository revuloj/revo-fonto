<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>ftiz</rad>/o</kap>
<drv mrk="ftiz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="sin" cel="tuberk.0o">Tuberkulozo</ref>, precipe de la
      pulmoj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_h;&c_o;&c_t;&c_y;</trd>,
    <trd>&c_t;&c_u;&c_b;&c_e;&c_r;&c_k;&c_u;&c_l;&c_jo;&c_z;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schwindsucht</trd>,
    <trd>Tuberkulose</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>phthisis</trd>,
    <trd>consumption</trd>,
    <trd>pulmonary tuberculosis</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>phtisie</trd>,
    <trd>tuberculose</trd>
  </trdgrp>
  <trdgrp lng="hu">
  <trd>g&uuml;m&odblac;k&oacute;r</trd>,
    <trd>tbc</trd>,
  <trd>turberkul&oacute;zis</trd>
  </trdgrp>
  <trd lng="it">tisi</trd>
  <trdgrp lng="nl">
    <trd>ftisis</trd>,
    <trd>tering</trd>,
    <trd>longtering</trd>,
    <trd>turberculose</trd>,
    <trd>longtuberculose</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>tuberculose pulmonar</trd>,
    <trd>pulmotuberculose</trd>,
    <trd>t&iacute;sica</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_u;&c_b;&c_je;&c_r;&c_k;&c_u;&c_l;&c_jo;&c_z;</trd>,
    <trd>&c_ch;&c_a;&c_h;&c_o;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
