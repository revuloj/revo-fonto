<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>drup</rad>/o
</kap>
<drv mrk="drup.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="frukt.ferm0o">Fermfrukto</ref>, kies
      perikarpo tavoli&gcirc;as al interna malmola kerno kaj
      ekstera suke karna (pruno, &ccirc;erizo) a&ubreve; ledeca
      (juglando, migdalo) parto.
    </dif>
    <ref tip="sin" cel="kern.0ofrukto">kernofrukto</ref>
  </snc>
  <trd lng="de">Steinfrucht</trd>
  <trdgrp lng="en">
    <trd>drupe</trd>,
    <trd>stone fruit</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_s;&c_t;&c_ja;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_s;&c_t;&c_o;&c_ch;&c_k;&c_o;&c_v;&c_y;&c_j; &c_p;&c_l;&c_o;&c_d;</trd>
  </trdgrp>
</drv>
</art>
