<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: drup.xml,v 1.21 2023/11/04 18:50:52 revo Exp $">
<kap>
<rad>drup</rad>/o
</kap>
<drv mrk="drup.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="frukt.ferm0o">Fermfrukto</ref>, kies
      perikarpo tavoli&gcirc;as al interna malmola <ref tip="prt" cel="kern.0o.BOT">kerno</ref>
      kaj ekstera suke karna (<ref tip="sub" cel="prun.0o">pruno</ref>,
      <ref tip="sub" cel="cxeriz.0o">&ccirc;erizo</ref>) a&ubreve; ledeca
      (<ref tip="sub" cel="juglan.0o">juglando</ref>, 
      <ref tip="sub" cel="migdal.0o">migdalo</ref>) parto:
      <ekz>
        juglando <klr>[&#x2026;]</klr> evoluas el sube staranta ovario, 
        kiu estas <tld/>o kun ekstere verda, karneca, tavolo defalanta je maturi&gcirc;o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=22305">Juglando (genro)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;erizo <klr>[&#x2026;]</klr> estas <tld/>o, en la fruktokarno ku&scirc;as unu kerno kun diametro de &ccirc;.
        0,5 cm
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=23200">&Ccirc;erizarbo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="kern.0ofrukto">kernofrukto</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_o;&c_s;&c_t;&c_a;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_n;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>drupa</trd>,
    <trd>peckovice</trd>
  </trdgrp>
  <trd lng="de">Steinfrucht</trd>
  <trdgrp lng="en">
    <trd>drupe</trd>,
    <trd>stone fruit</trd>
  </trdgrp>
  <trd lng="es">drupa</trd>
  <trd lng="fr">drupe</trd>
  <trd lng="hu">csonth&eacute;jas gy&uuml;m&ouml;lcs</trd>
  <trd lng="ja">&#x77F3;&#x679C;  <pr>&#x305B;&#x304D;&#x304B;</pr></trd>
  <trd lng="pl">pestkowiec</trd>
  <trd lng="pt">drupa</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_s;&c_t;&c_ja;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_s;&c_t;&c_o;&c_ch;&c_k;&c_o;&c_v;&c_y;&c_j; &c_p;&c_l;&c_o;&c_d;</trd>
  </trdgrp>
  <trd lng="sk">k&ocirc;stkovit&eacute; ovocie</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_ib;&c_s;&c_t;&#x44F;&#x43D;&#x43A;&c_a;</trd>,
    <trd>&#x43A;&c_ib;&c_s;&c_t;&#x43E;&c_ch;&#x43A;&#x43E;&c_v;&c_i;&c_j; &#x43F;&#x43B;&c_ib;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6838;&#x679C; <pr>h&#xE9;gu&#x1D2;</pr></trd>,
    <trd>&#x82CC; <pr>ch&#xE1;ng</pr></trd>
  </trdgrp>
</drv>
</art>
