<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Sirdarj</rad>/o
    </kap>

    <drv mrk="sirdarj.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
	<dif>
	  Granda rivero de la
	  <ref tip="malprt" cel="azi.InternaA0o">Interna Azio</ref>,
	  fluanta el la Fergana Valo al (la Malranda)
	  <ref cel="aral.0o">Aralo</ref>.
	</dif>
      </snc>
      <trdgrp lng="en">
	<trd>Syr Darya</trd>,
	<trd>Jaxartes</trd>,
	<trd>Yaxartes</trd>
      </trdgrp>
      <trdgrp lng="ru">
	<trd>&c_S;&c_y;&c_r;&c_d;&c_a;&c_r;&c_mol;&c_ja;</trd>,
	<trd>&c_Ja;&c_k;&c_s;&c_a;&c_r;&c_t;</trd>
      </trdgrp>
    </drv>

  </art>

