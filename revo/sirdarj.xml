<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sirdarj.xml,v 1.14 2023/12/02 21:23:05 revo Exp $">
<kap>
  <rad>Sirdarj</rad>/o <fnt><bib>BoER</bib></fnt>
</kap>

<drv mrk="sirdarj.S0o">
  <kap><tld/>o</kap>
  <snc mrk="sirdarj.S0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Granda <ref tip="lst" cel="river.0o" lst="voko:riveroj">rivero</ref> de la
      <ref tip="malprt" cel="azi.InternaA0o">Interna Azio</ref>,
      fluanta el la Fergana Valo al (la Malgranda)
      <ref cel="aral.A0o">Aralo</ref>:
      <ekz>
        la 2212 kilometrojn longa rivero <tld/>o en la antikva epoko <klr>[&#8230;]</klr>
        priskribita inter alie de la kronikisto Tito Livio, situas en Centra Azio kaj
        enfluas la lagon Aralo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=336096">Sirdarjo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        por havi sufi&ccirc;an akvon por <klr>[&#8230;]</klr> kultivo de kotono, granda kvanto el la
        akvo de la riveroj Amudarjo kaj <tld/>o estis forkondukita en sennombrajn
        kanaletojn, tiel ke multe maltro da rivera akvo atingas la lagon Aralo, kiu
        sekve pli kaj pli malaperas
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=336985">To&#x2BB;rtko&#x2BB;l</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_S;&c_y;&c_r;&c_d;&c_a;&c_r;'&c_ja;</trd>
  <trd lng="ca">Sirdari&agrave;</trd>
  <trd lng="de">Syr-Darja</trd>
  <trdgrp lng="en">
   <trd>Syr Darya</trd>,
   <trd>Jaxartes</trd>,
   <trd>Yaxartes</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_s;&f_y;&a_r;&a_d;&a_r;&f_y;&a_A;</trd>,
    <trd>&a_s;&f_y;&a_H;&a_w;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">Syr-Daria</trd>
  <trd lng="hu">Szir-darja</trd>
  <trd lng="pl">Syr-daria</trd>
  <trdgrp lng="ru">
    <trd>&c_S;&c_y;&c_r;&c_d;&c_a;&c_r;&c_mol;&c_ja;</trd>,
    <trd>&c_Ja;&c_k;&c_s;&c_a;&c_r;&c_t;</trd>
  </trdgrp>
  <trd lng="uk">&c_S;&c_i;&c_r;&c_d;&c_a;&c_r;&#8217;&#x44F;</trd>
</drv>
</art>
