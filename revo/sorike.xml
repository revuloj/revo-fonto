<art mrk="$Id: sorike.xml,v 1.10 2023/09/28 21:21:41 revo Exp $">
<kap>
  <rad>soriked</rad>/oj
</kap>
<drv mrk="sorike.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt" cel="mangx.insekto0uloj">insektoman&gcirc;uloj</ref>
      (<trd lng="la">Soricidae</trd>), kun rostrosimila bu&scirc;o:
      <ekz>
        <klr>[en]</klr> Etiopaj montaraj arbustetaroj <klr>[&#8230;]</klr> abundas la ron&gcirc;uloj
        kaj la <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=443346">Etiopaj montaraj
          arbustetaroj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="sorik.0o">soriko</ref>
  </snc>
  <trd lng="be">&c_z;&c_je;&c_m;&c_l;&c_ja;&c_r;&c_y;&c_j;&c_k;&c_ib; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Spitzm&auml;use</trd>
  <trd lng="en">shrews</trd>
  <trd lng="hu">cick&aacute;nyf&eacute;l&eacute;k</trd>
  <trd lng="nl">spitsmuizen</trd>
  <trdgrp lng="pl">
    <trd>sorki</trd>,
    <trd>ryj&oacute;wkowate </trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_je;&c_m;&c_l;&c_je;&c_r;&c_o;&c_j;&c_k;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&c_z;&c_je;&#x43C;&#x43B;&c_je;&c_r;&c_i;&c_j;&#x43A;&#x43E;&c_v;&c_ib;</trd>
</drv>
</art>
