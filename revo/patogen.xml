<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: patogen.xml,v 1.3 2024/06/27 08:31:17 afido Exp $">
    <kap>
      <rad>patogen</rad>/a
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="patogen.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Malsaniga:
      <ekz>
        aidoso pro difektigo de imunstato akcelas la disvasti&gcirc;on de <klr>[&#8230;]</klr> aliaj
        malsanoj, multaj kutime ne <tld/>aj kaj kondi&ccirc;o-<tld/>aj mikroboj fari&gcirc;as tre
        dan&gcirc;eraj kaj ka&ubreve;zas aidoso-ligitajn malsanojn
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2004/008278.html">Damokla glavo super la homaro</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        la infektan (a&ubreve; <tld/>an) prionan proteinon oni signas per &leftquot;<nom>PrPSc</nom>&rightquot;
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2006/008861.php">Nova etapo en biologio kaj medicino</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve;difine, la homa imuna sistemo defendas onin kontra&ubreve; eksteraj kaj <tld/>aj
        elementoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2013/010953.php">Zorga gardisto nenion preteratentas</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">pathogen</trd>
    <trd lng="en">pathogen</trd>
    <trd lng="fr">pathog&egrave;ne</trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_t;&c_a;&c_g;&c_je;&c_n;&c_n;&c_y;</trd>
  <trdgrp lng="zh">
    <trd>&#x81F4;&#x75C5;&#24615; <pr>zh&#xEC;b&#xEC;ngx&#xEC;ng</pr></trd>,
    <trd>&#x81F4;&#x75C5; <pr>zh&#xEC;b&#xEC;ng</pr></trd>
  </trdgrp>
</drv>

  </art>

