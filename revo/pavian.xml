<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>pavian</rad>/o
</kap>
<drv mrk="pavian.0o">
  <kap><tld/>o</kap>
  <snc mrk="pavian.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Mezgranda <ref tip="lst" cel="simi.0o"
      lst="voko:simioj">simio</ref> 
      kun longa hundosimila muzelo, proksimaj okuloj, kurba vosto kaj
      senhara 
      pugo, vivanta en Afriko inklude de Egipto kaj Sudano;
            <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio
      <ref tip="malprt" cel="cerkop1.0oj">cerkopitekedoj</ref>
      (<trd lng="la">Papio</trd>):
      <ekz>
        <tld/>oj kaj aliaj simioj &scirc;atas malfermi tiun &ccirc;i frukton por
        man&gcirc;i la pulpon
        <fnt>
        <aut>d. pardue</aut>,
        <vrk>Re: Baobabo</vrk>,
        <lok>soc.culture.esperanto, 1996-05-09</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_v;&c_ib;&c_ja;&c_n;</trd>
  <trd lng="de">Pavian</trd>
  <trd lng="en">baboon</trd>
  <trd lng="fr">babouin</trd>
  <trd lng="hu">p&aacute;vi&aacute;n</trd>
  <trd lng="id">babun</trd>
  <trd lng="nl">baviaan</trd>
  <trd lng="pt">babu&iacute;no</trd>
  <trd lng="ru">&c_p;&c_a;&c_v;&c_i;&c_a;&c_n;</trd>
</drv>
</art>
