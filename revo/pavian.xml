<kap>
  <ofc>2</ofc>
  <rad>pavian</rad>/o
</kap>
<drv mrk="pavian.0o">
  <kap><tld/>o</kap>
  <snc mrk="pavian.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Mezgranda <ref tip="lst" cel="simi.0o" lst="voko:simioj">simio</ref> 
      kun longa hundosimila muzelo, proksimaj okuloj, kurba vosto kaj
      senhara pugo, vivanta en Afriko inklude de Egipto kaj Sudano;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio
      <ref tip="malprt" cel="cerkop1.0oj">cerkopitekedoj</ref>
      (<trd lng="la">Papio</trd>):
      <ekz>
        desegnita <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="&Monato;/2007/009009.php">Ruza leporo kaj &ccirc;iuj aliaj</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj kaj aliaj simioj &scirc;atas malfermi tiun &ccirc;i frukton por
        man&gcirc;i la pulpon
        <fnt>
        <aut>d. pardue</aut>,
        <vrk>Re: Baobabo</vrk>,
        <lok>soc.culture.esperanto, 1996-05-09</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/d/dd/Baviaan2.JPG/600px-Baviaan2.JPG" prm="PD"><tld/>familio, Parko Dieren en Emmen
        <fnt>
          <aut>Dick Mudde, propra verko</aut>
          <lok><url ref="&WCW;/File:Baviaan2.JPG">&WCW;/File:Baviaan2.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_v;&c_ib;&c_ja;&c_n;</trd>
  <trd lng="cs">pavi&aacute;n</trd>
  <trd lng="de">Pavian</trd>
  <trd lng="en">baboon</trd>
  <trd lng="fr">babouin</trd>
  <trd lng="hu">p&aacute;vi&aacute;n</trd>
  <trd lng="id">babun</trd>
  <trd lng="ja">&#x30D2;&#x30D2;</trd>
  <trd lng="nl">baviaan</trd>
  <trd lng="pl">pawian</trd>
  <trd lng="pt">babu&iacute;no</trd>
  <trd lng="ro">babuin</trd>
  <trd lng="ru">&c_p;&c_a;&c_v;&c_i;&c_a;&c_n;</trd>
  <trd lng="sk">pavi&aacute;n</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x430;&#x432;&#x456;&#x430;&#x43D;</trd>,
    <trd>&#x431;&#x430;&#x431;&#x443;&#x457;&#x43D;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x72D2; <pr>f&#xE8;i</pr></trd>,
    <trd>&#x72D2;&#x72D2; <pr>f&#xE8;if&#xE8;i</pr></trd>,
    <trd>&#x72D2;&#x72D2;&#x5C5E; <pr>f&#xE8;if&#xE8;ish&#x1D4;</pr></trd>,
    <trd>&#x72D2;&#x72D2;&#x5C6C; <pr>f&#xE8;if&#xE8;ish&#x1D4;</pr></trd>
