<drv mrk="tie.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="tie.0a.KOMUNE">
    <dif>
      Tiuloka, <tld/> situanta:
      <ekz>
        ekzemple: tiama, &ccirc;iama, kioma, <tld/>a, &ccirc;i-<tld/>a, <tld/>ulo,
        tiamulo
        <fnt><bib>F</bib>, <lok>&FE; 30</lok></fnt>;
      </ekz>
      <ekz>
        <nom>Jeremia</nom> iris <klr>[&#x2026;]</klr> en la landon de <nom>Benjamen</nom>, por ricevi sian
        <tld/>an
        hereda&jcirc;on
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 37:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        Jesuo jam formovi&gcirc;is pro la <tld/>a homamaso
        <fnt>
          <bib>NT</bib>
          <lok>Johano 5:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        en <nom>Antio&hcirc;ia</nom>, en la <tld/>a eklezio
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 13:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <klr>[&#x2026;]</klr> restis en la universitata urbo por
        <klr>[&#x2026;]</klr> fari publikan esperanto-kurson al <tld/>aj
        lo&gcirc;antoj
        <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro 3a, p.
        19a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">local<klr> (from there)</klr></trd>
  <trd lng="be">&c_t;&c_a;&c_m;&c_t;&c_e;&c_j;&c_sh;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>tamn&iacute;</trd>,
    <trd>tam&ecaron;j&scaron;&iacute;</trd>
  </trdgrp>
  <trd lng="es">local<klr> (del lugar)</klr></trd>
  <trd lng="fr">local<klr> (de l&agrave;-bas)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x305D;&#x3053;&#x306E;</trd>,
    <trd>&#x305D;&#x306E;&#x571F;&#x5730;&#x306E;  <pr>&#x305D;&#x306E;&#x3068;&#x3061;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>tamten</trd>,
    <trd>tamtejszy</trd>
  </trdgrp>
  <trd lng="ro">local</trd>
  <trd lng="ru">&c_t;&c_a;&c_m;&c_o;&c_sh;&c_n;&c_i;&c_j;</trd>
  <trd lng="sk">tamoj&scaron;&iacute;</trd>
  <trd lng="uk">&c_t;&c_a;&c_m;&c_t;&c_je;&c_sh;&c_n;&c_ib;&c_j;</trd>
  <trd lng="zh">&#x672C;&#x5730; <pr>b&#x11B;nd&#xEC;</pr></trd>
</drv>

