<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tie</rad>
</kap>
<drv mrk="tie.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Adverbo signifanta &leftquot;en tiu loko&rightquot;:
      <ekz>
        kion vi faras <tld/>?<fnt>Z</fnt>
      </ekz>
      <ekz>
        <tld/>n metu lavujon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        sendu <tld/>n por rigardi<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li maltrankvile pa&scirc;as tra la &ccirc;ambro <tld/>n kaj
        reen<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la vorto &leftquot;&gcirc;ardenon&rightquot; staras <tld/>
        &ccirc;i en akuz.<fnt>Z</fnt>;
      </ekz>
      <ekz>
        se ni iam post prepozicio uzas la akuz-on, la akuz. <tld/>
        dependas ne de la prepozicio, sed de aliaj
        ka&ubreve;zoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ofte kunvenas personoj de malsamaj nacioj, sed <tld/> komprenas
        sin reciproke nur tre malgranda parto da
        kunvenintoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        oni suferas <tld/>, kie oni estas vundita;
      </ekz>
      <ekz>
        kie estas mielo, <tld/> mu&scirc;oj ne mankas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        kie regas la forto, <tld/> rajto silentas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        kie dento doloras, <tld/>n iras la lango
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi estas &ccirc;i-<tld/>ulo?
      </ekz>
    </dif>
  <trd lng="be">&c_t;&c_a;&c_m;</trd>
  <trdgrp lng="de">
    <trd>da</trd>,
    <trd>dort</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>l&agrave;<klr> (&agrave; distance)</klr></trd>,
    <trd>l&agrave;-bas</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>daar</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>tam</trd>,
    <trd>&oacute;wdzie</trd>,
    <trd>tam&zdot;e</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_m;</trd>
  <trd lng="vo">us</trd>
  </snc>
</drv>

<drv mrk="tie.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Tiuloka, tie situanta:
      <ekz>
        mi <klr>...</klr> restis en la universitata urbo por
        <klr>...</klr> fari publikan esperanto-kurson al <tld/>aj
        lo&gcirc;antoj
        <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro 3a, p.
        19a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_m;&c_t;&c_e;&c_j;&c_sh;&c_y;</trd>
  <trd lng="fr">local<klr> (de l&agrave;-bas)</klr></trd>
  <trdgrp lng="pl">
    <trd>tamten</trd>,
    <trd>tamtejszy</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_m;&c_o;&c_sh;&c_n;&c_i;&c_j;</trd>
</drv>

<drv mrk="tie.0.cxi">
  <kap><tld/> &ccirc;i</kap>
  <snc>
    <ref tip="dif" cel="tie.cxi0">&ccirc;i tie</ref>
    <ekz>
      for de <tld/> &ccirc;i<fnt><bib>F</bib>, <lok>&FE; 26</lok></fnt>!
    </ekz>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_t;</trd>
  <trd lng="de">hier</trd>
  <trdgrp lng="fr">
    <trd>ici</trd>,
    <trd>l&agrave;<klr> (proche)</klr></trd>
  </trdgrp>
  <trd lng="nl">hier</trd>
  <trdgrp lng="pl">
    <trd>tu</trd>,
    <trd>tutaj</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_d;&c_je;&c_s;&c_mol;</trd>,
    <trd>&c_t;&c_u;&c_t;</trd>
  </trdgrp>
  <trd lng="vo">is</trd>
</drv>

<drv mrk="tie.cxi0">
  <kap>&ccirc;i <tld/></kap>
  <snc>
    <dif>
      En tiu (rilate al la parolanto) proksima loko.
    </dif>
    <ref tip="sin" cel="tie.0.cxi"><tld/> &ccirc;i</ref>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_t;</trd>
  <trd lng="de">hier</trd>
  <trdgrp lng="fr">
    <trd>ici</trd>,
    <trd>l&agrave;<klr> (proche)</klr></trd>
  </trdgrp>
  <trd lng="nl">hier</trd>
  <trdgrp lng="pl">
    <trd>tu</trd>,
    <trd>tutaj</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_d;&c_je;&c_s;&c_mol;</trd>,
    <trd>&c_t;&c_u;&c_t;</trd>
  </trdgrp>
  <trd lng="vo">is</trd>
</drv>
</art>
