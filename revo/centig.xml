<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: centig.xml,v 1.20 2019/02/16 17:10:19 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>centigram</rad>/o
</kap>
<drv mrk="centig.0o">
  <kap><tld/>o</kap>
  <mlg kod="SI">cg</mlg>
  <snc>
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de maso,
      centono de <ref tip="malprt" cel="gram.0o">gramo</ref>:
      <ekz>
        pilolo malpli peza ol kvin <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=244113">Granulo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_a;&c_n;&c_t;&c_y;&c_g;&c_r;&c_a;&c_m;</trd>
    <trd lng="ca">centigram</trd>
    <trd lng="el">&epsilon;&kappa;&alfa;&tau;&omikron;&sigma;&tau;&rho;&omikron_ton;&gamma;&rho;&alfa;&my;&my;&alfa;</trd>
    <trd lng="en">centigram</trd>
    <trd lng="es">centigramo</trd>
    <trd lng="fa">&a_s;&a_A;&a_n;&a_t;&f_y;&f_g;&a_r;&a_m;</trd>
    <trd lng="fr">centigramme</trd>
    <trd lng="hu">centigramm</trd>
    <trd lng="it">centigrammo</trd>
    <trd lng="nl">centigram</trd>
    <trd lng="pl">centygram</trd>
    <trd lng="pt">centigrama</trd>
    <trd lng="ru">&c_s;&c_a;&c_n;&c_t;&c_i;&c_g;&c_r;&c_a;&c_m;&c_m;</trd>
    <trd lng="sk">centigram</trd>
  </snc>
  <trd lng="cs">centigram</trd>
<trd lng="de">Centigramm</trd>
</drv>
</art>
