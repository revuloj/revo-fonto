<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: samizdat.xml,v 1.8 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>samizdat</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>

<drv mrk="samizdat.0o">
  <kap><tld/>o</kap>
  <snc mrk="samizdat.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Eldona&jcirc;o de malpermesita literaturo ka&scirc;e farita per amatora
      rimedo:
      <ekz>
        estis eldonitaj &leftquot;samizdate&rightquot; multaj a&ubreve;toraj kantaroj
        <fnt>
          <bib>Jamburg</bib>,
          <lok>pri la a&ubreve;toro, p. 135a</lok>
        </fnt>;
      </ekz>
      <ekz>
        post la falo de la fera kurteno multaj el la <tld/>aj verkoj
        estis
        normale publikigitaj en gazetoj a&ubreve; libroforme
        <fnt>
          <bib>Viki</bib>,
          <lok><url
          ref="http://eo.wikipedia.org/wiki/Samizdato">artikolo
          &leftquot;samizdato&rightquot;</url>, 2005-11-29</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_m;&c_v;&c_y;&c_d;&c_a;&c_t;</trd>
  <trd lng="bg">&c_S;&c_a;&c_m;&c_i;&c_z;&c_d;&c_a;&c_t;</trd>
  <trd lng="cs">samizdat</trd>
  <trd lng="de">Samisdat</trd>
  <trd lng="en">Samizdat</trd>
  <trd lng="fr">samizdat</trd>
  <trd lng="it">samizdat</trd>
  <trd lng="pl">wydawnictwo podziemne</trd>
  <trdgrp lng="pt">
   <trd>samizdat</trd>
  </trdgrp>
  <trd lng="sk">samizdat</trd>
</drv>
</art>

