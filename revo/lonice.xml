<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>lonicer</rad>/o
</kap>
<drv mrk="lonice.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="kaprif.0acoj">kaprifoliacoj</ref>
      (<trd lng="la">Lonicera</trd>),
      grimpanta arbeto kun tordaj tubformaj bonodoraj floroj.
    </dif>
    <ref tip="sub" cel="kaprif.0o">kaprifolio</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Heckenkirsche</trd>,
    <trd>Gei&szlig;blatt</trd>
  </trdgrp>
</drv>
</art>
