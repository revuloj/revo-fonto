<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lonicer</rad>/o
</kap>
<drv mrk="lonice.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super"
      cel="kaprif1.0oj">kaprifoliacoj</ref>
      (<trd lng="la">Lonicera</trd>),
      grimpanta arbeto kun tordaj tubformaj bonodoraj floroj.
    </dif>
    <ref tip="sub" cel="kaprif.0o">kaprifolio</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Heckenkirsche</trd>,
    <trd>Gei&szlig;blatt</trd>
  </trdgrp>
  <trd lng="hu">lonc</trd>
  <trd lng="nl">kamperfolie</trd>
  <trd lng="ru">&c_zh;&c_i;&c_m;&c_o;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>
</art>
