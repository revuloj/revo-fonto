   <trd lng="be">&c_b;&c_u;&c_k;&c_o;&c_l;&c_ib;&c_k;&c_a;</trd>
   <trdgrp lng="es">
     <trd>buc&oacute;lico<klr> (sust.)</klr></trd>,
