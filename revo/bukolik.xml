<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bukolik.xml,v 1.9 2023/08/15 16:32:40 revo Exp $">
<kap>
   <rad>bukolik</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="bukolik.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">POE</uzo>
   <snc>
     <dif>
       <ref tip="super" cel="poezi.0o.arto">Poezio</ref> de pa&scirc;tistoj;
       <ref tip="dif" cel="eklog.0o">eklogo</ref>:
       <ekz>
         mirindaj <tld/>oj kantataj kaj reprezentataj de li
         mem<fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 24a, p. 175a</lok></fnt>;
       </ekz>
      <ekz>
        latinlingvaj reprezentantoj de la <tld/>o estas <nom>Calpurnius Siculus</nom> kaj
        <nom>Nemesian</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=219261">Bukoliko</url></lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_b;&c_u;&c_k;&c_o;&c_l;&c_ib;&c_k;&c_a;</trd>
  <trd lng="cs">bukolika</trd>
  <trd lng="de">Bukolik</trd>
  <trd lng="en">bucolic</trd>
  <trdgrp lng="es">
    <trd>buc&oacute;lico<klr> (sust.)</klr></trd>,
    <trd>&eacute;gloga</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bucolique<klr> (subst.)</klr></trd>,
    <trd>&eacute;glogue</trd>
  </trdgrp>
  <trd lng="it">bucolica</trd>
  <trd lng="la">bucolicum</trd>
  <trd lng="ru">&c_b;&c_u;&c_k;&c_o;&c_l;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">pastierske b&aacute;sne</trd>
</drv>
</art>
