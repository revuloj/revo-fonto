<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gxin.xml,v 1.22 2023/08/31 14:29:56 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>&gcirc;in</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="gxin.0o">
  <kap><tld/>o</kap>
  <snc mrk="gxin.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="dif" cel="brand.juniper0o.KUI">Juniperbrando</ref>:
      <ekz>
        ili kolektis botelon enhavintan <tld/>on, skatoletojn por
        sardeloj, poton por konfita&jcirc;oj
        <fnt>
          <aut>Henri Vallienne</aut>,
          <vrk>Kastelo de Prelongo</vrk>,
          <lok>1907</lok>
        </fnt>;
      </ekz>
      <ekz>
        Martinio estas aperitiva koktelo farita el <tld/>o kaj blanka vermuto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=235788">Martinio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="gxin.0o.RIM_HOM">
    <ref tip="hom" cel="gxin1.0o">&Gcirc;ino</ref> estas anka&ubreve;
    nomo de speco de naturspirito kaj &ccirc;ina mezurunuo de pezo:
    <ref tip="hom" cel="gxin2.0o">&gcirc;ino</ref>.
  </rim>
  <trd lng="be">&c_d;&c_zh;&c_y;&c_n; <klr tip="amb"> 
(&c_n;&c_a;&c_p;&c_ib;&c_t;&c_a;&c_k;)</klr></trd>
  <trd lng="ca">ginebra</trd>
  <trdgrp lng="de">
    <trd>Wacholderbranntwein</trd>,
    <trd>Wacholderschnaps</trd>,
    <trd>Gin</trd>
  </trdgrp>
  <trd lng="en">gin</trd>
  <trd lng="es">ginebra</trd>
  <trd lng="fr">gin</trd>
  <trdgrp lng="hu">
    <trd>gin</trd>,
    <trd>bor&oacute;kap&aacute;linka</trd>
  </trdgrp>
  <trd lng="it">gin</trd>
  <trd lng="ja">&#x30B8;&#x30F3;</trd>
  <trd lng="nl">gin</trd>
  <trdgrp lng="pl">
    <trd>gin</trd>,
    <trd>d&zdot;in</trd>,
    <trd>ja&lstroke;owc&oacute;wka</trd>
  </trdgrp>
  <trd lng="pt">gim</trd>
  <trd lng="ru">&c_d;&c_zh;&c_i;&c_n;</trd>
  <trd lng="tr">cin<klr>(i&ccedil;ecek)</klr></trd>
  <trd lng="uk">&#x434;&#x436;&#x438;&#x43D;</trd>
</drv>
</art>
