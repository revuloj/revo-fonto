<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
  <rad>&gcirc;in</rad>/o
</kap>
<drv mrk="gxin.0o">
  <kap><tld/>o</kap>
  <snc mrk="gxin.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Greno<ref tip="super" cel="brand.0o.KUI">brando</ref> aromigita 
      per juniperaj beroj:
      <ekz>
        ili kolektis botelon enhavintan <tld/>on, skatoletojn por 
        sardeloj, poton por konfita&jcirc;oj
        <fnt>
          <aut>Henri Vallienne</aut>,
          <vrk>Kastelo de Prelongo</vrk>,
          <lok>1907</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_zh;&c_y;&c_n; <klr tip="amb">(&c_n;&c_a;&c_p;&c_ib;&c_t;&c_a;&c_k;)</klr></trd>
  <trd lng="ca">ginebra</trd>
  <trdgrp lng="de">
    <trd>Wacholderbranntwein</trd>,
    <trd>Wacholderschnaps</trd>,
    <trd>Gin</trd>
  </trdgrp>
  <trd lng="en">gin</trd>
  <trd lng="es">ginebra</trd>
  <trd lng="fr">gin</trd>
  <trdgrp lng="hu">
    <trd>gin</trd>,
    <trd>bor&oacute;kap&aacute;linka</trd>
  </trdgrp>
  <trd lng="nl">gin</trd>
  <trd lng="pt">gim</trd>
  <trd lng="ru">&c_d;&c_zh;&c_i;&c_n;</trd>
</drv>
</art>
