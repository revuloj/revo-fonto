<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>stal</rad>/o
</kap>
<drv mrk="stal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Lo&gcirc;ejo por brutoj:
      <ekz>
        malpura kiel <tld/>o;
      </ekz>
      <ekz>
        &ccirc;eval<tld/>o, bovo<tld/>o, &scirc;af<tld/>o, porko<tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Stall</trd>
  <trd lng="nl">stal</trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_l;&c_je;&c_v;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_j;&c_l;&c_o;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_ju;&c_sh;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="stal.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Homo, kies metio estas zorgi pri <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Stallknecht</trd>
  <trd lng="nl">stalknecht</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_n;&c_ju;&c_h;</trd>,
    <trd>&c_s;&c_k;&c_o;&c_t;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
