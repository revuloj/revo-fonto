<fnt><bib>JED</bib></fnt></kap>
<drv mrk="dokume.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Skriba&jcirc;o a&ubreve; signifa objekto, kiu servas kiel pruvo
      kaj
      atesto:
      <ekz>
        <tld/>oj pri la origino de la milito;
      </ekz>
      <ekz>
        genealogiaj <tld/>oj
        <fnt><bib>MT</bib> <lok>&Ezr; 2:62</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="arhxiv.0o">arkivo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_k;&c_u;&c_m;&c_e;&c_n;&c_t;</trd>
  <trd lng="bg">&c_d;&c_o;&c_k;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>
  <trd lng="bo">&#xF51;&#xF54;&#xF7A;&#xF0B;&#xF46;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>doklad</trd>,
    <trd>dokument</trd>,
    <trd>listina</trd>,
    <trd>listinn&yacute; pr&uring;kaz</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Urkunde</trd>,
    <trd>Dokument</trd>
  </trdgrp>
  <trd lng="en">document</trd>
  <trd lng="es">documento</trd>
  <trd lng="fr">document</trd>
  <trdgrp lng="hu">
    <trd>okm&aacute;ny</trd>,
    <trd>dokumentum</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6587;&#x66F8;  <pr>&#x3076;&#x3093;&#x3057;&#x3087;</pr></trd>,
    <trd>&#x8CC7;&#x6599;  <pr>&#x3057;&#x308A;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x8A18;&#x9332;  <pr>&#x304D;&#x308D;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">document</trd>
  <trd lng="no">dokument</trd>
  <trdgrp lng="pl">
    <trd>dokument</trd>,
    <trd>pismo</trd>
  </trdgrp>
  <trd lng="pt">documento</trd>
  <trd lng="ro">scris</trd>
  <trd lng="ru">&c_d;&c_o;&c_k;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>doklad</trd>,
    <trd>dokument</trd>,
    <trd>listina</trd>
  </trdgrp>
  <trd lng="uk">&c_d;&#x43E;&#x43A;&c_u;&#x43C;&c_je;&#x43D;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#x8CC7;&#x6599; <pr>z&#x12B;li&#xE0;o</pr></trd>,
    <trd>&#x7279;&#x8A31;&#x8B49; <pr>t&#xE8;x&#x1D4;zh&#xE8;ng</pr></trd>,
    <trd>&#x5951;&#x636E; <pr>q&#xEC;j&#xF9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="dokume.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="klr">(ion)</uzo>
    <dif>
      <ref tip="vid" cel="pruv.0i.KOMUNE_io">Pruvi<sncref/></ref> per <tld/>oj:
      <ekz>
        fotoj, po&scirc;tkartoj, geografiaj mapoj ... <tld/>as lian eksterordinaran voja&gcirc;on
        <fnt>
          <aut>-</aut>
          <vrk>Ina&ubreve;guro de la retejo pri la voja&gcirc;o de Lucien P&eacute;raire (1028-1932)</vrk>
          <lok><url ref="https://esperantolyon.org/2024/11/08/gis-cent-novembro-2024/">
          &Gcirc;is Cent, 2024-11</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">beurkunden</trd>
    <trd lng="pl">udokumentowywa&cacute;</trd>
  </snc>
  <snc>
    <uzo tip="klr">(iun)</uzo>
    <dif>
      Provizi per <tld/>oj.
    </dif>
    <trd lng="pl">dokumentowa&cacute;</trd>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_k;&c_u;&c_m;&c_e;&c_n;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>dokl&aacute;dat</trd>,
    <trd>dokumentovat</trd>,
    <trd>prok&aacute;zat</trd>
  </trdgrp>
  <trd lng="de">dokumentieren</trd>
  <trd lng="en">document</trd>
  <trd lng="es">documentar</trd>
  <trd lng="fr">documenter</trd>
  <trd lng="he">&#x5DC;&#x5EA;&#x5E2;&#x5D3;</trd>
  <trd lng="hu">dokument&aacute;l</trd>
  <trdgrp lng="ja">
    <trd>&#x8CC7;&#x6599;&#x3067;&#x8A3C;&#x660E;&#x3059;&#x308B;  <pr>&#x3057;&#x308A;&#x3087;&#x3046;&#x3067;&#x3057;&#x3087;&#x3046;&#x3081;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x8A18;&#x9332;&#x306B;&#x6B8B;&#x3059;  <pr>&#x304D;&#x308D;&#x304F;&#x306B;&#x306E;&#x3053;&#x3059;</pr></trd>
  </trdgrp>
  <trd lng="nl">documenteren</trd>
  <trd lng="no">dokumentere</trd>
  <trd lng="pt">documentar</trd>
  <trd lng="ru">&c_d;&c_o;&c_k;&c_u;&c_m;&c_je;&c_n;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">dokumentova&tcaron;</trd>
  <trd lng="uk">&c_d;&#x43E;&#x43A;&c_u;&#x43C;&c_je;&#x43D;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  <trd lng="zh">&#x6587;&#x732E; <pr>w&#xE9;nxi&#xE0;n</pr></trd>
</drv>

<drv mrk="dokume.0aro">
  <kap><tld/>aro</kap>
  <snc>
    <dif>
      Tuto de la <tld/>oj, uzataj por difinita celo.
    </dif>
  </snc>
  <snc>
    <dif>
      Tuto de la detalaj priskriboj, klarigoj k manlibroj de komputila
      programo.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_k;&c_u;&c_m;&c_e;&c_n;&c_t;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trd lng="de">Dokumentation</trd>
  <trd lng="en">documentation</trd>
  <trd lng="es">documentaci&oacute;n</trd>
  <trd lng="fr">documentation</trd>
  <trd lng="it">documentazione</trd>
  <trdgrp lng="ja">
    <trd>&#x4E00;&#x4EF6;&#x66F8;&#x985E;  <pr>&#x3044;&#x3063;&#x3051;&#x3093;&#x3057;&#x3087;&#x308B;&#x3044;</pr></trd>,
    <trd>&#x53C2;&#x8003;&#x8CC7;&#x6599;&#x96C6;  <pr>&#x3055;&#x3093;&#x3053;&#x3046;&#x3057;&#x308A;&#x3087;&#x3046;&#x3057;&#x3085;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">documentatie</trd>
  <trd lng="pl">dokumentacja</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&#x43E;&c_s;&#x44C;&c_jeu;</trd>,
    <trd>&c_s;&#x43F;&c_r;&c_a;&c_v;&c_a; (&#x43A;&c_a;&#x43D;&c_c;&c_je;&#x43B;&#x44F;&c_r;&c_s;&#x44C;&#x43A;&c_a;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6587;&#x4EF6;&#x5F59;&#x7DE8; <pr>w&#xE9;nji&#xE0;nhu&#xEC;bi&amacron;n</pr></trd>,
    <trd>&#x7528;&#x4F5C;&#x8B49;&#x64DA;&#x7684;&#x6587;&#x4EF6;
    <pr>y&#xF2;ngzu&#xF2;zh&#xE8;ngj&#xF9;dew&#xE9;nji&#xE0;n</pr></trd>,
    <trd>&#x6587;&#x4EF6;&#x6574;&#x7406; <pr>w&#xE9;nji&#xE0;nzh&#x11B;ngl&#x1D0;</pr></trd>,
    <trd>&#x8A18;&#x9304;&#x7247; <pr>j&#xEC;l&#xF9;pi&#xE0;n</pr></trd>,
    <trd>&#x6587;&#x732E;&#x8D44;&#x6599; <pr>w&#xE9;nxi&#xE0;nz&#x12B;li&#xE0;o</pr></trd>,
    <trd>&#x8B49;&#x66F8; <pr>zh&#xE8;ngsh&#x16B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="dokume.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <dif>
      Plata, ortangula portebla ujo, el ledo a&ubreve; plasto, por enteni
      <tld/>ojn.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_e;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_p;&c_k;&c_a; <klr>(&c_d;&c_l;&c_ja; &c_d;&c_a;&c_k;&c_u;&c_m;&c_e;&c_n;&c_t;&c_a;&c_w;)</klr></trd>
  </trdgrp>
  <trd lng="en">briefcase</trd>
  <trd lng="fr">porte-document(s)</trd>
  <trd lng="ja">&#x66F8;&#x985E;&#x7BB1;  <pr>&#x3057;&#x3087;&#x308B;&#x3044;&#x306F;&#x3053;</pr></trd>
  <trd lng="pl">akt&oacute;wka</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_a;&#x43F;&#x43A;&c_a; &c_d;&#x43B;&#x44F; &c_d;&#x43E;&#x43A;&c_u;&#x43C;&c_je;&#x43D;&c_t;&c_ib;&c_v;</trd>,
    <trd>&c_t;&c_je;&c_ch;&#x43A;&c_a; &c_d;&#x43B;&#x44F; &c_d;&#x43E;&#x43A;&c_u;&#x43C;&c_je;&#x43D;&c_t;&c_ib;&c_v;</trd>
  </trdgrp>
  <trd lng="zh">&#x5377;&#x5B97; <pr>ju&#xE0;nz&#x14D;ng</pr></trd>
</drv>

<drv mrk="dokume.0akredito">
  <kap>
  <tld/>a kredito
  </kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="akredi1.0o">Akreditivo</ref>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_r;&c_e;&c_d;&c_y;&c_t;&c_y;&c_w;</trd>
  <trdgrp lng="de">
    <trd>Akkreditiv</trd>
  </trdgrp>
  <trd lng="en">letter of credit</trd>
  <trdgrp lng="fr">
    <trd>lettre de cr&eacute;ance</trd>,
    <trd>accr&eacute;ditif</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>okm&aacute;nyos meghitelez&eacute;s</trd>,
    <trd>akkredit&iacute;v</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>accreditief<klr tip="ind"> (documentair krediet)</klr></trd>,
    <trd>documentair <ind>krediet</ind></trd>
  </trdgrp>
  <trd lng="pl">akredytywa</trd>
  <trdgrp lng="zh">
    <trd>&#x4ED8;&#x6B3E;&#x51ED;&#x5355; <pr>f&#xF9;ku&#x1CE;np&#xED;ngd&amacron;n</pr></trd>,
    <trd>&#x4FE1;&#x7528;&#x8B49; <pr>x&#xEC;ny&#xF2;ngzh&#xE8;ng</pr></trd>,
    <trd>&#x4FE1;&#x7528;&#x5F81; <pr>x&#xEC;ny&#xF2;ngzh&emacron;ng</pr></trd>
  </trdgrp>
</drv>
