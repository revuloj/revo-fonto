<drv mrk="dokume.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Skriba&jcirc;o a&ubreve; signifa objekto, kiu servas kiel pruvo
      kaj
      atesto:
      <ekz>
        <tld/>oj pri la origino de la milito;
      </ekz>
      <ekz>
        genealogiaj <tld/>oj
        <fnt><bib>MT</bib>, <lok>&Ezr; 2:62</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="arhxiv.0o">arkivo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_k;&c_u;&c_m;&c_e;&c_n;&c_t;</trd>
  <trd lng="bg">&c_d;&c_o;&c_k;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>
  <trd lng="bo">&#xF51;&#xF54;&#xF7A;&#xF0B;&#xF46;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>doklad</trd>,
    <trd>dokument</trd>,
    <trd>listina</trd>,
    <trd>listinn&yacute; pr&uring;kaz</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Urkunde</trd>,
    <trd>Dokument</trd>
  </trdgrp>
  <trd lng="en">document</trd>
  <trd lng="es">documento</trd>
  <trd lng="fr">document</trd>
  <trdgrp lng="hu">
    <trd>okm&aacute;ny</trd>,
    <trd>dokumentum</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6587;&#x66F8;  [<ind>&#x3076;&#x3093;&#x3057;&#x3087;</ind>]</trd>,
    <trd>&#x8CC7;&#x6599;  [<ind>&#x3057;&#x308A;&#x3087;&#x3046;</ind>]</trd>,
    <trd>&#x8A18;&#x9332;  [<ind>&#x304D;&#x308D;&#x304F;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">document</trd>
  <trd lng="no">dokument</trd>
  <trdgrp lng="pl">
    <trd>dokument</trd>,
    <trd>pismo</trd>
  </trdgrp>
  <trd lng="pt">documento</trd>
  <trd lng="ro">scris</trd>
  <trd lng="ru">&c_d;&c_o;&c_k;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>doklad</trd>,
    <trd>dokument</trd>,
    <trd>listina</trd>
  </trdgrp>
  <trd lng="uk">&#x434;&#x43E;&#x43A;&#x443;&#x43C;&#x435;&#x43D;&#x442;</trd>
</drv>

<drv mrk="dokume.0i">
