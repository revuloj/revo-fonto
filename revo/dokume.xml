<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc> <ofc>1</ofc>
<rad>dokument</rad>/o
</kap>
<drv mrk="dokume.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Skriba&jcirc;o a&ubreve; signifa objekto, kiu servas kiel pruvo kaj
      atesto:
      <ekz>
        <tld/>oj pri la origino de la milito;
      </ekz>
      <ekz>
        genealogiaj <tld/>oj
        <fnt><bib>MT</bib>, <lok>&Ezr; 2:62</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="arkiv.0o">arkivo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Urkunde</trd>,
    <trd>Dokument</trd>
  </trdgrp>
  <trd lng="es">documento</trd>
  <trd lng="fr">document</trd>
  <trd lng="ru">&c_d;&c_o;&c_k;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>
</drv>

<drv mrk="dokume.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="klr">(ion)</uzo>
    <dif>
      Pruvi per <tld/>oj.
    </dif>
    <trd lng="de">beurkunden</trd>
  </snc>
  <snc>
    <uzo tip="klr">(iun)</uzo>
    <dif>
      Provizi per <tld/>oj.
    </dif>
  </snc>
  <trd lng="de">dokumentieren</trd>
  <trd lng="es">documentar</trd>
  <trd lng="fr">documenter</trd>
  <trd
  lng="ru">&c_d;&c_o;&c_k;&c_u;&c_m;&c_je;&c_n;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="dokume.0akredito">
  <kap>
  <tld/>a kredito
  </kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="akredi1.0o">Akreditivo</ref>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Akkreditiv</trd>
  </trdgrp>
</drv>
</art>
