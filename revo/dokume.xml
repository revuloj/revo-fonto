<drv mrk="dokume.0akredito">
  <kap>
  <tld/>a kredito
  </kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="akredi1.0o">Akreditivo</ref>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Akkreditiv</trd>
  </trdgrp>
  <trd lng="en">letter of credit</trd>
  <trdgrp lng="fr">
    <trd>lettre de cr&eacute;ance</trd>,
    <trd>accr&eacute;ditif</trd>
  </trdgrp>
