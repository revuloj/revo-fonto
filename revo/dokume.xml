    <trd>akkredit&iacute;v</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>accreditief<klr tip="ind"> (documentair krediet)</klr></trd>,
    <trd>documentair <ind>krediet</ind></trd>
