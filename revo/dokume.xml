<drv mrk="dokume.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <dif>
      Plata, ortangula portebla ujo, el ledo a&ubreve; plasto, por enteni
      <tld/>ojn.
    </dif>
  </snc>
  <trd lng="en">briefcase</trd>
  <trd lng="fr">porte-document(s)</trd>
  <trd lng="ja">&#x66F8;&#x985E;&#x7BB1;  [<ind>&#x3057;&#x3087;&#x308B;&#x3044;&#x306F;&#x3053;</ind>]</trd>
