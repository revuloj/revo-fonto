<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc> <ofc>1</ofc>
<rad>dokument</rad>/o
</kap>
<drv mrk="dokume.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Skriba&jcirc;o a&ubreve; signifa objekto, kiu servas kiel pruvo k atesto:
      <ekz>
        <tld/>oj pri la origino de la milito;
      </ekz>
      <ekz>
        genealogiaj <tld/>oj
        <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Ezra 2, 62</lok>)</fnt>.
      </ekz> 
    </dif>
    <refgrp tip="sin">
      <ref cel="arkiv.0o">arkivo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Urkunde</trd>,
    <trd>Dokument</trd>
  </trdgrp>
</drv>
<drv mrk="dokume.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1">
    <dif>
      Pruvi per <tld/>oj. 
    </dif>
    <trd lng="de">beurkunden</trd>
  </snc>
  <snc num="2">
    <dif>
      Provizi per <tld/>oj.  
    </dif>
  </snc>
  <trd lng="de">dokumentieren</trd>
</drv>
</art>
