<art mrk="$Id: planta.xml,v 1.27 2023/09/03 12:45:08 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>plantag</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="planta.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="planta1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Plantago</trd>):
      <ekz>
        tiel oni malkovris, ke la lancforma <tld/>o estis nekonata en
        Ameriko &gcirc;is la alveno de la koloniistoj<fnt><aut>Robert
        Roze</aut>: <vrk>Vivisekcio de
        polenero</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero 11a, p.
        12a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/plantag.jpg">
    <ind>granda <tld/>o</ind>
    <trd lng="hu">nagy <ind>&uacute;tilapu</ind></trd>
    <trd lng="la">Plantago major</trd>
  </bld>
  <trd lng="be">&c_t;&c_r;&c_y;&c_p;&c_u;&c_t;&c_n;&c_ib;&c_k;</trd>
  <trd lng="cs">jitrocel</trd>
  <trd lng="de">Wegerich</trd>
  <trdgrp lng="en">
    <trd>plantago</trd>,
    <trd>plantain</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>plantain</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&uacute;tif&udblac;</trd>,
    <trd>&uacute;tilapu</trd>
  </trdgrp>
  <trd lng="ja">&#x30AA;&#x30AA;&#x30D0;&#x30B3;</trd>
  <trd lng="nl">weegbree</trd>
  <trd lng="no">groblad</trd>
  <trd lng="pl">babka  <klr>(ro&sacute;lina)</klr></trd>
  <trdgrp lng="pt">
    <trd>plantago</trd>,
    <trd>tanchagem</trd>
  </trdgrp>
  <trd lng="ro">p&abreve;tlagin&abreve;</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_o;&c_r;&c_o;&c_zh;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">skorocel</trd>
  <trd lng="uk">&#x43F;&#x43E;&c_d;&#x43E;&c_r;&#x43E;&c_zh;&#x43D;&c_i;&#x43A;</trd>
</drv>
</art>
