    <trd lng="la">Plantago major</trd>
  </bld>
  <trd lng="de">Wegerich</trd>
</drv>
<drv mrk="planta.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Plantagaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Wegerichgew&auml;chse</trd>
</drv>
