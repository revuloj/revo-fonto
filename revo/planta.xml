<drv mrk="planta.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="planta.0acoj"><tld/>acoj</ref>
