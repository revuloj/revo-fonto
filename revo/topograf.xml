<kap>
  <ofc>7</ofc><rad>topograf</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="topograf.0o">
  <kap><ofc>7</ofc><tld/>o</kap>
  <snc mrk="topograf.0o.GEOD">
    <uzo tip="fak">GEOD</uzo>
    <dif>
      <ref tip="dif" cel="topogr.0isto.GEOD">Topografiisto</ref>:
      <ekz>
        merkrede alvenis in&gcirc;enieroj, agronomoj, hidrologoj, <tld/>oj
        kaj ter-mezuristoj
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de
        Diego</aut>, <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>;
      </ekz>
      <ekz>
        li fari&gcirc;is membro de la instrubrigado <klr>[&#8230;]</klr> poste
        li laboris kiel <tld/>o
        <fnt>
          <aut>H. Concepci&oacute;n Toledo</aut>,
          <vrk><url ref="http://www.radiosanctispiritus.cu/Version%20Esperanto/TerrorismoEsp/chorrera.htm">
          La krimo en La Chorrera</url></vrk>,
          <lok>Radio Sancti Sp&iacute;ritus, 2007-04-13</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Anka&#365; eblas diri <ke>mapsciencisto</ke>.
  </rim>

  <trd lng="be">&c_t;&c_a;&c_p;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
  <trdgrp lng="cs">
    <trd>m&iacute;stopisec</trd>,
    <trd>topograf</trd>,
    <trd>zem&ecaron;m&ecaron;&rcaron;i&ccaron;</trd>
  </trdgrp>
  <trd lng="de">Topograf</trd>
  <trd lng="en">topographer</trd>
  <trd lng="fr">topographe</trd>
  <trd lng="it">topografo</trd>
  <trdgrp lng="ja">
    <trd>&#22320;&#24418;&#23398;&#32773;  <pr>&#12385;&#12369;&#12356;&#12364;&#12367;&#12375;&#12419;</pr></trd>,
    <trd>&#22320;&#24418;&#22259;&#20316;&#35069;&#32773;  <pr>&#12385;&#12369;&#12356;&#12378;&#12373;&#12367;&#12379;&#12356;&#12375;&#12419;</pr></trd>
  </trdgrp>
  <trd lng="pl">topograf</trd>
  <trdgrp lng="sk">
    <trd>topograf</trd>,
    <trd>zememera&ccaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_t;&#1086;&#1087;&#1086;&c_g;&c_r;&c_a;&c_f;</trd>
</drv>
