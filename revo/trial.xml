<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: trial.xml,v 1.3 2022/04/21 08:46:49 revo Exp $">
<kap>
  <rad>trial</rad>/o
</kap>
<drv mrk="trial.0o">
  <kap><tld/>o</kap>
  <snc mrk="trial.0o.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      <ref tip="dif" cel="nombr.tri0o.GRA">Trinombro</ref>:
      <ekz>
        <tld/>o estas gramatika nombro ekzistanta en tre malmultaj lingvoj de la mondo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=267698"
              >Trinombro</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_a;&c_ib;&c_s;&c_t;&c_y; &c_l;&c_ib;&c_k;</trd>
  <trdgrp lng="de">
    <trd>Trial</trd>,
    <trd>Dreizahl</trd>
  </trdgrp>
  <trd lng="en">trial</trd>
  <trd lng="es">trial</trd>
  <trd lng="fr">triel<klr tip="ind"> (nombre)</klr></trd>
  <trdgrp lng="nl">
    <trd>trialis</trd>,
    <trd>drievoud</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_o;&c_j;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;&c_je; &c_ch;&c_i;&c_s;&c_l;&c_o;</trd>
</drv>
</art>

