<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: eskort.xml,v 1.25 2023/11/07 20:34:11 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>eskort</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="eskort.0o">
  <kap><tld/>o</kap>
  <snc mrk="eskort.0o.KOMUNE">
    <dif>
    <tld lit="E"/>antaro:
      <ekz>
        <tld/>o de servistoj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        venigu la &ccirc;efpastrojn &ccirc;i tien, sub protekta <tld/>o
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la princo kun siaj du kunuloj kaj kun la nova <tld/>o <klr>[&#x2026;]</klr> ekveturis
        anta&ubreve;en
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o levis la lancojn kaj ritme klakante per la ferizitaj kaligoj eliris
        <fnt>
          <bib>MkM</bib>
          <lok>2</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun granda <tld/>o de soldatoj, amikoj kaj najbaroj
        ni forlasas <nom>Gundinci</nom>
        <fnt>
          <aut>S. Maul</aut>,
          <vrk>El verva vivo jurnalista</vrk>,
          <lok>2005</lok>
        </fnt>.
      </ekz>  
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_s;&c_k;&c_o;&c_r;&c_t;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_v;&c_o;&c_j;</trd>,
    <trd>&c_s;&c_u;&c_p;&c_r;&c_a;&c_v;&c_a;&c_d;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="bg">&c_je;&c_s;&c_k;&c_o;&c_r;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>eskorta</trd>,
    <trd>ochrann&yacute; doprovod</trd>,
    <trd>ozbrojen&yacute; ochrann&yacute; pr&uring;vod</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Eskorte</trd>,
    <trd>Begleitmannschaft</trd>,
    <trd>Geleitschutz</trd>,
    <trd>Begleitschutz</trd>
  </trdgrp>
  <trd lng="en">escort</trd>
  <trd lng="es">escolta</trd>
  <trd lng="fa">&a_A;&a_s;&f_k;&a_w;&a_r;&a_t;</trd>
  <trd lng="fr">escorte</trd>
  <trdgrp lng="ja">
    <trd>&#x8B77;&#x885B;&#x968A;  <pr>&#x3054;&#x3048;&#x3044;&#x305F;&#x3044;</pr></trd>,
    <trd>&#x8B77;&#x9001;&#x968A;  <pr>&#x3054;&#x305D;&#x3046;&#x305F;&#x3044;</pr></trd>,
    <trd>&#x4ED8;&#x304D;&#x6DFB;&#x3044;  <pr>&#x3064;&#x304D;&#x305D;&#x3044;</pr></trd>,
    <trd>&#x8B77;&#x885B;  <pr>&#x3054;&#x3048;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">escorte</trd>
  <trdgrp lng="pl">
    <trd>eskorta</trd>,
    <trd>konw&oacute;j</trd>
  </trdgrp>
  <trd lng="pt">escolta</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_s;&c_k;&c_o;&c_r;&c_t;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_v;&c_o;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">ozbrojen&yacute; ochrann&yacute; sprievod</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_o;&c_n;&c_v;&c_o;&c_j;</trd>,
    <trd>&c_je;&c_s;&c_k;&c_o;&c_r;&c_t;</trd>,
    <trd>&c_o;&c_h;&c_o;&c_r;&c_o;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#x62A4;&#x9001; <pr>h&#xF9;s&#xF2;ng</pr></trd>
</drv>

<drv mrk="eskort.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="eskort.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="akompa.0i.kunesti">Akompani</ref> iun por lin gardi, helpi, honori;
      akompani kaptiton por malhelpi lin forkuri:
      <ekz>
        dudek Turkoj, armitaj per sabroj kaj pafiloj, <tld/>is ilin
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        lian portilon, portatan de ses sklavoj, &ccirc;iam <tld/>is tri homoj: unu portis la
        ventumilon, dua la hakilon de l' ministro, tria keston kun papirusoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        dekkelke da junaj fervoruloj kure <tld/>is lian veturilon, jen super&scirc;utante lin
        per la&ubreve;doj, jen la&ubreve;tvo&ccirc;e sciigante al la popolo, ke tio estas la fama
        ducentjarulo
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> devos <tld/>i la krimulojn, la &ccirc;arojn kun la ekzekutilaro kaj la
        ekzekutistojn
        <fnt>
          <bib>MkM</bib>
          <lok>2</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_s;&c_k;&c_a;&c_r;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_v;&c_a;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_u;&c_p;&c_r;&c_a;&c_v;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_je;&c_s;&c_k;&c_o;&c_r;&c_t;&c_i;&c_r;&c_a;&c_m;</trd>
  <trd lng="cs">eskortovat</trd>
  <trdgrp lng="de">
    <trd>eskortieren</trd>,
    <trd>geleiten</trd>,
    <trd>Geleitschutz geben</trd></trdgrp>
  <trd lng="en">escort</trd>
  <trd lng="es">escoltar</trd>
  <trd lng="fa">&a_A;&a_s;&f_k;&a_w;&a_r;&a_t; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">escorter</trd>
  <trd lng="he">&lamed;&#x5B0;&lamed;&#x5B7;&vav;&vav;&#x5B9;&tav;</trd>
  <trdgrp lng="ja">
    <trd>&#x8B77;&#x885B;&#x3059;&#x308B;  <pr>&#x3054;&#x3048;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x8B77;&#x9001;&#x3059;&#x308B;  <pr>&#x3054;&#x305D;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x4ED8;&#x304D;&#x6DFB;&#x3046;  <pr>&#x3064;&#x304D;&#x305D;&#x3046;</pr></trd>,
    <trd>&#x30A8;&#x30B9;&#x30B3;&#x30FC;&#x30C8;&#x3059;&#x308B;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>escorteren</trd>,
    <trd>begeleiden</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>eskortowa&cacute;</trd>,
    <trd>konwojowa&cacute;</trd>,
    <trd>odprowadza&cacute;</trd>
  </trdgrp>
  <trd lng="pt">escoltar</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_s;&c_k;&c_o;&c_r;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_v;&c_o;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_o;&c_p;&c_r;&c_o;&c_v;&c_o;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">eskortova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_o;&c_n;&c_v;&c_o;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_je;&c_s;&c_k;&c_o;&c_r;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_u;&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x62BC; <pr>y&#x101;</pr></trd>
</drv>

</art>

