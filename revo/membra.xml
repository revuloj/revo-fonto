<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: membra.xml,v 1.39 2024/01/25 20:10:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>membran</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="membra.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="membra.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <uzo tip="fak">BIO</uzo>
    <dif>
      Organika <ref tip="super" cel="hist.0o">histo</ref> formanta maldikan flekseblan apartigan
      tavolon:
      <ekz>
        la muka <tld/>o de la stomako;
      </ekz>
      <ekz>
        cerba <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2006/008861.php">Nova etapo en biologio kaj medicino</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        multnombraj mukecaj glandoj ekzistas en la <tld/>o de la laringo, speciale en la
        epigloto
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref tip="sub" cel="pleuxr.0o">ple&ubreve;ro</ref>: sereca <tld/>o, kiu surkovras la internon 
        de la brusto kaj tegas la pulmojn
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 12 (110)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="diafra.0o">diafragmo</ref>,
      <ref cel="hauxt.0o.ANA">ha&ubreve;to<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_m;&c_e;&c_m;&c_b;&c_r;&c_a;&c_n;&c_a;</trd>,
      <trd>&c_p;&c_l;&c_jo;&c_n;&c_k;&c_a;</trd>,
      <trd>&c_p;&c_l;&c_je;&c_w;&c_k;&c_a;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_a;&c_p;&c_o;&c_n;&c_k;&c_a;</trd>
    </trdgrp>                   
    <trd lng="br">koc'henn</trd>
    <trd lng="en">membrane</trd>  
    <trd lng="pl">b&lstroke;ona</trd>
    <trdgrp lng="ru">
      <trd>&c_m;&c_je;&c_m;&c_b;&c_r;&c_a;&c_n;&c_a;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_je;&c_p;&c_o;&c_n;&c_k;&c_a;</trd>,
      <trd>&c_p;&c_l;&c_jo;&c_n;&c_k;&c_a;</trd>,
      <trd>&c_p;&c_l;&c_je;&c_v;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="membra.0o.KOMUNE">
    <dif>
      Maldika fleksebla <ref tip="sin" cel="foli.0o.TEK">folio</ref>:
      <ekz>
        celuloida <tld/>o por fotografado;
      </ekz>
      <ekz>
        ne estas multe da prujno, nur kvaza&ubreve; <tld/>o
        <fnt>
          <bib>VRA</bib>
          <lok>Bj&ouml;rn Erik H&ouml;ijer</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="film.0o">filmo</ref>,
      <ref cel="sxel.0o.KOMUNE">&scirc;elo<sncref/></ref>,
      <ref cel="tavol.0o.KOMUNE">tavolo<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_m;&c_e;&c_m;&c_b;&c_r;&c_a;&c_n;&c_a;</trd>,
      <trd>&c_p;&c_l;&c_jo;&c_n;&c_k;&c_a;</trd>
    </trdgrp>      
    <trd lng="br">diafragm</trd>
    <trd lng="de">Folie</trd>
    <trd lng="pl">b&lstroke;ona</trd>
    <trdgrp lng="ru">
      <trd>&c_m;&c_je;&c_m;&c_b;&c_r;&c_a;&c_n;&c_a;</trd>,
      <trd>&c_p;&c_l;&c_jo;&c_n;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="membra.0o.ELE">
    <uzo tip="fak">ELE</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Fleksebla <ref tip="super" cel="lamen.0o">lameno</ref>
      funkcianta per vibrado, precipe por produkti a&ubreve; elektrigi
      sonon en telefona a&ubreve;skultilo, mikrofono, tamburo ktp:
      <ekz>
        la principo de la komuna funkciado de la mikrofono kaj
        telefono estas la jena: pro la vibradoj de la <tld/>o la
        karba pulvoro estas alterne pli kaj malpli premata, kaj
        pro tio varias la elektra rezisto de la pulvoro
        <fnt>
          <aut>Ir. J. R. G. Isbr&uuml;cker</aut>:
          <vrk><url ref="&STEB;ingxenierado/telekomuniko/telefoniado1928/telefoniado1928.htm">Evoluo
          de telefonio</url></vrk>.
          <lok>1928</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_e;&c_m;&c_b;&c_r;&c_a;&c_n;&c_a;</trd>
    <trd lng="hu">membr&aacute;n<klr tip="ind"> (telefon)</klr></trd>
    <trd lng="ru">&c_m;&c_je;&c_m;&c_b;&c_r;&c_a;&c_n;&c_a;</trd>
  </snc>
  <trd lng="bg">&c_m;&c_je;&c_m;&c_b;&c_r;&c_a;&c_n;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>bl&aacute;na</trd>,
    <trd>membr&aacute;na</trd>,
    <trd>m&aacute;zdra</trd>
  </trdgrp>
  <trd lng="de">Membran</trd>
  <trd lng="fa">&a_gh;&a_sh;&a_A;</trd>
  <trd lng="fr">membrane</trd>
  <trd lng="it">membrana</trd>
  <trdgrp lng="ja">
    <trd>&#x819C;  <pr>&#x307E;&#x304F;</pr></trd>,
    <trd>&#x8584;&#x819C;  <pr>&#x3046;&#x3059;&#x307E;&#x304F;</pr></trd>,
    <trd>&#x632F;&#x52D5;&#x677F;  <pr>&#x3057;&#x3093;&#x3069;&#x3046;&#x3070;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>membraan</trd>,
    <trd>vlies</trd>
  </trdgrp>
  <trd lng="pt">membrana</trd>
  <trdgrp lng="sk">
    <trd>blana</trd>,
    <trd>membr&aacute;na</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_o;&c_b;&c_o;&c_l;&c_o;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_t;&c_i;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_je;&c_m;&c_b;&c_r;&c_a;&c_n;&c_a;</trd>,
    <trd>&c_p;&c_l;&c_ib;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_l;&c_ib;&c_v;&c_a;</trd>,
    <trd>&c_m;&c_je;&c_m;&c_b;&c_r;&c_a;&c_n;&c_a;</trd>,
    <trd>&c_d;&c_ib;&c_a;&c_f;&c_r;&c_a;&c_g;&c_m;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x819C; <pr>m&#xF3;</pr></trd>,
    <trd>&#x8584;&#x819C; <pr>b&#xF3;m&#xF3;</pr></trd>,
    <trd>&#x7D19;&#x76C6; <pr>zh&#x1D0;p&#xE9;n</pr></trd>,
    <trd>&#x7EB8;&#x76C6; <pr>zh&#x1D0;p&#xE9;n</pr></trd>,
    <trd>&#x7F8A;&#x76AE;&#x7D19; <pr>y&#xE1;ngp&#xED;zh&#x1D0;</pr></trd>,
    <trd>&#x7F8A;&#x76AE;&#x7EB8; <pr>y&#xE1;ngp&#xED;zh&#x1D0;</pr></trd>,
    <trd>&#x8188; <pr>g&#xE9;</pr></trd>,
    <trd>&#x819C;&#x7247; <pr>m&#xF3;pi&#xE0;n</pr></trd>,
    <trd>&#x9694;&#x819C; <pr>g&#xE9;m&#xF3;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="membra.muk0o">
  <kap>muk<tld/>o</kap>
  <snc mrk="membra.muk0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      <ref tip="dif" cel="mukoz.0o">Mukozo</ref>:
      <ekz>
        la virusoj komence penetras la epiteliajn &ccirc;elojn de la muk<tld/>o de la
        utercerviko
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2010/010159.php">Vakcino preventas kancerojn</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_l;&c_ib;&c_z;&c_mol;&c_n;&c_ib;&c_c;&c_a;</trd>,
    <trd>&c_s;&c_mol;&c_l;&c_ib;&c_z;&c_mol;&c_n;&c_je;&c_v;&c_a;&c_ja; &c_a;&c_b;&c_a;&c_l;&c_o;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">sliznice</trd>
  <trd lng="de">Schleimhaut</trd>
  <trd lng="en">mucous membrane</trd>
  <trd lng="fr">muqueuse</trd>
  <trd lng="ja">&#x7C98;&#x819C;  <pr>&#x306D;&#x3093;&#x307E;&#x304F;</pr></trd>
  <trd lng="sk">sliznica</trd>
  <trd lng="uk">&c_s;&c_l;&c_i;&c_z;&c_o;&c_v;&c_a; &c_o;&c_b;&c_o;&c_l;&c_o;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x7C98;&#x819C; <pr>ni&#xE1;nm&#xF3;</pr></trd>,
    <trd>&#x9ECF;&#x819C; <pr>ni&#xE1;nm&#xF3;</pr></trd>,
    <trd>&#x9ECF;&#x819C; <pr>ni&#xE1;nm&#xF2;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="membra.nagx0o">
  <kap>na&gcirc;<tld/>o</kap>
  <snc mrk="membra.nagx0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <tld lit="M"/>o, kiu kunigas la fingrojn de ekzemple amfibioj kaj iuj
      akvobirdoj, igante sinpelon en akvo pli efika:
      <ekz>
        la manoj kun na&gcirc;<tld/>o dislar&gcirc;i&gcirc;is en formo de ventumilo
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>!
      </ekz>
      <ekz>
        ru&gcirc;e brilis tiuj piedoj <klr>[&#x2026;]</klr>
        kiel la na&gcirc;<tld/>oj de anseroj
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; <ind>&c_p;&c_je;&c_r;&c_a;&c_p;&c_o;&c_n;&c_k;&c_a;</ind></trd>
  <trd lng="de">Schwimmhaut</trd>
  <trd lng="fr">palmure</trd>
  <trd lng="ja">&#x6C34;&#x304B;&#x304D;  <pr>&#x307F;&#x305A;&#x304B;&#x304D;</pr></trd>
  <trd lng="zh">&#x8E7C; <pr>p&#x1D4;</pr></trd>
</drv>

<drv mrk="membra.orel0o">
  <kap>
    orel<tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="M"/>o de la <ref tip="malprt" cel="timpan.0o.ANA">timpano</ref>:
      <ekz>
        grandega terno <klr>[&#x2026;]</klr> preska&ubreve; eksplodigas la orel<tld/>ojn
        <fnt>
          <bib>VRA</bib>
          <lok>Moa Martinson</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_u;&c_b;&c_je;&c_n;&c_n;&c_a;&c_ja; <ind>&c_b;&c_a;&c_l;&c_o;&c_n;&c_a;</ind></trd>,
    <trd>&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n;&c_n;&c_a;&c_ja; <ind>&c_p;&c_je;&c_r;&c_a;&c_p;&c_o;&c_n;&c_k;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="de">Trommelfell</trd>
  <trd lng="la">membrana tympani</trd>
  <trd lng="zh">&#x9F13;&#x819C; <pr>g&#x1D4;m&#xF2;</pr></trd>
</drv>










