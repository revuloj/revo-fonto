<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>membran</rad>/o
</kap>
<drv mrk="membra.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <uzo tip="fak">ANA</uzo>
    <dif>
      Organika teksa&jcirc;o formanta maldikan flekseblan folion:
      <ekz>
        la muka <tld/>o de la stomako.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_m;&c_e;&c_m;&c_b;&c_r;&c_a;&c_n;&c_a;</trd>,
      <trd>&c_p;&c_l;&c_jo;&c_n;&c_k;&c_a;</trd>,
      <trd>&c_p;&c_l;&c_je;&c_w;&c_k;&c_a;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_a;&c_p;&c_o;&c_n;&c_k;&c_a;</trd>
    </trdgrp>                   
    <trd lng="br">koc'henn</trd>
    <trd lng="de">Membran</trd>
    <trd lng="en">membrane</trd>  
    <trd lng="pl">b&lstroke;ona</trd>
    <trdgrp lng="ru">
      <trd>&c_m;&c_je;&c_m;&c_b;&c_r;&c_a;&c_n;&c_a;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_je;&c_p;&c_o;&c_n;&c_k;&c_a;</trd>,
      <trd>&c_p;&c_l;&c_jo;&c_n;&c_k;&c_a;</trd>,
      <trd>&c_p;&c_l;&c_je;&c_v;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Maldika fleksebla folio:
      <ekz>
        celuloida <tld/>o por fotografado
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="film.0o">filmo</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_m;&c_e;&c_m;&c_b;&c_r;&c_a;&c_n;&c_a;</trd>,
      <trd>&c_p;&c_l;&c_jo;&c_n;&c_k;&c_a;</trd>
    </trdgrp>      
    <trd lng="br">diafragm</trd>
    <trdgrp lng="ru">
      <trd>&c_m;&c_je;&c_m;&c_b;&c_r;&c_a;&c_n;&c_a;</trd>,
      <trd>&c_p;&c_l;&c_jo;&c_n;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">membrane</trd>
  <trdgrp lng="nl">
      <trd>membraan</trd>,
      <trd>vlies</trd>
  </trdgrp>
  <trd lng="pt">membrana</trd>
</drv>
</art>
