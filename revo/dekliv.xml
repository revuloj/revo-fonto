<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dekliv.xml,v 1.22 2021/05/15 20:52:03 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>dekliv</rad>/o
</kap>
<drv mrk="dekliv.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="dekliv.0o.KOMUNE">
    <dif>
      Oblikva, klinita supra&jcirc;o rilate al la horizontalo:
      <ekz>
        kruta <tld/>o de monto.
      </ekz>
    </dif>
    <trdgrp lng="th">
      <trd>&#xE25;&#xE32;&#xE14;</trd>,
      <trd>&#xE40;&#xE2D;&#xE35;&#xE22;&#xE07;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_h;&c_ib;&c_l;</trd>,
    <trd>&c_a;&c_d;&c_h;&c_o;&c_n;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>declivi</trd>,
    <trd>inclinaci&oacute;</trd>,
    <trd>pendent</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>str&aacute;&ncaron;</trd>,
    <trd>svah</trd>
  </trdgrp>
  <trd lng="de">Abhang</trd>
  <trd lng="en">slope</trd>
  <trdgrp lng="es">
    <trd>declive</trd>,
    <trd>inclinaci&oacute;n</trd>,
    <trd>pendiente</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_s;&a_r;&a_A;&a_z;&f_y;&a_r;&f_y;</trd>,
    <trd>&a_s;&a_r;&a_A;&a_sh;&f_y;&a_b;&f_y;</trd>,
    <trd>&a_sh;&f_y;&a_b;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;clivit&eacute;</trd>,
    <trd>pente</trd>,
    <trd>versant</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&#x5DE;&#x5B4;&#x5D3;&#x5E8;&#x5D5;&#x5B9;&#x5DF;</trd>,
    <trd>&#x5E9;&#x5D9;&#x5E4;&#x5D5;&#x5E2;</trd>
  </trdgrp>
  <trd lng="hu">lejt&odblac;</trd>
  <trd lng="nl">helling</trd>
  <trdgrp lng="pl">
    <trd>pochy&lstroke;o&sacute;&cacute;</trd>,
    <trd>stok</trd>,
    <trd>zbocze</trd>,
    <trd>spadek</trd>,
    <trd>spad</trd>,
    <trd>pochylnia</trd>,
    <trd>podjazd</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>declive</trd>,
    <trd>ladeira</trd>,
    <trd>rampa</trd>,
    <trd>vertente</trd>,
    <trd>encosta</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_l;&c_o;&c_n;</trd>,
    <trd>&c_o;&c_t;&c_k;&c_o;&c_s;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">svah</trd>
</drv>

<drv mrk="dekliv.0i">
  <kap><tld/>i</kap>
  <snc mrk="dekliv.0i.KOMUNE">
    <dif>
      Havi alsupre a&ubreve; alsube klinitan surfacon:
      <ekz>
        la vojo <klr>...</klr> zigzagis &ccirc;iudirekten, <tld/>ante
        unue
        supren kaj poste malsupren
        <fnt><bib>DOz</bib>, <lok>&leftquot;la kavo de la
        draketoj&rightquot;</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>inclinar-se</trd>,
    <trd>tenir pendent</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>m&iacute;t sklon</trd>,
    <trd>sva&zcaron;ovat se</trd>
  </trdgrp>
  <trd lng="en">slope</trd>
  <trdgrp lng="es">
    <trd>inclinarse</trd>,
    <trd>tener pendiente</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'incliner<klr tip="amb"> (&ecirc;tre en pente)</klr></trd>,
    <trd>&ecirc;tre en <ind>pente</ind></trd>
  </trdgrp>
  <trd lng="he">&#x5DE;&#x5B4;&#x5D3;&#x5E8;&#x5D5;&#x5B9;&#x5DF;</trd>
  <trdgrp lng="nl">
    <trd>hellen</trd>,
    <trd>glooien</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>mie&cacute; <ind>spad</ind></trd>,
    <trd>by&cacute; <ind>pochy&lstroke;ym</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ma&tcaron; sklon</trd>,
    <trd>zva&zcaron;ova&tcaron; sa</trd>
  </trdgrp>
</drv>
</art>
