<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dekliv.xml,v 1.25 2022/04/21 23:29:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>dekliv</rad>/o
</kap>
<drv mrk="dekliv.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="dekliv.0o.KOMUNE">
    <dif>
      <ref cel="oblikv.0a">Oblikva</ref>, klinita supra&jcirc;o 
      rilate al la horizontalo:
      <ekz>
        la <tld/>o de la valoj <klr>[&#8230;]</klr> turni&gcirc;as al la kolonio
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 21:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        multe da homoj iras <klr>[&#8230;]</klr> la&ubreve; la <tld/>o de la monto
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 13:34</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen ili staris sur kruta <tld/>o apud klara lago
        <fnt>
          <bib>Fab3</bib>
          <lok>Ib kaj malgranda kristino</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ekvidis arbarkovritan <tld/>on senpere apud klara golfeto
        <fnt>
          <bib>Fab2</bib>
          <lok>Malgranda Tuk</lok>
        </fnt>;
      </ekz>
      <ekz>
        milde levi&gcirc;anta <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Malgranda Tuk</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="th">
      <trd>&#xE25;&#xE32;&#xE14;</trd>,
      <trd>&#xE40;&#xE2D;&#xE35;&#xE22;&#xE07;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_h;&c_ib;&c_l;</trd>,
    <trd>&c_a;&c_d;&c_h;&c_o;&c_n;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>declivi</trd>,
    <trd>inclinaci&oacute;</trd>,
    <trd>pendent</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>str&aacute;&ncaron;</trd>,
    <trd>svah</trd>
  </trdgrp>
  <trd lng="de">Abhang</trd>
  <trdgrp lng="de">
    <trd>Abhang</trd>,
    <trd>Hang</trd>,
    <trd>Gef&auml;lle</trd>,
    <trd>Neigung</trd>
  </trdgrp>
  <trd lng="en">slope</trd>
  <trdgrp lng="es">
    <trd>declive</trd>,
    <trd>inclinaci&oacute;n</trd>,
    <trd>pendiente</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_s;&a_r;&a_A;&a_z;&f_y;&a_r;&f_y;</trd>,
    <trd>&a_s;&a_r;&a_A;&a_sh;&f_y;&a_b;&f_y;</trd>,
    <trd>&a_sh;&f_y;&a_b;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;clivit&eacute;</trd>,
    <trd>pente</trd>,
    <trd>versant</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&mem;&#x5B4;&dalet;&resh;&vav;&#x5B9;&fnun;</trd>,
    <trd>&shin;&yod;&pe;&vav;&ayin;</trd>
  </trdgrp>
  <trd lng="hu">lejt&odblac;</trd>
  <trd lng="nl">helling</trd>
  <trdgrp lng="pl">
    <trd>pochy&lstroke;o&sacute;&cacute;</trd>,
    <trd>stok</trd>,
    <trd>zbocze</trd>,
    <trd>spadek</trd>,
    <trd>spad</trd>,
    <trd>pochylnia</trd>,
    <trd>podjazd</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>declive</trd>,
    <trd>ladeira</trd>,
    <trd>rampa</trd>,
    <trd>vertente</trd>,
    <trd>encosta</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_l;&c_o;&c_n;</trd>,
    <trd>&c_o;&c_t;&c_k;&c_o;&c_s;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">svah</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_h;&c_i;&#x43B;</trd>,
    <trd>&c_s;&#x43F;&c_a;&c_d;</trd>,
    <trd>&c_u;&#x43A;&c_ib;&c_s;</trd>,
    <trd>&#x43E;&c_b;&c_r;&c_i;&c_v;</trd>
  </trdgrp>
</drv>

<drv mrk="dekliv.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Havanta klinitan supra&jcirc;on:
      <ekz>
        <nom>Phut</nom> metis la piedojn sur la <tld/>an plankon 
        kaj tra la lar&gcirc;a koridoro komencis iri malsupren
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu grava konstrua&jcirc;o havis <tld/>ajn murojn, kvaza&ubreve; kuni&gcirc;antajn supren
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;evaloj pa&scirc;ti&gcirc;as sur la <ind><tld/>a&jcirc;o</ind>
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>.
        <trd lng="de">B&ouml;schung</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>geneigt</trd>,
    <trd>schr&auml;g</trd>,
    <trd>absch&uuml;ssig</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&c_h;&c_i;&#x43B;&c_i;&c_j;</trd>,
    <trd>&c_s;&#x43F;&c_a;&c_d;&c_i;&c_s;&c_t;&c_i;&c_j;</trd>,
    <trd>&#x43F;&#x43E;&#x43B;&#x43E;&c_zh;&c_i;&c_s;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
</drv>


<drv mrk="dekliv.0i">
  <kap><tld/>i</kap>
  <snc mrk="dekliv.0i.KOMUNE">
    <dif>
      Havi alsupre a&ubreve; alsube klinitan surfacon:
      <ekz>
        &gcirc;uste ekster mia pordo la strato <tld/>is malsupren al malalta&jcirc;o
        <fnt>
          <bib>Kiso</bib>
        </fnt>;
      </ekz>
      <ekz>
        la vojo <klr>[&#8230;]</klr> zigzagis &ccirc;iudirekten, <tld/>ante
        unue supren kaj poste malsupren
        <fnt><bib>DOz</bib>, <lok>&leftquot;la kavo de la
        draketoj&rightquot;</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="supr.al0i">alsupri</ref>,
      <ref cel="ascend.0i.supreniri">ascendi</ref>,
      <ref cel="descend.0i">descendi</ref>,
      <ref cel="supr.de0i">desupri</ref>
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>inclinar-se</trd>,
    <trd>tenir pendent</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>m&iacute;t sklon</trd>,
    <trd>sva&zcaron;ovat se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><ind>schr&auml;g</ind> verlaufen</trd>,
    <trd>sich <ind>neigen</ind></trd>
  </trdgrp>
  <trd lng="en">slope</trd>
  <trdgrp lng="es">
    <trd>inclinarse</trd>,
    <trd>tener pendiente</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'incliner<klr tip="amb"> (&ecirc;tre en pente)</klr></trd>,
    <trd>&ecirc;tre en <ind>pente</ind></trd>
  </trdgrp>
  <trd lng="he">&mem;&#x5B4;&dalet;&resh;&vav;&#x5B9;&fnun;</trd>
  <trdgrp lng="nl">
    <trd>hellen</trd>,
    <trd>glooien</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>mie&cacute; <ind>spad</ind></trd>,
    <trd>by&cacute; <ind>pochy&lstroke;ym</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ma&tcaron; sklon</trd>,
    <trd>zva&zcaron;ova&tcaron; sa</trd>
  </trdgrp>
</drv>
</art>
