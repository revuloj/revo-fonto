<vortaro>
<art mrk="$Id$">
<kap>
<rad>eglefin</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="eglefi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="gaded.0oj">gadedoj</ref>
      (<trd lng="la">Melanogrammus</trd>):
      <ekz>
        <uzo tip="fak">KUI</uzo>
        <ind>fuma&jcirc;ita eglefino</ind>.
        <trd lng="fr">haddock</trd>
        <trd lng="nl">gerookte <ind>schelvis</ind></trd>
  <trdgrp lng="pt">
    <trd>hadoque</trd>,
    <trd>eglefim</trd>,
    <trd>arinca</trd>
  </trdgrp>
        <trd lng="hu">f&uuml;st&ouml;lt <ind>t&odblac;kehal</ind></trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_k;&c_sh;&c_a;</trd>
  <trd lng="de">Schellfisch</trd>
  <trd lng="en">haddock</trd>
  <trd lng="es">eglefino</trd>
  <trdgrp lng="fr">
    <trd>aiglefin</trd>,
    <trd>&eacute;glefin</trd></trdgrp> 
  <trd lng="hu">foltos <ind>t&odblac;kehal</ind></trd>
  <trd lng="it">asinello</trd>
  <trd lng="nl">schelvis</trd>
  <trd lng="pt">arinca</trd>
  <trd lng="ru">&c_p;&c_i;&c_k;&c_sh;&c_a;</trd>
</drv>
</art>
