<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pedago.xml,v 1.21 2023/10/20 14:25:20 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>pedagog</rad>/o
</kap>
<drv mrk="pedago.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PED</uzo>
  <snc mrk="pedago.0o.instruisto">
    <dif>
      <ref tip="dif" cel="instru.0isto">Instruisto</ref>:
      <ekz>
       la fama hungara <tld/>o <nom>Polgar</nom> <klr>[&#x2026;]</klr> decidis kaj
       sukcese faris el siaj filinoj mond&ccirc;ampionojn pri &scirc;ako
       <fnt>
         <aut>Zlatko Ti&scaron;ljar</aut>: <vrk>Kosteli&cacute; - skia
         miraklo</vrk>, <bib>Monato</bib><lok>jaro 2002a, numero 8a,
         p. 21a</lok>
       </fnt>.
      </ekz>
      <ekz>
        la le&gcirc;o fari&gcirc;is nia <tld/>o, kondukanta al Kristo, por ke ni pravi&gcirc;u per fido
        <fnt>
          <bib>NT</bib>
          <lok>Galatoj 3:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        pliakri&gcirc;as vera instruisto-manko, &ccirc;ar multaj <tld/>oj estas pli-ol-50-jara&gcirc;aj
        kaj balda&ubreve; pensii&gcirc;os
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2013/010855.php">Kver-el-e el la ekonomio</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    
    <trd lng="en">teacher</trd>
    <trdgrp lng="hu">
      <trd>pedag&oacute;gus</trd>,
      <trd>nevel&odblac;</trd>
    </trdgrp>
  </snc>
  <snc mrk="pedago.0o.pedagogiisto">
    <dif>
      <ref tip="dif" cel="pedago1.0isto">Pedagogiisto</ref>:
      <ekz>
        sciencisto-medicinisto, <tld/>o, a&ubreve;toro de multaj sciencaj verkoj, dum multaj
        jaroj li estis profesoro kaj vicdekano de Samarkanda Medicina Instituto
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2013/011019.php">Unu hirundo printempon ne alportas: sed du?</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_d;&c_a;&c_g;&c_o;&c_g;</trd>
  <trd lng="ca">pedagog, educador</trd>
  <trdgrp lng="cs">
    <trd>pedagog</trd>,
    <trd>vychovatel</trd>
  </trdgrp>
  <trd lng="de">P&auml;dagoge</trd>
  <trd lng="en">pedagogue</trd>
  <trdgrp lng="fr">
    <trd>p&eacute;dagogue</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nevel&eacute;studom&aacute;nyi szakember</trd>,
    <trd>pedag&oacute;gus <klr tip="amb">(tud&oacute;s)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6559;&#x80B2;&#x8005;  <pr>&#x304D;&#x3087;&#x3046;&#x3044;&#x304F;&#x3057;&#x3083;</pr></trd>,
    <trd>&#x6559;&#x5E2B;  <pr>&#x304D;&#x3087;&#x3046;&#x3057;</pr></trd>,
    <trd>&#x6559;&#x80B2;&#x5B66;&#x8005;  <pr>&#x304D;&#x3087;&#x3046;&#x3044;&#x304F;&#x304C;&#x304F;&#x3057;&#x3083;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>opvoedkundige</trd>,
    <trd>pedagoog</trd>
  </trdgrp>
  <trd lng="pl">pedagog</trd>
  <trd lng="pt">pedagogo</trd>
  <trd lng="ru">&c_p;&c_je;&c_d;&c_a;&c_g;&c_o;&c_g;</trd>
  <trdgrp lng="sk">
    <trd>pedag&oacute;g</trd>,
    <trd>vychov&aacute;vate&lcaron;</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x435;&#x434;&#x430;&#x433;&#x43E;&#x433;</trd>
</drv>

<drv mrk="pedago.0a">
  <kap><tld/>a</kap>
  <snc mrk="pedago.0a.rilata">
    <dif>
      De <tld/>o; rilata al <tld/>o:
      <ekz>
        bone konata en sferoj <tld/>aj lingvisto
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a tasko pli ta&ubreve;gis la&ubreve; lia naturo
        <fnt>
          <bib>VivZam</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_d;&c_a;&c_g;&c_a;&c_g;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">pedagogia</trd>
  <trd lng="cs">vychovatelsk&yacute;</trd>
  <trd lng="de">p&auml;dagogisch</trd>
  <trd lng="en">pedagogical</trd>
  <trdgrp lng="fr">
    <trd>p&eacute;dagogique<klr> (de p&eacute;dagogues)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x6559;&#x80B2;&#x8005;&#x306E;  <pr>&#x304D;&#x3087;&#x3046;&#x3044;&#x304F;&#x3057;&#x3083;&#x306E;</pr></trd>
  <trd lng="nl">pedagogisch</trd>
  <trd lng="pl">pedagogiczny</trd>
  <trd lng="ru">&c_p;&c_je;&c_d;&c_a;&c_g;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">pedagogick&yacute;</trd>
</drv>
</art>
