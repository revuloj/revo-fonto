<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>pedagog</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="pedago.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Instruisto kaj edukisto de infanoj.
    </dif>
    <trd lng="de">P&auml;dagoge</trd>
  </snc>
  <snc>
    <dif>
      Pedagogiisto.
    </dif>
  </snc>
  <trdgrp lng="nl">
    <trd>opvoedkundige</trd>,
    <trd>pedagoog</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_d;&c_a;&c_g;&c_o;&c_g;</trd>
</drv>

<drv mrk="pedago.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      De <tld/>o:
      <ekz>
        bone konata en sferoj <tld/>aj lingvisto<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">p&auml;dagogisch</trd>
  <trd lng="nl">pedagogisch</trd>
  <trd lng="ru">&c_p;&c_je;&c_d;&c_a;&c_g;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
