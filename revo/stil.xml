    <trd>&c_s;&c_t;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">dobr&eacute;ho &scaron;t&yacute;lu</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_a;&c_r;&#x43D;&#x43E;&c_g;&#x43E; &c_s;&c_t;&c_i;&#x43B;&#x44E;</trd>,
    <trd>&c_v;&c_i;&c_t;&#x43E;&#x43D;&c_ch;&c_je;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x52FB;&#x7A31; <pr>y&#xFA;nch&#xE8;n</pr></trd>,
    <trd>&#x5300;&#x79F0; <pr>y&#xFA;nch&#xE8;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="stil.jun0o">
  <kap>
    jun<tld/>o
  </kap>
  <snc mrk="stil.jun0o.secesia">
    <dif>
      <ref tip="super" cel="secesi.0a_stilo">Secesia stilo</ref>:
      <ekz>
        la &ccirc;efaj verkoj de <nom>Schiele</nom> kaj <nom>Klimt</nom>
        pa&scirc;on post pa&scirc;o montras la evoluon de jun<tld/>o al ekspresionismo
        <fnt>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2014/011324.php">Tamen arto!</url></vrk>
          <bib>Monato</bib>
          <lok>2014/10, p. 7</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ju;&c_g;&c_je;&c_n;&c_d;&c_s;&c_t;&c_y;&c_l;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_d;&c_e;&c_r;&c_n;</trd>
  </trdgrp>
  <trd lng="de">Jugendstil</trd>
  <trd lng="en">modern style</trd>
  <trd lng="fr">art nouveau</trd>
  <trd lng="pl">styl m&lstroke;odzie&zdot;owy</trd>
  <trdgrp lng="zh">
    <trd>&#x65B0;&#x827A;&#x672F;&#x8FD0;&#x52A8; <pr>x&#x12B;ny&#xEC;sh&#xF9;y&#xF9;nd&#xF2;ng</pr></trd>,
    <trd>&#x65B0;&#x85DD;&#x8853;&#x904B;&#x52D5; <pr>x&#x12B;ny&#xEC;sh&#xF9;y&#xF9;nd&#xF2;ng</pr></trd>,
    <trd>&#x65B0;&#x827A;&#x672F;&#x98CE;&#x683C; <pr>x&#x12B;ny&#xEC;sh&#xF9;f&#x113;ngg&#xE9;</pr></trd>,
    <trd>&#x65B0;&#x85DD;&#x8853;&#x98A8;&#x683C; <pr>x&#x12B;ny&#xEC;sh&#xF9;f&#x113;ngg&#xE9;</pr></trd>,
    <trd>&#x9752;&#x6625;&#x827A;&#x672F;&#x98CE;&#x683C; <pr>q&#x12B;ngch&#x16B;ny&#xEC;sh&#xF9;f&#x113;ngg&#xE9;</pr></trd>,
    <trd>&#x9752;&#x6625;&#x85DD;&#x8853;&#x98A8;&#x683C; <pr>q&#x12B;ngch&#x16B;ny&#xEC;sh&#xF9;f&#x113;ngg&#xE9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="stil.malbon0a">
  <kap>malbon<tld/>a</kap>
  <snc mrk="stil.malbon0a.BELE">
    <dif>
      Kiu impresas malagrable, <ref tip="vid" cel="elegan.mal0a.KOMUNE">malelegante</ref>:
      <ekz>
        teksto ignoranta la komunikan vortordon restas komprenebla &mdash; sed malbon<tld/>a kaj malfacile
        komprenebla
        <fnt>
          <aut>S. Pokrovskij</aut>
          <vrk><url ref="https://lingvakritiko.com/2024/02/23/pri-la-vortordo-en-esperanto/">
          Pri la vortordo en Esperanto</url></vrk>
          <lok>Lingva Kritiko, 2024-02-23</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>de mauvais <ind>style</ind></trd>,
    <trd>in&eacute;l&eacute;gant</trd>
  </trdgrp>
</drv>

<drv mrk="stil.nov0a">
  <kap>nov<tld/>a</kap>
  <snc mrk="stil.nov0a.KOMUNE">
    <dif>
      Kiu akordas kun nova <tld/>o;
      <ref tip="vid" cel="mod.laux0a.MOR">la&ubreve;moda</ref>:
      <ekz>
        por lo&gcirc;igi la laboristojn estis konstruitaj nov<tld/>aj domoj
        <fnt>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2014/011239.php">Ejoj kaj a&jcirc;oj de niaj
          avoj</url></vrk>
          <bib>Monato</bib>
          <lok>2014/05, p. 19</lok>
        </fnt>;
      </ekz>
      <ekz>
        kursoj nov<tld/>aj por instruontoj
        <fnt>
          <aut>Jungkee Lee</aut>,
          <vrk><url ref="http://esperanto.center/2016/10/07/111?ckattempt=1">
          Kursoj novstilaj por instruontoj,
          video-tradukemuloj</url></vrk>,
          <lok>UEA, 2016</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> estos akceptitaj en nov<tld/>a armeo
        <fnt>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2008/009627.php">Ek al profesia armeo</url></vrk>
          <bib>Monato</bib>
          <lok>2008/12, p. 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        okazis do nov<tld/>a diservo: la &ccirc;eestantoj restis en siaj parkumitaj a&ubreve;toj apud
        la pre&gcirc;ejo kaj a&ubreve;skultis radie dissendatan diservon
        <fnt>
          <aut>AMik</aut>
          <vrk><url ref="&Monato;/2012/010520.php">Di(s)sendite</url></vrk>
          <bib>Monato</bib>
          <lok>2012/01, p. 10</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="modern.0a">moderna</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_ch;&c_a;&c_s;&c_n;&c_a;&c_g;&c_a; <ind>&c_s;&c_t;&c_y;&c_l;&c_ju;</ind></trd>,
    <trd>&c_s;&c_u;&c_ch;&c_a;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_m;&c_a;&c_d;&c_e;&c_r;&c_n;&c_a;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>modern</trd>,
    <trd>neuartig</trd>,
    <trd>zeitgem&auml;&szlig;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&agrave; la page</trd>,
    <trd>tendance<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x65B0;&#x5F0F;&#x306E;  <pr>&#x3057;&#x3093;&#x3057;&#x304D;&#x306E;</pr></trd>
  <trdgrp lng="pl">
    <trd>nowoczesny</trd>,
    <trd>w nowym  <ind>stylu</ind></trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x65B0;&#x5F0F; <pr>x&#x12B;nsh&#xEC;</pr></trd>,
    <trd>&#x6469;&#x767B; <pr>m&#xF3;d&#x113;ng</pr></trd>,
    <trd>&#x65B0;&#x6F6E; <pr>x&#x12B;nch&#xE1;o</pr></trd>,
    <trd>&#x65F6;&#x9AE6; <pr>sh&#xED;m&#xE1;o</pr></trd>,
    <trd>&#x6642;&#x9AE6; <pr>sh&#xED;m&#xE1;o</pr></trd>,
    <trd>&#x65F6;&#x5C1A; <pr>sh&#xED;sh&#xE0;ng</pr></trd>,
    <trd>&#x6642;&#x5C1A; <pr>sh&#xED;sh&#xE0;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="stil.kolonia0o">
  <kap>kolonia <tld/>o</kap>
  <snc mrk="stil.kolonia0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Konstru- kaj ornamo<tld/>o de domoj en tropika klimato,
      kun balkonoj, verandoj kaj similaj plezuraran&gcirc;oj,
      origine uzita de e&ubreve;ropaj kolonianoj:
      <ekz>
        apud belaj koloni<tld/>aj vilaoj kun &gcirc;ardenoj kaj komfortaj
        apartamentoj, etendi&gcirc;as implika&jcirc;o de malpuraj stratetoj
        lo&gcirc;ataj de amaso da malri&ccirc;uloj
        <fnt>
          <aut>W. Kristianasen, trad. J.-M. Cash</aut>,
          <vrk><url ref="&Mondediplo;article1441.html">
          Enrigardo en la islaman Baraton</url></vrk>,
          <lok>le Monde diplomatique, 2009-01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_ja;&c_n;&c_ib;&c_ja;&c_l;&c_mol;&c_n;&c_y; <ind>&c_s;&c_t;&c_y;&c_l;&c_mol;</ind></trd>
  <trd lng="de">Kolonialstil</trd>
  <trd lng="fr">style colonial</trd>
  <trd lng="id"><ind>gaya</ind> kolonial</trd>
  <trd lng="pl">styl kolonialny</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_o;&c_n;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_s;&c_t;&c_i;&c_l;&c_mol;</trd>
</drv>
</art>
