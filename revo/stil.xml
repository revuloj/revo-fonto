<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>stil</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="stil.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="stil.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      <ref tip="vid" cel="manier.0o.gxenerale">Maniero</ref> verki,
      fari arta&jcirc;on:
    </dif>
    <subsnc>
      <dif>
        karakteriza de homo, grupo, skolo:
        <ekz>
          ili skribas senerare, sed per multepeza malagrabla
          <tld/>o<fnt>Z</fnt>;
        </ekz>
        <ekz>
          la vera <tld/>o Esperanta estas nek slava, nek
          germana, nek romana, &gcirc;i estas &mdash; a&ubreve; almena&ubreve; devas esti &mdash; nur
          <tld/>o
          simpla kaj logika
          <fnt><bib>LR</bib>,
            <lok>Respondo 1, La Revuo, 1906, Decembro</lok></fnt>;
        </ekz>
        <ekz>
        la <tld/>o Esperanta ne imitas blinde la <tld/>ojn de aliaj
        lingvoj, sed havas sian karakteron tute specialan kaj
        memstaran<fnt><bib>LR</bib>, <lok>Respondo 1, La Revuo, 1906,
        Decembro</lok></fnt>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc>
      <dif>
        karakteriza de popolo, epoko:
        <ekz>
          la baroka <tld/>o;
        </ekz>
        <ekz>
          modernaj domoj en kazerne-karcera <tld/>o.
        </ekz>
      </dif>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="kolor.0o.stilo">koloro <sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_y;&c_l;&c_mol;</trd>
  <trd lng="ca">estil</trd>
  <trdgrp lng="cs">
    <trd>sloh</trd>,
    <trd>styl</trd>
  </trdgrp>
  <trd lng="de">Stil</trd>
  <trd lng="en">style</trd>
  <trd lng="es">estilo</trd>
  <trd lng="fr">style</trd>
  <trd lng="hu">st&iacute;lus</trd>
  <trd lng="id">gaya</trd>
  <trd lng="nl">stijl</trd>
  <trd lng="ru">&c_s;&c_t;&c_i;&c_l;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>sloh</trd>,
    <trd>&scaron;t&yacute;l</trd>
  </trdgrp>
</drv>

<drv mrk="stil.bon0a">
  <kap>bon<tld/>a</kap>
  <snc mrk="stil.bon0a.BELE">
    <dif>
      <ref tip="vid" cel="elegan.0a">Eleganta</ref> kaj pla&ccirc;a per sia
      <tld/>o, sen eraroj, krudo a&ubreve; pezeco:
      <ekz>
        ege interesa kaj bon<tld/>a letero
        <fnt>
        <aut>E. Bar-Jahalom</aut>,
        <vrk>Kiu lingvo pli facilas</vrk>,
        <lok>soc.culture.esperanto, 1993-02-10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">dobr&eacute;ho stylu</trd>
  <trd lng="fr">bien tourn&eacute;</trd>
      <trdgrp lng="ru">
        <trd>&c_h;&c_o;&c_r;&c_o;&c_sh;&c_je;&c_g;&c_o; <ind>&c_s;&c_t;&c_i;&c_l;&c_ja;</ind></trd>,
        <trd>&c_s;&c_t;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
      </trdgrp>
  <trd lng="sk">dobr&eacute;ho &scaron;t&yacute;lu</trd>
</drv>

<drv mrk="stil.jun0o">
  <kap>
    jun<tld/>o
  </kap>
  <snc mrk="stil.jun0o.secesia">
    <dif>
      <ref tip="super" cel="secesi.0a_stilo">Secesia stilo</ref>:
      <ekz>
        la &ccirc;efaj verkoj de <nom>Schiele</nom> kaj <nom>Klimt</nom>
        pa&scirc;on post pa&scirc;o montras la evoluon de jun<tld/>o al ekspresionismo
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="https://www.monato.be/2014/011324.php"
              >Tamen arto!</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Jugendstil</trd>
  <trd lng="en">modern style</trd>
  <trd lng="fr">art nouveau</trd>
</drv>

<drv mrk="stil.nov0a">
  <kap>nov<tld/>a</kap>
  <snc mrk="stil.nov0a.KOMUNE">
    <dif>
      Kiu akordas kun nova <tld/>o;
      <ref tip="vid" cel="mod.laux0a.MOR">la&ubreve;moda</ref>:
      <ekz>
        kursoj nov<tld/>aj por instruontoj
        <fnt>
          <aut>Jungkee Lee</aut>,
          <vrk><url
          ref="http://esperanto.center/2016/10/07/111?ckattempt=1">
          Kursoj novstilaj por instruontoj,
          video-tradukemuloj</url></vrk>,
          <lok>UEA, 2016</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>&agrave; la page</trd>,
    <trd>tendance<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="stil.kolonia0o">
  <kap>kolonia <tld/>o</kap>
  <snc mrk="stil.kolonia0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Konstru- kaj ornam<tld/>o de domoj en tropika klimato,
      kun balkonoj, verandoj kaj similaj plezuraran&gcirc;oj,
      origine uzita de e&ubreve;ropaj kolonianoj:
      <ekz>
        apud belaj koloni<tld/>aj vilaoj kun &gcirc;ardenoj kaj komfortaj 
        apartamentoj, etendi&gcirc;as implika&jcirc;o de malpuraj stratetoj
        lo&gcirc;ataj 
        de amaso da malri&ccirc;uloj
        <fnt>
          <aut>W. Kristianasen, trad. J.-M. Cash</aut>,
          <vrk><url ref="http://eo.mondediplo.com/article1441.html">
          Enrigardo en la islaman Baraton</url></vrk>, 
          <lok>le Monde diplomatique, 2009-01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">style colonial</trd>
  <trd lng="id"><ind>gaya</ind> kolonial</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_o;&c_n;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_s;&c_t;&c_i;&c_l;&c_mol;</trd>
</drv>
</art>
