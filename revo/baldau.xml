      <ekz>
        &scirc;i ne volis pensi pri <tld/>o,
        sed gustumi la nunajn liberajn flugojn
        de sentoj
        <fnt><aut>S. Larbar</aut>, <vrk>Karuseloj</vrk>, 
        <lok>1987</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x8FD1;&#x671F; <pr>j&#xEC;nq&#x12B;</pr></trd>
</snc>
  <trd lng="be">&c_n;&c_a;&c_j;&c_b;&c_l;&c_ib;&c_zh;&c_e;&c_j;&c_sh;&c_y; &c_ch;&c_a;&c_s;</trd>
  <trdgrp lng="de">
    <trd>nahe <ind>Zukunft</ind></trd>,
    <trd>das <ind>Morgen</ind></trd>,
    <trd>die n&auml;chste <ind>Zeit</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>imminence</trd>,
    <trd>imminency <klr>(seldom)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>inminentemente</trd>,
    <trd>en el futuro cercano</trd>
  </trdgrp>
  <trd lng="fr">le <ind>futur proche</ind></trd>
