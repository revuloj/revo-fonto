<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>balda&ubreve;</rad> <fnt><bib>PV</bib></fnt> 
</kap> 
<drv mrk="baldau.0">
  <kap><ofc>*</ofc><tld/></kap>
  <gra><vspec>adverbo</vspec></gra>
  <snc mrk="baldau.0.KOMUNE">
    <dif>
      Post mallonga tempo:
      <ekz>
        mi ne atendis vin tiel <tld/>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_u;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_n;&c_je;&c_w;&c_z;&c_a;&c_b;&c_a;&c_v;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>bremaik</trd>,
    <trd>hep <ind>dale</ind></trd>,
    <trd>dizale</trd>
  </trdgrp>
  <trd lng="cs">brzy</trd>
  <trd lng="de">bald</trd>
  <trdgrp lng="es">
    <trd>pronto</trd>,
    <trd>dentro de poco</trd>
  </trdgrp>
  <trd lng="en">soon</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_h; &a_z;&a_w;&a_d;&f_y;</trd>
    <trd>&a_z;&a_w;&a_d;</trd>
  </trdgrp>
  <trdgrp lng="fi">
    <trd>pian</trd>,
    <trd>kohta</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bient&ocirc;t</trd>,
    <trd>t&ocirc;t</trd>,
    <trd>sous peu</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&bet;&qof;&resh;&vav;&bet;</trd>,
    <trd>&bet;&mem;&he;&resh;&he;</trd>,
  </trdgrp>
  <trd lng="hu">hamarosan</trd>
  <trd lng="id">segera</trd>
  <trdgrp lng="ca">
    <trd>prest</trd>,
    <trd>aviat</trd>,
    <trd>prompte</trd>,
    <trd>tost</trd>
  </trdgrp>
  <trdgrp lng="nl">
      <trd>weldra</trd>,
      <trd>spoedig</trd>
    </trdgrp>
  <trdgrp lng="pl">
    <trd>wkr&oacute;tce</trd>,
    <trd>wnet</trd>,
    <trd>niebawem</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>em breve</trd>,
    <trd>brevemente</trd>
  </trdgrp>
  <trd lng="ro">&icirc;n cur&acirc;nd</trd>
  <trdgrp lng="ru">
     <trd>&c_s;&c_k;&c_o;&c_r;&c_o;</trd>,
     <trd>&c_v;&c_s;&c_k;&c_o;&c_r;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">skoro <klr>(r&yacute;chlo)</klr></trd>
  <trd lng="sv">snart</trd>
  <trd lng="vo">sun</trd>
</drv>

<drv mrk="baldau.0a">
  <kap><tld/>a</kap>
  <snc mrk="baldau.0a.KOMUNE">
    <dif>
      Okazonta post mallonga tempo:
      <ekz>
        &gcirc;is <tld/>a revido.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>imminent</trd>,
    <trd>forthcoming</trd>, <trd>impending</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>prochain</trd>,
    <trd>proche<klr> (dans le temps)</klr></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>prest</trd>,
    <trd>prompte</trd>
  </trdgrp>
  <trd lng="pl">rych&lstroke;y</trd>
</drv>

<drv mrk="baldau.0o">
  <kap><tld/>o</kap>
  <snc mrk="baldau.0o.KOMUNE">
    <dif>
      Proksima estonteco:
      <ekz>
        &scirc;i ne volis pensi pri <tld/>o,
        sed gustumi la nunajn liberajn flugojn
        de sentoj
        <fnt><aut>S. Larbar</aut>, <vrk>Karuseloj</vrk>, 
        <lok>1987</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>imminence</trd>,
    <trd>imminency <klr>(seldom)</klr></trd>
  </trdgrp>
  <trd lng="fr">le <ind>futur proche</ind></trd>
</drv>
</art>
