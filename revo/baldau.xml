<drv mrk="baldau.0a">
  <kap><tld/>a</kap>
  <snc mrk="baldau.0a.KOMUNE">
    <dif>
      Okazonta post mallonga tempo:
      <ekz>
        <nom>Knut</nom> estas en plena malespero pro mia <tld/>a forveturo
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>!
      </ekz>
      <ekz>
        nenio malhelpas vian <tld/>an revenon
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        esperante viajn <tld/>ajn mendojn
        <fnt>
          <bib>LSB</bib>
          <lok>La Stranga Butiko</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="proksi.0a.kiam">proksima<sncref/></ref>,
      <ref cel="ven.0onta">venonta</ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#x968F;&#x5373; <pr>su&#xED;j&#xED;</pr></trd>,
    <trd>&#x9760;&#x8FD1; <pr>k&#xE0;oj&#xEC;n</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_j;&c_b;&c_l;&c_ib;&c_zh;&c_e;&c_j;&c_sh;&c_y; <klr>(&c_u; &c_ch;&c_a;&c_s;&c_je;)</klr></trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_u;&c_p;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_y;&c_h;&c_o;&c_d;&c_z;&c_ja;&c_ch;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>prest</trd>,
    <trd>prompte</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>bevorstehend</trd>,
    <trd>baldigst</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>imminent</trd>,
    <trd>forthcoming</trd>, <trd>impending</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>pr&oacute;ximo<klr> (en el tiempo)</klr></trd>,
    <trd>inminente</trd>
  </trdgrp>
  <trdgrp lng="fr">
