<drv mrk="kudr.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kudr.0i.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="kun.0igi">Kunigi</ref>, kunligi per fadeno a&ubreve; &scirc;nureto, kiun oni traigas
      tra la
      kunligota&jcirc;ojn per speciala pinta instrumento:
      <ekz>
        per <tld/>ilo ni <tld/>as
        <fnt><bib>F</bib>, <lok>&FE; 34</lok></fnt>;
      </ekz>
      <ekz>
        mi volas al<tld/>i butonon al mia surtuto
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        por ne piki la fingron &ccirc;e l' <tld/>ado, oni portas <ind>fingringo</ind>n
        <fnt>
          <bib>DL</bib>
        </fnt>;
        <trd lng="de">Fingerhut</trd>
      </ekz>
      <ekz>
        &scirc;i povosciis <tld/>i, <ref cel="brod.0i">brodi</ref> kaj <ref cel="trik.0i">triki</ref>
        <fnt>
          <bib>Fab4</bib>
          <lok>Knabino, kiu pa&scirc;is sur panon</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> distran&ccirc;is grandan pecon da silka &scirc;tofo kaj <tld/>is malgrandan
        beletan saketon
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum la somero vi devas <tld/>i vian dota&jcirc;on
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili kun<tld/>is foliojn de figarbo kaj faris al si zona&jcirc;ojn
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 3:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        estas tempo por dis&scirc;iri, kaj tempo por kun<tld/>i
        <fnt>
          <bib>MT</bib>
          <lok>Predikanto 3:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        neniu al<tld/>as flika&jcirc;on el nefulita drapo sur malnovan veston
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 9:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        malnova anta&ubreve;lita tapi&scirc;o <klr>[&#x2026;]</klr> kun<tld/>ita el ru&gcirc;aj kaj bluaj strioj
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        blanka vesto <klr>[&#x2026;]</klr> re<tld/>ita el pli granda, malnova vesto
        <fnt>
          <bib>Fab2</bib>
          <lok>El fenestro en Vartou</lok>
        </fnt>;
      </ekz>
      <ekz>
        en leda saketo sur la brusto li havis kelke da al<tld/>itaj oraj moneroj
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia malgranda fratino Anastazio, en<tld/>ita en felon de kapro, pendis sur mia
        dorso
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>adis iam vestojn por vi kaj mi portadis ilin al vi
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="masxin.kudro0o">kudroma&scirc;ino</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_c;&c_mol;</trd>
  <trd lng="ca">cosir</trd>
  <trdgrp lng="cs">
    <trd>&scaron;&iacute;t</trd>,
    <trd>u&scaron;&iacute;t</trd>
  </trdgrp>
  <trd lng="de">n&auml;hen</trd>
  <trdgrp lng="en">
    <trd>sew</trd>,
    <trd>stitch</trd>,
    <trd>tack</trd>
  </trdgrp>
  <trd lng="es">coser</trd>
  <trd lng="fr">coudre</trd>
  <trd lng="hu">varr</trd>
  <trdgrp lng="ja">
    <trd>&#x7E2B;&#x3046;  <pr>&#x306C;&#x3046;</pr></trd>,
    <trd>&#x7E2B;&#x3044;&#x5408;&#x308F;&#x305B;&#x308B;  <pr>&#x306C;&#x3044;&#x3042;&#x308F;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">naaien</trd>
  <trdgrp lng="pl">
    <trd>zszy&cacute;</trd>,
    <trd>szy&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>costurar</trd>,
    <trd>coser</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">&scaron;i&tcaron;</trd>
  <trd lng="sv">sy</trd>
  <trd lng="uk">&c_sh;&c_i;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x7E2B; <pr>f&#xE9;ng</pr></trd>,
    <trd>&#x7F1D; <pr>f&#xE9;ng</pr></trd>,
    <trd>&#x6453; <pr>f&#xE9;ng</pr></trd>,
    <trd>&#x7E2B;&#x88FD; <pr>f&#xE9;ngzh&#xEC;</pr></trd>,
    <trd>&#x7F1D;&#x5236; <pr>f&#xE9;ngzh&#xEC;</pr></trd>,
    <trd>&#x7E2B;&#x7D4D; <pr>f&#xE9;ngr&#xE8;n</pr></trd>,
    <trd>&#x7F1D;&#x7D4D; <pr>f&#xE9;ngr&#xE8;n</pr></trd>,
    <trd>&#x6579; <pr>li&#xE1;o</pr></trd>,
    <trd>&#x7E2B;&#x5408; <pr>f&#xE9;ngh&#xE9;</pr></trd>,
    <trd>&#x7F1D;&#x5408; <pr>f&#xE9;ngh&#xE9;</pr></trd>,
    <trd>&#x505A;&#x8863;&#x670D; <pr>zu&#xF2;y&#x12B;fu</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kudr.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Kunigo farita per <tld/>ado:
      <ekz>
        tiu ligilo <klr>(per gluo)</klr> estis pli fortika ol la <tld/>o
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la tuniko estis <ind>sen<tld/>a</ind>, desupre teksita tra la tuto
        <fnt>
          <bib>NT</bib>
          <lok>Johano 19:23</lok>
        </fnt>;
        <trd lng="be">&c_b;&c_ja;&c_s;&c_sh;&c_v;&c_o;&c_v;&c_y;</trd>
        <trd lng="de">nahtlos</trd>
        <trd lng="en">seamless</trd>
      </ekz>
      <ekz>
        la &scirc;uoj por la dancoj estas ellaboritaj el blanka lano kaj ne havas
        <tld/>oliniojn
        <fnt>
          <aut>Maxime Lechien</aut>
          <vrk><url ref="&Monato;/2000/006955.php">Fama karnavalo en Binche</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 16</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_v;&c_o;</trd>
  <trd lng="de">Naht</trd>
  <trd lng="en">seam</trd>
  <trdgrp lng="ja">
    <trd>&#x7E2B;&#x3046;&#x3053;&#x3068;  <pr>&#x306C;&#x3046;&#x3053;&#x3068;</pr></trd>,
    <trd>&#x7E2B;&#x3044;&#x76EE;  <pr>&#x306C;&#x3044;&#x3081;</pr></trd>,
    <trd>&#x7E2B;&#x3044;&#x5408;&#x308F;&#x305B;  <pr>&#x306C;&#x3044;&#x3042;&#x308F;&#x305B;</pr></trd>
  </trdgrp>
  <trd lng="nl">naad</trd>
  <trdgrp lng="zh">
    <trd>&#x63A5;&#x7E2B; <pr>ji&#x113;f&#xE8;ng</pr></trd>,
    <trd>&#x63A5;&#x7F1D; <pr>ji&#x113;f&#xE8;ng</pr></trd>,
    <trd>&#x7E2B;&#x91DD;&#x8DE1; <pr>f&#xE9;ngzh&#x113;nj&#xEC;</pr></trd>,
    <trd>&#x7F1D;&#x9488;&#x8FF9; <pr>f&#xE9;ngzh&#x113;nj&#xEC;</pr></trd>
  </trdgrp>
</drv>


<drv mrk="kudr.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc>
    <dif>
      Tio, kion oni pri<tld/>as, kio ne estas fin<tld/>ita:
      <ekz>
        tra la densa nebulo, kiu kovris &scirc;iajn okulojn, &scirc;i ne vidis plu la <tld/>a&jcirc;on
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> kaptis de la kuireja tablo korbon kun tenilo <klr>[&#x2026;]</klr> por
        la <tld/>a&jcirc;o
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_t;&c_v;&c_o;</trd>
  <trdgrp lng="de">
    <trd>N&auml;harbeit</trd>,
    <trd>N&auml;hzeug</trd>
  </trdgrp>
  <trd lng="ja">&#x7E2B;&#x3044;&#x7269;  <pr>&#x306C;&#x3044;&#x3082;&#x306E;</pr></trd>
  <trd lng="nl">naaiwerk</trd>
  <trdgrp lng="uk">
    <trd>&c_sh;&c_i;&c_t;&c_t;&#x44F; (&#x43F;&c_r;&c_je;&c_d;&#x43C;&c_je;&c_t;)</trd>,
    <trd>&c_sh;&c_v;&c_je;&c_j;&#x43D;&c_i;&c_j; &c_v;&c_i;&c_r;&c_ib;&c_b;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x505A;&#x91DD;&#x7DDA;&#x6D3B; <pr>zu&#xF2;zh&#x113;nxi&#xE0;nhu&#xF3;</pr></trd>,
    <trd>&#x505A;&#x9488;&#x7EBF;&#x6D3B; <pr>zu&#xF2;zh&#x113;nxi&#xE0;nhu&#xF3;</pr></trd>
  </trdgrp>
</drv>


<drv mrk="kudr.0ejo">
  <kap>
    <tld/>ejo
  </kap>
  <snc>
    <dif>
      <ref cel="meti.0ejo">Metiejo</ref>, laborejo, kie oni <tld/>as ks:
      <ekz>
        pa&ccirc;jo iras en la oficejon, la fratineto en la <tld/>ejon
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi estas jam tiel kompetenta, ke mi povus mem fondi <tld/>ejon a&ubreve; magazenon, se
        mi havus monon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili malsupreni&gcirc;is al la <tld/>ejo, kie ia ma&scirc;ino <klr>[&#x2026;]</klr> &ccirc;irka&ubreve;pasis la&ubreve;
        la randoj de la vestoj
        <fnt>
          <bib>VaK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_ja;</trd>,
    <trd>&c_sh;&c_v;&c_je;&c_j;&c_n;&c_a;&c_ja; &c_m;&c_a;&c_j;&c_s;&c_t;&c_e;&c_r;&c_n;&c_ja;</trd>,
    <trd>&c_sh;&c_v;&c_je;&c_j;&c_n;&c_y; &c_c;&c_e;&c_h;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>N&auml;herei</trd>,
    <trd>N&auml;hstube</trd>
  </trdgrp>
  <trd lng="ja">&#x88C1;&#x7E2B;&#x5BA4;  <pr>&#x3055;&#x3044;&#x307B;&#x3046;&#x3057;&#x3064;</pr></trd>
  <trdgrp lng="uk">
    <trd>&c_sh;&c_v;&c_a;&#x43B;&#x44C;&#x43D;&#x44F;</trd>,
    <trd>&c_sh;&c_v;&c_a;&c_c;&#x44C;&#x43A;&c_a;/&c_sh;&c_v;&c_je;&c_j;&#x43D;&c_a; &#x43C;&c_a;&c_j;&c_s;&c_t;&c_je;&c_r;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>


