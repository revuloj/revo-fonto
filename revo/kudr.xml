<drv mrk="kudr.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kudr.cxirkaux0i.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <tld lit="K"/>i &ccirc;e la &ccirc;irka&ubreve;o:
      <ekz>
        &scirc;i &ccirc;irka&ubreve;<tld/>adis &scirc;uojn
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        en vintra tempo la hundo neniam eliris, ne ricevinte sur
        sin pritonditan kaj bele &ccirc;irka&ubreve;<tld/>itan
        &scirc;afidan felon
        <fnt><bib>Fab1</bib><lok>Kupra porko</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i portis pelton el blua atlaso, &ccirc;irka&ubreve;<tld/>itan per cigna lanugo
        <fnt>
        <aut>E. Orzeszko, trad. K. Bein</aut>
          <vrk><url ref="http://esperanto.nu/upsala/la_bona.html">
          La bona sinjorino</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        jen estas puntoj <nom>Cluny</nom> por &ccirc;irka&ubreve;<tld/>ado
        de falbaloj
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="border.0i.sxtofon">borderi</ref>,
      <ref cel="orl.0i">orli</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_sh;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>orlar</trd>,
    <trd>enrivetar</trd>
  </trdgrp>
  <trd lng="de">umn&auml;hen</trd>
  <trd lng="hu">beszeg</trd>
  <trd lng="nl">borduren<klr tip="ind"> (randen met naald
  aanbrengen)</klr></trd>
  <trdgrp lng="pt">
    <trd>embainhar</trd>,
    <trd>debruar</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>obszy&cacute;</trd>,
    <trd>obszywa&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_b;&c_sh;&c_i;&c_t;&c_mol;</trd>
  <trd lng="uk">&#x43E;&c_b;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="kudr.flik0i">
  <kap>flik<tld/>i</kap>
  <snc>
    <dif>
      <ref tip="super" cel="flik.0i.ripari">Fliki</ref> per al<tld/>o:
      <ekz>
        pupo, kiu estis iom malnova kaj havis flik<tld/>itan kolon
        <fnt><bib>Fab3</bib>, <lok>&scirc;parmonujo</lok></fnt>;
      </ekz>
      <ekz>
        li devis mem brosadi al si siajn botojn kaj flikadi ilin per flik<tld/>ilo
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_sh;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_l;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_l;&c_a;&c_t;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">sargir</trd>
  <trdgrp lng="de">
    <trd>flicken</trd>,
    <trd>stopfen <klr>(Strumpf)</klr></trd>
  </trdgrp>
  <trd lng="fr">rapi&eacute;cer<klr> (un tissu)</klr></trd>
  <trdgrp lng="hu">
