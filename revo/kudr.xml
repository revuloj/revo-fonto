  </trdgrp>
</drv>

<drv mrk="kudr.mal0i">
  <kap>mal<tld/>i</kap>
  <snc mrk="kudr.mal0i.TEKS">
    <dif>
      Forigi la fadenon de <tld/>o, disigi kun<tld/>itajn
      a&jcirc;ojn:
      <ekz>
        katastrofo! &ccirc;u vi &gcirc;in mal<tld/>os
        <fnt>
          <bib>IK</bib>
        </fnt>?
      </ekz>
      <ekz>
        mal<tld/>inte flankon de la sako, mi for&scirc;telis, ne panon,
        sed grandajn pecojn da fritita lardo
        <fnt>
          <aut>E. de Bonaero</aut>,
          <vrk>La Friponeto de Tormes</vrk>,
          <lok>1973</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">auftr&ouml;seln</trd>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_p;&c_o;&c_r;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">descosir</trd>
  <trdgrp lng="cs">
    <trd>p&aacute;rat</trd>,
    <trd>rozp&aacute;rat</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;coudre</trd>
  <trd lng="ja">&#x307B;&#x3069;&#x304F;</trd>
  <trdgrp lng="sk">
    <trd>odp&aacute;ra&tcaron; <klr>(&scaron;it&eacute;)</klr></trd>,
    <trd>vyp&aacute;ra&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x43E;&c_r;&#x43E;&c_t;&c_i;</trd>
</drv>
