    <trd>&c_l;&c_a;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_l;&c_a;&c_t;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4FEE;&#x8865; <pr>xi&#x16B;b&#x1D4;</pr></trd>,
    <trd>&#x4FEE;&#x88DC; <pr>xi&#x16B;b&#x1D4;</pr></trd>,
    <trd>&#x4FEE;&#x8865;&#x8FC7; <pr>xi&#x16B;b&#x1D4;gu&#xF2;</pr></trd>,
    <trd>&#x4FEE;&#x88DC;&#x904E; <pr>xi&#x16B;b&#x1D4;gu&#xF2;</pr></trd>,
    <trd>&#x6253;&#x4E86;&#x8865;&#x4E01; <pr>d&#x1CE;li&#x1CE;ob&#x1D4;d&#x12B;ng</pr></trd>,
    <trd>&#x6253;&#x4E86;&#x88DC;&#x4E01; <pr>d&#x1CE;li&#x1CE;ob&#x1D4;d&#x12B;ng</pr></trd>,
