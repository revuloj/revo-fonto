  <snc mrk="plank.0o.KON">
    <uzo tip="fak">KON</uzo>
    <dif>
      Malsupra <ref tip="super" cel="eben.0o.tereno">ebeno</ref> de &ccirc;ambro, sur kiu oni pa&scirc;as:
      <ekz>
        &scirc;tona <tld/>o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        mozaika <tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iujn <tld/>ojn li kovris per cedro
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 7:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bona poeto ku&scirc;is sur la <tld/>o kaj ploris
        <fnt>
          <bib>Fab1</bib>
          <lok>Malbonkonduta knabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li mar&scirc;adas sur la <tld/>o tien kaj reen
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o estis &scirc;mirvi&scirc;ata
        <fnt>
          <bib>Fab1</bib>
          <lok>Fluganta kofro</lok>
        </fnt>;
      </ekz>
      <ekz>
        du musetoj <klr>[&#8230;]</klr> lo&gcirc;as sub la <tld/>o en la provizejo
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tapi&scirc;o sur la <tld/>o estis dika kaj mola
        <fnt>
          <bib>Fab3</bib>
          <lok>Lasta perlo</lok>
        </fnt>.
      </ekz>
