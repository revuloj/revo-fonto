<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>plank</rad>/o
</kap>
<drv mrk="plank.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Horizontala ebeno konsistiganta malsupran internan parton de &ccirc;ambro,
      galerio ks, kaj sur kiu oni pa&scirc;as:
      <ekz>
        ligna, mozaika <tld/>o;
      </ekz>
      <ekz>
        <tld/>o de ponto;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu movado ekzistas nur <ind>sub la <tld/>o</ind>
        <klr>(sekrete, ka&scirc;e)</klr>.
        <trd lng="be">&c_p;&c_a;&c_d;&c_p;&c_o;&c_l;&c_mol;&c_n;&c_a;</trd>
        <trd lng="ru">&c_p;&c_o;&c_d;&c_p;&c_o;&c_l;&c_mol;&c_n;&c_o;</trd>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="pavim.0o">pavimo</ref>,
      <ref cel="parget.0o">pargeto</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_l;&c_o;&c_g;&c_a;</trd>
  <trd lng="cs">podlaha</trd>
  <trd lng="de">Fu&szlig;boden</trd>
  <trd lng="en">floor</trd>
  <trd lng="hu">padl&oacute;</trd>
  <trd lng="nl">vloer</trd>
  <trdgrp lng="pt">
    <trd>soalho</trd>,
    <trd>assoalho</trd>,
    <trd>pavimento</trd>,        
    <trd>piso</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_l;</trd>
  <trd lng="sv">golv</trd>
</drv>
</art>
