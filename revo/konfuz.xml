    <trd lng="ru">&c_n;&c_je;&c_v;&c_o;&c_z;&c_m;&c_u;&c_t;&c_i;&c_m;&c_y;&c_j;</trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x53D6;&#x308A;&#x9055;&#x3048;&#x3088;&#x3046;&#x306E;&#x306A;&#x3044;  <pr>&#x3068;&#x308A;&#x3061;&#x304C;&#x3048;&#x3088;&#x3046;&#x306E;&#x306A;&#x3044;</pr></trd>,
    <trd>&#x660E;&#x3089;&#x304B;&#x306A;  <pr>&#x3042;&#x304D;&#x3089;&#x304B;&#x306A;</pr></trd>,
    <trd>&#x843D;&#x3061;&#x7740;&#x304D;&#x306F;&#x3089;&#x3063;&#x305F;  <pr>&#x304A;&#x3061;&#x3064;&#x304D;&#x306F;&#x3089;&#x3063;&#x305F;</pr></trd>
  </trdgrp>
  <trd lng="pl">niew&aogonek;tpliwy</trd>     
  <trd lng="ro">ne&icirc;ndoios</trd>
</drv>

<drv mrk="konfuz.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="konfuz.sen0a.orda">
    <dif>
      Sen <tld/>o, hezito a&ubreve; eraro, <ref cel="rekt.0a.senpera">rekta</ref>, 
      <ref tip="sin" cel="klar.0a.malkonfuza">klara</ref>:
      <ekz>
        vojo natura, sen<tld/>a kaj sendan&gcirc;era
        <fnt>
          <bib>F</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        Neniu povas resti tute pura // A&ubreve; sen<tld/>a. Ni ne devas esti // Ju&gcirc;antoj pri
        ni mem
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Josefina</nom> &scirc;atas nin kvaza&ubreve; &scirc;i estus nia baptopatrino, &ndash; sen<tld/>e klarigis
        <nom>Lidia</nom>
        <fnt>
          <bib>LOdE</bib>
          <aut>Georgij Kokolija</aut>
          <vrk><url ref="&Tekstaro;?nomo=ondo-de-esperanto">Mensogulinoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Ma</nom> admonis ke &scirc;i fondu apartajn kontojn por la du domoj 
        respektive, kaj sen<tld/>e
        <fnt>
          <aut>Pu Songling, trad. Guozhu</aut>,
          <vrk>La Krizantema Feino</vrk>,
          <lok><url ref="http://www.elerno.cn/penseo/pen71.htm">
          Penseo, 1997-09, numero 71a</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pur.0a.neniu_malbona">pura<sncref/></ref>,
      <ref cel="artifi.sen0a">senartifika</ref>,
      <ref cel="virg.0a">virga</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>ordentlich</trd>,
      <trd>wohlgeordnet</trd>,
      <trd>geradlinig</trd>,
      <trd>unbefangen</trd>,
      <trd>eindeutig</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_z;&c_a;&c_b;&c_l;&c_y;&c_t;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_ja;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_p;&c_a;&c_m;&c_y;&c_l;&c_k;&c_o;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>distinctement</trd>,
    <trd>sans m&eacute;prise</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6DF7;&#x4E71;&#x306E;&#x306A;&#x3044;  <pr>&#x3053;&#x3093;&#x3089;&#x3093;&#x306E;&#x306A;&#x3044;</pr></trd>,
    <trd>&#x306F;&#x3063;&#x304D;&#x308A;&#x3057;&#x305F;</trd>,
    <trd>&#x660E;&#x78BA;&#x306A;  <pr>&#x3081;&#x3044;&#x304B;&#x304F;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wyra&zacute;nie</trd>,
    <trd>bez pomy&lstroke;ki</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>clar</trd>,
    <trd>f&abreve;r&abreve; eroare</trd>
  </trdgrp> 
  <trdgrp lng="zh">
    <trd>&#x62C6;&#x5206; <pr>ch&#x101;if&#x113;n</pr></trd>,
    <trd>&#x5206;&#x660E; <pr>f&#x113;nm&#xED;ng</pr></trd>
  </trdgrp>
</drv>
