<drv mrk="konfuz.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Tiel senorda, ke oni nenion povas distingi; implika:
      <ekz>
        malfacila kaj <tld/>a malreguleco de la akcento<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a murmuro, bruo de vo&ccirc;oj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>aj ideoj, rememoroj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a parolo, stilo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Esperanto perfekti&gcirc;ados per vojo natura, sen<tld/>a kaj
        sendan&gcirc;era<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">durcheinander</trd>
    <trd lng="es">confuso</trd>
    <trd lng="hu">zavaros</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_u;&c_t;&c_a;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_p;&c_u;&c_t;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_z;&c_a;&c_p;&c_u;&c_t;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Maltrankvila, senaplomba, perpleksa, hontigita:
      <ekz>
        li restis <tld/>a anta&ubreve; la pecoj de la rompita vazo.
      </ekz>
    </dif>
    <rim>
      Oni uzas plejofte <em>konfuza</em> en la senco 1, kaj
      <em>konfuzita</em>
      en la senco 2.
    </rim>
    <trdgrp lng="de">
      <trd>verwirrt</trd>,
      <trd>verst&ouml;rt</trd>
    </trdgrp>
    <trd lng="hu">zavart</trd>
    <trd lng="ru">&c_s;&c_m;&c_u;&c_shch;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trd lng="bo">&t_ma;&t_ga;&t_naro;&t_tsheg;&t_ra;&t_nyata;&t_naro;&t_ga;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>rozh&aacute;ran&yacute;</trd>,
    <trd>zmaten&yacute;</trd>
  </trdgrp>
  <trd lng="fr">confus</trd>
  <trd lng="nl">verwarrend</trd>
  <trdgrp lng="pl">
    <trd>ba&lstroke;amutny</trd>,
    <trd>bez&lstroke;adny</trd>,    
    <trd>chaotyczny</trd>,    
    <trd>myl&aogonek;cy</trd>,    
    <trd>zawi&lstroke;y</trd>,    
    <trd>zawstydzony</trd>,  
    <trd>zmieszany</trd>    
  </trdgrp>
  <trdgrp lng="ro">
    <trd>confuz</trd>,
    <trd>haotic</trd>,    
    <trd>&icirc;n&#537;el&abreve;toare</trd>,       
    <trd>jenat</trd>    
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rozh&aacute;ran&yacute;</trd>,
    <trd>zm&auml;ten&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="konfuz.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Io <tld/>a:
      <ekz>
        oni renkontas en la lingvoj naturaj multegajn malfacila&jcirc;ojn kaj
        <tld/>a&jcirc;ojn<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Durcheinander</trd>,
      <trd>Verwirrung</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>warboel</trd>,
      <trd>chaos</trd>
    </trdgrp>
    <trd lng="hu">zavaros &uuml;gy</trd>
    <trd lng="ru">&c_p;&c_u;&c_t;&c_a;&c_n;&c_i;&c_c;&c_a;</trd>
  </snc>
  <trd lng="fr">confusion</trd>
  <trd lng="pl">chaos</trd>     
  <trd lng="ro">confuzie</trd>
</drv>

<drv mrk="konfuz.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="konfuz.0igxi.malordigxi">
    <dif>
      Miksi&gcirc;i, malordi&gcirc;i:
      <ekz>
        &ccirc;io <tld/>i&gcirc;is en lia spirito;
      </ekz>
      <ekz>
        &ccirc;iurangaj homoj <tld/>i&gcirc;as en la abismo de la morto.
      </ekz>
    </dif>
  </snc>
  <snc mrk="konfuz.0igxi.maldistingigxi">
    <dif>
      Intermiksi&gcirc;i, maldistingi&gcirc;i.
    </dif>
  </snc>
  <snc mrk="konfuz.0igxi.perpleksigxi">
    <dif>
      Senaplombi&gcirc;i, resti senhelpa, ne plu scii kiel agi:
      <ekz>
        prenite &ccirc;e l' fre&scirc;a faro li <tld/>i&gcirc;is.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>d&eacute;contenanc&eacute;</ind></trd>,
    <trd>s'<ind>embrouiller</ind></trd>,
    <trd>se <ind>troubler</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zaczerwieni&cacute; si&eogonek;</trd>,
    <trd>zmiesza&cacute; si&eogonek;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>&icirc;nro&#537;i</trd>,
    <trd>se amestec&abreve;</trd>
  </trdgrp> 
</drv>

<drv mrk="konfuz.inter0i">
  <kap>inter<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="konfuz.inter0i.malordigi">
    <dif>
      Malorde <ref tip="vid" cel="miks.inter0i.KOMUNE">intermiksi</ref>;
      <ref tip="dif" cel="konfuz.0i.malordigi"><tld/>i<sncref/></ref>:
      <ekz>
        inter<tld/>itaj haroj<fnt>Z</fnt>,
      </ekz>
      <ekz>
        vojetoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="fr">embrouiller</trd>
  </snc>
  <snc mrk="konfuz.inter0i.maldistingi">
    <dif>
      Kredi ke iu afero estas alia afero, a&ubreve; ke iu &gcirc;ia eco estas
      alia ol reale;
      <ref tip="dif" cel="konfuz.0i.maldistingi"><tld/>i<sncref/></ref>.
    </dif>
    <trd lng="fr">confondre<klr> (des choses entre elles)</klr></trd>
  </snc>
  <trd lng="pl">pomiesza&cacute;</trd>     
  <trd lng="ro">amesteca</trd>
</drv>

<drv mrk="konfuz.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malmiksi, ordigi, malimpliki, klarigi:
      <ekz>
        mal<tld/>i &hcirc;aoson da libroj, siajn ideojn, malsimplan demandon
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="distin.0i">distingi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>rozpl&eacute;st</trd>,
    <trd>uspo&rcaron;&aacute;dat</trd>,
    <trd>vyjasnit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>entwirren</trd>,
    <trd>ordnen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;m&ecirc;ler<klr> (mettre au clair)</klr></trd>,
    <trd>&eacute;claircir<klr> (mettre au clair)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tiszt&aacute;z</trd>,
    <trd>rendez</trd>
  </trdgrp>
  <trd lng="nl">ontwarren</trd>
  <trdgrp lng="pl">
    <trd>odr&oacute;&zdot;nia&cacute;</trd>,
    <trd>rozr&oacute;&zdot;nia&cacute;</trd>
  </trdgrp>  
  <trd lng="ro">distinge</trd>
  <trdgrp lng="sk">
    <trd>usporiada&tcaron;</trd>,
    <trd>vyjasni&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="konfuz.ne0ebla">
  <kap>ne<tld/>ebla</kap>
  <snc>
    <dif>
      Ne povanta esti maldistingita de:
      <ekz>
        la du kvalitoj estas ne<tld/>eblaj.
      </ekz>
    </dif>
    <trd lng="fr">impossible &agrave; confondre</trd>
    <trd lng="hu">&ouml;sszet&eacute;veszthetetlen</trd>
    <trd lng="nl">duidelijk <ind>onderscheidend</ind></trd>
    <trd lng="ru">&c_k;&c_o;&c_t;&c_o;&c_r;&c_y;&c_j; &c_n;&c_je;&c_v;&c_o;&c_z;&c_m;&c_o;&c_zh;&c_n;&c_o; &c_s;&c_p;&c_u;&c_t;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Ne povanta esti maltrankviligita; &ccirc;iamflegma:
      <ekz>
        ne<tld/>ebla krimulo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aplomb.0o">aplombo</ref>
    </refgrp>
    <trd lng="fr">imperturbable</trd>
    <trd lng="hu">rend&iacute;thetetlen</trd>
    <trdgrp lng="nl">
      <trd>onverstoorbaar</trd>,
      <trd>flegmatiek</trd>
    </trdgrp>
    <trd lng="ru">&c_n;&c_je;&c_v;&c_o;&c_z;&c_m;&c_u;&c_t;&c_i;&c_m;&c_y;&c_j;</trd>
  </snc>
  <trd lng="pl">niew&aogonek;tpliwy</trd>     
  <trd lng="ro">ne&icirc;ndoios</trd>
</drv>

<drv mrk="konfuz.sen0e">
  <kap>sen<tld/>e</kap>
  <snc mrk="konfuz.sen0e.ARG">
    <dif>
      Sen <tld/>o, hezito a&ubreve; eraro, rekte, klare:
      <ekz>
        Ma admonis ke &scirc;i fondu apartajn kontojn por la du domoj 
        respektive, kaj sen<tld/>e
        <fnt>
          <aut>Pu Songling, trad. Guozhu</aut>,
          <vrk>La Krizantema Feino</vrk>,
          <lok><url ref="http://www.elerno.cn/penseo/pen71.htm">
          Penseo, 1997-09, numero 71a</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>distinctement</trd>,
    <trd>sans m&eacute;prise</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wyra&zacute;nie</trd>,
    <trd>bez pomy&lstroke;ki</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>clar</trd>,
    <trd>f&abreve;r&abreve; eroare</trd>
  </trdgrp> 
</drv>
