<drv mrk="konfuz.0o">
  <kap><tld/>o</kap>
  <dif>
    Stato de iu a&ubreve; io <ref cel="konfuz.0i">konfuzita</ref>:
  </dif>
  <snc mrk="konfuz.0o.malordo" ref="konfuz.0i.malordigi">
    <ekz>
      de &ccirc;iam la babilona <tld/>o de lingvoj estis granda malhelpo al la spirita
      komuniki&gcirc;o inter la nacioj
      <fnt>
        <bib>FK</bib>
      </fnt>;
    </ekz>
    <ekz>
      ili venis al la batalejo, kaj jen ili ekvidis, ke <klr>[&#x2026;]</klr> la <tld/>o
      estas tre granda
      <fnt>
        <bib>MT</bib>
        <lok>I. Samuel 14:20</lok>
      </fnt>;
    </ekz>
    <ekz>
      kie estas &jcirc;aluzo kaj malpaco, tie estas <tld/>o kaj &ccirc;ia malnobla ago
      <fnt>
        <bib>NT</bib>
        <lok>Jakobo 3:16</lok>
      </fnt>;
    </ekz>
    <ekz>
      estas malfacile sekvi viajn pensojn, <klr>[&#x2026;]</klr> ili en mia kapo e&ccirc; zumas en
      granda <tld/>o
      <fnt>
        <bib>Fab4</bib>
        <lok>&leftquot;Bela&rightquot;</lok>
      </fnt>;
    </ekz>
    <ekz>
      <klr>[atentu ke]</klr> la kokino <klr>[&#x2026;]</klr> ne forkuru en la &gcirc;enerala <tld/>o
      <fnt>
        <bib>Fab4</bib>
        <lok>Kion la patro faras, estas &ccirc;iam &gcirc;usta</lok>
      </fnt>;
    </ekz>
    <ekz>
      dum la tumulto kaj <tld/>o mi en&scirc;teli&gcirc;is en la pre&gcirc;ejon
      <fnt>
        <bib>Rabistoj</bib>
        <lok>Akto Dua</lok>
      </fnt>;
    </ekz>
    <ekz>
      <klr>[en tiu]</klr> plej preciza ordo, <klr>[&#x2026;]</klr> unu peco da puntoj
      neatente &jcirc;etita faras <tld/>on en la magazeno
      <fnt>
        <bib>Marta</bib>
      </fnt>.
    </ekz>
    <refgrp tip="vid">
      <ref cel="hxaos.0o.FIG">&hcirc;aoso<sncref/></ref>,
      <ref cel="salat.0o.FIG">salato<sncref/></ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_b;&c_l;&c_y;&c_t;&c_a;&c_n;&c_ib;&c_n;&c_a;</trd>,
      <trd>&c_k;&c_a;&c_l;&c_a;&c_m;&c_je;&c_s;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Durcheinander</trd>,
      <trd>Verwirrung</trd>
    </trdgrp>
    <trd lng="es">confusi&oacute;n</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_m;&c_je;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_p;&c_u;&c_t;&c_a;&c_n;&c_i;&c_c;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="konfuz.0o.maldistingo" ref="konfuz.0i.maldistingi">
    <ekz>
      <tld/>o inter &leftquot;vesto&rightquot; kaj &leftquot;vesta&jcirc;o&rightquot;
      <fnt>
        <bib>Monato</bib>
        <aut>Christian Declerck</aut>
        <vrk><url ref="&Monato;/2009/009758.php">Panoramo de la Nerona Romo</url></vrk>
        <lok>2009</lok>
      </fnt>;
    </ekz>
    <ekz>
      <tld/>o inter &leftquot;l&rightquot; kaj &leftquot;r&rightquot;
      <fnt>
        <bib>Monato</bib>
        <aut>Vilhelmo Rowe </aut>
        <vrk><url ref="&Monato;/2007/009109.php">La karaktero de la &ccirc;ina lingvo kaj la sekvoj por
        Esperanto</url></vrk>
        <lok>2007</lok>
      </fnt>;
    </ekz>
    <ekz>
      ni devas peni, ke la vortoj simplaj ne donu ian <tld/>on kun vortoj kunmetitaj
      (kiel ekzemple la &gcirc;isnuna vorto defendi povus signifi de-fendi)
      <fnt>
        <bib>RdE</bib>
      </fnt>;
    </ekz>
    <ekz>
      magnetofonoj<klr>[n&#x2026;]</klr> franclingvanoj nomis &leftquot;<nac>enregistreurs</nac>&rightquot;, <klr>[&#x2026;]</klr>
      se riskis okazi <tld/>o kun alia tipo de &leftquot;registrilo&rightquot;, oni diris
      &leftquot;<nac>enregistreur magn&eacute;tique</nac>&rightquot;
      <fnt>
        <bib>BonaLingvo</bib>
      </fnt>;
    </ekz>
    <ekz>
      <klr>[li falas en]</klr> beda&ubreve;rindan <tld/>on inter siaj deziroj kaj la reala&jcirc;oj
      <fnt>
        <bib>Monato</bib>
        <aut>Andr&eacute; Cherpillod</aut>
        <vrk><url ref="&Monato;/2011/010530.php">Pura kretena&jcirc;o</url></vrk>
        <lok>2011</lok>
      </fnt>.
    </ekz>
    <trdgrp lng="be">
      <trd>&c_b;&c_l;&c_y;&c_t;&c_a;&c_n;&c_ib;&c_n;&c_a;</trd>,
      <trd>&c_z;&c_mol;&c_m;&c_ja;&c_sh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Verwechslung</trd>,
      <trd>Vermischung</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_m;&c_je;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_p;&c_u;&c_t;&c_a;&c_n;&c_i;&c_c;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="konfuz.0o.perplekso" ref="konfuz.0i.perpleksigi">
    <ekz>
      sur la viza&gcirc;o de la sankta <nom>Mefres</nom> oni povis vidi <tld/>on
      <fnt>
        <bib>Far2</bib>
        <lok>&Ccirc;apitro XVI</lok>
      </fnt>;
    </ekz>
    <ekz>
      sur la viza&gcirc;o de Mario <nom>Rudzinska</nom> aperis miro, aflikto kaj <tld/>o
      <fnt>
        <bib>Marta</bib>
      </fnt>;
    </ekz>
    <ekz>
      &scirc;ia <tld/>o devenis &gcirc;uste de la maniero, per kiu li &scirc;in rigardis
      <fnt>
        <bib>IK</bib>
      </fnt>;
    </ekz>
    <ekz>
      kun sento de feli&ccirc;o kaj doloro kuni&gcirc;is <tld/>o
      <fnt>
        <bib>IK</bib>
      </fnt>.
    </ekz>
    <trdgrp lng="be">
      <trd>&c_z;&c_mol;&c_b;&c_ja;&c_n;&c_t;&c_e;&c_zh;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_m;&c_ja;&c_sh;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Verwirrung</trd>,
      <trd>Irritation</trd>,
      <trd>Verst&ouml;rung</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_m;&c_u;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_z;&c_a;&c_m;&c_je;&c_sh;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>,
      <trd>&c_k;&c_o;&c_n;&c_f;&c_u;&c_z;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>chaos</trd>,
    <trd>nepo&rcaron;&aacute;dek</trd>,
    <trd>zmatek</trd>
  </trdgrp>
  
  <trdgrp lng="fr">
    <trd>confusion</trd>,
    <trd>m&eacute;prise</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>zavar</trd>,
    <trd>zavarodotts&aacute;g</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6DF7;&#x4E71;  <pr>&#x3053;&#x3093;&#x3089;&#x3093;</pr></trd>,
    <trd>&#x6DF7;&#x540C;  <pr>&#x3053;&#x3093;&#x3069;&#x3046;</pr></trd>,
    <trd>&#x5F53;&#x60D1;  <pr>&#x3068;&#x3046;&#x308F;&#x304F;</pr></trd>,
    <trd>&#x72FC;&#x72FD;  <pr>&#x308D;&#x3046;&#x3070;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">verwarring</trd>
  <trdgrp lng="pl">
