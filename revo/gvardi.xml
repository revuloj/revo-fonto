<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>gvardi</rad>/o</kap>
<drv mrk="gvardi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Militista ta&ccirc;mento komisiita por observi kaj gardi:
      <ekz>
        meti <tld/>on &ccirc;e &ccirc;iujn pordegojn de la urbo.
      </ekz>
    </dif>
    <trd lng="de">Garde</trd>
    <trd lng="hu">&odblac;rs&eacute;g</trd>
  </snc>
  <snc>
    <dif>
      Plej bonaj regimentoj, speciale elektitaj por la gardo de regnestro.
    </dif>
    <trdgrp lng="de">
      <trd>Leibgarde</trd>,
      <trd>Leibstandarte</trd>
    </trdgrp>
    <trd lng="hu">g&aacute;rda</trd>
  </snc>
  <adm>
    Referenci al &leftquot;gardistaro&rightquot;. <aut>MB</aut>
  </adm>
  <trd lng="en">guard</trd>
  <trd lng="es">guardia</trd>
  <trd lng="fr">garde<klr tip="ind"> (soldats)</klr></trd>
  <trd lng="nl">wacht</trd>
  <trd lng="ru">&c_g;&c_v;&c_a;&c_r;&c_d;&c_i;&c_ja;</trd>
</drv>

<drv mrk="gvardi.antaux0o">
  <kap>anta&ubreve;<tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <ref tip="dif" cel="avanga.0o">avangardo.</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Vortrupp</trd>,
    <trd>Vorhut</trd>,
    <trd>Vorausabteilung</trd>
  </trdgrp>
  <trd lng="es">vanguardia</trd>
    <trd lng="de">Garde</trd>
    <trd lng="hu">el&odblac;&ouml;rs</trd>
  <trd lng="nl">voorhoede</trd>
  <trd lng="ru">&c_a;&c_v;&c_a;&c_n;&c_g;&c_a;&c_r;&c_d;</trd>
</drv>

<drv mrk="gvardi.post0o">
  <kap>post<tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <ref tip="dif" cel="arierg.0o">ariergardo</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Nachtrupp</trd>,
    <trd>Nachhut</trd>
  </trdgrp>
  <trd lng="es">retaguardia</trd>
  <trd lng="fr">arri&egrave;re-garde</trd>
  <trd lng="hu">h&aacute;tv&eacute;d</trd>
  <trd lng="nl">achterhoede</trd>
  <trd lng="ru">&c_a;&c_r;&c_mol;&c_je;&c_r;&c_g;&c_a;&c_r;&c_d;</trd>
</drv>
</art>
