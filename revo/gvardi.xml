<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gvardi.xml,v 1.26 2023/11/11 11:36:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>gvardi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="gvardi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gvardi.0o.MIL_gardistaro">
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="prt" cel="gard.0isto">Gardist</ref>aro;
      militista <ref tip="super" cel="tacxme.0o">ta&ccirc;mento</ref> komisiita por observi kaj gardi:
      <ekz>
        meti <tld/>on &ccirc;e &ccirc;iujn pordegojn de la urbo.
      </ekz>
    </dif>
    <trd lng="ca">guarda</trd>
    <trd lng="de">Garde</trd>
    <trd lng="hu">&odblac;rs&eacute;g</trd>
  </snc>
  <snc mrk="gvardi.0o.MIL_elita_gardistaro">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Plej bonaj <ref cel="regime.0o">regimento</ref>j, speciale elektitaj por la gardo de
      regnestro:
      <ekz>
        la faraona <tld/>o
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ordonis al la unua regimento de l' <tld/>o stari&gcirc;i en vicoj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;is la sufoko de la ribelo de l' pastroj, 
        mi transdonas mian povon al Tutmozis, la &ccirc;efo de la <tld/>o
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">gu&agrave;rdia</trd>
    <trdgrp lng="de">
      <trd>Leibgarde</trd>,
      <trd>Leibstandarte</trd>
    </trdgrp>
    <trd lng="hu">g&aacute;rda</trd>
    <trd lng="zh">&#x864E;&#x8CC1; <pr>h&#x1D4;b&#x113;n</pr></trd>
</snc>
  <adm>
    Referenci al &leftquot;gardistaro&rightquot;. <aut>MB</aut>
  </adm>
  <trd lng="be">&c_g;&c_v;&c_a;&c_r;&c_d;&c_y;&c_ja;</trd>
  <trd lng="en">guard</trd>
  <trd lng="es">guardia</trd>
  <trd lng="fr">garde<klr tip="ind"> (soldats)</klr></trd>
  <trd lng="it">guardia<klr> (gruppo)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x89AA;&#x885B;&#x968A;  <pr>&#x3057;&#x3093;&#x3048;&#x3044;&#x305F;&#x3044;</pr></trd>,
    <trd>&#x8FD1;&#x885B;&#x9023;&#x968A;  <pr>&#x3053;&#x306E;&#x3048;&#x308C;&#x3093;&#x305F;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">wacht</trd>
  <trd lng="pt">guarda<klr tip="amb"> (tropa)</klr></trd>
  <trd lng="ru">&c_g;&c_v;&c_a;&c_r;&c_d;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>garda</trd>,
    <trd>gardov&yacute; pluk</trd>,
    <trd>str&aacute;&zcaron;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>garda</trd>,
    <trd>gardov&yacute; pluk</trd>,
    <trd>guardia</trd>,
    <trd>guardie</trd>,
    <trd>str&aacute;&zcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_gu;&c_v;&c_a;&c_r;&c_d;&c_ib;&c_ja;</trd>
</drv>

<drv mrk="gvardi.antaux0o">
  <kap>anta&ubreve;<tld/>o</kap>
  <snc mrk="gvardi.antaux0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="dif" cel="avanga.0o.MIL">Avangardo<sncref/></ref>:
      <ekz>
        &ccirc;u vi ne trafis sur la vojo tra la arbaro anta&ubreve;<tld/>on?
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 17a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_v;&c_a;&c_n;&c_g;&c_a;&c_r;&c_d;</trd>
  <trd lng="ca">avantguarda</trd>
  <trdgrp lng="de">
    <trd>Vortrupp</trd>,
    <trd>Vorhut</trd>,
    <trd>Vorausabteilung</trd>
  </trdgrp>
  <trd lng="en">vanguard</trd>
  <trd lng="es">vanguardia</trd>
  <trd lng="de">Garde</trd>
  <trd lng="fr">avant-garde</trd>
  <trd lng="hu">el&odblac;&ouml;rs</trd>
  <trd lng="it">avanguardia</trd>
  <trd lng="nl">voorhoede</trd>
  <trd lng="pt">vanguarda<klr tip="amb"> (tropa)</klr></trd>
  <trd lng="ru">&c_a;&c_v;&c_a;&c_n;&c_g;&c_a;&c_r;&c_d;</trd>
  <trdgrp lng="zh">
    <trd>&#x5148;&#x92D2;&#x968A; <pr>xi&#x101;nf&#x113;ngdu&#xEC;</pr></trd>,
    <trd>&#x5148;&#x9A45; <pr>xi&#x101;nq&#x16B;</pr></trd>,
    <trd>&#x5148;&#x92D2; <pr>xi&#x101;nf&#x113;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="gvardi.post0o">
  <kap>post<tld/>o</kap>
  <snc mrk="gvardi.post0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="dif" cel="arierg.0o">Ariergardo</ref>:
      <ekz>
        la post<tld/>on formis kvar sklavoj, peze armitaj, rajdantaj sur
        dromedaroj
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 2a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;'&c_je;&c_r;&c_g;&c_a;&c_r;&c_d;</trd>
  <trd lng="ca">rereguarda</trd>
  <trdgrp lng="de">
    <trd>Nachtrupp</trd>,
    <trd>Nachhut</trd>
  </trdgrp>
  <trd lng="en">rearguard</trd>
  <trd lng="es">retaguardia</trd>
  <trd lng="fr">arri&egrave;re-garde</trd>
  <trd lng="hu">h&aacute;tv&eacute;d</trd>
  <trd lng="it">retroguardia</trd>
  <trdgrp lng="ja">
    <trd>&#x5F8C;&#x885B;  <pr>&#x3053;&#x3046;&#x3048;&#x3044;</pr></trd>,
    <trd>&#x5F8C;&#x885B;&#x90E8;&#x968A;  <pr>&#x3053;&#x3046;&#x3048;&#x3044;&#x3076;&#x305F;&#x3044;</pr></trd>,
    <trd>&#x6BBF;&#x8ECD;  <pr>&#x3067;&#x3093;&#x3050;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">achterhoede</trd>
  <trd lng="pt">retaguarda<klr tip="amb"> (tropa)</klr></trd>
  <trd lng="ru">&c_a;&c_r;&c_mol;&c_je;&c_r;&c_g;&c_a;&c_r;&c_d;</trd>
  <trd lng="zh">&#x540E;&#x536B; <pr>h&#xF2;uw&#xE8;i</pr></trd>
</drv>
</art>
