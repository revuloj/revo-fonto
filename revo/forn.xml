<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
 <ofc>*</ofc>
 <rad>forn</rad>/o
</kap>
<drv mrk="forn.0o">
 <kap><ofc>*</ofc><tld/>o</kap>
 <uzo tip="fak">KUI</uzo>
 <uzo tip="fak">TEK</uzo>
 <snc num="1">
  <dif>
   Masonita fajrujo por baki panon, fandi metalojn, kuiri potojn,
   kalko&scirc;tonojn, brikojn, a&ubreve; por aliaj industriaj celoj. 
  </dif>
 </snc>
 <snc num="2">
  <dif>
   Fajrujo por kuiri man&gcirc;a&jcirc;ojn. 
  </dif>
 </snc>
 <snc num="3">
  <dif>
   Fajrujo por hejti &ccirc;ambron, lo&gcirc;ejon:
   <ekz>
     vivi inter <tld/>o hejtita k tablo kovrita
     <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>
     <klr>(lar&gcirc;e k komforte)</klr>.
   </ekz> 
  </dif>
 </snc>
 <trdgrp lng="cs">
  <trd>kamna</trd>,
  <trd>pec</trd>
 </trdgrp>
 <trd lng="de">Ofen</trd>
 <trd lng="nl">oven</trd>
 <trdgrp lng="fr">
  <trd>four</trd>,
  <trd>fourneau</trd>
 </trdgrp>
 <trd lng="ru">&c_p;&c_je;&c_ch;&c_mol;</trd>
 <trd lng="tr">ocak</trd>
</drv>
<drv mrk="forn.0egoalt0ofand0o">
 <kap><tld/>ego, alt<tld/>o, fand<tld/>o</kap>
 <uzo tip="fak">TEK</uzo>
 <snc>
  <dif>
   <tld lit="F"/>o por fandi metalmina&jcirc;ojn.  
  </dif>
 </snc>
 <trd lng="de">Hochofen</trd>
 <trd lng="fr">haut-fourneau</trd>
 <trd lng="nl">hoogoven</trd>
 <trdgrp lng="ru">
   <trd>&c_d;&c_o;&c_m;&c_n;&c_a;</trd>,
   <trd>&c_d;&c_o;&c_m;&c_je;&c_n;&c_n;&c_a;&c_ja; &c_p;&c_je;&c_ch;&c_mol;</trd>
 </trdgrp>
</drv>
<drv mrk="forn.0osida">
 <kap><tld/>osida<fnt>Z</fnt></kap>
 <uzo tip="stl">FIG</uzo> 
 <snc num="1">
  <dif>
   Frostotima. 
  </dif>
 </snc>
 <snc num="2">
  <dif>
   Hejmama.  
  </dif>
 </snc>
  <trdgrp lng="fr">
    <trd>casanier</trd>,
    <trd>pantouflard</trd>,
    <trd>frileux<klr tip="ind"> (adj.)</klr></trd></trdgrp> 
</drv>
</art>
