<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: forn.xml,v 1.52 2023/11/10 06:07:59 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>forn</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="forn.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc mrk="forn.0o.TEK">
    <dif>
      Masonita fajrujo por baki panon, fandi metalojn, kuiri potojn,
      kalko&scirc;tonojn, brikojn, a&ubreve; por aliaj celoj:
      <ekz>
        en la vila&gcirc;o estas pli bone: <klr>[...]</klr> prenu al vi virinon
        kaj ku&scirc;u la tutan vivon apud la <tld/>o kaj man&gcirc;u kukojn
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili ne havas, kion man&gcirc;i, ili ne havas, per kio hejti la <tld/>on
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        tuj post la hejto la <tld/>o estis varmega, post unu horo
        &gcirc;i estis jam nur varma, <klr>[...]</klr> kaj post tri horoj &gcirc;i estis jam tute malvarma
        <fnt>
          <bib>F</bib>
          <lok>&FE; 38</lok>
        </fnt>;
      </ekz>
      <ekz>
        vivi inter <tld/>o hejtita kaj tablo kovrita
        <klr>(vivi tre komforte)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <subsnc mrk="forn.0o.KUI">
      <uzo tip="fak">KUI</uzo>
      <dif>
        <ref tip="dif" cel="forn.kuir0o.KUI">Kuir<tld/>o</ref>:
        <ekz>
          &scirc;i iris <klr>[...]</klr> al <tld/>o, kiu estis plena de pano
          <fnt>
            <bib>ElektFab</bib>
            <lok>La maljunulino Holle</lok>
          </fnt>;
        </ekz>
        <ekz>
          la mastro havas du rostitajn anserojn en la <tld/>o
          <fnt>
            <bib>ElektFab</bib>
            <lok>Kamarado Gaja</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="forn.0o.hejtilo">
      <dif>
        <ref tip="dif" cel="forn.hejto0o.HEJM">Hejto<tld/>o</ref>:
        <ekz>
          en la vintro oni hejtas la <tld/>ojn
          <fnt>
            <bib>F</bib>
            <lok>&FE; 16</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_ch;</trd>
  <trdgrp lng="cs">
    <trd>kamna</trd>,
    <trd>pec</trd>
  </trdgrp>
  <trd lng="de">Ofen</trd>
  <trdgrp lng="en">
  <trd>furnace</trd>
  <trd>kiln</trd>
  <trd>oven</trd>
  <trd>stove</trd>
  </trdgrp>
  <trd lng="es">horno</trd>
  <trd lng="fr">four</trd>
  <trd lng="nl">oven</trd>
  <trdgrp lng="pl">
    <trd>kocio&lstroke;</trd>,
    <trd>kuchnia</trd>,
    <trd>piec  <klr>(do ogrzewania)</klr></trd>
  </trdgrp>
  <trd lng="pt">forno</trd>
  <trdgrp lng="ro">
    <trd>c&abreve;ldare</trd>,
    <trd>sob&abreve;</trd>,
    <trd>cuptor  <klr>(pentru &icirc;nc&abreve;lzire)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_ch;&c_mol;<klr tip="amb"> (&c_s;&c_u;&c_shch;.)</klr></trd>
  <trdgrp lng="sk">
    <trd>kachle</trd>,
    <trd>pec</trd>,
    <trd>&scaron;por&aacute;k</trd>
  </trdgrp>
  <trd lng="tr">ocak</trd>
  <trd lng="zh">&#x70E4;&#x7089; <pr>k&#x1CE;ol&#xFA;</pr></trd>
</drv>

<drv mrk="forn.F0o">
  <kap><tld lit="F"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
  </kap>
  <mlg>For</mlg>
  <snc mrk="forn.F0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Sud-hemisfera <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref> (<trd lng="la">Fornax</trd>):
      <ekz>
        NGC 689 <klr>[...]</klr> estas galaksio en konstelacio <tld lit="F"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=110273">NGC 689</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_P;&c_je;&c_ch; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#x2019;&c_je;)</klr></trd>
  <trd lng="bo">&#xF50;&#xF56;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>kamna</trd>,
    <trd>pec</trd>
  </trdgrp>
  <trd lng="de">Ofen</trd>
  <trd lng="en">Fornax</trd>
  <trd lng="es">Horno</trd>
  <trd lng="fr">Fourneau<klr tip="ind"> (constellation)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x7089;  <pr>&#x308D;</pr></trd>,
    <trd>&#x304B;&#x307E;&#x3069;</trd>,
    <trd>&#x6696;&#x7089;  <pr>&#x3060;&#x3093;&#x308D;</pr></trd>,
    <trd>&#x30B9;&#x30C8;&#x30FC;&#x30D6;</trd>,
    <trd>&#x3053;&#x3093;&#x308D;</trd>
  </trdgrp>
  <trd lng="nl">Oven<klr> (sterrenbeeld)</klr></trd>
  <trd lng="no">Smelteovnen<klr> (stjernebilde)</klr></trd>
  <trd lng="pl">Piec  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Cuptorul  <klr>(constela&#x21B;ie)</klr></trd>
  <trdgrp lng="sk">
    <trd>kachle</trd>,
    <trd>pec</trd>,
    <trd>&scaron;por&aacute;k</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&c_ib;&c_ch;</trd>
</drv>

<drv mrk="forn.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="forn.0ego.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Industria <ref tip="super" cel="forn.0o.TEK"><tld/>o</ref> por
      fandi metalmina&jcirc;ojn:
      <ekz>
        el ar&gcirc;ento en <tld/>egoj fandita kaj verge fandita ili
        stampis briletajn &ccirc;e&hcirc;ajn gro&scirc;ojn
        <fnt>
          <aut>Alois Jir&aacute;sek, trad. Josef Vondrou&scaron;ek</aut>:
          <vrk>Malnovaj Mitoj &Ccirc;e&hcirc;aj</vrk>
          <lok>Ministoj de Kutn&aacute; Hora</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_o;&c_m;&c_n;&c_a;</trd>,
    <trd>&c_d;&c_o;&c_m;&c_je;&c_n;&c_n;&c_a;&c_ja; &c_p;&c_je;&c_ch;</trd>
  </trdgrp>
  <trd lng="cs">velk&aacute; pec || vysok&aacute; pec</trd>
  <trd lng="de">Hochofen</trd>
  <trd lng="fr">haut-fourneau</trd>
  <trdgrp lng="ja">
    <trd>&#x5927;&#x304B;&#x307E;&#x3069;  <pr>&#x3060;&#x3044;&#x304B;&#x307E;&#x3069;</pr></trd>,
    <trd>&#x9AD8;&#x7089;  <pr>&#x3053;&#x3046;&#x308D;</pr></trd>
  </trdgrp>
  <trd lng="nl">hoogoven</trd>
  <trd lng="pl">piec hutniczy</trd>
  <trd lng="ro">cuptor de turn&abreve;torie</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_m;&c_n;&c_a;</trd>,
    <trd>&c_d;&c_o;&c_m;&c_je;&c_n;&c_n;&c_a;&c_ja;
    &c_p;&c_je;&c_ch;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">ve&lcaron;k&aacute; pec</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&#x43E;&#x43C;&c_je;&#x43D;&#x43D;&c_a; &#x43F;&c_ib;&c_ch;</trd>,
    <trd>&c_d;&#x43E;&#x43C;&#x43D;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#x9AD8;&#x7089; <pr>g&#x101;ol&#xFA;</pr></trd>
</drv>

<drv mrk="forn.alt0o">
  <kap>alt<tld/>o</kap>
  <uzo tip="stl">EVI</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="forn.0ego"><tld lit="F"/>ego</ref>:
      <ekz>
        mi veturigas fer&scirc;tonon kaj karbon al la alt<tld/>o.
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>1a parto, &ccirc;apitro 1a</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">vysok&aacute; pec</trd>
  <trd lng="ja">&#x9AD8;&#x7089;  <pr>&#x3053;&#x3046;&#x308D;</pr></trd>
  <trd lng="pl">wielki  <ind>piec</ind></trd>
  <trd lng="ro">furnal</trd>
  <trd lng="sk">vysok&aacute; pec</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&#x43E;&#x43C;&c_je;&#x43D;&#x43D;&c_a; &#x43F;&c_ib;&c_ch;</trd>,
    <trd>&c_d;&#x43E;&#x43C;&#x43D;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#x9AD8;&#x7089; <pr>g&#x101;ol&#xFA;</pr></trd>
</drv>

<drv mrk="forn.bak0o">
  <kap>bak<tld/>o</kap>
  <snc mrk="forn.bak0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="forn.0o.TEK"><tld lit="F"/>o</ref> en kiu
      oni povas <ref tip="vid" cel="bak.0i">baki</ref>:
      <ekz>
        bluaj porcelana&jcirc;oj anta&ubreve; nelonge malkovritaj en la ruino de
        bak<tld/>oj de Hongzhou, Fengcheng, Jiangxi-provinco, donis
        atestojn, ke la loko estis unu el la luliloj de porcelana
        produktado de &Ccirc;inio
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.china.org.cn/esperanto1/world/shi-kaogu/tang.htm">
          Nova Elkovro de Porcelanaj Bakujoj...</url></vrk>,
          <lok>[1999?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_ch; <klr>(&c_p;&c_ja;&c_k;&c_a;&c_r;&c_n;&c_a;&c_ja;)</klr></trd>
  <trdgrp lng="cs">
    <trd>peka&rcaron;sk&aacute; pec</trd>,
    <trd>pe&ccaron;&iacute;c&iacute; trouba</trd>
  </trdgrp>
  <trd lng="fr">four de cuisson</trd>
  <trd lng="he">&#x5EA;&#x5E0;&#x5D5;&#x5E8; &#x5D0;&#x5E4;&#x5D9;&#x5D4;</trd>
  <trdgrp lng="ja">
    <trd>&#x30D1;&#x30F3;&#x713C;&#x304D;&#x304C;&#x307E;
    <pr>&#x3071;&#x3093;&#x3084;&#x304D;&#x304C;&#x307E;</pr></trd>,
    <trd>&#x30AA;&#x30FC;&#x30D6;&#x30F3;  <pr>&#x304A;&#x30FC;&#x3076;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>piekarnik</trd>,
    <trd>piec chlebowy</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>forno</trd>,
    <trd>fog&atilde;o</trd>,
    <trd>estufa</trd>
  </trdgrp>
  <trd lng="ro">cuptor de p&acirc;ine</trd>
  <trdgrp lng="sk">
    <trd>pec na pe&ccaron;enie</trd>,
    <trd>pec na vypa&lcaron;ovanie hrncov</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&c_ib;&c_ch; &c_d;&#x43B;&#x44F; &c_v;&c_i;&#x43F;&c_ib;&#x43A;&c_a;&#x43D;&#x43D;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x70D8;&#x7BB1; <pr>h&#x14D;ngxi&#x101;ng</pr></trd>,
    <trd>&#x70E4;&#x7BB1; <pr>k&#x1CE;oxi&#x101;ng</pr></trd>,
    <trd>&#x7089;&#x5B50; <pr>l&#xFA;zi</pr></trd>,
    <trd>&#x706B;&#x7089; <pr>hu&#x1D2;l&#xFA;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="forn.fand0o">
