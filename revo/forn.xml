  <trdgrp lng="sk">
    <trd>taviaca</trd>,
    <trd>vysok&aacute; pec</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x43B;&c_a;&c_v;&c_i;&#x43B;&#x44C;&#x43D;&c_a; &#x43F;&c_ib;&c_ch;</trd>
  <trd lng="zh">&#x9AD8;&#x7089; <pr>g&#x101;ol&#xFA;</pr></trd>
