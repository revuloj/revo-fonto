<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>forn</rad>/o
</kap>
<drv mrk="forn.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc mrk="forn.0o.TEK">
    <dif>
      Masonita fajrujo por baki panon, fandi metalojn, kuiri potojn,
      kalko&scirc;tonojn, brikojn, a&ubreve; por aliaj industriaj celoj.
    </dif>
  </snc>
  <snc mrk="forn.0o.KUI">
  <uzo tip="fak">KUI</uzo>
    <dif>
      Fajrujo por kuiri man&gcirc;a&jcirc;ojn.
    </dif>
  </snc>
  <snc mrk="forn.0o.hejtilo">
    <dif>
      Fajrujo uzata kiel <ref tip="super" cel="hejt.0ilo">hejtilo</ref> en &ccirc;ambro, lo&gcirc;ejo:
      <ekz>
        vivi inter <tld/>o hejtita kaj tablo kovrita
        <fnt><bib>PrV</bib></fnt>
        <klr>(vivi tre komforte)</klr>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>kamna</trd>,
    <trd>pec</trd>
  </trdgrp>
  <trd lng="de">Ofen</trd>
  <trd lng="nl">oven</trd>
  <trdgrp lng="fr">
    <trd>four</trd>,
    <trd>fourneau</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_ch;&c_mol;</trd>
  <trd lng="tr">ocak</trd>
</drv>

<drv mrk="forn.0ego">
  <kap><tld/>ego</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      <tld lit="F"/>o por fandi metalmina&jcirc;ojn.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="forn.alt0o">alt<tld/>o</ref>,
    <ref cel="forn.fand0o">fand<tld/>o</ref>
  </refgrp>
  <trd lng="de">Hochofen</trd>
  <trd lng="fr">haut-fourneau</trd>
  <trd lng="nl">hoogoven</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_m;&c_n;&c_a;</trd>,
    <trd>&c_d;&c_o;&c_m;&c_je;&c_n;&c_n;&c_a;&c_ja; &c_p;&c_je;&c_ch;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="forn.alt0o">
  <kap>alt<tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <ref tip="dif" cel="forn.0ego"><tld lit="F"/>ego</ref>
  </snc>
</drv>

<drv mrk="forn.0osida">
  <kap><tld/>osida<fnt>Z</fnt></kap>
  <uzo tip="stl">FIG</uzo>
  <snc mrk="forn.0osida.frostotima">
    <dif>
      Frostotima.
    </dif>
    <trdgrp lng="fr">
      <trd>frileux<klr> (adj.)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="forn.0osida.hejmama">
    <ref tip="dif" cel="hejm.0ama">Hejmama</ref>
    <trdgrp lng="fr">
      <trd>casanier</trd>,
      <trd>pantouflard</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="forn.fand0o">
  <kap>fand<tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <ref tip="dif" cel="forn.0ego"><tld lit="F"/>ego</ref>
  </snc>
</drv>
</art>
