<drv mrk="forn.hejto0o">
  <kap>hejto<tld/>o, <var><kap>hejt<tld/>o</kap></var></kap>
  <snc mrk="forn.hejto0o.HEJM">
    <dif>
      <ref tip="super" cel="forn.0o.TEK"><tld lit="F"/>o</ref> uzata
      kiel <ref tip="super" cel="hejt.0ilo">hejtilo</ref> en &ccirc;ambro,
      lo&gcirc;ejo...:
      <ekz>
        dum ser&ccirc;adoj oni elfosis pecojn de ceramika&jcirc;o por hejt<tld/>o
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.kagenfels.org/histokag_eo.htm">
          La kastelo Kagenfels</url></vrk>,
          <lok>[vidita en 2008-01]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_ch; <klr>(&c_a;&c_c;&c_ja;&c_p;&c_l;&c_ja;&c_l;&c_mol;&c_n;&c_a;&c_ja;)</klr></trd>
  <trdgrp lng="cs">
    <trd>kamna na vyt&aacute;p&ecaron;n&iacute;</trd>,
    <trd>pokojov&aacute; kamna</trd>
  </trdgrp>
  <trd lng="fr">po&ecirc;le<klr> (appareil de chauffage)</klr></trd>
  <trdgrp lng="hu">
    <trd>k&aacute;lyha</trd>,
    <trd>t&udblac;zhely</trd>
  </trdgrp>
  <trd lng="ja">&#x30B9;&#x30C8;&#x30FC;&#x30D6;</trd>
  <trd lng="pl">piec grzewczy</trd>
  <trd lng="pt">aquecedor <klr tip="amb">(aparelho)</klr></trd>
  <trd lng="ro">soba de  &icirc;nc&abreve;lzire</trd>
  <trd lng="ru">&c_p;&c_je;&c_ch;&c_mol;<klr tip="amb"> (&c_s;&c_u;&c_shch;.)</klr></trd>
  <trd lng="sk">izbov&eacute; kachle</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_r;&c_u;&c_b;&#x43A;&c_a;</trd>,
    <trd>&#x43E;&#x43F;&c_a;&#x43B;&#x44E;&c_v;&c_a;&#x43B;&#x44C;&#x43D;&c_a; &#x43F;&c_ib;&c_ch;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7089;&#x706B;&#x53D6;&#x6696; <pr>l&#xFA;hu&#x1D2;q&#x1D4;nu&#x1CE;n</pr></trd>,
    <trd>&#x70E7;&#x7164;&#x53D6;&#x6696; <pr>sh&#x101;om&#xE9;iq&#x1D4;nu&#x1CE;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="forn.kuir0o">
  <kap>kuir<tld/>o</kap>
  <snc mrk="forn.kuir0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="dif" cel="forn.bak0o">Bak<tld/>o</ref> por kuiro de
      man&gcirc;a&jcirc;oj:
      <ekz>
        en Japanio hejma kuir<tld/>o estas malgranda kompare kun tiu en
        e&ubreve;ropa hejmo
        <fnt>
          <aut>Tahira M.</aut>,
          <vrk>Kataj familioj apud mi</vrk>,
          <lok><url ref="http://groups.yahoo.com/group/verdakato/message/440">
          grupo verdakato, 2007-11-09</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_ch; <klr>(&c_k;&c_u;&c_h;&c_o;&c_n;&c_n;&c_a;&c_ja;)</klr></trd>
  <trd lng="cs">spor&aacute;k</trd>
  <trd lng="fr">four de cuisine</trd>
  <trdgrp lng="ja">
    <trd>&#x6599;&#x7406;&#x7528;&#x304B;&#x307E;&#x3069;  <pr>&#x308A;&#x3087;&#x3046;&#x308A;&#x3088;&#x3046;&#x304B;&#x307E;&#x3069;</pr></trd>,
    <trd>&#x30EC;&#x30F3;&#x30B8;</trd>,
    <trd>&#x3053;&#x3093;&#x308D;</trd>
  </trdgrp>
  <trd lng="pl">piec do gotowania</trd>
  <trdgrp lng="pt">
    <trd>forno</trd>,
    <trd>fog&atilde;o</trd>,
    <trd>estufa</trd>
  </trdgrp>
  <trd lng="ro">cuptor pentru g&abreve;tit</trd>
  <trd lng="sk">spor&aacute;k</trd>
  <trd lng="uk">&#x43A;&c_u;&c_h;&#x43E;&#x43D;&#x43D;&c_a; &#x43F;&c_ib;&c_ch;</trd>
  <trdgrp lng="zh">
    <trd>&#x7076; <pr>z&#xE0;o</pr></trd>,
    <trd>&#x5786; <pr>l&#xFA;</pr></trd>
  </trdgrp>
</drv>
</art>
