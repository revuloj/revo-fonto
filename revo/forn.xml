  <snc mrk="forn.fand0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="dif" cel="forn.0ego"><tld lit="F"/>ego</ref>:
      <ekz>
        fandado okazas en speciala fand<tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=274923">Orbatisto</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        rekonstruo de fergisado en fumanta fand<tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=311887">W&lstroke;odzimierz Sedlak</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_o;&c_m;&c_n;&c_a;</trd>,
    <trd>&c_d;&c_o;&c_m;&c_je;&c_n;&c_n;&c_a;&c_ja; &c_p;&c_je;&c_ch;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>tav&iacute;c&iacute; pec</trd>,
    <trd>v&yacute;he&ncaron;</trd>
  </trdgrp>
  <trd lng="de">Hochofen</trd>
  <trd lng="fr">haut-fourneau</trd>
  <trd lng="ja">&#x6EB6;&#x9271;&#x7089;  <pr>&#x3088;&#x3046;&#x3053;&#x3046;&#x308D;</pr></trd>
  <trd lng="nl">hoogoven</trd>
  <trd lng="pl">piec odlewniczy</trd>
  <trd lng="ro">cuptor de turn&abreve;torie</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_m;&c_n;&c_a;</trd>,
    <trd>&c_d;&c_o;&c_m;&c_je;&c_n;&c_n;&c_a;&c_ja;
    &c_p;&c_je;&c_ch;&c_mol;</trd>
