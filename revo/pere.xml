    <trd>&#27531; <pr>c&aacute;n</pr></trd>,
    <trd>&#27544; <pr>c&aacute;n</pr></trd>,
    <trd>&#27584;&#28357; <pr>hu&#464;mi&egrave;</pr></trd>,
    <trd>&#27585;&#28781; <pr>hu&#464;mi&egrave;</pr></trd>,
    <trd>&#20606; <pr>f&egrave;n</pr></trd>,
    <trd>&#20712; <pr>f&egrave;n</pr></trd>,
    <trd>&#25703;&#27584; <pr>cu&#299;hu&#464;</pr></trd>,
    <trd>&#25703;&#27585; <pr>cu&#299;hu&#464;</pr></trd>,
    <trd>&#27524; <pr>ti&#462;n</pr></trd>,
