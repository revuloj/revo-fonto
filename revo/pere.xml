<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pere</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pere.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Neatendite morti per akcidento a&ubreve; perforto:
      <ekz>
        <tld/>i en batalo;
      </ekz>
      <ekz>
        &ccirc;e manko de konsili&gcirc;o popoloj <tld/>as<fnt>Z</fnt>;
      </ekz>
      <ekz>
        hipokrita bu&scirc;o ka&ubreve;zas <tld/>on de justuloj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">umkommen</trd>
    <trdgrp lng="hu">
      <trd>elv&eacute;sz <klr tip="amb">(meghal)</klr></trd>,
      <trd>odav&eacute;sz</trd>,
      <trd>elesik</trd>,
      <trd>ottmarad</trd>
    </trdgrp>
    <trd lng="nl">omkomen</trd>
    <trdgrp lng="pt">
      <trd>perecer</trd>,
      <trd>destruir-se</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Neatendite detrui&gcirc;i, nenii&gcirc;i:
      <ekz>
        diron oni neas, skribo ne <tld/>as
        <fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        feli&ccirc;o le&gcirc;on ne obeas, subite naski&gcirc;as, subite
        <tld/>as
        <fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        &ccirc;iuj liaj vortoj kaj laboroj <tld/>is vane<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la konfido <tld/>is<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la espero de malpiuloj <tld/>os;
      </ekz>
      <ekz>
        neniam lia nomo <tld/>os<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la fruktoj de lia laboro neniam <tld/>os<fnt>Z</fnt>;
      </ekz>
      <ekz>
        senfrukte <tld/>inta provo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        lia gloro estas ne<tld/>onta.
      </ekz>
    </dif>
    <trd lng="de">verfallen</trd>
    <trd lng="hu">elpusztul</trd>
    <trd lng="nl">vergaan</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_ib;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_ib;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_n;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_n;&c_ib;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hynout</trd>,
    <trd>uhynout</trd>,
    <trd>vzít za své</trd>,
    <trd>zahynout</trd>,
    <trd>zaniknout</trd>,
    <trd>zemřít</trd>
  </trdgrp>
  <trd lng="en">perish</trd>
  <trd lng="es">perecer</trd>
  <trdgrp lng="fr">
    <trd>se <ind>perdre</ind><klr> (p&eacute;rir)</klr></trd>,
    <trd>p&eacute;rir</trd>,
    <trd>succomber</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_i;&c_b;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_g;&c_i;&c_b;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_g;&c_i;&c_b;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vyhynúť</trd>,
    <trd>zahynúť</trd>,
    <trd>zhynúť</trd>
  </trdgrp>
</drv>

<drv mrk="pere.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Mortigi:
      <ekz>
        la birdo rapidas al la kaptilo kaj <tld/>igas sian vivon.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Detrui:
      <ekz>
        sekeco <tld/>igas la grenon;
      </ekz>
      <ekz>
        tiuj nesingardaj vortoj povos havi terure <tld/>igan
        efikon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        via agado povas doni <tld/>igan rezultaton por nia komuna
        ideo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        eksplode <tld/>igi &scirc;ipon<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_b;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_n;&c_ib;&c_sh;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_b;&c_u;&c_r;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_g;&c_u;&c_b;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hubit</trd>,
    <trd>ničit</trd>,
    <trd>zachvátit</trd>,
    <trd>zahubit</trd>,
    <trd>zničit</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;truire</trd>,
    <trd>d&eacute;vaster</trd>,
    <trd>faire <ind>p&eacute;rir</ind></trd>,
    <trd>ruiner</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>elpuszt&iacute;t</trd>,
    <trd>elveszejt</trd>,
    <trd>kiirt</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_g;&c_u;&c_b;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_g;&c_u;&c_b;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>skynožiť</trd>,
    <trd>zahubiť</trd>,
    <trd>zničiť</trd>
  </trdgrp>
</drv>

<drv mrk="pere.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="pere.for0i.KOMUNE">
    <dif>
      Malaperi <tld/>inte:
      <ekz>
        vi for<tld/>os de sur tiu tero, kiun donis al vi la
        Eternulo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_n;&c_ib;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>
  <trd lng="cs">zahynout</trd>
  <trd lng="fr">s'<ind>&eacute;teindre</ind><klr> (disparaitre)</klr></trd>
  <trd lng="ru">&c_s;&c_g;&c_i;&c_n;&c_u;&c_t;&c_mol;</trd>
  <trd lng="sk">zahynúť</trd>
</drv>

<drv mrk="pere.sxip0o">
  <kap>&scirc;ip<tld/>o</kap>
  <snc mrk="pere.sxip0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      <tld lit="P"/>o de &scirc;ipo pro subakvi&gcirc;o, frakasi&gcirc;o...:
      <ekz>
        estos multaj la &scirc;ip<tld/>oj pro tia vetero
        <fnt><aut>H. Vallienne</aut>,<vrk>Kastelo de 
        Prelongo</vrk>,<lok>1907</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">naufrage</trd>
</drv>
</art>
