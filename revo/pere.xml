    <trd>&#30772;&#28357;&#12373;&#12379;&#12427;  <pr>&#12399;&#12417;&#12388;&#12373;&#12379;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>g&lstroke;adzi&cacute;</trd>,
    <trd>niweczy&cacute;</trd>,
    <trd>zaprzepaszcza&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>distruge</trd>,
    <trd>strica</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_g;&c_u;&c_b;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_g;&c_u;&c_b;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>skyno&zcaron;i&tcaron;</trd>,
    <trd>zahubi&tcaron;</trd>,
    <trd>zni&ccaron;i&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_g;&c_u;&c_b;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_n;&c_i;&c_shch;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_v;&c_je;&c_s;&c_t;&c_i; &c_z;&c_ib; &c_s;&c_v;&c_ib;&c_t;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#25439;&#27585; <pr>s&#468;nhu&#464;</pr></trd>,
    <trd>&#25613;&#27584; <pr>s&#468;nhu&#464;</pr></trd>,
    <trd>&#25703;&#27531; <pr>cu&#299;c&aacute;n</pr></trd>,
