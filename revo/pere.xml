    <trd>zaniknout</trd>,
    <trd>zem&rcaron;&iacute;t</trd>
  </trdgrp>
  <trd lng="en">perish</trd>
  <trd lng="es">perecer</trd>
  <trdgrp lng="fr">
    <trd>se <ind>perdre</ind><klr> (p&eacute;rir)</klr></trd>,
    <trd>p&eacute;rir</trd>,
    <trd>succomber</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#27515;&#12396;  <pr>&#12375;&#12396;</pr></trd>,
    <trd>&#28357;&#12403;&#12427;  <pr>&#12411;&#12429;&#12403;&#12427;</pr></trd>,
    <trd>&#28040;&#12360;&#12427;  <pr>&#12365;&#12360;&#12427;</pr></trd>,
    <trd>&#28357;&#20129;&#12377;&#12427;  <pr>&#12417;&#12388;&#12412;&#12358;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>by&cacute; zgubionym</trd>,
    <trd>przepada&cacute;</trd>,
    <trd>polec</trd>,
    <trd>zagin&aogonek;&cacute;</trd>
  </trdgrp>
  <trd lng="ro">muri</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_i;&c_b;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_g;&c_i;&c_b;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_g;&c_i;&c_b;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vyhyn&uacute;&tcaron;</trd>,
    <trd>zahyn&uacute;&tcaron;</trd>,
    <trd>zhyn&uacute;&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_i;&c_n;&c_u;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_n;&c_i;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_p;&c_a;&c_d;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
