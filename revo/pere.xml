    <trd>&#28138;&#27794;&#21930;&#20129; <pr>l&uacute;nm&ograve;s&agrave;ngw&aacute;ng</pr></trd>,
    <trd>&#36935;&#38590; <pr>y&ugrave;n&agrave;n</pr></trd>,
