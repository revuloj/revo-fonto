<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pere.xml,v 1.32 2023/10/20 14:25:20 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pere</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pere.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="pere.0i.morti">
    <dif>
      Neatendite <ref tip="super" cel="mort.0i.ekmalvivi">morti</ref> per akcidento a&ubreve; perforto:
      <ekz>
        <tld/>i en batalo;
      </ekz>
      <ekz>
        &ccirc;e manko de prikonsili&gcirc;o popolo <tld/>as
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 11:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        batalo kontra&ubreve; du virinoj kunligitaj por lia <tld/>o
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekkvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        hipokrita bu&scirc;o ka&ubreve;zas <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 26:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        de atendo kaj espero <tld/>is multaj sur la tero
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>umkommen</trd>,
      <trd>ums Leben kommen</trd>,
      <trd>hinscheiden</trd>,
      <trd>verscheiden</trd>,
      <trd>versterben</trd>,
      <trd>fallen<klr> (im Krieg)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>elv&eacute;sz <klr tip="amb">(meghal)</klr></trd>,
      <trd>odav&eacute;sz</trd>,
      <trd>elesik</trd>,
      <trd>ottmarad</trd>
    </trdgrp>
    <trd lng="nl">omkomen</trd>
    <trdgrp lng="pt">
      <trd>perecer</trd>,
      <trd>destruir-se</trd>
    </trdgrp>
  </snc>
  <snc mrk="pere.0i.detruigxi">
    <dif>
      Neatendite <ref tip="super" cel="detru.0igxi">detrui&gcirc;i</ref>, nenii&gcirc;i:
      <ekz>
        minacas al ni la dan&gcirc;ero, ke &ccirc;iuj niaj laboroj <tld/>os vane
        <fnt>
          <bib>EE</bib>
          <lok>I</lok>
        </fnt>;
      </ekz>
      <ekz>
        konfido fine akirita post centoj kaj miloj da faroj <klr>[&#x2026;]</klr>, <tld/>us por
        &ccirc;iam
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado en la Guildhall (Londono) en la 21a de a&ubreve;gusto 1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la espero de malpiuloj <tld/>os
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 10:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fruktoj de lia laboro neniam <tld/>os
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sepa Kongreso Esperantista en Antwerpen en la 21a de a&ubreve;gusto 1911</vrk>
        </fnt>;
      </ekz>
      <ekz>
        senfrukte <tld/>intaj provoj
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili faras tion, por ricevi <tld/>ontan kronon, sed ni, ne<tld/>ontan
        <fnt>
          <bib>NT</bib>
          <lok>I. Korintanoj 9:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>diron oni neas, skribo ne <tld/>as</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="de">wer schreibt der <ind>bleibt</ind></trd>
      </ekz>
      <ekz>
        feli&ccirc;o le&gcirc;on ne obeas, subite naski&gcirc;as, subite
        <tld/>as
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>vergehen</trd>,
      <trd>verfallen</trd>,
      <trd>verschwinden</trd>
    </trdgrp>
    <trd lng="hu">elpusztul</trd>
    <trd lng="nl">vergaan</trd>
  </snc>
  <refgrp tip="vid">
    <ref cel="dron.0i">droni</ref>,
    <ref cel="abism.en0igxi">enabismi&gcirc;i</ref>,
    <ref cel="aper.mal0i.neniigxi">malaperi<sncref/></ref>,
    <ref cel="perd.0igxi">perdi&gcirc;i</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_g;&c_ib;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_ib;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_n;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_n;&c_ib;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hynout</trd>,
    <trd>uhynout</trd>,
    <trd>vz&iacute;t za sv&eacute;</trd>,
    <trd>zahynout</trd>,
    <trd>zaniknout</trd>,
    <trd>zem&rcaron;&iacute;t</trd>
  </trdgrp>
  <trd lng="en">perish</trd>
  <trd lng="es">perecer</trd>
  <trdgrp lng="fr">
    <trd>se <ind>perdre</ind><klr> (p&eacute;rir)</klr></trd>,
    <trd>p&eacute;rir</trd>,
    <trd>succomber</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6B7B;&#x306C;  <pr>&#x3057;&#x306C;</pr></trd>,
    <trd>&#x6EC5;&#x3073;&#x308B;  <pr>&#x307B;&#x308D;&#x3073;&#x308B;</pr></trd>,
    <trd>&#x6D88;&#x3048;&#x308B;  <pr>&#x304D;&#x3048;&#x308B;</pr></trd>,
    <trd>&#x6EC5;&#x4EA1;&#x3059;&#x308B;  <pr>&#x3081;&#x3064;&#x307C;&#x3046;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>by&cacute; zgubionym</trd>,
    <trd>przepada&cacute;</trd>,
    <trd>polec</trd>,
    <trd>zagin&aogonek;&cacute;</trd>
  </trdgrp>
  <trd lng="ro">muri</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_i;&c_b;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_g;&c_i;&c_b;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_g;&c_i;&c_b;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vyhyn&uacute;&tcaron;</trd>,
    <trd>zahyn&uacute;&tcaron;</trd>,
    <trd>zhyn&uacute;&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_i;&c_n;&c_u;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_n;&c_i;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_p;&c_a;&c_d;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="pere.0iga">
  <kap><tld/>iga</kap>
  <snc mrk="pere.0iga.KOMUNE">
    <dif>
      Kiu ka&ubreve;zas <tld/>on,
      <ref tip="vid" cel="fatal.0a">fatala</ref>:
      <ekz>
        ni komprenis tre bone, kiel <tld/>iga povas fari&gcirc;i
        por nia tuta afero &ccirc;ia publika malpaco
        <fnt>
          <aut>L. L. Zamenhof</aut>
          <vrk>Cirkulera letero al &ccirc;iuj Esperantistoj</vrk>
          <lok>Lingvo Internacia, 1908 (13), p. 73a-77a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>calamiteux</trd>
    <trd>funeste</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x81F4;&#x547D;&#x7684;&#x306A;  <pr>&#x3061;&#x3081;&#x3044;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x751F;&#x6B7B;&#x306B;&#x304B;&#x304B;&#x308F;&#x308B;  <pr>&#x305B;&#x3044;&#x3057;&#x306B;&#x304B;&#x304B;&#x308F;&#x308B;</pr></trd>,
    <trd>&#x7834;&#x6EC5;&#x7684;&#x306A;  <pr>&#x306F;&#x3081;&#x3064;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x58CA;&#x6EC5;&#x7684;&#x306A;  <pr>&#x304B;&#x3044;&#x3081;&#x3064;&#x3066;&#x304D;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>fatalny</trd>,
    <trd>feralny</trd>,
    <trd>zgubny</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>fatal</trd>,
    <trd>feral</trd>,
    <trd>pernicios</trd>
  </trdgrp>
  <trd lng="uk">&c_z;&c_g;&c_u;&c_b;&c_n;&c_i;&c_j;</trd>
</drv>

<drv mrk="pere.0igi">
  <kap><ofc>*</ofc><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pere.0igi.mortigi">
    <dif>
      <ref tip="super" cel="mort.0igi">Mortigi</ref>:
      <ekz>
        birdo rapidas al la kaptilo, kaj ne scias, ke &gcirc;i <tld/>igas sian vivon
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 7:23</lok>
        </fnt>.
      </ekz>
    </dif>
  <trdgrp lng="de">
      <trd>umbringen</trd>,
      <trd>t&ouml;ten</trd>,
      <trd>ausl&ouml;schen</trd>,
      <trd>ausrotten</trd>
  </trdgrp>
  </snc>
  <snc mrk="pere.0igi.detrui">
    <dif>
      <ref cel="detru.0i">Detrui</ref>:
      <ekz>
        sekeco <tld/>igas la grenon;
      </ekz>
      <ekz>
        unu pekulo <tld/>igas multe da bono
        <fnt>
          <bib>MT</bib>
          <lok>Predikanto 9:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni volas ne <tld/>igi la religion, sed savi &gcirc;in de la senduba <tld/>o
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Pri la homaranismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        tiuj nesingardaj vortoj povos havi terure <tld/>igan
        efikon;
      </ekz>
      <ekz>
        la malamegita aspiranto posedis sekreton, kies malkovro nepre <tld/>igos la
        honoron de la patro
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> ne &scirc;anceli&gcirc;is kondamni senkulpan viron kaj pereigi lian
        reputacion
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        celado tro granda povas for&scirc;iri de ni la celon pli malgrandan kaj la homaranismo
        povas <tld/>igi Esperanton
        <fnt>
          <bib>LBe</bib>
          <lok>Nefermita letero al s-ro de Beaufront.</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
        <trd>vernichten</trd>,
        <trd>zerst&ouml;ren</trd>,
        <trd>ins <ind>Verderben</ind> st&uuml;rzen</trd>,
        <trd>ruinieren</trd>,
        <trd>zugrunde <ind>richten</ind></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_b;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_n;&c_ib;&c_sh;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_b;&c_u;&c_r;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_g;&c_u;&c_b;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hubit</trd>,
    <trd>ni&ccaron;it</trd>,
    <trd>zachv&aacute;tit</trd>,
    <trd>zahubit</trd>,
    <trd>zni&ccaron;it</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>destruir</trd>,
    <trd>devastar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;truire</trd>,
    <trd>d&eacute;vaster</trd>,
    <trd>faire <ind>p&eacute;rir</ind></trd>,
    <trd>ruiner</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>elpuszt&iacute;t</trd>,
    <trd>elveszejt</trd>,
    <trd>kiirt</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6BBA;&#x3059;  <pr>&#x3053;&#x308D;&#x3059;</pr></trd>,
    <trd>&#x7834;&#x6EC5;&#x3055;&#x305B;&#x308B;  <pr>&#x306F;&#x3081;&#x3064;&#x3055;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>g&lstroke;adzi&cacute;</trd>,
    <trd>niweczy&cacute;</trd>,
    <trd>zaprzepaszcza&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>distruge</trd>,
    <trd>strica</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_g;&c_u;&c_b;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_g;&c_u;&c_b;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>skyno&zcaron;i&tcaron;</trd>,
    <trd>zahubi&tcaron;</trd>,
    <trd>zni&ccaron;i&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_g;&c_u;&c_b;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_n;&c_i;&c_shch;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_v;&c_je;&c_s;&c_t;&c_i; &c_z;&c_ib; &c_s;&c_v;&c_ib;&c_t;&c_u;</trd>
  </trdgrp>
</drv>

<drv mrk="pere.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="pere.for0i.KOMUNE">
    <dif>
      Malaperi <tld/>inte:
      <ekz>
        vi for<tld/>os de sur tiu tero, kiun donis al vi la
        Eternulo
        <fnt>
          <bib>MT</bib>
          <lok>Josuo 23:13</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_n;&c_ib;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>
  <trd lng="cs">zahynout</trd>
  <trd lng="es"><ind>extinguir</ind>se<klr> (desaparecer)</klr></trd>
  <trd lng="fr">s'<ind>&eacute;teindre</ind><klr> (disparaitre)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x6EC5;&#x3073;&#x53BB;&#x308B;  <pr>&#x307B;&#x308D;&#x3073;&#x3055;&#x308B;</pr></trd>,
    <trd>&#x6D88;&#x3048;&#x3066;&#x306A;&#x304F;&#x306A;&#x308B;  <pr>&#x304D;&#x3048;&#x3066;&#x306A;&#x304F;&#x306A;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wymiera&cacute;</trd>,
    <trd>znika&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>disp&abreve;rea</trd>,
    <trd>pieri</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_g;&c_i;&c_n;&c_u;&c_t;&c_mol;</trd>
  <trd lng="sk">zahyn&uacute;&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_g;&c_i;&c_b;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_shch;&c_je;&c_z;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="pere.sxip0o">
  <kap>&scirc;ip<tld/>o</kap>
  <snc mrk="pere.sxip0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      <tld lit="P"/>o de &scirc;ipo pro subakvi&gcirc;o, frakasi&gcirc;o...:
      <ekz>
        estos multaj la &scirc;ip<tld/>oj pro tia vetero
        <fnt><aut>H. Vallienne</aut>,<vrk>Kastelo de 
        Prelongo</vrk>,<lok>1907</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="romp.sxip0igxo">&scirc;iprompi&gcirc;o</ref>
    <refgrp tip="vid">
      <ref cel="averi.0o.SHI">averio<sncref/></ref>
    </refgrp>
  </snc>
    <trdgrp lng="de">
      <trd>Schiffbruch</trd>,
      <trd>Untergang</trd>
    </trdgrp>
  <trd lng="es">naufragio</trd>
  <trd lng="fr">naufrage</trd>
  <trdgrp lng="ja">
    <trd>&#x6C88;&#x6CA1;  <pr>&#x3061;&#x3093;&#x307C;&#x3064;</pr></trd>,
    <trd>&#x96E3;&#x7834;  <pr>&#x306A;&#x3093;&#x3071;</pr></trd>,
    <trd>&#x906D;&#x96E3;  <pr>&#x305D;&#x3046;&#x306A;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="pl">zatoni&eogonek;cie statku</trd>
  <trd lng="ro">scufundarea navei</trd>
  <trd lng="uk">&c_z;&c_a;&c_g;&c_i;&c_b;&c_je;&c_l;&c_mol; &c_k;&c_o;&c_r;&c_a;&c_b;&c_l;&c_ja;</trd>
</drv>
</art>
