<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Alask</rad>/o
    </kap>
    <drv mrk="alask.0o">
      <kap><tld/>o</kap>
      <snc>
        <uzo tip="fak">GEOG</uzo>
        <uzo tip="fak">POL</uzo>
        <dif>
          Teritorio de la nord-okcidenta parto de
          <ref tip="malprt" cel="amerik.Nord0o">Nordameriko</ref>, kiu,
          vendite de <ref cel="rus.R0ujo">Rusujo</ref> al
          <ref tip="malprt" cel="uson.0o">Usono</ref> en 1867 i&gcirc;is ties
          49&#xAA; <ref cel="sxtat.sub0o">sub&scirc;tato</ref>.
        </dif>
      </snc>
      <bld lok="../bld/Beringa.png"><tld/>o
        <fnt><lok>La&ubreve; mapo el <url
 ref="http://www.pmel.noaa.gov/bering/pages/geog.html">NOAA</url></lok></fnt>
      </bld>
      <trd lng="en">Alaska</trd>
      <trd lng="fr">Alaska</trd>
      <trd lng="nl">Alaska</trd>
      <trd lng="ru">&c_sh;&c_t;&c_a;&c_t; <ind>&c_A;&c_l;&c_ja;&c_s;&c_k;&c_a;</ind></trd>
    </drv>
    <drv mrk="alask.0aDuoninsulo">
      <kap><tld/>a duoninsulo</kap>
      <snc>
        <uzo tip="fak">GEOG</uzo>
        <dif>
          <ref tip="malprt" cel="amerik.Nord0o">Nordamerika</ref>
          duoninsulo limanta la <ref cel="bering.B0aMaro">Beringan maron</ref>
          sud-oriente disde la resto de <ref cel="pacifik.0o">Pacifiko</ref>.
        </dif>
        <ref tip="malprt" cel="alask.0o"><tld/>o</ref>
      </snc>
      <trd lng="en">Alaska Peninsula</trd>
      <trd lng="nl">Schiereiland van <ind>Alaska</ind></trd>
      <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_o;&c_s;&c_t;&c_r;&c_o;&c_v; <ind>&c_A;&c_l;&c_ja;&c_s;&c_k;&c_a;</ind></trd>
    </drv>
    <drv mrk="alask.0aGolfo">
      <kap><tld/>a Golfo</kap>
      <snc>
        <uzo tip="fak">GEOG</uzo>
        <dif>
          <ref tip="super" cel="golf.0o">Golfo</ref> en la norda 
          <ref tip="malprt" cel="pacifik.0o">Pacifiko</ref>,
          sud-oriente de la
          <ref tip="vid" cel="alask.0aDuoninsulo"><tld/>a duoninsulo</ref>.
        </dif>
      </snc>
      <trd lng="en">Alaska Golf</trd>
      <trd lng="en">Golfe d'Alaska</trd>
      <trd lng="nl">Golf van <ind>Alaska</ind></trd>
      <trdgrp lng="ru">
        <trd>&c_A;&c_l;&c_ja;&c_s;&c_k;&c_i;&c_n;&c_s;&c_k;&c_i;&c_j; &c_p;&c_r;&c_o;&c_l;&c_i;&c_v;</trd>,
        <trd>&c_p;&c_r;&c_o;&c_l;&c_i;&c_v; <ind>&c_A;&c_l;&c_ja;&c_s;&c_k;&c_a;</ind></trd></trdgrp>
    </drv>
  </art>

