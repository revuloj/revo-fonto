<art mrk="$Id: oligoc.xml,v 1.21 2024/01/30 16:09:26 revo Exp $">
<kap>
  <rad>oligocen</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="oligoc.0o">
  <kap><tld/>o</kap>
  <snc mrk="oligoc.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Geologia <ref cel="tercia.0a.GEOL">terciara<sncref/></ref>
      <ref tip="lst" cel="epok.0o.periodo" lst="voko:geologiaj_epokoj" val="-33900000">epoko</ref>
      inter
      proksimume 34 kaj 23 milionoj jaroj anta&ubreve; hodia&ubreve;,
      post <ref cel="eocen.0o">eoceno</ref> kaj anta&ubreve; <ref cel="miocen.0o">mioceno</ref>:
      <ekz>
        la desmostilioj estas formortinta ordo de maraj mamuloj kiu vivis en la malfrua
        <tld lit="O"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16366">Desmostilioj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_ib;&c_g;&c_a;&c_c;&c_e;&c_n;</trd>
  <trd lng="cs">oligoc&eacute;n</trd>
  <trdgrp lng="de">
     <trd>Oligoz&auml;n</trd>,
     <trd>Mittelterti&auml;r</trd>
  </trdgrp>
  <trd lng="fr">Oligoc&egrave;ne</trd>
  <trd lng="hu">oligoc&eacute;n</trd>
  <trdgrp lng="ja">
    <trd>&#x6F38;&#x65B0;&#x4E16;  <pr>&#x305C;&#x3093;&#x3057;&#x3093;&#x305B;&#x3044;</pr></trd>,
    <trd>&#x6F38;&#x65B0;&#x7D71; <pr>&#x305C;&#x3093;&#x3057;&#x3093;&#x3068;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">oligoceen</trd>
  <trd lng="pl">oligocen</trd>
  <trd lng="pt">oligoceno</trd>
  <trd lng="ru">&c_o;&c_l;&c_i;&c_g;&c_o;&c_c;&c_je;&c_n;</trd>
  <trd lng="sk">oligoc&eacute;n</trd>
  <trd lng="uk">&#x43E;&#x43B;&c_ib;&c_g;&#x43E;&c_c;&c_je;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x6E10;&#x65B0;&#x4E16; <pr>ji&#x101;nx&#x12B;nsh&#xEC;</pr></trd>,
    <trd>&#x6F38;&#x65B0;&#x4E16; <pr>ji&#x101;nx&#x12B;nsh&#xEC;</pr></trd>,
    <trd>&#x6E10;&#x65B0;&#x7EDF; <pr>ji&#x101;nx&#x12B;nt&#x1D2;ng</pr></trd>,
    <trd>&#x6F38;&#x65B0;&#x7D71; <pr>ji&#x101;nx&#x12B;nt&#x1D2;ng</pr></trd>
  </trdgrp>
</drv>
</art>
