<trd lng="fr">y&egrave;ble</trd>
<trdgrp lng="hu">
  <trd>f&ouml;ldi <ind>bodza</ind></trd>,
  <trd>gyalogbodza</trd>,
  <trd>borzang</trd></trdgrp>
<trd lng="nl">kruidvlier</trd>
<trd lng="ru">&c_t;&c_r;&c_a;&c_v;&c_ja;&c_n;&c_i;&c_s;&c_t;&c_a;&c_ja;
&c_b;&c_u;&c_z;&c_i;&c_n;&c_a;</trd>
