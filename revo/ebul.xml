<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ebul.xml,v 1.21 2023/10/14 15:34:33 revo Exp $">
<kap>
  <rad>ebul</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ebul.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_specioj" cel="speci.0o">Specio</ref>
      de <ref tip="super" cel="sambuk.0o">sambuko</ref>
      (<trd lng="la">Sambucus ebulus</trd>),
      malbonodora arbedo kun ru&gcirc;etaj floroj kaj
      venenaj, nigraj beroj, kiuj estas uzataj en medicino:
      <ekz>
        <ref cel="skarab.or0o">orskarabo</ref>j tre &scirc;atas man&gcirc;i la polenon de <tld/>ofloroj.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/d/db/Sambucus_ebulus_Bantiger_1.jpg/640px-Sambucus_ebulus_Bantiger_1.jpg"
    prm="CC BY-SA 4.0">floroj kaj nematuraj beroj de <tld/>o (Sambucus ebulus), &ccirc;e Bantiger, kantono
    de Bern, Svislando
        <fnt>
          <aut>MurielBendel, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Sambucus_ebulus_Bantiger_1.jpg">&WCW;/File:Sambucus_ebulus_Bantiger_1.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_u;&c_z;&c_ib;&c_n;&c_a; &c_t;&c_r;&c_a;&c_v;&c_ja;&c_n;&c_ib;&c_s;&c_t;&c_a;&c_ja;</trd>,
    <trd>&c_b;&c_u;&c_z;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bez chlebd&iacute;</trd>,
    <trd>chlebd&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Zwergholunder</trd>,
    <trd>Attich</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>dwarf <ind>elder</ind></trd>,
    <trd>danewort</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_q;&a_T;&f_y; &f_k;&a_w;&f_ch;&f_k;</trd>,
    <trd>&a_sh;&a_fatha;&a_w;&a_fatha;&a_n;&a_d;</trd>,
    <trd>&f_p;&a_fatha;&a_l;&a_fatha;&a_m;</trd>
  </trdgrp>
  <trd lng="fr">y&egrave;ble</trd>
  <trdgrp lng="hu">
  <trd>f&ouml;ldi <ind>bodza</ind></trd>,
  <trd>gyalogbodza</trd>,
  <trd>borzang</trd></trdgrp>
  <trd lng="ja">&#x30CB;&#x30EF;&#x30C8;&#x30B3;&#x5C5E;&#x306E;&#x4E00;&#x7A2E; 
  <pr>&#x30CB;&#x30EF;&#x30C8;&#x30B3;&#x305E;&#x304F;&#x306E;&#12356;&#12387;&#12375;&#12421;</pr></trd>
  <trd lng="nl">kruidvlier</trd>
  <trd lng="pl">hebd</trd>
  <trdgrp lng="pt">
    <trd>&eacute;bulo</trd>,
    <trd>engos</trd>,
    <trd>sabugueirinho</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_a;&c_v;&c_ja;&c_n;&c_i;&c_s;&c_t;&c_a;&c_ja;
&c_b;&c_u;&c_z;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">chabzda</trd>
  <trd lng="uk">&c_t;&c_r;&c_a;&c_v;&#8217;&#x44F;&#x43D;&c_i;&c_s;&c_t;&c_a; &c_b;&c_u;&c_z;&c_i;&#x43D;&c_a;</trd>
</drv>
</art>
