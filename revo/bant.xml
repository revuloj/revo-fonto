<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>bant</rad>/o
</kap>
<drv mrk="bant.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Ornama nodo: 
      <ekz>
        ligi &scirc;uon per <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kokard.0o">kokardo</ref>,
      <ref cel="roz.0eto">rozeto</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_n;&c_t;</trd>
  <trd lng="br">rozennig<klr tip="ind"> (skoulm)</klr></trd>
  <trd lng="de">Schleife</trd>
  <trd lng="en">bow</trd>
  <trdgrp lng="fr">
    <trd>ruban</trd>,
    <trd>faveur <klr tip="ind">(ruban)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>masni</trd>,
    <trd>hajszalag</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_a;&c_n;&c_t;</trd>
</drv>
</art>
