<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>bant</rad>/o
</kap>
<drv mrk="bant.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Ornama nodo: 
      <ekz>
        ligi &scirc;uon per <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kokard.0o">kokardo</ref>,
      <ref cel="roz.0eto">rozeto</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Schleife</trd>
  <trdgrp lng="fr">
    <trd>ruban</trd>,
    <trd>faveur <klr tip="ind">(ruban)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_a;&c_n;&c_t;</trd>
</drv>
</art>
