<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>bant</rad>/o
</kap>
<drv mrk="bant.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
    <snc mrk="bant.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Ornama nodo: 
      <ekz>
        ligi &scirc;uon per <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kokard.0o">kokardo</ref>,
      <ref cel="roz.0eto">rozeto</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_n;&c_t;</trd>
  <trd lng="br">rozennig<klr tip="ind"> (skoulm)</klr></trd>
  <trd lng="de">Schleife</trd>
  <trdgrp lng="en">
    <trd>bow</trd>,
    <trd>knot</trd>
  </trdgrp>
  <trd lng="es">lazo <klr>(ornamental)</klr></trd>
  <trdgrp lng="fr">
    <trd>noeud de <ind>ruban</ind></trd>,
    <trd>faveur<klr tip="amb"> (ruban)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>masni</trd>,
    <trd>hajszalag</trd>
  </trdgrp>
  <trd lng="pl">ozdobny <ind>w&eogonek;ze&lstroke;</ind></trd>
  <trd lng="ru">&c_b;&c_a;&c_n;&c_t;</trd>
  <trd lng="sk">ozdobn√Ω uzol</trd>
</drv>
</art>
