<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bant.xml,v 1.24 2025/10/03 12:54:28 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>bant</rad>/o
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="bant.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
    <snc mrk="bant.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Ornama <ref tip="super" cel="nod.0o.TEKS">nodo</ref>:
      <ekz>
        &scirc;i portas bluan <tld/>on
        <fnt>
          <bib>ChL</bib>
          <lok>Noto De L&#x2019; A&ubreve;toro</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;uoj kun <tld/>oj el nigra &scirc;amo
        <fnt>
          <bib>MkM</bib>
          <lok>13</lok>
        </fnt>;
      </ekz>
      <ekz>
        major-<tld/>o
        <fnt>
          <bib>SrT</bib>
          <lok>Libro IX.</lok>
        </fnt>;
      </ekz>
      <ekz>
        belega pentra&jcirc;o <klr>[&#x2026;]</klr> super la supro de la kadro pendis la&ubreve;ra krono,
        sed inter la verdaj folioj trovi&gcirc;is plektita nigra <tld/>o, kaj de &gcirc;i pendis
        malsupren longa funebra krepo
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kravat.bant0o">bantkravato</ref>,
      <ref cel="masx.0o">ma&scirc;o</ref>,
      <ref cel="kokard.0o">kokardo</ref>,
      <ref cel="roz.0eto">rozeto</ref>
    </refgrp>
    <bld
      lok="&WCU;/thumb/0/0a/1939_Jahre_circa_Walter_Sealtiel_Bleistiftzeichnung_Kofa_Portrait_eines_M&auml;dchens_mit_Schleife_im_Haar.jpg/452px-1939_Jahre_circa_Walter_Sealtiel_Bleistiftzeichnung_Kofa_Portrait_eines_M&auml;dchens_mit_Schleife_im_Haar.jpg"
      prm="PD"
      >knabino kun granda <tld/>o en la hararo
        <fnt>
          <aut>Walter Sealtiel, https://www.marktplaats.nl/v/antiek-en-kunst/kunst-schilderijen-klassiek/m2255160315-walt-sealtiel-1890-1948-meisjesportret</aut>
          <lok><url ref="&WCW;/File:1939_Jahre_circa_Walter_Sealtiel_Bleistiftzeichnung_Kofa_Portrait_eines_M%C3%A4dchens_mit_Schleife_im_Haar.jpg"
              >https://commons.wikimedia.org/wiki/File:1939_Jahre_circa_Walter_Sealtiel_Bleistiftzeichnung_Kofa_Portrait_eines_M&auml;dchens_mit_Schleife_im_Haar.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="zh">
    <trd>&#x8774;&#x8776;&#x7D50; <pr>h&uacute;di&eacute;ji&eacute;</pr></trd>,
      <trd>&#x5934;&#x7EF3; <pr>t&oacute;ush&eacute;ng</pr></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_n;&c_t;</trd>
  <trd lng="br">rozennig<klr tip="ind"> (skoulm)</klr></trd>
  <trdgrp lng="cs">
    <trd>ozdobn&yacute; uzel <klr>(ma&scaron;le)</klr></trd>,
    <trd>poutko</trd>
  </trdgrp>
  <trd lng="de">Schleife</trd>
  <trdgrp lng="en">
    <trd>bow</trd>,
    <trd>knot</trd>
  </trdgrp>
  <trd lng="es">lazo <klr>(ornamental)</klr></trd>
  <trdgrp lng="fr">
    <trd>noeud de <ind>ruban</ind></trd>,
    <trd>faveur<klr tip="amb"> (ruban)</klr></trd>
  </trdgrp>
  <trd lng="he">&qof;&shin;&resh; &ayin;&nun;&yod;&bet;&he;</trd>
  <trdgrp lng="hu">
    <trd>masni</trd>,
    <trd>hajszalag</trd>
  </trdgrp>
  <trd lng="ja">&#x98FE;&#x308A;&#x7D50;&#x3073;  <pr>&#x304B;&#x3056;&#x308A;&#x3080;&#x3059;&#x3073;</pr></trd>
  <trd lng="pl">ozdobny <ind>w&eogonek;ze&lstroke;</ind></trd>
  <trd lng="ru">&c_b;&c_a;&c_n;&c_t;</trd>
  <trd lng="sk">ozdobn&yacute; uzol</trd>
  <trd lng="uk">&c_b;&c_a;&c_n;&c_t;</trd>
</drv>
</art>
