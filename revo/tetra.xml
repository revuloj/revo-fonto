<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tetra</rad>/o
</kap>
<drv mrk="tetra.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tetra.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super"
      cel="tetraon1.0oj">tetraonedoj</ref>
      (<trd lng="la">Tetrastes</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_b;&c_ch;&c_y;&c_k;</trd>
  <trdgrp lng="ca">
    <trd>gr&egrave;vol</trd>,
    <trd>g&egrave;nere Tetrastes</trd>
  </trdgrp>
  <trd lng="cs">je&rcaron;&aacute;bek lesni</trd>
  <trd lng="de">Haselhuhn</trd>
  <trd lng="en">hazel hen</trd>
  <trd lng="fr">g&eacute;linotte</trd>
  <trd lng="hu">cs&aacute;sz&aacute;rmad&aacute;r</trd>
  <trd lng="nl">hazelhoen</trd>
  <trd lng="pl">jarz&aogonek;bek</trd>
  <trdgrp lng="pt">
    <trd>tetraz</trd>,
    <trd>galo-da-charneca</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_ja;&c_b;&c_ch;&c_i;&c_k;</trd>
</drv>
</art>
