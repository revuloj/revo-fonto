<vortaro>
<art mrk="$Id$">
<kap>
   <rad>magnezi</rad>/o
   <fnt>Z</fnt>
</kap>
<drv mrk="magnez1.0o">
   <kap>
     <tld/>o
   </kap>
   <mlg>Mg</mlg>
   <uzo tip="fak">KEM</uzo>
   <uzo tip="fak">MED</uzo>
   <uzo tip="fak">FOT</uzo>
   <snc>
     <dif>
       &Hcirc;emia elemento kun atomnumero 12, atompezo 24,305, simbolo Mg.
       Blanka, malpeza <ref tip="super" cel="metal.0o">metalo</ref>, kiu 
inkandeske brulas en aero
kombini&gcirc;ante
       kun oksigeno.
     </dif>
   </snc>
   <trd lng="be">&c_m;&c_a;&c_g;&c_n;&c_ib;&c_j;</trd>       
   <trd lng="br">magneziom</trd>
   <trd lng="ca">magnesi</trd>
   <trd lng="de">Magnesium</trd>
   <trd lng="en">magnesium</trd>
   <trd lng="es">magnesio</trd>
   <trd lng="fr">magn&eacute;sium</trd>
   <trd lng="hu">magn&eacute;zium</trd>
   <trd lng="it">magnesio</trd>
   <trd lng="nl">magnesium</trd>
   <trd lng="pt">magn&eacute;sio</trd>
   <trd lng="ru">&c_m;&c_a;&c_g;&c_n;&c_i;&c_j;</trd>
</drv>
</art>
