<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kovari.xml,v 1.8 2022/05/01 08:09:25 revo Exp $">
  <kap>
    <rad>kovarianc</rad>/o
  </kap>
<drv mrk="kovari.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>EKV</bib><lok>&para;237</lok></fnt>
  <snc>
    <dif>
      <ref tip="dif" cel="varian1.kun0o">Kunvarianco</ref>:
      <ekz>
        la spektra komponanto <frm am="S_k"
        ><k>S</k><sub><k>k</k></sub></frm> estas speco de korelacio a&ubreve;
        <tld/>o inter <frm am="R_m"
        ><k>R</k><sub><k>m</k></sub></frm> kaj
        harmona funkcio <frm am="cos(pi km//M)"
        ><k>cos</k>(<k>&pi;km/M</k>)</frm>
        <fnt>
          <aut>Nils Utne</aut>
          <vrk><url
          ref="&STEB;ingxenierado/hidrologio/marondoj/marondoj.htm"
              >Mezurado kaj statistika analizo de marondoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_v;&c_a;&c_r;&c_y;&c_ja;&c_c;&c_y;&c_ja;</trd>
  <trd lng="ca">covari&agrave;ncia</trd>
  <trdgrp lng="de">
    <trd>Kovarianz</trd>,
    <trd>Covarianz</trd>
  </trdgrp>
  <trd lng="en">covariance</trd>
  <trd lng="es">covarianza</trd>
  <trd lng="fr">covariance</trd>
  <trd lng="hu">kovariancia</trd>  
  <trd lng="pl">kowariancja</trd>
  <trdgrp lng="pt">
    <trd>co-vari&acirc;ncia</trd>,
    <trd>co-varia&ccedil;&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_v;&c_a;&c_r;&c_i;&c_a;&c_c;&c_i;&c_ja;</trd>
</drv>
</art>

