  <trd lng="fr">pr&eacute;paratoire</trd>
  <trd lng="hu">el&odblac;k&eacute;sz&iacute;t&odblac;</trd>
  <trd lng="ja">&#x6E96;&#x5099;&#x306E;  <pr>&#x3058;&#x3085;&#x3093;&#x3073;&#x306E;</pr></trd>
  <trd lng="nl">voorbereidend</trd>
  <trdgrp lng="pl">
    <trd>przygotowawczy</trd>,
    <trd>przedwst&eogonek;pny</trd>
  </trdgrp>
