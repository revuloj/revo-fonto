    <trd>&#35226;&#24735;&#12364;&#12391;&#12365;&#12427;
    <pr>&#12363;&#12367;&#12372;&#12364;&#12391;&#12365;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="pl">przygotowa&cacute; si&eogonek;</trd>
  <trd lng="th">&#3648;&#3605;&#3619;&#3637;&#3618;&#3617;&#3614;&#3619;&#3657;&#3629;&#3617;</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_o;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_g;&c_o;&c_t;&c_o;&c_v;&c_l;&c_ja;&c_t;&c_i;&c_s;&c_ja;</trd>
