<drv mrk="prepar.0eal">
  <kap><tld/>e al</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="prepar.0eal.KOMUNE">
    <dif>
      <ref tip="super" cel="antaux.0.tempe">Anta&ubreve;<sncref/></ref>,
      <ref tip="vid" cel="antici.0eal.KOMUNE">anticipe al</ref>,
      &ccirc;ion pretigante por anta&ubreve;vidata okazo:
      <ekz>
        la brusela diskuto okazis <tld/>e al la aperigo en a&ubreve;tuno
        <klr>[&#8230;]</klr> de &leftquot;verda libro&#8221;
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2007/009190.php">Semajno pri neel&ccirc;erpi&gcirc;onta energio</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e al planata transloki&gcirc;o, mi decidis raciigi mian ar&hcirc;ivon
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2012/010663.php">De papero al komputilo: bitigo de presitaj
          dokumentoj</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Dunkan</nom> eklevis sian paka&jcirc;on de la trotuaro <tld/>e al eniro en
        la a&ubreve;tobuson
        <fnt><aut>J. Francis</aut>, <vrk>La Granda Kaldrono</vrk>,
        <lok>1978</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_d;&c_ch;&c_a;&c_s; <ind>&c_p;&c_a;&c_d;&c_r;&c_y;&c_h;&c_t;&c_o;&c_w;&c_k;&c_ib;</ind></trd>,
      <trd>&c_r;&c_y;&c_h;&c_t;&c_u;&c_ju;&c_ch;&c_y;&c_s;&c_ja; &c_d;&c_a;&#8230;</trd>
    </trdgrp>
  <trd lng="fr">en vue de</trd>
  <trd lng="id"><ind>dengan</ind> maksud</trd>
  <trd lng="pl">z <ind>przygotowaniem</ind>  do</trd>
  <trdgrp lng="zh">
    <trd>&#20197;&#26399; <pr>y&#464;q&imacron;</pr></trd>,
    <trd>&#20026; <pr>w&egrave;i</pr></trd>,
    <trd>&#29234; <pr>w&egrave;i</pr></trd>,
    <trd>&#26377;&#37492;&#20110;&#27492; <pr>y&#466;uji&agrave;ny&uacute;c&#464;</pr></trd>,
    <trd>&#26377;&#37970;&#26044;&#27492; <pr>y&#466;uji&agrave;ny&uacute;c&#464;</pr></trd>,
    <trd>&#24403;&#21069; <pr>d&amacron;ngqi&aacute;n</pr></trd>,
    <trd>&#30070;&#21069; <pr>d&amacron;ngqi&aacute;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="prepar.0o">
  <kap><tld/>o</kap>
  <snc mrk="prepar.0o.KOMUNE">
    <dif>
      Anta&ubreve;<ref tip="super" cel="arangx.0o.KOMUNE">aran&gcirc;o</ref>:
      <ekz>
        ne valoras la faro la koston de l' <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        li konstruu la konstrua&jcirc;on, por kiu mi faris la <tld/>ojn
        <fnt>
          <bib>MT</bib>
