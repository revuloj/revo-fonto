<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>trajn</rad>/o
</kap>
<drv mrk="trajn.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <uzo tip="fak">FER</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="sin" cel="vagon.0aro">Vagonaro</ref> movata de
      <ref tip="prt" cel="lokomo1.0o">lokomotivo</ref> a&ubreve; alia motora forto.
    </dif>
    <ref tip="super" cel="vetur.0ilo">veturilo</ref>
    <trd lng="de">Zug</trd>
    <trd lng="nl">trein</trd>
    <trd lng="pl">poci&aogonek;g</trd>
    <trd lng="ru">&c_p;&c_o;&c_je;&c_z;&c_d;</trd>
    <trd lng="sv">t&aring;g</trd>
  </snc>
  <snc num="2">
    <fnt>Z</fnt>
    <dif>
      Veturilaro sekvanta armeon. 
    </dif>
    <trd lng="nl">legertros</trd>
    <trd lng="ru">&c_o;&c_b;&c_o;&c_z;</trd>
  </snc>
  <trd lng="en">train</trd>
</drv>
</art>
