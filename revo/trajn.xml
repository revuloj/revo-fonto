      Helpa veturilaro sekvanta armeon.
    </dif>
    <trd lng="be">&c_a;&c_b;&c_o;&c_z;</trd>
    <trd lng="en">column</trd>
    <trd lng="es">columna<klr> (militar)</klr></trd>
    <trdgrp lng="fr">
      <trd>train<klr> (militaire)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>tr&eacute;n</trd>,
      <trd>sz&aacute;ll&iacute;t&oacute;oszlop</trd>
    </trdgrp>
