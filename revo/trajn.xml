<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>trajn</rad>/o
</kap>
<drv mrk="trajn.0o">
  <kap><tld/>o</kap>
  <snc mrk="trajn.0o.FER">
    <uzo tip="fak">FER</uzo>
    <uzo tip="fak">TEK</uzo>
    <ref tip="dif" cel="vagon.0aro">vagonaro</ref>
    <ekz>
      dum kelkaj semajnoj voja&gcirc;i per <tld/>o fari&gcirc;is loterio<fnt><aut>Paul
      Gubbins</aut>: <vrk>Politiko elreligita</vrk>,
      <bib>Monato</bib><lok>jaro 2001a, numero 2a, p. 6a</lok></fnt>.
    </ekz>
    <trd lng="de">Zug</trd>
    <trdgrp lng="fr">
      <trd>train</trd>
    </trdgrp>
    <trd lng="nl">trein</trd>
    <trd lng="pl">poci&aogonek;g</trd>
    <trd lng="ru">&c_p;&c_o;&c_je;&c_z;&c_d;</trd>
    <trd lng="sv">t&aring;g</trd>
  </snc>
  <snc mrk="trajn.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <fnt>Z</fnt>
    <dif>
      Helpa veturilaro sekvanta armeon.
    </dif>
    <trdgrp lng="fr">
      <trd>train<klr> (militaire)</klr></trd>
    </trdgrp>
    <trd lng="nl">legertros</trd>
    <trd lng="ru">&c_o;&c_b;&c_o;&c_z;</trd>
  </snc>
  <trd lng="en">train</trd>
</drv>
</art>
