<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: trajn.xml,v 1.34 2021/05/31 14:40:33 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>trajn</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="trajn.0o">
  <kap><tld/>o</kap>
  <snc mrk="trajn.0o.FER">
    <tezrad fak="FER"/>
    <uzo tip="fak">FER</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="dif" cel="vagon.0aro">Vagonaro</ref>:
      <ekz>
        mi alvenis &gcirc;uste je tagi&gcirc;o al la urbo Tula; la <tld/>o
        de tie al <nom>Koslovka</nom> <klr>[&#x2026;]</klr> estis forironta pli
        poste en la mateno
        <fnt><bib>Voja&gcirc;Imp</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_c;&c_ja;&c_g;&c_n;&c_ib;&c_k;</trd>
    <trd lng="bg">&c_v;&c_l;&c_a;&c_k;</trd>
    <trd lng="ca">tren</trd>
    <trd lng="de">Zug</trd>
    <trd lng="en">train</trd>
    <trd lng="es">tren</trd>
    <trdgrp lng="fr">
      <trd>train</trd>
    </trdgrp>
    <trd lng="he">&resh;&kaf;&bet;&tav;</trd>
    <trd lng="hu">vonat</trd>
    <trdgrp lng="it">
      <trd>treno</trd>,
      <trd>convoglio<klr> ferroviario</klr></trd>
    </trdgrp>
    <trd lng="nl">trein</trd>
    <trd lng="pl">poci&aogonek;g</trd>
    <trdgrp lng="pt">
      <trd>trem</trd>,
      <trd>comboio</trd>
    </trdgrp>
    <trd lng="ro">tren</trd>
    <trd lng="ru">&c_p;&c_o;&c_je;&c_z;&c_d;</trd>
    <trd lng="sv">t&aring;g</trd>
  </snc>
  <snc mrk="trajn.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <fnt>Z</fnt>
    <dif>
      Helpa veturilaro sekvanta armeon.
    </dif>
    <trd lng="be">&c_a;&c_b;&c_o;&c_z;</trd>
    <trdgrp lng="de">
      <trd>Train</trd>,
      <trd>Tross</trd>
    </trdgrp>
    <trd lng="en">column</trd>
    <trd lng="es">columna<klr> (militar)</klr></trd>
    <trdgrp lng="fr">
      <trd>train<klr> (militaire)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>tr&eacute;n</trd>,
      <trd>sz&aacute;ll&iacute;t&oacute;oszlop</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>colonna<klr> (mil.)</klr></trd>,
      <trd>convoglio<klr> (mil.)</klr></trd>
    </trdgrp>
    <trd lng="nl">legertros</trd>
    <trd lng="ru">&c_o;&c_b;&c_o;&c_z;</trd>
  </snc>
  <trd lng="af">trein</trd>
  <trd lng="am">&#x1263;&#x1261;&#x122D;</trd>
  <trd lng="ar">&#x642;&#x637;&#x627;&#x631;</trd>
  <trd lng="bo">&#xF62;&#xF72;&#xF0B;&#xF63;&#xF72;</trd>
  <trd lng="cs">vlak</trd>
  <trd lng="ne">&#x930;&#x947;&#x932;</trd>
  <trd lng="sk">vlak</trd>
  <trd lng="sq">tren</trd>
</drv>

<drv mrk="trajn.kargo0o">
  <kap>kargo<tld/>o,
  <var><kap>var<tld/>o</kap></var>
  </kap>
  <snc mrk="trajn.kargo0o.FER">
    <uzo tip="fak">FER</uzo>
    <dif>
      <ref tip="super" cel="trajn.0o.FER"><tld lit="T"/>o<sncref/></ref>
      portanta kargon de objektoj:
      <ekz>
        longa kargo<tld/>o el bluaj kaj oran&gcirc;koloraj cisternaj
        vagonoj staras kaj atendas pluan transporton
        <fnt>
          <aut>K. Kniivil&auml;</aut>
          <vrk>Homoj de Putin</vrk>
          <lok>2016</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">train de marchandises</trd>
</drv>

<drv mrk="trajn.pasagxer0o">
  <kap>pasa&gcirc;er<tld/>o</kap>
  <snc mrk="trajn.pasagxer0o.FER">
    <uzo tip="fak">FER</uzo>
    <dif>
      <ref tip="super" cel="trajn.0o.FER"><tld lit="T"/>o<sncref/></ref>
      portanta homojn, voja&gcirc;antojn:
      <ekz>
        Kenja prezidento <nom>Uhuru Kenyatta</nom> merkrede
        lan&ccirc;is la servon de pasa&gcirc;er<tld/>o de la 480-kilometra
        normal&scirc;pura fervojo inter Mombaso kaj Najrobio
        <fnt>
          <aut>Yu</aut>
          <vrk><url ref="http://esperanto.cri.cn/2521/2017/06/01/179s193447.htm">
          Kenjo oficiale malfermis normal&scirc;puran fervojon</url></vrk>
          <lok>&Ccirc;ina Radio Internacia, 2017-06-01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">train de voyageurs</trd>
</drv>
</art>
