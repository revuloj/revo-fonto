<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: protes.xml,v 1.35 2023/09/03 12:45:13 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>protest</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="protes.0i">
  <kap><tld/>i</kap>
  <snc mrk="protes.0i.malkonsenti">
    <gra><vspec>ntr</vspec></gra>
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Akre esprimi <ref tip="super" cel="akcept.mal0i.KOMUNE">malakcepton</ref> 
      a&ubreve; <ref cel="konten.mal0a">malkontenton</ref> pri io 
      per <ref tip="sin" cel="dir.kontraux0i">kontra&ubreve;diro</ref> 
      a&ubreve; nepretervidebla signo:
      <ekz>
        ili &scirc;iris al li la haregojn, suriris &gcirc;ian dorson, <klr>[&#x2026;]</klr> la urso tute ne
        <tld/>is
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino kaj Rozulino</lok>
        </fnt>;
      </ekz><ekz>
        kiam la faraono anoncis sian intencon eliri en la &gcirc;ardenon, ili ne <tld/>is
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz><ekz>
        mi ne komprenas &ccirc;ion, kion vi al mi diras <klr>[sed]</klr> mia prudento
        <klr>[&#x2026;]</klr> <tld/>as kontra&ubreve; tiuj multaj neebla&jcirc;oj
        <fnt><bib>Marta</bib></fnt>;
      </ekz><ekz>
        kontra&ubreve; tiu &ccirc;i mensoga, malnobla kaj kalumnia kulpigo ni <tld/>as plej energie,
        ni <tld/>as per &ccirc;iuj fibroj de nia koro
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado en la Guildhall (Londono) en la 21a de a&ubreve;gusto 1907</vrk>
        </fnt>!
      </ekz><ekz>
        <klr>[tiuj]</klr> eble <tld/>is kontra&ubreve; la enkondukado de arta
        alfabeto
        <fnt><bib>EE</bib></fnt>;
      </ekz><ekz>
        kiam ilia prudento kontra&ubreve; tio &ccirc;i ek<tld/>is, ili devis tute defali de &ccirc;ia
        religio
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Pri la homaranismo</vrk>
        </fnt>;
      </ekz><ekz>
        kontra&ubreve; tio &ccirc;i <tld/>us la vivo mem
        <fnt><bib>EE</bib></fnt>;
      </ekz><ekz>
        mi tamen tuj for&jcirc;etis tiun formon, kontra&ubreve; kiu <tld/>is mia lingva sento
        <fnt>
          <bib>LR</bib>
          <lok>11. Verboj</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="indign.0i">indigni</ref>,
      <ref cel="ne.0i.diri">nei</ref>,
      <ref cel="refut.0i">refuti</ref>,
      <ref cel="rifuz.0i.proponon">rifuzi</ref>
    </refgrp>
    <trd lng="be">&c_p;&c_r;&c_a;&c_t;&c_e;&c_s;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="de">sich <ind>entr&uuml;sten</ind></trd>
    <trdgrp lng="hu">
      <trd>tiltakozik</trd>,
      <trd>protest&aacute;l</trd>
    </trdgrp>
    <trd lng="nl">protesteren</trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_je;&c_s;&c_t;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="protes.0i.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      <ref tip="sin" cel="dir.kontraux0i">Kontra&ubreve;diri</ref> per 
      formala deklaro ion, kion oni opinias nevera a&ubreve; nejusta:
      <ekz>
        iri al la magistrato kaj <tld/>i, ke la kanajlo tiel profanas
        mian nomon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz><ekz>
        mi esprimas <tld/>on kontra&ubreve; &ccirc;ia farita ju&gcirc;a decido, respondis <nom>Chassi&eacute;</nom> kaj
        forlasis triumfe la salonon de ju&gcirc;o
        <fnt><bib>FK</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_p;&c_r;&c_a;&c_t;&c_e;&c_s;&c_t;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_s;&c_p;&c_r;&c_e;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de"><ind>Einspruch</ind> erheben</trd>
  </snc>
  <snc mrk="protes.0i.kontesti">
    <gra><vspec>tr</vspec></gra>
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="kontes.0i">Kontesti</ref>, <ref cel="rifuz.0i.proponon">rifuzi<sncref/></ref>:
      <ekz>
        mi pov<klr>[u]</klr>s <tld/>i tiun ri&ccirc;a&jcirc;on, &gcirc;in redoni al &gcirc;ia vera posedanto kaj
        poste pafi kuglon en mian kapon
        <fnt><bib>KPr</bib></fnt>.
      </ekz>
    </dif>
<!-- ne traduku evitindajn:
    <trdgrp lng="be">
      <trd>&c_a;&c_p;&c_r;&c_a;&c_t;&c_e;&c_s;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_s;&c_p;&c_r;&c_e;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>betwisten</trd>,
      <trd>bestrijden</trd>
    </trdgrp>
    <trd lng="hu">vitat</trd>
    <trd lng="ru">&c_o;&c_p;&c_r;&c_o;&c_t;&c_je;&c_s;&c_t;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
-->
  </snc>
  <ref tip="hom" cel="protes1.0o">protestant&middot;o</ref>
  <trd lng="bo">&t_nga;&t_naro;&t_tsheg;&t_ra;&t_gata;&t_naro;&t_la;&t_tsheg;&t_ba;&t_yata;&t_drengbu;&t_da;&t_tsheg;</trd>
  <trd lng="ca">protestar</trd>
  <trdgrp lng="cs">
    <trd>nam&iacute;tat</trd>,
    <trd>ohradit se</trd>,
    <trd>protestovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>protestieren</trd>,
    <trd>sich <ind>verwahren</ind></trd>,
  </trdgrp>
  <trd lng="en">protest</trd>
  <trdgrp lng="fr">
    <trd>mettre en cause</trd>,
    <trd>contester</trd>,
    <trd>protester</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6297;&#x8B70;&#x3059;&#x308B;  [<ind>&#x3053;&#x3046;&#x304E;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x7570;&#x8B70;&#x3092;&#x5531;&#x3048;&#x308B;  [<ind>&#x3044;&#x304E;&#x3092;&#x3068;&#x306A;&#x3048;&#x308B;</ind>]</trd>,
    <trd>&#x53CD;&#x5BFE;&#x3092;&#x8868;&#x660E;&#x3059;&#x308B;  [<ind>&#x306F;&#x3093;&#x305F;&#x3044;&#x3092;&#x3072;&#x3087;&#x3046;&#x3081;&#x3044;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x652F;&#x6255;&#x3092;&#x62D2;&#x7D76;&#x3059;&#x308B;  [<ind>&#x3057;&#x306F;&#x3089;&#x3044;&#x3092;&#x304D;&#x3087;&#x305C;&#x3064;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>protestowa&cacute;</trd>,
    <trd>sprzeciwia&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>protestar</trd>,
    <trd>contestar</trd>,
    <trd>impugnar</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ohradi&tcaron; sa</trd>,
    <trd>protestova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_o;&c_t;&c_je;&c_s;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_je;&c_r;&c_je;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_i; &c_n;&c_je;&c_z;&c_g;&c_o;&c_d;&c_u;</trd>
  </trdgrp>
</drv>

<drv mrk="protes.0o">
  <kap><tld/>o</kap>
  <snc mrk="protes.0o.ago">
    <dif>
      Ago <tld/>i:
      <ekz>
        <klr>[ili]</klr> akceptis la verdikton sen <tld/>o
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tri fratoj</lok>
        </fnt>;
      </ekz><ekz>
        en liaj vortoj estis sentebla ia <tld/>o kontra&ubreve; &ccirc;io suferita, eble longe ka&scirc;ata
        malamo kontra&ubreve; la aristokrataro, kiu fine eksplodis
        <fnt><bib>IK</bib></fnt>;
      </ekz><ekz>
        provu nur diri unu vorton de <tld/>o, li tuj sendos al vi en vian domon tutan
        regimenton
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz><ekz>
        kun energia <tld/>o ni for&jcirc;etas tiun &ccirc;i postulon
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Dua Kongreso Esperantista en Gen&egrave;ve en la 28a de a&ubreve;gusto 1906</vrk>
        </fnt>;
      </ekz><ekz>
        la movado <klr>[&#x2026;]</klr> planas organizi denove sid<tld/>ojn 
        <klr>(vd <ref cel="blokad.0o.manifestacio">blokado<sncref/></ref>)</klr> sur la centra placo
        de la &scirc;tata &ccirc;efurbo
        <fnt>
          <bib>MD05</bib>
          <vrk>Fronte al miselektita prezidanto</vrk>
        </fnt>;
      </ekz><ekz>
        en Latinameriko, pro grandskalaj popol<tld/>oj
        <klr>(kp <ref cel="ribel.popol0o">popolribelo</ref>, <ref cel="tumult.0o">tumulto</ref>)</klr>, 
        pluraj prezidantoj rezignis pri sia posteno
        <fnt>
          <bib>MD11</bib>
          <vrk>Libio &ndash; justo kaj maljusto</vrk>
        </fnt>;
      </ekz><ekz>
        <ind>kontra&ubreve; neesto ne helpas <tld/>o</ind>
        <trd lng="hu">ahol nincs, ott ne keresd!</trd>
        <fnt><bib>PrV</bib></fnt>;
      </ekz><ekz>
        sendemanda ek<tld/>o estas ofte kulpatesto
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Protest</trd>,
      <trd>Entr&uuml;stung</trd>
    </trdgrp>
    <trd lng="en">protest</trd>
  </snc>
  <snc mrk="protes.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <uzo tip="fak">JUR</uzo>
    <dif>
      La&ubreve;le&gcirc;a konstato de nepago de kambio.
    </dif>
    <trd lng="de">Verwahrung</trd>
    <trd lng="en">complaint (for breach of promissory note)</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_t;&c_e;&c_s;&c_t;</trd>,
    <trd>&c_a;&c_p;&c_r;&c_a;&c_t;&c_e;&c_s;&c_t;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_a;&c_s;&c_p;&c_r;&c_e;&c_ch;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="bo">&t_nga;&t_naro;&t_tsheg;&t_ra;&t_gata;&t_naro;&t_la;&t_tsheg;</trd>
  <trd lng="ca">protesta</trd>
  <trdgrp lng="cs">
    <trd>odvol&aacute;n&iacute;</trd>,
    <trd>protest</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Einspruch</trd>,
  </trdgrp>
  <trdgrp lng="fr">
    <trd>contestation</trd>,
    <trd>protestation</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tiltakoz&aacute;s</trd>,
    <trd>vitat&aacute;s</trd>,
    <trd>meg&oacute;vatol&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6297;&#x8B70;  [<ind>&#x3053;&#x3046;&#x304E;</ind>]</trd>,
    <trd>&#x7570;&#x8B70;  [<ind>&#x3044;&#x304E;</ind>]</trd>,
    <trd>&#x53CD;&#x5BFE;&#x8868;&#x660E;  [<ind>&#x306F;&#x3093;&#x305F;&#x3044;&#x3072;&#x3087;&#x3046;&#x3081;&#x3044;</ind>]</trd>,
    <trd>&#x62D2;&#x7D76;&#x8A3C;&#x66F8;  [<ind>&#x304D;&#x3087;&#x305C;&#x3064;&#x3057;&#x3087;&#x3046;&#x3057;&#x3087;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">protest</trd>
  <trd lng="pl">protest</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_je;&c_s;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>nes&uacute;hlas</trd>,
    <trd>n&aacute;mietka</trd>,
    <trd>protest</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_o;&c_t;&c_je;&c_s;&c_t;</trd>,
    <trd>&c_n;&c_je;&c_z;&c_g;&c_o;&c_d;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_je;&c_r;&c_je;&c_ch;&c_je;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>

