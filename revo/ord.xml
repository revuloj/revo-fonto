<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc> <rad>ord</rad>/o
</kap>
<drv mrk="ord.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <subdrv>
    <dif>
      Regula aran&gcirc;o de aferoj, la&ubreve; kiu ili ta&ubreve;ge kaj
      oportune rilatas inter si:
    </dif>
    <snc>
      <dif>
        En spaco:
        <ekz>
          la paperoj, libroj estas en <tld/>o;
        </ekz>
        <ekz>
          mar&scirc;i sen <tld/>o;
        </ekz>
        <ekz>
          batal<tld/>o.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>Ordnung</trd>,
        <trd>Anordnung</trd>
      </trdgrp>
    </snc>
    <snc>
      <dif>
        En tempo:
        <ekz>
          alfabeta, kronologia <tld/>o;
        </ekz>
        <ekz>
          observi la <tld/>on de sinsekvo.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>zeitliche Ordnung</trd>,
        <trd>Reihenfolge</trd>
      </trdgrp>
    </snc>
    <snc>
      <dif>
        En spirito:
        <ekz>
          meti <tld/>on en siaj ideoj;
        </ekz>
        <ekz>
          la stilo estas nur la <tld/>o kaj la movado, kiun oni metas en la
          pensoj;
        </ekz>
        <ekz>
          havi konsciencon en <tld/>o.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>Ordnung</trd>,
        <trd>Sinn</trd>
      </trdgrp>
    </snc>
    <snc>
      <dif>
        En socia vivo:
        <ekz>
          <tld/>o bezonas ordonon<fnt>Z</fnt>
          <klr>(neniu <tld/>o sen estro)</klr>;
        </ekz>
        <ekz>
          sen <tld/>o en afero ne ekzistas prospero
          <fnt><bib>PrV</bib></fnt>;
        </ekz>
        <ekz>
          tio ne estas la&ubreve; la natura <tld/>o;
        </ekz>
        <ekz>
          la publika, tradicia <tld/>o estis konfuzita;
        </ekz>
        <ekz>
          meti <tld/>on en la financoj.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>Ordnung</trd>,
        <trd>Organisation</trd>
      </trdgrp>
    </snc>
    <snc mrk="ord.0o.rilato">
      <uzo tip="fak">MAT</uzo>
      <fnt><bib>PIV2</bib></fnt>
      <ref tip="dif" cel="rilat.ordo0o"><tld/>o-rilato</ref>
      <rim>
        &Ccirc;i-sence troveblas <ctl><tld/>igo</ctl> en
        <fnt><bib>EKV</bib></fnt>.
      </rim>
      <trd lng="de">Ordnungsrelation</trd>
      <trd lng="en">order relation</trd>
      <trd lng="fr">relation d'<ind>ordre</ind></trd>
      <trd lng="hu">rendez&eacute;s</trd>
      <trdgrp lng="pl">
        <trd>relacja porz&aogonek;dkowa</trd>,
        <trd>relacja porz&aogonek;dku</trd>,
        <trd>porz&aogonek;dek<klr tip="ind"> (relacja)</klr></trd>
      </trdgrp>
      <trd lng="ru">&c_o;&c_t;&c_n;&c_o;&c_sh;&c_je;&c_n;&c_i;&c_je; &c_p;&c_o;&c_r;&c_ja;&c_d;&c_k;&c_a;</trd>
    </snc>
    <snc mrk="ord.0o.rango">
      <uzo tip="fak">MAT</uzo>
      <dif>
        Rango de matematika objekto, rilate nefake difinitan
        hierar&hcirc;ion a&ubreve; &gcirc;ia pozicio en tia klasifiko.
      </dif>
      <subsnc mrk="ord.0o.derivajxo">
        <fnt><bib>PIV1</bib></fnt>
        <dif>
          <klr>(de <ref cel="deriv.0ajxo.MAT">deriva&jcirc;o</ref>)</klr>:
          <ekz>
            la dua deriva&jcirc;o de funkcio estas deriva&jcirc;o de
            <tld/>o <frm>2</frm>;
          </ekz>
          <ekz>
            dua<tld/>a diferenciala ekvacio.
          </ekz>
        </dif>
        <trd lng="de"><klr tip="amb">(Ableitungs-)</klr>ordnung</trd>
        <trd lng="en">order<klr tip="ind"> (of a derivative)</klr></trd>
        <trd lng="fr">ordre<klr tip="ind"> (d'une d&eacute;riv&eacute;e)</klr></trd>
        <trd lng="hu">rend<klr tip="ind"> (deriv&aacute;lt&eacute;)</klr></trd>
        <trd lng="pl">rz&aogonek;d<klr tip="ind"> (pochodnej, r&oacute;wnania
        r&oacute;&zdot;niczkowego)</klr></trd>
        <trd lng="ru">&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_k;<klr tip="ind"> (&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_o;&c_d;&c_n;&c_o;&c_j;)</klr></trd>
      </subsnc>
      <subsnc mrk="ord.0o.engrupo">
        <dif>
          <klr>(de elemento <frm><k>a</k></frm> en
          <ref cel="grup.0o.MAT">grupo</ref>)</klr>
          Tia plej malgranda nenula pozitiva entjero <frm><k>n</k></frm>,
          ke <frm><k>a</k><sup><k>n</k></sup></frm> egalas al la unuo.
        </dif>
        <trd lng="de">Ordnung</trd>
        <trd lng="en">order<klr tip="ind"> (of an element of a
        group)</klr></trd>
        <trd lng="fr">ordre<klr tip="ind"> (d'un &eacute;l&eacute;ment d'un
        groupe)</klr></trd>
        <trd lng="hu">rend<klr tip="ind"> (csoportelem&eacute;)</klr></trd>
        <trd lng="pl">rz&aogonek;d<klr tip="ind"> (elementu grupy)</klr></trd>
        <trd lng="ru">&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_k;<klr tip="ind"> (&c_e;&c_l;&c_je;&c_m;&c_je;&c_n;&c_t;&c_a; &c_g;&c_r;&c_u;&c_p;&c_p;&c_y;)</klr></trd>
      </subsnc>
      <subsnc mrk="ord.0o.grupo">
        <fnt><bib>EKV</bib>, <lok>&para;309</lok></fnt>
        <dif>
          <klr>(de finia <ref cel="grup.0o.MAT">grupo</ref>)</klr>
          Nombro de &gcirc;iaj elementoj:
          <ekz>
            la <tld/>o <sncref ref="ord.0o.engrupo"/> de elemento en
            grupo dividas la <tld/>on de la grupo.
          </ekz>
        </dif>
        <trd lng="de">Ordnung<klr tip="ind"> (einer Gruppe)</klr></trd>
        <trd lng="en">order<klr tip="ind"> (of a group)</klr></trd>
        <trd lng="fr">ordre<klr tip="ind"> (d'un groupe)</klr></trd>
        <trd lng="hu">rend<klr tip="ind"> (csoport&eacute;)</klr></trd>
        <trd lng="pl">rz&aogonek;d<klr tip="ind"> (grupy)</klr></trd>
        <trd lng="ru">&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_k;<klr tip="ind"> (&c_g;&c_r;&c_u;&c_p;&c_p;&c_y;)</klr></trd>
      </subsnc>
      <subsnc mrk="ord.0o.radiko">
        <dif>
          <klr>(de polinoma <ref
          cel="radik.0o.polinomo">radiko</ref>)</klr>
        </dif>
        <ref tip="dif" cel="obl.0eco">obleco.</ref>
      </subsnc>
      <subsnc mrk="ord.0o.infinitezimo">
        <fnt><bib>MatTerm</bib>, <lok>p. 18</lok></fnt>
        <dif>
          <klr>(de <ref tip="vid"
          cel="infini1.0o">infinitezimo</ref>)</klr>
        </dif>
        <ref tip="vid" cel="infini1.cxef0o">&ccirc;efinfinitezimo.</ref>
        <trd lng="de">Ordnung<klr tip="ind"> (der unendlich kleinen
        Gr&ouml;&szlig;en)</klr></trd>
        <trd lng="en">order<klr tip="ind"> (of an
        infinitesimal)</klr></trd>
        <trd lng="fr">ordre<klr tip="ind"> (d'un infiniment
        petit)</klr></trd>
        <trd lng="hu">rend<klr tip="ind"> (infinitezim&aacute;lisok&eacute;)</klr></trd>
        <trd lng="pl">rz&aogonek;d<klr tip="ind"> (niesko&nacute;czenie ma&lstroke;ej)</klr></trd>
        <trd lng="ru">&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_k;<klr tip="ind"> (&c_b;&c_je;&c_s;&c_k;&c_o;&c_n;&c_je;&c_ch;&c_n;&c_o; &c_m;&c_a;&c_l;&c_o;&c_j;)</klr></trd>
      </subsnc>
      <subsnc mrk="ord.0o.grafeo">
        <dif>
          <klr>(de <ref cel="grafe.0o">grafeo</ref>)</klr>
          Nombro de &gcirc;iaj verticoj.
        </dif>
        <trd lng="de">Ordnung<klr tip="ind"> (eines Graphen)</klr></trd>
        <trd lng="en">order<klr tip="ind"> (of a graph)</klr></trd>
        <trd lng="fr">ordre<klr tip="ind"> (d'un graphe)</klr></trd>
        <trd lng="hu">cs&uacute;cssz&aacute;m<klr tip="ind"> (gr&aacute;f&eacute;)</klr></trd>
        <trd lng="pl">rz&aogonek;d<klr tip="ind"> (grafu)</klr></trd>
        <trd lng="ru">&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_k;<klr tip="ind"> (&c_g;&c_r;&c_a;&c_f;&c_a;)</klr></trd>
      </subsnc>
      <adm>
        Mi ne trovis apogon por "~o de radiko", "~o de elemento en grupo",
        "~o de grafeo" sed MatVort konas: ~o de kurbo/surfaco, ~o de cifero
        (en pozicia frakcio), ~o de subdeterminanto, ~o de kontakto.
        <aut>MB</aut>
      </adm>
    </snc>
    <trd lng="es">orden</trd>
    <trd lng="fr">ordre</trd>
    <trd lng="hu">rend</trd>
    <trd lng="ru">&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_k;</trd>
  </subdrv>
  <subdrv>
    <dif>
      &Ccirc;iu el la &ccirc;efaj grupoj, kiujn enhavas klasifiko:
    </dif>
    <snc mrk="ord.0o.BIO">
      <uzo tip="fak">BOT</uzo>
      <uzo tip="fak">ZOO</uzo>
      <dif>
        &Scirc;tupo en la botanika kaj zoologia klasifikoj
        (<trd lng="la">ordo</trd>).
      </dif>
      <rim>
        La <tld/>onomoj de la zoologia klasifiko estas plej ofte
        formitaj per tipa kvalito kaj la sufikso -ul:
        <ekz>
          parhufuloj, skvamuloj;
        </ekz>
        a&ubreve; per tipa genro kaj la elemento -formaj:
        <ekz>
          paseroformaj, strigoformaj.
        </ekz>
        La internacia nomado tendencas al la dua formo.
        Pli esperanteca nomado, proponita anka&ubreve; de NBN estus
        per tipa genro kaj -ord:
        <ekz>
          paser<tld/>o, paser<tld/>ano.
        </ekz>
      </rim>
      <ref tip="malprt" cel="klas.0o.BIO">klaso</ref>
      <ref tip="prt" cel="famili.0o.BIO">familio</ref>
      <trd lng="de">Ordnung</trd>
      <trd lng="hu">rend <klr>(biol&oacute;giai oszt&aacute;lyoz&aacute;sban)</klr></trd>
      <trdgrp lng="ru">
        <trd>&c_o;&c_t;&c_r;&c_ja;&c_d; <klr>(&c_v; &c_s;&c_i;&c_s;&c_t;&c_je;&c_m;&c_a;&c_t;&c_i;&c_k;&c_je; &c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_y;&c_h;)</klr></trd>,
        <trd>&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_k; <klr>(&c_v; &c_s;&c_i;&c_s;&c_t;&c_je;&c_m;&c_a;&c_t;&c_i;&c_k;&c_je; &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_j; &c_i;
        &c_b;&c_a;&c_k;&c_t;&c_je;&c_r;&c_i;&c_j;)</klr></trd>
      </trdgrp>
    </snc>
    <snc>
      <ekz>
        <uzo tip="fak">ARKI</uzo>
        la kvin arkitekturaj <tld/>oj.
      </ekz>
      <trd lng="ru">&c_o;&c_r;&c_d;&c_je;&c_r; <klr tip="amb">(&c_v; &c_a;&c_r;&c_h;&c_i;&c_t;&c_je;&c_k;&c_t;&c_u;&c_r;&c_je;)</klr></trd>
    </snc>
    <trd lng="fr">ordre</trd>
  </subdrv>
  <trd lng="en">order</trd>
  <trd lng="es">orden</trd>
  <trd lng="cs">po&rcaron;&aacute;dek</trd>
  <trd lng="hu">rend</trd>
  <trd lng="nl">orde</trd>
  <trd lng="pt">ordem</trd>
</drv>

<drv mrk="ord.0a">
  <kap><tld/>a</kap>
  <snc>
    <uzo tip="klr">(io)</uzo>
    <dif>
      Aran&gcirc;ita la&ubreve; ta&ubreve;ga <tld/>o:
      <ekz>
        <tld/>a &ccirc;ambro, tirkesto;
      </ekz>
      <ekz>
        <tld/>e <klr>(regule)</klr> elektitaj delegitoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">ordentlich</trd>
    <trd lng="hu">rendes</trd>
    <trdgrp lng="pt">
      <trd>ordenado</trd>,
      <trd>organizado</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="klr">(iu)</uzo>
      <ref tip="dif" cel="ord.0ema"><tld lit="O"/>ema</ref>
      <ekz>
        bon<tld/>aj virinoj <klr>...</klr> sidas hejme
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    <trd lng="hu">rendszeret&odblac;</trd>
  </snc>
  <snc mrk="ord.0a.aro">
    <uzo tip="fak">MAT</uzo>
    <ref tip="vid" cel="ar.orda0o"><tld/>a aro.</ref>
  </snc>
  <trd lng="hu">rendezett</trd>
  <trd lng="nl">ordelijk</trd>
</drv>

<drv mrk="ord.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <dif>
      Kutimanta aran&gcirc;i aferojn la&ubreve; <tld/>o:
      <ekz>
        li estas <tld/>ema &gcirc;is pedanteco.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>ordentlich</trd>,
    <trd>ordnungsliebend</trd>
  </trdgrp>
  <trd lng="hu">rendszeret&odblac;</trd>
  <trd lng="nl">ordelievend</trd>
  <trd lng="pt">organizado</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_k;&c_k;&c_u;&c_r;&c_a;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_k;&c_l;&c_o;&c_n;&c_n;&c_y;&c_j; &c_k; <ind>&c_p;&c_o;&c_r;&c_ja;&c_d;&c_k;&c_u;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="ord.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Aran&gcirc;i la&ubreve; <tld/>o:
      <ekz>
        levi&gcirc;u, kaj <tld/>igu vian liton
        <fnt><bib>NT</bib>, <lok>&Ago; 9:34</lok></fnt>.
      </ekz>
      <ekz>
        ni devos interrompi la eldonadon &gcirc;is ni tute <tld/>igos la
        aferon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tie la ideoj kun siaj signoj estis <tld/>itaj la&ubreve; specoj
        <fnt><bib>FK</bib>, <lok>6, artikoloj pri esperanto, al la
        historio de la provoj de lingvoj tutmondaj De Leibnitz &gcirc;is la nuna
        tempo, publika parolado, havita en la Nurnberga klubo de
        instruistoj la 11 de Novembro 1884 de L. Einstein (el la
        &leftquot;Bayerische Lehrer-Zeitung&rightquot; No. 11 kaj 12 1885)</lok></fnt>.
      </ekz>
    </dif>
    <subsnc mrk="ord.0igi.KOMP">
      <uzo tip="fak">KOMP</uzo>
      <dif>
        Aran&gcirc;i datumstrukturon la&ubreve; <tld/>o difinita super ties
        anoj:
        <ekz>
          alfabete <tld/>igi nomojn en listo;
        </ekz>
        <ekz>
          <tld/>igi liston la&ubreve; la a&gcirc;o de la listanoj;
        </ekz>
        <ekz>
          kunfandi <tld/>igitajn listojn.
        </ekz>
      </dif>
      <trd lng="en">to <ind>sort</ind></trd>
      <trdgrp lng="ru">
        <trd>&c_s;&c_o;&c_r;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_u;&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_ch;&c_i;&c_t;&c_mol;</trd>
      </trdgrp>
    </subsnc>
  </snc>
  <trdgrp lng="de">
    <trd>ordnen</trd>,
    <trd>anordnen</trd>
  </trdgrp>
  <trd lng="es">ordenar</trd>
  <trdgrp lng="fr">
    <trd>ordonner<klr tip="ind"> (classer)</klr></trd>,
    <trd>classer</trd>,
    <trd>ranger</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>rendez</trd>,
    <trd>elrendez</trd>,
    <trd>rendbe tesz</trd>
  </trdgrp>
  <trd lng="nl">ordenen</trd>
  <trd lng="pt">ordenar</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_ch;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_v;&c_je;&c_s;&c_t;&c_i; &c_v; <ind>&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_k;</ind></trd>
  </trdgrp>
  <adm>
    PIV citas kelkajn Zamenhofajn ekzemplojn de ~i kaj konsideras, ke
    ~igi estas sinonimo. Indus esplori la demandon pli funde.
    De matematika vidpunkto oni povus konsideri ~i, kiel sinonimon
    de &leftquot;provizi per ~o&rightquot; (fakula &jcirc;argono).
    <aut>MB</aut>
  </adm>
</drv>

<drv mrk="ord.en0o">
  <kap>en <tld/>o!</kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc>
    <dif>
      Konsentite, tio bonas, ta&ubreve;gas, &gcirc;ustas.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>d'accord!</trd>,
    <trd>OK!</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>rendben!</trd>,
    <trd>ok&eacute;!</trd>
  </trdgrp>
</drv>

<drv mrk="ord.kun0igi">
  <kap>kun<tld/>igi</kap>
  <snc>
    <dif>
      Aran&gcirc;i la&ubreve; difinitaj rilatoj diversajn partojn por formi
      harmonian tuton:
      <ekz>
        kun<tld/>igi la partojn de sistemo.
      </ekz>
    </dif>
    <trd lng="nl">bijeenschikken</trd>
    <trd lng="ru">&c_k;&c_o;&c_o;&c_r;&c_d;&c_i;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <uzo tip="fak">GRA</uzo>
    <dif>
      Kun<tld/>igi du samfunkciajn propoziciojn (amba&ubreve; &ccirc;efajn
      a&ubreve; amba&ubreve; same dependajn) por ilin ligi a&ubreve;
      kontrastigi:
      <ekz>
        la konjunkcioj <em>kaj</em>, <em>sed</em>, <em>nek</em>
        servas por kun<tld/>igi la propoziciojn.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">coordinar</trd>
  <trd lng="fr">coordonner</trd>
  <trdgrp lng="hu">
    <trd>koordin&aacute;l</trd>,
    <trd>&ouml;sszerendez</trd>,
    <trd>&ouml;sszehangol</trd>
  </trdgrp>
  <trd lng="pt">coordenar</trd>
</drv>

<drv mrk="ord.kun0igilo">
  <kap>kun<tld/>igilo</kap>
  <snc mrk="ord.kun0igilo.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      Kun<tld/>iga konjunkcio.
    </dif>
  </snc>
  <trd lng="es">conjunci&oacute;n coordinativa</trd>
  <trd lng="fr">conjonction de coordination</trd>
  <trd lng="hu">mell&eacute;rendel&odblac; <ind>k&ouml;t&odblac;sz&oacute;</ind></trd>
  <trd lng="nl">nevenschikkende zin</trd>
  <trd lng="pt">conjun&ccedil;&atilde;o coordenada</trd>
  <trd lng="ru">&c_s;&c_o;&c_ch;&c_i;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_s;&c_o;&c_ju;&c_z;</trd>
</drv>

<drv mrk="ord.mal0o">
  <kap>mal<tld/>o</kap>
  <snc>
    <dif>
      Manko de <tld/>o:
      <ekz>
        jen la &ccirc;i tiea po&scirc;testro absolute nenion faras; &ccirc;iuj aferoj estas
        en granda mal<tld/>o
        <fnt><bib>Revizoro</bib>, <lok>akto 4a, sceno 6a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Unordnung</trd>
  <trd lng="fr">d&eacute;sordre</trd>
  <trd lng="hu">rendetlens&eacute;g</trd>
  <trd lng="nl">wanorde</trd>
  <trd lng="pt">desordem</trd>
  <trd lng="ru">&c_b;&c_je;&c_s;&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_k;</trd>
</drv>

<drv mrk="ord.mal0ajxo">
  <kap>mal<tld/>a&jcirc;o</kap>
  <snc mrk="ord.mal0ajxo.KOMUNE">
    <dif>
      Io kontra&ubreve;a al la <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>rendetlen dolog</trd>,
    <trd>rendezetlen dolog</trd>
  </trdgrp>
</drv>

<drv mrk="ord.mal0igi">
  <kap>mal<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Konfuzi, detrui la bonan aran&gcirc;on de aferoj:
      <ekz>
        mi &jcirc;us balais kaj metis &ccirc;ion en &gcirc;ustan lokon, kaj vi &ccirc;ion
        mal<tld/>igas
        <fnt><bib>ElektFab</bib>, <lok>tri oraj haroj de la
        diablo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>d&eacute;ranger</trd>,
    <trd>d&eacute;sorganiser</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&ouml;sszekusz&aacute;l</trd>,
    <trd>felforgat</trd>
  </trdgrp>
</drv>

<drv mrk="ord.re0igi">
  <kap>re<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Aran&gcirc;i denove la&ubreve; anta&ubreve;a <tld/>o:
      <ekz>
        la diplomatoj penas re<tld/>igi la rilatojn inter la
        popoloj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la virineto de maro <klr>...</klr> re<tld/>igis liajn malsekajn
        harojn
        <fnt><bib>Fab1</bib>, <lok>la virineto de maro</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">reordenar</trd>
  <trd lng="fr">r&eacute;ordonner</trd>
  <trdgrp lng="hu">
    <trd>&uacute;jrarendez</trd>,
    <trd>visszarendez</trd>
  </trdgrp>
  <trd lng="nl">herschikken</trd>
  <trd lng="pt">reordenar</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_u;&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_ch;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_v;&c_je;&c_s;&c_t;&c_i; &c_v; <ind>&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_k;</ind></trd>,
    <trd>&c_v;&c_o;&c_s;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_i;&c_t;&c_mol; <ind>&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_k;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="ord.sen0o">
  <kap>sen<tld/>o</kap>
  <snc>
    <ref tip="dif" cel="ord.mal0o">mal<tld/>o</ref>
    <ekz>
      se li diras, ke tie estas sen<tld/>o, do tie efektive estas
      sen<tld/>o
      <fnt><bib>Metrop</bib></fnt>!
    </ekz>
  </snc>
  <trd lng="de">Unordnung</trd>
  <trd lng="fr">d&eacute;sordre</trd>
  <trd lng="hu">rendetlens&eacute;g</trd>
  <trd lng="nl">wanorde</trd>
  <trd lng="pt">desordem</trd>
  <trd lng="ru">&c_b;&c_je;&c_s;&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_k;</trd>
</drv>

<drv mrk="ord.sub0igi">
  <kap>sub<tld/>igi</kap>
  <snc>
    <dif>
      Igi iun a&ubreve; ion sub ies a&ubreve;toritato:
      <ekz>
        la tribunaloj estas sub<tld/>igitaj unuj al aliaj;
      </ekz>
      <ekz>
        tiu a&ubreve;toritato, suverena a&ubreve; sub<tld/>igita.
      </ekz>
    </dif>
    <trd lng="de">unterordnen</trd>
  </snc>
  <snc mrk="ord.sub0igi.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      Kunigi du propoziciojn, kies unu dependas de alia:
      <ekz>
        la konjunkcioj <em>ke</em>, <em>kiam</em>, <em>se</em>
        kaj ceteraj servas por sub<tld/>igi la propoziciojn.
      </ekz>
    </dif>
    <trd lng="es">subordinar</trd>
  </snc>
  <trd lng="nl">ondergeschikt maken</trd>
  <trd lng="fr">subordonner</trd>
  <trd lng="hu">al&aacute;rendel</trd>
  <trd lng="pt">subordinar</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_ch;&c_i;&c_n;&c_ja;&c_t;&c_mol;</trd>
</drv>

<drv mrk="ord.sub0igilo">
  <kap>sub<tld/>igilo</kap>
  <uzo tip="fak">GRA</uzo>
  <snc mrk="ord.sub0igilo.GRA">
    <dif>
      Sub<tld/>iga konjunkcio:
      <ekz>
        &ccirc;ar estas jen kun<tld/>igilo, jen sub<tld/>igilo;
      </ekz>
      <ekz>
        kelkaj prepozicioj, kiel <em>dum</em>, <em>&gcirc;is</em>, estas
        anka&ubreve; sub<tld/>igiloj.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">subjunci&oacute;n, conjunci&oacute;n subordinante</trd>
  <trd lng="fr">conjonction de subordination</trd>
  <trd lng="hu">al&aacute;rendel&odblac; <ind>k&ouml;t&odblac;sz&oacute;</ind></trd>
  <trd lng="nl">ondergeschikte zin</trd>
  <trd lng="pt">conjun&ccedil;&atilde;o subordinada</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_ch;&c_i;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_s;&c_o;&c_ju;&c_z;</trd>
</drv>

<drv mrk="ord.bona0o">
  <kap>bona <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      Tia <ref cel="rilat.ordo0o"><tld/>o-rilato</ref>, ke la&ubreve; &gcirc;i
      &ccirc;iu subaro akceptas <ref cel="minimu.0o.MAT">minimumon</ref>:
      <ekz>
        la&ubreve; aksiomo fare de Zermelo por &ccirc;iu aro eblas trovi
        bonan <tld/>on.
      </ekz>
    </dif>
    <ref tip="vid" cel="ord.bon0a.MAT">bon<tld/>a.</ref>
  </snc>
  <adm>
    Mi ne trovis auxtoritatan fonton, sed la formo bon~a nerekte
    pravigas cxi tiun terminon. <aut>MB</aut>
  </adm>
  <trd lng="en">well-ordering</trd>
  <trd lng="de">Wohlordnung</trd>
  <trd lng="fr">bon <ind>ordre</ind><klr tip="ind"> (math.)</klr></trd>
  <trd lng="hu">j&oacute;lrendez&eacute;s</trd>
  <trd lng="pl">dobry <ind>porz&aogonek;dek</ind></trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_n;&c_y;&c_j; <ind>&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_k;</ind></trd>
</drv>

<drv mrk="ord.bon0a">
  <kap>bon<tld/>a</kap>
  <snc mrk="ord.bon0a.KOMUNE">
    <dif>
      Deca, konvena, bone aran&gcirc;ita, bonmora:
      <ekz>
        sur la planko la floroj dancis tre &ccirc;arme unuj kun la aliaj,
        formis bon<tld/>ajn &ccirc;enojn kaj tenis sin reciproke je la
        longaj verdaj folioj
        <fnt><bib>Fab1</bib>, <lok>floroj de la malgranda Ida</lok></fnt>;
      </ekz>
      <ekz>
        la tulipoj kaj fajrolilioj estas matronoj, kiuj observas,
        ke oni bele dancu kaj ke oni kondutu bon<tld/>e
        <fnt><bib>Fab1</bib>, <lok>floroj de la malgranda Ida</lok></fnt>;
      </ekz>
      <ekz>
        mia sola plezuro estas post la tagman&gcirc;o ku&scirc;i pura kaj
        bon<tld/>a sur la kornico kaj prudente babili kun la kamaradoj
        <fnt><bib>Fab1</bib>, <lok>fluganta kofro</lok></fnt>;
      </ekz>
      <ekz>
        mi nun bon<tld/>e aerumos vian kranion tiel, ke <klr>...</klr>
        vi &ccirc;ion vidos anta&ubreve; vi kiel vivantan
        <fnt><bib>Fab2</bib>, <lok>malnova strata lanterno</lok></fnt>;
      </ekz>
      <ekz>
        virino bon<tld/>a estas muta kaj surda
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        se vi forigos viajn abomeninda&jcirc;ojn de anta&ubreve; Mia
        viza&gcirc;o, kaj ne plu vagados, kaj se vi en vero, bon<tld/>o,
        kaj pieco &jcirc;uros: <ctl>Kiel la Eternulo vivas</ctl>,
        tiam anka&ubreve; la popoloj benos sin per Li
        <fnt><bib>MT</bib>, <lok>&Jer; 4:1</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>convenable<klr tip="ind"> (comme il faut)</klr></trd>,
      <trd>rang&eacute;<klr tip="ind"> (convenable)</klr></trd>,
      <trd>sage</trd>,
      <trd>comme il faut</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>rendezett</trd>,
      <trd>rendes</trd>
    </trdgrp>
    <trd lng="nl">in goede <ind>orde</ind></trd>
    <trdgrp lng="ru">
      <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_p;&c_o;&c_l;&c_u;&c_ch;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_n;&c_a;&c_h;&c_o;&c_d;&c_ja;&c_shch;&c_i;&c_j;&c_s;&c_ja; &c_v; &c_p;&c_o;&c_l;&c_n;&c_o;&c_m; <ind>&c_p;&c_o;&c_r;&c_ja;&c_d;&c_k;&c_je;</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="ord.bon0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>EKV</bib>, <lok>&para;52</lok></fnt>
    <dif>
      <klr>(p.p. <ref cel="ord.0a.aro"><tld/>a</ref> aro)</klr>
      Tia, ke &gcirc;ia <ref cel="ord.0o.rilato"><tld/>o</ref> estas
      <ref cel="ord.bona0o">bona <tld/>o</ref>:
      <ekz>
        oni ankora&ubreve; ne sukcesis trovi <tld/>on, kiu igas
        la aron de reeloj bon<tld/>a;
      </ekz>
      <ekz>
        grava eco de la aro de <ref cel="entjer.natura0o">naturaj
        entjeroj</ref> estas &gcirc;ia bon<tld/>eco.
      </ekz>
    </dif>
    <trd lng="de">wohlgeordnet<klr tip="ind"> (-e Menge)</klr></trd>
    <trd lng="en">well-ordered<klr tip="ind"> (set)</klr></trd>
    <trd lng="fr"><klr tip="ind">(ensemble) </klr>bien
    <ind>ordonn&eacute;</ind></trd>
    <trd lng="hu">j&oacute;lrendezett</trd>
    <trd lng="pl"><klr tip="ind">(zbi&oacute;r) </klr>dobrze
    <ind>uporz&aogonek;dkowany</ind></trd>
    <trd lng="ru">&c_v;&c_p;&c_o;&c_l;&c_n;&c_je; &c_u;&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_ch;&c_je;&c_n;&c_n;&c_o;&c_je;<klr tip="amb">
    (&c_m;&c_n;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </snc>
</drv>

<drv mrk="ord.tag0o">
  <kap>tag<tld/>o</kap>
  <snc>
    <dif>
      Listo de la demandoj pritraktotaj en iu kunveno; programo:
      <ekz>
        meti <klr>(enskribi)</klr> ion en la tag<tld/>on;
      </ekz>
      <ekz>
        super la okazintaj faktoj ni povus silente transiri al la
        tag<tld/>o<fnt>Z</fnt>
        <klr>(forpu&scirc;i proponon por reveni al la anoncita
        tag<tld/>o)</klr>;
      </ekz>
      <ekz>
        &ccirc;ar la kunveno deflanki&gcirc;is de la tag<tld/>o, mi fermas &gcirc;in
        <fnt><bib>Metrop</bib></fnt>!
      </ekz>
    </dif>
    <trd lng="de">Tagesordnung</trd>
    <trd lng="es">orden del d&iacute;a</trd>
    <trd lng="hu">napirend</trd>
    <trd lng="nl">agenda</trd>
    <trd lng="ru">&c_p;&c_o;&c_v;&c_je;&c_s;&c_t;&c_k;&c_a; &c_d;&c_n;&c_ja;</trd>
  </snc>
  <snc>
    <dif>
      Instrukcioj por la hodia&ubreve;a tago sciigataj al soldatoj.
    </dif>
    <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_k; &c_d;&c_n;&c_ja;</trd>
  </snc>
  <trd lng="de">Tagesbefehl</trd>
  <trd lng="fr">ordre du jour</trd>
  <trd lng="hu">napiparancs</trd>
  <trd lng="nl">dagorder</trd>
</drv>
