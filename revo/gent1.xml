<vortaro>
<art mrk="$Id$">

<kap>
<rad>Gent</rad>/o
</kap>
<drv mrk="gent1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="urb.0o">Urbo</ref> en la belga provinco 
      <ref tip="malprt" cel="flandr.F0ujo.prov">Orient-Flandrujo</ref>, en norda 
      Belgujo, &ccirc;e la bordo de la 
      rivero <ref tip="vid" cel="skeld.0o">Skeldo</ref> (3&ring;44'E, 51&ring;03'N).
    </dif>
  </snc>
  
  <trd lng="de">Gent</trd>
  <trd lng="el">&Gamma;&#940;&ny;&delta;&eta;</trd>
  <trd lng="en">Ghent</trd>
  <trd lng="es">Gante</trd>
  <trd lng="fr">Gand</trd>
  <trd lng="it">Gand</trd>
  <trd lng="lat">Gandava</trd>
  <trd lng="nl">Gent</trd>
  <trd lng="pl">Gandawa</trd>
</drv>


</art>


