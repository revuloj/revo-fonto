<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: demimond.xml,v 1.7 2025/02/08 22:25:46 revo Exp $">
<kap>
  <rad>demimond</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="demimond.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="demimond.0o.POL">
    <dif>
      <ref tip="dif" cel="mond.duon0umo.POL">Duonmondumo</ref>:
      <ekz>
        &leftquot;mi volis monon,&rightquot; diris la <tld/>anino
        <fnt>
         <aut>Lord Dunsany, trad. K. Miner</aut>,
         <vrk>La Demagogo kaj la Demimondanino</vrk>,
         <lok>en: <url ref="http://sunflower.com/~miner/DUNSANY_package/dunsany.html">
         Fabeloj</url>, [2007]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <!-- formon maloftan traduku prefere per malofta formo nacilingva -->
  <trd lng="de">Demimonde</trd>
  <trd lng="he">&#x5E2;&#x5D5;&#x5DC;&#x5DD; &#x5D1;&#x5D9;&#x5E0;&#x5D9;&#x5D9;&#x5DD;</trd>
  <trd lng="sk">polosvet</trd>
  <trdgrp lng="cs">
    <trd>demi - monde</trd>,
    <trd>polosv&ecaron;t</trd>
  </trdgrp>
  <trd lng="pl">p&oacute;&lstroke;&sacute;wiatek</trd>
  <trd lng="ro">lumea interlop&abreve;</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_je;&#x43C;&c_ib;&#x43C;&#x43E;&#x43D;&c_d;</trd>,
    <trd>&#x43A;&c_u;&c_r;&c_t;&c_i;&c_z;&c_a;&#x43D;&#x43A;&c_i;</trd>
  </trdgrp>
</drv>
</art>

