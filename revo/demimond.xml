<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>demimond</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="demimond.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="demimond.0o.POL">
    <ref tip="dif" cel="mond.duon0umo.POL">duonmondumo</ref>
      <ekz>
        &leftquot;mi volis monon,&rightquot; diris la <tld/>anino
        <fnt>
         <aut>Lord Dunsany, trad. K. Miner</aut>,
         <vrk>La Demagogo kaj la Demimondanino</vrk>,
         <lok>en: <url
         ref="http://sunflower.com/~miner/DUNSANY_package/dunsany.html">
         Fabeloj</url>, [2007]</lok>
        </fnt>.
      </ekz>
  </snc>
  <!-- formon maloftan traduku prefere per malofta formo nacilingva -->
  <trd lng="sk">polosvet</trd>
</drv>
</art>

