<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bremen.xml,v 1.24 2023/11/01 21:22:37 revo Exp $">
<kap>
  <rad>Bremen</rad>/o <fnt><bib>PIV1</bib></fnt>,
  <var><kap><rad var="m">Brem</rad>/o <fnt><bib>Fab3</bib></fnt></kap></var>
</kap>

<drv mrk="bremen.0o">
  <kap><tld/>o, <var><kap><tld var="m"/>o</kap></var></kap>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
  <snc mrk="bremen.0o.urbo">
    <dif>
      Havena <ref tip="lst" cel="urb.0o" lst="voko:urboj">urbo</ref>
      en norda <ref tip="malprt" cel="german1.G0ujo">Germanujo</ref>
      (8&#730;71'E, 53&#730;21'N), situanta &ccirc;e la rivero Vesero, regurbo de la
      federacia lando <ref tip="dif" cel="bremen.0o.subsxtato">Bremeno<sncref/></ref>:
      <ekz>
        mi iras <tld/>on
        <fnt><bib>ElektFab</bib> <lok>Bremenaj muzikistoj</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a biero
        <fnt><bib>Fab1</bib> <lok>Galo&scirc;oj de feli&ccirc;o</lok></fnt>.
      </ekz>
      <ekz>
        komercistoj de <tld var="m"/>o kaj Lubeko
        <fnt><bib>Fab3</bib>, <lok>Nokta &ccirc;apo de
        fra&ubreve;lo</lok></fnt>;
      </ekz>
      <ekz>
        li rakontis la fabelojn pri <klr>[&#8230;]</klr> &leftquot;Ne&gcirc;ulino&rightquot;, pri &leftquot;<tld var="m"/>aj muzikistoj&rightquot;
        kaj aliajn
        <fnt>
          <bib>IdO</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        pluraj esperantistoj navigis en a&ubreve;gusto de 1910 de <tld/>o &gcirc;is Va&scirc;ingtono al la
        6a UK
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="&Monato;/2004/008392.html"
              >Reveno de heroo</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vapor&scirc;ipo portas <klr>[&#8230;]</klr> sur la anta&ubreve;a masto la standardon de <tld/>o,
        blanka-ru&gcirc;an
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="lat">Brema</trd>
    <trdgrp lng="zh">
      <trd>&#19981;&#20358;&#26757; <pr>b&ugrave;l&aacute;im&eacute;i</pr></trd>,
      <trd>&#24067;&#20358;&#26757; <pr>b&ugrave;l&aacute;im&eacute;i</pr></trd>,
      <trd>&#19981;&#33713;&#26757; <pr>b&ugrave;l&aacute;im&eacute;i</pr></trd>
    </trdgrp>
  </snc>
  <snc mrk="bremen.0o.subsxtato">
    <dif>
      La plej malgranda el la federaciaj &scirc;tatoj de <ref tip="malprt" cel="german1.G0ujo">Germanujo</ref> kun
      regurbo kaj plej granda urbo <ref tip="dif" cel="bremen.0o.urbo">Bremeno<sncref/></ref>
      kaj dua plej granda urbo Bremerhaveno.
    </dif>
    <trd lng="de"><klr>(Stadtstaat) </klr>Bremen</trd>
  </snc>
  <trd lng="be">&c_B;&c_r;&c_e;&c_m;&c_e;&c_n;</trd>
  <trd lng="bg">&c_B;&c_r;&c_je;&c_m;&c_je;&c_n;</trd>
  <trd lng="en">Bremen</trd>
  <trd lng="fr">Br&ecirc;me</trd>
  <trd lng="de">Bremen</trd>
  <trd lng="hu">Br&eacute;ma</trd>
  <trd lng="ja">&#12502;&#12524;&#12540;&#12513;&#12531;</trd>
  <trd lng="nl">Bremen</trd>
  <trd lng="fr">Br&ecirc;me</trd>
  <trd lng="pl">Brema</trd>
  <trd lng="ru">&c_B;&c_r;&c_je;&c_m;&c_je;&c_n;</trd>
  <trd lng="uk">&c_B;&c_r;&c_je;&c_m;&c_je;&c_n;</trd>
</drv>

<drv mrk="bremen.b0ano">
  <kap><tld lit="b"/>ano</kap>
  <snc>
    <dif>
      Civitano de Bremeno.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_e;&c_m;&c_e;&c_n;&c_je;&c_c;</trd>
  <trd lng="de">Bremer</trd>
  <trd lng="hu">br&eacute;mai (lakos)</trd>
  <trd lng="nl">Bremer</trd>
  <trd lng="pl">breme&nacute;czyk</trd>
  <trd lng="ru">&c_b;&c_r;&c_je;&c_m;&c_je;&c_n;&c_je;&c_c;</trd>
</drv>
</art>
