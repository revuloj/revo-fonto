<art mrk="$Id: ardez.xml,v 1.21 2021/02/28 18:35:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ardez</rad>/o
</kap>
<drv mrk="ardez.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="ardez.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Nigregriza <ref tip="super" cel="sxton.0o.MIN">&scirc;tono</ref>, 
      facile disigebla en tavolojn, per kiu oni tegas
      tegmentojn.
    </dif>
  </snc>
  <trd lng="br">maen-glas</trd>
  <trd lng="ca">pissarra</trd>
  <trd lng="cs">b&rcaron;idlice</trd>
  <trdgrp lng="de">
     <trd>Schiefer</trd>,
     <trd>Tonschiefer</trd>
  </trdgrp>
  <trd lng="en">slate</trd>
  <trd lng="es">pizarra</trd>
  <trdgrp lng="fa">
    <trd>&f_p;&a_l;&a_m;&a_h;&zwnj;&a_s;&a_n;&f_g;</trd>,
    <trd>&a_s;&a_n;&f_g; &a_l;&a_w;&a_H;</trd>
  </trdgrp>
  <trd lng="fr">ardoise</trd>
  <trd lng="he">&#x5E6;&#x5B4;&#x5E4;&#x5D7;&#x5B8;&#x5D4;</trd>
  <trd lng="hu">pala</trd>
  <trd lng="nl">lei</trd>
  <trdgrp lng="pl">
    <trd>&lstroke;upek</trd>,
    <trd>szyfer</trd>
  </trdgrp>
  <trd lng="pt">ard&oacute;sia</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_s;&c_p;&c_i;&c_d;</trd>,
    <trd>&c_sh;&c_i;&c_f;&c_je;&c_r;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_n;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="sk">bridlica</trd>
  <trd lng="sv">skiffer</trd>
</drv>

<drv mrk="ardez.0a">
<kap><tld/>a</kap>
  <snc>
    <uzo tip="fak">MIN</uzo>
    <dif>
      Farita el <tld/>o: 
      <ekz>
      ni skribis sur <tld/>aj tabuletoj kaj matematikis per fingroj
       <fnt><aut>Stefan Maul</aut>,
       <vrk>El verva vivo jurnalista</vrk>, <lok>p. 43</lok></fnt>.
     </ekz>
    </dif>
  </snc>
  <trd lng="nl">leien</trd>
  <trd lng="pl">&lstroke;upkowy</trd>
  </drv>
</art>
