<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ardez</rad>/o
</kap>
<drv mrk="ardez.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Nigregriza &scirc;tono, facile disigebla en tavolojn, per kiu oni tegas
      tegmentojn.
    </dif>
  </snc>
  <trd lng="ca">pissarra</trd>
  <trdgrp lng="de">
     <trd>Schiefer</trd>,
     <trd>Tonschiefer</trd>
  </trdgrp>
  <trd lng="en">slate</trd>
  <trd lng="es">pizarra</trd>
  <trd lng="fr">ardoise</trd>
  <trd lng="hu">pala</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_s;&c_p;&c_i;&c_d;</trd>,
    <trd>&c_sh;&c_i;&c_f;&c_je;&c_r;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_n;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="sk">bridlica</trd>
  <trd lng="sv">skiffer</trd>
</drv>
</art>
