<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ardez.xml,v 1.29 2024/11/24 17:40:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ardez</rad>/o
  <fnt><bib>UV</bib></fnt>

</kap>
<drv mrk="ardez.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="ardez.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Nigregriza <ref tip="super" cel="sxton.0o.MIN">&scirc;tono</ref>,
      facile disigebla en tavolojn, uzata i.a. por tegi
      tegmentojn kaj por skribtabuloj:
      <ekz>
        la kastelo estis konstruita sur la krutega roko <klr>[&#8230;]</klr>, mal&gcirc;usta pa&scirc;o a&ubreve;
        <tld/>o glitiga, kaj la junulo estos rapidigita el ducent metroj da alteco
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la pordo estis fiksita <tld/>o, al kiu krajono ali&gcirc;is per &ccirc;eneto
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Deksesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        la menciitaj fosilioj trovi&gcirc;as en <tld/>o kaj a&gcirc;as 2100 milionojn da jaroj
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2014/011280.php">De Gabono al A&ubreve;strio por unika
          ekspozicio</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni povas sekvi la evoluon de konstrutekniko, rigardante &gcirc;ian tegmenton, kiun
        oni kovris per <tld/>platoj la&ubreve; du malsamaj metodoj
        <fnt>
          <bib>Monato</bib>
          <aut>Pierre Grollemund</aut>
          <vrk><url ref="&Monato;/2012/010736.php">La stacidomo de Kolmaro</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
    lok="&WCU;/thumb/b/b8/Schwarzer_Schiefer_-_Black_shale_sample.jpg/640px-Schwarzer_Schiefer_-_Black_shale_sample.jpg"
    prm="CC BY 3.0">&jcirc;urasa <tld/>o kun premspuroj de konkoj, Holzmaden, Baden-Virtembergo, Germanujo
        <fnt>
          <aut>Der_Messer, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Schwarzer_Schiefer_-_Black_shale_sample.jpg">&WCW;/File:Schwarzer_Schiefer_-_Black_shale_sample.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_s;&c_mol;&c_p;&c_ib;&c_d; <klr tip="amb">(&c_m;&c_ib;&c_n;&c_e;&c_r;&c_a;&c_l;)</klr></trd>,
    <trd>&c_sh;&c_y;&c_f;&c_e;&c_r; <klr tip="amb">(&c_m;&c_ib;&c_n;&c_e;&c_r;&c_a;&c_l;)</klr></trd>,
    <trd>&c_s;&c_l;&c_a;&c_n;&c_je;&c_c; <klr tip="amb">(&c_m;&c_ib;&c_n;&c_e;&c_r;&c_a;&c_l;)</klr></trd>,
    <trd>&c_l;&c_u;&c_p;&c_n;&c_ja;&c_k;</trd>
  </trdgrp>
  <trd lng="br">maen-glas</trd>
  <trd lng="ca">pissarra</trd>
  <trd lng="cs">b&rcaron;idlice</trd>
  <trdgrp lng="de">
     <trd>Schiefer</trd>,
     <trd>Tonschiefer</trd>
  </trdgrp>
  <trd lng="en">slate</trd>
  <trd lng="es">pizarra</trd>
  <trdgrp lng="fa">
    <trd>&f_p;&a_l;&a_m;&a_h;&zwnj;&a_s;&a_n;&f_g;</trd>,
    <trd>&a_s;&a_n;&f_g; &a_l;&a_w;&a_H;</trd>
  </trdgrp>
  <trd lng="fr">ardoise</trd>
  <trd lng="he">&tsadi;&#1460;&pe;&het;&#1464;&he;</trd>
  <trd lng="hu">pala</trd>
  <trdgrp lng="ja">
    <trd>&#31896;&#26495;&#23721;  <pr>&#12397;&#12435;&#12400;&#12435;&#12364;&#12435;</pr></trd>,
    <trd>&#30707;&#30436;  <pr>&#12379;&#12365;&#12400;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">lei</trd>
  <trdgrp lng="pl">
    <trd>&lstroke;upek</trd>,
    <trd>szyfer</trd>
  </trdgrp>
  <trd lng="pt">ard&oacute;sia</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_s;&c_p;&c_i;&c_d;</trd>,
    <trd>&c_sh;&c_i;&c_f;&c_je;&c_r;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_n;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="sk">bridlica</trd>
  <trd lng="sv">skiffer</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_l;&c_a;&c_n;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_s;&c_p;&c_ib;&c_d;</trd>,
    <trd>&c_sh;&c_i;&c_f;&c_je;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="ardez.0a">
<kap><tld/>a</kap>
  <snc>
    <uzo tip="fak">MIN</uzo>
    <dif>
      Farita el <tld/>o:
      <ekz>
        <klr>[en]</klr> la <ind><tld/>a tabuleto</ind>
        <klr>(vd <ref cel="tabul.skrib0o">skribtabulo</ref>)</klr>
        <klr>[&#8230;]</klr> senti&gcirc;is &scirc;irado kaj pin&ccirc;ado,
        &ccirc;ar en la ekzemplon de kalkulado en&scirc;teli&gcirc;is mal&gcirc;usta nombro
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
        <trd lng="de">Schiefertafel</trd>
      </ekz>
      <ekz>
        skulptitaj lukoj plibeligas la <tld/>ajn tegmentojn
        <fnt>
          <bib>Monato</bib>
          <aut>Jean-Yves Santerre</aut>
          <vrk><url ref="&Monato;/2009/009817.php">Fortika&jcirc;urbo Gerando</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
       ni skribis sur <tld/>aj tabuletoj kaj matematikis per fingroj
       <fnt><aut>Stefan Maul</aut>,
       <vrk>El verva vivo jurnalista</vrk>, <lok>p. 43</lok></fnt>.
     </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_l;&c_a;&c_n;&c_c;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_l;&c_u;&c_p;&c_n;&c_ja;&c_k;&c_o;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="de">schiefern</trd>
  <trd lng="nl">leien</trd>
  <trd lng="pl">&lstroke;upkowy</trd>
  </drv>
</art>
