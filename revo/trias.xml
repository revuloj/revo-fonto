<art mrk="$Id: trias.xml,v 1.21 2023/10/21 14:37:42 revo Exp $">
<kap>
  <rad>trias</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="trias.0o">
  <kap><tld/>o</kap>
  <snc mrk="trias.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Unua geologia <ref tip="lst" cel="period.0o.dauxro" lst="voko:geologiaj_periodoj" val="-251000000">periodo</ref> de
      <ref tip="malprt" cel="mezozoik.0o">mezozoiko</ref>:
      <ekz>
        <tld/>o estas grava terhistoria epoko ekde fini&gcirc;o de permio, de 251 &gcirc;is 
        200 milionoj da jaroj, komenco de &Jcirc;uras<klr>[&#x2026;]</klr>o,
        dum <tld/>o aperis kokolitoj, unuaj mamuloj, <ref tip="prt" cel="dinosa.0oj">dinosa&ubreve;roj</ref>, 
        ranoj, abundis diplopodoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=138862">Triaso</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_y;&c_ja;&c_s;</trd>
  <trd lng="cs">trias</trd>
  <trd lng="de">Trias</trd>
  <trd lng="en">Triassic</trd>
  <trd lng="fr">Trias</trd>
  <trd lng="hu">tri&aacute;sz</trd>
  <trdgrp lng="ja">
    <trd>&#x4E09;&#x7573;&#x7D00;  <pr>&#x3055;&#x3093;&#x3058;&#x3087;&#x3046;&#x304D;</pr></trd>,
    <trd>&#x4E09;&#x7573;&#x7CFB;  <pr>&#x307F;&#x3063;&#x3058;&#x3087;&#x3046;&#x3051;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">trias</trd>
  <trd lng="pl">trias</trd>
  <trd lng="pt">tri&aacute;ssico</trd>
  <trd lng="ru">&c_t;&c_r;&c_i;&c_a;&c_s;</trd>
  <trd lng="sk">trias</trd>
  <trdgrp lng="uk">
    <trd>&#x442;&#x440;&#x456;&#x430;&#x441;</trd>,
    <trd>&#x442;&#x440;&#x456;&#x430;&#x441;&#x43E;&#x432;&#x438;&#x439; &#x43F;&#x435;&#x440;&#x456;&#x43E;&#x434;</trd>,
    <trd>&#x442;&#x440;&#x456;&#x430;&#x441;&#x43E;&#x432;&#x430; &#x441;&#x438;&#x441;&#x442;&#x435;&#x43C;&#x430;</trd>
  </trdgrp>
</drv>
