<vortaro>
<art mrk="$Id$">
<kap>
  <rad>trias</rad>/o
</kap>
<drv mrk="trias.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Geologia formacio de la sekundara epoko.
    </dif>
    <refgrp tip="vid">
      <ref cel="devon.0o">devono</ref>,
      <ref cel="dias">diaso</ref>,
      <ref cel="jxuras.0o">&jcirc;uraso</ref>,
      <ref cel="kretac.0o">kretaceo</ref>,
      <ref cel="lias.0o">liaso</ref>,
      <ref cel="silur.0o">siluro</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_y;&c_ja;&c_s;</trd>
  <trd lng="de">Trias</trd>
  <trd lng="nl">trias</trd>
  <trd lng="ru">&c_t;&c_r;&c_i;&c_a;&c_s;</trd>
</drv>
</art>
