<vortaro>
<art mrk="$Id: unkari.xml,v 1.13 2023/12/11 12:46:38 revo Exp $">
<kap>
   <rad>unkari</rad>/o
</kap>
<drv mrk="unkari.0o">
   <kap><tld/>o</kap>
   <snc mrk="unkari.0o.BOT">
     <uzo tip="fak">BOT</uzo>
     <dif>
       <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
       tip="malprt" cel="rubi.0oj">rubiacoj</ref>
       (<trd lng="la">Uncaria</trd>) kun &ccirc;. 40 tropikaj specioj:
       <ekz>
         <nom>Ora</nom> haste sendis a&ccirc;eti iom da <tld/>o
         <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 84a, volumo 3a, p.
         50a</lok></fnt>.
       </ekz>
     </dif>
    <bld
    lok="&WCU;/thumb/8/88/Uncaria_rhynchophylla_kagikzr01.jpg/319px-Uncaria_rhynchophylla_kagikzr01.jpg"
    prm="GFDL"><tld/>o (Uncaria rhynchophylla)
        <fnt>
          <aut>keisotyo</aut>
          <lok><url ref="&WCW;/Category:Uncaria">&WCW;/Category:Uncaria</url></lok>
        </fnt>
    </bld>
   </snc>
   <trd lng="be">&c_u;&c_n;&c_k;&c_a;&c_r;&c_y;&c_ja;</trd>
   <trdgrp lng="ca">
     <trd>unc&agrave;ria</trd>,
     <trd>g&egrave;nere Uncaria</trd>
   </trdgrp>
   <trdgrp lng="de">
    <trd>Klimmstrauch</trd>,
    <trd>Katzenkralle</trd>
  </trdgrp>
   <trdgrp lng="en">
     <trd>gambir</trd>,
     <trd>gambier</trd>
   </trdgrp>
   <trdgrp lng="fi">
     <trd>Uncaria<klr> (kasvisuku, mm. gambiirikasvi ja
perunkynsiliaani)</klr></trd>
   </trdgrp>
   <trd lng="fr">gambier</trd>
   <trdgrp lng="hu">
     <trd>gambir(cserje)</trd>,
     <trd>kambir</trd>
   </trdgrp>
   <trd lng="it">uncaria</trd>
   <trd lng="nl">uncaria<klr tip="ind"> (geslacht)</klr></trd>
   <trd lng="pl">czepota</trd>
   <trd lng="pt">gambir</trd>
</drv>
</art>

