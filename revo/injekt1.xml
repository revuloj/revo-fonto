<vortaro>
<art mrk="$Id: injekt1.xml,v 1.18 2025/12/05 22:34:32 revo Exp $">
<kap>
  <rad>injektor</rad>/o
<fnt><bib>PVEF</bib></fnt></kap>
<drv mrk="injekt1.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">TEK</uzo>
    <dif>
      Speciala pervapora <ref tip="super" cel="injekt.0ilo.TEK">injektilo</ref>
      por enigi akvon en kaldronon de vaporma&scirc;ino per la premo mem de la vaporo de tiu kaldrono:
      <ekz>
        vaporaj <tld/>oj
        <fnt>
          <vrk><url
          ref="https://sites.google.com/site/verkojenesperanto/fakaj-verkoj/kaldronegoj">Kaldronegoj
          - Verkoj en Esperanto - Google Sites</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ajut.0o">ajuto</ref>,
      <ref cel="duz.0o">duzo</ref>,
      <ref cel="sucx.0i">su&ccirc;i</ref>,
      <ref cel="sorb.0i">sorbi</ref>
    </refgrp>
  </snc>
  <adm>
    Revizii la difinon (kp PIV2).
    <aut>MB</aut>
  </adm>
  <trd lng="be">&c_ib;&c_n;&c_zh;&c_e;&c_k;&c_t;&c_a;&c_r;</trd>
  <trdgrp lng="de">
     <trd>Injektor</trd>,
     <trd>Wasserstrahld&uuml;se</trd>,
     <trd>Dampfstrahlwasserpumpe</trd>
  </trdgrp>
  <trd lng="es">inyector</trd>
  <trd lng="fr">injecteur</trd>
  <trdgrp lng="hu">
     <trd>injektor</trd>,
     <trd>sug&aacute;rszivatty&uacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30A4;&#x30F3;&#x30B8;&#x30A7;&#x30AF;&#x30BF;&#x30FC;</trd>,
    <trd>&#22132;&#x5C04;&#x5F0F;&#x6CE8;&#x6C34;&#22120;  <pr>&#12405;&#12435;&#12375;&#12419;&#12375;&#x304D;&#12385;&#12421;&#12358;&#12377;&#12356;&#12358;&#12388;&#x308F;</pr></trd>,
    <trd>&#22132;&#x5C04;&#x5F0F;&#x6CE8;&#20837;&#x88C5;&#x7F6E; 
    <pr>&#12405;&#12435;&#12375;&#12419;&#12375;&#x304D;&#12385;&#12421;&#12358;&#x306B;&#12421;&#12358;&#x305D;&#12358;&#12385;</pr></trd>
  </trdgrp>
  <trd lng="nl">injector</trd>
  <trdgrp lng="pl">
    <trd>iniektor</trd>,
    <trd>wtryskiwacz</trd>
  </trdgrp>
  <trd lng="pt">injetor<klr tip="amb"> (tecnologia)</klr></trd>
  <trd lng="ru">&c_i;&c_n;&c_zh;&c_je;&c_k;&c_t;&c_o;&c_r;</trd>
  <trdgrp lng="uk">
    <trd>&c_sh;&#x43F;&c_r;&c_i;&c_c;</trd>,
    <trd>&c_ib;&#x43D;&#8217;&c_jeu;&#x43A;&c_t;&#x43E;&c_r;</trd>,
    <trd>&c_ib;&#x43D;&c_zh;&c_je;&#x43A;&c_t;&#x43E;&c_r;</trd>,
    <trd>&c_zh;&c_i;&c_v;&c_i;&#x43B;&#x44C;&#x43D;&c_i;&c_j; &#x43F;&c_r;&c_i;&#x43B;&c_a;&c_d;</trd>
  </trdgrp>
</drv>
</art>
