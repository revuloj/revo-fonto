<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>injektor</rad>/o
</kap>
<drv mrk="injekt1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Speciala pervapora
      <ref tip="super" cel="injekt.0ilo.TEK">injektilo</ref>
      por enigi akvon en kaldronon
      de vaporma&scirc;ino per la premo mem de la vaporo de tiu kaldrono.  
    </dif>
  </snc>
  <adm>
    Revizii la difinon (kp PIV2).
    <aut>MB</aut>
  </adm>
  <trd lng="be">&c_ib;&c_n;&c_zh;&c_e;&c_k;&c_t;&c_a;&c_r;</trd>
  <trdgrp lng="de">
     <trd>Injektor</trd>,
     <trd>Wasserstrahld&uuml;se</trd>,
     <trd>Dampfstrahlwasserpumpe</trd>
  </trdgrp>
  <trd lng="es">inyector</trd>
  <trdgrp lng="hu">
     <trd>injektor</trd>,
     <trd>sug&aacute;rszivatty&uacute;</trd>
  </trdgrp>
  <trd lng="nl">injector</trd>
  <trd lng="pt">injetor<klr tip="amb"> (tecnologia)</klr></trd>
  <trd lng="ru">&c_i;&c_n;&c_zh;&c_je;&c_k;&c_t;&c_o;&c_r;</trd>
</drv>
</art>
