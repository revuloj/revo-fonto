  <trd lng="pt">repes</trd>
  <trd lng="ru">&c_r;&c_je;&c_p;&c_s;</trd>
  <trd lng="sk">rips</trd>
  <trd lng="uk">&c_r;&c_je;&#x43F;&c_s; (&c_t;&#x43A;&c_a;&#x43D;&c_i;&#x43D;&c_a;)</trd>
