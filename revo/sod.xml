<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>sod</rad>/o
</kap>
<drv mrk="sod.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      NaOH. Senkolora alkala bazo, natria hidroksido uzata por multaj
      industriaj celoj.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_o;&c_d;&c_a;</trd>
  <trd lng="bg">&c_s;&c_o;&c_d;&c_a;</trd>
  <trd lng="de">Soda</trd>
  <trd lng="en">soda</trd>
  <trd lng="es">sosa</trd>
  <trd lng="fr">soude</trd>
  <trd lng="hu">sz&oacute;da</trd>
  <trd lng="nl">soda</trd>
  <trd lng="ru">&c_s;&c_o;&c_d;&c_a;</trd>
  <trdgrp lng="sv">
    <trd>kaustiksoda</trd>,
    <trd>natriumhydroxid</trd>
  </trdgrp>
</drv>
</art>
