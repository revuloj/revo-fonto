<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>agropir</rad>/o
</kap>
<drv mrk="agropi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Agropyron</trd>), kun longaj horizontale
      diskreskantaj
      radikoj, kiu multe malutilas al la kulturkampoj.
    </dif>
    <ref tip="sin" cel="herb.hund0o">hundherbo</ref>
  </snc>
  <trd lng="be">&c_p;&c_y;&c_r;&c_n;&c_ib;&c_k;</trd>
  <trd lng="de">Quecke</trd>
  <trdgrp lng="en">
    <trd>couch grass</trd>,
    <trd>dog grass</trd>,
    <trd>quitch</trd>,
    <trd>twitch</trd>
  </trdgrp>
  <trd lng="fr">chiendent</trd>
  <trd lng="hu">b&uacute;zaf&udblac;</trd>
  <trd lng="nl">kweek<klr tip="ind"> (plant)</klr></trd>
  <trd lng="pt">grama</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_y;&c_r;&c_je;&c_j;</trd>,
    <trd>&c_zh;&c_i;&c_t;&c_n;&c_ja;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">p&yacute;r</trd>
  <trd lng="sv">kamvete</trd>
</drv>
</art>
