<kap>
  <rad>agropir</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="agropi.0o">
  <kap><tld/>o</kap>
  <snc mrk="agropi.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Agropyron</trd>):
      <ekz>
        <tld/>o <klr>...</klr> prem&scirc;topis la lar&gcirc;ajn
        juntojn <klr>[de kaheloj]</klr>
        <fnt>
          <aut>V. Catal&agrave;, trad. J. Ventura i Freixas</aut>,
          <vrk>Soleco</vrk>,
          <lok>1967</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_y;&c_r;&c_n;&c_ib;&c_k;</trd>
  <trd lng="ca">g&egrave;nere Agropyron</trd>
  <trdgrp lng="cs">
    <trd>p&yacute;r</trd>,
    <trd>p&yacute;&rcaron;avka</trd>
  </trdgrp>
  <trd lng="de">Kammquecke</trd>
  <trdgrp lng="en">
    <trd>couch grass</trd>,
    <trd>dog grass</trd>,
    <trd>quitch</trd>,
    <trd>twitch</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>grama</trd>,
    <trd>agropyron</trd>
  </trdgrp>
  <trd lng="fr">chiendent</trd>
  <trd lng="hu">b&uacute;zaf&udblac;</trd>
  <trd lng="it">agropiro</trd>
  <trd lng="ja">&#x30AB;&#x30E2;&#x30B8;&#x30B0;&#x30B5;</trd>
  <trd lng="nl">kweek<klr tip="ind"> (plant)</klr></trd>
  <trd lng="pl">perzyk</trd>
  <trd lng="pt">grama</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_y;&c_r;&c_je;&c_j;</trd>,
    <trd>&c_zh;&c_i;&c_t;&c_n;&c_ja;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">p&yacute;r</trd>
  <trd lng="sv">kamvete</trd>
  <trd lng="tr">ayr&inodot;kotu</trd>
  <trdgrp lng="uk">
    <trd>&c_zh;&c_i;&c_t;&#x43D;&#x44F;&#x43A;</trd>,
    <trd>&#x43F;&c_i;&c_r;&c_ib;&c_j;</trd>
  </trdgrp>
</drv>
