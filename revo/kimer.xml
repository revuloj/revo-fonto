<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kimer.xml,v 1.14 2018/08/31 20:10:19 revo Exp $">
<kap>
  <rad>kimer</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kimer.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="hximer.0o">&Hcirc;imero</ref>:
      <ekz>
        mi volas revi pri amo kaj <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Geraldo Mattos</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006863.html">Kio estas Amo?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de la katedralo <nom>Notre-Dame</nom> de Parizo rigarde al la <esc>grandurbo</esc>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=6664">Nia Damo de Parizo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_ib;&c_m;&c_e;&c_r;&c_a;</trd>
  <trd lng="bg">&c_h;&c_i;&c_m;&c_je;&c_r;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>chim&eacute;ra</trd>,
    <trd>fantasmagorie</trd>,
    <trd>p&rcaron;elud</trd>,
    <trd>vidina</trd>
  </trdgrp>
  <trd lng="de">Chim&auml;re</trd>
  <trd lng="fr">chim&egrave;re</trd>
  <trdgrp lng="hu">
    <trd>agyr&eacute;m</trd>,
    <trd>&aacute;br&aacute;ndk&eacute;p</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>droombeeld</trd>,
    <trd>chim&egrave;re</trd>,
    <trd>hersenschim</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>chimera</trd>,
    <trd>mrzonka</trd>,
    <trd>przywidzenie</trd>,
    <trd>wymys&lstroke;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chim&eacute;ra</trd>,
    <trd>prelud</trd>
  </trdgrp>
</drv>
</art>
