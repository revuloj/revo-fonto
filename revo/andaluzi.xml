<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: andaluzi.xml,v 1.10 2024/04/19 23:25:10 revo Exp $">
<kap>
  <rad>Andaluzi</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>

<drv mrk="andaluzi.A0o">
  <kap><tld/>o</kap>
  <snc mrk="andaluzi.A0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="andaluz.A0ujo">Andaluzujo</ref>:
      <ekz>
        <tld/>aj floroj granataj
        <fnt><bib>RBa</bib></fnt>
      </ekz>
      <ekz>
        kiel en la mezepoka <tld/>o, ma&ubreve;roj, judoj kaj kristanoj kunvivis en harmonio
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2004/008265.html">Parakosmo</url></vrk>
          <bib>Monato</bib>
          <lok>2004/05, p. 27</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tri a&ubreve;tonomiaj komunumoj kun apartaj televid-elsendoj ne regataj de PP &ndash; <tld/>o,
        Katalunio kaj E&ubreve;skio
        <fnt>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2004/008303.html">Amarega venko</url></vrk>
          <bib>Monato</bib>
          <lok>2004/05, p. 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        li voja&gcirc;is al <tld/>o, supozeble por studi la hispanan kaj la flamenkan gitaron
        <fnt>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2008/009326.php">Analoj de Bailenambeann</url></vrk>
          <bib>Monato</bib>
          <lok>2008/03, p. 22</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ar">&a_A;&a_l;&a_A_hamza_sure;&a_n;&a_d;&a_l;&a_s;</trd>
    <trd lng="az">&#x18F;nd&#601;l&uuml;s</trd>
    <trd lng="be">&c_A;&c_n;&c_d;&c_a;&c_l;&c_ju;&c_z;&c_ib;&c_ja;</trd>
    <trd lng="bg">&c_A;&c_n;&c_d;&c_a;&c_l;&c_u;&c_z;&c_i;&c_ja;</trd>
    <trd lng="ca">Andalusia</trd>
    <trd lng="cs">Andalusie</trd>
    <trd lng="de">Andalusien</trd>
    <trd lng="el">&Alfa;&ny;&delta;&alfa;&lambda;&omikron;&ypsilon;&sigma;&jota_ton;&alfa;</trd>
    <trd lng="en">Andalusia</trd>
    <trd lng="es">Andaluc&iacute;a</trd>
    <trd lng="fa">&a_A;&a_n;&a_d;&a_l;&a_s;</trd>
    <trd lng="fr">Andalousie</trd>
    <trd lng="fy">Andal&ucirc;sje</trd>
    <trd lng="gl">Andaluc&iacute;a</trd>
    <trd lng="hu">Andal&uacute;zia</trd>
    <trd lng="io">Andaluzia</trd>
    <trd lng="it">Andalusia</trd>
    <trd lng="lv">Andal&umacron;zija</trd>
    <trd lng="mk">&c_A;&c_n;&c_d;&c_a;&c_l;&c_u;&c_z;&c_i;&#1112;&c_a;</trd>
    <trdgrp lng="nl">
      <trd>Andalusi&euml;</trd>,
      <trd>Andaloesi&euml;</trd>
    </trdgrp>
    <trd lng="pl">Andaluzja</trd>
    <trd lng="pt">Andaluzia</trd>
    <trdgrp lng="ro">
      <trd>Andaluzia</trd>,
      <trd>Andalusia</trd>
    </trdgrp>
    <trd lng="ru">&c_A;&c_n;&c_d;&c_a;&c_l;&c_u;&c_s;&c_i;&c_ja;</trd>
    <trd lng="sh">Andaluzija</trd>
    <trd lng="sk">Andal&uacute;zia</trd>
    <trd lng="sl">Andal&uacute;zija</trd>
    <trd lng="sv">Andalusien</trd>
    <trd lng="tr">End&uuml;l&uuml;s</trd>
    <trd lng="uk">&c_A;&c_n;&c_d;&c_a;&c_l;&c_u;&c_s;&c_ib;&c_ja;</trd>
  </snc>
  <trd lng="ja">&#x30A2;&#x30F3;&#x30C0;&#x30EB;&#x30B7;&#x30A2;</trd>
</drv>
</art>

