<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>monoteism</rad>/o
</kap>
<drv mrk="monote.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Religia a&ubreve; filozofia doktrino, kiu asertas ekzistadon de unusola
      dio, kreinto de la mondo.
    </dif>
  </snc>
  <trd lng="nl">monothe&iuml;sme</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_n;&c_o;&c_t;&c_je;&c_i;&c_z;&c_m;</trd>,
    <trd>&c_je;&c_d;&c_i;&c_n;&c_o;&c_b;&c_o;&c_zh;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="monote.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Adepto de monoteismo.
    </dif>
  </snc>
  <trd lng="nl">monothe&iuml;st</trd>
</drv>
</art>
