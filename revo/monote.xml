<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>monoteism</rad>/o
</kap>

<drv mrk="monote.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Religia a&ubreve; filozofia doktrino, kiu asertas ekzistadon de unusola
      dio, kreinto de la mondo.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_a;&c_t;&c_e;&c_ib;&c_z;&c_m;</trd>
  <trd lng="bg">&c_m;&c_o;&c_n;&c_o;&c_t;&c_je;&c_i;&c_z;&c_malmol;&c_m;</trd>
  <trd lng="de">Monotheismus</trd>
  <trd lng="en">monotheism</trd>
  <trd lng="es">monote&iacute;smo</trd>
  <trdgrp lng="fa">
    <trd>&f_y;&f_k;&a_t;&a_A;&f_p;&a_r;&a_s;&a_t;&f_y;</trd>
    <trd>&a_t;&a_w;&a_H;&f_y;&a_d;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>egyistenhit</trd>,
    <trd>monoteizmus</trd>
  </trdgrp>
  <trd lng="nl">monothe&iuml;sme</trd>
  <trd lng="pt">monote&iacute;smo</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_n;&c_o;&c_t;&c_je;&c_i;&c_z;&c_m;</trd>,
    <trd>&c_je;&c_d;&c_i;&c_n;&c_o;&c_b;&c_o;&c_zh;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
