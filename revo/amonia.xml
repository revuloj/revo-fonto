<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: amonia.xml,v 1.29 2024/08/26 21:26:10 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>amoniak</rad>/o
</kap>
<drv mrk="amonia.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">KEM</uzo>
    <dif>
      NH<sub>3</sub>. <ref tip="super" cel="gas.0o.FIZ">Gaso</ref> konsistanta el azoto kaj
      hidrogeno, ofte
      uzata en akva solva&jcirc;o:
      <ekz>
        la industria <tld/>-produktado okazas grandparte per la
        <nom>Haber</nom>&ndash;<nom>Bosch</nom>-sintezo el elementoj,
        krom tio esti&gcirc;as <tld/>o e&ccirc; kiel kromprodukto de la koakso-produktado
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=99896">Amoniako</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_ib;&c_ja;&c_k;</trd>
  <trd lng="br">amoniak</trd>
  <trdgrp lng="cs">
    <trd>amoniak</trd>,
    <trd>&ccaron;pavek</trd>
  </trdgrp>
  <trd lng="de">Ammoniak</trd>
  <trd lng="en">anhydrous ammonia</trd>
  <trd lng="es">amoniaco</trd>
  <trd lng="fa">&a_A_madda;&a_m;&a_w;&a_n;&f_y;&a_A;&f_k;</trd>
  <trd lng="fr">ammoniac</trd>
  <trd lng="he">&#x5D0;&#x5B7;&#x5DE;&#x5D5;&#x5B9;&#x5E0;&#x5B4;&#x5D9;&#x5B8;&#x5D4;</trd>
  <trdgrp lng="ja">
    <trd>&#x30A2;&#x30F3;&#x30E2;&#x30CB;&#x30A2;  <pr>&#12354;&#12435;&#12418;&#x306B;&#12354;</pr></trd>,
    <trd>&#x30A2;&#x30F3;&#x30E2;&#x30CB;&#x30A2;&#x6C34; 
    <pr>&#x30A2;&#x30F3;&#x30E2;&#x30CB;&#x30A2;&#12377;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">ammoniak</trd>
  <trd lng="pl">amoniak</trd>
  <trd lng="pt">amon&iacute;aco</trd>
  <trd lng="ro">amoniac</trd>
  <trd lng="ru">&c_a;&c_m;&c_m;&c_i;&c_a;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>amoniak</trd>,
    <trd>&ccaron;pavok</trd>
  </trdgrp>
  <trd lng="sv">ammoniak</trd>
  <trd lng="uk">&c_a;&#x43C;&c_ib;&c_a;&#x43A;</trd>
</drv>

<drv mrk="amonia.sal0o">
  <kap><ofc>*</ofc>sal<tld/>o</kap>
  <snc>
    <uzo tip="fak">KEM</uzo>
    <dif>
      NH<sub>4</sub>Cl a&ubreve; amonia klorido, blanka kristala salo, alte
      solvebla en akvo.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_sh;&c_a;&c_t;&c_y;&c_r;</trd>,
    <trd>&c_s;&c_a;&c_l;&c_mol;&c_m;&c_ja;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Salmiak</trd>
  <trdgrp lng="en">
    <trd>ammonium chloride</trd>,
    <trd>sal ammoniac</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>chlorure d'ammonium</trd>,
    <trd>sal ammoniac</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>salmiak</trd>,
    <trd>chlorek amonu</trd>
  </trdgrp>
  <trd lng="uk">&#x43D;&c_a;&c_sh;&c_a;&c_t;&c_i;&c_r;</trd>
</drv>
</art>
