<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>amoniak</rad>/o
</kap>
<drv mrk="amonia.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      NH<sub>3</sub>. Gaso konsistanta el azoto kaj hidrogeno, ofte uzata
      en akva solva&jcirc;o.
    </dif>
  </snc>
  <trd lng="de">Ammoniak</trd>
  <trd lng="en">anhydrous ammonia</trd>
  <trd lng="es">amoniaco</trd>
  <trd lng="fr">ammoniac</trd>
  <trd lng="nl">ammoniak</trd>
  <trd lng="ru">&c_a;&c_m;&c_m;&c_i;&c_a;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>amoniak</trd>,
    <trd>&ccaron;pavok</trd>
  </trdgrp>
</drv>
</art>
