<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: obstru.xml,v 1.18 2021/06/07 02:04:08 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>obstrukc</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="obstru.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="obstru.0i.KOMUNE">
    <dif>
      <ref tip="vid" cel="embara.0i.MOV">Embarasi</ref>,
      &gcirc;eni la normalan fluon, cirkulon, iron;
      parte 
      <ref tip="vid" cel="sxtop.0i.bari">&scirc;topi</ref>:
      <ekz>
        la veturiloj <tld/>is la avenuon.
      </ekz>
    </dif>
    <trd lng="de">verstopfen</trd>
    <trdgrp lng="fr">
      <trd>engorger</trd>,
      <trd>obstruer</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>elt&ouml;m&iacute;t</trd>,
      <trd>eldugaszol</trd>
    </trdgrp>
    <trd lng="nl">belemmeren</trd>
    <trdgrp lng="pl">
      <trd>zatka&cacute;</trd>,
      <trd>zapcha&cacute;</trd>,
      <trd>zatwardzi&cacute; &zdot;o&lstroke;&aogonek;dek</trd>,
      <trd>robi&cacute; utrudnienia</trd>
    </trdgrp>
    <trdgrp lng="ro">
       <trd>priza</trd>,
       <trd>&icirc;nfunda</trd>,
       <trd>&icirc;nt&abreve;ri stomacul</trd>,
       <trd>face dificultati</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_k;&c_u;&c_p;&c_o;&c_r;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_g;&c_r;&c_o;&c_m;&c_o;&c_z;&c_d;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_u;&c_s;&c_t;&c_r;&c_o;&c_i;&c_t;&c_mol; &c_z;&c_a;&c_t;&c_o;&c_r;</trd>
    </trdgrp>
  </snc>
  <snc mrk="obstru.0i.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Malhelpi parlamentan laboron per diversaj la&ubreve;le&gcirc;aj rimedoj:
      <ekz>
        La opoziciantaro provis <tld/>i la balotadon de la le&gcirc;o.
      </ekz>
    </dif>
    <trd lng="de">verschleppen</trd>
    <trd lng="en">filibuster</trd>
    <trd lng="fr">faire obstruction</trd>
    <trd lng="hu">obstru&aacute;l</trd>
    <trd lng="nl">obstructie voeren</trd>
    <trd lng="ru">&c_u;&c_s;&c_t;&c_r;&c_o;&c_i;&c_t;&c_mol; &c_o;&c_b;&c_s;&c_t;&c_r;&c_u;&c_k;&c_c;&c_i;&c_ju;</trd>
  </snc>
  <trd lng="bo">&#xF42;&#xF7C;&#xF42; &#xF56;&#xF40;&#xF42; &#xF51;&#xF42;&#xF42;
  &#xF41;&#xF7C;&#xF42;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>ma&rcaron;it jedn&aacute;n&iacute;</trd>,
    <trd>obstruovat</trd>,
    <trd>prov&aacute;d&ecaron;t obstrukci</trd>,
    <trd>p&rcaron;ek&aacute;&zcaron;et</trd>
  </trdgrp>
  <trd lng="en">obstruct</trd>
  <trd lng="es">obstruir</trd>
  <trd lng="pt">obstruir</trd>
  <trdgrp lng="sk">
    <trd>mari&tcaron; jednanie</trd>,
    <trd>ob&scaron;truova&tcaron;</trd>,
    <trd>prek&aacute;&zcaron;a&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="obstru.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="obstru.0o.KOMUNE">
    <dif>
      Embaraso, parta a&ubreve; provizora
      <ref tip="vid" cel="sxtop">&scirc;topado</ref>
      ka&ubreve;zita de amaso da objektoj, kiuj malhelpas cirkuladon,
      trairadon tra vojo, kanalo ktp:
      <ekz>
        granda <tld/>o sur strato;
      </ekz>
      <ekz>
        <tld/>o de tubo, de kloako, de la intesto.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Verstopfung</trd>,
      <trd>Hindernis</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>stoppage</trd>,
      <trd>congestion</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>engorgement</trd>,
      <trd>obstruction</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>eldugul&aacute;s</trd>,
      <trd>t&ouml;m&iacute;t&odblac;d&eacute;s</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>obstructie</trd>,
      <trd>verstopping</trd>,
      <trd>belemmering</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_k;&c_u;&c_p;&c_o;&c_r;&c_k;&c_a;</trd>,
      <trd>&c_z;&c_a;&c_t;&c_o;&c_r;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_b;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="obstru.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Intenca malhelpado al parlamenta laboro, farita per &ccirc;iuj le&gcirc;aj
      rimedoj.
    </dif>
    <trd lng="de">Obstruktion</trd>
    <trdgrp lng="en">
      <trd>filibuster</trd>
    </trdgrp>
    <trd lng="fr">obstruction</trd>
    <trd lng="hu">obstrukci&oacute;</trd>
    <trd lng="nl">obstructie</trd>
    <trd lng="ru">&c_o;&c_b;&c_s;&c_t;&c_r;&c_u;&c_k;&c_c;&c_i;&c_ja;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>obstrukce</trd>,
    <trd>ucp&aacute;n&iacute;</trd>,
    <trd>z&aacute;cpa</trd>
  </trdgrp>
  <trd lng="en">obstruction</trd>
  <trd lng="es">obstrucci&oacute;n</trd>
  <trdgrp lng="pl">
    <trd>obstrukcja</trd>,
    <trd>zator</trd>,
    <trd>zatwardzenie</trd>
  </trdgrp>
  <trd lng="pt">obstru&ccedil;&atilde;o</trd>
  <trdgrp lng="ro">
    <trd>constipa&#539;ie</trd>,
    <trd>blocaj</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ob&scaron;trukcia</trd>,
    <trd>zapchanie</trd>,
    <trd>z&aacute;pcha</trd>
  </trdgrp>
</drv>
</art>
