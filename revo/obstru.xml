<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>obstrukc</rad>/o
</kap>
<drv mrk="obstru.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Embaraso ka&ubreve;zita de amaso da objektoj, kiuj malhelpas cirkuladon,
      trairadon tra vojo, kanalo ktp:
      <ekz>
        granda <tld/>o sur strato;
      </ekz>
      <ekz>
        <tld/>o de tubo, de kloako, de la intesto.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="sxtop">&scirc;topado</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Verstopfung</trd>,
      <trd>Hindernis</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>stoppage</trd>,
      <trd>congestion</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>engorgement</trd>,
      <trd>obstruction</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>eldugul&aacute;s</trd>,
      <trd>t&ouml;m&iacute;t&odblac;d&eacute;s</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>obstructie</trd>,
      <trd>verstopping</trd>,
      <trd>belemmering</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_k;&c_u;&c_p;&c_o;&c_r;&c_k;&c_a;</trd>,
      <trd>&c_z;&c_a;&c_t;&c_o;&c_r;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_b;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">POL</uzo>
    <dif>
      Intenca malhelpado al parlamenta laboro, farita per &ccirc;iuj le&gcirc;aj
      rimedoj.
    </dif>
    <trd lng="de">Obstruktion</trd>
    <trdgrp lng="en">
      <trd>filibuster</trd>
    </trdgrp>
    <trd lng="fr">obstruction</trd>
    <trd lng="hu">obstrukci&oacute;</trd>
    <trd lng="nl">obstructie</trd>
    <trd lng="ru">&c_o;&c_b;&c_s;&c_t;&c_r;&c_u;&c_k;&c_c;&c_i;&c_ja;</trd>
  </snc>
  <trd lng="en">obstruction</trd>
  <trd lng="es">obstrucci&oacute;n</trd>
  <trd lng="pt">obstru&ccedil;&atilde;o</trd>
  <trdgrp lng="sk">
    <trd>obštrukcia</trd>,
    <trd>zapchanie</trd>,
    <trd>zápcha</trd>
  </trdgrp>
</drv>

<drv mrk="obstru.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <dif>
    Fari <tld/>on:
  </dif>
  <snc>
    <ekz>
      la veturiloj <tld/>is la avenuon.
    </ekz>
    <trd lng="de">verstopfen</trd>
    <trdgrp lng="fr">
      <trd>engorger</trd>,
      <trd>obstruer</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>elt&ouml;m&iacute;t</trd>,
      <trd>eldugaszol</trd>
    </trdgrp>
    <trd lng="nl">belemmeren</trd>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_k;&c_u;&c_p;&c_o;&c_r;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_g;&c_r;&c_o;&c_m;&c_o;&c_z;&c_d;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_u;&c_s;&c_t;&c_r;&c_o;&c_i;&c_t;&c_mol; &c_z;&c_a;&c_t;&c_o;&c_r;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">POL</uzo>
    <ekz>
      La opoziciantaro provis <tld/>i la balotadon de la le&gcirc;o.
    </ekz>
    <trd lng="de">verschleppen</trd>
    <trd lng="en">filibuster</trd>
    <trd lng="fr">faire obstruction</trd>
    <trd lng="hu">obstru&aacute;l</trd>
    <trd lng="nl">obstructie voeren</trd>
    <trd lng="ru">&c_u;&c_s;&c_t;&c_r;&c_o;&c_i;&c_t;&c_mol; &c_o;&c_b;&c_s;&c_t;&c_r;&c_u;&c_k;&c_c;&c_i;&c_ju;</trd>
  </snc>
  <trd lng="en">obstruct</trd>
  <trd lng="es">obstruir</trd>
  <trd lng="pt">obstruir</trd>
  <trdgrp lng="sk">
    <trd>mariť jednanie</trd>,
    <trd>obštruovať</trd>,
    <trd>prekážať</trd>
  </trdgrp>
</drv>
</art>
