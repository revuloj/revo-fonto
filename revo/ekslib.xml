<art mrk="$Id: ekslib.xml,v 1.20 2023/11/07 19:22:36 revo Exp $">
<kap>
  <rad>ekslibris</rad>/o
</kap>
<drv mrk="ekslib.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="skrib.sur0o">Surskribo</ref>
      a&ubreve; bildo, kiun iu surmetas sur la libroj de sia
      biblioteko, por montri, ke ili estas lia propra&jcirc;o:
      <ekz>
        pura ekzemplero kompleta, sen <tld/>o a&ubreve; stampo a&ubreve; dedi&ccirc;o, nur kun tre klara
        nomindiko en malnova manskribo per nigra inko
        <fnt>
          <bib>Monato</bib>
          <aut>Bernhard Pabst</aut>
          <vrk><url ref="&Monato;/2008/009376.php">Inter libroj posttagmeze</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i gaje malfermis la libron kaj ne povis ne rimarki la belan glua&jcirc;on en &gcirc;i:
        <tld/>o kun papilio sidanta sur malfermita libro
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="&Monato;/2010/010053.php">Ekslibriso</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_s;&c_l;&c_ib;&c_b;&c_r;&c_y;&c_s;</trd>
  <trd lng="ca">ex-libris</trd>
  <trd lng="cs">exlibris</trd>
  <trd lng="de">Exlibris</trd>
  <trd lng="en">ex libris</trd>
  <trd lng="es">ex libris</trd>
  <trd lng="fr">ex-libris</trd>
  <trd lng="hu">ex libris</trd>
  <trdgrp lng="ja">
    <trd>&#x8535;&#x66F8;&#x7968;  <pr>&#x305E;&#x3046;&#x3057;&#x3087;&#x3072;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x8535;&#x66F8;&#x5370;  <pr>&#x305E;&#x3046;&#x3057;&#x3087;&#x3044;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ekslibris</trd>,
    <trd>ex-libris</trd>
  </trdgrp>
  <trd lng="pt">ex-libris</trd>
  <trd lng="ru">&c_e;&c_k;&c_s;&c_l;&c_i;&c_b;&c_r;&c_i;&c_s;</trd>
  <trd lng="sk">exlibris</trd>
  <trd lng="uk">&#x435;&#x43A;&#x441;&#x43B;&#x456;&#x431;&#x440;&#x438;&#x441;</trd>
  <trd lng="zh">&#x85CF;&#x4E66;&#x7968; <pr>c&#xE1;ngsh&#x16B;pi&#xE0;o</pr></trd>
</drv>
