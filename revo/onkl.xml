<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>onkl</rad>/o
</kap>
<drv mrk="onkl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">GEN</uzo>
  <snc mrk="onkl.0o.patrapatrina">
    <dif>
      Frato de patro a&ubreve; patrino:
      <ekz>
        Kritio, dis&ccirc;iplo de Sokrato kaj <tld/>o de Platono<fnt><aut>Andr&eacute;
        Cherpillod</aut>: <vrk>Atlantido &ndash; la perdita kontinento</vrk>,
        <bib>Monato</bib><lok>jaro 1997a, numero 7a, p. 21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="onkl.0o.boonklo">
    <dif>
      Edzo de <tld/>ino.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_ja;&c_d;&c_z;&c_mol;&c_k;&c_a;</trd>,
    <trd>&c_d;&c_z;&c_ja;&c_d;&c_z;&c_ja;</trd>
  </trdgrp>
  <trd lng="ca">oncle</trd>
  <trd lng="cs">str&yacute;c</trd>
  <trd lng="de">Onkel</trd>
  <trd lng="el">&theta;&epsilon;&jota_ton;&omikron;&sigma_fina;</trd>
  <trd lng="en">uncle</trd>
  <trd lng="fr">oncle</trd>
  <trdgrp lng="hu">
    <trd>b&aacute;csi</trd>,
    <trd>nagyb&aacute;csi</trd>
  </trdgrp>
  <trd lng="it">zio</trd>
  <trd lng="nl">oom</trd>
  <trd lng="pt">tio</trd>
  <trd lng="ru">&c_d;&c_ja;&c_d;&c_ja;</trd>
  <trdgrp lng="sv">
    <trd>farbror</trd>,
    <trd>morbror</trd>
  </trdgrp>
  <trd lng="vo">n&ouml;k</trd>
</drv>

<drv mrk="onkl.0ino">
  <kap><tld/>ino</kap>
  <uzo tip="fak">GEN</uzo>
  <snc mrk="onkl.0ino.patrapatrina">
    <dif>
      Fratino de patro a&ubreve; patrino:
      <ekz>
        mia <tld/>ino Bosa veturigos min<fnt><aut>Spomenka &Scirc;timec</aut>:
        <vrk>Hejmo en Meze&ubreve;ropo</vrk>, <bib>Monato</bib><lok>jaro 1996a,
        numero 1a, p. 20a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="sv">
      <trd>faster</trd>,
      <trd>moster</trd>
    </trdgrp>
  </snc>
  <snc mrk="onkl.0ino.boonklino">
    <dif>
      Edzino de <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_jo;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_c;&c_jo;&c_c;&c_ja;</trd>
  </trdgrp>
  <trd lng="ca">tia</trd>
  <trd lng="de">Tante</trd>
  <trd lng="el">&theta;&epsilon;&jota_ton;&alfa;</trd>
  <trd lng="en">aunt</trd>
  <trd lng="fr">tante</trd>
  <trdgrp lng="hu">
    <trd>n&eacute;ni</trd>,
    <trd>nagyn&eacute;ni</trd>
  </trdgrp>
  <trd lng="it">zia</trd>
  <trd lng="nl">tante</trd>
  <trd lng="pt">tia</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_jo;&c_t;&c_ja;</trd>,
    <trd>&c_t;&c_jo;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="vo">jin&ouml;k</trd>
</drv>
</art>
