<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>onkl</rad>/o
</kap>
<drv mrk="onkl.0o">
<kap><ofc>*</ofc><tld/>o</kap>
<snc>
<dif>
Frato de patro a&ubreve; patrino. 
</dif>
</snc>
<trd lng="cs">str&yacute;c</trd>
<trd lng="en">uncle</trd>
<trd lng="nl">oom</trd>
<trdgrp lng="sv">
   <trd>farbror</trd>,
   <trd>morbror</trd>
</trdgrp>
</drv>
<drv mrk="onkl.0ino">
<kap><tld/>ino</kap>
<snc num="1">
<dif>
Fratino de patro a&ubreve; patrino. 
</dif>
<trdgrp lng="sv">
   <trd>faster</trd>,
   <trd>moster</trd>
</trdgrp>
</snc>
<snc num="2">
<dif>
Edzino de <tld/>o.  
</dif>
</snc>
<trd lng="nl">tante</trd>
</drv>
</art>
