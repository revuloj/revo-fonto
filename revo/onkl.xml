<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>onkl</rad>/o
</kap>
<drv mrk="onkl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Frato de patro a&ubreve; patrino.
    </dif>
  </snc>
  <trd lng="cs">str&yacute;c</trd>
  <trd lng="de">Onkel</trd>
  <trd lng="el">&theta;&epsilon;&jota_ton;&omikron;&sigma_fina;</trd>
  <trd lng="en">uncle</trd>
  <trd lng="fr">oncle</trd>
  <trd lng="it">zio</trd>
  <trd lng="nl">oom</trd>
  <trd lng="pt">tio</trd>
  <trd lng="ru">&c_d;&c_ja;&c_d;&c_ja;</trd>
  <trdgrp lng="sv">
    <trd>farbror</trd>,
    <trd>morbror</trd>
  </trdgrp>
</drv>

<drv mrk="onkl.0ino">
  <kap><tld/>ino</kap>
  <snc>
    <dif>
      Fratino de patro a&ubreve; patrino.
    </dif>
    <trdgrp lng="sv">
      <trd>faster</trd>,
      <trd>moster</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Edzino de <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Tante</trd>
  <trd lng="el">&theta;&epsilon;&jota_ton;&alfa;</trd>
  <trd lng="en">aunt</trd>
  <trd lng="fr">tante</trd>
  <trd lng="it">zia</trd>
  <trd lng="nl">tante</trd>
  <trd lng="pt">tia</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_jo;&c_t;&c_ja;</trd>,
    <trd>&c_t;&c_jo;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
