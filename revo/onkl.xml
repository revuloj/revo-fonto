<drv mrk="onkl.ge0oj">
  <kap>
    ge<tld/>oj
  </kap>
  <snc>
    <dif>
      <tld lit="O"/>o kaj <tld/>ino, a&ubreve; <tld/>oj kaj <tld/>inoj:
      <ekz>
        mi veturos hodia&ubreve; al miaj ge<tld/>oj
        <fnt>
          <bib>DL</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_ja;&c_d;&c_z;&c_mol;&c_k;&c_a; &c_ib; &c_c;&c_jo;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_d;&c_z;&c_ja;&c_d;&c_z;&c_mol;&c_k;&c_ib; &c_ib; &c_c;&c_jo;&c_t;&c_k;&c_ib;</trd>
  </trdgrp>

</drv>

