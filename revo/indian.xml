<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>indian</rad>/o
  <fnt>Z</fnt>
</kap>

<drv mrk="indian.0o">
  <kap><tld/>o</kap>
  <snc mrk="indian.0o.ETN">
    <dif>
      Ano de iu el la pralo&gcirc;aj popoloj de Ameriko anta&ubreve;aj
      al la e&ubreve;ropa enmigro kaj koloniigo en la 16a jarcento:
      <ekz>
        ni pasigis tre amuzan nokton inter <tld/>oj, <tld/>inoj,
        <tld/>idoj, kune kun hundoj kaj malsova&gcirc;aj papagoj
        <fnt><aut>Werner Schad</aut>, <vrk>Orinoko-delto: Indianoj,
        mar&ccirc;oj, arbarego</vrk>, <bib>Monato</bib>, <lok>jaro 
2000a, numero
        11a, p. 20a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_n;&c_d;&c_y;&c_ja;&c_n;&c_ib;&c_n;</trd>,
    <trd>&c_ib;&c_n;&c_d;&c_z;&c_je;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="bg">&c_i;&c_n;&c_d;&c_i;&c_a;&c_n;&c_je;&c_c;</trd>
  <trd lng="de">Indianer</trd>
  <trd lng="en">Indian</trd>
  <trd lng="es">indio</trd>
  <trdgrp lng="fr">
    <trd>Indien<klr tip="ind"> (d'Am&eacute;rique)</klr></trd>,
    <trd>Am&eacute;rindien</trd>
  </trdgrp>
  <trd lng="hu">indi&aacute;n</trd>
  <trd lng="nl">Indiaan</trd>
  <trdgrp lng="pt">
    <trd>&iacute;ndio</trd>,
    <trd>amer&iacute;ndio</trd>,
    <trd>ind&iacute;gena</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_n;&c_d;&c_je;&c_je;&c_c;</trd>
  <trd lng="sv">indian</trd>
</drv>

<drv mrk="indian.I0o">
  <kap><tld lit="I"/>o</kap>
  <uzo tip="fak">AST</uzo>
  <mlg>Ind</mlg>
  <snc>
    <dif>
      Sud-hemisfera <ref tip="super" cel="stel.0figuro">stelfiguro</ref>
      
(<trd lng="la">Indus</trd>).
    </dif>
  </snc>
  <trd lng="de">Inder</trd>
  <trd lng="en">Indus</trd>
  <trd lng="es">Indio</trd>
</drv>

<drv mrk="indian.0a">
  <kap><tld/>a</kap>
  <snc mrk="indian.0a.ETN">
    <dif>
      Rilata al <tld/>oj, al ties kulturoj:
      <ekz>
        kune kun la federaciaj <tld/>aj triboj, ili atakis kaj okupis la
        urbon Teno&ccirc;titlan
        <fnt><bib>SatirRak</bib>, <lok>la guarbo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_n;&c_d;&c_y;&c_ja;&c_n;&c_s;&c_k;&c_ib;</trd>,
    <trd>&c_ib;&c_n;&c_d;&c_z;&c_je;&c_j;&c_s;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="en">Indian</trd>
  <trdgrp lng="fr">
    <trd>indien<klr tip="ind"> (d'Am&eacute;rique, adj.)</klr></trd>,
    <trd>am&eacute;rindien<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="de">indianisch</trd>
  <trd lng="hu">indi&aacute;n</trd>
  <trd lng="ru">&c_i;&c_n;&c_d;&c_je;&c_j;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
