<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: indian.xml,v 1.32 2023/11/20 06:31:21 revo Exp $">
<kap>
  <rad>indian</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>

<drv mrk="indian.0o">
  <kap><tld/>o</kap>
  <snc mrk="indian.0o.ETN">
    <dif>
      Iu el la <ref tip="super" cel="logx.pra0anto">pralo&gcirc;anto</ref>j de
      <ref tip="malprt" cel="amerik.A0o">Ameriko</ref> anta&ubreve;aj
      al la e&ubreve;ropa enmigro kaj koloniigo en la 16a jarcento:
      <ekz>
        ni pasigis tre amuzan nokton inter <tld/>oj, <tld/>inoj,
        <tld/>idoj, kune kun hundoj kaj malsova&gcirc;aj papagoj
        <fnt>
          <aut>Werner Schad</aut>,
          <vrk>Orinoko-delto: Indianoj, mar&ccirc;oj, arbarego</vrk>,
          <bib>Monato</bib>, <lok>jaro 2000a, numero 11a, p. 20a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kultura genocido kontra&ubreve; la amerikaj <tld/>oj
        <fnt>
	<aut>Gerrit Berveling</aut>
	<vrk><url ref="&Monato;/2000/006823.html">Tro senkosta pentemo</url></vrk>
	<bib>Monato</bib>
	<lok>2000/08, p. 22</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi rimarkis, ke <tld/>devenaj homoj kaj blankuloj preferas la pli sekurajn
        hamakojn, en kiuj serpentoj kaj skorpioj preska&ubreve; ne povas atingi ilin
        <fnt>
       <aut>Werner Schad</aut>
          <vrk><url ref="&Monato;/2000/006868.php">Inter maro kaj arbarego</url></vrk>
          <bib>Monato</bib>
          <lok>2000/05, p. 14</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="indigx.0o">indi&gcirc;eno</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_n;&c_d;&c_y;&c_ja;&c_n;&c_ib;&c_n;</trd>,
    <trd>&c_ib;&c_n;&c_d;&c_z;&c_je;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="bg">&c_i;&c_n;&c_d;&c_i;&c_a;&c_n;&c_je;&c_c;</trd>
  <trd lng="cs">Indi&aacute;n</trd>
  <trd lng="de">Indianer</trd>
  <trd lng="en">Indian</trd>
  <trd lng="es">indio</trd>
  <trdgrp lng="fr">
    <trd>Indien<klr tip="ind"> (d'Am&eacute;rique)</klr></trd>,
    <trd>Am&eacute;rindien</trd>
  </trdgrp>
  <trd lng="hu">indi&aacute;n</trd>
  <trd lng="nl">Indiaan</trd>
  <trd lng="pl">indianin</trd>
  <trdgrp lng="pt">
    <trd>&iacute;ndio</trd>,
    <trd>amer&iacute;ndio</trd>,
    <trd>ind&iacute;gena</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>amerindieni</trd>,
    <trd>indian</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_n;&c_d;&c_je;&c_je;&c_c;</trd>
  <trd lng="sk">Indi&aacute;n</trd>
  <trd lng="sv">indian</trd>
  <trd lng="zh">&#21360;&#31532;&#23433;&#22899;&#20154; <pr>y&igrave;nd&igrave;&#257;nn&#474;r&eacute;n</pr></trd>
</drv>

<drv mrk="indian.I0o">
  <kap><tld lit="I"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>,
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
  </kap>
  <uzo tip="fak">AST</uzo>
  <mlg>Ind</mlg>
  <snc mrk="indian.I0o.AST">
    <dif>
      Sud-hemisfera <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref>
      (<trd lng="la">Indus</trd>):
      <ekz>
        <nom>NGC&#8239;7123</nom> estas galaksio en konstelacio <tld lit="I"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=120494"
              >NGC 7123</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="indus.0o">Induso</ref>,
       <ref cel="hind.0o">hindo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_Ib;&c_n;&c_d;&c_y;&c_ja;&c_n;&c_ib;&c_n; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#8217;&c_je;)</klr></trd>
  <trd lng="cs">Indi&aacute;n</trd>
  <trdgrp lng="de">
    <trd>Indianer</trd>,
    <trd>Inder</trd>
  </trdgrp>
  <trd lng="en">Indus</trd>
  <trd lng="es">Indio</trd>
  <trd lng="fr">Indien<klr tip="ind"> (constellation)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#12450;&#12513;&#12522;&#12459;&#12452;&#12531;&#12487;&#12451;&#12450;&#12531;</trd>,
    <trd>&#12452;&#12531;&#12487;&#12451;&#12458;</trd>
  </trdgrp>
  <trd lng="pl">Indianin  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Indianul  <klr>(constela&tcomma;ie)</klr></trd>
  <trd lng="sk">Indi&aacute;n</trd>
  <trd lng="zh">&#21360;&#31532;&#23433;&#20154; <pr>y&igrave;nd&igrave;&#257;nr&eacute;n</pr></trd>
</drv>

<drv mrk="indian.0a">
  <kap><tld/>a</kap>
  <snc mrk="indian.0a.ETN">
    <dif>
      Rilata al <tld/>oj, al ties kulturoj:
      <ekz>
        <tld/>aj lingvoj
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2007/009226.php">Nanoq la unua arkta muzeo</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj man&gcirc;a&jcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2008/009386.php">La kukurbo frukto multviza&gcirc;a</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>aj popoloj unue enmigris anta&ubreve; 30&#8239;000&#8239;-&#8239;35&#8239;000 jaroj
        <fnt>
          <bib>Monato</bib>
          <aut>Werner Fu&szlig;</aut>
          <vrk><url ref="&Monato;/2003/007969.html">&Ccirc;u la homaro lernis pensi per la
          lingvo?</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        en 1980 la usona registaro donacis monon kaj teron en Norda kaj Suda Dakoto al
        <tld/>aj triboj, kiuj centon da jaroj anta&ubreve;e perdis sian tradician terenon
        <fnt>
          <bib>Monato</bib>
          <aut>Elson B. Snow</aut>
          <vrk><url ref="&Monato;/2010/009960.php">Narkotaj Dakotoj</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        kune kun la federaciaj <tld/>aj triboj, ili atakis kaj okupis la
        urbon <nom>Teno&ccirc;titlan</nom>
        <fnt><bib>SatirRak</bib>, <lok>la guarbo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_n;&c_d;&c_y;&c_ja;&c_n;&c_s;&c_k;&c_ib;</trd>,
    <trd>&c_ib;&c_n;&c_d;&c_z;&c_je;&c_j;&c_s;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="en">Indian</trd>
  <trdgrp lng="fr">
    <trd>indien<klr tip="ind"> (d'Am&eacute;rique, adj.)</klr></trd>,
    <trd>am&eacute;rindien<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="de">indianisch</trd>
  <trd lng="hu">indi&aacute;n</trd>
  <trdgrp lng="ja">
    <trd>&#12450;&#12513;&#12522;&#12459;&#12452;&#12531;&#12487;&#12451;&#12450;&#12531;&#12398;</trd>,
    <trd>&#12452;&#12531;&#12487;&#12451;&#12458;&#12398;</trd>
  </trdgrp>
  <trd lng="pl">india&nacute;ski</trd>
  <trdgrp lng="ro">
    <trd>amerindian</trd>,
    <trd>indian</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_n;&c_d;&c_je;&c_j;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
