<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kolokazi.xml,v 1.9 2023/12/03 09:58:52 revo Exp $">
<kap>
  <rad>kolokazi</rad>/o <fnt><bib>PIV2</bib></fnt>,
  <var><kap><rad var="s">kolokasi</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>
<drv mrk="kolokazi.0o">
  <kap><tld/>o, <var><kap><tld var="s"/>o</kap></var></kap>
  <snc mrk="kolokazi.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio arumacoj el varmaj landoj (<trd lng="la">Colocasia</trd>),
      de rizomaj vegeta&jcirc;oj kun man&gcirc;eblaj radiktuberoj kaj folioj:
      <ekz>
        satoimo estas <tld/>o de japana speco
        <fnt>
          <aut>iriza</aut>,
          <vrk><url ref="http://iriza.blog.so-net.ne.jp/2007-05-22">
          Pri Satoimo</url></vrk>,
          <lok>Mi volas vidi irizon !!, 2007-05-22</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili povus travivi per <tld var="s"/>oj nutrori&ccirc;aj
        <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>&leftquot;tragedio de man&gcirc;o en
        la insulo Mindanao&rightquot;, p. 40a</lok></fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/7/70/TaroAKL.jpg/320px-TaroAKL.jpg" prm="PD"><tld/>o (Colocasia
    esculenta), A&ubreve;klando, Nov-Zelando
        <fnt>
          <aut>Kahuroa, propra verko</aut>
          <lok><url ref="&WCW;/File:TaroAKL.jpg">&WCW;/File:TaroAKL.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_r;&c_a; <klr tip="amb">(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trd lng="de">Colocasia</trd>
  <trdgrp lng="fr">
    <trd>chou de chine</trd>,
    <trd>colocase</trd>
  </trdgrp>
  <trd lng="hu">kolok&aacute;zia</trd>
  <trd lng="ja">&#x30B5;&#x30C8;&#x30A4;&#x30E2;</trd>
  <trd lng="nl">colocasia</trd>
  <trd lng="pl">kolokazja</trd>
  <trdgrp lng="pt">
    <trd>coloc&aacute;sia</trd>,
    <trd>inhame</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_a;&c_r;&#x43E;</trd>,
    <trd>&#x43A;&#x43E;&#x43B;&#x43E;&#x43A;&c_a;&c_z;&c_ib;&#x44F;</trd>
  </trdgrp>
  <trd lng="zh">&#x91CE;&#x828B; <pr>y&#x11B;y&#xF9;</pr></trd>
</drv>
</art>

