<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kolokazi</rad>/o <fnt><bib>PIV2</bib></fnt>,
  <var><kap><rad>kolokasi</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>
<drv mrk="kolokazi.0o">
  <kap><tld/>o, <var><kap>kolokasio</kap></var></kap>
  <snc mrk="kolokazi.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro de rizomaj vegeta&jcirc;oj kun man&gcirc;eblaj radiktuberoj kaj
      folioj, el varmaj landoj
      (<trd lng="la">Colocasia</trd>):
      <ekz>
        satoimo estas <tld/>o de japana speco
        <fnt>
          <aut>iriza</aut>,
          <vrk><url ref="http://iriza.blog.so-net.ne.jp/2007-05-22">
          Pri Satoimo</url></vrk>,
          <lok>Mi volas vidi irizon !!, 2007-05-22</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili povus travivi per kolokasioj nutrori&ccirc;aj
        <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>&leftquot;tragedio de man&gcirc;o en
        la insulo Mindanao&rightquot;, p. 40a</lok></fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/7/70/TaroAKL.jpg/320px-TaroAKL.jpg"
      prm="PD"
      ><tld/>o (Colocasia esculenta), A&ubreve;klando, Nov-Zelando
        <fnt>
          <aut>Kahuroa, propra verko</aut>
          <lok><url ref="&WCW;/File:TaroAKL.jpg"
              >https://commons.wikimedia.org/wiki/File:TaroAKL.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="de">Colocasia</trd>
  <trdgrp lng="fr">
    <trd>chou de chine</trd>,
    <trd>colocase</trd>
  </trdgrp>
  <trd lng="hu">kolok&aacute;zia</trd>
  <trd lng="nl">colocasia</trd>
  <trd lng="pl">kolokazja</trd>
  <trdgrp lng="pt">
    <trd>coloc&aacute;sia</trd>,
    <trd>inhame</trd>
  </trdgrp>
</drv>
</art>

