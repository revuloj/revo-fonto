<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kosxmar.xml,v 1.19 2023/12/03 09:58:59 revo Exp $">
<kap>
   <rad>ko&scirc;mar</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kosxmar.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="inkub.0o.songxo">Inkubo <sncref/></ref>:
      <ekz>
        nokte Neringa veki&gcirc;is pro <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2005/008559.php">Pri la
          aferoj misteraj</url></vrk>
        </fnt>;
      </ekz><ekz>
         la koro batas maltrankvile kvaza&ubreve; en <tld/>o
         <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 78a, p. 657a</lok></fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        la perspektivo de unukultura mondo estas <tld/>o, ne utopio
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj'</aut>
          <vrk><url ref="&Monato;/2013/011022.php">Kien la
          franca?</url></vrk>
        </fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        por ekspluati skistogason la&ubreve; la nuna teknologio, necesas
        koktelo 
        de pli ol 500 kemia&jcirc;oj kaj miloj da litroj da akvo
        <klr>[&#x2026;]</klr> tiu <tld/>o jam trafas Usonon
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="&Monato;/2011/010487.php">Skistogasa
          ko&scirc;maro!</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_sh;&c_m;&c_a;&c_r;</trd>
  <trd lng="bg">&c_k;&c_o;&c_sh;&c_m;&c_a;&c_r;</trd>
  <trd lng="ca">malson</trd>
  <trd lng="de">Albtraum</trd>
  <trd lng="fa">&f_k;&a_A;&a_b;&a_w;&a_s;</trd>
  <trd lng="en">nightmare</trd>
  <trd lng="es">pesadilla</trd>
  <trd lng="fr">cauchemar</trd>
  <trdgrp lng="hu">
     <trd>r&eacute;m&aacute;lom</trd>,   
     <trd>lid&eacute;rces &aacute;lom</trd>
  </trdgrp>
  <trd lng="nl">nachtmerrie</trd>
  <trd lng="pl">koszmar</trd>
  <trd lng="pt">pesadelo</trd>
  <trd lng="ro">co&#x219;mar</trd>
  <trd lng="ru">&c_k;&c_o;&c_sh;&c_m;&c_a;&c_r;</trd>
  <trd lng="sv">mardr&ouml;m</trd>
  <trdgrp lng="zh">
    <trd>&#x6076;&#x68A6; <pr>&#xE8;m&#xE8;ng</pr></trd>,
    <trd>&#x5922;&#x9B58; <pr>m&#xE8;ngy&#x1CE;n</pr></trd>,
    <trd>&#x9B47; <pr>y&#x1CE;n</pr></trd>,
    <trd>&#x5669;&#x5922; <pr>&#xE8;m&#xE8;ng</pr></trd>,
    <trd>&#x5BCE; <pr>b&#xEC;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kosxmar.0a">
  <kap><tld/>a</kap>
  <snc mrk="kosxmar.0a.rilata">
    <dif>
      Rilata al <tld/>o.
    </dif>
  </snc>
  <snc mrk="kosxmar.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
     <ref tip="sin" cel="terur.0a.timiga">Terura</ref>;
     malbonega, a&ccirc;ega, abomena:
     <ekz>
       la romano priskribas malutopion, <tld/>an mondon, en kiu la
       &scirc;tato konstante spionas la mizeran lo&gcirc;antaron
       <fnt>
         <aut>Edmund GRIMLEY EVANS</aut>:
         <vrk>Legenda klasika&jcirc;o pri malutopio</vrk>. <bib>Monato</bib>
       </fnt>.
     </ekz>
    </dif>
    <trd lng="de">albtraumhaft</trd>
    <trd lng="en">nightmarish</trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_sh;&c_m;&c_a;&c_r;&c_n;&c_y;</trd>
  <trd lng="ru">&c_k;&c_o;&c_sh;&c_m;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="kosxmar.0i">
  <kap><tld/>i</kap>
  <snc>
    <dif>
      Son&gcirc;i <tld/>on:
     <ekz>
       Vi &gcirc;uos eternan vivon, <klr>(...)</klr> kies mankon vi
       <tld/>is dum la lastaj tagoj, monatoj kaj e&ccirc; jaroj
       <fnt>
         <aut>Jorge Camacho</aut>:
         <vrk>La Majstro kaj Martinelli</vrk>.
         <lok>Budape&scirc;to, 1993. &Ccirc;ap. 3.</lok>
       </fnt>.
     </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_ch;&c_y;&c_c;&c_mol; <ind>&c_k;&c_a;&c_sh;&c_m;&c_a;&c_r;&c_n;&c_y;</ind> &c_s;&c_o;&c_n;</trd>
    <trdgrp lng="de">
      <trd>einen Alptraum haben</trd>,
      <trd>alptr&auml;umenen</trd>
    </trdgrp>
  <trd lng="ru">&c_v;&c_i;&c_d;&c_je;&c_t;&c_mol; &c_k;&c_o;&c_sh;&c_m;&c_a;&c_r;&c_n;&c_y;&c_j; &c_s;&c_o;&c_n;</trd>
</drv>

</art>

