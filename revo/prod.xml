<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: prod.xml,v 1.11 2024/02/14 09:21:28 revo Exp $">
<kap>
   <rad>prod</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="prod.0o">
   <kap><tld/>o</kap>
   <uzo tip="stl">POE</uzo>
   <uzo tip="stl">RAR</uzo>
   <snc>
     <dif>
       <klr>(pri <ref tip="super" cel="kavali.0o.HIS">kavaliro</ref>j)</klr>
       Batalsperta <ref tip="super" cel="brav.0ulo">bravulo</ref> plenuminta
       noblajn, dan&gcirc;erajn agojn:
       <ekz>
         ekzistis inter ili tiaj <tld/>oj, ke ili savis, ne nur unu
         regnon,
         sed multajn
         <fnt><bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 6a, p.
         426a</lok></fnt>
       </ekz>
    </dif>
    <ref tip="sin" cel="hero.0o.kuragxulo">heroo<sncref/></ref>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_r;&c_o;&c_j;</trd>
  <trd lng="cs">jezdeck&yacute; bojovn&iacute;k vynikaj&iacute;c&iacute; odvahou</trd>
  <trdgrp lng="de">
    <trd>Recke</trd>,
    <trd>Krieger</trd>
  </trdgrp>
  <trd lng="en">warrior</trd>
  <trd lng="fr">preux<klr> (subst.)</klr></trd>
  <trdgrp lng="hu">
     <trd>h&odblac;s lovag</trd>,
     <trd>vit&eacute;z</trd>
   </trdgrp>
  <trd lng="nl">koene <ind>strijder</ind></trd>
  <trd lng="pl">odwa&zdot;ny  <ind>rycerz</ind></trd>
  <trd lng="pt">bravo</trd>
  <trd lng="ro">curajos  <ind>cavaler</ind></trd>
  <trdgrp lng="ru">
     <trd>&c_b;&c_o;&c_g;&c_a;&c_t;&c_y;&c_r;&c_mol;</trd>,
     <trd>&c_v;&c_i;&c_t;&c_ja;&c_z;&c_mol;</trd>,
     <trd>&c_h;&c_r;&c_a;&c_b;&c_r;&c_je;&c_c;</trd>,
     <trd>&c_g;&c_je;&c_r;&c_o;&c_j;</trd>
   </trdgrp>
  <trd lng="sk">rytier zn&aacute;my svojou odvahou a dobrodru&zcaron;stvami</trd>
  <trd lng="uk">&c_b;&c_o;&c_g;&c_a;&c_t;&c_i;&c_r;</trd>
</drv>

<drv mrk="prod.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="stl">POE</uzo>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      La&ubreve;dinda <ref tip="super" cel="hero.0ajxo">heroa&jcirc;o</ref>
      <klr>(precipe en batalo)</klr>:
      <ekz>
         la historio dol&ccirc;a kaj &ccirc;arma de liaj am-aferoj kaj
         <tld/>a&jcirc;oj
         <fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 13a, p.
         92a</lok></fnt>;
      </ekz>
      <ekz>
        tio ja &scirc;ajnas e&ccirc; pli forta ol la <tld/>a&jcirc;oj pri kiuj rakontis Ivano
        <fnt>
          <bib>MkM</bib>
          <lok>24</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la Esperanto-movado luktas por idealo nobla kaj atinginda, <klr>[&#8230;]</klr> &gcirc;i jam
        faris heroa&jcirc;ojn kaj <tld/>a&jcirc;ojn dum tiu lukto, eble &gcirc;i nun degeneras
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470;4</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
   <trdgrp lng="be">
     <trd>&c_p;&c_o;&c_d;&c_z;&c_mol;&c_v;&c_ib;&c_g;</trd>,
     <trd>&c_g;&c_je;&c_r;&c_a;&c_ib;&c_ch;&c_n;&c_y; &c_w;&c_ch;&c_y;&c_n;&c_a;&c_k;</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>Heldentat</trd>,
     <trd>Ruhmestat</trd>
   </trdgrp>
   <trd lng="fr">prouesse</trd>
   <trd lng="nl">koenheid</trd>
   <trdgrp lng="hu">
     <trd>h&odblac;stett</trd>,
     <trd>vit&eacute;zi tett</trd>
   </trdgrp>
  <trd lng="pl">czyn  <ind>rycerski</ind></trd>
  <trd lng="ro">fapt&abreve;  <ind>cavalereasc&abreve;</ind></trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_v;&c_i;&c_g;</trd>
  <trd lng="uk">&c_b;&c_o;&c_g;&c_a;&c_t;&c_i;&c_r;&c_s;&c_mol;&c_k;&c_i;&c_j; &c_p;&c_o;&c_d;&c_v;&c_i;&c_g;</trd>
</drv>
</art>

