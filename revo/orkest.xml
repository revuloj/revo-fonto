<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc> <rad>orkestr</rad>/o
</kap>
<drv mrk="orkest.0o">
  <kap><tld/>o</kap>
  <snc mrk="orkest.0o.MUZ_grupo">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Granda grupo de muzikistoj, ludantaj diversajn instrumentojn
      ordinare sub gvido de unu estro:
      <ekz>
        nia <tld/>o ludis valson el Fa&ubreve;sto
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="hu">zenekar</trd>
  </snc>
  <snc mrk="orkest.0o.MUZ_loko">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Parto de moderna teatrejo, kie sidas la muzikistoj akompanantaj la
      aktorojn.
    </dif>
    <trd lng="be">&c_a;&c_r;&c_k;&c_je;&c_s;&c_t;&c_r;&c_a;&c_v;&c_a;&c_ja; &c_ja;&c_m;&c_a;</trd>
    <trd lng="hu">zenekari &aacute;rok</trd>
    <trd lng="ru">&c_o;&c_r;&c_k;&c_je;&c_s;&c_t;&c_r;&c_o;&c_v;&c_a;&c_ja; &c_ja;&c_m;&c_a;</trd>
  </snc>
  <snc mrk="orkest.0o.TEA_moderna">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Parto de teatrejo inter la muzikista <tld/>o kaj la partero,
      enhavanta
      lokojn por a&ubreve;skultantoj a&ubreve; rigardantoj.
    </dif>
    <trd lng="hu">zenekari &uuml;l&eacute;s</trd>
  </snc>
  <snc mrk="orkest.0o.TEA_antikva">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Parto de la greka antikva teatrejo, kie staris la &hcirc;oro.
    </dif>
    <trdgrp lng="hu">
      <trd>k&oacute;rus helye</trd>,
      <trd>orkesztra</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_k;&c_je;&c_s;&c_t;&c_a;&c_r;</trd>
  <trd lng="de">Orchester</trd>
  <trd lng="el">&omikron;&rho;&chi;&eta_ton;&sigma;&tau;&rho;&alfa;</trd>
  <trd lng="fr">orchestre</trd>
  <trd lng="nl">orkest</trd>
  <trd lng="pt">orquestra</trd>
  <trd lng="ru">&c_o;&c_r;&c_k;&c_je;&c_s;&c_t;&c_r;</trd>
</drv>
</art>
