<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tof.xml,v 1.23 2024/02/01 01:27:00 revo Exp $">
<kap>
  <rad>tof</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tof.0o">
  <kap><tld/>o</kap>
  <snc mrk="tof.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      &Scirc;toneca tavolo ne penetrebla de la vegeta&jcirc;aj radikoj,
      kiu etendi&gcirc;as en kelkaj grundoj sub la tavolo de humo:
      <ekz>
        <tld/>a grifelo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la kirko estas masonita en kombinado de &ccirc;izitaj blokoj
        el <tld/>o kaj brikoj
        <fnt>
          <aut>V. Galeski</aut>,
          <vrk><url
          ref="http://www.aliaflanko.de/urbo/prilep/Kulturhistoriaj_monumentoj_de_Prilep-nova.pdf">
          Kulturhistoriaj monumentoj en Prilep</url></vrk>,
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_f;</trd>
  <trdgrp lng="ca">
    <trd>tuf</trd>,
    <trd>cinerita</trd>
  </trdgrp>
  <trd lng="cs">tuf</trd>
  <trd lng="de">Tuff</trd>
  <trd lng="fr">tuf</trd>
  <trd lng="hu">tufa</trd>
  <trdgrp lng="ja">
    <trd>&#x51DD;&#28784;&#x5CA9;  <pr>&#x304E;&#12423;&#12358;&#x304B;&#12356;&#x304C;&#12435;</pr></trd>,
    <trd>&#x75DB;&#x98A8;&#x7D50;&#x7BC0;  <pr>&#12388;&#12358;&#12405;&#12358;&#12369;&#12388;&#x305B;&#12388;</pr></trd>
  </trdgrp>
  <trd lng="nl">tufsteen</trd>
  <trdgrp lng="pl">
    <trd>tuf</trd>,
    <trd>martwica<klr> (geol.)</klr></trd>
  </trdgrp>
  <trd lng="pt">tufa</trd>
  <trd lng="ru">&c_t;&c_u;&c_f;</trd>
  <trd lng="sk">tuf</trd>
  <trd lng="uk">&c_t;&c_u;&c_f;</trd>
</drv>

<drv mrk="tof.kalko0o">
<kap>kalko<tld/>o, <var><kap>kalk<tld/>o</kap></var></kap>
  <snc mrk="tof.kalko0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      <ref tip="super" cel="tof.0o.MIN"><tld lit="T"/>o</ref>
      konsistanta precipe el kalko:
      <ekz>
        ni vidas tre belajn resta&jcirc;ojn de kalko<tld/>o, kies materion
        parte forigis la akvoj
        <fnt>
          <aut>L. L&eacute;n&aacute;rt</aut>,
          <vrk>La Grotbanejo de Miskolc-Tapolca</vrk>,
          <lok><url ref="http://epa.oszk.hu/01000/01049/00028/pdf/00028.pdf">
          Nordhungaria Informo, 2007-12</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
    <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_a;&c_v;&c_e;&c_r;&c_t;&c_y;&c_n;</trd>,
    <trd>&c_v;&c_a;&c_p;&c_n;&c_ja;&c_k;&c_o;&c_v;&c_y; <ind>&c_t;&c_u;&c_f;</ind></trd>
  </trdgrp>
  <trd lng="ca">travert&iacute;</trd>
  <trdgrp lng="de">
    <trd>Kalktuff</trd>,
    <trd>S&uuml;&szlig;wasserkalk</trd>,
    <trd>Quellkalk</trd>,
    <trd>Travertin</trd>
  </trdgrp>
  <trd lng="en">Travertine</trd>
  <trd lng="fr">travertine</trd>
  <trd lng="hu">m&eacute;sztufa</trd>
  <trd lng="pl">trawertyn</trd>
</drv>
</art>
