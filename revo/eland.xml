<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: eland.xml,v 1.11 2024/08/16 09:30:28 revo Exp $">
<kap>
  <rad>eland</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="eland.0o">
  <kap><tld/>o</kap>
  <snc mrk="eland.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda rekta<ref tip="prt" cel="korn.0o.besta">korna</ref>
      kvarpiedulo vivanta en Afrika savano;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref>
      el familio <ref tip="malprt" cel="boved.0oj.ZOO">bovedoj</ref>
      (<trd lng="la">Taurotragus</trd>):
      <ekz>
        la plej majesta <klr>[besto]</klr> iom similis
        al la giganta <tld/>o de Afriko
        <fnt>
         <aut>E. Rice Burroughs, trad. G. Mickle</aut>,
         <vrk><url ref="http://home.arcor.de/gmickle/koro_14.html">
         &Ccirc;e la koro de la Tero</url></vrk>,
         <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pa&scirc;tejoj kaj altaj arbaroj lo&gcirc;igas multajn
        birdojn kaj
        diversajn bestojn, interalie kelkajn blankajn rinocerojn,
        elefantojn, <klr>...</klr> <tld/>ojn
        <fnt>
          <bib>Viki</bib>,
          <lok><url
          ref="&Viki;/Nacia_Parko_Mosi-oa-Tunya">
          Nacia Parko Mosi-oa-Tunya</url>, 2011-11-26</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="tauxrotrag.0o.ZOO">ta&ubreve;rotrago</ref>
    </refgrp>
    <bld
      lok="&WCU;/thumb/e/ec/Taurotragus_oryx.jpg/640px-Taurotragus_oryx.jpg"
      prm="CC BY-SA 4.0"
      ><tld/>oj &ccirc;e akvejo, Nacia Parko Eto&scirc;a, Namibio
        <fnt>
          <aut>Hans Hillewaert, propra verko</aut>
          <lok><url ref="&WCW;/File:Taurotragus_oryx.jpg"
              >&WCW;/File:Taurotragus_oryx.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_a; <klr tip="amb">(&c_a;&c_n;&c_t;&c_y;&c_l;&c_jo;&c_p;&c_a;)</klr></trd>
  <trd lng="cs">antilopa los&iacute;</trd>
  <trd lng="de">Elenantilope</trd>
  <trdgrp lng="en">
    <trd>eland</trd>,
    <trd>common eland</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>taurotragus</trd>,
    <trd>&eacute;land</trd>
  </trdgrp>
  <trd lng="nl">elandantilope</trd>
  <trd lng="pl">eland</trd>
  <trd lng="sk">antilopa losia</trd>
</drv>
</art>

