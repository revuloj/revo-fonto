<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: monokl.xml,v 1.17 2023/09/02 06:04:13 revo Exp $">
<kap>
  <rad>monokl</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="monokl.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Vidvitro por unu okulo:
      <ekz>
        li ekhaltis en la pordo, surmetis <tld/>on sur la okulon,
        rigardis &ccirc;irka&ubreve;e
        <fnt><bib>RabistFab</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_o;&c_k;&c_l;&c_mol;</trd>
  <trd lng="bg">&c_m;&c_o;&c_n;&c_o;&c_k;&c_malmol;&c_l;</trd>
  <trd lng="cs">monokl</trd>
  <trd lng="de">Monokel</trd>
  <trd lng="en">monocle</trd>
  <trd lng="fr">monocle</trd>
  <trd lng="hu">monokli</trd>
  <trdgrp lng="ja">
    <trd>&#x30E2;&#x30CE;&#x30AF;&#x30EB;</trd>,
    <trd>&#x7247;&#x3081;&#x304C;&#x306D;  [<ind>&#x304B;&#x305F;&#x3081;&#x304C;&#x306D;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">monocle</trd>
  <trd lng="pl">monokl</trd>
  <trd lng="pt">mon&oacute;ide</trd>
  <trd lng="ro">monoclu</trd>
  <trd lng="ru">&c_m;&c_o;&c_n;&c_o;&c_k;&c_l;&c_mol;</trd>
  <trd lng="sk">monokel</trd>
  <trd lng="uk">&#x43C;&#x43E;&#x43D;&#x43E;&#x43A;&#x43B;&#x44C;</trd>
</drv>
</art>
