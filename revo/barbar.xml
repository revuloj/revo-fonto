<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>barbar</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="barbar.0o">
  <kap><tld/>o</kap>
  <snc mrk="barbar.0o.MOR">
    <dif>
      Necivilizita homo:
      <ekz>
        <tld/>oj invadis E&ubreve;ropon dum la 5a jarcento;
      </ekz>
      <ekz>
        kiam en la centra E&ubreve;ropo la <tld/>oj vestitaj per
        maldelikataj
        feloj sin ka&scirc;is en kavernoj, Egipto jam posedis altan
        socian organizon
        <fnt><bib>Far1</bib>, <lok>anta&ubreve;parolo</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Wilder</trd>,
      <trd>Barbar</trd>
    </trdgrp>
    <trd lng="pl">barbarzy&nacute;ca</trd>
  </snc>
  <snc mrk="barbar.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kruelulo, senkompatulo.
    </dif>
    <trdgrp lng="de">
      <trd>Rohling</trd>,
      <trd>W&uuml;stling</trd>,
      <trd>grausamer Mensch</trd>
    </trdgrp>
    <trd lng="pl">barbarzy&nacute;ca</trd>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_r;&c_b;&c_a;&c_r;</trd>
  <trd lng="ca">b&agrave;rbar<klr> (subst.)</klr></trd>
  <trdgrp lng="cs">
    <trd>barbar</trd>,
    <trd>primitiv</trd>
  </trdgrp>
  <trd lng="en">barbarian</trd>
  <trd lng="es">b&aacute;rbaro</trd>
  <trd lng="fr">barbare<klr> (subst.)</klr></trd>
  <trd lng="hu">barb&aacute;r</trd>
  <trd lng="nl">barbaar</trd>
  <trd lng="ru">&c_v;&c_a;&c_r;&c_v;&c_a;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>barbar</trd>,
    <trd>nevzdelanec</trd>,
    <trd>surovec</trd>
  </trdgrp>
  <trd lng="sv">barbar</trd>
</drv>

<drv mrk="barbar.0a">
  <kap><tld/>a</kap>
  <snc mrk="barbar.0a.MOR">
    <dif>
      Necivilizita, kruda, maldelikata, kruela:
      <ekz>
        <tld/>a lingvo, konduto, ago, kutimoj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="human">malhumana</ref>,
      <ref cel="sovagx.0a">sova&gcirc;a</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_r;&c_b;&c_a;&c_r;&c_s;&c_k;&c_ib;</trd>
  <trd lng="ca">b&agrave;rbar<klr> (adj.)</klr></trd>
  <trdgrp lng="cs">
    <trd>barbarsk&yacute;</trd>,
    <trd>surov&yacute; <klr>(surovec)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wild</trd>,
    <trd>roh</trd>,
    <trd>barbarisch</trd>
  </trdgrp>
  <trd lng="en">barbaric</trd>
  <trd lng="es">b&aacute;rbaro</trd>
  <trdgrp lng="fa">
    <trd>&a_w;&a_H;&a_sh;&f_y;</trd>,
    <trd>&a_b;&f_y;&zwnj;&a_f;&a_r;&a_h;&a_n;&f_g;</trd>
  </trdgrp>
  <trd lng="fr">barbare<klr> (adj.)</klr></trd>
  <trd lng="hu">barb&aacute;r</trd>
  <trd lng="nl">barbaars</trd>
  <trd lng="pl">barbarzy&nacute;ski</trd>
  <trd lng="ru">&c_v;&c_a;&c_r;&c_v;&c_a;&c_r;&c_s;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>barbarsk&yacute;</trd>,
    <trd>surov&yacute;</trd>
  </trdgrp>
  <trd lng="sv">barbarisk</trd>
</drv>

<drv mrk="barbar.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="barbar.0ajxo.MOR">
    <dif>
      <tld lit="B"/>a ago.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_r;&c_b;&c_a;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>
  <trd lng="ca">barbaritat</trd>
  <trd lng="en">barbarity</trd>
  <trdgrp lng="de">
    <trd>Barbarei</trd>,
    <trd>Rohheit</trd>,
    <trd>grausame Taten</trd>
  </trdgrp>
  <trd lng="es">barbaridad</trd>
  <trd lng="fr">acte de <ind>barbarie</ind></trd>
  <trd lng="hu">barb&aacute;r cselekedet</trd>
  <trd lng="nl">iets <ind>barbaars</ind></trd>
  <trd lng="pl">barbarzy&nacute;stwo</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_a;&c_r;&c_v;&c_a;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_v;&c_a;&c_r;&c_v;&c_a;&c_r;&c_s;&c_k;&c_i;&c_j;
    &c_p;&c_o;&c_s;&c_t;&c_u;&c_p;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">barbarstvo</trd>
</drv>

<drv mrk="barbar.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="barbar.0eco.MOR">
    <dif>
      Eco de iu a&ubreve; io <tld/>a.
    </dif>
  </snc>
  <trd lng="ca">barb&agrave;rie</trd>
  <trd lng="cs">barbarstv&iacute; <klr>(vlastnost)</klr></trd>
  <trdgrp lng="de">
    <trd>Wildheit</trd>,
    <trd>Rohheit</trd>,
    <trd>Barbarentum</trd>
  </trdgrp>
  <trd lng="en">barbarism</trd>
  <trd lng="es">barbaridad</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_r;&a_b;&a_r;&f_y;&a_t;</trd>,
    <trd>&a_w;&a_H;&a_sh;&f_y;&zwnj;&f_g;&a_r;&f_y;</trd>,
    <trd>&a_t;&a_w;&a_H;&a_sh;</trd>
    <trd>&a_j;&a_A;&a_h;&a_l;&f_y;&a_t;</trd>
  </trdgrp>
  <trd lng="fr">barbarie</trd>
  <trd lng="hu">barb&aacute;rs&aacute;g</trd>
  <trd lng="nl">barbaarsheid</trd>
  <trd lng="pl">barbarzy&nacute;stwo</trd>
  <trd lng="ru">&c_v;&c_a;&c_r;&c_v;&c_a;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>barbarskos&tcaron;</trd>,
    <trd>barbarstvo  <klr>(vlastnos&tcaron;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="barbar.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="barbar.0ismo.MOR">
    <dif>
      <tld lit="B"/>a sinteno, agado:
      <ekz>
        la milito por la plej multaj el ili estas ankora&ubreve; pura 
        krucmilito kontra&ubreve; <tld/>ismo, kontra&ubreve; perfortanto de 
        malfortuloj  
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>, 
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">barbarie</trd>
  </snc>
  <snc mrk="barbar.0ismo.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      Kontra&ubreve;gramatika konstruo de vorto:
      <ekz>
        formoj kiel <ctl>kapti&gcirc;ata</ctl>, <ctl>naski&gcirc;ita</ctl>
        ktp. estas simplaj <tld/>ismoj
        <fnt>
          <aut>Societo por la Internacia Lingvo Esperanto en Brno</aut>,
          <vrk>Mediala vo&ccirc;o ne toleras pasivajn fina&jcirc;ojn</vrk>,
          <lok><url
          ref="http://www.literatura.bucek.name/brno/Invitilo%202014-I.pdf">
          Informilo, 2004:11</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">barbarisme</trd>
  </snc>
</drv>
</art>
