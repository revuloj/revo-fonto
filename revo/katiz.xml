<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: katiz.xml,v 1.24 2023/11/26 09:24:08 revo Exp $">
<kap>
  <rad>katiz</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="katiz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="katiz.0i.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="apretu.0i">Apreturi</ref> &scirc;tofon per uzo de gumakvo kaj posta premegado:
      <ekz>
        &scirc;ia &ccirc;apelo estis eta, ebena, katizita, nova plat&ccirc;apo
        <fnt>
          <aut> L. M. Montgomery, trad. Treffel&eacute; Metcier</aut>
          <vrk><url
          ref="https://books.google.de/books?id=DL2vEAAAQBAJ&amp;lpg=PA80&amp;ots=4yE6waOP--&amp;pg=PA80#v=onepage"
              >Anna de Verdaj Gabloj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="amel.0i">ameli</ref>,
      <ref cel="glace.0igi">glaceigi</ref>,
      <ref cel="kaland1.0i">kalandri</ref>
    </refgrp>
    <adm>
      La indiko pri gumakvo trovi&gcirc;as en PIV, sed ne en la franclingvaj vortaroj, kiujn mi konsultis.
      Sen tiu aldona&jcirc;o &leftquot;katizi&rightquot; kaj &leftquot;apreturi&rightquot; estus &scirc;ajne sinonimoj. Entute la vorto estas eble nur
      uzebla en la Francujo de 19a kaj 20a jc - do ne tre internacia.
      <aut>W. Diestel</aut>
    </adm>
  </snc>
  <trd lng="be">&c_g;&c_l;&c_ja;&c_n;&c_c;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">aprestar</trd>
  <trdgrp lng="cs">
    <trd>apretovat</trd>,
    <trd>zu&scaron;lech&tcaron;ovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>glanzpressen</trd>,
    <trd>pressen</trd>,
    <trd>gummieren <klr>(Stoff)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>size</trd>,
    <trd>glaze</trd>,
    <trd>gloss</trd>
  </trdgrp>
  <trd lng="es">aprestar</trd>
  <trd lng="fr">catir</trd>
  <trdgrp lng="hu">
    <trd>f&eacute;nyez</trd>,
    <trd>csinoz</trd>,
    <trd>kat&iacute;roz</trd>,
    <trd>kat&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12388;&#12420;&#20986;&#12375;&#12377;&#12427;  <pr>&#12388;&#12420;&#12384;&#12375;&#12377;&#12427;</pr></trd>,
    <trd>&#20809;&#27810;&#12434;&#12388;&#12369;&#12427;  <pr>&#12371;&#12358;&#12383;&#12367;&#12434;&#12388;&#12369;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">doen <ind>glanzen</ind></trd>
  <trd lng="pl">katyzowa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_shch;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_g;&c_l;&c_ja;&c_n;&c_c;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>apretova&tcaron;</trd>,
    <trd>zu&scaron;&lcaron;ach&tcaron;ova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_l;&c_o;&c_shch;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_n;&c_a;&c_n;&c_o;&c_s;&c_i;&c_t;&c_i; &c_g;&c_l;&c_ja;&c_n;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="katiz.0o">
  <kap><tld/>o</kap>
  <snc mrk="katiz.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="apretu.0o">Apreturo</ref> farita la&ubreve; tiu procedo:
      <!--
      <ekz>
        <tld/>on oni prifaras per hidra&ubreve;lika ma&scirc;ino;
      </ekz>
      -->
      <ekz>
        <klr>[&#8230;]</klr> aldonanta <tld/>on al nova verda &ccirc;asredingoto
        <fnt>
          <aut>C. Dickens, trad. W. Auld</aut>,
          <vrk>La Postlasitaj Paperoj de la Klubo Pikvika</vrk>,
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_l;&c_ja;&c_n;&c_je;&c_c;</trd>
  <trd lng="ca">aprest</trd>
  <trd lng="cs">apretura</trd>
  <trdgrp lng="de">
    <trd>Pressglanz</trd>,
    <trd>Gummierung <klr>(Stoff)</klr></trd>
  </trdgrp>
  <trd lng="es">apresto</trd>
  <trdgrp lng="fr">
    <trd>cati</trd>,
    <trd>catissage</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>f&eacute;nyez&eacute;s</trd>,
    <trd>csinoz&aacute;s</trd>,
    <trd>kat&iacute;roz&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12388;&#12420;</trd>,
    <trd>&#20809;&#27810;  <pr>&#12371;&#12358;&#12383;&#12367;</pr></trd>
  </trdgrp>
  <trd lng="nl">glans</trd>
  <trd lng="pl">katyzacja</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_s;&c_k;</trd>,
    <trd>&c_g;&c_l;&c_ja;&c_n;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="sk">apret&uacute;ra</trd>
  <trdgrp lng="uk">
    <trd>&c_l;&c_i;&c_s;&c_k;</trd>,
    <trd>&c_g;&c_l;&c_ja;&c_n;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
</drv>

<!-- ekster PV ne uzata, &ccirc;u?
<drv mrk="katiz.de0i">
  <kap>de<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">EVI</uzo>
  <fnt><bib>PV</bib></fnt>
  <snc>
    <ref tip="dif" cel="katiz.mal0i">mal<tld/>i</ref>
  </snc>
</drv>
-->

<drv mrk="katiz.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="katiz.mal0i.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Forpreni, forigi la apreturon de &scirc;tofo.
    </dif>
  </snc>
  <trd lng="ca">desllustrar <klr>(un teixit)</klr></trd>
  <trd lng="cs">odstranit apreturu</trd>
  <trd lng="es">quitar el <ind>apresto</ind> <klr>(a un
  tejido)</klr></trd>
  <trd lng="fr">d&eacute;catir<klr tip="ind"> (&ocirc;ter le cati)</klr></trd>
  <trdgrp lng="hu">
    <trd>f&eacute;nytelen&iacute;t</trd>,
    <trd>dekat&iacute;roz</trd>,
    <trd>dekat&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12388;&#12420;&#28040;&#12375;&#12377;&#12427;  <pr>&#12388;&#12420;&#12369;&#12375;&#12377;&#12427;</pr></trd>,
    <trd>&#20809;&#27810;&#12434;&#21462;&#12427;  <pr>&#12371;&#12358;&#12383;&#12367;&#12434;&#12392;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">ontglanzen</trd>
  <trd lng="pl">dekatyzowa&cacute;</trd>
  <trd lng="sk">odstr&aacute;ni&tcaron; apret&uacute;ru</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i; &c_l;&c_i;&c_s;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_z;&c_b;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_i; &c_l;&c_i;&c_s;&c_k;&c_u;</trd>
  </trdgrp>
</drv>
</art>
