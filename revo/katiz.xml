<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>katiz</rad>/i
</kap>
<drv mrk="katiz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Doni apreturan gluon, brilecon al &scirc;tofo. 
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>glanzpressen</trd>,
    <trd>pressen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>size</trd>,
      <trd>glaze</trd>
  </trdgrp>
  <trd lng="nl">doen <ind>glanzen</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_shch;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_g;&c_l;&c_ja;&c_n;&c_c;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="katiz.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i a&ubreve; la rezulto de tiu ago:
      <ekz><tld/>on oni prifaras per hidra&ubreve;lika ma&scirc;ino.</ekz>
    </dif>
  </snc>
  <trd lng="de">Pressglanz</trd>
  <trd lng="nl">glans</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_s;&c_k;</trd>,
    <trd>&c_g;&c_l;&c_ja;&c_n;&c_je;&c_c;</trd>
  </trdgrp>
</drv>
<drv mrk="katiz.de0i">
  <kap>de<tld/>i</kap>
  <snc>
    <dif>
      Forpreni, forigi la apreturan gluon, brilecon de &scirc;tofo.  
    </dif>
  </snc>
  <trd lng="nl">ontglanzen</trd>
</drv>
</art>
