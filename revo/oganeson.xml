<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <rad>oganeson</rad>/o
</kap>
<drv mrk="oganeson.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">KEM</uzo>
  <mlg kod="IUPAC">Og</mlg>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
      lst="voko:&hcirc;emiaj_elementoj" val="118">elemento</ref> kun
      atomnumero 118, atommaso 294&#8239;u:
      <ekz>
        la lastaj elementoj kiuj estis malkovrataj estis nihonio (Nh),
        moskovio (Mc), teneso (Ts) kaj <tld/>o (Og) kun la nombroj 113,
        115, 117 kaj 118
        <fnt>
          <aut>F. van Hertrooij</aut>
          <vrk><url
          ref="https://scivolemo.wordpress.com/2018/09/19/trovi-elementojn-estas-paciencludo/">Trovi
          elementojn estas paciencludo</url></vrk>
          <lok>Scivolemo, 2018-09-19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">oganess&oacute;</trd>
  <trd lng="de">Oganesson</trd>
  <trd lng="en">oganesson</trd>
  <trd lng="es">oganes&oacute;n</trd>
  <trd lng="fr">oganesson</trd>
  <trd lng="ja">&#12458;&#12460;&#12493;&#12477;&#12531; [<ind>&#12362;&#12364;&#12397;&#12381;&#12435;</ind>]</trd>
  <trd lng="la">oganesson</trd>
  <trd lng="nl">oganesson</trd>
  <trdgrp lng="pt">
    <trd>ogan&eacute;sson</trd>,
    <trd>oganess&ocirc;nio</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_g;&c_a;&c_n;&c_je;&c_s;&c_o;&c_n;</trd>
  <trd lng="sv">oganesson</trd>
  <trd lng="zh">&#40939; [<ind>&agrave;o</ind>]</trd>
</drv>
</art>

