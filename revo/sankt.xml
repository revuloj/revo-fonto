<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sankt</rad>/a <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="sankt.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="sankt.0a.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Respekteginda pro speciala religia karaktero:
      <ekz>
        la <ind><tld lit="S"/>a Skribo</ind>;
        <trd lng="hu">Szent&iacute;r&aacute;s</trd>
        <trd lng="pl">Pismo &Sacute;wi&eogonek;te</trd>
        <trd lng="ru">
        &c_s;&c_v;&c_ja;&c_shch;&c_je;&c_n;&c_n;&c_o;&c_je;
        &c_p;&c_i;&c_s;&c_a;&c_n;&c_i;&c_je;
        </trd>
      </ekz>
      <ekz>
        faru <tld/>ajn vestojn por Aaron<fnt>Z</fnt>;
      </ekz>
      <ekz>
        observu la sabaton &ccirc;ar &gcirc;i estas <tld/>a por 
        vi<fnt>Z</fnt>;
      </ekz>
      <ekz>
        prenu la <tld/>an oleon kaj <tld/>oleu la altaron
        <fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>olei re&gcirc;on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">KAT</uzo>
        <uzo tip="fak">POL</uzo>
        la <ind><tld/>a se&gcirc;o</ind>, trono, katedro
        <klr>(papa registaro)</klr>.
        <trd lng="de">Heiliger Stuhl</trd>
        <trd lng="hu">Szentsz&eacute;k</trd>
        <trd lng="nl">Heilige <ind>Stoel</ind></trd>
        <trd lng="pl">Tron Piotrowy</trd>
        <trd lng="ru">&c_s;&c_v;&c_ja;&c_t;&c_o;&c_j; &c_p;&c_r;&c_je;&c_s;&c_t;&c_o;&c_l;</trd>
      </ekz>
    </dif>
  <subsnc mrk="sankt.0a.ula">
    <uzo tip="fak">KRI</uzo>
    <dif>
      Epiteto de <tld/>ulo a&ubreve; <tld/>ulino:
      <ekz>
        <tld/>a Petro.
      </ekz>
    </dif>
    <trd lng="ru">&c_s;&c_v;&c_ja;&c_t;&c_o;&c_j;</trd>
  </subsnc>
  <subsnc mrk="sankt.0a.semajna">
    <uzo tip="fak">KRI</uzo>
    <dif>
      La epiteto de la tagoj de la
      <ref cel="sankt.0a.semajno.KRI"><tld lit="S"/>a Semajno</ref>:
      <ekz>
        <ind><tld lit="S"/>a &Jcirc;a&ubreve;do</ind>;
        <trd lng="en">Maundy <ind>Thursday</ind></trd>
        <trd lng="de">Gr&uuml;ndonnerstag</trd>
        <trd lng="hu">nagycs&uuml;t&ouml;rt&ouml;k</trd>
        <trd lng="it"><ind>Gioved&igrave;</ind> Santo</trd>
        <trd lng="nl">Witte <ind>Donderdag</ind></trd>
        <trd lng="pl">Wielki Czwartek</trd>
        <trdgrp lng="ru">
          <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_i;&c_j; &c_ch;&c_je;&c_t;&c_v;&c_je;&c_r;&c_g;</trd>,
          <trd>&c_ch;&c_i;&c_s;&c_t;&c_y;&c_j; &c_ch;&c_je;&c_t;&c_v;&c_je;&c_r;&c_g;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind><tld lit="S"/>a Vendredo</ind>;
        <trd lng="en">Good <ind>Friday</ind></trd>
        <trd lng="de">Karfreitag</trd>
        <trd lng="hu">nagyp&eacute;ntek</trd>
        <trd lng="it"><ind>Venerd&igrave;</ind> Santo</trd>
        <trd lng="nl">Goede <ind>Vrijdag</ind></trd>
        <trd lng="pl">Wielki Pi&aogonek;tek</trd>
        <trdgrp lng="ru">
          <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_i;&c_j; &c_p;&c_ja;&c_t;&c_o;&c_k;</trd>,
          <trd>&c_s;&c_t;&c_r;&c_a;&c_s;&c_t;&c_n;&c_a;&c_ja; &c_p;&c_ja;&c_t;&c_n;&c_i;&c_c;&c_a;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        kaj ili venas &ccirc;iujn fastajn tagojn &gcirc;is la
        <ind><tld lit="S"/>a Sabato</ind>, kiu estas
        la tago anta&ubreve; Pasko
        <fnt>
          <aut>M. Polo, trad. D. Moirand</aut>:
          <vrk>La libro de la mirinda&jcirc;oj</vrk>,
          <lok>2001</lok>
        </fnt>.
        <trd lng="hu">nagyszombat</trd>
        <trd lng="nl">paaszaterdag</trd>
        <trd lng="pl">Wielka Sobota</trd>
        <trdgrp lng="ru">
          <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_a;&c_ja; &c_s;&c_u;&c_b;&c_b;&c_o;&c_t;&c_a;</trd>,
          <trd>&c_s;&c_t;&c_r;&c_a;&c_s;&c_t;&c_n;&c_a;&c_ja; &c_s;&c_u;&c_b;&c_b;&c_o;&c_t;&c_a;</trd>
        </trdgrp>
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_s;&c_t;&c_r;&c_a;&c_s;&c_t;&c_n;&c_o;&c_j;</trd>
    </trdgrp>
  </subsnc>
    <trdgrp lng="ru">
      <trd>&c_s;&c_v;&c_ja;&c_shch;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_v;&c_ja;&c_t;&c_o;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sankt.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kvaza&ubreve;religie soleninda; plej respekteginda:
      <ekz>
        nia standardo estas por ni io <tld/>a<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a estas por ni la hodia&ubreve;a tago<fnt>Z</fnt>;
      </ekz>
      <ekz>
        en via bu&scirc;o la <tld/>a vorto &leftquot;amo&rightquot;
        malpuri&gcirc;as<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ho patriotismo, kiam fine via <tld/>a nomo &ccirc;esos esti
        armilo en la manoj de malhonestuloj?<fnt>Z</fnt>
      </ekz>
      <ekz>
        la celo <tld/>igas la rimedojn.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_s;&c_v;&c_ja;&c_shch;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_v;&c_ja;&c_t;&c_o;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="bg">&c_s;&c_v;&c_je;&c_t;&c_i;</trd>
  <trd lng="cs">svat&yacute;</trd>
  <trd lng="de">heilig</trd>
  <trd lng="en">sacred</trd>
  <trd lng="fr">saint<klr> (adj.)</klr></trd>
  <trd lng="hu">szent</trd>
  <trd lng="it">santo<klr> (agg.)</klr></trd>
  <trd lng="nl">heilig</trd>
  <trd lng="pl">&sacute;wi&eogonek;ty</trd>
</drv>

<drv mrk="sankt.0ega">
  <kap><tld/>ega</kap>
  <snc mrk="sankt.0ega.REL">
    <dif>
      Eminente, eksterordinare <tld/>a:
      <ekz>
        la&ubreve;don kaj gloron al Vi, <tld lit="S"/>ega Triunuo,
        unika kaj supera Dio!
        <fnt>
          <aut>-</aut>,
          <lok><url ref=
          "http://b-eichkorn.homepage.t-online.de/dibe40a.htm">
          Dio benu, 2000:10, N-ro 4:40</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        En Esperanto li <klr>(Johano Pa&ubreve;lo la Dua)</klr>
        diras: &leftquot;<klr>...</klr> <tld lit="S"/>ega Maria
        akompanu Vin!&rightquot;
        <fnt>
          <vrk>La papoj kaj Esperanto</vrk>.
          <lok><url ref=
"http://skoltoj.blogspot.com/2005/11/la-papoj-kaj-esperanto.html">
          Pa&gcirc;oj en Esperanto por la Esperantistaj Katolikaj
              Geskoltoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">tr&egrave;s saint</trd>
  <trd lng="it">santissimo</trd>
  <trd lng="nl">allerheiligst</trd>
  <trd lng="pl">przenaj&sacute;wi&eogonek;tszy</trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_s;&c_v;&c_ja;&c_t;&c_o;&c_j;
  </trd>
</drv>

<drv mrk="sankt.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sankt.0igi.REL">
    <dif>
      Fari <tld/>a:
      <ekz>
        per &ccirc;i tiu tuko, kiu estas <tld/>igita,
        mi forpelos malproksimen de vi la malbonajn
        spiritojn
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 12</lok></fnt>;
      </ekz>
      <ekz>
        kiu mal<tld/>igos la sabaton, tiu mortu<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konsek.0i">konsekri</ref>,
      <ref cel="profan.mal0igi">malprofanigi</ref>
    </refgrp>
  </snc>
  <trd lng="de">heiligen</trd>
  <trd lng="fr">sanctifier</trd>
  <trd lng="hu">megszentel</trd>
  <trd lng="it">santificare<klr> (rendere santo)</klr></trd>
  <trd lng="nl">heiligen</trd>
  <trdgrp lng="pl">
    <trd>&sacute;wi&eogonek;ci&cacute;</trd>,
    <trd>u&sacute;wi&eogonek;ca&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_s;&c_v;&c_ja;&c_t;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="sankt.0ejo">
  <kap><tld/>ejo<fnt>Z</fnt></kap>
  <snc mrk="sankt.0ejo.REL">
    <tezrad fak="REL"/>
    <uzo tip="fak">REL</uzo>
    <dif>
      Loko a&ubreve; domo de kulto:
      <ekz>
        sur ilia standardo estas pentrita la kruco, kiu estas same
        la insigno de dio Kecalkoatl kaj staras skulptite el
        &scirc;tono en la <tld/>ejo en &Ccirc;olula
        <fnt><bib>SatirRak</bib>, <lok>la guarbo</lok></fnt>;
      </ekz>
      <ekz>
        kvardek ses jarojn &ccirc;i tiu <tld/>ejo estis konstruata
        <fnt><bib>NT</bib>, <lok>&Joh; 2:20.</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="templ.0o.rezidejo">templo</ref>
    <ref tip="sub" cel="sankt.plej0ejo">plej<tld/>ejo</ref>
  </snc>
  <trd lng="en">sanctuary</trd>
  <trd lng="fr">sanctuaire</trd>
  <trd lng="it">santuario</trd>
  <trd lng="nl">heiligdom</trd>
  <trdgrp lng="pl">
    <trd>&sacute;wi&aogonek;tynia</trd>,
    <trd>sanktuarium</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_v;&c_ja;&c_t;&c_i;&c_l;&c_i;&c_shch;&c_je;
  </trd>
</drv>

<drv mrk="sankt.0ulo">
  <kap><tld/>ulo<fnt>Z</fnt></kap>
  <snc mrk="sankt.0ulo.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      En diversaj religioj (kristanismo, islamo), mita a&ubreve;
      historia persono al kiu la kredantoj atribuas piecon, favoron
      de dio(j) kaj rolon de peranto inter dio(j) kaj la homoj:
      <ekz>
        se Dio ne volas, <tld/>ulo ne helpos
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">KRI</uzo>
        <ind>loke honorata <tld/>ulo</ind>
        <klr>(en la antikva eklezio kaj en ortodoksismo:
          persono kies publikan honoradon en sia episkopujo
          sankciis episkopo)</klr>;
        <trd lng="ru">
      &c_m;&c_je;&c_s;&c_t;&c_n;&c_o;&c_ch;&c_t;&c_i;&c_m;&c_y;&c_j;
        &c_s;&c_v;&c_ja;&c_t;&c_o;&c_j;
        </trd>
      </ekz>
      <ekz>
        li rememoris, kiel la <tld/>a Elisabeto, kiam &scirc;i vivis
        ankora&ubreve; sur la tero, &scirc;i, la <tld/>ulino de lia
        hejmo kaj
        de lia infaneco, la nobla dukino de Turingio, la alte
        eminenta sinjorino, mem iradis en la plej malri&ccirc;ajn
        lokojn, kaj alportadis al la malsanuloj konsolon kaj
        refre&scirc;igon
        <fnt><bib>Fab3</bib>,
        <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok></fnt>;
      </ekz>
      <ekz>
        festo de &ccirc;iuj <tld/>uloj.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Heilige</trd>
  <trd lng="fr">saint<klr> (subst.)</klr></trd>
  <trd lng="it">santo<klr> (sost.)</klr></trd>
  <trd lng="nl">heilige</trd>
  <trd lng="pl">&sacute;wi&eogonek;ty</trd>
  <trd lng="ru">&c_s;&c_v;&c_ja;&c_t;&c_o;&c_j;<klr>
  (&c_v; &c_z;&c_n;&c_a;&c_ch;. &c_s;&c_u;&c_shch;.)</klr></trd>
  <trdgrp lng="tr">
    <trd>aziz</trd>, <trd>veli</trd>
  </trdgrp>
</drv>

<drv mrk="sankt.0uligi">
  <kap><tld/>uligi<fnt>Z</fnt></kap>
  <snc mrk="sankt.0uligi.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      Agnoski iun <tld/>ulo <klr>(ago de eklezio)</klr>.
    </dif>
    <ref tip="vid" cel="kanon.en0igi">enkanonigi</ref>
  </snc>
  <trd lng="de">heilig sprechen</trd>
  <trd lng="fr">canoniser</trd>
  <trd lng="hu">szentt&eacute; <ind>avat</ind></trd>
  <trd lng="it">canonizzare</trd>
  <trd lng="nl"><ind>heilig</ind> verklaren</trd>
 <trd lng="ru">&c_p;&c_r;&c_i;&c_ch;&c_i;&c_s;&c_l;&c_i;&c_t;&c_mol;
  &c_k; &c_l;&c_i;&c_k;&c_u; &c_s;&c_v;&c_ja;&c_t;&c_y;&c_h;</trd>
</drv>

<drv mrk="sankt.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="sankt.mal0a.REL">
    <dif>
      Religie malbona, malutila,
      <ref tip="vid" cel="pi.mal0a.REL">malpia</ref>,
      <ref tip="vid" cel="diabl.0a.REL">diabla</ref>:
      <ekz>
        sciu, ke la dogmaro, kiun vi deklaras, estas 
        dogmaro de la mal<tld/>a spirito
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">impie</trd>
</drv>

<drv mrk="sankt.kolono0ulo">
  <kap>kolono<tld/>ulo</kap>
  <snc mrk="sankt.kolono0ulo.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      <ref tip="super" cel="asket.0o.REL">Asketo</ref>
      kiu konstante vivas sur la supro de kolona konstrua&jcirc;o:
      <ekz>
        Ramajun estas nia plej fama kolono<tld/>ulo
        <fnt>
          <aut>Hevesi L., trad. Vizi L.</aut>,
          <vrk><url
          ref="https://sites.google.com/site/laaventurojdeandreojelky">
          La aventuroj de Andreo Jelky</url></vrk>,
          <lok>[2010]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">stylite</trd>
</drv>

<drv mrk="sankt.plej0ejo">
  <kap>plej<tld/>ejo<fnt>Z</fnt></kap>
  <snc mrk="sankt.plej0ejo.BIB">
    <uzo tip="fak">BIB</uzo>
    <dif>
      La plej <tld/>a parto de la jerusalema
      <ref tip="malprt" cel="templ.0o.rezidejo">templo</ref>:
      <ekz>
        la plej<tld/>ejon li aran&gcirc;is en la domo por tio,
        ke oni starigu tie la keston de interligo de la Eternulo
        <fnt><bib>MT</bib>, <lok>&Re1; 6:19.</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">saint des saints</trd>
  <trd lng="it">sancta santorum</trd>
  <trd lng="nl">allerheiligste<klr tip="ind"> (zn.)</klr></trd>
  <trd lng="ru">
  &c_s;&c_v;&c_ja;&c_t;&c_a;&c_ja; &c_s;&c_v;&c_ja;&c_t;&c_y;&c_h;
  </trd>
</drv>

<drv mrk="sankt.0a.semajno">
  <kap><tld lit="S"/>a Semajno</kap>
  <snc mrk="sankt.0a.semajno.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      La semajno kiu komenci&gcirc;as de la
      <ref cel="palm.0ofesto.KRI">Palma Diman&ccirc;o</ref> kaj
      anta&ubreve;as
      la kristanan <ref cel="pask.0o.KRI">Paskon</ref>.
    </dif>
  </snc>
  <trd lng="de">Karwoche</trd>
  <trd lng="en">Holy <ind>Week</ind></trd>
  <trd lng="fr">semaine sainte</trd>
  <trd lng="hu">nagyh&eacute;t</trd>
  <trd lng="it"><ind>Settimana</ind> Santa</trd>
  <trd lng="nl">Goede <ind>Week</ind></trd>
  <trd lng="pl">Wielki Tydzie&nacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_r;&c_a;&c_s;&c_t;&c_n;&c_a;&c_ja; &c_n;&c_je;&c_d;&c_je;&c_l;&c_ja;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_a;&c_ja; &c_s;&c_je;&c_d;&c_m;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
</drv>
</art>
