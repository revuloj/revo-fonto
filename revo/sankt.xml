<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sankt.xml,v 1.52 2022/08/12 09:31:09 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sankt</rad>/a
</kap>

<drv mrk="sankt.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="sankt.0a.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Respekteginda pro speciala religia karaktero:
      <ekz>
        la <ind><tld lit="S"/>a Skribo</ind>;
        <trd lng="hu">Szent&iacute;r&aacute;s</trd>
        <trd lng="pl">Pismo &Sacute;wi&eogonek;te</trd>
        <trd lng="ru">
        &c_s;&c_v;&c_ja;&c_shch;&c_je;&c_n;&c_n;&c_o;&c_je;
        &c_p;&c_i;&c_s;&c_a;&c_n;&c_i;&c_je;
        </trd>
      </ekz>
      <ekz>
        faru <tld/>ajn vestojn por via frato <nom>Aaron</nom>
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 28:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        observu la sabaton &ccirc;ar &gcirc;i estas <tld/>a por 
        vi
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 31:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        prenu la <tld/>an oleon kaj <klr>[&#8230;]</klr> <tld/>oleu la altaron
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 40:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>olei re&gcirc;on
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 9:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la sepa tago faru al vi <tld/>an kunvenon, nenia laboro estu
        farata en tiuj tagoj
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 12:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Sankta Kruco
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a devo
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
         katenoj <tld/>aj, tamen sklavaj
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
         rompi sian <tld/>an promeson
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">KAT</uzo>
        <uzo tip="fak">POL</uzo>
        la <ind><tld/>a se&gcirc;o</ind>, trono, katedro
        <klr>(papa registaro)</klr>;
        <trd lng="de">Heiliger Stuhl</trd>
        <trd lng="hu">Szentsz&eacute;k</trd>
        <trd lng="nl">Heilige <ind>Stoel</ind></trd>
        <trd lng="pl">Tron Piotrowy</trd>
        <trd lng="ru">&c_s;&c_v;&c_ja;&c_t;&c_o;&c_j; &c_p;&c_r;&c_je;&c_s;&c_t;&c_o;&c_l;</trd>
      </ekz>
      <ekz>
        latino estas ankora&ubreve; la oficiala lingvo de la <tld lit="S"/>a
        Se&gcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="&Monato;/2004/008394.html">La angla,
          &ccirc;u troja &ccirc;evalo?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  <subsnc mrk="sankt.0a.ula">
    <uzo tip="fak">KRI</uzo>
    <dif>
      Epiteto de <tld/>ulo a&ubreve; <tld/>ulino:
      <ekz>
        <tld/>a Petro
        <fnt>
          <bib>ElektFab</bib>
          <lok>Kamarado Gaja</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a Johano
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="S"/>a Virgulino
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la mona&hcirc;ejo de la <tld lit="S"/>a Stefano
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_s;&c_v;&c_ja;&c_t;&c_o;&c_j;</trd>
  </subsnc>
  <subsnc mrk="sankt.0a.semajna">
    <uzo tip="fak">KRI</uzo>
    <dif>
      La epiteto de la tagoj de la
      <ref cel="sankt.0a_semajno.KRI"><tld lit="S"/>a Semajno</ref>:
      <ekz>
        <ind><tld lit="S"/>a &Jcirc;a&ubreve;do</ind>;
        <trd lng="en">Maundy <ind>Thursday</ind></trd>
        <trd lng="de">Gr&uuml;ndonnerstag</trd>
        <trd lng="hu">nagycs&uuml;t&ouml;rt&ouml;k</trd>
        <trd lng="it"><ind>Gioved&igrave;</ind> Santo</trd>
        <trd lng="nl">Witte <ind>Donderdag</ind></trd>
        <trd lng="pl">Wielki Czwartek</trd>
        <trdgrp lng="ru">
          <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_i;&c_j; &c_ch;&c_je;&c_t;&c_v;&c_je;&c_r;&c_g;</trd>,
          <trd>&c_ch;&c_i;&c_s;&c_t;&c_y;&c_j; &c_ch;&c_je;&c_t;&c_v;&c_je;&c_r;&c_g;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind><tld lit="S"/>a Vendredo</ind>;
        <trd lng="be">&c_p;&c_a;&c_k;&c_u;&c_t;&c_n;&c_a;&c_ja; <ind>&c_p;&c_ja;&c_t;&c_n;&c_ib;&c_c;&c_a;</ind></trd>
        <trd lng="en">Good <ind>Friday</ind></trd>
        <trd lng="de">Karfreitag</trd>
        <trd lng="hu">nagyp&eacute;ntek</trd>
        <trd lng="it"><ind>Venerd&igrave;</ind> Santo</trd>
        <trd lng="nl">Goede <ind>Vrijdag</ind></trd>
        <trd lng="pl">Wielki Pi&aogonek;tek</trd>
        <trdgrp lng="ru">
          <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_i;&c_j; &c_p;&c_ja;&c_t;&c_o;&c_k;</trd>,
          <trd>&c_s;&c_t;&c_r;&c_a;&c_s;&c_t;&c_n;&c_a;&c_ja; &c_p;&c_ja;&c_t;&c_n;&c_i;&c_c;&c_a;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        kaj ili venas &ccirc;iujn fastajn tagojn &gcirc;is la
        <ind><tld lit="S"/>a Sabato</ind>, kiu estas
        la tago anta&ubreve; Pasko
        <fnt>
          <aut>M. Polo, trad. D. Moirand</aut>:
          <vrk>La libro de la mirinda&jcirc;oj</vrk>,
          <lok>2001</lok>
        </fnt>.sankta
        <trd lng="be">&c_p;&c_a;&c_k;&c_u;&c_t;&c_n;&c_a;&c_ja; <ind>&c_s;&c_u;&c_b;&c_o;&c_t;&c_a;</ind></trd>
        <trd lng="hu">nagyszombat</trd>
        <trd lng="nl">paaszaterdag</trd>
        <trd lng="pl">Wielka Sobota</trd>
        <trdgrp lng="ru">
          <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_a;&c_ja; &c_s;&c_u;&c_b;&c_b;&c_o;&c_t;&c_a;</trd>,
          <trd>&c_s;&c_t;&c_r;&c_a;&c_s;&c_t;&c_n;&c_a;&c_ja; &c_s;&c_u;&c_b;&c_b;&c_o;&c_t;&c_a;</trd>
        </trdgrp>
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_s;&c_t;&c_r;&c_a;&c_s;&c_t;&c_n;&c_o;&c_j;</trd>
    </trdgrp>
  </subsnc>
    <trdgrp lng="ru">
      <trd>&c_s;&c_v;&c_ja;&c_shch;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_v;&c_ja;&c_t;&c_o;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sankt.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kvaza&ubreve;religie soleninda; plej respekteginda:
      <ekz>
        amiko de la <tld/>a ideo
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        sub la <tld/>a signo de l' espero, kolekti&gcirc;as pacaj batalantoj
        <fnt>
          <aut>L. L. Zamenhof</aut>
          <vrk>La Espero</vrk>
        </fnt>;
      </ekz>
      <ekz>
        ho sankta Kabe, helpu 
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga
          Butiko</url></vrk>
        </fnt>!
      </ekz>
      <ekz>
        <tld/>a estas por ni la hodia&ubreve;a tago
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista en Boulogne
          sur Mer en la 5a de a&ubreve;gusto 1905</vrk>
        </fnt>;
      </ekz>
      <ekz>
        en via bu&scirc;o la <tld/>a vorto &leftquot;amo&rightquot; malpuri&gcirc;as
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado en la Guildhall (Londono) en la 21a de a&ubreve;gusto
          1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        ho patriotismo, kiam fine via <tld/>a nomo &ccirc;esos esti
        armilo en la manoj de malhonestuloj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado en la Guildhall (Londono) en la 21a de a&ubreve;gusto
          1907</vrk>
        </fnt>?
      </ekz>
      <ekz>
        la celo <tld/>igas la rimedojn.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_s;&c_v;&c_ja;&c_shch;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_v;&c_ja;&c_t;&c_o;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_v;&c_ja;&c_t;&c_y;</trd>,
    <trd>&c_s;&c_mol;&c_v;&c_ja;&c_sh;&c_ch;&c_e;&c_n;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_v;&c_je;&c_t;&c_i;</trd>
  <trd lng="bo">&t_ra;&t_tsata;&t_tsheg;&t_cha;&t_drengbu;&t_na;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>posv&aacute;tn&yacute;</trd>,
    <trd>svat&yacute;</trd>
  </trdgrp>
  <trd lng="de">heilig</trd>
  <trd lng="en">sacred</trd>
  <trdgrp lng="es">
    <trd>santo</trd>,
    <trd>santa</trd>
  </trdgrp>
  <trd lng="fr">saint<klr> (adj.)</klr></trd>
  <trd lng="hu">szent</trd>
  <trdgrp lng="id">
    <trd>kudus</trd>,
    <trd>sakral</trd>,
    <trd>suci</trd>
  </trdgrp>
  <trd lng="it">santo<klr> (agg.)</klr></trd>
  <trd lng="nl">heilig</trd>
  <trd lng="pl">&sacute;wi&eogonek;ty</trd>
  <trdgrp lng="pt">
    <trd>santo</trd>,
    <trd>s&atilde;o</trd>
    <trd>sagrado</trd>
    <trd>sacro</trd>
  </trdgrp>
  <trd lng="sk">posv&auml;tn&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_v;&c_ja;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_v;&c_ja;&c_shch;&c_je;&c_n;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="sankt.0ega">
  <kap><tld/>ega</kap>
  <snc mrk="sankt.0ega.REL">
    <dif>
      Eminente, eksterordinare <tld/>a:
      <ekz>
        inter la sanktaj libroj de la judoj, la plej <tld/>ega estas la
        Torao
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/007059.html">La influo
          
          de la hebrea lingvo sur Esperanto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve;don kaj gloron al Vi, <tld lit="S"/>ega Triunuo,
        unika kaj supera Dio!
        <fnt>
          <aut>-</aut>,
          <lok><url ref="http://b-eichkorn.homepage.t-online.de/dibe40a.htm">
          Dio benu, 2000:10, N-ro 4:40</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        En Esperanto li <klr>(Johano Pa&ubreve;lo la Dua)</klr>
        diras: &leftquot;<klr>[&#8230;]</klr> <tld lit="S"/>ega Maria
        akompanu Vin!&rightquot;
        <fnt>
          <vrk>La papoj kaj Esperanto</vrk>.
          <lok><url ref="http://skoltoj.blogspot.com/2005/11/la-papoj-kaj-esperanto.html">
          Pa&gcirc;oj en Esperanto por la Esperantistaj Katolikaj
              Geskoltoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_s;&c_mol;&c_v;&c_ja;&c_t;&c_y;</trd>
  <trd lng="de">allerheiligst</trd>
  <trdgrp lng="es">
    <trd>sant&iacute;simo</trd>,
    <trd>sant&iacute;sima</trd>
  </trdgrp>
  <trd lng="fr">tr&egrave;s saint</trd>
  <trd lng="id"><ind>maha</ind>suci</trd>
  <trd lng="it">santissimo</trd>
  <trd lng="nl">allerheiligst</trd>
  <trd lng="pl">przenaj&sacute;wi&eogonek;tszy</trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_s;&c_v;&c_ja;&c_t;&c_o;&c_j;
  </trd>
</drv>

<drv mrk="sankt.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sankt.0igi.REL">
    <dif>
      Fari <tld/>a:
      <ekz>
        per &ccirc;i tiu tuko, kiu estas <tld/>igita,
        mi forpelos malproksimen de vi la malbonajn
        spiritojn
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 12</lok></fnt>;
      </ekz>
      <ekz>
        Moseo malsupreniris de la monto al la popolo kaj <tld/>igis la
        popolon, kaj ili lavis siajn vestojn 
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 19:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu mal<tld/>igos la sabaton, tiu mortu
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 31:14</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="profan.0i">profani</ref>
    <refgrp tip="vid">    
      <ref cel="konsek.0i">konsekri</ref>,
      <ref cel="ben.0i">beni</ref>,
      <ref cel="bapt.0i">bapti</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_mol;&c_v;&c_ja;&c_ch;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">posv&ecaron;tit <klr>(n&ecaron;co| n&ecaron;koho)</klr></trd>
  <trd lng="de">heiligen</trd>
  <trd lng="es">santificar</trd>
  <trd lng="fr">sanctifier</trd>
  <trd lng="hu">megszentel</trd>
  <trd lng="id"><baz>suci</baz>menyucikan</trd>
  <trd lng="it">santificare<klr> (rendere santo)</klr></trd>
  <trd lng="nl">heiligen</trd>
  <trdgrp lng="pl">
    <trd>&sacute;wi&eogonek;ci&cacute;</trd>,
    <trd>u&sacute;wi&eogonek;ca&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_s;&c_v;&c_ja;&c_t;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>posv&auml;cova&tcaron;</trd>,
    <trd>zasv&auml;ti&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_v;&c_ja;&c_t;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_s;&c_v;&c_ja;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="sankt.0ejo">
  <kap><ofc>*</ofc><tld/>ejo</kap>
  <snc mrk="sankt.0ejo.REL">
    <tezrad fak="REL"/>
    <uzo tip="fak">REL</uzo>
    <dif>
      Loko a&ubreve; domo de kulto:
      <ekz>
        kvardek ses jarojn &ccirc;i tiu <tld/>ejo estis konstruata
        <fnt><bib>NT</bib>, <lok>&Joh; 2:20.</lok></fnt>.
      </ekz>
      <ekz>
        tabernaklo estis pretigita: la unua, en kiu estis la kandelabro
        kaj la tablo kaj la panoj de propono; &gcirc;i estas nomata la <tld lit="S"/>ejo
        <fnt>
          <bib>NT</bib>
          <lok>Hebreoj 9:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne permesu al viaj soldatoj sakrilegi la <tld/>ejojn kaj ataki
        la pastrojn
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        purigita kaj incensita, li iris en malgrandan kapelon, for&scirc;iris
        de 
        la pordo la argilan sigelon kaj eniris sola en la <tld/>ejon, 
        kie sur ebura lito ripozis la mirakla statuo de la dio Oziriso
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en la ri&ccirc;an magazenon eniris virino &#8208; en tiun <tld/>ejon de
        ri&ccirc;eco &scirc;i en&scirc;ovi&gcirc;is kiel fantomo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        sur ilia standardo estas pentrita la kruco, kiu estas same
        la insigno de dio <nom>Kecalkoatl</nom> kaj staras skulptite el
        &scirc;tono en la <tld/>ejo en <nom>&Ccirc;olula</nom>
        <fnt><bib>SatirRak</bib>, <lok>la guarbo</lok></fnt>;
      </ekz>
      <ekz>
        Uluru, rokego en la centro de la kontinento kaj <tld/>ejo por
        aborigena kulturo
        <fnt>
          <bib>Monato</bib>
          <aut>Ralph Harry</aut>
          <vrk><url ref="&Steloj;monato2/007103.html">Pa&scirc;oj al
          amikeco</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        por la nepalanoj la religio estas parto de la &ccirc;iutaga vivo: etaj
        <tld/>ejoj estas en &ccirc;iu strato kaj vila&gcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Vera Barandovsk&aacute;-frank</aut>
          <vrk><url ref="&Monato;/2004/008388.html">Nepalo tra
          e&ubreve;ropaj okuloj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="sankt.plej0ejo">plej<tld/>ejo</ref>,
      <ref cel="templ.0o.rezidejo">templo</ref>,
      <ref cel="pregx.0ejo">pre&gcirc;ejo</ref>,
      <ref cel="kapel.0o">kapelo</ref>,
      <ref cel="altar.0o">altaro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_v;&c_ja;&c_c;i&c_l;&c_ib;&c_sh;&c_ch;&c_a;</trd>
  <trd lng="bo">&t_ma;&t_cha;&t_naro;&t_da;&t_tsheg;&t_kha;&t_nga;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>chr&aacute;m</trd>,
    <trd>svatyn&ecaron;</trd>
  </trdgrp>
  <trd lng="de">Heiligtum</trd>
  <trd lng="en">sanctuary</trd>
  <trd lng="es">santuario</trd>
  <trd lng="fr">sanctuaire</trd>
  <trd lng="it">santuario</trd>
  <trd lng="nl">heiligdom</trd>
  <trdgrp lng="pl">
    <trd>&sacute;wi&aogonek;tynia</trd>,
    <trd>sanktuarium</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_v;&c_ja;&c_t;&c_i;&c_l;&c_i;&c_shch;&c_je;
  </trd>
  <trdgrp lng="sk">
    <trd>chr&aacute;m</trd>,
    <trd>sv&auml;ty&ncaron;a</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_v;&c_ja;&c_t;&c_je; &c_m;&c_ib;&c_s;&c_c;&c_je;</trd>,
    <trd>&c_s;&c_v;&c_ja;&c_t;&c_i;&c_n;&c_ja;</trd>,
    <trd>&c_s;&c_v;&c_ja;&c_t;&c_i;&c_l;&c_i;&c_shch;&c_je;</trd>,
    <trd>&c_h;&c_r;&c_a;&c_m;</trd>
  </trdgrp>
</drv>

<drv mrk="sankt.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="sankt.0ulo.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      En diversaj religioj (kristanismo, islamo), mita a&ubreve;
      historia persono al kiu la kredantoj atribuas piecon, favoron
      de dio(j) kaj rolon de peranto inter dio(j) kaj la homoj:
      <ekz>
        <uzo tip="fak">KRI</uzo>
        <ind>loke honorata <tld/>ulo</ind>
        <klr>(en la antikva eklezio kaj en ortodoksismo:
          persono kies publikan honoradon en sia episkopujo
          sankciis episkopo)</klr>;
        <trd lng="ru">
      &c_m;&c_je;&c_s;&c_t;&c_n;&c_o;&c_ch;&c_t;&c_i;&c_m;&c_y;&c_j;
        &c_s;&c_v;&c_ja;&c_t;&c_o;&c_j;
        </trd>
      </ekz>
      <ekz>
        li rememoris, kiel la <tld/>a Elisabeto, kiam &scirc;i vivis
        ankora&ubreve; sur la tero, &scirc;i, la <tld/>ulino de lia
        hejmo kaj de lia infaneco, la nobla dukino de Turingio, la alte
        eminenta sinjorino, mem iradis en la plej malri&ccirc;ajn
        lokojn, kaj alportadis al la malsanuloj konsolon kaj
        refre&scirc;igon
        <fnt><bib>Fab3</bib>,
        <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok></fnt>;
      </ekz>
      <ekz>
        halovino <klr>[&#8230;]</klr>, vespero de &ccirc;iuj <tld/>uloj
        <klr>(vd <ref cel="haloven.0o">Haloveno</ref>)</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="https://www.monato.be/2005/008488.php"
              >Kuk-urbo</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        ortodoksuloj tion faras ne dum la <ind>tago de &ccirc;iuj <tld/>uloj</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="https://www.monato.be/2007/009031.php"
              >Kredo kaj angla lingvo</url></vrk>
          <lok>2007</lok>
        </fnt>;
        <trd lng="de">Allerheiligen</trd>
      </ekz>
      <ekz>
        se Dio ne volas, <tld/>ulo ne helpos
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_v;&c_ja;&c_t;&c_y; <klr>(&c_n;&c_a;&c_z;.)</klr></trd>
  <trd lng="cs">sv&ecaron;tec</trd>
  <trdgrp lng="de">
    <trd>Heiliger</trd>,
    <trd>Heilige</trd>
  </trdgrp>
  <trd lng="es">santo <klr tip="amb">(persona)</klr></trd>
  <trd lng="fr">saint<klr> (subst.)</klr></trd>
  <trdgrp lng="id">
    <trd>santa</trd>,
    <trd>santo</trd>,
    <trd><ind>orang</ind> suci</trd>
  </trdgrp>
  <trd lng="it">santo<klr> (sost.)</klr></trd>
  <trd lng="nl">heilige</trd>
  <trd lng="pl">&sacute;wi&eogonek;ty</trd>
  <trd lng="ru">&c_s;&c_v;&c_ja;&c_t;&c_o;&c_j;<klr>
  (&c_v; &c_z;&c_n;&c_a;&c_ch;. &c_s;&c_u;&c_shch;.)</klr></trd>
  <trd lng="sk">sv&auml;tec</trd>
  <trdgrp lng="tr">
    <trd>aziz</trd>, <trd>veli</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_v;&c_ja;&c_t;&c_i;&c_j; (&c_ib;&c_m;.)</trd>
</drv>

<drv mrk="sankt.0uligi">
  <kap><tld/>uligi</kap>
  <snc mrk="sankt.0uligi.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      Agnoski iun <tld/>ulo <klr>(ago de eklezio)</klr>:
      <ekz>
        la papo Siksto la 5-a konsideris la mona&hcirc;on kiel martiron kaj
        pripensis <tld/>uligi lin
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=220014">Jacques
          Cl&eacute;ment</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kanon.en0igi">enkanonigi</ref>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">kanonizovat</trd>
  <trd lng="de">heilig sprechen</trd>
  <trd lng="es">santificar <klr tip="amb">(a una persona)</klr></trd>
  <trd lng="fr">canoniser</trd>
  <trd lng="hu">szentt&eacute; <ind>avat</ind></trd>
  <trd lng="it">canonizzare</trd>
  <trd lng="nl"><ind>heilig</ind> verklaren</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_ch;&c_i;&c_s;&c_l;&c_i;&c_t;&c_mol;
  &c_k; &c_l;&c_i;&c_k;&c_u; &c_s;&c_v;&c_ja;&c_t;&c_y;&c_h;</trd>
  <trdgrp lng="sk">
    <trd>kanonizova&tcaron;</trd>,
    <trd>vyhl&aacute;si&tcaron; za sv&auml;t&eacute;ho</trd>
  </trdgrp>
</drv>

<drv mrk="sankt.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="sankt.mal0a.REL">
    <dif>
      Religie malbona, malutila,
      <ref tip="vid" cel="pi.mal0a.REL">malpia</ref>,
      <ref tip="vid" cel="diabl.0a.REL">diabla</ref>:
      <ekz>
        la le&gcirc;o ekzistas ne por justulo, sed por homoj senle&gcirc;aj kaj
        malobeemaj, 
        malpiaj kaj pekemaj, mal<tld/>aj kaj profanaj
        <fnt>
          <bib>NT</bib>
          <lok>I. timoteo 1:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        sciu, ke la dogmaro, kiun vi deklaras, estas 
        dogmaro de la mal<tld/>a spirito
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_r;&c_e;&c_sh;&c_n;&c_y;</trd>,
    <trd>&c_d;&#8217;&c_ja;&c_b;&c_a;&c_l;&c_mol;&c_s;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>unheilig</trd>,
    <trd>ketzerisch</trd>,
    <trd>heidnisch</trd>,
    <trd>profan</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>imp&iacute;o</trd>,
    <trd>diab&oacute;lico</trd>
  </trdgrp>
  <trd lng="fr">impie</trd>
  <trd lng="id">najis</trd>
</drv>

<drv mrk="sankt.kolono0ulo">
  <kap>kolono<tld/>ulo</kap>
  <snc mrk="sankt.kolono0ulo.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      <ref tip="super" cel="asket.0o.REL">Asketo</ref>
      kiu konstante vivas sur la supro de kolona konstrua&jcirc;o:
      <ekz>
        <nom>Ramajun</nom> estas nia plej fama kolono<tld/>ulo
        <fnt>
          <aut>Hevesi L., trad. Vizi L.</aut>,
          <vrk><url ref="https://sites.google.com/site/laaventurojdeandreojelky">
          La aventuroj de Andreo Jelky</url></vrk>,
          <lok>[2010]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_o;&c_w;&c_p;&c_n;&c_ib;&c_k;</trd>
  <trd lng="de">S&auml;ulenheiliger</trd>
  <trd lng="es">estilita</trd>
  <trd lng="fr">stylite</trd>
</drv>

<drv mrk="sankt.plej0ejo">
  <kap>plej<tld/>ejo</kap>
  <snc mrk="sankt.plej0ejo.BIB">
    <uzo tip="fak">BIB</uzo>
    <dif>
      Plej <tld/>a loko ene de pre&gcirc;ejo, templo, kiu ofte estas
      malpermesita por aliro de publiko;
      aparte la plej <tld/>a parto de la jerusalema
      <ref tip="malprt" cel="templ.0o.rezidejo">templo</ref>:
      <ekz>
        la kurteno faru por vi apartigon inter la <tld/>ejo kaj la
        plej<tld/>ejo 
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 26:33</lok>
        </fnt>;
      </ekz>
      <ekz>
        li aran&gcirc;is &ccirc;e la muro de la domo galeriojn &ccirc;irka&ubreve; la muroj
        de la domo, &ccirc;irka&ubreve; la templo kaj la plej<tld/>ejo
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 6:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tuta spaco sur la supro de la monto &ccirc;irka&ubreve;e estas
        plej<tld/>ejo
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 43:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        la plej<tld/>ejon li aran&gcirc;is en la domo por tio,
        ke oni starigu tie la keston de interligo de la Eternulo
        <fnt><bib>MT</bib>, <lok>&Re1; 6:19.</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">nejsv&ecaron;t&ecaron;j&scaron;&iacute; m&iacute;sto chr&aacute;mu</trd>
  <trd lng="de">Allerheiligstes</trd>
  <trdgrp lng="es">
    <trd>Sanctasant&oacute;rum <klr tip="amb">(lugar m&aacute;s sagrado del Templo de
    Jerusal&eacute;n)</klr></trd>,
    <trd>Santo de los Santos <klr tip="amb">(lugar m&aacute;s sagrado del
    Templo de Jerusal&eacute;n)</klr></trd>
  </trdgrp>
  <trd lng="fr">saint des saints</trd>
  <trd lng="it">sancta santorum</trd>
  <trd lng="nl">allerheiligste<klr tip="ind"> (zn.)</klr></trd>
  <trd lng="ru">
  &c_s;&c_v;&c_ja;&c_t;&c_a;&c_ja; &c_s;&c_v;&c_ja;&c_t;&c_y;&c_h;
  </trd>
  <trd lng="sk">najsv&auml;tej&scaron;ie miesto v chr&aacute;me</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_v;&c_ja;&c_t;&c_i;&c_l;&c_i;&c_shch;&c_je;</trd>,
    <trd>&c_s;&c_v;&c_ja;&c_t;&c_a;&c_ja; &c_s;&c_v;&c_ja;&c_t;&c_i;&c_h;</trd>
  </trdgrp>
</drv>

<drv mrk="sankt.0a_semajno">
  <kap><tld lit="S"/>a Semajno</kap>
  <snc mrk="sankt.0a_semajno.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      La semajno kiu komenci&gcirc;as de la
      <ref cel="palm.0ofesto.KRI">Palma Diman&ccirc;o</ref> kaj
      anta&ubreve;as
      la kristanan <ref cel="pask.0o.KRI">Paskon</ref>:
      <ekz>
        la <tld lit="S"/>a Semajno en Sevilo estas katolika festo kiu 
        rememoras, per gravaj artaj skulpta&jcirc;oj, &ccirc;efe en strata
        ekspo<klr>[z]</klr>icio, la pasio kaj morto de Jesuo Kristo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18495">Sankta
          Semajno en Sevilo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        post la karnavalo, la katolika eklezio festas karesmon, kiu
        estas kvardek-taga 
        periodo de spirita preparo por la <tld lit="S"/>a Semajno
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17573">Karesmo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_k;&c_u;&c_t;&c_n;&c_y; <ind>&c_t;&c_y;&c_d;&c_z;&c_je;&c_n;&c_mol;</ind></trd>
  <trd lng="de">Karwoche</trd>
  <trd lng="en">Holy <ind>Week</ind></trd>
  <trd lng="es">Semana Santa</trd>
  <trd lng="fr">semaine sainte</trd>
  <trd lng="hu">nagyh&eacute;t</trd>
  <trd lng="it"><ind>Settimana</ind> Santa</trd>
  <trd lng="nl">Goede <ind>Week</ind></trd>
  <trd lng="pl">Wielki Tydzie&nacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_r;&c_a;&c_s;&c_t;&c_n;&c_a;&c_ja; &c_n;&c_je;&c_d;&c_je;&c_l;&c_ja;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_a;&c_ja; &c_s;&c_je;&c_d;&c_m;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
</drv>
