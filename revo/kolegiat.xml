<vortaro>
  <art mrk="$Id: kolegiat.xml,v 1.1 2025/04/25 18:37:06 afido Exp $">
    <kap>
      <rad>kolegiat</rad>/o
    </kap>

<drv mrk="kolegiat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KRI</uzo>
  <snc>
    <dif>
      Kristana komunumo, kies membroj ne ligis sin per ordena voto:
      <ekz>
        Virina <tld lit="K"/>o <nom>Fischbeck</nom> kaj ties &gcirc;ardenoj
        <fnt>
          <vrk><url ref="https://www.esperanto.de/sites/default/files/esperanto.de/og-hameln/Rata_Rendevuo/Einladung%20zum%2017.%20Rata%20Rendevuo%20in%20Hessisch%20Oldendorf%20und%20Fischbeck%20_Esperanto_.pdf"
              >17-a &leftquot;Rata Rendevuo&rightquot;&#8221; en Hessisch Oldendorf kaj Fischbeck</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> <tld lit="K"/>a Pre&gcirc;ejo <klr>(kp <ref tip="super" cel="kanoni.0a_pregxejo">kanonika pre&gcirc;ejo</ref>)</klr>
        de s-ta Marteno fari&gcirc;is plia objekto en
        la listo de la polaj Historiaj Monumentoj
        <fnt>
          <vrk><url ref="https://pola-retradio.org/2023/03/koninda-pollando_opatow-kaj-gia-historimonumento/"
              >Koninda Pollando &ndash; Opat&oacute;w kaj &gcirc;ia Historimonumento</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="kanoni.0ejo">kanonikejo</ref>
    <trd lng="de">Kollegiatsstift</trd>
    <trd lng="en">collegiate church</trd>
    <trd lng="fr">coll&eacute;giale</trd>
    <trd lng="ru">&c_k;&c_o;&c_l;&c_l;&c_je;&c_g;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_c;&c_je;&c_r;&c_k;&c_o;&c_v;&c_mol;</trd>
  </snc>
</drv>

  </art>

