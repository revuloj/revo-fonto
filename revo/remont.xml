<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: remont.xml,v 1.15 2023/09/03 07:53:10 revo Exp $">
<kap>
  <rad>remont</rad>/o
</kap>
<drv mrk="remont.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="super" cel="proviz.0ado">Provizado</ref> de junaj <ref cel="cxeval.0o.genro">&ccirc;evalo</ref>j, 
      precipe en la armeoj de 19a jc, anstata&ubreve;igante la maljunajn a&ubreve; mortintajn:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        oni decidis transporti la pacienton en la sanigejon &leftquot;Sub verda stelo&rightquot;
        <klr>[kaj]</klr> famuloj decidis <klr>[&#x2026;]</klr> fari &leftquot;<tld/>on&rightquot; en la
        organismo de la paciento: forigi la neta&ubreve;gan kaj konservi la utilan
        <fnt>
          <vrk><url ref="https://eo.wikisource.org/wiki/Pa%C4%9Do:Verkoj_de_FeZ.pdf/177">&Ccirc;e la
          lito</url></vrk>
          <lok>Pola Esperantisto, Nr. 10 1929</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur unu de la plej grandaj varsoviaj stratoj estas <tld/>o de tramtrakoj, do mi
        devas veturi per tramo alitrake
        <fnt>
          <vrk><url ref="http://dobromilaesperante.blogspot.com/2007/09/la-granda-alikonstruado.html">La
          granda alikonstruado </url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="anstat.0igi">anstata&ubreve;igi</ref>,
       <ref cel="kompen1.0i">kompensi</ref>,
       <ref cel="san.re0igi">resanigi</ref>,
       <ref cel="nov.re0igi">renovigi</ref>
    </refgrp>
  </snc>
  <trd lng="cs">dod&aacute;vka kon&iacute;</trd>
  <trd lng="de">Remonte</trd>
  <trd lng="fr">remonte</trd>
  <trdgrp lng="hu">
    <trd>remonda</trd>,
    <trd>l&oacute;&aacute;llom&aacute;ny friss&iacute;t&eacute;se</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8ECD;&#x99AC;&#x306E;&#x88DC;&#x5145;  [<ind>&#x3050;&#x3093;&#x3070;&#x306E;&#x307B;&#x3058;&#x3085;&#x3046;</ind>]</trd>,
    <trd>&#x65B0;&#x99AC;  [<ind>&#x3057;&#x3093;&#x3070;</ind>]</trd>,
    <trd>&#x88DC;&#x5145;&#x99AC;  [<ind>&#x307B;&#x3058;&#x3085;&#x3046;&#x3046;&#x307E;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">remonte</trd>
  <trd lng="pl">remont</trd>
  <trd lng="pt">remonta</trd>
  <trdgrp lng="ro">
    <trd>repara&#x21B;ie</trd>,
    <trd>renovare</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_p;&c_o;&c_l;&c_n;&c_je;&c_n;&c_i;&c_je; &c_k;&c_o;&c_n;&c_s;&c_k;&c_i;&c_m; &c_s;&c_o;&c_s;&c_t;&c_a;&c_v;&c_o;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>nevycvi&ccaron;en&yacute; k&ocirc;&ncaron;</trd>,
    <trd>remonta</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x43E;&#x43F;&#x43E;&c_v;&#x43D;&c_je;&#x43D;&#x43D;&#x44F;
  &#x43A;&c_ib;&#x43D;&c_s;&#x44C;&#x43A;&c_i;&#x43C; &c_s;&#x43A;&#x43B;&c_a;&c_d;&#x43E;&#x43C;</trd>
</drv>
</art>
