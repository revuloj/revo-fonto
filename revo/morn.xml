<vortaro>
<art mrk="$Id: morn.xml,v 1.15 2024/01/26 19:05:53 revo Exp $">
<kap>
  <rad>morn</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="morn.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">POE</uzo>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="morn.0a.PSI">
    <dif>
      Malgajiga, ka&ubreve;zanta melankolion,
      <ref tip="sin" cel="melank.0a.melankoliiga">melankolia<sncref/></ref>:
      <ekz>
        en la son&gcirc;o &scirc;i vidis nekonatan pejza&gcirc;on &ndash;
        senesperan, <tld/>an, sub malserena &ccirc;ielo de frua printempo
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 19a, p. 190a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_n;&c_u;&c_r;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_h;&c_m;&c_u;&c_r;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_m;&c_r;&c_a;&c_ch;&c_je;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>chmurn&yacute;</trd>,
    <trd>pochmurn&yacute;</trd>,
    <trd>smutn&yacute;</trd>,
    <trd>t&iacute;sniv&yacute;</trd>,
    <trd>zasmu&scaron;il&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>d&uuml;ster</trd>,
    <trd>tr&uuml;be</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>gloomy</trd>,
    <trd>bleak</trd>
  </trdgrp>
  <trd lng="fr">morne</trd>
  <trdgrp lng="it">
    <trd>melanconico</trd>,
    <trd>triste</trd>,   
   <trd>tristo</trd>,   
   <trd>cupo</trd>
  </trdgrp>
  <trd lng="nl">somber</trd>
  <trdgrp lng="pl">
    <trd>ponury</trd>,
    <trd>do&lstroke;uj&aogonek;cy <klr>(pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>opressivamente <ind>triste</ind></trd>,
    <trd>melanc&oacute;lico</trd>,
    <trd>taciturno</trd>,   
   <trd>entristecedor</trd>,   
   <trd>nebuloso</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_g;&c_r;&c_ju;&c_m;&c_y;&c_j;</trd>,
    <trd>&c_h;&c_m;&c_u;&c_r;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chm&uacute;rny</trd>,
    <trd>drsn&yacute;</trd>,
    <trd>zasmu&scaron;il&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&c_h;&#x43C;&c_u;&c_r;&c_i;&c_j;</trd>,
    <trd>&c_g;&#x43D;&c_ib;&c_t;&#x44E;&c_ch;&c_i;&c_j;</trd>,
    <trd>&#x43F;&#x43E;&#x43D;&c_u;&c_r;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_r;&c_i;&c_g;&#x43D;&c_ib;&c_ch;&c_je;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9EEA; <pr>c&#x1CE;n</pr></trd>,
    <trd>&#x9EF2; <pr>c&#x1CE;n</pr></trd>,
    <trd>&#x60E8;&#x6DE1; <pr>c&#x1CE;nd&#xE0;n</pr></trd>,
    <trd>&#x6158;&#x6DE1; <pr>c&#x1CE;nd&#xE0;n</pr></trd>,
    <trd>&#x9634;&#x6C89; <pr>y&#x12B;nch&#xE9;n</pr></trd>,
    <trd>&#x9670;&#x6C89; <pr>y&#x12B;nch&#xE9;n</pr></trd>
  </trdgrp>
</drv>
</art>
