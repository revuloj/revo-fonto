<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>1</ofc>
   <rad>problem</rad>/o
</kap>
<drv mrk="proble.0o">
   <kap><tld/>o</kap>
   <snc>
     <dif>
       Solvenda demando, malfacila&jcirc;o:
       <ekz>
         aritmetika, fizika, filozofia <tld/>o;
       </ekz><ekz>
         la socia <tld/>o;
       </ekz><ekz>
         tio estas unu el la plej gravaj <tld/>oj, kiuj staras 
         anta&ubreve; ni;
       </ekz><ekz>
         solvi la <tld/>on de lingvo tutmonda<fnt>Z</fnt>;
       </ekz><ekz>
         esperanto kontentige solvis la <tld/>on pri lingvo internacia
         <fnt>
           <aut>E. Lanti</aut>:
           <vrk>Vortoj de Kamarado E. Lanti</vrk>.
           <lok>Nia Pozicio</lok>
         </fnt>;
       </ekz><ekz>
         Vidu: mi ne havas <tld/>ojn pri mono, almena&ubreve; por la pano mi
         havas &ccirc;iam
         <fnt>
           <aut>G.L. Arosev</aut>: <vrk>Nerekonebla</vrk>.
           <bib>Monato</bib>.
         </fnt>.
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="enigm.0o">enigmo</ref>.
     </refgrp>
   </snc>
  <trdgrp lng="be">
     <trd>&c_p;&c_r;&c_a;&c_b;&c_l;&c_je;&c_m;&c_a;</trd>,
     <trd>&c_z;&c_a;&c_d;&c_a;&c_ch;&c_a;</trd>
   </trdgrp>
  <trd lng="bg">&c_p;&c_r;&c_o;&c_b;&c_l;&c_je;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>obt&iacute;&zcaron;</trd>,
    <trd>ot&aacute;zka</trd>,
    <trd>probl&eacute;m</trd>,
    <trd>t&ecaron;&zcaron;kost</trd>,
    <trd>z&aacute;hada</trd>,
    <trd>&uacute;loha</trd>
  </trdgrp>
  <trd lng="de">Problem</trd>
  <trd lng="en">problem</trd>
  <trd lng="es">problema</trd>
  <trdgrp lng="fa">
     <trd>&a_m;&a_s;&a_y_hamza;&a_l;&a_h;</trd>,
     <trd>&a_m;&a_sh;&f_k;&a_l;</trd>
   </trdgrp>
  <trd lng="fr">probl&egrave;me</trd>
  <trd lng="hu">probl&eacute;ma</trd>
  <trdgrp lng="nl">
     <trd>probleem</trd>,
     <trd>vraagstuk</trd>
   </trdgrp>
  <trd lng="pt">problema</trd>
  <trdgrp lng="ru">
     <trd>&c_p;&c_r;&c_o;&c_b;&c_l;&c_je;&c_m;&c_a;</trd>,
     <trd>&c_z;&c_a;&c_d;&c_a;&c_ch;&c_a;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>obtia&zcaron;</trd>,
    <trd>probl&eacute;m</trd>,
    <trd>z&aacute;hada</trd>,
    <trd>&uacute;loha</trd>,
    <trd>&tcaron;a&zcaron;kos&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">problem</trd>
  <trd lng="zh">&#38382;&#39064;</trd>
</drv>

<drv mrk="proble.0a">
   <kap><tld/>a</kap>
   <snc>
     <dif>
       Karakterizas ion, kies solvo a&ubreve; efektivi&gcirc;o estas duba:
       <ekz>
         <tld/>a opinio, sukceso;
       </ekz><ekz>
         rifuzi la faktan nuna&jcirc;on pro la <tld/>a 
         estonta&jcirc;o<fnt>Z</fnt>.
       </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_b;&c_l;&c_je;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="bg">&c_p;&c_r;&c_o;&c_b;&c_l;&c_je;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>problematick&yacute;</trd>,
    <trd>z&aacute;hadn&yacute;</trd>
  </trdgrp>
  <trd lng="de">problematisch</trd>
  <trd lng="en">problematic</trd>
  <trdgrp lng="fa">
     <trd>&a_m;&a_sh;&f_k;&a_l;</trd>,
     <trd>&a_d;&a_sh;&a_w;&a_A;&a_r;</trd>,
     <trd>&a_m;&a_sh;&f_k;&a_l;&zwnj;&a_A_madda;&a_f;&a_r;&f_y;&a_n;</trd>,
     <trd>&a_m;&a_s;&a_y_hamza;&a_l;&a_h;&zwnj;&a_d;&a_A;&a_r;</trd>
   </trdgrp>
  <trd lng="fr">probl&eacute;matique</trd>
  <trdgrp lng="hu">
     <trd>problematikus</trd>,
     <trd>probl&eacute;m&aacute;s</trd>
   </trdgrp>
  <trd lng="nl">problematisch</trd>
  <trd 
lng="ru">&c_p;&c_r;&c_o;&c_b;&c_l;&c_je;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">problematick&yacute;</trd>
  <trd lng="sv">problematisk</trd>
</drv>
</art>
