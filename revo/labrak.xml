<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: labrak.xml,v 1.10 2019/02/23 08:10:16 revo Exp $">
<kap>
    <rad>labrak</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="labrak.0o">
  <kap><tld/>o</kap>
  <snc mrk="labrak.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Mara <ref tip="lst" cel="fisx.0o" lst="voko:fi&scirc;oj">fi&scirc;o</ref>
      kun pufaj lipoj kaj dorna dorsa na&gcirc;ilo,
      fama pro voremo kaj pro tio plurlingve
      alinomata <ctl>lupo</ctl>, <ctl>mara lupo</ctl>...
      (<trd lng="la">Dicentrarchus labrax</trd>);
      <ref tip="lst" cel="speci.0o.BIO"
      lst="voko:zoologiaj_specioj">specio</ref>
      el familio <ref tip="super" cel="morone.0oj">moronedoj</ref>:
      <ekz>
        ekde anta&ubreve; kelkaj jardekoj la akvokulturo (mara kaj kontinenta) forte
        disvolvi&gcirc;is, elstarante la bredadon de orfi&scirc;oj, <tld/>oj, mituloj, trutoj,
        turbotoj kaj salmo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1596"
              >Hispanio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/3/3f/Dicentrarchus_labrax.jpg/491px-Dicentrarchus_labrax.jpg"
      prm="CC-BY-SA-3.0"
      ><tld/>oj (Dicentrarchus labrax)
        <fnt>
          <aut>Georges Jansoone, propra foto</aut>
          <lok><url ref="&WCW;/File:Dicentrarchus_labrax.jpg"
              >https://commons.wikimedia.org/wiki/File:Dicentrarchus_labrax.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="bg">&c_je;&c_v;&c_r;&c_o;&c_p;&c_je;&c_j;&c_s;&c_k;&c_a; <ind>&c_l;&c_a;&c_v;&c_r;&c_a;&c_k;</ind></trd>
  <trd lng="ca">llobarro</trd>
  <trd lng="de">Europ&auml;ischer Wolfsbarsch</trd>
  <trd lng="en">European seabass</trd>
  <trdgrp lng="es">
    <trd>lubina</trd>, 
    <trd> sabalo</trd>,
    <trd> r&oacute;balo</trd> 
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bar<klr> (poisson)</klr></trd>, 
    <trd>loup<klr> (poisson)</klr></trd>,
    <trd>perche de mer</trd>
  </trdgrp>
  <trd lng="nl">Europese <ind>zeebaars</ind></trd>
  <trdgrp lng="pl">
    <trd>labraks</trd>,
    <trd>bar</trd>,
    <trd>moren</trd>,
    <trd>oko&nacute; morski</trd>,
    <trd>wilk morski <klr>(b&lstroke;&eogonek;dnie)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_L;&c_a;&c_v;&c_r;&c_a;&c_k;</trd>
</drv>
</art>

