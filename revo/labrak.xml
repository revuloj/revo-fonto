<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>labrak</rad>/o 
</kap>
<drv mrk="labrak.0o">
  <kap><tld/>o</kap>
  <snc mrk="labrak.0o.SNC">
  <uzo tip="fak">ZOO</uzo>
      <dif>
      Mara <ref tip="super" cel="fisx.0o">fi&scirc;o</ref> (Dicentrarchus
      labrax) el familio de la <ref tip="super"
      cel="morone.0oj">moronedoj</ref>
      (Moronidae), kun avidaj lipoj kaj dorna na&gcirc;ilo sur la dorso.
      <tld lit="L"/>o famas pro voremeco kaj pro tio &gcirc;i havas la
      alinomon "lupo" a&ubreve; "mara lupo".
      </dif>
  </snc>
  <trd lng="de">Europ&auml;ische Wolfsbarsch</trd>
  <trd lng="en">European seabass</trd>
  <trdgrp lng="es">
    <trd>lubina</trd>, 
    <trd> sabalo</trd>,
    <trd> r&oacute;balo</trd> 
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bar(poisson)</trd>, 
    <trd>loup(poisson)</trd>
    <trd>perche de mer</trd>
  </trdgrp>
  <trd lng="ru">&c_L;&c_a;&c_v;&c_r;&c_a;&c_k;</trd>
</drv>
</art>

