<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: forges.xml,v 1.42 2023/08/31 13:11:30 revo Exp $">
<kap><ofc>*</ofc>
  <rad>forges</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="forges.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="forges.0i.ne_memori">
    <dif>
      Ne plu <ref tip="ant" cel="memor.0i">memori</ref> pri; ne plu konservi memoron pri,
      <ref tip="sin" cel="memor.mal0i.KOMUNE">malmemori</ref>:
      <ekz>
        <tld/>i la pasintan malfeli&ccirc;on, instruon, ies
        nomon, rendevuon;
      </ekz>
      <ekz>
        <tld/>u mian konsilon, averton;
      </ekz>
      <ekz>
        ne <tld/>u min;
      </ekz>
      <ekz>
        mi &ccirc;iam <tld/>as, kiel estas via nomo kaj patronomo
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        balda&ubreve; <tld/>i pri mi
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i maljusta&jcirc;ojn, faritajn al li mem
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        se li povus <tld/>i la kriojn de l' homo, kiun oni surver&scirc;is per
        bolanta pe&ccirc;o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>!
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="visx.0i">vi&scirc;i</ref>,
      <ref cel="flug.for0i">forflugi</ref>.
    </refgrp>
    <trdgrp lng="pt">
      <trd>esquecer</trd>,
      <trd>olvidar</trd>
    </trdgrp>
  </snc>
  <snc mrk="forges.0i.neglekti">
    <dif>
      Ne <ref tip="ant" cel="atent.0i.gravigi">atenti</ref> pri, ne 
      <ref tip="ant" cel="zorg.0i.pri_io_bone">zorgi</ref> pri, <ref tip="sin" cel="las.preter0i">preterlasi</ref>:
      <ekz>
        <tld/>i nomon en la listo;
      </ekz>
      <ekz>
        <tld/>i siajn promesojn, siajn devojn;
      </ekz>
      <ekz>
        oni facile <tld/>as la ricevitajn bonfarojn;
      </ekz>
      <ekz>
        <nom>Ramzes</nom> estis tiel ekscitita, ke li jam <tld/>is pri sia rango
        kaj pri siaj devoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&ccirc;u]</klr> la popola&ccirc;o <klr>[...]</klr> havas la rajton
        <tld/>i siajn devojn al ni
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>?
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tomb.en0igi">entombigi</ref>,
      <ref cel="ter.en0igi">enterigi</ref>,
      <ref cel="atent.preter0i">preteratenti</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="arhxiv.0o">arkivo</ref>,
      <ref cel="kalend">kalendo</ref>
    </refgrp>
    <trdgrp lng="pt">
      <trd>esquecer</trd>,
      <trd>omitir</trd>
    </trdgrp>
  </snc>
  <snc mrk="forges.0i.ne_kunporti">
    <dif>
      <ref tip="super" cel="las.0i.restigi">Lasi</ref>, 
      <ref tip="super" cel="rest.0igi">restigi</ref> objekton pro malatento:
      <ekz>
        mi <tld/>is hejme la monujon;
      </ekz>
      <ekz>
        <tld/>i sakon en vagonaro;
      </ekz>
      <ekz>
        &scirc;i volis nur vidi, &ccirc;u oni ne <tld/>is ian figon a&ubreve; pomon
        <klr>[inter la bran&ccirc;oj]</klr>
        <fnt>
          <bib>Fab2</bib>
          <lok>Abio</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">liegen <ind>lassen</ind></trd>
    <trdgrp lng="hu">
      <trd>ottfelejt</trd>,
      <trd>otthagy</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_b;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_b;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_z;&c_a;&c_b;&c_r;&c_a;&c_v;&c_ja;&c_m;</trd>
  <trd lng="bo">&t_ba;&t_ra;&t_jata;&t_drengbu;&t_da;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trdgrp lng="ca">
    <trd>oblidar</trd>,
    <trd>oblidar-se</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>zapomenout</trd>,
    <trd>zapom&iacute;nat</trd>
  </trdgrp>
  <trd lng="de">vergessen</trd>
  <trd lng="en">forget</trd>
  <trd lng="es">olvidar</trd>
  <trd lng="fr">oublier</trd>
  <trdgrp lng="hu">
    <trd>felejt</trd>,
    <trd>elfelejt</trd>
  </trdgrp>
  <trd lng="ia">oblidar</trd>
  <trdgrp lng="ja">
    <trd>&#x5FD8;&#x308C;&#x308B;  [<ind>&#x308F;&#x3059;&#x308C;&#x308B;</ind>]</trd>,
    <trd>&#x5931;&#x5FF5;&#x3059;&#x308B;  [<ind>&#x3057;&#x3064;&#x306D;&#x3093;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x306A;&#x304A;&#x3056;&#x308A;&#x306B;&#x3059;&#x308B;</trd>,
    <trd>&#x304A;&#x308D;&#x305D;&#x304B;&#x306B;&#x3059;&#x308B;</trd>
  </trdgrp>
  <trd lng="nl">vergeten</trd>
  <trd lng="pl">zapomnie&cacute;</trd>
  <trd lng="ro">uita</trd>
  <trd lng="ru">&c_z;&c_a;&c_b;&c_y;&c_t;&c_mol;</trd>
  <trd lng="sk">zabudn&uacute;&tcaron;</trd>
  <trd lng="uk">&c_z;&c_a;&c_b;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>
  <trd lng="zh">&#x5FD8;&#x8BB0;</trd>
</drv>

<drv mrk="forges.0o">
  <kap><tld/>o</kap>
  <snc mrk="forges.0o.malmemoro">
    <uzo tip="fak">PSI</uzo>
    <dif>
      Malmemoro:
      <ekz>
        formeti la aferon en la keston de <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la morga&ubreve;a tago ofte ruinigadis kaj &jcirc;etadis en abismon 
        de <tld/>o tion, kion &scirc;i kun granda malfacileco sukcesis atingi
        hiera&ubreve;
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">POE</uzo>
        nebulo de <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="forges.0o.neglekto">
    <dif>
      Nezorgo, preterlaso, <ref cel="neglek.0o">neglekto</ref>:
      <ekz>
        plej facile promeso rimi&gcirc;as kun <tld/>o
        <fnt>
          <bib>Prv</bib>
        </fnt>;
      </ekz>
      <ekz>
        kelktempa &ccirc;eso ne estas <tld/>o
        <fnt>
          <bib>Prv</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_b;&c_y;&c_c;&c_mol;&c_c;&c_jo;</trd>
  <trdgrp lng="cs">
    <trd>zapomenut&iacute;</trd>,
    <trd>zapomn&ecaron;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Vergessen</trd>
  <trd lng="en">forgetting</trd>
  <trd lng="es">olvido</trd>
  <trd lng="fr">oubli</trd>
  <trd lng="ja">&#x5FD8;&#x5374;  [<ind>&#x307C;&#x3046;&#x304D;&#x3083;&#x304F;</ind>]</trd>
  <trd lng="pl">zapomnienie</trd>
  <trd lng="ro">uitare</trd>
  <trd lng="ru">&c_z;&c_a;&c_b;&c_v;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">zabudnutie</trd>
  <trd lng="uk">&c_z;&c_a;&c_b;&c_u;&c_t;&c_t;&#x44F;</trd>
</drv>

<drv mrk="forges.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="forges.0ema.KOMUNE">
    <dif>
      Kiu facile <tld/>as, ne bone memoras:
      <ekz>
        la virinoj estas <tld/>emaj
        <fnt>
          <aut>Ba Jin, trad. Li Shijun</aut>,
          <vrk>A&ubreve;tuno en la Printempo</vrk>,
          <lok>1980</lok>
        </fnt>;
      </ekz>
      <ekz>
        Epafo trovis teruran morton: Hera ne estis <tld/>ema 
        <klr>[pardonema]</klr> diino, kaj la fama vivo de Epafo
        kolerigis &scirc;in
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=157957">Epafo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_b;&c_y;&c_w;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_d;&c_b;&c_a;&c_j;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">zapom&ecaron;tliv&yacute;</trd>
  <trd lng="de">vergesslich</trd>
  <trd lng="en">forgetful</trd>
  <trdgrp lng="es">
    <trd>olvidadizo</trd>,
    <trd>desmemoriado</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;tourdi</trd>,
    <trd>oublieux</trd>
  </trdgrp>
  <trd lng="ja">&#x5FD8;&#x308C;&#x3063;&#x307D;&#x3044;  [<ind>&#x308F;&#x3059;&#x308C;&#x3063;&#x307D;&#x3044;</ind>]</trd>
  <trd lng="pl">zapominalski</trd>
  <trd lng="ro">uituc</trd>
  <trd lng="sk">z&aacute;budliv&yacute;</trd>
  <trd lng="uk">&c_z;&c_a;&c_b;&c_u;&c_d;&#x44C;&#x43A;&c_u;&c_v;&c_a;&c_t;&c_i;&c_j;</trd>
</drv>

<drv mrk="forges.mem0o">
  <kap>mem<tld/>o, <var><kap>sin<tld/>o</kap></var></kap>
  <snc mrk="forges.mem0o.malegosimo">
    <dif>
      Abnegacio, malegoismo:
      <ekz>
        en tiu okazo li montris plenan sin<tld/>on.
      </ekz>
    </dif>
    <trd lng="ca">abnegaci&oacute;</trd>
    <trdgrp lng="de">
      <trd>Selbstlosigkeit</trd>,
      <trd>Selbstvergessenheit</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>abnegaci&oacute;n</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>renoncement</trd>,
      <trd>sacrifice<klr tip="ind"> (abn&eacute;gation)</klr></trd>
    </trdgrp>
    <trd lng="be">&c_s;&c_a;&c_m;&c_a;&c_z;&c_a;&c_b;&c_y;&c_c;&c_c;&c_jo;</trd>
    <trdgrp lng="hu">
      <trd>&ouml;nzetlens&eacute;g</trd>,
      <trd>&ouml;nfel&aacute;ldoz&aacute;s</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>po&sacute;wi&eogonek;cenie</trd>,
      <trd>oddanie</trd>,
      <trd>abnegacja</trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>sacrificiu</trd>,
      <trd>dedicare</trd>,
      <trd>abnega&#x21B;ie</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_a;&c_m;&c_o;&c_z;&c_a;&c_b;&c_v;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="forges.mem0o.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      Malatento al si kaj al la situacio
      pro absorbi&gcirc;o, revado, malklara konscio...:
      <ekz>
        ili batalis kun <klr>[...]</klr> furioza kolero kaj mem<tld/>o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <klr>[&#x2026;]</klr> eliris, malrapide transirante de vitrino
        al vitrino kaj pririgardante kun grandega plezuro
        montratajn tie a&jcirc;ojn, en kion &scirc;i sin<tld/>e absorbi&gcirc;is
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/mondo.html">
              La mondo brilanta</url></vrk>,
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="obsed.0o">obsedo</ref>
    </refgrp>
    <trd lng="es">distracci&oacute;n</trd>
    <trd lng="fr">distraction</trd>
  </snc>
  <trd lng="en">abnegation</trd>
  <trdgrp lng="ja">
    <trd>&#x7121;&#x79C1;  [<ind>&#x3080;&#x3057;</ind>]</trd>,
    <trd>&#x5FD8;&#x6211;  [<ind>&#x307C;&#x3046;&#x304C;</ind>]</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_a;&#x43C;&#x43E;&c_z;&c_a;&c_b;&c_u;&c_t;&c_t;&#x44F;</trd>
</drv>

<drv mrk="forges.ne0ebla">
  <kap>ne<tld/>ebla</kap>
  <snc mrk="forges.ne0ebla.KOMUNE">
    <dif>
      Inda je memoro:
      <ekz>
        ne<tld/>ebla amiko, okazo;
      </ekz>
      <ekz>
        ni ali&gcirc;os al la Eternulo per interligo eterna, ne<tld/>ebla   
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 50:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel forte ni &ccirc;iuj estis entuziasmigitaj en Bulonjo &ccirc;e l' Maro
        <klr>[...]</klr> 
        &ccirc;iuj &gcirc;in nomas la <ctl>ne<tld/>ebla kongreso</ctl> 
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Dua Kongreso Esperantista en Gen&egrave;ve en
          la 28a de a&ubreve;gusto 1906</vrk>
        </fnt>;
      </ekz>
      <ekz>
        nia ne<tld/>ebla amiko <nom>Javal</nom> plu ne ekzistas
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Tria Kongreso Esperantista en Cambridge
          en la 12a de a&ubreve;gusto 1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        dank' al la ne<tld/>ebla granda merito de la pastro <nom>Johann
        Martin Schleyer</nom>, nia ideo ricevis sian unuan disvolvi&gcirc;on
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en Dresden
          en la 17a de a&ubreve;gusto 1908</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_z;&c_a;&c_b;&c_y;&c_w;&c_n;&c_y;</trd>
  <trd lng="bg">&c_n;&c_je;&c_z;&c_a;&c_b;&c_r;&c_a;&c_v;&c_i;&c_m;</trd>
  <trd lng="ca">inoblidable</trd>
  <trd lng="cs">nezapomenuteln&yacute;</trd>
  <trd lng="de">unvergesslich</trd>
  <trd lng="en">unforgettable</trd>
  <trd lng="es">inolvidable</trd>
  <trd lng="fr">inoubliable</trd>
  <trd lng="hu">felejthetetlen</trd>
  <trd lng="ia">inoblidable</trd>
  <trd lng="ja">&#x5FD8;&#x308C;&#x3089;&#x308C;&#x306A;&#x3044;  [<ind>&#x308F;&#x3059;&#x308C;&#x3089;&#x308C;&#x306A;&#x3044;</ind>]</trd>
  <trd lng="nl">onvergetelijk</trd>
  <trd lng="pl">niezapomniany</trd>
  <trd lng="ro">de neuitat</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_z;&c_a;&c_b;&c_y;&c_v;&c_a;&c_je;&c_m;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_z;&c_a;&c_b;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">nezabudnute&lcaron;n&yacute;</trd>
  <trd lng="uk">&#x43D;&c_je;&c_z;&c_a;&c_b;&c_u;&c_t;&#x43D;&c_ib;&c_j;</trd>
</drv>

<drv mrk="forges.ne0umino">
  <kap>ne<tld/>umino</kap>
  <snc mrk="forges.ne0umino.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Florherbo kun radiformaj, plej ofte bluaj floroj;
      genro <trd lng="la">Mysotis</trd>:
      <ekz>
        aleo bordata de florantaj ne<tld/>uminojn gvidas la vizitanton &gcirc;is ornamita
        ferpordego
        <fnt>
          <bib>MD14</bib>
          <vrk>Adorata de la lo&gcirc;antaro, la industrio de hamburger kaj de fritita kokida&jcirc;o malsatigas
          siajn laboristojn</vrk>
        </fnt>;
      </ekz>
      <ekz>
        La&ubreve; la papiliaj moroj, //
        Lia flirt' sen elimin' //
        Karesadis &ccirc;iujn florojn &mdash; //
        Krom la ne<tld/>umin'.
        <fnt>
          <aut>Boufflers</aut>, <vrk>Bagatela&jcirc;oj</vrk>,
          <lok>La Nica Literatura Revuo, 1956-11 &gcirc;is 12 (2:2), p.
          80a</lok>
        </fnt>
      </ekz>
    </dif>
    <ref tip="sin" cel="miozot.0o">miozoto</ref>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_z;&c_a;&c_b;&c_u;&c_d;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>nezabudka</trd>,
    <trd>pom&ecaron;nka</trd>
  </trdgrp>
  <trd lng="de">Vergissmeinnicht</trd>
  <trd lng="en">forget-me-not</trd>
  <trdgrp lng="es">
    <trd>miosota</trd>,
    <trd>myosotis</trd>,
    <trd>miosotis</trd>,
    <trd>raspilla</trd>,
    <trd>nomeolvides</trd>
  </trdgrp>
  <trd lng="fr">myosotis</trd>
  <trdgrp lng="ja">
    <trd>&#x5FD8;&#x308C;&#x306A;&#x8349;  [<ind>&#x308F;&#x3059;&#x308C;&#x306A;&#x3050;&#x3055;</ind>]</trd>,
    <trd>&#x30EF;&#x30B9;&#x30EC;&#x30CA;&#x30B0;&#x30B5;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>niezapominajka</trd>,
    <trd>u&lstroke;udka</trd>
  </trdgrp>
  <trd lng="ro">nu-m&abreve;-uita</trd>
  <trd lng="ru">&c_n;&c_je;&c_z;&c_a;&c_b;&c_u;&c_d;&c_k;&c_a;</trd>
  <trd lng="sk">nez&aacute;budka</trd>
  <trd lng="tr">unutma beni</trd>
  <trd lng="uk">&#x43D;&c_je;&c_z;&c_a;&c_b;&c_u;&c_d;&#x43A;&c_a;</trd>
</drv>

<drv mrk="forges.sen0e">
  <kap>sen<tld/>e</kap>
  <snc mrk="forges.sen0e.KOMUNE">
    <dif>
      Agante sen <tld/>o, neglekto a&ubreve; preteratento;
      <ref tip="vid" cel="mank.sen0e.KOMUNE">senmanke</ref>,
      <ref tip="vid" cel="nepr.0e.KOMUNE">nepre</ref>:
      <ekz>
        Diego promesis pendigi <klr>[la informilon]</klr> 
        &ccirc;iufoje kaj sen<tld/>e en la indikita loko
        <fnt>
          <aut>G. C. Fighiera</aut>,
