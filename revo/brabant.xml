<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>Brabant</rad>/o
</kap>
<drv mrk="brabant.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>PIV1</bib></fnt>
 
  <snc>
    <dif>
      Nomo de unu <ref tip="super" cel="provin.0o">provinco</ref> en  
      <ref tip="malprt" cel="flandr.F0ujo.reg">Flandrujo</ref> (Flandra <tld/>o kun
      &ccirc;efurbo <ref tip="prt" cel="loven.0o">Loveno</ref>), unu en
      <ref tip="malprt" cel="valon.V0ujo.reg">Valonujo</ref> (Valona <tld/>o) kaj unu en  
      <ref tip="malprt" cel="nederl.0o">Nederlando</ref> (Norda <tld/>o).
    </dif>
  </snc>
   
  <trd lng="es">Brabante</trd>
  <trd lng="fr">Brabant</trd>
  <trd lng="it">Brabante</trd>
  <trd lng="nl">Brabant</trd>
  <trd lng="pl">Brabancja</trd>
  <trd lng="pt">Brabante</trd>
  </drv>


</art>



