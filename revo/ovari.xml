<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>ovari</rad>/o
</kap>
<drv mrk="ovari.0o">
  <kap><tld/>o</kap>
  <snc mrk="ovari.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Organo, kiu &ccirc;e ina genro entenas la ovulojn.
    </dif>
  </snc>
  <trd lng="en">ovary</trd>
  <snc mrk="ovari.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Malsupra parto de la <ref tip="malprt"
      cel="pist.0ilo.BOT">pistilo</ref> enhavanta la
      <ref tip="prt" cel="ovul.0o">ovulojn</ref>.
    </dif>
    <trd lng="de">Fruchtknoten</trd>
  </snc>
  <trd lng="en">ovary</trd>
</drv>
</art>
