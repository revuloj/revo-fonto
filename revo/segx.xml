<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>se&gcirc;</rad>/o
</kap>
<drv mrk="segx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Meblo destinita por sido de unu persono.
    </dif>
  </snc>
  <trd lng="cs">&zcaron;idle</trd>
  <trd lng="de">Stuhl</trd>
  <trd lng="en">chair</trd>
  <trd lng="nl">stoel</trd>
  <trd lng="pl">krzes&lstroke;o</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_u;&c_l;</trd>,
    <trd>&c_s;&c_i;&c_d;&c_je;&c_n;&c_mol;&c_je;</trd>
  </trdgrp>
  <trd lng="sv">stol</trd>
</drv>

<drv mrk="segx.apog0o">
  <kap>apog<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="S"/>o kun dorso kaj brakoj por sin apogi sidante.
    </dif>
  </snc>
  <trd lng="nl">leunstoel</trd>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_s;&c_l;&c_o;</trd>
</drv>
</art>
