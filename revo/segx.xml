<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>se&gcirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="segx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="segx.0o.meblo">
    <dif>
      <ref tip="super" cel="mebl.0o">Meblo</ref> destinita por sido de
      unu
      persono:
      <ekz>
        nun &ccirc;iu stari&gcirc;u sur sia <tld/>o, tralavu la gor&gcirc;on
        kaj, vigle svingante la flagon, koruse kunkantu
        <fnt>
          <aut>B. Golden</aut>,
          <vrk>Propono por elimini la lingvokaoson en E&ubreve;ropa
          Unio</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 1997a, numero 5a, p. 34a</lok>
         </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">chaise</trd>
    <trd lng="it">sedia</trd>
    <trd lng="nl">stoel</trd>
  </snc>
  <snc mrk="segx.0o.posteno">
    <dif>
      Parlamenta a&ubreve; alia balotdependa posteno en asembleo:
      <ekz>
        mankas al la kristandemokratoj unu <tld/>o
        <fnt>
          <aut>G. Van Damme</aut>,
          <vrk>Demokratio blokita</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2001a, numero 1a, p. 8a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Sitz</trd>,
      <trd>Parlamentssitz</trd>
    </trdgrp>
    <trd lng="es">esca&ntilde;o</trd>
    <trd lng="fr">si&egrave;ge<klr> (d'assembl&eacute;e)</klr></trd>
    <trd lng="it">seggio<klr> (d'assemblea)</klr></trd>
    <trd lng="nl">zetel</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_u;&c_l;</trd>,
    <trd>&c_k;&c_r;&c_e;&c_s;&c_l;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_t;&c_o;&c_l;</trd>
  <trd lng="cs">&zcaron;idle</trd>
  <trdgrp lng="de">
    <trd>Sitz</trd>,
    <trd>Stuhl</trd>
  </trdgrp>
  <trd lng="en">chair</trd>
  <trd lng="es">silla</trd>
  <trdgrp lng="he">
    <trd>&kaf;&samekh;&alef;</trd>,
    <trd>&mem;&vav;&shin;&bet;</trd>
  </trdgrp>
  <trd lng="hu">sz&eacute;k</trd>
  <trd lng="pl">krzes&lstroke;o</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_u;&c_l;</trd>,
    <trd>&c_s;&c_i;&c_d;&c_je;&c_n;&c_mol;&c_je;</trd>
  </trdgrp>
  <trd lng="sv">stol</trd>
</drv>

<drv mrk="segx.apog0o">
  <kap>apog<tld/>o, <var><kap>brak<tld/>o</kap></var></kap>
  <snc mrk="segx.apog0o.MEBL">
    <dif>
      <tld lit="S"/>o kun dors- kaj brakapogiloj por pli komforta sido.
    </dif>
  </snc>
  <trd lng="de">Lehnstuhl</trd>
  <trd lng="en">armchair</trd>
  <trd lng="fr">fauteuil</trd>
  <trd lng="he">&kaf;&vav;&resh;&samekh;&he;</trd>
  <trd lng="hu">karossz&eacute;k</trd>
  <trd lng="it"><ind>sedia</ind> con braccioli</trd>
  <trd lng="nl">leunstoel</trd>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_s;&c_l;&c_o;</trd>
  <trd lng="sv">karmstol</trd>
</drv>

<drv mrk="segx.balanco0o">
  <kap>balanco<tld/>o, <var><kap>lul<tld/>o</kap></var></kap>
  <snc mrk="segx.balanco0o.MEBL">
    <dif>
      <tld lit="S"/>o en kiu oni povas sin balanci:
      <ekz>
        sinjoro Rodrigues purigas la dentojn per dentpikilo, tre 
        komforte sidanta sur balanco<tld/>o   
        <fnt>
          <aut>A. Azevedo, trad. R. de Castro Gotardi</aut>,
          <vrk>Plebiscito</vrk>,
          <lok><url
          ref="http://www.lakaravelo.com/LK_000/LK_012_(pr).pdf">
          La Karavelo, 2008-11 &gcirc;is 12 (12)</url>, p. 17-18a</lok>
        </fnt>.  
      </ekz>
    </dif>    
  </snc>
  <trd lng="br">kador-vra&ntilde;sell</trd>
  <trd lng="de">Schaukelstuhl</trd>
  <trd lng="es">mecedora</trd>
  <trd lng="hu">hintasz&eacute;k</trd>  
  <trdgrp lng="fr">    
    <trd>chaise &agrave; bascule</trd>,
    <trd>rocking-chair</trd>
  </trdgrp>
  <trd lng="nl">schommelstoel</trd>
  <trd lng="pt">cadeira de balan&ccedil;o</trd>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_s;&c_l;&c_o;-&c_k;&c_a;&c_ch;&c_a;&c_l;&c_k;&c_a;</trd>
</drv>

<drv mrk="segx.porto0o">
  <kap>porto<tld/>o</kap>
  <snc mrk="segx.porto0o.TRA">
    <dif>
      <tld lit="S"/>o kun horizontalaj stangoj, per kiuj
      anta&ubreve;a kaj malanta&ubreve;a portistoj povas &gcirc;in levi kaj movi:
      <ekz>
        prefere per porto<tld/>o, &ccirc;ar la muska vojo glitigas
        <fnt>
          <bib>RugxDom</bib>,
          <lok>&ccirc;apitro 75a, p. 587a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>litter</trd>,
    <trd>sedan chair</trd>
  </trdgrp>
  <trd lng="fr">chaise &agrave; porteurs</trd>
  <trdgrp lng="it">
    <trd>portantina<klr> (sedia)</klr></trd>,
    <trd><ind>sedia</ind> gestatoria<klr> (portantina)</klr></trd>
  </trdgrp>
  <trd lng="nl">draagstoel</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_r;&c_t;&c_sh;&c_je;&c_z;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_a;&c_n;&c_k;&c_i;&c_n;</trd>,
    <trd>&c_n;&c_o;&c_s;&c_i;&c_l;&c_k;&c_i;<klr> (&c_s; &c_s;&c_i;&c_d;&c_je;&c_n;&c_mol;&c_je;&c_m;)</klr></trd>
  </trdgrp>
  <trd lng="sv">b&auml;rstol</trd>
</drv>

<drv mrk="segx.rul0o">
  <kap>rul<tld/>o</kap>
  <snc mrk="segx.rul0o.MEBL">
    <dif>
      Radhava <tld/>o per kiu eblas sin movi sidante:
      <ekz>
        li sidas en rul<tld/>o hodia&ubreve;; povas mem iri, sed devas ne
        tro vigli
        <fnt>
          <aut>A. Tilmont</aut>,
          <vrk>Arthur C. Clarke, kosmopolita civitano...</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 1998a, numero 4a, p. 9a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Rollstuhl</trd>
  <trd lng="en">wheelchair</trd>
  <trd lng="es"><ind>silla</ind> de ruedas</trd>
  <trdgrp lng="fr">
    <trd>chaise roulante</trd>,
    <trd>fauteuil roulant</trd>
  </trdgrp>
  <trd lng="he">&kaf;&samekh;&alef; &gimel;&lamed;&gimel;&lamed;&yod;&fmem;</trd>
  <trd lng="hu">tol&oacute;sz&eacute;k</trd>
  <trd lng="it"><ind>sedia</ind> a rotelle</trd>
  <trd lng="nl">rolstoel</trd>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_s;&c_l;&c_o;-&c_k;&c_a;&c_t;&c_a;&c_l;&c_k;&c_a;</trd>
  <trd lng="sv">rullstol</trd>
</drv>
</art>
