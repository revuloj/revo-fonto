<drv mrk="segx.apog0o">
  <kap>apog<tld/>o, <var><kap>brak<tld/>o</kap></var></kap>
  <snc mrk="segx.apog0o.MEBL">
    <dif>
      <ref tip="super" cel="segx.0o.meblo"><tld lit="S"/>o</ref>
      kun dors- kaj brak<ref tip="prt" cel="apog.0ilo">apogilo</ref>j por pli komforta sido:
      <ekz>
        la ju&gcirc;istoj sidis en siaj apog<tld/>oj kaj havis malanta&ubreve; la kapo kusenojn el
        lanugo
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la &ccirc;ambro staris brak<tld/>oj kun altaj skulptitaj dorsoj kaj kun brakoj sur
        amba&ubreve; flankoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
      </ekz>
      <ekz>
        amba&ubreve; sidis en ledaj remburitaj brak<tld/>oj
        <fnt>
          <bib>ChB</bib>
          <lok>20</lok>
        </fnt>;
      </ekz>
      <ekz>
        sidi&gcirc;u diris &scirc;i, montrante al li tabureton apud sia apog<tld/>o
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj pasa&gcirc;eroj de la supera ferdeko <klr>[&#x2026;]</klr> falis de la kanaj
        apog<tld/>oj
        <fnt>
          <bib>FdO</bib>
        </fnt>;
      </ekz>
      <ekz>
        brak<tld/>oj tegitaj per punca lana damasko
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        malalta brak<tld/>o sur balancarkoj
        <klr>(vd <ref cel="segx.balanco0o">lulse&gcirc;o</ref>)</klr>
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="fotel.0o">fotelo</ref>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_t;&c_e;&c_l;&c_mol;</trd>
  <trd lng="br">kador-vrec'h</trd>
  <trdgrp lng="de">
