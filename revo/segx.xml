<drv mrk="segx.apog0o">
  <kap>apog<tld/>o, <var><kap>brak<tld/>o</kap></var></kap>
  <snc mrk="segx.apog0o.MEBL">
    <dif>
      <ref tip="super" cel="segx.0o.meblo"><tld lit="S"/>o</ref>
      kun dors- kaj brak<ref tip="prt" cel="apog.0ilo">apogilo</ref>j por pli komforta sido:
      <ekz>
        la ju&gcirc;istoj sidis en siaj apog<tld/>oj kaj havis malanta&ubreve; la kapo kusenojn el
        lanugo
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la &ccirc;ambro staris brak<tld/>oj kun altaj skulptitaj dorsoj kaj kun brakoj sur
        amba&ubreve; flankoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
      </ekz>
      <ekz>
        amba&ubreve; sidis en ledaj remburitaj brak<tld/>oj
        <fnt>
          <bib>ChB</bib>
          <lok>20</lok>
        </fnt>;
      </ekz>
      <ekz>
        sidi&gcirc;u diris &scirc;i, montrante al li tabureton apud sia apog<tld/>o
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj pasa&gcirc;eroj de la supera ferdeko <klr>[&#8230;]</klr> falis de la kanaj
        apog<tld/>oj
        <fnt>
          <bib>FdO</bib>
        </fnt>;
      </ekz>
      <ekz>
        brak<tld/>oj tegitaj per punca lana damasko
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        malalta brak<tld/>o sur balancarkoj
        <klr>(vd <ref cel="segx.balanco0o">lulse&gcirc;o</ref>)</klr>
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="fotel.0o">fotelo</ref>
  </snc>
  <trd lng="br">kador-vrec'h</trd>
  <trdgrp lng="de">
    <trd>Lehnstuhl</trd>,
    <trd>Sessel</trd>
  </trdgrp>
  <trd lng="en">armchair</trd>
  <trd lng="es">sill&oacute;n</trd>
  <trd lng="fa">&a_m;&a_b;&a_l;</trd>
  <trd lng="fr">fauteuil</trd>
  <trd lng="he">&kaf;&vav;&resh;&samekh;&he;</trd>
  <trd lng="hu">karossz&eacute;k</trd>
  <trd lng="it"><ind>sedia</ind> con braccioli</trd>
  <trd lng="nl">leunstoel</trd>
  <trd lng="pl">fotel</trd>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_s;&c_l;&c_o;</trd>
  <trd lng="sk">kreslo</trd>
  <trdgrp lng="sv">
    <trd>f&aring;t&ouml;lj</trd>,
    <trd>l&auml;nstol</trd>
  </trdgrp>
  <trd lng="tr">koltuk</trd>
  <trd lng="uk">&c_k;&c_r;&c_ib;&c_s;&c_l;&c_o;</trd>
</drv>

<drv mrk="segx.balanco0o">
  <kap>balanco<tld/>o, <var><kap>lul<tld/>o</kap></var></kap>
  <snc mrk="segx.balanco0o.MEBL">
    <dif>
      <ref tip="super" cel="segx.0o.meblo"><tld lit="S"/>o</ref> en kiu oni povas sin balanci:
      <ekz>
        tie staris balanco<tld/>oj, sofoj kun silka tego, grandaj tabloj
        <fnt>
          <bib>Fab2</bib>
          <lok>Abio</lok>
        </fnt>;
      </ekz>   
      <ekz>
        la avino sidi&gcirc;as en la lul<tld/>o kaj komencas luli la nepon kaj sin mem al
        la trankvilo
        <fnt>
          <bib>VRA</bib>
          <lok>Moa Martinson</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <ind>balancarko</ind>j de la <tld/>o-lulilo brue ekfrapis la plankon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
        <trd lng="de">Kufe<klr> (Wiege)</klr></trd>
      </ekz>
      <ekz>
        mi vidis la viron sidanta sur lul<tld/>o anta&ubreve; sia domo
        <fnt>
          <bib>Monato</bib>
          <aut>G. O. &Ograve;sh&oacute;-Davies</aut>
          <vrk><url ref="&Monato;/2012/010421.php"
              >Po&scirc;telefono</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        sinjoro <nom>Rodrigues</nom> purigas la dentojn per dentpikilo, tre 
        komforte sidanta sur balanco<tld/>o   
        <fnt>
          <aut>A. Azevedo, trad. R. de Castro Gotardi</aut>,
          <vrk>Plebiscito</vrk>,
          <lok><url ref="http://www.lakaravelo.com/LK_000/LK_012_(pr).pdf">
          La Karavelo, 2008-11 &gcirc;is 12 (12)</url>, p. 17-18a</lok>
        </fnt>.  
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/b/b2/69.79.1_threequarter_PS6.jpg/290px-69.79.1_threequarter_PS6.jpg"
      prm="No restrictions"
      >lul<tld/>o &ccirc;. 1860, Muzeo de Brooklyn
        <fnt>
          <aut>Michael Thonet, Brooklyn Museum</aut>
          <lok><url ref="&WCW;/File:69.79.1_threequarter_PS6.jpg"
              >&WCW;/File:69.79.1_threequarter_PS6.jpg</url></lok>
        </fnt>
    </bld>      
  </snc>
  <trd lng="br">kador-vra&ntilde;sell</trd>
  <trd lng="de">Schaukelstuhl</trd>
  <trd lng="en">rocking chair</trd>
  <trd lng="es">mecedora</trd>
  <trd lng="fr">chaise &agrave; bascule</trd>
  <trd lng="hu">hintasz&eacute;k</trd>
  <trd lng="id"><ind>kursi</ind> goyang</trd>  
  <trd lng="fa">&a_S;&a_n;&a_d;&a_l;&f_y; &f_g;&a_h;&a_w;&a_A;&a_r;&a_h;&zwnj;&a_A;&f_y;</trd>
  <trdgrp lng="fr">    
    <trd>chaise &agrave; bascule</trd>,
    <trd>rocking-chair</trd>
  </trdgrp>
  <trd lng="nl">schommelstoel</trd>
  <trdgrp lng="pl">
    <trd>fotel bujany</trd>,
    <trd>fotel na biegunach</trd>
  </trdgrp>
  <trd lng="pt">cadeira de balan&ccedil;o</trd>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_s;&c_l;&c_o;-&c_k;&c_a;&c_ch;&c_a;&c_l;&c_k;&c_a;</trd>
  <trd lng="uk">&c_k;&c_r;&c_ib;&c_s;&c_l;&c_o;-&c_g;&c_o;&c_j;&c_d;&c_a;&c_l;&c_k;&c_a;</trd>
</drv>

<drv mrk="segx.kusx0o">
  <kap>ku&scirc;<tld/>o,
  <var><kap>ku&scirc;o<tld/>o</kap></var></kap>
  <snc mrk="segx.kusx0o.MEBL">
    <dif>
      <ref tip="super" cel="segx.0o.meblo"><tld lit="S"/>o</ref> kun klinita dorsapogilo kaj
      longa anta&ubreve;a&jcirc;o sur kiu oni etendas la krurojn,
      tiel ke oni per &gcirc;i sidas duonku&scirc;e:
      <ekz>
        li ripozas en ku&scirc;o<tld/>o
        <fnt>
          <bib>KrB</bib>
        </fnt>;
      </ekz>
      <ekz>
        la foiraj mastroj disponigis al la publiko ku&scirc;<tld/>ojn en konvena distanco
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2015/011419.php"
              >10 x 15</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        li sunbruni&gcirc;us la tutan tagon je la marbordo sur ku&scirc;<tld/>o,
        se &gcirc;i ne estus rompita
        <fnt>
          <aut>E. Ki&scirc;on, trad. G. Ziegler</aut>,
          <vrk><url
          ref="http://esperantaretradio.blogspot.fr/2017/05/la-13-tribo-de-la-moderna-israelo.html">
          La 13-a tribo de la moderna Israelo</url></vrk>,
          <lok>en: Elektitaj Satiroj, 1981</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Liegestuhl</trd>,
    <trd>Strandliege</trd>
  </trdgrp>
  <trd lng="fr">transat<klr> (si&egrave;ge)</klr></trd>
</drv>

<drv mrk="segx.porto0o">
  <kap>porto<tld/>o</kap>
  <snc mrk="segx.porto0o.TRA">
    <dif>
      <ref tip="super" cel="segx.0o.meblo"><tld lit="S"/>o</ref> kun horizontalaj stangoj, per kiuj
      anta&ubreve;a kaj malanta&ubreve;a portistoj povas &gcirc;in levi kaj movi:
      <ekz>
        Re&gcirc;a Mo&scirc;to, la port<tld/>o de via Majesto estas 
        en <nom>Padova</nom> kaj la barko estas
        preta
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        prefere per porto<tld/>o, &ccirc;ar la muska vojo glitigas
        <fnt>
          <bib>RugxDom</bib>,
          <lok>&ccirc;apitro 75a, p. 587a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="palank.0o">palankeno</ref>
  </snc>
  <trd lng="de">S&auml;nfte</trd>
  <trdgrp lng="en">
    <trd>litter</trd>,
    <trd>sedan chair</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd><ind>silla</ind> de manos</trd>,
    <trd>palanqu&iacute;n</trd>,
    <trd>litera</trd>
  </trdgrp>
