<drv mrk="segx.apog0o">
  <kap>apog<tld/>o, <var><kap>brak<tld/>o</kap></var></kap>
  <snc mrk="segx.apog0o.MEBL">
    <dif>
      <tld lit="S"/>o kun dors- kaj brakapogiloj por pli komforta sido:
      <ekz>
        malalta brak<tld/>o sur balancarkoj
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">kador-vrec'h</trd>
  <trdgrp lng="de">
    <trd>Lehnstuhl</trd>,
    <trd>Sessel</trd>
  </trdgrp>
  <trd lng="en">armchair</trd>
  <trd lng="es">sill&oacute;n</trd>
  <trd lng="fa">&a_m;&a_b;&a_l;</trd>
  <trd lng="fr">fauteuil</trd>
  <trd lng="he">&kaf;&vav;&resh;&samekh;&he;</trd>
  <trd lng="hu">karossz&eacute;k</trd>
  <trd lng="it"><ind>sedia</ind> con braccioli</trd>
  <trd lng="nl">leunstoel</trd>
  <trd lng="pl">fotel</trd>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_s;&c_l;&c_o;</trd>
  <trd lng="sk">kreslo</trd>
  <trdgrp lng="sv">
    <trd>f&aring;t&ouml;lj</trd>,
    <trd>l&auml;nstol</trd>
  </trdgrp>
  <trd lng="tr">koltuk</trd>
</drv>

<drv mrk="segx.balanco0o">
  <kap>balanco<tld/>o, <var><kap>lul<tld/>o</kap></var></kap>
  <snc mrk="segx.balanco0o.MEBL">
    <dif>
      <tld lit="S"/>o en kiu oni povas sin balanci:
      <ekz>
        sinjoro Rodrigues purigas la dentojn per dentpikilo, tre 
        komforte sidanta sur balanco<tld/>o   
        <fnt>
          <aut>A. Azevedo, trad. R. de Castro Gotardi</aut>,
          <vrk>Plebiscito</vrk>,
          <lok><url ref="http://www.lakaravelo.com/LK_000/LK_012_(pr).pdf">
          La Karavelo, 2008-11 &gcirc;is 12 (12)</url>, p. 17-18a</lok>
        </fnt>.  
      </ekz>
    </dif>    
  </snc>
  <trd lng="br">kador-vra&ntilde;sell</trd>
  <trd lng="de">Schaukelstuhl</trd>
  <trd lng="es">mecedora</trd>
  <trd lng="hu">hintasz&eacute;k</trd>
  <trd lng="id"><ind>kursi</ind> goyang</trd>  
  <trd lng="fa">&a_S;&a_n;&a_d;&a_l;&f_y; &f_g;&a_h;&a_w;&a_A;&a_r;&a_h;&zwnj;&a_A;&f_y;</trd>
  <trdgrp lng="fr">    
    <trd>chaise &agrave; bascule</trd>,
    <trd>rocking-chair</trd>
  </trdgrp>
  <trd lng="nl">schommelstoel</trd>
  <trdgrp lng="pl">
    <trd>fotel bujany</trd>,
    <trd>fotel na biegunach</trd>
  </trdgrp>
  <trd lng="pt">cadeira de balan&ccedil;o</trd>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_s;&c_l;&c_o;-&c_k;&c_a;&c_ch;&c_a;&c_l;&c_k;&c_a;</trd>
</drv>

<drv mrk="segx.kusx0o">
  <kap>ku&scirc;<tld/>o,
  <var><kap>ku&scirc;o<tld/>o</kap></var></kap>
  <snc mrk="segx.kusx0o.MEBL">
    <dif>
      <tld lit="S"/>o kun klinita dorsapogilo kaj
      longa anta&ubreve;a&jcirc;o sur kiu oni etendas la krurojn,
      tiel ke oni per &gcirc;i sidas duonku&scirc;e:
      <ekz>
        li sunbruni&gcirc;us la tutan tagon je la marbordo sur ku&scirc;<tld/>o,
        se &gcirc;i ne estus rompita
        <fnt>
          <aut>E. Ki&scirc;on, trad. G. Ziegler</aut>,
          <vrk><url ref="http://esperantaretradio.blogspot.fr/2017/05/la-13-tribo-de-la-moderna-israelo.html">
          La 13-a tribo de la moderna Israelo</url></vrk>,
          <lok>en: Elektitaj Satiroj, 1981</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">transat<klr> (si&egrave;ge)</klr></trd>
</drv>

<drv mrk="segx.porto0o">
  <kap>porto<tld/>o</kap>
  <snc mrk="segx.porto0o.TRA">
    <dif>
      <tld lit="S"/>o kun horizontalaj stangoj, per kiuj
      anta&ubreve;a kaj malanta&ubreve;a portistoj povas &gcirc;in levi kaj movi:
      <ekz>
        prefere per porto<tld/>o, &ccirc;ar la muska vojo glitigas
        <fnt>
          <bib>RugxDom</bib>,
          <lok>&ccirc;apitro 75a, p. 587a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>litter</trd>,
    <trd>sedan chair</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd><ind>silla</ind> de manos</trd>,
    <trd>palanqu&iacute;n</trd>,
    <trd>litera</trd>
  </trdgrp>
  <trd lng="fr">chaise &agrave; porteurs</trd>
  <trdgrp lng="it">
    <trd>portantina<klr> (sedia)</klr></trd>,
    <trd><ind>sedia</ind> gestatoria<klr> (portantina)</klr></trd>
  </trdgrp>
  <trd lng="nl">draagstoel</trd>
  <trdgrp lng="pl">
    <trd>lektyka</trd>,
    <trd>basterna</trd>,
    <trd>palankin</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_r;&c_t;&c_sh;&c_je;&c_z;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_a;&c_n;&c_k;&c_i;&c_n;</trd>,
    <trd>&c_n;&c_o;&c_s;&c_i;&c_l;&c_k;&c_i;<klr> (&c_s; &c_s;&c_i;&c_d;&c_je;&c_n;&c_mol;&c_je;&c_m;)</klr></trd>
  </trdgrp>
  <trd lng="sv">b&auml;rstol</trd>
</drv>

<drv mrk="segx.rul0o">
  <kap>rul<tld/>o, <var><kap>rad<tld/>o</kap></var></kap>
  <snc mrk="segx.rul0o.MEBL">
    <dif>
      Radhava <tld/>o per kiu eblas sin movi sidante:
      <ekz>
        li sidas en rul<tld/>o hodia&ubreve;; povas mem iri, sed devas ne
        tro vigli
        <fnt>
          <aut>A. Tilmont</aut>,
          <vrk>Arthur C. Clarke, kosmopolita civitano...</vrk>,
          <bib>Monato</bib>,
          <lok>1998:4, p. 9a</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>...</klr> rezulte sidas paralizite sur rad<tld/>o
        <fnt>
          <aut>J. Camacho</aut>,
          <vrk><url ref="http://www.liberafolio.org/2006/kantojdeanteo">
          Poeto &ccirc;e mar&ccirc;ando kun Anubo</url></vrk>,
          <lok>libera folio, 2008-01-26</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bo">&#xF41;&#xF7C;&#xF62;&#xF0B;&#xF63;&#xF7C;&#xF0B;&#xF62;&#xF90;&#xF74;&#xF56;&#xF0B;&#xF40;&#xFB1;&#xF42;&#xF0B;</trd>
  <trd lng="de">Rollstuhl</trd>
  <trd lng="en">wheelchair</trd>
  <trd lng="es"><ind>silla</ind> de ruedas</trd>
  <trdgrp lng="fa">
    <trd>&a_S;&a_n;&a_d;&a_l;&f_y; &f_ch;&a_r;&a_kh;&a_d;&a_A;&a_r;</trd>,
    <trd>&f_ch;&a_r;&a_kh;&f_k;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>chaise roulante</trd>,
    <trd>fauteuil roulant</trd>
  </trdgrp>
  <trd lng="he">&kaf;&samekh;&alef; &gimel;&lamed;&gimel;&lamed;&yod;&fmem;</trd>
  <trd lng="hu">tol&oacute;sz&eacute;k</trd>
  <trd lng="id"><ind>kursi</ind> roda</trd>
  <trd lng="it"><ind>sedia</ind> a rotelle</trd>
  <trd lng="nl">rolstoel</trd>
  <trd lng="pl">w&oacute;zek inwalidzki</trd>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_s;&c_l;&c_o;-&c_k;&c_a;&c_t;&c_a;&c_l;&c_k;&c_a;</trd>
  <trd lng="sv">rullstol</trd>
</drv>
