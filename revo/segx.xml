<drv mrk="segx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="segx.0o.meblo">
    <dif>
      <ref tip="lst" cel="mebl.0o.MEBL" lst="voko:mebloj">Meblo</ref>
      destinita por sido de unu homo:
      <ekz>
        nun &ccirc;iu stari&gcirc;u sur sia <tld/>o, tralavu la gor&gcirc;on
        kaj, vigle svingante la flagon, koruse kunkantu
        <fnt>
          <aut>B. Golden</aut>,
          <vrk>Propono por elimini la lingvokaoson en E&ubreve;ropa
          Unio</vrk>,
          <bib>Monato</bib>,
          <lok>1997:5, p. 34a</lok>
         </fnt>.
      </ekz>
    </dif>
    <trd lng="es">silla</trd>
    <trd lng="fa">&a_S;&a_n;&a_d;&a_l;&f_y;</trd>
    <trd lng="fr">chaise</trd>
    <trd lng="it">sedia</trd>
    <trd lng="nl">stoel</trd>
    <trdgrp lng="pl">
      <trd>krzes&lstroke;o</trd>,
      <trd>stolec <klr>(przest.)</klr></trd>
  </trdgrp>
  </snc>
  <snc mrk="segx.0o.posteno">
    <dif>
      Parlamenta a&ubreve; alia balotdependa posteno en asembleo:
      <ekz>
        mankas al la kristandemokratoj unu <tld/>o
        <fnt>
          <aut>G. Van Damme</aut>,
          <vrk>Demokratio blokita</vrk>,
          <bib>Monato</bib>,
          <lok>2001:1, p. 8a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Sitz</trd>,
      <trd>Parlamentssitz</trd>
    </trdgrp>
    <trd lng="es">esca&ntilde;o</trd>
    <trd lng="fa">&f_k;&a_r;&a_s;&f_y;<klr tip="amb"> (&a_m;&a_j;&a_l;&a_s;&a_komo; &f_p;&a_A;&a_r;&a_l;&a_m;&a_A;&a_n;)</klr></trd>
    <trd lng="fr">si&egrave;ge<klr> (d'assembl&eacute;e)</klr></trd>
    <trd lng="it">seggio<klr> (d'assemblea)</klr></trd>
    <trd lng="nl">zetel</trd>
    <trd lng="pl">miejsce</trd>
  </snc>
  <trd lng="af">stoel</trd>
  <trd lng="am">&#x12C8;&#x1295;&#x1260;&#x122D;</trd>
  <trd lng="ar">&#x643;&#x631;&#x633;&#x64A;</trd>
