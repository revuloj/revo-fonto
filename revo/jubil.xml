<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>jubil</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="jubil.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="stl">POE</uzo>
  <snc>
    <dif>
      Elmontri grandan &gcirc;ojon per krioj, kantoj, dancoj kaj similaj:
      <ekz>
        jubilis homoj en islamaj landoj
        <fnt><aut>Stefan Maul</aut>: <vrk>Murdi je nomo de dio</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 10a, p. 5a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>exulter</trd>,
    <trd>jubiler</trd>,
    <trd>se <ind>r&eacute;jouir</ind></trd>
  </trdgrp>
</drv>

<drv mrk="jubil.0e">
  <kap><tld/>e</kap>
  <uzo tip="stl">POE</uzo>
  <snc>
    <dif>
      Kun vervaj signoj de &gcirc;ojo:
      <ekz>
        la oldulinaro <tld/>e bolis &leftquot;Vi pravegas!&rightquot;
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 56a, p. 247a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
</drv>

<drv mrk="jubil.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">POE</uzo>
  <snc>
    <dif>
      Granda elmontro de &gcirc;ojo.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>exultation</trd>,
    <trd>jubilation</trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
-->
</vortaro>