<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <rad>jubil</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="jubil.0i">
   <kap><tld/>i</kap>
   <gra><vspec>ntr</vspec></gra>
   <snc>
     <dif>
       Elmontri grandan &gcirc;ojon per krioj, kantoj, dancoj kaj similaj:
       <ekz>
         jubilis homoj en islamaj landoj
         <fnt><aut>Stefan Maul</aut>: <vrk>Murdi je nomo de dio</vrk>,
         <bib>Monato</bib>, <lok>jaro 2001a, numero 10a, p.
         5a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
  <trdgrp lng="be">
     <trd>&c_r;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
     <trd>&c_t;&c_r;&c_y;&c_ju;&c_m;&c_f;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_u;&c_r;&c_a;&c_ch;&c_y;&c_s;&c_t;&c_a; 
&c_s;&c_p;&c_r;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>
   </trdgrp>
  <trdgrp lng="ca">
     <trd>gaubar-se</trd>,
     <trd>exultar</trd>,
     <trd>sentir joia</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>jubilovat</trd>,
    <trd>jásat</trd>,
    <trd>plesat</trd>
  </trdgrp>
  <trd lng="es">estar jubiloso</trd>
  <trd lng="fa">&a_sh;&a_A;&a_d;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
     <trd>exulter</trd>,
     <trd>jubiler</trd>,
     <trd>se <ind>r&eacute;jouir</ind></trd>
   </trdgrp>
  <trdgrp lng="hu">
     <trd>ujjong</trd>,
     <trd>&ouml;rvend</trd>
     <trd>&ouml;rvendezik</trd>,
   </trdgrp>
  <trdgrp lng="it">
    <trd>giubilare</trd>,
    <trd>esultare</trd>
  </trdgrp>
  <trd lng="nl">jubelen</trd>
  <trdgrp lng="pt">
    <trd>jubilar-se</trd>,
    <trd>rejubilar-se</trd>
  </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_r;&c_a;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
     <trd>
&c_t;&c_o;&c_r;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_p;&c_r;&c_a;&c_z;&c_d;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>jasať</trd>,
    <trd>plesať</trd>,
    <trd>sláviť</trd>
  </trdgrp>
</drv>

<drv mrk="jubil.0e">
   <kap><tld/>e</kap>
   <snc>
     <dif>
       Kun vervaj signoj de &gcirc;ojo:
       <ekz>
         la oldulinaro <tld/>e bolis &leftquot;Vi pravegas!&rightquot;
         <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 56a, p. 247a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
  <trd lng="ca">gaiament</trd>
  <trd lng="cs">jubiloso</trd>
  <trdgrp lng="hu">
     <trd>ujjongva</trd>,
     <trd>&ouml;rvendezve</trd>
   </trdgrp>
  <trd lng="nl">jubelend</trd>
  <trdgrp lng="ru">
     <trd>&c_r;&c_a;&c_d;&c_o;&c_s;&c_t;&c_n;&c_o;</trd>,
     <trd>
&c_t;&c_o;&c_r;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_u;&c_ju;&c_shch;&c_je;</trd>
   </trdgrp>
  <trd lng="sk">jasavo</trd>
</drv>

<drv mrk="jubil.0o">
   <kap><tld/>o</kap>
   <snc>
     <dif>
       Granda elmontro de &gcirc;ojo.
     </dif>
   </snc>
  <trdgrp lng="ca">
     <trd>joia</trd>,
     <trd>exultaci&oacute;</trd>,
     <trd>goig</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>jásot</trd>,
    <trd>jásání</trd>
  </trdgrp>
  <trd lng="es">j&uacute;bilo</trd>
  <trdgrp lng="fr">
     <trd>exultation</trd>,
     <trd>jubilation</trd>
   </trdgrp>
  <trdgrp lng="hu">
     <trd>ujjong&aacute;s</trd>,
     <trd>&ouml;rvendez&eacute;s</trd>
   </trdgrp>
  <trdgrp lng="it">
    <trd>giubilo</trd>,
    <trd>esultanza</trd>
  </trdgrp>
  <trd lng="nl">gejubel</trd>
  <trdgrp lng="ru">
     <trd>&c_t;&c_r;&c_i;&c_u;&c_m;&c_f;</trd>,
     <trd>&c_t;&c_o;&c_r;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>jasanie</trd>,
    <trd>jasot</trd>
  </trdgrp>
</drv>
</art>
