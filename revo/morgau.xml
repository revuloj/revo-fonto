<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>morga&ubreve;</rad>
</kap>
<drv mrk="morgau.0">
   <kap><ofc>*</ofc><tld/></kap>
   <gra><vspec>adverbo</vspec></gra>
   <snc mrk="morgau.0.KOMUNE">
     <dif>
       En la tago tuj venonta post la hodia&ubreve;a:
       <ekz>
         hodia&ubreve; por mono, <tld/> senpage<fnt>Z</fnt>;
       </ekz>
       <ekz>
         &ccirc;iu <tld/> havas sian zorgon
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         <ind><tld/> estas la amata tago de mallaboruloj</ind>
         <trd lng="fr">l'avenir appartient &agrave; ceux qui se l&egrave;vent
t&ocirc;t</trd>
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         vi &ccirc;iam promesas al mi nur <tld/>a&jcirc;ojn.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_z;&c_a;&c_w;&c_t;&c_r;&c_a;</trd>
   <trd lng="br">warc'hoazh</trd>
   <trd lng="cs">z&iacute;tra</trd>
   <trd lng="de">morgen</trd>
   <trd lng="en">tomorrow</trd>
   <trd lng="fa">&a_f;&a_r;&a_d;&a_A;</trd>
   <trd lng="fi">huomenna</trd>
   <trd lng="fr">demain</trd>
   <trd lng="he">&mem;&het;&resh;</trd>
   <trd lng="hu">holnap</trd>
   <trd lng="id">besok</trd>
   <trd lng="ja">&#26126;&#26085; [<ind>&#12354;&#12375;&#12383;</ind>]</trd>
   <trd lng="nl">morgen</trd>
   <trd lng="pl">jutro</trd>
   <trd lng="pt">amanh&atilde;</trd>
   <trd lng="ro">m&acirc;ine</trd>
   <trd lng="ru">&c_z;&c_a;&c_v;&c_t;&c_r;&c_a;</trd>
   <trd lng="sk">zajtra</trd>
   <trd lng="sv">i <ind>morgon</ind></trd>
   <trd lng="tr">yar&inodot;n</trd>
   <trd lng="vo">model</trd>
</drv>

<drv mrk="morgau.0a">
   <kap><tld/>a</kap>
   <snc mrk="morgau.0a.KOMUNE">
     <dif>
       Okazonta <tld/>:
       <ekz>
         trovi&gcirc;u dum la <tld/>a mateno en la iama apartamento de
         fra&ubreve;lino de Savinako
         <fnt>
           <aut>Henri Vallienne</aut>,
           <vrk>Kastelo de Prelongo</vrk>,
           <lok>1907</lok>
         </fnt>;
       </ekz>
       <ekz>
         &ccirc;u vi &ccirc;eestos la <tld/>an feston?
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="fi">
     <trd>huominen <klr tip="ind"> (adj.)</klr></trd>,
     <trd>huomis-</trd>
   </trdgrp>
   <trd lng="fr">de <ind>demain</ind></trd>
</drv>

<drv mrk="morgau.0o">
   <kap><tld/>o</kap>
   <snc mrk="morgau.0o.KOMUNE">
     <ref tip="dif" cel="tag.posta0o">posta tago</ref>
     <ekz>
       mi vizitos lin la <tld/>on post lia reveno.
     </ekz>
   </snc>
   <trdgrp lng="br">
     <trd>tronoz</trd>,
     <trd>deiz war-lerc'h</trd>
   </trdgrp>
   <trdgrp lng="fi">
     <trd>huominen <klr tip="ind"> (subst.)</klr></trd>,
     <trd>huomisp&auml;iv&auml;</trd>,
     <trd>seuraava p&auml;iv&auml;</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>lendemain</trd>,
     <trd>jour suivant</trd>
   </trdgrp>
   <trd lng="pt">o <ind>amanh&atilde;</ind></trd>
   <trdgrp lng="ru">
     <trd>&c_z;&c_a;&c_v;&c_t;&c_r;&c_a;&c_sh;&c_n;&c_i;&c_j;
&c_d;&c_je;&c_n;&c_mol;</trd>,
     <trd>&c_s;&c_l;&c_je;&c_d;&c_u;&c_ju;&c_shch;&c_i;&c_j;
&c_d;&c_je;&c_n;&c_mol;</trd>
   </trdgrp>
</drv>

<drv mrk="morgau.post0">
   <kap>post<tld/></kap>
   <snc mrk="morgau.post0.KOMUNE">
     <dif>
       En la dua tago post la hodia&ubreve;a.
     </dif>
   </snc>
   <trd
lng="be">&c_p;&c_a;&c_s;&c_mol;&c_l;&c_ja;&c_z;&c_a;&c_w;&c_t;&c_r;&c_a;</trd>
   <trdgrp lng="br">
     <trd>goude <ind>warc'hoazh</ind></trd>,
     <trd>antronoz all</trd>
   </trdgrp>
   <trd lng="de">&uuml;bermorgen</trd>
   <trd lng="fa">&f_p;&a_s;&zwnj;&a_f;&a_r;&a_d;&a_A;</trd>
   <trd lng="fi">ylihuomenna</trd>
   <trd lng="fr">apr&egrave;s-demain</trd>
   <trd lng="he">&mem;&het;&resh;&tav;&yod;&yod;&fmem;</trd>
   <trd lng="id">lusa</trd>
   <trd lng="ja">&#26126;&#24460;&#26085;
[<ind>&#12354;&#12373;&#12387;&#12390;</ind>]</trd>
   <trd lng="nl">overmorgen</trd>
   <trd lng="pl">pojutrze</trd>
   <trd lng="pt">depois de amanh&atilde;</trd>
   <trd lng="ro">poim&acirc;ine</trd>
   <trd lng="ru">&c_p;&c_o;&c_s;&c_l;&c_je;&c_z;&c_a;&c_v;&c_t;&c_r;&c_a;</trd>
   <trd lng="sk">pozajtra</trd>
   <trd lng="sv">i <ind>&ouml;vermorgon</ind></trd>
   <trd lng="tr">&ouml;b&uuml;r g&uuml;n</trd>
</drv>
</art>
