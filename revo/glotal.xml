<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: glotal.xml,v 1.8 2024/01/11 07:25:11 revo Exp $">
<kap>
    <rad>glotal</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="glotal.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">FON</uzo>
    <dif>
      <ref tip="dif" cel="glot.0akonsonanto">Glota konsonanto</ref>:
      <ekz>
        la afara lingvo apartenas al la ku&scirc;ida lingvofamilio kaj sonas
        pro siaj diversaj
        faringaloj kaj <tld/>oj ekstreme ra&ubreve;ke
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref="https://eo.wikipedia.org/?curid=229672">Afaroj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="gutura.0o">guturalo</ref>
  </snc>
  <trd lng="be">&c_g;&c_l;&c_ja;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_y; <ind>&c_z;&c_y;&c_ch;&c_n;&c_y;</ind></trd>
  <trd lng="cs">r&aacute;z</trd>
  <trd lng="de">Glottal</trd>
  <trd lng="fr">glottale<klr> (substantif)</klr></trd>
  <trd lng="pl">sp&oacute;&lstroke;g&lstroke;oska krtaniowa</trd>
  <trd lng="ro">consoan&abreve; glotal&abreve;</trd>
  <trd lng="sk">r&aacute;z</trd>
</drv>
</art>

