<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>glotal</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="glotal.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">FON</uzo>
    <dif>
    Parolsono, konsonanto artikulaciata per la gloto (h).
    </dif>
    <refgrp tip="vid">
  <ref cel="labial.0o">labialo</ref>,
  <ref cel="dental.0o">dentalo</ref>,
  <ref cel="palata.0o">palatalo</ref>,
  <ref cel="velar.0o">velaro</ref>,
    </refgrp>
  </snc>
  <ref tip="sin" cel="plosiv.glota0o">glota plosivo</ref>
  <trd lng="fr">glottale</trd>
</drv>
</art>

