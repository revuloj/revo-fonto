<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>glotal</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="glotal.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">FON</uzo>
    <dif>
      <ref tip="dif" cel="glot.0akonsonanto">Glota konsonanto</ref>:
      <ekz>
        la afara lingvo apartenas al la ku&scirc;ida lingvofamilio kaj sonas pro siaj diversaj
        faringaloj kaj <tld/>oj ekstreme ra&ubreve;ke
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=229672">Afaroj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="gutura.0o">guturalo</ref>
  </snc>
  <trd lng="cs">r&aacute;z</trd>
  <trd lng="de">Glottal</trd>
  <trd lng="fr">glottale<klr> (substantif)</klr></trd>
  <trd lng="sk">r&aacute;z</trd>
</drv>
</art>

