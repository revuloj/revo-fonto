<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: celastr.xml,v 1.16 2023/10/31 13:37:03 revo Exp $">
<kap>
  <rad>celastrac</rad>/oj
</kap>
<drv mrk="celastr.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref>
      de <ref tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Celastraceae</trd>):
      <ekz>
        celastro estas plantgenro de grimpantaj arbustoj el la familio de la <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=565209">Celastro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="evonim.0o">evonimo</ref>
  </snc>
  <trd lng="ca">celastr&agrave;cies</trd>
  <trd lng="de">Spindelbaumgew&auml;chse</trd>
  <trd lng="fr">c&eacute;lastrac&eacute;es</trd>
  <trd lng="hu">kecsker&aacute;g&oacute;f&eacute;l&eacute;k</trd>
  <trd lng="it">celastracee</trd>
  <trd lng="pl">trzmielinowate</trd>
  <trd lng="pt">celastr&aacute;ceas</trd>
  <trd lng="ru">&c_b;&c_je;&c_r;&c_je;&c_s;&c_k;&c_l;&c_je;&c_t;&c_o;&c_v;&c_y;&c_je;
  <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&#x431;&#x440;&#x443;&#x441;&#x43B;&#x438;&#x43D;&#x43E;&#x432;&#x456;</trd>
  <trd lng="zh">&#x536B;&#x77DB;&#x76EE; <pr>w&#xE8;im&#xE1;om&#xF9;</pr></trd>
</drv>
</art>
