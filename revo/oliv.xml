<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>oliv</rad>/o
</kap>
<drv mrk="oliv.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <fnt><bib>KrED</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="frukt.0o.KUI">Frukto</ref> el 
      <ref tip="malprt" cel="oliv.0arbo"><tld/>arbo</ref>,
      kiun oni man&gcirc;as preparitan kaj kiu liveras bonegan oleon:
      <ekz>
        <ind><tld/>kolora</ind> jupo.                              
        <trd lng="br">a-liv gant an <ind>olivez</ind></trd>
        <trd lng="be">&c_a;&c_l;&c_ib;&c_w;&c_k;&c_a;&c_v;&c_y; <klr
        tip="amb">(&c_k;&c_o;&c_l;&c_je;&c_r;)</klr></trd>
        <trdgrp lng="fa">
          <trd>&a_z;&f_y;&a_t;&a_w;&a_n;&f_y;&zwnj;&a_r;&a_n;&f_g;</trd>,
          <trd>&a_z;&f_y;&a_t;&a_w;&a_n;&f_y;</trd>
        </trdgrp>
        <trd lng="pl">oliwkowy<klr> (kolor)</klr></trd>
        <trd lng="ru">&c_o;&c_l;&c_i;&c_v;&c_k;&c_o;&c_v;&c_y;&c_j; <klr
        tip="amb">(&c_o; &c_c;&c_v;&c_je;&c_t;&c_je;)</klr></trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_l;&c_ib;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a; <klr
    tip="amb">(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  </trdgrp>                    
  <trd lng="br">olivezenn<klr tip="ind"> (frouezhenn)</klr></trd>
  <trd lng="de">Olive</trd>
  <trd lng="el">&epsilon;&lambda;&alfa;&jota;&alfa_ton;</trd>
  <trd lng="en">olive</trd>
  <trd lng="fa">&a_z;&f_y;&a_t;&a_w;&a_n;</trd>
  <trd lng="fr">olive</trd>
  <trd lng="he">&zayin;&yod;&tav;</trd>
  <trd lng="nl">olijf</trd>
  <trd lng="hu">olajbogy&oacute;</trd>
  <trd lng="pl">oliwka</trd>
  <trdgrp lng="pt">
    <trd>azeitona</trd>,
    <trd>oliva</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_l;&c_i;&c_v;&c_a;</trd>,
    <trd>&c_o;&c_l;&c_i;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_s;&c_l;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="sv">oliv</trd>
  <trd lng="tr">zeytin</trd>
</drv>

<drv mrk="oliv.0arbo">
  <kap><tld/>arbo, <tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <fnt><bib>KrED</bib></fnt>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="ole1.0o">oleo</ref>
      (<trd lng="la">Olea europea</trd>).
    </dif>
  </snc>              
  <trdgrp lng="be">
    <trd>&c_a;&c_l;&c_ib;&c_w;&c_k;&c_a;&c_v;&c_a;&c_je;
    &c_d;&c_r;&c_e;&c_v;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a; <klr
    tip="amb">(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>gwezenn-olivez</trd>,
    <trd>olivezenn<klr tip="ind"> (gwezenn)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Olivenbaum</trd>,
     <trd>&Ouml;lbaum</trd>
  </trdgrp>
  <trd lng="el">&epsilon;&lambda;&alfa;&jota;&alfa_ton;</trd>
  <trd lng="fa">&a_d;&a_r;&a_kh;&a_t; <ind>&a_z;&f_y;&a_t;&a_w;&a_n;</ind></trd>
  <trd lng="fr">olivier</trd>
  <trd lng="he">&ayin;&ftsadi; &zayin;&yod;&tav;</trd>
  <trd lng="hu">olajfa</trd>
  <trd lng="la">Olea europaea</trd>
  <trd lng="nl">olijfboom</trd>
  <trd lng="pl">drzewo <ind>oliwne</ind></trd>
  <trdgrp lng="pt">
    <trd>oliva</trd>,
    <trd>oliveira</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_l;&c_i;&c_v;&c_k;&c_o;&c_v;&c_o;&c_je;
  &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  <trd lng="sv">olivtr&auml;d</trd>
</drv>
</art>
