<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>oliv</rad>/o
</kap>
<drv mrk="oliv.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Frukto el <ref tip="malprt" cel="oliv.0arbo"><tld/>arbo</ref>,
      kiun oni man&gcirc;as preparitan kaj kiu liveras bonegan oleon:
      <ekz>
        <tld/>kolora jupo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Olive</trd>
  <trd lng="fr">olive</trd>
  <trd lng="tr">zeytin</trd>
</drv>
<drv mrk="oliv.0arbo">
  <kap><tld/>arbo, <tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="ole1.0o">oleo</ref>
      (<trd lng="la">Olea europea</trd>).
    </dif>
  </snc>
  <trd lng="fr">olivier</trd>
</drv>
</art>
