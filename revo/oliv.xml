<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>oliv</rad>/o
</kap>
<drv mrk="oliv.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <fnt><bib>KrED</bib></fnt>
  <snc>
    <dif>
      Frukto el <ref tip="malprt" cel="oliv.0arbo"><tld/>arbo</ref>,
      kiun oni man&gcirc;as preparitan kaj kiu liveras bonegan oleon:
      <ekz>
        <ind><tld/>kolora</ind> jupo.
        <trd lng="ru">&c_o;&c_l;&c_i;&c_v;&c_k;&c_o;&c_v;&c_y;&c_j; <klr tip="amb">(&c_o; &c_c;&c_v;&c_je;&c_t;&c_je;)</klr></trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Olive</trd>
  <trd lng="el">&epsilon;&lambda;&alfa;&jota;&alfa_ton;</trd>
  <trd lng="en">olive</trd>
  <trd lng="fr">olive</trd>
  <trd lng="he">&zayin;&yod;&tav;</trd>
  <trd lng="hu">olijf</trd>
  <trd lng="nl">olajbogy&oacute;</trd>
  <trdgrp lng="pt">
    <trd>azeitona</trd>,
    <trd>oliva</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_l;&c_i;&c_v;&c_a;</trd>,
    <trd>&c_o;&c_l;&c_i;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_s;&c_l;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="sv">oliv</trd>
  <trd lng="tr">zeytin</trd>
</drv>

<drv mrk="oliv.0arbo">
  <kap><tld/>arbo, <tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <fnt><bib>KrED</bib></fnt>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="ole1.0o">oleo</ref>
      (<trd lng="la">Olea europea</trd>).
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Olivenbaum</trd>,
     <trd>&Ouml;lbaum</trd>
  </trdgrp>
  <trd lng="el">&epsilon;&lambda;&alfa;&jota;&alfa_ton;</trd>
  <trd lng="fr">olivier</trd>
  <trd lng="he">&ayin;&ftsadi; &zayin;&yod;&tav;</trd>
  <trd lng="hu">olajfa</trd>
  <trd lng="la">Olea europaea</trd>
  <trd lng="nl">olijfboom</trd>
  <trdgrp lng="pt">
    <trd>oliva</trd>,
    <trd>oliveira</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_l;&c_i;&c_v;&c_k;&c_o;&c_v;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
</drv>
</art>
