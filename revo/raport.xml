<drv mrk="raport.0o">
  <kap><tld/>o</kap>
  <snc mrk="raport.0o.KOMUNE">
    <dif>
      Transdono de akirita informo.
    </dif>
    <subsnc mrk="raport.0o.vocxa">
      <dif>
        <ref tip="super" cel="klar.0igo">Klarigo</ref> pri <ref cel="spert.0o">sperto</ref> al iu, 
        kiu ne mem spertis:
        <ekz>
          mi alportis al li <tld/>on la&ubreve; mia konscienco
          <fnt>
            <bib>MT</bib>
            <lok>&Jos; 14:7</lok>
          </fnt>;
        </ekz>
        <ekz>
          postulu de ili kalkulan <tld/>on pri la mono, transdonita en iliajn manojn
          <fnt>
            <bib>MT</bib>
            <lok>&Re2; 22:7</lok>
          </fnt>;
        </ekz>
        <ekz>
          la <tld/>o pri tio estis a&ubreve;dita de la eklezio
          <fnt>
            <bib>NT</bib>
            <lok>&Ago; 11:22</lok>
          </fnt>;
        </ekz>
        <ekz>
          <ind>ne &ccirc;iu <tld/>o estas vera vorto</ind>
          <fnt><bib>PrV</bib></fnt>;
          <trd lng="hu">nem minden h&iacute;rnek igaz a szava</trd>
        </ekz><ekz>
          fari al iu fidelan <tld/>on.
        </ekz>
      </dif>
      <trd lng="hu">besz&aacute;mol&oacute;</trd>
      <trdgrp lng="pl">
        <trd>raport</trd>,
        <trd>relacja</trd>,
        <trd>sprawozdanie</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="raport.0o.dokumenta">
      <dif>
        <ref tip="super" cel="dokume.0o">Dokumento</ref> prezentanta rezulton de tema esploro:
        <ekz>
          li estis leginta neniun <tld/>on
          <fnt>
            <bib>Metrop</bib>
          </fnt>;
        </ekz>
        <ekz>
          <nom>Vitalij</nom> estis leganta la <tld/>on pri sia voja&gcirc;o
          <fnt>
            <bib>Metrop</bib>
          </fnt>;
        </ekz>
        <ekz>
          la komandanto trovi&gcirc;is <klr>[&#x2026;]</klr> laborante la redakton de <tld/>o, kiam iu
          frapis sur lian pordon
          <fnt>
            <bib>KPr</bib>
          </fnt>;
        </ekz>
        <ekz>
          gazet<tld/>o
          <fnt>
            <bib>LSB</bib>
          </fnt>;
        </ekz>
        <ekz>
          la nekropsia <tld/>o kaj la <tld/>o de la kuracisto <klr>[&#x2026;]</klr> sciigis
          nenion neatenditan
          <fnt>
            <bib>ChB</bib>
          </fnt>;
        </ekz>
        <ekz>
          la ar&hcirc;ivaj <tld/>oj rivelis neniun interesan fakton
          <fnt>
            <bib>ChB</bib>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="disert.0ajxo">diserta&jcirc;o</ref>,
        <ref cel="refer.0ajxo">refera&jcirc;o</ref>,
        <ref cel="refera.0o">referato</ref>,
        <ref cel="protok.0o.raporto">protokolo<sncref/></ref>
      </refgrp>
      <trdgrp lng="bg">
        <trd>&c_d;&c_o;&c_k;&c_l;&c_a;&c_d;</trd>,
        <trd>&c_r;&c_a;&c_p;&c_o;&c_r;&c_t;</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Bericht</trd>,
        <trd>Referat</trd>,
        <trd>Vortrag</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>jelent&eacute;s</trd>,
        <trd>tud&oacute;s&iacute;t&aacute;s</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>raport</trd>,
        <trd>meldunek</trd>
      </trdgrp>
      <trd lng="pt">relato</trd>
    </subsnc>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_r;&c_a;&c_v;&c_a;&c_z;&c_d;&c_a;&c_ch;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_p;&c_a;&c_r;&c_t;</trd>,
    <trd>&c_r;&c_e;&c_p;&c_a;&c_r;&c_t;&c_a;&c_zh;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_r;&c_je;&c_p;&c_o;&c_r;&c_t;&c_a;&c_zh;</trd>,
    <trd>&c_o;&c_t;&c_ch;&c_je;&c_t;</trd>
  </trdgrp>
  <trd lng="bo">&t_sa;&t_nyata;&t_na;&t_tsheg;&t_zha;&t_shabchu;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>meldunk <klr>(zast.)</klr></trd>,
    <trd>meldu&ncaron;k (zast.)</trd>,
    <trd>raport</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bericht</trd>
  </trdgrp>
  <trd lng="en">report</trd>
  <trdgrp lng="es">
    <trd>cuento</trd>,
    <trd>relato</trd>
  </trdgrp>
  <trd lng="fa">&f_g;&a_z;&a_A;&a_r;&a_sh;</trd>
  <trdgrp lng="fr">
    <trd>compte rendu</trd>,
    <trd>rapport<klr tip="amb"> (compte rendu)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5831;&#x544A;  <pr>&#x307B;&#x3046;&#x3053;&#x304F;</pr></trd>,
    <trd>&#x5831;&#x9053;  <pr>&#x307B;&#x3046;&#x3069;&#x3046;</pr></trd>,
    <trd>&#x5831;&#x544A;&#x66F8;  <pr>&#x307B;&#x3046;&#x3053;&#x304F;&#x3057;&#x3087;</pr></trd>,
    <trd>&#x7B54;&#x7533;&#x66F8;  <pr>&#x3068;&#x3046;&#x3057;&#x3093;&#x3057;&#x3087;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verslag</trd>,
    <trd>rapport</trd>
  </trdgrp>
  <trd lng="pt">relato</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_k;&c_l;&c_a;&c_d;</trd>,
    <trd>&c_o;&c_t;&c_ch;&c_jo;&c_t;</trd>,
    <trd>&c_r;&c_a;&c_p;&c_o;&c_r;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hl&aacute;senie</trd>,
    <trd>refer&aacute;t</trd>,
    <trd>report&aacute;&zcaron;</trd>,
    <trd>spr&aacute;va</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_o;&c_p;&c_o;&c_v;&c_ib;&c_d;&c_mol;</trd>,
    <trd>&c_z;&c_v;&c_ib;&c_t;</trd>,
    <trd>&c_r;&c_a;&c_p;&c_o;&c_r;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5F59;&#x5831; <pr>hu&#xEC;b&#xE0;o</pr></trd>,
    <trd>&#x6C47;&#x62A5; <pr>hu&#xEC;b&#xE0;o</pr></trd>,
    <trd>&#x5831;&#x544A; <pr>b&#xE0;og&#xE0;o</pr></trd>,
    <trd>&#x62A5;&#x544A; <pr>b&#xE0;og&#xE0;o</pr></trd>,
    <trd>&#x5831; <pr>b&#xE0;o</pr></trd>,
    <trd>&#x62A5; <pr>b&#xE0;o</pr></trd>,
    <trd>&#x5831;&#x9053; <pr>b&#xE0;od&#xE0;o</pr></trd>,
    <trd>&#x62A5;&#x9053; <pr>b&#xE0;od&#xE0;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="raport.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="raport.0isto.INF">
    <dif>
      <ref tip="super" cel="jxurna.0isto.INF">&Jcirc;urnalisto</ref>,
      kiu <tld/>as pri okaza&jcirc;oj, aktuala&jcirc;oj:
      <ekz>
        <ind>eksterlandaj <tld/>istoj</ind>
        <fnt>
          <bib>EeP</bib>
        </fnt>;
        <trd lng="de">Auslandsberichterstatter</trd>
      </ekz>
      <ekz>
        <ind>loka <tld/>isto</ind>
        <fnt>
          <bib>Kon11</bib>
        </fnt>;
        <trd lng="de">Lokalberichterstatter</trd>
      </ekz>
      <ekz>
        <tld/>istoj de Esperanto-gazetoj
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        iama <ind>fotoraportisto</ind> de la <nom>Stern</nom> <klr>[&#x2026;]</klr> kunigis ne nur
        kolekta&jcirc;on de
        fotoj, sed unikan tuta&jcirc;on de magazinoj kaj &jcirc;urnaloj
        <fnt>
          <bib>MD02</bib>
        </fnt>;
        <trd lng="de">Fotoreporter</trd>
      </ekz>
      <ekz>
        <ind>radio-televidaj <tld/>istoj</ind>
        <fnt>
          <bib>EeP</bib>
        </fnt>;
        <trd lng="de">Rundfunkreporter</trd>
      </ekz>
      <ekz>
        inter la mortintoj trovi&gcirc;is konataj <ind>televidaj <tld/>istoj</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Keyhan Sayadpour</aut>
          <vrk><url ref="&Monato;/2006/008856.php">Embargo kontribuis al katastrofo</url></vrk>
          <lok>2006</lok>
        </fnt>.
        <trd lng="de">Fernsehreporter</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_p;&c_a;&c_r;&c_t;&c_e;&c_r;</trd>,
    <trd>&c_d;&c_a;&c_k;&c_l;&c_a;&c_d;&c_ch;&c_y;&c_k;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_v;&c_ja;&c_sh;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_r;&c_je;&c_p;&c_o;&c_r;&c_t;&c_je;&c_r;</trd>,
