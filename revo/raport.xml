    <trd>informar</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&f_g;&a_z;&a_A;&a_r;&a_sh; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&f_g;&a_z;&a_A;&a_r;&a_sh; &a_d;&a_A;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>pr&eacute;senter un <ind>rapport</ind></trd>,
    <trd>rapporter<klr> (t&eacute;moigner de)</klr></trd>,
    <trd>t&eacute;moigner de</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>jelent</trd>,
    <trd>besz&aacute;mol</trd>,
    <trd>tud&oacute;s&iacute;t</trd>,
    <trd>elmes&eacute;l</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5831;&#x544A;&#x3059;&#x308B;  <pr>&#x307B;&#x3046;&#x3053;&#x304F;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x4F1D;&#x3048;&#x308B;  <pr>&#x3064;&#x305F;&#x3048;&#x308B;</pr></trd>,
    <trd>&#x5831;&#x9053;&#x3059;&#x308B;  <pr>&#x307B;&#x3046;&#x3069;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x7B54;&#x7533;&#x3059;&#x308B;  <pr>&#x3068;&#x3046;&#x3057;&#x3093;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verslag uitbrengen</trd>,
    <trd>raporteren</trd>
  </trdgrp>
  <trd lng="pt">reportar</trd>
  <trdgrp lng="sk">
