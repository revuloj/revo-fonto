<drv mrk="raport.mis0o">
  <kap>
    mis<tld/>o
  </kap>
  <snc>
    <dif>
      Mal&gcirc;usta, malvera <tld/>o:
      <ekz>
        eblas anka&ubreve; eraroj a&ubreve; mis<tld/>oj, kiuj povas ka&ubreve;zi enlistigon de serviloj, kiuj
        ne sendas rubpo&scirc;ton
        <fnt>
          <bib>Monato</bib>
          <aut>Franck Arnaud</aut>
          <vrk><url ref="&Monato;/2003/008177.html">Kia solvo por rubpo&scirc;to?</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Falschmeldung</trd>
  <trd lng="en">false report</trd>
  <trd lng="pl">dezinformacja</trd>
  <trd lng="ro">dezinformare</trd>
</drv>






