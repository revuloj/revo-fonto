<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>1</ofc> <rad>ovaci</rad>/o</kap>
<drv mrk="ovaci.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;e antikvaj romanoj, ceremonio malpli solena ol triumfo.
    </dif>
  </snc>
  <snc>
    <dif>
      Honora entuziasma amasa akcepto al iu.
    </dif>
    <trd lng="es">ovaci&oacute;n</trd>
    <trd lng="pt">ova&ccedil;&atilde;o</trd>
  </snc>
  <trd lng="be">&c_a;&c_v;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Ovation</trd>,
    <trd>Huldigung</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ov&aacute;ci&oacute;</trd>,
    <trd>&uuml;dvrivalg&aacute;s</trd>,
    <trd>&eacute;ljenz&eacute;s</trd>
  </trdgrp>
  <trd lng="nl">ovatie</trd>
  <trd lng="ru">&c_o;&c_v;&c_a;&c_c;&c_i;&c_ja;</trd>
</drv>

<drv mrk="ovaci.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on al iu.
    </dif>
  </snc>
  <trd lng="de">zujubeln</trd>
  <trd lng="es">ovacionar</trd>
  <trd lng="hu">&eacute;ljenez</trd>
  <trdgrp lng="nl">
    <trd>een <ind>ovatie</ind> geven</trd>,
    <trd>iem. <ind>toejuichen</ind></trd>
  </trdgrp>
  <trd lng="pt">ovacionar</trd>
</drv>
</art>
