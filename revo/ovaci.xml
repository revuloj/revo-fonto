<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ovaci.xml,v 1.25 2024/01/30 16:09:26 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>ovaci</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ovaci.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ovaci.0i.MOR">
    <dif>
      Entuziasme amase honore akcepti a&ubreve; <ref tip="super" cel="aplaux.0i">apla&ubreve;di</ref>:
      <ekz>
        oni starante <tld/>is al li
        <fnt>
          <bib>Monato</bib>
          <aut>Angela Tellier</aut>
          <vrk><url ref="&Monato;/2004/008306.html">La&ubreve;proverbaj fabeloj</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi a&ubreve;dis? &dash; kia dramo! <klr>[&#x2026;]</klr> &ccirc;iuj, &ccirc;iuj, &dash; jen surstrate, jen endome
        <tld/>as
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
          <lok>Telegramo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la orkestro restis sur la scenejo, la tuta publiko ekstaris kaj freneze <tld/>is
        &gcirc;in
        <fnt>
          <bib>Monato</bib>
          <aut>Julia Sigmond</aut>
          <vrk><url ref="&Monato;/2011/010147.php">Georgo</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kant.glor0o">glorkanti</ref>,
      <ref cel="hura.0i">hurai</ref>,
      <ref cel="jubil.0i">jubili</ref>
    </refgrp>
   </snc>
   <trdgrp lng="de">
    <trd>zujubeln</trd>,
    <trd>bejubeln</trd>
  </trdgrp>
   <trd lng="es">ovacionar</trd>
   <trd lng="fr">ovationner</trd>
   <trd lng="hu">&eacute;ljenez</trd>
   <trdgrp lng="ja">
    <trd>&#x5927;&#x559D;&#x91C7;&#x3059;&#x308B;  <pr>&#x3060;&#x3044;&#x304B;&#x3063;&#x3055;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x71B1;&#x70C8;&#x306B;&#x6B53;&#x8FCE;&#x3059;&#x308B;  <pr>&#x306D;&#x3064;&#x308C;&#x3064;&#x306B;&#x304B;&#x3093;&#x3052;&#x3044;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
     <trd>een <ind>ovatie</ind> geven</trd>,
     <trd>iem. <ind>toejuichen</ind></trd>
   </trdgrp>
   <trdgrp lng="pl">
     <trd>bi&cacute;  <ind>brawo</ind></trd>,
     <trd>klaska&cacute;</trd>
   </trdgrp>
   <trd lng="pt">ovacionar</trd>
   <trd lng="ro">aplauda</trd>
  <trdgrp lng="uk">
    <trd>&#x443;&#x43B;&#x430;&#x448;&#x442;&#x443;&#x432;&#x430;&#x442;&#x438; &#x43E;&#x432;&#x430;&#x446;&#x456;&#x44E;</trd>,
    <trd>&#x432;&#x456;&#x442;&#x430;&#x442;&#x438; &#x43E;&#x432;&#x430;&#x446;&#x456;&#x44F;&#x43C;&#x438;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x559D;&#x5F69; <pr>h&#x113;c&#x1CE;i</pr></trd>,
    <trd>&#x559D;&#x91C7; <pr>h&#xE8;c&#x1CE;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ovaci.0o">
  <kap><tld/>o</kap>
  <snc mrk="ovaci.0o.MOR">
    <dif>
      Entuziasma amasa honora <ref tip="super" cel="akcept.0o.ago">akcepto</ref> 
      a&ubreve; apla&ubreve;do:
      <ekz>
        la koncerto okazis, kun granda sukceso kaj fina <tld/>o kaj entuziasmaj la&ubreve;doj
        fare de la lokaj kritikistoj
        <fnt>
          <bib>Monato</bib>
          <aut>Julia Sigmond kaj Sen Rodin</aut>
          <vrk><url ref="&Monato;/2010/009988.php">La mal&jcirc;aluzo de Klorinda</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
     </dif>
     <trd lng="pt">ova&ccedil;&atilde;o</trd>
   </snc>
   <snc mrk="ovaci.0o.HIS">
     <uzo tip="fak">HIS</uzo>
     <dif>
       &Ccirc;e antikvaj romianoj, ceremonio malpli solena ol
       <ref tip="vid" cel="triumf.0o.HIS">triumfo</ref>.
     </dif>
   </snc>
  <trd lng="be">&c_a;&c_v;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>oslava</trd>,
    <trd>ovace</trd>,
    <trd>p&rcaron;iv&iacute;t&aacute;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Ovation</trd>,
     <trd>Huldigung</trd>
   </trdgrp>
  <trd lng="en">ovation</trd>
  <trd lng="es">ovaci&oacute;n</trd>
  <trd lng="fr">ovation</trd>
  <trdgrp lng="hu">
     <trd>ov&aacute;ci&oacute;</trd>,
     <trd>&uuml;dvrivalg&aacute;s</trd>,
     <trd>&eacute;ljenz&eacute;s</trd>
   </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5927;&#x559D;&#x91C7;  <pr>&#x3060;&#x3044;&#x304B;&#x3063;&#x3055;&#x3044;</pr></trd>,
    <trd>&#x71B1;&#x70C8;&#x306A;&#x6B53;&#x8FCE;  <pr>&#x306D;&#x3064;&#x308C;&#x3064;&#x306A;&#x304B;&#x3093;&#x3052;&#x3044;</pr></trd>,
    <trd>&#x7565;&#x5F0F;&#x306E;&#x51F1;&#x65CB;&#x795D;&#x3044;  <pr>&#x308A;&#x3083;&#x304F;&#x3057;&#x304D;&#x306E;&#x304C;&#x3044;&#x305B;&#x3093;&#x3044;&#x308F;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">ovatie</trd>
  <trdgrp lng="pl">
    <trd>oklaski {pl}</trd>,
    <trd>owacja</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>aplauze</trd>,
    <trd>ova&#x21B;ie</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_v;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>oslava</trd>,
    <trd>ov&aacute;cia</trd>,
    <trd>priv&iacute;tanie</trd>
  </trdgrp>
  <trd lng="uk">&#x43E;&#x432;&#x430;&#x446;&#x456;&#x44F;</trd>
</drv>
</art>
