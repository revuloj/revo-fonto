<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>1</ofc> <rad>ovaci</rad>/o</kap>
<drv mrk="ovaci.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;e antikvaj romanoj, ceremonio malpli solena ol triumfo.
    </dif>
  </snc>
  <snc>
    <dif>
      Honora entuziasma amasa akcepto al iu.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Ovation</trd>,
    <trd>Huldigung</trd>
  </trdgrp>
  <trd lng="nl">ovatie</trd>
  <trd lng="ru">&c_o;&c_v;&c_a;&c_c;&c_i;&c_ja;</trd>
</drv>

<drv mrk="ovaci.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on al iu.
    </dif>
  </snc>
  <trd lng="de">zujubeln</trd>
</drv>
</art>
