<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ovaci.xml,v 1.27 2025/03/27 06:39:12 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>ovaci</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="ovaci.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ovaci.0i.MOR">
    <dif>
      Entuziasme amase honore akcepti a&ubreve; <ref tip="super" cel="aplaux.0i">apla&ubreve;di</ref>:
      <ekz>
        oni starante <tld/>is al li
        <fnt>
          <bib>Monato</bib>
          <aut>Angela Tellier</aut>
          <vrk><url ref="&Monato;/2004/008306.html">La&ubreve;proverbaj fabeloj</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi a&ubreve;dis? &dash; kia dramo! <klr>[&#8230;]</klr> &ccirc;iuj, &ccirc;iuj, &dash; jen surstrate, jen endome
        <tld/>as
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
          <lok>Telegramo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la orkestro restis sur la scenejo, la tuta publiko ekstaris kaj freneze <tld/>is
        &gcirc;in
        <fnt>
          <bib>Monato</bib>
          <aut>Julia Sigmond</aut>
          <vrk><url ref="&Monato;/2011/010147.php">Georgo</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kant.glor0o">glorkanti</ref>,
      <ref cel="hura.0i">hurai</ref>,
      <ref cel="jubil.0i">jubili</ref>
    </refgrp>
   </snc>
   <trdgrp lng="be">
     <trd>&c_v;&c_ib;&c_t;&c_a;&c_c;&c_mol; <ind>&c_a;&c_v;&c_a;&c_c;&c_y;&c_ja;&c_j;</ind></trd>,
     <trd>&c_l;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol; <ind>&c_a;&c_v;&c_a;&c_c;&c_y;&c_ju;</ind></trd>
   </trdgrp>
   <trdgrp lng="de">
    <trd>zujubeln</trd>,
    <trd>bejubeln</trd>
  </trdgrp>
   <trd lng="es">ovacionar</trd>
   <trd lng="fr">ovationner</trd>
   <trd lng="hu">&eacute;ljenez</trd>
   <trdgrp lng="ja">
    <trd>&#22823;&#x559D;&#x91C7;&#12377;&#x308B;  <pr>&#12384;&#12356;&#x304B;&#12387;&#12373;&#12356;&#12377;&#x308B;</pr></trd>,
    <trd>&#x71B1;&#x70C8;&#x306B;&#x6B53;&#x8FCE;&#12377;&#x308B; 
    <pr>&#x306D;&#12388;&#x308C;&#12388;&#x306B;&#x304B;&#12435;&#12370;&#12356;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
     <trd>een <ind>ovatie</ind> geven</trd>,
     <trd>iem. <ind>toejuichen</ind></trd>
   </trdgrp>
   <trdgrp lng="pl">
     <trd>bi&cacute;  <ind>brawo</ind></trd>,
     <trd>klaska&cacute;</trd>
   </trdgrp>
   <trd lng="pt">ovacionar</trd>
   <trd lng="ro">aplauda</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&#x43B;&c_a;&c_sh;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i; &#x43E;&c_v;&c_a;&c_c;&c_ib;&#x44E;</trd>,
    <trd>&c_v;&c_ib;&c_t;&c_a;&c_t;&c_i; &#x43E;&c_v;&c_a;&c_c;&c_ib;&#x44F;&#x43C;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x559D;&#x5F69; <pr>h&emacron;c&#x1CE;i</pr></trd>,
    <trd>&#x559D;&#x91C7; <pr>h&#xE8;c&#x1CE;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ovaci.0o">
  <kap><tld/>o</kap>
  <snc mrk="ovaci.0o.MOR">
    <dif>
      Entuziasma amasa honora <ref tip="super" cel="akcept.0o.ago">akcepto</ref>
      a&ubreve; apla&ubreve;do:
      <ekz>
        la koncerto okazis, kun granda sukceso kaj fina <tld/>o kaj entuziasmaj la&ubreve;doj
        fare de la lokaj kritikistoj
        <fnt>
          <bib>Monato</bib>
          <aut>Julia Sigmond kaj Sen Rodin</aut>
          <vrk><url ref="&Monato;/2010/009988.php">La mal&jcirc;aluzo de Klorinda</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
     </dif>
     <trd lng="pt">ova&ccedil;&atilde;o</trd>
   </snc>
   <snc mrk="ovaci.0o.HIS">
     <uzo tip="fak">HIS</uzo>
     <dif>
       &Ccirc;e antikvaj romianoj, ceremonio malpli solena ol
       <ref tip="vid" cel="triumf.0o.HIS">triumfo</ref>.
     </dif>
   </snc>
  <trd lng="be">&c_a;&c_v;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>oslava</trd>,
    <trd>ovace</trd>,
    <trd>p&rcaron;iv&iacute;t&aacute;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Ovation</trd>,
     <trd>Huldigung</trd>
   </trdgrp>
  <trd lng="en">ovation</trd>
  <trd lng="es">ovaci&oacute;n</trd>
  <trd lng="fr">ovation</trd>
  <trdgrp lng="hu">
     <trd>ov&aacute;ci&oacute;</trd>,
     <trd>&uuml;dvrivalg&aacute;s</trd>,
     <trd>&eacute;ljenz&eacute;s</trd>
   </trdgrp>
  <trdgrp lng="ja">
    <trd>&#22823;&#x559D;&#x91C7;  <pr>&#12384;&#12356;&#x304B;&#12387;&#12373;&#12356;</pr></trd>,
    <trd>&#x71B1;&#x70C8;&#x306A;&#x6B53;&#x8FCE; 
    <pr>&#x306D;&#12388;&#x308C;&#12388;&#x306A;&#x304B;&#12435;&#12370;&#12356;</pr></trd>,
    <trd>&#30053;&#x5F0F;&#x306E;&#x51F1;&#x65CB;&#x795D;&#12356; 
    <pr>&#x308A;&#12419;&#x304F;&#12375;&#x304D;&#x306E;&#x304C;&#12356;&#x305B;&#12435;&#12356;&#x308F;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">ovatie</trd>
  <trdgrp lng="pl">
    <trd>oklaski {pl}</trd>,
    <trd>owacja</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>aplauze</trd>,
    <trd>ova&#x21B;ie</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_v;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>oslava</trd>,
    <trd>ov&aacute;cia</trd>,
    <trd>priv&iacute;tanie</trd>
  </trdgrp>
  <trd lng="uk">&#x43E;&c_v;&c_a;&c_c;&c_ib;&#x44F;</trd>
</drv>
</art>
