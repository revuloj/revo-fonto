<kap>
  <rad>forniks</rad>/o
  <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="fornik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc mrk="fornik.0o.ANA">
    <dif>
      Volba, arkforma anatomia&jcirc;o:
      <ekz>
        <ind>cerba <tld/>o</ind>
        <trd lng="it">fornice cerebrale</trd>
      </ekz>
      <ekz>
        Aleksandro <nom>Achillini</nom> <klr>[&#x2026;]</klr> malkovris la tarson, konsistanta je sep
        ostoj, krom tio, li malkovris la <tld/>on <klr>[&#x2026;]</klr> en la cerbo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=489659">Aleksandro Achillini</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        konjunktiva <tld/>o;
      </ekz>
      <ekz>
        <ind>vagina <tld/>o</ind>.
        <trd lng="it">fornice vaginale</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_l;&c_ja;&c_p;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_a;&c_r;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>fornix</trd>,
    <trd>klenba</trd>,
    <trd>klenut&iacute;</trd>,
    <trd>oblouk</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Fornix</trd>,
     <trd>Gew&ouml;lbe (Anatomie)</trd>,
     <trd>W&ouml;lbung (Anatomie)</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fornix</trd>,
    <trd>trigone</trd></trdgrp>
  <trd lng="it">fornice</trd>
  <trd lng="ja">&#x5186;&#x84CB;  <pr>&#x3048;&#x3093;&#x304C;&#x3044;</pr></trd>
  <trd lng="pl">sklepienie</trd>
  <trdgrp lng="pt">
    <trd>f&oacute;rnice</trd>,
    <trd>f&oacute;rnix</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_v;&c_o;&c_d;</trd>,
    <trd>&c_a;&c_r;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>fornix</trd>,
    <trd>klenba</trd>,
    <trd>obl&uacute;k</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_k;&c_l;&c_je;&c_p;&c_ib;&c_n;&c_n;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#x7A79;&#x7ABF; <pr>qi&#x14D;ngl&#xF3;ng</pr></trd>,
    <trd>&#x62F1;&#x9802; <pr>g&#x1D2;ngd&#x1D0;ng</pr></trd>,
    <trd>&#x5C71;&#x51F9; <pr>sh&#x101;nw&#x101;</pr></trd>,
    <trd>&#x62F1;&#x5F62;&#x7D50;&#x69CB; <pr>g&#x1D2;ngx&#xED;ngji&#xE9;g&#xF2;u</pr></trd>
  </trdgrp>
</drv>
