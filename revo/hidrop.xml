<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>hidroplan</rad>/o
</kap>
<drv mrk="hidrop.0o">
<kap><tld/>o</kap>
<uzo tip="fak">AVI</uzo>
<snc>
<dif>
Aeroplano kiu levi&gcirc;as de sur la akvo k mallevi&gcirc;as sur &gcirc;in.
</dif>
<refgrp tip="super">
<ref cel="aeropl.0o">aeroplano</ref>,
<ref cel="aviad.0ilo">aviadilo</ref>,
<ref cel="avi.0o">avio</ref>
</refgrp>
<refgrp tip="sin">
<ref cel="biplan.0o">biplano</ref>,
<ref cel="monopl.0o">monoplano</ref>.
</refgrp>
<refgrp tip="vid">
<ref cel="aerost">aerostato</ref>,
<ref cel="aviad.0o">aviado</ref>,
<ref cel="balon.0o">balono</ref>.
</refgrp>
</snc>
<trdgrp lng="en">
  <trd>seaplane</trd>,
  <trd>hydroplane</trd>
</trdgrp>
<trdgrp lng="ru">
  <trd>&c_g;&c_i;&c_d;&c_r;&c_o;&c_s;&c_a;&c_m;&c_o;&c_l;&#1105;&c_t;</trd>,
  <trd>&c_g;&c_i;&c_d;&c_r;&c_o;&c_p;&c_l;&c_a;&c_n;</trd>
</trdgrp>
</drv>
</art>

