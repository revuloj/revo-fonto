<art mrk="$Id: hidrop.xml,v 1.26 2025/03/30 11:03:08 revo Exp $">
<kap>
  <rad>hidroplan</rad>/o
</kap>
<drv mrk="hidrop.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AVI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="avi.0o">Avio</ref>, kiu levi&gcirc;as de sur akvo
      kaj mallevi&gcirc;as sur &gcirc;in:
      <ekz>
        dume kavalerio ekperdis gravon kaj aviado atingis pinton (&ccirc;asaviadiloj,
        bombadaviadiloj, <tld/>oj ktp, kaj zepelino malaperis)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=11951">Unua mondmilito</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        en 1925 <nom>Amundsen</nom> provis atingi la Nordan Poluson per du <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=27948">Roald Amundsen</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Anka&#365; eblas diri <ke>maraviadilo</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_g;&c_ib;&c_d;&c_r;&c_a;&c_s;&c_a;&c_m;&c_a;&c_l;&c_jo;&c_t;</trd>,
    <trd>&c_g;&c_ib;&c_d;&c_r;&c_a;&c_p;&c_l;&c_ja;&c_n;</trd>
  </trdgrp>
  <trd lng="cs">hydropl&aacute;n</trd>
  <trd lng="de">Wasserflugzeug</trd>
  <trd lng="el">&ypsilon;&delta;&rho;&omikron;&pi;&lambda;&alfa_ton;&ny;&omikron;</trd>
  <trd lng="en">seaplane</trd>
  <trd lng="es">hidroavi&oacute;n</trd>
  <trd lng="fr">hydravion</trd>
  <trd lng="hu">hidropl&aacute;n</trd>
  <trd lng="it">idrovolante</trd>
  <trd lng="ja">&#27700;&#19978;&#39131;&#34892;&#27231;  <pr>&#12377;&#12356;&#12376;&#12423;&#12358;&#12402;&#12371;&#12358;&#12365;</pr></trd>
  <trd lng="nl">watervliegtuig</trd>
  <trdgrp lng="pl">
    <trd>hydroplan</trd>, <trd>wodnop&lstroke;atowiec</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>hidroavi&atilde;o</trd>,
    <trd>hidravi&atilde;o</trd>,
    <trd>hidroplano</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_i;&c_d;&c_r;&c_o;&c_s;&c_a;&c_m;&c_o;&c_l;&c_jo;&c_t;</trd>,
    <trd>&c_g;&c_i;&c_d;&c_r;&c_o;&c_p;&c_l;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="sk">hydropl&aacute;n</trd>
  <trdgrp lng="uk">
    <trd>&#1075;&#1110;&#1076;&#1088;&#1086;&#1083;&#1110;&#1090;&#1072;&#1082;</trd>,
    <trd>&#1075;&#1110;&#1076;&#1088;&#1086;&#1087;&#1083;&#1072;&#1085;</trd>
  </trdgrp>
  <trd lng="zh">&#27700;&#19978;&#39131;&#27231; <pr>shu&#464;sh&#224;ngf&#275;ij&#299;</pr></trd>
</drv>
</art>
