<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hidroplan</rad>/o
</kap>
<drv mrk="hidrop.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AVI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="avi.0o">Avio</ref>, kiu levi&gcirc;as de sur akvo
      kaj mallevi&gcirc;as sur &gcirc;in.
    </dif>
  </snc>
  <trd lng="de">Wasserflugzeug</trd>
  <trd lng="el">&ypsilon;&delta;&rho;&omikron;&pi;&lambda;&alfa_ton;&ny;&omikron;</trd>
  <trd lng="en">seaplane</trd>
  <trd lng="es">hidroavi&oacute;n</trd>
  <trd lng="fr">hydravion</trd>
  <trd lng="hu">hidropl&aacute;n</trd>
  <trd lng="nl">watervliegtuig</trd>
  <trdgrp lng="pl">
    <trd>hydroplan</trd>, <trd>wodnop&lstroke;atowiec</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>hidravi&atilde;o</trd>,
    <trd>hidrplano</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_i;&c_d;&c_r;&c_o;&c_s;&c_a;&c_m;&c_o;&c_l;&c_jo;&c_t;</trd>,
    <trd>&c_g;&c_i;&c_d;&c_r;&c_o;&c_p;&c_l;&c_a;&c_n;</trd>
  </trdgrp>
</drv>
</art>
