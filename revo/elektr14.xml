<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>elektron</rad>/o
</kap>
<drv mrk="elektr14.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">ELE</uzo>
  <snc mrk="elektr14.0o.gxenerale">
    <uzo tip="klr">(&gcirc;enerale)</uzo>
    <dif>
      <ref tip="super" cel="lepton.0o">Leptono</ref> &scirc;argita per
      fundamenta unuo de elektro negativa a&ubreve; pozitiva.
    </dif>
    <refgrp tip="sub">
      <ref cel="negaton.0o">negatrono</ref>,
      <ref cel="poziton.0o">pozitrono</ref>
    </refgrp>
    <ref tip="vid" cel="partik2.0o">partiklo</ref>
  </snc>
  <snc mrk="elektr14.0o.ofte">
    <uzo tip="klr">(ofte)</uzo>
    <dif>
      Negativa elektrono <sncref ref="elektr14.0o.gxenerale"/>, a&ubreve;
      <ref tip="sin" cel="negaton.0o">negatrono</ref>.
    </dif>
    <ref tip="malprt" cel="atom.0o">atomo;</ref>
    <refgrp tip="vid">
      <ref cel="proton.0o">protono</ref>,
      <ref cel="neuxtr2.0o">ne&ubreve;trono</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Elektron</trd>
  <trd lng="en">electron</trd>
  <trd lng="nl">elektron</trd>
  <trd lng="fr">&eacute;lectron</trd>
  <trd lng="ru">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_n;</trd>
</drv>
</art>
