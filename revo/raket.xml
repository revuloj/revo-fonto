<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>raket</rad>/o
</kap>
<drv mrk="raket.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Artefarita fajra&jcirc;o, kiun oni pafas vertikale en aeron kaj kiu
      eksplodas el&jcirc;etante multajn brilajn fajrerojn:
      <ekz>
        li a&ccirc;etis <tld/>ojn, krakpizojn, kaj diversajn
        fajra&jcirc;ojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        oni pafis lumigantajn <tld/>ojn al la &ccirc;ielo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu poeto ja <tld/>as per vortoj
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="jxongl.0i">&jcirc;ongli</ref>.
    </refgrp>
    <trd lng="de">Rakete</trd>
    <trdgrp lng="en">
      <trd>flare</trd>,
      <trd>rocket</trd>
    </trdgrp>
    <trd lng="ca">coet</trd>
    <trd lng="hu">rak&eacute;ta</trd>
    <trd lng="nl">vuurpijl</trd>
    <trd lng="pt">morteiro</trd>
    <trd lng="ru">&c_r;&c_a;&c_k;&c_je;&c_t;&c_a;</trd>
  </snc>
  <snc mrk="raket.0o.AVI">
    <uzo tip="fak">AVI</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Flugaparato a&ubreve; kuglego, movi&gcirc;anta per
      <ref tip="vid" cel="reakci.0o.TEK">reakcio</ref>,
      aperanta pro &jcirc;etado malanta&ubreve;en de parto de propra pezo:
      <ekz>
        kosma <tld/>o.
      </ekz>
    </dif>
    <trd lng="de">Rakete</trd>
    <trdgrp lng="en">
      <trd>rocket</trd>,
      <trd>missile</trd>
    </trdgrp>
    <trd lng="ca">coet</trd>
    <trd lng="cy">roced</trd>
    <trd lng="da">raket</trd>
    <trd lng="es">cohete</trd>
    <trd lng="fr">fus&eacute;e</trd>
    <trd lng="hu">rak&eacute;ta</trd>
    <trd lng="ja">&#12429;&#12369;&#12387;&#12392;</trd>
    <trd lng="nl">raket</trd>
    <trd lng="pt">foguete</trd>
    <trd lng="tr">f&uuml;ze</trd>
    <trd lng="ru">&c_r;&c_a;&c_k;&c_je;&c_t;&c_a;</trd>
  </snc>
  <snc mrk="raket.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Plata pilko&jcirc;etilo el interplektitaj &scirc;nuroj.
    </dif>
    <ref tip="sin" cel="raked.0o">rakedo</ref>
    <trd lng="ca">raqueta</trd>
    <trd lng="en">racket</trd>
    <trd lng="de">Schl&auml;ger</trd>
    <trd lng="hu">tenisz&uuml;t&odblac;</trd>
    <trd lng="nl">racket</trd>
    <trd lng="ru">&c_r;&c_a;&c_k;&c_je;&c_t;&c_k;&c_a;</trd>
  </snc>
</drv>
</art>
