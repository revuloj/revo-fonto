<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: raket.xml,v 1.32 2023/05/01 11:23:21 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>raket</rad>/o
</kap>
<drv mrk="raket.0o">
  <kap><tld/>o</kap>
  <snc mrk="raket.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Artefarita fajra&jcirc;o, kiun oni pafas vertikale en aeron kaj kiu
      eksplodas el&jcirc;etante multajn brilajn fajrerojn:
      <ekz>
        la &scirc;ipanoj dancis sur la ferdeko, kaj <klr>[&#8230;]</klr> pli ol cent <tld/>oj
        levi&gcirc;is en la aeron
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        li a&ccirc;etis <tld/>ojn, krakpizojn kaj plej diversajn fajra&jcirc;ojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Fluganta kofro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>oj flugis al la &ccirc;ielo, disvolvi&gcirc;is kvaza&ubreve; floroj kaj falis teren, kiel
        pluvo de diverskoloraj steloj
        <fnt>
          <bib>IKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiel helpsignalo a&ubreve; avertsignalo oni uzas montare a&ubreve; mare <ind>signal<tld/>o</ind>jn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=278546">Signalo</url></lok>
        </fnt>.
        <trd lng="de">Signalrakete</trd>
        <trd lng="pl">rakieta sygna&lstroke;owa</trd>
        <trd lng="ru">&c_s;&c_i;&c_g;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_r;&c_a;&c_k;&c_je;&c_t;&c_a;</trd>
      </ekz>
    </dif>
    <ref tip="super" cel="pirote.0ajxo">piroteknika&jcirc;o</ref>
    <trd lng="be">&c_r;&c_a;&c_k;&c_je;&c_t;&c_a;</trd>
    <trd lng="de">Rakete <klr>(Feuerwerk etc.)</klr></trd>
    <trdgrp lng="en">
      <trd>flare</trd>,
      <trd>rocket</trd>
    </trdgrp>
    <trd lng="fr">fus&eacute;e <klr>(de feu d'artifice)</klr></trd>
    <trd lng="ca">coet</trd>
    <trd lng="hu">rak&eacute;ta</trd>
    <trdgrp lng="it">
      <trd>razzo</trd>,
      <trd><ind>fuoco</ind> artificiale (razzo)</trd>,
      <trd><ind>gioco</ind> pirotecnico (razzo)</trd>
    </trdgrp>
    <trd lng="ja">&#25171;&#12385;&#19978;&#12370;&#33457;&#28779;
    [<ind>&#12358;&#12385;&#12354;&#12370;&#12399;&#12394;&#12403;</ind>]</trd>
    <trd lng="nl">vuurpijl</trd>
    <trd lng="pl">rakieta</trd>
    <trd lng="pt">morteiro</trd>
    <trd lng="ru">&c_r;&c_a;&c_k;&c_je;&c_t;&c_a;</trd>
  </snc>
  <snc mrk="raket.0o.AVI">
    <uzo tip="fak">AVI</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="super" cel="aparat.flug0o">Flugaparato</ref> a&ubreve; pafa&jcirc;o, pelata per el&jcirc;eto de fuza&jcirc;o:
      <ekz>
        &ccirc;u vere, la poloj jam pafas tiom multajn kosmo<tld/>ojn al la Luno
        <fnt>
          <bib>SatirRak</bib>
        </fnt>?
      </ekz>
      <ekz>
        satelitoj, iom kontraste al <klr>[&#8230;]</klr> la <tld/>oj necesaj por pafi ilin
        alten, tute ne kapricas
        <fnt>
          <aut>PEJNO Simono</aut>
          <vrk><url ref="&Monato;/1995/004358.html">Poloj per pelvo</url></vrk>
          <bib>Monato</bib>
          <lok>1995/04, p. 17</lok>
        </fnt>;
      </ekz>
      <ekz>
        cele al defendo de tiu bara&jcirc;lago Turkio zorgeme instalis apud &gcirc;i
        ter-aer-<tld/>ojn
        <fnt>
       <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2000/006866.php">La blua &leftquot;oro&#8221; &ccirc;iam malsufi&ccirc;os</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 20</lok>
        </fnt>;
      </ekz>
      <ekz>
        pereis tri homoj, kiam armea <tld/>o erare trafis na&ubreve;eta&gcirc;an domon
        <fnt>
          <aut>Araneo</aut>
            <vrk><url ref="&Monato;/2000/007026.php">Mis-ilo</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 12</lok>
          </fnt>;
      </ekz>
      <ekz>
        trieta&gcirc;a interkontinenta balistika <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=130346"
              >LGM-30 Minuteman</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="misil.0o">misilo</ref>,
       <ref cel="vetur.kosmo0ilo">kosmoveturilo</ref>
    </refgrp>
    <trd lng="be">&c_r;&c_a;&c_k;&c_je;&c_t;&c_a;</trd>
    <trd lng="de">Rakete</trd>
    <trdgrp lng="en">
      <trd>rocket</trd>,
      <trd>missile</trd>
    </trdgrp>
    <trd lng="ca">coet</trd>
    <trd lng="cy">roced</trd>
    <trd lng="da">raket</trd>
    <trd lng="es">cohete</trd>
    <trdgrp lng="fr">
        <trd>fus&eacute;e</trd>,
        <trd>roquette</trd>
    </trdgrp>
    <trd lng="hu">rak&eacute;ta</trd>
    <trdgrp lng="it">
      <trd>razzo</trd>,
      <trd>missile</trd>
    </trdgrp>
    <trd lng="ja">&#12525;&#12465;&#12483;&#12488; [<ind>&#12429;&#12369;&#12387;&#12392;</ind>]</trd>
    <trd lng="nl">raket</trd>
    <trd lng="pl">rakieta</trd>
    <trd lng="pt">foguete</trd>
    <trd lng="tr">f&uuml;ze</trd>
    <trd lng="ru">&c_r;&c_a;&c_k;&c_je;&c_t;&c_a;</trd>
  </snc>
  <snc mrk="raket.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="dif" cel="raked.0o">Rakedo</ref>.
    </dif>
    <trd lng="be">&c_r;&c_a;&c_k;&c_je;&c_t;&c_k;&c_a;</trd>
    <trd lng="ca">raqueta</trd>
    <trd lng="en">racket</trd>
    <trd lng="de">Schl&auml;ger</trd>
    <trd lng="fr">raquette</trd>
    <trd lng="hu">tenisz&uuml;t&odblac;</trd>
    <trd lng="it">racchetta</trd>
    <trd lng="nl">racket</trd>
    <trd lng="pl">rakietka</trd>
    <trd lng="ru">&c_r;&c_a;&c_k;&c_je;&c_t;&c_k;&c_a;</trd>
  </snc>
  <trd lng="cs">raketa <klr>(nap&rcaron;. tenisov&aacute;)</klr></trd>
  <trd lng="pl">rakieta</trd>
  <trd lng="ro">rachet&abreve;</trd>
  <trd lng="sk">raketa <klr>(tenisov&aacute;)</klr></trd>
  <trd lng="uk">&c_r;&c_a;&c_k;&c_je;&c_t;&c_a;</trd>
</drv>
</art>
