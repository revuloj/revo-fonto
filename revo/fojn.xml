<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>fojn</rad>/o</kap>
<drv mrk="fojn.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">AGR</uzo>
   <snc>
     <dif>
       Matura herbo, kiun oni fal&ccirc;as kaj sekigas &ccirc;e la suno
       por &gcirc;in konservi kiel nutra&jcirc;on por la brutoj:
       <ekz>
         <ctl>Fal&ccirc;ita kiel <tld/>o</ctl>
         <fnt><aut>Paul Veenman</aut>: <vrk>Krimoj...
         por &ccirc;iuj temperamentoj (Sten Johansson, Fal&ccirc;ita kiel fojno)</vrk>,
         <bib>Monato</bib><lok>jaro 1997a, numero 10a, p. 19a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_s;&c_je;&c_n;&c_a;</trd>
   <trd lng="fr">foin<klr tip="ind"> (herbe)</klr></trd>
   <trd lng="de">Heu</trd>
   <trd lng="hu">sz&eacute;na</trd>
   <trd lng="nl">hooi</trd>
   <trd lng="ru">&c_s;&c_je;&c_n;&c_o;</trd>
</drv>

<drv mrk="fojn.0ejo">
   <kap><tld/>ejo</kap>
   <uzo tip="fak">AGR</uzo>
   <snc>
     <dif>
       Loko &gcirc;enerale subtegmenta por finsekigo kaj konservado de <tld/>o:
       <ekz>
         petveturi, noktumi sub&ccirc;iele a&ubreve; en <tld/>ejoj<fnt><aut>Dmitrij
         Cibulevskij</aut>: <vrk>Danio pli proksima (Poul Thorsen, Babile
         kun mi mem)</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero 8a, p.
         23a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="fr">
     <trd>fenil</trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>sz&eacute;napadl&aacute;s</trd>,
     <trd>sz&eacute;napajta</trd>
   </trdgrp>
   <trdgrp lng="nl">
     <trd>hooizolder</trd>,
     <trd>hooischuur</trd>
   </trdgrp>
   <trd lng="ru">&c_s;&c_je;&c_n;&c_o;&c_v;&c_a;&c_l;</trd>
</drv>

<drv mrk="fojn.post0o">
   <kap>post<tld/>o</kap>
   <uzo tip="fak">AGR</uzo>
   <snc>
     <dif>
       <tld lit="F"/>o el dua rikolto en sama jaro.
     </dif>
     <ref tip="vid" cel="falcx.post0ajxo">postfal&ccirc;a&jcirc;o</ref>
   </snc>
   <trdgrp lng="fr">
     <trd>arri&egrave;re-foin</trd>,
     <trd>refoin</trd>,
     <trd>regain<klr tip="ind"> (foin de seconde coupe)</klr></trd>
   </trdgrp>
   <trd lng="hu">sarj&uacute;sz&eacute;na</trd>
   <trd lng="ru">&c_o;&c_t;&c_a;&c_v;&c_a;</trd>
</drv>
</art>
