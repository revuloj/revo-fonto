<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>magnet</rad>/o
</kap>
<drv mrk="magnet.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Natura feroksida &scirc;tono havanta la proprecon altiri iujn metalojn,
      precipe feron.
    </dif>
    <trd lng="en">lodestone</trd>
    <trd lng="ca">imant</trd>
  </snc>
  <snc>
    <dif>
      Ferpeco al kiu oni komunikis tiun altiran proprecon.
    </dif>
    <trd lng="en">magnet</trd>
    <trdgrp lng="ca">
      <trd>atractiu</trd>
      <trd>encant</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_g;&c_n;&c_ib;&c_t;</trd>
  <trd lng="de">Magnet</trd>
  <trd lng="es">im&aacute;n</trd>
  <trd lng="hu">m&aacute;gnes</trd>
  <trd lng="nl">magneet</trd>
  <trdgrp lng="pt">
    <trd>magneto</trd>,
    <trd>&iacute;m&atilde;</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_a;&c_g;&c_n;&c_i;&c_t;</trd>
  <trd lng="sk">magnet</trd>
  <trd lng="sv">magnet</trd>
</drv>

<drv mrk="magnet.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Havanta la econ propran al <tld/>o:
      <ekz>
        <tld/>a fero, <tld/>a montrilo de kompaso.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a fenomeno, forto.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Mistere altiranta per nekontra&ubreve;starebla forto:
      <ekz>
        <tld/>a rigardo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_g;&c_n;&c_ib;&c_t;&c_n;&c_y;</trd>
  <trd lng="de">magnetisch</trd>
  <trd lng="en">magnetic</trd>
  <trdgrp lng="hu">
    <trd>m&aacute;gnes-</trd>,
    <trd>m&aacute;gneses</trd>,
    <trd>magnetikus</trd>
  </trdgrp>
  <trd lng="nl">magnetisch</trd>
  <trd lng="ru">&c_m;&c_a;&c_g;&c_n;&c_i;&c_t;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">magnetick&yacute;</trd>
  <trd lng="sv">magnetisk</trd>
</drv>

<drv mrk="magnet.0igi">
  <kap>
    <tld/>igi
  </kap>
  <snc>
    <dif>
      Komuniki la altirecon de <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_m;&c_a;&c_g;&c_n;&c_ib;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">magnetisieren</trd>
  <trd lng="en">magnetize</trd>
  <trd lng="hu">m&aacute;gnesez</trd>
  <trd lng="nl">magnetiseren</trd>
  <trd lng="ru">&c_n;&c_a;&c_m;&c_a;&c_g;&c_n;&c_i;&c_t;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sv">magnetisera</trd>
</drv>
</art>
