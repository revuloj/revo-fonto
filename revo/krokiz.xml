<kap>
  <rad>krokiz</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="krokiz.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="krokiz.0i.BELA">
    <dif>
      Krude, rapide <ref tip="dif" cel="skiz.0i.KOMUNE">skizi</ref>:
      <ekz>
        la&ubreve; la <tld/>ita mapo ni iris
        <fnt>
          <aut>Basyoo, trad. Sasaki T.</aut>,
          <vrk><url ref="http://members.jcom.home.ne.jp/verda/sasaki%20okunohosomiti.pdf">
          Streta vojo al fora interno</url></vrk>,
          <lok>[vidita en 2011]</lok>
        </fnt>;
      </ekz>
      <ekz>
        la nuna tre oportune reprenas la originalajn ilustra&jcirc;ojn de la
        unua itala
        eldono, kiuj reportas nin <tld/>e al &ccirc;arma fabela mondo
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2003/008067.html">Marioneto &ccirc;iam
          aktuala</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>skizi</ke>.
  </rim>

  <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_n;&c_a;&c_k;&c_ib;&c_d;&c_a;&c_k;</ind></trd>
  <trdgrp lng="cs">
    <trd>skicovat</trd>,
    <trd>&ccaron;rtat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>skizzieren</trd>,
    <trd>entwerfen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>back of the napkin sketch</trd>,
    <trd>quick drawing</trd>,
    <trd>crude sketch (drawing)</trd>,
    <trd>draft (drawing)</trd>
  </trdgrp>
  <trd lng="fr">esquisser</trd>
  <trd lng="pl">szkicowa&cacute;</trd>
  <trd lng="ro">schi&#539;&abreve;</trd>
  <trdgrp lng="sk">
    <trd>na&ccaron;rtn&uacute;&tcaron;</trd>,
    <trd>&ccaron;rta&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_r;&#1086;&c_b;&c_i;&c_t;&c_i; &#1082;&c_r;&#1086;&#1082;&c_ib;</trd>
  <trdgrp lng="zh">
    <trd>&#21246;&#21010; <pr>g&#333;uhu&#224;</pr></trd>,
    <trd>&#21246;&#21123; <pr>g&#333;uhu&#224;</pr></trd>,
    <trd>&#35373;&#35336;&#22909; <pr>sh&#232;j&#236;h&#462;o</pr></trd>,
    <trd>&#35774;&#35745;&#22909; <pr>sh&#232;j&#236;h&#462;o</pr></trd>,
    <trd>&#32032;&#25551; <pr>s&#249;mi&#225;o</pr></trd>,
    <trd>&#24501;&#21484; <pr>zh&emacron;ngzh&#224;o</pr></trd>,
    <trd>&#25991;&#31295; <pr>w&#233;ng&#462;o</pr></trd>
  </trdgrp>
</drv>
</art>

