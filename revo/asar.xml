<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: asar.xml,v 1.13 2024/09/04 01:49:41 revo Exp $">
<kap>
  <rad>asar</rad>/o <fnt>Z</fnt>
</kap>
<drv mrk="asar.0o">
  <kap><tld/>o</kap>
  <snc mrk="asar.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Antikva <ref tip="lst" cel="unu.mezur0o"
      lst="voko:kvantoj">mezurunuo</ref> de maso
      kaj <ref tip="lst" cel="unu.mon0o" lst="voko:monunuoj">monunuo</ref>,
      16-ono de  <ref tip="malprt" cel="denar.0o">denaro</ref>:
      <ekz>
        &ccirc;u oni ne vendas kvin paserojn por du <tld/>oj
        <fnt><bib>NT</bib>, <lok>&Luk; 12:6</lok></fnt>?
      </ekz>
    </dif>
    <ref tip="sin" cel="as.0o.HIS">aso</ref>
    <ref tip="prt" cel="kvadrant.0o">kvadranto</ref>
    <trd lng="be">&c_a;&c_s; <klr tip="amb">(&c_s;&c_t;&c_a;&c_r;&c_a;&c_zh;&c_y;&c_t;&c_n;&c_a;&c_ja; &c_g;&c_r;&c_a;&c_sh;&c_o;&c_v;&c_a;&c_ja; &c_a;&c_d;&c_z;&c_ib;&c_n;&c_k;&c_a;)</klr></trd>
    <trd lng="ca">as</trd>
    <trd lng="de">As</trd>
    <trd lng="en">farthing</trd>
    <trd lng="fr">as<klr tip="ind"> (monnaie antique)</klr></trd>
    <trd lng="grc">&alfa_psili;&sigma;&sigma;&alfa_ton;&rho;&jota;&omikron;&ny;</trd>
    <trd lng="hu">as</trd>
    <trd lng="it">asse<klr> (mon. e mis.)</klr></trd>
    <trd lng="la">as, assis</trd>
    <trd lng="pl">as</trd>
    <trd lng="ro">as <klr>(moneda)</klr></trd>
    <trd lng="ru">&c_a;&c_s; <klr tip="amb">(&c_a;&c_n;&c_t;&c_i;&c_ch;&c_n;&c_a;&c_ja; &c_m;&c_o;&c_n;&c_je;&c_t;&c_a;)</klr></trd>
  </snc>
</drv>
</art>
