<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>asar</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="asar.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Antikva <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de maso
      kaj <ref tip="super" cel="unu.mon0o">monunuo</ref>, ono de
      <ref tip="malprt" cel="denar.0o">denaro</ref>:
      <ekz>
        &ccirc;u oni ne vendas kvin paserojn por du <tld/>oj?
        <fnt><bib>NT</bib>, <lok>&Luk; 12:6</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="as.0o.HIS">aso.</ref>
    <trd lng="fr">as<klr tip="ind"> (monnaie antique)</klr></trd>
    <trd lng="grc">&alfa_psili;&sigma;&sigma;&alfa_acute;&rho;&jota;&omikron;&ny;</trd>
    <trd lng="hu">as</trd>
    <trd lng="la">as, assis</trd>
    <trd lng="ru">&c_a;&c_s; <klr tip="amb">(&c_a;&c_n;&c_t;&c_i;&c_ch;&c_n;&c_a;&c_ja; &c_m;&c_o;&c_n;&c_je;&c_t;&c_a;)</klr></trd>
  </snc>
</drv>
</art>
