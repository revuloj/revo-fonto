<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hades.xml,v 1.16 2024/02/02 09:23:25 revo Exp $">
<kap>
  <rad>Hades</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="hades.0o">
  <kap><tld/>o</kap>
  <snc mrk="hades.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      <ref tip="super" cel="infer.0o.REL">Infero</ref> en la antikva
      greka
religio:
      <ekz>
        la rokoj de la <tld/>a pordego
        <fnt><bib>SatirRak</bib>, <lok>Pythagoras</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_A;&c_ib;&c_d;</trd>,
    <trd>&c_G;&c_a;&c_d;&c_e;&c_s;</trd>
  </trdgrp>
  <trd lng="ca">l'hades</trd>
  <trdgrp lng="cs">
    <trd>Hades</trd>,
    <trd>H&aacute;des</trd>
  </trdgrp>
  <trd lng="de">Hades</trd>
  <trd lng="en">Hades</trd>
  <trd lng="fr">Had&egrave;s</trd>
  <trd lng="hu">Had&eacute;sz</trd>
  <trd lng="it">Ade</trd>
  <trdgrp lng="ja">
    <trd>&#x30CF;&#x30C7;&#x30B9;</trd>,
    <trd>&#x51A5;&#x754C;  <pr>&#12417;&#12356;&#x304B;&#12356;</pr></trd>,
    <trd>&#38512;&#x5E9C;  <pr>&#12356;&#12435;&#12407;</pr></trd>,
    <trd>&#x9EC4;&#x6CC9;  <pr>&#12371;&#12358;&#x305B;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">Hades</trd>
  <trd lng="pl">Hades</trd>
  <trd lng="pt">Hades</trd>
  <trdgrp lng="sk">
    <trd>Hades</trd>,
    <trd>H&aacute;des</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_G;&c_a;&c_d;&c_je;&c_s;</trd>,
    <trd>&c_A;&c_ji;&c_d;</trd>
  </trdgrp>
  <trd lng="zh">&#x54C8;&#x5E95;&#x65AF; <pr>h&amacron;d&#x1D0;s&#x12B;</pr></trd>
</drv>
</art>

