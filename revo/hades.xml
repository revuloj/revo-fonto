<vortaro>
<art mrk="$Id: hades.xml,v 1.13 2023/08/31 15:46:13 revo Exp $">
<kap>
  <rad>Hades</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="hades.0o">
  <kap><tld/>o</kap>
  <snc mrk="hades.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      <ref tip="super" cel="infer.0o.REL">Infero</ref> en la antikva
      greka
religio:
      <ekz>
        la rokoj de la <tld/>a pordego
        <fnt><bib>SatirRak</bib>, <lok>Pythagoras</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">l'hades</trd>
  <trdgrp lng="cs">
    <trd>Hades</trd>,
    <trd>H&aacute;des</trd>
  </trdgrp>
  <trd lng="de">Hades</trd>
  <trd lng="en">Hades</trd>
  <trd lng="fr">Had&egrave;s</trd>
  <trd lng="hu">Had&eacute;sz</trd>
  <trd lng="it">Ade</trd>
  <trdgrp lng="ja">
    <trd>&#x30CF;&#x30C7;&#x30B9;</trd>,
    <trd>&#x51A5;&#x754C;  [<ind>&#x3081;&#x3044;&#x304B;&#x3044;</ind>]</trd>,
    <trd>&#x9670;&#x5E9C;  [<ind>&#x3044;&#x3093;&#x3077;</ind>]</trd>,
    <trd>&#x9EC4;&#x6CC9;  [<ind>&#x3053;&#x3046;&#x305B;&#x3093;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">Hades</trd>
  <trd lng="pl">Hades</trd>
  <trd lng="pt">Hades</trd>
  <trdgrp lng="sk">
    <trd>Hades</trd>,
    <trd>H&aacute;des</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x413;&#x430;&#x434;&#x435;&#x441;</trd>,
    <trd>&#x410;&#x457;&#x434;</trd>
  </trdgrp>
</drv>
</art>

