<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sedativ.xml,v 1.17 2024/03/24 20:00:00 revo Exp $">
<kap>
  <rad>sedativ</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sedativ.0o">
  <kap><tld/>o</kap>
  <snc mrk="sedativ.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Kvietiga <ref tip="super" cel="medika1.0o">medikamento</ref>:
      <ekz>
        kuracisto <klr>[&#x2026;]</klr> preskribis al li <tld/>on sufi&ccirc;e fortan, sed
        <klr>[&#x2026;]</klr> tiuj kuraciloj ka&ubreve;zas dependon kaj tute ne garantias bonan
        kvaliton de dormo
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2012/010764.php">Dormu bone, vivu bone (per
          melatonino)</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Ora</nom> sekrete ordonis, ke <nom>Penetra</nom> donu al li <tld/>on
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 100a, volumo 3a, p.
        301a</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i sendormis tutajn noktojn, sen&ccirc;ese &gcirc;emante kaj turni&gcirc;ante pro doloro, <tld/>a
        injekto jam ne efikis sur &scirc;in
        <fnt>
          <bib>Monato</bib>
          <aut>Lin Sheng</aut>
          <vrk><url ref="&Monato;/2006/008670.php">Forpasis mia karulino</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aneste.0ilo">anestezilo</ref>,
<ref cel="dorm.0igilo">dormigilo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_c;&c_ib;&c_sh;&c_a;&c_l;&c_mol;&c_n;&c_y; &c_s;&c_r;&c_o;&c_d;&c_a;&c_k;</trd>,
    <trd>&c_s;&c_e;&c_d;&c_a;&c_c;&c_y;&c_j;&c_n;&c_y; &c_s;&c_r;&c_o;&c_d;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_je;&c_d;&c_a;&c_t;&c_i;&c_v;</trd>
  <trd lng="ca">sedant</trd>
  <trdgrp lng="cs">
    <trd>sedativum</trd>,
    <trd>uti&scaron;uj&iacute;c&iacute; droga</trd>
  </trdgrp>
  <trd lng="de">Sedativ</trd>
  <trd lng="en">sedative</trd>
  <trd lng="es">sedante</trd>
  <trd lng="fr">s&eacute;datif<klr> (subst.)</klr></trd>
  <trd lng="hu">nyugtat&oacute;<klr tip="ind"> (orvoss&aacute;g)</klr></trd>
  <trd lng="it">sedativo<klr> (sost.)</klr></trd>
  <trd lng="ja">&#x93AE;&#x9759;&#x5264;  <pr>&#x3061;&#x3093;&#x305B;&#x3044;&#x3056;&#x3044;</pr></trd>
  <trdgrp lng="nl">
    <trd>sedativum</trd>,
    <trd>kalmerend middel</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&sacute;rodek uspokajaj&aogonek;cy</trd>,
    <trd>sedativum</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_p;&c_o;&c_k;&c_o;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je; &c_s;&c_r;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_s;&c_je;&c_d;&c_a;&c_t;&c_i;&c_v;&c_n;&c_o;&c_je; &c_s;&c_r;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>sedat&iacute;vum</trd>,
    <trd>uti&scaron;uj&uacute;ca droga</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&#x43E;&#x43B;&c_je;&c_z;&c_a;&c_s;&#x43F;&#x43E;&#x43A;&c_ib;&c_j;&#x43B;&c_i;&c_v;&c_i;&c_j; &c_z;&c_a;&c_s;&c_ib;&c_b;</trd>,
    <trd>&c_b;&#x43E;&#x43B;&c_je;&c_z;&c_a;&c_s;&#x43F;&#x43E;&#x43A;&c_ib;&c_j;&#x43B;&c_i;&c_v;&c_ib; &#x43B;&c_ib;&#x43A;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x93AE;&#x975C;&#x85E5; <pr>zh&#xE8;nj&#xEC;ngy&#xE0;o</pr></trd>,
    <trd>&#x9547;&#x9759;&#x836F; <pr>zh&#xE8;nj&#xEC;ngy&#xE0;o</pr></trd>,
    <trd>&#x93AE;&#x5B9A;&#x85E5; <pr>zh&#xE8;nd&#xEC;ngy&#xE0;o</pr></trd>,
    <trd>&#x9547;&#x5B9A;&#x836F; <pr>zh&#xE8;nd&#xEC;ngy&#xE0;o</pr></trd>,
    <trd>&#x93AE;&#x75DB;&#x4F5C;&#x7528; <pr>zh&#xE8;nt&#xF2;ngzu&#xF2;y&#xF2;ng</pr></trd>,
    <trd>&#x9547;&#x75DB;&#x4F5C;&#x7528; <pr>zh&#xE8;nt&#xF2;ngzu&#xF2;y&#xF2;ng</pr></trd>,
    <trd>&#x93AE;&#x975C;&#x5291; <pr>zh&#xE8;nj&#xEC;ngj&#xEC;</pr></trd>,
    <trd>&#x9547;&#x9759;&#x5242; <pr>zh&#xE8;nj&#xEC;ngj&#xEC;</pr></trd>
  </trdgrp>
</drv>
