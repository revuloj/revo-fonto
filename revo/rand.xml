<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rand</rad>/o
</kap>
<drv mrk="rand.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;irka&ubreve;a ekstrema parto de io, konsiderata la&ubreve; la longo:
      <ekz>
        <tld/>o de tablo, lito<fnt>Z</fnt>,
      </ekz>
      <ekz>
        kadro<fnt>Z</fnt>,
      </ekz>
      <ekz>
        vesto<fnt>Z</fnt>,
      </ekz>
      <ekz>
        telero, glaso<fnt>B</fnt>;
      </ekz>
      <ekz>
        akra <tld/>o de tran&ccirc;ilo;
      </ekz>
      <ekz>
        <tld/>o de boato<fnt>Z</fnt>,
      </ekz>
      <ekz>
        &gcirc;ardeno<fnt>Z</fnt>,
      </ekz>
      <ekz>
        akvo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la <tld/>oj de iliaj tegmentoj estas turnitaj supren<fnt>B</fnt>;
      </ekz>
      <ekz>
        &ccirc;e la <tld/>o de la vojo, &ccirc;ielo<fnt>Z</fnt>,
      </ekz>
      <ekz>
        arbaro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        al vi venos nacioj de la <tld/>oj (plej malproksimaj ekstrema&jcirc;oj)
        de la tero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la domo pleni&gcirc;is de <tld/>o &gcirc;is <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        trovi&gcirc;i &ccirc;e la <tld/>o
        <klr>(tute proksime. <ref tip="sin"
        cel="sojl.0o">sojlo</ref>)</klr>
        de senfunda&jcirc;o <klr>(ruino, pereo)</klr>, de la morto;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sidi &ccirc;e la <tld/>o de eksi&gcirc;o <klr>(preska&ubreve;
        devi eksi&gcirc;i)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ru&gcirc;<tld/>aj palpebroj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        taso &gcirc;is<tld/>e plena<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>orefalde kudri <klr>(orli)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        drap<tld/>a&jcirc;oj &ccirc;irka&ubreve; la pordo gardis
        kontra&ubreve; &ccirc;iu trablovo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bord.0o">bordo</ref>,
      <ref cel="lim.0o">limo</ref>.
    </refgrp>
    <trd lng="de">Rand</trd>
    <trd lng="en">edge</trd>
    <trdgrp lng="nl">
      <trd>rand</trd>,
      <trd>kant</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_a;&c_j;</trd>,
      <trd>&c_k;&c_r;&c_o;&c_m;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="rand.0o.figuro">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <klr>(de <ref tip="malprt" cel="figur.geometria0o">geometria
      figuro</ref>)</klr>
      La <ref cel="kun.0ajxo.aro">kuna&jcirc;o</ref> de &ccirc;iuj
      linioj (se paroli pri surfaco) a&ubreve; de &ccirc;iuj
      surfacoj (se paroli pri solido), kiuj &gcirc;in limas:
      <ekz>
        la <tld/>o de sfera solido estas sfera surfaco;
      </ekz>
      <ekz>
        ebeno ne havas <tld/>on;
      </ekz>
      <ekz>
        la <tld/>o de plurlatero (resp. pluredro) konsistas el &gcirc;iaj
        lateroj (resp. edroj).
      </ekz>
    </dif>
    <trd lng="ru">&c_g;&c_r;&c_a;&c_n;&c_i;&c_c;&c_a; <klr tip="amb">(&c_g;&c_je;&c_o;&c_m;. &c_f;&c_i;&c_g;&c_u;&c_r;&c_y;)</klr></trd>
  </snc>
  <trdgrp lng="fr">
    <trd>bord</trd>,
    <trd>contour</trd>,
    <trd>lisi&egrave;re</trd>,
    <trd>pourtour</trd>
  </trdgrp>
</drv>
</art>
