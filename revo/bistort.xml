<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: bistort.xml,v 1.3 2023/12/14 07:24:49 afido Exp $">
    <kap>
      <rad>bistort</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="bistort.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="bistort.0o.BOT">
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio <ref tip="malprt" cel="poligo1.0oj">poligonacoj</ref> (<trd
      lng="la">Bistorta</trd>)
      kun rizomo, rektstara senbran&ccirc;a tigo, longecaj bazosidaj folioj,
      spika floraro de kvinpetalaj blankaj a&ubreve; ru&gcirc;etaj duseksaj floretoj:
      <ekz>
        Vincento <klr>[&#8230;]</klr> kirlis super fajro magiajn dekoktojn el <klr>[&#8230;]</klr>
        valeriano, mandragoro, <tld/>o, centa&ubreve;reo
        <fnt>
          <aut>Karel &Ccaron;apek, trad. Josef Vondrou&scaron;ek</aut>
          <vrk><url
          ref="https://github.com/LaPingvino/esperantolibroj/blob/master/Nau%27%20fabeloj%20-%20Karel%20C%27apek.md"
              >Na&ubreve; Fabeloj, Granda doktora fabelo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en Hungario la <tld/>o estas protektita planto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=58658"
              >Bistorto</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/e/e3/Illustration_Polygonum_bistorta0.jpg/454px-Illustration_Polygonum_bistorta0.jpg"
      prm="PD"
      >botanika tabulo de <tld/>a ordinara (Bistorta officinalis)
        <fnt>
          <lok><url ref="&WCW;/File:Illustration_Polygonum_bistorta0.jpg"
              >https://commons.wikimedia.org/wiki/File:Illustration_Polygonum_bistorta0.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="be">&c_z;&c_mol;&c_m;&c_je;&c_ja;&c_v;&c_ib;&c_k; <klr tip="amb">(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
    <trdgrp lng="de">
      <trd>Wiesenkn&ouml;terich</trd>,
      <trd>Schlangenkn&ouml;terich</trd>
    </trdgrp>
    <trd lng="en">Bistorta</trd>
    <trd lng="fr">Bistorta</trd>
  </snc>
</drv>

  </art>

