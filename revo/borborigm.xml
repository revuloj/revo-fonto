<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: borborigm.xml,v 1.4 2025/03/02 11:21:25 afido Exp $">
    <kap>
      <rad>borborigm</rad>/o
      <fnt><bib>PVEF</bib></fnt>
    </kap>

<drv mrk="borborigm.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZL</uzo>
  <snc>
    <dif>
      Intesta <ref cel="murmur.0i">murmurado</ref> ka&ubreve;zita de movataj gasoj kaj likva&jcirc;oj:
      <ekz>
        la <tld/>o de la maldika intesto estas glugla, sed tinta kiam la intestovando estas
        stre&ccirc;ita
        <fnt>
          <aut>Siegfried Stampa</aut>
          <vrk><url ref="https://scienca-revuo.info/files/1493-1-1631-1-10-20140426.pdf">Fiziologio kaj malsanoj de la digesta sistemo &ccirc;e farmbestoj sub tropikaj
              kondi&ccirc;oj</url></vrk>
          <lok>Scienca Revuo n-ro 4/2005, p. 210</lok>
        </fnt>;
      </ekz>
      <ekz>
        iliaj doktaj vortoj kaj paroloj alternis brue-flue kun la &scirc;macoj kaj moderaj
        <tld/>oj
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 3</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="glugl.0o">gluglo</ref>
    <trdgrp lng="de">
      <trd>Borborygmus</trd>,
      <trd>Darmger&auml;usch</trd>
    </trdgrp>
    <trd lng="en">stomach rumble</trd>
    <trd lng="fr">borborygme</trd>
    <trd lng="pl">burczenie <klr>(np. w brzuchu)</klr></trd>
  </snc>
<trdgrp lng="be">
  <trd>&c_b;&c_u;&c_r;&c_ch;&c_a;&c_n;&c_n;&c_je; &c_w; &c_zh;&c_y;&c_v;&c_a;&c_c;&c_je;</trd>,
  <trd>&c_k;&c_o;&c_l;&c_ib;&c_k;&c_ib;</trd>
</trdgrp>
</drv>
  </art>

