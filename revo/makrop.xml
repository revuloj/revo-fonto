<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: makrop.xml,v 1.21 2019/03/17 09:10:14 revo Exp $">
<kap>
  <rad>makrop</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="makrop.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="kangur.0o">Kanguruo</ref> de la 
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">genro</ref> 
      <trd lng="la">Macropus</trd> el la familio <ref tip="malprt" cel="makrop1.0oj">makropedoj</ref>:
      <ekz>
        la Griza <tld/>o a&ubreve; Griza valabio, <klr>[&#8230;]</klr> jam formortinta specio de
        valabio, <klr>[&#8230;]</klr> estis la plej eleganta, gracia kaj rapida specio de
        kanguruo <klr>[&#8230;]</klr>, ties saltado konsistis el du mallongaj saltetoj,
        poste alia longa sekvata de staro al &ccirc;ielo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=496640"
              >Griza makropo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/d/db/Red_necked_wallaby2.jpg/640px-Red_necked_wallaby2.jpg"
      prm="CC-BY-SA-3.0"
      >Ru&gcirc;nuka <tld/>o (Macropus rufogriseus), Victoria, A&ubreve;stralujo
        <fnt>
          <aut>Benjamint444</aut>
          <lok><url ref="&WCW;/File:Red_necked_wallaby2.jpg"
              >https://commons.wikimedia.org/wiki/File:Red_necked_wallaby2.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_k;&c_je;&c_n;&c_g;&c_u;&c_r;&c_u;</trd>
  <trdgrp lng="ca">
    <trd>cangur</trd>,
    <trd>ualabi</trd>
  </trdgrp>
  <trd lng="de">K&auml;nguru</trd>
  <trd lng="en">macropus</trd>
  <trd lng="es">canguro</trd>
  <trd lng="fr">macropus</trd>
  <trd lng="hu">kenguru</trd>
  <trd lng="nl">kangoeroe</trd>
  <trd lng="pl">kangur</trd>
  <trd lng="pt">canguru</trd>
  <trd lng="ru">&c_k;&c_je;&c_n;&c_g;&c_u;&c_r;&c_u;</trd>
</drv>
</art>
