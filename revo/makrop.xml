<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>makrop</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="makrop.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super"
      cel="makrop1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Macropus</trd>), alta besto, kiu movas sin saltante
      per la malanta&ubreve;aj kruroj kaj kies ino havas ventran
      po&scirc;on por tie gardi siajn idojn.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_je;&c_n;&c_g;&c_u;&c_r;&c_u;</trd>
  <trdgrp lng="ca">
    <trd>cangur</trd>,
    <trd>ualabi</trd>
  </trdgrp>
  <trd lng="de">K&auml;nguru</trd>
  <trd lng="en">macropus</trd>
  <trd lng="es">canguro</trd>
  <trd lng="fr">macropus</trd>
  <trd lng="hu">kenguru</trd>
  <trd lng="nl">kangoeroe</trd>
  <trd lng="pl">kangur</trd>
  <trd lng="pt">canguru</trd>
  <trd lng="ru">&c_k;&c_je;&c_n;&c_g;&c_u;&c_r;&c_u;</trd>
</drv>
</art>
