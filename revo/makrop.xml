<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>makrop</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="makrop.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="makrop.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Macropus</trd>), alta besto, kiu movas sin saltante
      per la malanta&ubreve;aj kruroj kaj kies ino havas ventran               
      po&scirc;on por tie gardi siajn idojn.
    </dif>
  </snc>
</drv>
<drv mrk="makrop.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la subklaso <ref tip="super" cel="marsup.0oj">marsupialoj</ref>
      (<trd lng="la">Macropodidae</trd>).
      <ref tip="prt" cel="kangur.0o">kanguruo</ref>
    </dif>
  </snc>
  <trd lng="de">K&auml;nguruhs <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
