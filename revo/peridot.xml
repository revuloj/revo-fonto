<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>peridot</rad>/o
</kap>
<drv mrk="peridot.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Krizolito, olivverda mineralo uzata en juvelarto.
    </dif>
    <refgrp tip="sin">
      <ref cel="krizol.0o">krizolito</ref>.
    </refgrp>
  </snc>
  <trd lng="ca">peridot</trd>
  <trd lng="de">Peridot</trd>
  <trd lng="en">peridot</trd>
  <trd lng="nl">peridot</trd>
  <trdgrp lng="hu">
    <trd>peridot</trd>,
    <trd>olivin</trd>
  </trdgrp>
  <trd lng="pt">peridoto</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_i;&c_d;&c_o;&c_t;</trd>,
    <trd>&c_o;&c_l;&c_i;&c_v;&c_i;&c_n;</trd>
  </trdgrp>
</drv>
</art>
