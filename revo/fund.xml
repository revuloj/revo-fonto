  <trd lng="km">&#x1794;&#x17B6;&#x178F;</trd>
  <trd lng="ko">&#xBC14;&#xB2E5;</trd>
  <trd lng="lo">&#xE97;&#xEB2;&#xE87;&#xEA5;&#xEB8;&#xEC8;&#xEA1;</trd>
  <trd lng="ml">&#xD05;&#xD1F;&#xD3F;&#xD24;&#xD4D;&#xD24;&#xD1F;&#xD4D;&#xD1F;&#xD4D;</trd>
  <trd lng="mr">&#x924;&#x933;</trd>
