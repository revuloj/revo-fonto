<drv mrk="fund.sur0igxi">
  <kap>sur<tld/>i&gcirc;i</kap>
  <snc mrk="fund.sur0igxi.KOMUNE">
    <dif>
      <ref tip="super" cel="lev.mal0igxi.malaltigxi">Mallevi&gcirc;i</ref> &gcirc;is la <tld/>o:
      <ekz>
        la &scirc;ipo tuj pleni&gcirc;is de akvo kaj sur<tld/>i&gcirc;is
        <fnt><bib>Munchhausen</bib> <lok>&ccirc;apitro 10a</lok></fnt>;
