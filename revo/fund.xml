<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>fund</rad>/o
</kap>
<drv mrk="fund.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc>
     <dif>
       Plej profunda malsupra parto de io kava:
       <ekz>
         <tld/>o de puto, maro, valo, vazo, barelo;
       </ekz>
       <ekz>
         detruita &gcirc;is la <tld/>o de l' fundamento<fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         renversi ion la <tld/>on supren;
       </ekz>
       <ekz>
         li esploris iom la <tld/>on de la glaso
         <klr>(li estas ebrieta)</klr><fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         al kavo sen<tld/>a &scirc;topado ne helpas<fnt><bib>PrV</bib></fnt>.
       </ekz>
     </dif>
     <trd lng="de">Grund</trd>
     <trd lng="nl">bodem</trd>
     <trd lng="ru">&c_d;&c_n;&c_o;</trd>
   </snc>
   <snc>
     <dif>
       Plej malanta&ubreve;a parto de io:
       <ekz>
         <tld/>o de &ccirc;ambro<fnt>B</fnt>, koridoro, scenejo, arbaro.
       </ekz>
     </dif>
   </snc>
   <snc>
     <dif>
       Efektive a&ubreve; &scirc;ajne plej malanta&ubreve;a parto de tio,
       kion oni vidas, konsiderata kontraste al la anta&ubreve;aj partoj:
       <ekz>
         <tld/>o de pejza&gcirc;o, horizonto, pentra&jcirc;o.
       </ekz>
     </dif>
     <ref tip="vid" cel="fon.0o">fono</ref>
     <trd lng="de">Hintergrund</trd>
     <trd lng="nl">achtergrond</trd>
     <trd lng="ru">&c_f;&c_o;&c_n;</trd>
   </snc>
   <snc>
     <uzo tip="fak">TEKS</uzo>
     <dif>
       Supra&jcirc;o de teksa&jcirc;o, sur kiu kontraste elvidi&gcirc;as
       desegnitaj a&ubreve; broditaj ornama&jcirc;oj:
       <ekz>
         ru&gcirc;a strio, verda stelo sur blanka <tld/>o.
       </ekz>
     </dif>
     <ref tip="vid" cel="fon.0o">fono.</ref>
     <trdgrp lng="de">
       <trd>Hintergrund</trd>,
       <trd>Grund</trd>
     </trdgrp>
     <trd lng="nl">achtergrond</trd>
   </snc>
   <snc>
     <uzo tip="stl">FIG</uzo>
     <dif>
       Plej intima, ka&scirc;ita parto:
       <ekz>
         danki el la <tld/>o de la koro;
       </ekz>
       <ekz>
         en la <tld/>o de sia koro li malamas &scirc;in.
       </ekz>
     </dif>
     <trd lng="de">Grund</trd>
     <trd lng="nl">grond</trd>
     <trd lng="ru">&c_g;&c_l;&c_u;&c_b;&c_i;&c_n;&c_a;</trd>
   </snc>
   <snc>
     <uzo tip="stl">FIG</uzo>
     <dif>
       Plej esenca parto de io:
       <ekz>
         la <tld/>o de la homa sa&gcirc;eco.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="baz.0o">bazo</ref>,
       <ref cel="fin.0o">fino</ref>,
       <ref cel="fundam.0o">fundamento</ref>,
       <ref cel="kern.0o">kerno</ref>
     </refgrp>
     <trd lng="nl">kern</trd>
     <trd lng="ru">&c_o;&c_s;&c_n;&c_o;&c_v;&c_a;</trd>
   </snc>
   <snc>
     <uzo tip="stl">FIG</uzo>
     <dif>
       Plej alta grado de malboneco:
       <ekz>
         la <tld/>o de la mizero<fnt>B</fnt>, de la malvirto.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="eg.0eco">egeco</ref>,
       <ref cel="ekstre.0o">ekstremo</ref>,
       <ref cel="zenit.0o">zenito</ref>,
       <ref cel="punkt.0o">plej supra punkto</ref>.
     </refgrp>
     <trd lng="nl">toppunt</trd>
     <trd lng="ru">&c_g;&c_l;&c_u;&c_b;&c_i;&c_n;&c_a;</trd>
   </snc>
   <adm>
     Reverkenda. Pluraj sencoj malaperis en PIV1. Cetere
     la kontrastigo kun "fono" ne estas suficxe klara.
     <aut>MB</aut>
   </adm>
   <trd lng="en">bottom</trd>
   <trd lng="es">fondo</trd>
   <trdgrp lng="fr">
     <trd>fond<klr tip="ind"> (de rivi&egrave;re, r&eacute;cipient, pi&egrave;ce...)</klr></trd>,
     <trd>bout</trd>
   </trdgrp>
</drv>

<drv mrk="fund.0a">
   <kap><tld/>a</kap>
   <snc>
     <dif>
       Estanta en la <tld/>o:
       <ekz>
         <tld/>a akvo, &scirc;limo, fe&ccirc;o, fi&scirc;o.
       </ekz>
     </dif>
     <trd lng="nl">diep</trd>
     <trdgrp lng="ru">
       <trd>&c_d;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>,
       <trd>&c_g;&c_l;&c_u;&c_b;&c_i;&c_n;&c_n;&c_y;&c_j;</trd>
     </trdgrp>
   </snc>
   <snc>
     <uzo tip="stl">FIG</uzo>
     <dif>
       Penetranta la esencon de la afero:
       <ekz>
         <tld/>aj konoj;
       </ekz>
       <ekz>
         <tld/>a klarigo.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="kern">kerna</ref>,
       <ref cel="profun.0a">profunda</ref>.
     </refgrp>
     <trdgrp lng="fr">
       <trd>de <ind>fond</ind></trd>,
       <trd>du <ind>fond</ind></trd>,
       <trd>approfondi</trd>,
       <trd>essentiel</trd>,
       <trd>foncier<klr tip="ind"> (essentiel)</klr></trd>,
       <trd>profond<klr tip="ind"> (essentiel)</klr></trd>
     </trdgrp>
     <trd lng="nl">diep</trd>
     <trdgrp lng="ru">
       <trd>&c_g;&c_l;&c_u;&c_b;&c_o;&c_k;&c_i;&c_j;</trd>,
       <trd>&c_o;&c_s;&c_n;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
     </trdgrp>
   </snc>
</drv>

<drv mrk="fund.0e">
   <kap><tld/>e</kap>
   <snc>
     <dif>
       &Ccirc;e la <tld/>o.
     </dif>
     <trd lng="nl">in de <ind>diepte</ind></trd>
     <trd lng="ru">&c_n;&c_a; &c_d;&c_n;&c_je;</trd>
   </snc>
   <snc>
     <uzo tip="stl">FIG</uzo>
     <dif>
       Plene, plendetale:
       <ekz>
         <tld/>e scipovi lingvon.
       </ekz>
     </dif>
     <trd lng="nl">grondig</trd>
     <trdgrp lng="ru">
       <trd>&c_o;&c_s;&c_n;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;</trd>,
       <trd>&c_d;&c_o;&c_s;&c_k;&c_o;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_o;</trd>
     </trdgrp>
   </snc>
   <trdgrp lng="fr">
     <trd>&agrave; <ind>fond</ind></trd>,
     <trd>au <ind>fond</ind></trd>,
     <trd>sur le <ind>fond</ind></trd>,
     <trd>de mani&egrave;re <ind>approfondie</ind></trd>
   </trdgrp>
</drv>

<drv mrk="fund.0ede">
   <kap><tld/>e de</kap>
   <gra><vspec>prepozicia&jcirc;o</vspec></gra>
   <snc>
     <dif>
       <ref tip="super" cel="en.0.kie">En</ref> la malsupra, baza, fundamenta parto de:
       <ekz>
         <tld/>e de la maro;
       </ekz>
       <ekz>unu el la esperoj ku&scirc;antaj <tld/>e de nia koro
       <fnt><bib>BonaLingvo</bib>, <lok>p. 107a</lok></fnt>.</ekz>
     </dif>
   </snc>
   <trd lng="fr">au fond de</trd>
   <trd lng="ru">&c_v; &c_g;&c_l;&c_u;&c_b;&c_i;&c_n;&c_je;</trd>
</drv>

<drv mrk="fund.gxis0e">
   <kap>&gcirc;is<tld/>e</kap>
   <snc>
     <dif>
       &Gcirc;is la <tld/>o.
     </dif>
     <trd lng="ru">&c_d;&c_o; &c_d;&c_n;&c_a;</trd>
   </snc>
   <snc>
     <uzo tip="stl">FIG</uzo>
     <dif>
       <tld lit="F"/>e 2:
       <ekz>
         esplori &gcirc;is<tld/>e la aferon.
       </ekz>
     </dif>
     <trd lng="de">gr&uuml;ndlich</trd>
     <trd lng="nl">grondig</trd>
     <trdgrp lng="ru">
       <trd>&c_o;&c_s;&c_n;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;</trd>,
       <trd>&c_d;&c_o;&c_s;&c_k;&c_o;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_o;</trd>
     </trdgrp>
   </snc>
   <trd lng="es">hasta el <ind>fondo</ind></trd>
   <trdgrp lng="fr">
     <trd>&agrave; <ind>fond</ind></trd>,
     <trd>jusqu'au <ind>fond</ind></trd>,
     <trd>de mani&egrave;re <ind>approfondie</ind></trd>
   </trdgrp>
</drv>

<drv mrk="fund.sen0ajxo">
   <kap>sen<tld/>a&jcirc;o<fnt>Z</fnt></kap>
   <snc>
     <ref tip="dif" cel="abism.0o">abismo</ref>
   </snc>
   <trd lng="de">Abgrund</trd>
   <trdgrp lng="fr">
     <trd>abime</trd>,
     <trd>pr&eacute;cipice</trd>
   </trdgrp>
   <trd lng="nl">afgrond</trd>
   <trd lng="ru">&c_b;&c_je;&c_z;&c_d;&c_n;&c_a;</trd>
</drv>

<drv mrk="fund.sur0ajxo">
   <kap>sur<tld/>a&jcirc;o</kap>
   <snc mrk="fund.sur0ajxo.MIN">
   <uzo tip="fak">MIN</uzo>
     <dif>
       &Scirc;limo el solidaj materioj, kiuj amasi&gcirc;as sur
       la <tld/>o de flua&jcirc;oj.
     </dif>
     <refgrp tip="vid">
       <ref cel="rekrem.0o">rekremento</ref>,
       <ref cel="sedime.0o">sedimento</ref>
     </refgrp>
     <trd lng="de"><klr tip="ind">(Boden-)</klr>Satz</trd>
     <trd lng="ru">&c_o;&c_s;&c_a;&c_d;&c_o;&c_k;</trd>
   </snc>
   <snc mrk="fund.sur0ajxo.FIG">
     <uzo tip="stl">ARK</uzo>
     <uzo tip="stl">FIG</uzo>
     <dif>
       Plej malbona, plej malestiminda parto:
       <ekz>
         la sur<tld/>a&jcirc;o de la lo&gcirc;antaro<fnt>Z</fnt>.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="fecx.0o">fe&ccirc;o</ref>
     </refgrp>
     <trd lng="de">Abschaum</trd>
     <trd lng="ru">&c_o;&c_t;&c_b;&c_r;&c_o;&c_s;&c_y;<klr tip="amb"> (&c_o;&c_b;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_a; 
&c_i; &c_t;.&c_p;.)</klr></trd>
   </snc>
   <trdgrp lng="fr">
     <trd>lie</trd>,
     <trd>fond<klr tip="ind"> (lie)</klr></trd>,
     <trd>rebut<klr tip="ind"> (fig.)</klr></trd>
   </trdgrp>
   <trd lng="nl">sediment</trd>
</drv>
</art>
