<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ornitolog.xml,v 1.15 2023/10/20 13:11:48 revo Exp $">
<kap>
  <ofc>7</ofc><rad>ornitolog</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ornitolog.0o">
  <kap><ofc>7</ofc><tld/>o</kap>
  <snc mrk="ornitolog.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="ornito.0isto.ZOO">Ornitologiisto</ref>:
      <ekz>
        <tld/>oj &ndash; kaj aliaj homoj, kiuj &scirc;atas havi Esperanto-nomojn
        por birdoj &ndash;, konigu<klr>[&#x2026;]</klr>
        <fnt>
        <aut>W. Pilger</aut>
        <vrk>Komunlingvaj nomoj de E&ubreve;ropaj birdoj</vrk>
        <lok>soc.culture.esperanto, 2002-05-10</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam rara birdo formortonta estas savita, <tld/>oj jubilas
        <fnt>
          <bib>Monato</bib>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url ref="&Monato;/2004/008386.html">Sociolingvistoj &ccirc;u
          glacikoraj?</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve; slovakaj <tld/>oj en Slovakio &ccirc;iujare nestas &ccirc;irka&ubreve; 1350 paroj da blankaj
        cikonioj
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2012/010711.php">Cikonioj interrete</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        la insulo, kiu apartenas al loka nobelo, nun estas protektata <tld/>ia kaj
        botanika rezervejo
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2009/009797.php">Pli pri la mistera
          insulo</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_n;&c_ib;&c_t;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="cs">ornitolog</trd>
  <trd lng="de">Ornithologe</trd>
  <trd lng="en">ornithologist</trd>
  <trd lng="fr">ornithologiste</trd>
  <trdgrp lng="ja">
    <trd>&#x9CE5;&#x5B66;&#x8005;  <pr>&#x3061;&#x3087;&#x3046;&#x304C;&#x304F;&#x3057;&#x3083;</pr></trd>,
    <trd>&#x9CE5;&#x985E;&#x5B66;&#x8005;  <pr>&#x3061;&#x3087;&#x3046;&#x308B;&#x3044;&#x304C;&#x304F;&#x3057;&#x3083;</pr></trd>
  </trdgrp>
  <trd lng="nl">ornitholoog</trd>
  <trd lng="pl">ornitolog</trd>
  <trdgrp lng="pt">
    <trd>ornit&oacute;logo</trd>,
    <trd>ornitologista</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_r;&c_n;&c_i;&c_t;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="sk">ornitol&oacute;g</trd>
  <trd lng="uk">&#x43E;&#x440;&#x43D;&#x456;&#x442;&#x43E;&#x43B;&#x43E;&#x433;</trd>
</drv>
</art>

