<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Kimri</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kimri.0o">
  <kap><tld/>o</kap>
  <snc mrk="kimri.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
     <dif>
      <ref tip="dif" cel="kimr.0ujo.GEOG">Kimrujo</ref>
      <ekz>
        en la nordo de <tld lit="K"/>o oni ofte a&ubreve;das plendojn ke la 
        Nacia Asembleo okupi&gcirc;as &ccirc;efe pri la sud-oriento de la 
        lando
        <fnt>
          <aut>H. Chapman</aut>,
          <vrk>en <url ref="http://eriketo.blogspot.com/2011/02/referendumo-en-kimrio.html">
          Referendumo en Kimrio</url></vrk>,
          <lok>2011-02-11</lok>
        </fnt>;
      </ekz>
      <ekz>
        en Anglio kaj <tld/>o oni povas senobstakle registri infanojn per keltaj nomoj
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="https://www.monato.be/2003/008157.html">Nomo egala, sed esenco mala</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Wales</trd>
  </snc>
  <trd lng="cs">Wales</trd>
  <trd lng="en">Wales</trd>
  <trdgrp lng="fr">
    <trd>Galles</trd>,
    <trd>pays de Galles</trd>
  </trdgrp>
  <trd lng="pl">Walia</trd>     
  <trd lng="ro">&#538;ara Galilor</trd>
  <trd lng="sk">Wales</trd>
</drv>

<drv mrk="kimri.novsud0o">
  <kap>Novsud<tld lit="k"/>o</kap>
  <snc mrk="kimri.novsud0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="dif" cel="kimr.novasud0ujo.GEOG">Nova Sudkimrujo</ref>:
      <ekz>
        Novsud<tld lit="k"/>o estas konata en la tuta mondo pro la 
        bela haveno de sia &ccirc;efurbo, Sidnejo
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Novsudkimrio">
          Novsudkimrio</url>, 2014-07-14</lok>
        </fnt>;
      </ekz>
      <ekz>
        la A&ubreve;stralia Nov-Sud-<tld/>a Societo pri La&ubreve;vola E&ubreve;tanazio
        <fnt>
          <bib>Monato</bib>
          <aut>Kep Enderby</aut>
          <vrk><url ref="https://www.monato.be/2005/008600.php">Morto, morfino kaj moralo</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Neus&uuml;dwales</trd>
  </snc>
  <trd lng="en">New South Wales</trd>
  <trd lng="fr">Nouvelle Galles du sud</trd>
  <trd lng="pl">Nowa Po&lstroke;udniowa Walia</trd>     
  <trd lng="ro">Noul Wales de Sud</trd>
</drv>

</art>

