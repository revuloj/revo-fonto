<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bar1.xml,v 1.2 2024/01/04 07:24:25 afido Exp $">
<kap>
    <rad>bar</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="bar1.0o">
  <kap><tld/>o</kap>
  <mlg>bar</mlg>
  <snc mrk="bar1.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
       <ref tip="lst" cel="unu.mezur0o" lst="voko:mezurunuoj">Mezurunuo</ref>
       de <ref cel="prem.0o.FIZ">premo</ref>
       uzata en submara medio, en industrio, en meterologio;
       unu baro egalas al 100000 <ref tip="vid" cel="paskal.p0o.FIZ">paskaloj</ref>:
      <ekz>
        pumpi <klr>[&#8230;]</klr> biciklajn pne&ubreve;ojn &gcirc;is aerpremo de 4 <tld/>oj
        <fnt>
          <aut>R. Haveman</aut>
          <vrk><url ref="http://www.tekstoj.nl/esperanto/artikoloj/homoniemen.htm">
          Homoniemen</url></vrk>
          <lok>[vidita en 2023]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="hom">
    <ref cel="bar.0o.KOMUNE">baro</ref>
  </refgrp>
  <refgrp tip="vid">
    <ref cel="barogr.0o">barografo</ref>,
     <ref cel="barome.0o">barometro</ref>,
    <ref cel="barosk.0o">baroskopo</ref>
  </refgrp>
  <trd lng="be">&c_b;&c_a;&c_r; <klr tip="amb">(&c_a;&c_d;&c_z;&c_ib;&c_n;&c_k;&c_a; &c_v;&c_y;&c_m;&c_ja;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>
  <trd lng="fr">bar<klr> (unit&eacute;)</klr></trd>
</drv>
</art>

