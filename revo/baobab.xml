<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>baobab</rad>/o
</kap>
<drv mrk="baobab.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio 
      <ref tip="super" cel="bombak.0acoj">bombakacoj</ref>
      (<trd lng="la">Adansonia</trd>), tropika arbo kun dikega 
      trunko kaj acidetaj kukumformaj fruktoj.
    </dif>
  </snc>
  <trdgrp lng="de"><trd>Baobab</trd>, <trd>Affenbrotbaum</trd></trdgrp>
</drv>
</art>
