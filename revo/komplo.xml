<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: komplo.xml,v 1.23 2023/12/03 09:58:54 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>komplot</rad>/o
</kap>
<drv mrk="komplo.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Sekreta ligo por malica a&ubreve; krima celo kontra&ubreve; iu:

      <ekz>
        <nom>Vinicius</nom> estis perfidita kaj murdita sekve de <tld/>o
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> eltrovis kaj denuncis la <tld/>on
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        pa&gcirc;io tre sindona al la juna sinjoro a&ubreve;dis &ccirc;ion kaj sciigis al li la <tld/>on
        <fnt>
          <bib>ElektFab</bib>
          <lok>La kura&gcirc;a tajloreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i suspektas <klr>[&#x2026;]</klr> <tld/>ojn &ccirc;ie
        <fnt>
          <bib>ChC</bib>
          <lok>11</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="konspi.0o">konspiro</ref>
    <refgrp tip="vid">
      <ref cel="intrig.0o">intrigo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_m;&c_o;&c_v;&c_a;</trd>
  <trd lng="ca">complot</trd>
  <trdgrp lng="cs">
    <trd>komplot</trd>,
    <trd>sdru&zcaron;en&iacute;</trd>,
    <trd>spiknut&iacute;</trd>,
    <trd>spol&ccaron;en&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Verschw&ouml;rung</trd>,
    <trd>Intrige</trd>,
    <trd>Komplott</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>conspiracy</trd>,
    <trd>intrigue</trd>,
    <trd>plot</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>complot</trd>,
    <trd>cabale</trd>,
    <trd>conspiration</trd>,
    <trd>intrigue<klr tip="ind"> (machination)</klr></trd>,
    <trd>machination</trd>,
    <trd><ind>manigance</ind>s</trd>
    <trd><ind>men&eacute;e</ind>s</trd>,
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&ouml;sszeesk&uuml;v&eacute;s</trd>,
    <trd>b&udblac;nsz&ouml;vetkez&eacute;s</trd>,
    <trd>b&udblac;nsz&ouml;vev&eacute;ny</trd>,
    <trd>komplott</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x9670;&#x8B00;  <pr>&#x3044;&#x3093;&#x307C;&#x3046;</pr></trd>,
    <trd>&#x7B56;&#x8B00;  <pr>&#x3055;&#x304F;&#x307C;&#x3046;</pr></trd>,
    <trd>&#x8B00;&#x8B70;  <pr>&#x307C;&#x3046;&#x304E;</pr></trd>,
    <trd>&#x5BC6;&#x8B70;  <pr>&#x307F;&#x3064;&#x304E;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>complot</trd>,
    <trd>samenzwering</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>spisek</trd>,
    <trd>sprzysi&eogonek;&zdot;enie</trd>,
    <trd>zmowa</trd>
  </trdgrp>   
  <trdgrp lng="pt">
    <trd>trama</trd>,
    <trd>conjura&ccedil;&atilde;o</trd>,
    <trd>conluio</trd>,
    <trd>compl&ocirc;</trd>
  </trdgrp>
  <trd lng="ro">conspira&#x21B;ie</trd>
  <trd lng="ru">&c_z;&c_a;&c_g;&c_o;&c_v;&c_o;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>komplot</trd>,
    <trd>sprisahanie</trd>
  </trdgrp>
  <trd lng="uk">&#x437;&#x43C;&#x43E;&#x432;&#x430;</trd>
  <trdgrp lng="zh">
    <trd>&#x5BC6;&#x8B00; <pr>m&#xEC;m&#xF3;u</pr></trd>,
    <trd>&#x9634;&#x8C0B; <pr>y&#x12B;nm&#xF3;u</pr></trd>,
    <trd>&#x5171;&#x8B00;&#x7F6A; <pr>g&#xF2;ngm&#xF3;uzu&#xEC;</pr></trd>,
    <trd>&#x5171;&#x8B00; <pr>g&#xF2;ngm&#xF3;u</pr></trd>
  </trdgrp>
</drv>

<drv mrk="komplo.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on:
      <ekz>
        kontra&ubreve; kiu <tld/>as la malpiuloj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>?
      </ekz>
      <ekz>
        la malgranda pastreto alprenis mienon de <tld/>anto
        <fnt>
          <bib>ChB</bib>
          <lok>10</lok>
        </fnt>;
      </ekz>
      <ekz>
        unuj Dioj <tld/>as militon kaj batalas kontra&ubreve; aliaj Dioj
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Du</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia instinkto de &jcirc;aluzema virino &scirc;in avertis, ke io estas <tld/>ita
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekkvara</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="konspi.0i.POL">konspiri<sncref/></ref>,
      <ref cel="jxur.kun0i">kun&jcirc;uri</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_l;&c_a;&c_d;&c_a;&c_c;&c_mol;
  <ind>&c_z;&c_m;&c_o;&c_v;&c_u;</ind></trd>
  <trd lng="ca">complotar</trd>
  <trdgrp lng="cs">
    <trd>kout pikle</trd>,
    <trd>spiknout se</trd>,
    <trd>zosnovat komplot</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sich <ind>verschw&ouml;ren</ind></trd>,
    <trd>ein <ind>Komplott</ind> schmieden</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>conspire</trd>,
    <trd>plot</trd>,
    <trd>scheme</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>comploter</trd>,
    <trd>conspirer</trd>,
    <trd>intriguer<klr tip="ind"> (comploter)</klr></trd>,
    <trd>manigancer</trd>,
    <trd>ourdir un complot</trd>,
    <trd>tramer un complot</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&ouml;sszesz&ouml;vetkezik</trd>,
    <trd>cimbor&aacute;l</trd>,
    <trd>&ouml;sszeesk&uuml;szik</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x9670;&#x8B00;&#x3092;&#x4F01;&#x3080;  <pr>&#x3044;&#x3093;&#x307C;&#x3046;&#x3092;&#x305F;&#x304F;&#x3089;&#x3080;</pr></trd>,
    <trd>&#x7B56;&#x8B00;&#x3092;&#x3081;&#x3050;&#x3089;&#x3059;  <pr>&#x3055;&#x304F;&#x307C;&#x3046;&#x3092;&#x3081;&#x3050;&#x3089;&#x3059;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>samenzweren</trd>,
    <trd>complotteren</trd>,
    <trd>samenspannen</trd>,
    <trd>conspireren</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>spiskowa&cacute;</trd>,
    <trd>knu&cacute;</trd>,
    <trd>sprzysi&aogonek;c si&eogonek;</trd>
  </trdgrp>
  <trd lng="ro">intrig&abreve;</trd>   
  <trd lng="ru">&c_s;&c_o;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;
  &c_z;&c_a;&c_g;&c_o;&c_v;&c_o;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>robi&tcaron; &uacute;klady</trd>,
    <trd>sprisaha&tcaron; sa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x437;&#x43C;&#x43E;&#x432;&#x43B;&#x44F;&#x442;&#x438;&#x441;&#x44F;</trd>,
    <trd>&#x440;&#x43E;&#x431;&#x438;&#x442;&#x438;/&#x447;&#x438;&#x43D;&#x438;&#x442;&#x438; &#x437;&#x43C;&#x43E;&#x432;&#x443;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5BC6;&#x8B00; <pr>m&#xEC;m&#xF3;u</pr></trd>,
    <trd>&#x5171;&#x8B00; <pr>g&#xF2;ngm&#xF3;u</pr></trd>,
    <trd>&#x56FE;&#x8C0B; <pr>t&#xFA;m&#xF3;u</pr></trd>,
    <trd>&#x9634;&#x8C0B; <pr>y&#x12B;nm&#xF3;u</pr></trd>,
    <trd>&#x6697;&#x7B97; <pr>&#xE0;nsu&#xE0;n</pr></trd>
  </trdgrp>
</drv>
</art>
