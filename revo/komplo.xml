<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>komplot</rad>/o
</kap>
<drv mrk="komplo.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Sekreta ligo por malica a&ubreve; krima celo kontra&ubreve; iu:
      <ekz>la &scirc;telistoj faris <tld/>on inter si.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="konspi.0o">konspiro</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_m;&c_o;&c_v;&c_a;</trd>
  <trdgrp lng="de">
     <trd>Verschw&ouml;rung</trd>,
     <trd>Intrige</trd>,
     <trd>Komplott</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>conspiracy</trd>,
    <trd>intrigue</trd>,
    <trd>plot</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>complot</trd>,
    <trd>samenzwering</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_g;&c_o;&c_v;&c_o;&c_r;</trd>
</drv>
<drv mrk="komplo.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on.  
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_l;&c_a;&c_d;&c_a;&c_c;&c_mol; <ind>&c_z;&c_m;&c_o;&c_v;&c_u;</ind></trd>
  <trdgrp lng="de">
     <trd>sich <ind>verschw&ouml;ren</ind></trd>,
     <trd>ein <ind>Komplott</ind> schmieden</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>conspire</trd>,
    <trd>plot</trd>,
    <trd>scheme</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol; &c_z;&c_a;&c_g;&c_o;&c_v;&c_o;&c_r;</trd>
</drv>
</art>
