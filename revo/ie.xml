<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ie.xml,v 1.23 2024/05/08 07:25:07 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ie</rad>
</kap>
<drv mrk="ie.0">
  <kap>
    <ofc>*</ofc><tld/>
  </kap>
  <snc>
    <dif>
      Adverbo signifanta &leftquot;en iu loko&rightquot;:
      <ekz>
        mi iam rekontis vin <tld/>, sed mi ne plu memoras kie;
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_je;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_d;&c_z;&c_je;-&c_n;&c_je;&c_b;&c_u;&c_d;&c_z;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_n;&c_ja;&c_k;&c_malmol;&c_d;&c_je;</trd>
  <trd lng="de">irgendwo</trd>
  <trdgrp lng="es">
   <trd>en alg&uacute;n <ind>lugar</ind></trd>,
   <trd>en alguna <ind>parte</ind></trd>
  </trdgrp>
  <trd lng="fr">quelque part</trd>
  <trd lng="hu">valahol</trd>
  <trd lng="it">da qualche <ind>parte</ind><klr> (stato in
  luogo)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#12393;&#12371;&#x304B;&#12391;</trd>,
    <trd>&#12354;&#x308B;&#12392;&#12371;&#x308D;&#x306B;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ergens</trd>,
    <trd>hier of daar</trd>,
    <trd>waar dan ook</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>gdzie&sacute;</trd>,
    <trd>gdziekolwiek</trd>,
    <trd>gdzie b&aogonek;d&zacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>em algum <ind>lugar</ind></trd>,
   <trd>algures</trd>
  </trdgrp>
  <trd lng="ro">undeva <klr> (fara precizarea locului)</klr> </trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_d;&c_je;-&c_t;&c_o;</trd>,
    <trd>&c_g;&c_d;&c_je;-&c_n;&c_i;&c_b;&c_u;&c_d;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">n&aring;gonstans</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_je;&c_s;&#x44C;</trd>,
    <trd>&c_d;&c_je;-&#x43D;&c_je;&c_b;&c_u;&c_d;&#x44C;</trd>,
    <trd>&c_b;&#x43E;&c_z;&#x43D;&c_a;-&c_d;&c_je;</trd>,
    <trd>&#x43A;&c_a;&c_z;&#x43D;&c_a;-&c_d;&c_je;</trd>,
    <trd>&c_h;&c_t;&#x43E;&c_z;&#x43D;&c_a;-&c_d;&c_je;</trd>
  </trdgrp>
  <trd lng="vo">egefo</trd>
  <trdgrp lng="zh">
    <trd>&#x67D0;&#22320; <pr>m&#x1D2;ud&#xEC;</pr></trd>,
    <trd>&#x67D0;&#22788; <pr>m&#x1D2;uch&#xF9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ie.0n">
  <kap>
    <ofc>*</ofc><tld/>n
  </kap>
  <snc>
    <dif>
      Adverbo signifanta &leftquot;en iun lokon&rightquot;:
      <ekz>
        la malvivan korpon li <tld/>n trenis for;
      </ekz>
      <ekz>
        ni iros <tld/>n en la montaro por ekskursi.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_u;&c_d;&c_y;&c_s;&c_mol;</trd>,
    <trd>&c_k;&c_u;&c_d;&c_y;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_k;&c_u;&c_d;&c_y;-&c_n;&c_je;&c_b;&c_u;&c_d;&c_z;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_n;&c_a;&c_n;&c_ja;&c_k;&c_malmol;&c_d;&c_je;</trd>
  <trd lng="de">irgendwohin</trd>
  <trd lng="it">da qualche <ind>parte</ind><klr> (moto a
  luogo)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#12393;&#12371;&#x304B;&#12408;</trd>,
    <trd>&#12354;&#x308B;&#12392;&#12371;&#x308D;&#12408;</trd>
  </trdgrp>
  <trd lng="nl">ergens heen</trd>
  <trdgrp lng="pl">
    <trd>dok&aogonek;d&sacute;</trd>,
    <trd>dok&aogonek;dkolwiek</trd>
  </trdgrp>
  <trd lng="ro">undeva</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_u;&c_d;&c_i;&c_s;&#x44C;</trd>,
    <trd>&#x43A;&c_u;&c_d;&c_i;-&#x43D;&c_je;&c_b;&c_u;&c_d;&#x44C;</trd>,
    <trd>&c_b;&#x43E;&c_z;&#x43D;&c_a;-&#x43A;&c_u;&c_d;&c_i;</trd>,
    <trd>&#x43A;&c_a;&c_z;&#x43D;&c_a;-&#x43A;&c_u;&c_d;&c_i;</trd>,
    <trd>&c_h;&c_t;&#x43E;&c_z;&#x43D;&c_a; &#x43A;&c_u;&c_d;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x65E0;&#x8BBA;&#x4F55;&#22788; <pr>w&#xFA;l&#xF9;nh&#xE9;ch&#xF9;</pr></trd>,
    <trd>&#x67D0;&#x4E00;&#x4E2A; <pr>m&#x1D2;uy&#x12B;g&#xE8;</pr></trd>
  </trdgrp>
</drv>
</art>
