<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>prus</rad>/o
</kap>
<drv mrk="prus.0o">
  <kap><tld/>o</kap>
  <snc mrk="prus.0o.balto">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Ano de <ref tip="super" cel="balt.0o">balta</ref> etno,
      kiu en la 1a jarmilo p.K. prilo&gcirc;is
      la teritorion de la suda bordo de la <tld lit="B"/>a Maro:
      <ekz>
        okcidentaj baltoj estis <tld/>oj
        <fnt>
        <aut>P. &Ccaron;eliauskas</aut>:
        <vrk>Litovio dum jarcentoj</vrk>.
        <lok>Kaunas, 2002. P. 13.</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_r;&c_u;&c_s;</trd>
    <trd lng="pl">Prus</trd>
    <trd lng="ru">&c_p;&c_r;&c_u;&c_s;&c_s;</trd>
  </snc>
  <snc mrk="prus.0o.prusujano">
    <uzo tip="fak">HIS</uzo>
    <dif>
      = <ref tip="sin" cel="prus.0ujano"><tld/>ujano</ref>.
    </dif>
  </snc>
</drv>

<drv mrk="prus.0a">
  <kap><tld/>a</kap>
  <snc mrk="prus.0a.balta">
    <dif>
      Rilata al la <ref cel="prus.0o.balto">prusoj</ref>:
      <ekz>
        <uzo tip="fak">LIN</uzo>
        la <ind><tld/>a lingvo</ind>.
        <trd lng="be">&c_p;&c_r;&c_u;&c_s;&c_k;&c_a;&c_ja; &c_m;&c_o;&c_v;&c_a;</trd>
        <trd lng="ru">&c_p;&c_r;&c_u;&c_s;&c_s;&c_k;&c_i;&c_j; &c_ja;&c_z;&c_y;&c_k;</trd>
      </ekz>
    </dif>
    <trd lng="ru">&c_p;&c_r;&c_u;&c_s;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
  <snc mrk="prus.0a.prusuja">
    <dif>
      <tld lit="P"/>uja.
    </dif>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_u;&c_s;&c_s;&c_a;&c_c;&c_k;&c_i;&c_j;</trd>
      <trd>&c_p;&c_r;&c_u;&c_s;&c_s;&c_k;&c_i;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="prus.0a.prusujana">
    <dif>
      <tld lit="P"/>ujana.
    </dif>
    <trd lng="be">&c_p;&c_r;&c_u;&c_s;&c_k;&c_ib;</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_u;&c_s;&c_s;&c_a;&c_c;&c_k;&c_i;&c_j;</trd>
      <trd>&c_p;&c_r;&c_u;&c_s;&c_s;&c_k;&c_i;&c_j;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="prus.P0ujo">
  <kap><tld lit="P"/>ujo</kap>
  <uzo tip="fak">HIS</uzo>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="prus.P0ujo.balta">
    <dif>
      La lando de la <ref cel="prus.0o.balto"><tld/>oj<sncref/></ref>.
    </dif>
  </snc>
  <snc mrk="prus.P0ujo.germana">
    <dif>
      La germana(j) lando(j) esti&gcirc;inta(j) sur la teritorio de
      de <ref cel="prus.0o.balto"><tld lit="P"/>ujo<sncref/></ref>.
    </dif>
  </snc>
  <trd lng="be">&c_P;&c_r;&c_u;&c_s;&c_ib;&c_ja;</trd>
  <trd lng="en">Prussia</trd>
  <trd lng="ru">&c_P;&c_r;&c_u;&c_s;&c_s;&c_i;&c_ja;</trd>
</drv>

<drv mrk="prus.0ujano">
  <kap><tld/>ujano</kap>
  <snc>
    <dif>
      Lo&gcirc;anto de <tld lit="P"/>ujo.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_u;&c_s;&c_a;&c_k;</trd>
  <trd lng="pl">Prusak</trd>
  <trd lng="ru">&c_p;&c_r;&c_u;&c_s;&c_s;&c_a;&c_k;</trd>
</drv>
</art>
