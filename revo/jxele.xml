<art mrk="$Id: jxele.xml,v 1.20 2019/04/19 16:10:17 revo Exp $">
<kap>
  <rad>&jcirc;ele</rad>/o
</kap>
<drv mrk="jxele.0o">
  <kap><tld/>o</kap>
  <snc mrk="jxele.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="dif" cel="gelate.0ajxo.KUI_frukta">Gelatena&jcirc;o<sncref/></ref>:
      <ekz>
        diversaj alman&gcirc;a&jcirc;oj, fi&scirc;oj en <tld/>o kaj majonezoj
        <fnt>
          <aut>Ilja Ilf, Evgenij Petrov, trad. Vladimir Okc</aut>
          <vrk><url ref="http://www.esperanto.mv.ru/Usono/truu4.html"
              >Unueta&gcirc;a Usono</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi revenis al mia kutima brita supermarkto kaj trovis <klr>[&#8230;]</klr> dinosa&ubreve;rajn
        <tld/>o-bombonojn
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9402-filmotondro.txt"
              >Filmotondro</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <adm>
    Vidu adm sub gelatena&jcirc;o.
    <aut>MB</aut>
  </adm>
  <trd lng="be">&c_zh;&c_e;&c_l;&c_je;</trd>
  <trd lng="bg">&c_zh;&c_je;&c_l;&c_je;</trd>
  <trdgrp lng="ca">
    <trd>gelea</trd>,
    <trd>melmelada</trd>
  </trdgrp>
  <trd lng="de">Gelee</trd>
  <trd lng="en">jelly</trd>
  <trd lng="es">gelatina</trd>
  <trdgrp lng="fa">
    <trd>&f_zh;&a_l;&a_h;</trd>,
    <trd>&a_l;&a_r;&a_z;&a_A;&a_n;&f_k;</trd>
  </trdgrp>
  <trd lng="fr">gel&eacute;e<klr> (de fruits)</klr></trd>
  <trdgrp lng="hu">
    <trd>zsel&eacute;</trd>,
    <trd>gy&uuml;m&ouml;lcskocsonya</trd>
  </trdgrp>
  <trd lng="nl">gelei</trd>
  <trd lng="pl">galareta</trd>
  <trd lng="pt">gel&eacute;ia</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_je;&c_l;&c_je;</trd>,
    <trd>&c_s;&c_t;&c_u;&c_d;&c_je;&c_n;&c_mol;</trd>
  </trdgrp>
  <trd lng="it">gelatina</trd>
</drv>
</art>
