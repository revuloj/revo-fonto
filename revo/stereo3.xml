<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stereo3.xml,v 1.23 2025/04/06 06:47:45 revo Exp $">
<kap>
  <rad>stereotip</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="stereo3.0o">
  <kap><tld/>o</kap>
  <snc mrk="stereo3.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Reliefe muldita plato uzata anstata&ubreve; moveblaj karakteroj
      por reprodukti multekzemplere ian presan komposta&jcirc;on.
    </dif>
    <refgrp tip="vid">
      <ref cel="klisx.0o.TIP">kli&scirc;o<sncref/></ref>.
    </refgrp>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_e;&c_r;&c_e;&c_a;&c_t;&c_y;&c_p;</trd>,
    <trd>&c_k;&c_l;&c_ib;&c_sh;&c_e;</trd>
  </trdgrp>
  <trd lng="de">Klischee</trd>
  <trd lng="en">stereotype</trd>
  <trdgrp lng="hu">
    <trd>sztereotip</trd>,
    <trd>t&ouml;m&ouml;ntv&eacute;ny</trd>
  </trdgrp>
  <trd lng="pl">stereotyp</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_l;&c_i;&c_sh;&c_je;</trd>,
    <trd>&c_s;&c_t;&c_je;&c_r;&c_je;&c_o;&c_t;&c_i;&c_p;</trd>
  </trdgrp>
  <trd lng="zh">&#21051;&#26495;&#21360;&#35937; <pr>k&#232;b&#462;ny&#236;nxi&#224;ng</pr></trd>
  </snc>
  <snc mrk="stereo3.0o.banalajxo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Banala&jcirc;o, multfoje ripetita sennuanca ideo a&ubreve; diro:
      <ekz>
        mi unue eksentis min la ruso mem, enkorpigo de
        &ccirc;iuj <tld/>oj pri mia nacio kaj lando
        <fnt>
          <aut>Stanislavo</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/stanislavo-belov/404307">
          Voja&gcirc;o al UK97: la 30-a de julio</url></vrk>,
          <lok>2012-08-14</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="klisx.0o.FIG">kli&scirc;o<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_e;&c_r;&c_e;&c_a;&c_t;&c_y;&c_p;</trd>,
    <trd>&c_k;&c_l;&c_ib;&c_sh;&c_e;</trd>,
    <trd>&c_b;&c_a;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
    <trd lng="pl">stereotyp</trd>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Anka&#365; eblas diri <ke>kli&#349;o</ke>, <ke>&#349;ablono</ke>, <ke>kli&#349;&#349;ablono</ke>, <ke>&#349;ablonkli&#349;o</ke>, <ke>&#285;enerala&#309;o</ke>, <ke>malorginala&#309;o</ke>.
  </rim>

  <trd lng="cs">stereotyp</trd>
  <trd lng="fr">st&eacute;r&eacute;otype</trd>
  <trdgrp lng="ja">
    <trd/>,
    <trd>&#12473;&#12486;&#12524;&#12458;&#12479;&#12452;&#12503;</trd>,
    <trd>&#32011;&#20999;&#12426;&#22411;  <pr>&#12418;&#12435;&#12365;&#12426;&#12364;&#12383;</pr></trd>
  </trdgrp>
  <trd lng="sk">stereotyp</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_je;&c_r;&c_je;&#1086;&c_t;&c_i;&#1087;</trd>,
    <trd>&#1082;&#1083;&c_ib;&c_sh;&c_je; </trd>
  </trdgrp>
</drv>

<drv mrk="stereo3.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="stereo3.0i.TIP">
    <dif>
      Reprodukti per <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_e;&c_r;&c_e;&c_a;&c_t;&c_y;&c_p;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>chovat se stereotypn&#283;</trd>,
    <trd>jednat stereotypn&#283;</trd>,
    <trd>matrici</trd>,
    <trd>vytvo&rcaron;it kli&scaron;&#233;</trd>
  </trdgrp>
  <trd lng="de">klischieren</trd>
  <trd lng="fr">st&eacute;r&eacute;otyper</trd>
  <trdgrp lng="hu">
    <trd>sztereotipet &ouml;nt</trd>,
    <trd>t&ouml;m&ouml;nt</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>stereotypowa&cacute;</trd>,
    <trd>odbija&cacute; ze  <ind>stereotypu</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_l;&c_i;&c_sh;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">jedna&tcaron; stereotypne</trd>
</drv>

<drv mrk="stereo3.0a">
  <kap><tld/>a</kap>
  <snc mrk="stereo3.0a.TIP">
    <dif>
      Reproduktita per <tld/>o.
    </dif>
  <trdgrp lng="hu">
    <trd>sztereotip</trd>,
    <trd>t&ouml;m&ouml;nt&ouml;tt</trd>
  </trdgrp>
  <trd lng="pl">stereotypowy</trd>
  </snc>
  <snc mrk="stereo3.0o.banala">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Banala, maloriginala:
      <ekz>
        li nur ripetas <tld/>ajn frazojn;
      </ekz>
      <ekz>
        mi komencis <tld/>an rakonton, kiun mi ripetadis jam multajn
        fojojn<fnt>B</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="rutin">rutina</ref>,
      <ref cel="gurd">gurdita</ref>.
    </refgrp>
    <trd lng="de">stereotyp</trd>
    <trdgrp lng="hu">
      <trd>sztereotip</trd>,
      <trd>sablonos</trd>
    </trdgrp>
    <trd lng="nl">stereotiep</trd>
    <trd lng="pl">stereotypowy</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_e;&c_r;&c_e;&c_a;&c_t;&c_y;&c_p;&c_n;&c_y;</trd>,
    <trd>&c_k;&c_l;&c_ib;&c_sh;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="fr">st&eacute;r&eacute;otyp&eacute;</trd>
  <trdgrp lng="ja">
    <trd>&#12473;&#12486;&#12525;&#29256;&#12398;
    <pr>&#12473;&#12486;&#12525;&#12399;&#12435;&#12398;</pr></trd>,
    <trd>&#12365;&#12414;&#12426;&#25991;&#21477;&#12398;
    <pr>&#12365;&#12414;&#12426;&#12418;&#12435;&#12367;&#12398;</pr></trd>,
    <trd>&#32011;&#20999;&#22411;&#12398; <pr>&#12418;&#12435;&#12365;&#12426;&#12364;&#12383;&#12398;</pr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_je;&c_r;&c_je;&c_o;&c_t;&c_i;&c_p;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_k;&c_l;&c_i;&c_sh;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_d;&c_r;&c_u;&#1082;&c_u;&c_v;&c_a;&#1085;&c_i;&c_j; &c_z;&c_ib; &c_s;&c_t;&c_je;&c_r;&c_je;&#1086;&c_t;&c_i;&#1087;&c_u;</trd>,
    <trd>&c_s;&c_t;&c_je;&c_r;&c_je;&#1086;&c_t;&c_i;&#1087;&#1085;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_t;&c_je;&c_r;&c_je;&#1086;&c_t;&c_i;&#1087;&#1085;&c_i;&c_j;</trd>,
    <trd>&c_sh;&c_a;&c_b;&#1083;&#1086;&#1085;&#1085;&c_i;&c_j;</trd>,
    <trd>&c_t;&c_r;&c_a;&c_f;&c_a;&c_r;&c_je;&c_t;&#1085;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20102;&#26080;&#26032;&#24847; <pr>li&#462;ow&#250;x&#299;ny&#236;</pr></trd>,
    <trd>&#20102;&#28961;&#26032;&#24847; <pr>li&#462;ow&#250;x&#299;ny&#236;</pr></trd>
  </trdgrp>
</drv>
</art>
