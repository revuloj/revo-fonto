<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>stereotip</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="stereo3.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Reliefe muldita plato uzata anstata&ubreve; moveblaj karakteroj por
      reprodukti multekzemplere ian presan komposta&jcirc;on.
    </dif>
    <refgrp tip="sin">
      <ref cel="klisx.0i">kli&scirc;i</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Klischee</trd>
  <trd lng="en">stereotype</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_l;&c_i;&c_sh;&c_je;</trd>,
    <trd>&c_s;&c_t;&c_je;&c_r;&c_je;&c_o;&c_t;&c_i;&c_p;</trd>
  </trdgrp>
</drv>

<drv mrk="stereo3.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Reprodukti per <tld/>o.
    </dif>
  </snc>
  <trd lng="de">klischieren</trd>
  <trd lng="ru">&c_k;&c_l;&c_i;&c_sh;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="stereo3.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Reproduktita per <tld/>o.
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Banala, maloriginala:
      <ekz>
        li nur ripetas <tld/>ajn frazojn;
      </ekz>
      <ekz>
        mi komencis <tld/>an rakonton, kiun mi ripetadis jam multajn
        fojojn<fnt>B</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="rutin">rutina</ref>,
      <ref cel="gurd">gurdita</ref>.
    </refgrp>
    <trd lng="de">stereotyp</trd>
    <trd lng="nl">stereotiep</trd>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_je;&c_r;&c_je;&c_o;&c_t;&c_i;&c_p;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_k;&c_l;&c_i;&c_sh;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
