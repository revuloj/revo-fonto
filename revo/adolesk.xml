<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: adolesk.xml,v 1.22 2023/10/27 13:42:09 revo Exp $">
<kap>
  <rad>adolesk</rad>/o
</kap>
<drv mrk="adolesk.0o">
  <kap><tld/>o</kap>
  <snc mrk="adolesk.0o.KOMUNE">
    <dif>
      Frua <ref cel="agx.jun0o">juna&gcirc;o</ref> de homo seksmatura,
      ankora&ubreve; ne plenkreskinta:
      <ekz>
        <nom>Tolstoj</nom> parolis pri &leftquot;la dezerto de l&#x2019; <tld/>o&rightquot;
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 12 (62)</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ctl><tld lit="A"/>o</ctl>
        <klr>(Biografia romano de Blazio <nom>Vaha</nom> el 1987)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pubert.0o">puberto</ref>
    </refgrp>
    <trd lng="zh">&#x82B1;&#x5B63; <pr>hu&#x101;j&#xEC;</pr></trd>
</snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_l;&c_je;&c_t;&c_k;&c_a;&c_v;&c_y; &c_w;&c_z;&c_r;&c_o;&c_s;&c_t;</trd>
  <trd lng="ca">adolesc&egrave;ncia</trd>
  <trd lng="cs">adolescence</trd>
  <trdgrp lng="de">
    <trd>Jugendalter</trd>,
    <trd>Adoleszenz</trd>
  </trdgrp>
  <trd lng="en">adolescence</trd>
  <trd lng="es">adolescencia</trd>
  <trd lng="fr">adolescence</trd>
  <trd lng="hu">kamaszkor</trd>
  <trd lng="it">adolescenza</trd>
  <trd lng="ms">remaja</trd>
  <trdgrp lng="nl">
    <trd>adolescentie</trd>,
    <trd>jongelingsjaren</trd>,
    <trd>jongemeisjesjaren</trd>
  </trdgrp>
  <trd lng="pl">dojrza&lstroke;o&sacute;&cacute;</trd>
  <trd lng="ro">adolescen&#x21B;&abreve;</trd>
  <trd lng="sk">vek dospievania</trd>
</drv>

<drv mrk="adolesk.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        fingrostampoj, kies malgrandeco signis infanon maksimume <tld/>an
        <fnt>
          <bib>ChR</bib>
          <lok>Kiu pafis al Pipulo?</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum sia adoleska periodo <klr>[&#x2026;]</klr>, <nom>Dante</nom>
        anka&ubreve; entreprenis novan
        <klr>[&#x2026;]</klr> poezian formon: vulgarlingvajn versojn rimitajn
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="https://www.monato.be/2004/008327.html">Pozitivaj kaj negativaj aspektoj de Dante</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj knabinoj, kiuj revas akiri iun princon,
        post kiam oni forigis iliajn
        dentokrampojn
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="https://www.monato.be/2005/008601.php">Torturado televida</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>pubert&auml;r</trd>,
    <trd>heranreifend</trd>,
    <trd>jugendlich</trd>
  </trdgrp>
</drv>

<drv mrk="adolesk.0i">
  <kap><tld/>i <fnt><bib>PIV1</bib></fnt></kap>
  <snc mrk="adolesk.0i.KOMUNE">
    <dif>
      Travivi <tld/>on:
      <ekz>
        kiam &scirc;i ankora&ubreve; <tld/>is, &scirc;i vokis la atenton de la pianisto
        <nom>Artur Rubinstein</nom>, kiu admiris &scirc;ian fortan muzikan
        temperamenton
        <fnt>
          <aut>F. Fonseca</aut>,
          <vrk><url ref="http://www.musicexpress.com.br/Artisto.asp?Artista=55">Sonia
          Rubinsky</url></vrk>,
          <lok>MusicExpress, [vidita je 2006-08-18]</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekde kiam mi <tld/>is, mi renkontis nur unu personon,
        kiu vere elmontris sinceran amon al mi
        <fnt>
          <bib>ChM</bib>
          <lok>7</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_y;&c_c;&c_mol; <ind>&c_p;&c_a;&c_d;&c_l;&c_je;&c_t;&c_k;&c_a;&c_m;</ind></trd>
  <trd lng="ca">&eacute;sser adolescent</trd>
  <trdgrp lng="de">
    <trd><ind>erwachsen</ind> werden</trd>,
    <trd>pubertieren</trd>
  </trdgrp>
  <trd lng="es">ser adolescente</trd>
  <trd lng="he">&gimel;&yod;&lamed; &he;&he;&tav;&bet;&gimel;&resh;&vav;&tav;</trd>
  <trd lng="hu">kamaszodni</trd>
  <trd lng="it">essere <ind>adolescente</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x9752;&#x5E74;&#x671F;&#x306B;&#x3042;&#x308B;  <pr>&#x305B;&#x3044;&#x306D;&#x3093;&#x304D;&#x306B;&#x3042;&#x308B;</pr></trd>,
    <trd>&#x601D;&#x6625;&#x671F;&#x306B;&#x3042;&#x308B;  <pr>&#x3057;&#x3057;&#x3085;&#x3093;&#x304D;&#x306B;&#x3042;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd><ind>jong</ind> zijn</trd>,
    <trd><ind>tiener</ind> zijn</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dojrzewa&cacute;</trd>,
    <trd>dorasta&cacute;</trd>,
  </trdgrp>
<trd lng="pt">adolescer<klr>(entrar na adolesc&ecirc;ncia)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_a;&c_v;&c_a;&c_t;&c_i; &c_d;&c_o;&c_r;&c_o;&c_s;&c_l;&c_i;&c_m;</trd>,
    <trd>&c_d;&c_o;&c_r;&c_o;&c_s;&c_l;&c_ib;&c_sh;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="adolesk.0ulo">
  <kap><tld/>ulo, <var><kap><tld/>anto</kap></var></kap>
  <snc mrk="adolesk.0ulo.KOMUNE">
    <dif>
      Juna <ref tip="vid" cel="hom.0o.ZOO">homo</ref> seksmatura,
      ankora&ubreve;
      ne plenkreskinta:
      <ekz>
        la sopiro &ccirc;i-vive renkonti perfektecon <klr>[&#x2026;]</klr> apartenas
        al la infana a&ubreve; <tld/>ula tavolo de nia psiko
        <fnt><bib>BonaLingvo</bib>, <lok>p. 107a</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>anto, kiu konfesis, parolis tute sincere, sed li
        rakontis son&gcirc;on
        <fnt>
          <bib>ChR</bib>
          <lok>Tien kaj reen</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x5341;&#x51E0;&#x5C81;&#x7684;&#x5C11;&#x5E74; <pr>sh&#xED;j&#x1D0;su&#xEC;desh&#xE0;oni&#xE1;n</pr></trd>,
    <trd>&#x5341;&#x5E7E;&#x6B72;&#x7684;&#x5C11;&#x5E74; <pr>sh&#xED;j&#x1D0;su&#xEC;desh&#xE0;oni&#xE1;n</pr></trd>,
    <trd>&#x5E74;&#x8F15;&#x4EBA; <pr>ni&#xE1;nq&#x12B;ngr&#xE9;n</pr></trd>,
    <trd>&#x5E74;&#x8F7B;&#x4EBA; <pr>ni&#xE1;nq&#x12B;ngr&#xE9;n</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_l;&c_je;&c_t;&c_a;&c_k;</trd>
  <trd lng="ca">adolescent</trd>
  <trdgrp lng="cs">
    <trd>dorostenec</trd>,
    <trd>dosp&iacute;vaj&iacute;c&iacute; mlad&iacute;k</trd>,
    <trd>junior adolescent</trd>
  </trdgrp>
  <trd lng="de">Jugendliche</trd>
  <trd lng="en">adolescent</trd>
  <trd lng="es">adolescente</trd>
  <trd lng="fr">adolescent<klr> (subst.)</klr></trd>
  <trd lng="he">&mem;&#x5B4;&tav;&bet;&#x5BC;&#x5B7;&gimel;&#x5B5;&resh;</trd>
  <trd lng="hu">kamasz</trd>
  <trd lng="it">adolescente</trd>
  <trd lng="ja">&#x9752;&#x5E74;&#x671F;&#x7537;&#x6027;  <pr>&#x305B;&#x3044;&#x306D;&#x3093;&#x304D;&#x3060;&#x3093;&#x305B;&#x3044;</pr></trd>
  <trdgrp lng="nl">
    <trd>tiener</trd>,
    <trd>adolescent</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dorastaj&aogonek;cy</trd>,
    <trd>nastolatek</trd>,
    <trd>m&lstroke;odzieniec</trd>
  </trdgrp>
  <trd lng="ro">adolescent</trd>
  <trd lng="sk">dospievaj&uacute;ci mlad&iacute;k</trd>
  <trdgrp lng="uk">
    <trd>&c_ju;&c_n;&c_a;&c_k;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_l;&c_ib;&c_t;&c_o;&c_k;</trd>
  </trdgrp>
</drv>
</art>

