<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>adolesk</rad>/o
</kap>
<drv mrk="adolesk.0o">
  <kap><tld/>o</kap>
  <snc mrk="adolesk.0o.KOMUNE">
    <dif>
      Frua juna&gcirc;o de homo seksmatura, ankora&ubreve; ne plenkreskinta:
      <ekz>
        <ctl><tld lit="A"/>o</ctl>
        <klr>(Biografia romano de Blazio Vaha el 1987)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pubert.0o">puberto</ref>
    </refgrp>
  </snc>
  <trd lng="fr">adolescence</trd>
</drv>

<drv mrk="adolesk.0i">
  <kap><tld/>i <fnt><bib>PIV1</bib></fnt></kap>
  <snc mrk="adolesk.0i.KOMUNE">
    <dif>
      Travivi <tld/>on:
      <ekz>
        kiam &scirc;i ankora&ubreve; <tld/>is, &scirc;i vokis la atenton de la pianisto 
        Artur Rubinstein, kiu admiris &scirc;ian fortan muzikan temperamenton  
        <fnt>
        <aut>F. Fonseca</aut>,
        <vrk><url ref="http://www.musicexpress.com.br/Artisto.asp?Artista=55">Sonia Rubinsky</url></vrk>,
        <lok>MusicExpress, [vidita je 2006-08-18]</lok></fnt>
      </ekz>
    </dif>
  </snc>
</drv>

<drv mrk="adolesk.0ulo">
  <kap><tld/>ulo, <var><kap><tld/>anto</kap></var></kap>
  <snc mrk="adolesk.0ulo.KOMUNE">
    <dif>
      Juna <ref tip="vid" cel="hom.0o.ZOO">homo</ref> seksmatura, ankora&ubreve; 
      ne plenkreskinta:
      <ekz>
        la sopiro &ccirc;i-vive renkonti perfektecon <klr>...</klr> apartenas al
        la infana a&ubreve; <tld/>ula tavolo de nia psiko
        <fnt><bib>BonaLingvo</bib>, <lok>p. 107a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">adolescent<klr> (subst.)</klr></trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>