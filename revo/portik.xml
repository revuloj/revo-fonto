<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>portik</rad>/o
</kap>
<drv mrk="portik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ARKI</uzo>
  <snc>
    <dif>
      Tegmentita galerio, subportata per kolonoj.
    </dif>
  </snc>
  <trd lng="de">Portikus</trd>
  <trdgrp lng="hu">
    <trd>portikusz</trd>,
    <trd>oszlopfolyos&oacute;</trd>,
    <trd>oszlopos <ind>el&odblac;csarnok</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zuilengang</trd>,
    <trd>portiek</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_r;&c_t;&c_i;&c_k;</trd>
</drv>
</art>
