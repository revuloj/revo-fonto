<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: menu.xml,v 1.33 2024/01/25 20:10:36 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>menu</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="menu.0o">
  <kap><tld/>o</kap>
  <snc mrk="menu.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="list.0o.KOMUNE">Listo</ref> proponata por
      elekto de eroj:
    </dif>
    <subsnc mrk="menu.0o.bankeda">
      <uzo tip="fak">KUI</uzo>
      <dif>
        Listo de man&gcirc;a&jcirc;oj de bankedo, oficiala a&ubreve;
        solena
        man&gcirc;o:
        <ekz>
          aran&gcirc;i delikatan, rafinitan <tld/>on<fnt>PIV</fnt>.
        </ekz>
      </dif>
      <trd lng="de">Men&uuml;</trd>
      <trdgrp lng="hu">
        <trd>men&uuml;</trd>,
        <trd>&eacute;trend</trd>
      </trdgrp>
      <trd lng="pl">menu</trd>
    <trdgrp lng="pt">
      <trd>card&aacute;pio</trd>,
      <trd>menu <klr>(de restaurantes, banquete)</klr></trd>
    </trdgrp>
    </subsnc>
    <subsnc mrk="menu.0o.restoracia">
      <uzo tip="fak">KUI</uzo>
      <ref tip="dif" cel="kart.mangxo0o.KUI">man&gcirc;okarto</ref>
      <trdgrp lng="de">
        <trd>Speisekarte</trd>,
        <trd>Men&uuml;</trd>
      </trdgrp>
      <trd lng="hu">&eacute;tlap</trd>
      <trdgrp lng="pl">
        <trd>jad&lstroke;ospis</trd>,
        <trd>karta da&nacute;</trd>,
        <trd>menu</trd>
      </trdgrp>
      <trdgrp lng="pt">
        <trd>card&aacute;pio</trd>,
        <trd>menu <klr>(de restaurante, banquete)</klr></trd>
      </trdgrp>
      <trd lng="ro">menu</trd>
    </subsnc>
    <subsnc mrk="menu.0o.KOMP">
      <uzo tip="fak">KOMP</uzo>
      <dif>
        En <ref tip="malprt" cel="dialog.0o.KOMP">dialogo</ref> kun
        komputilo, listo da komandoj a&ubreve; opcioj aperanta sur la
        ekrano, el kiu listo la uzanto elektas la bezonaton klavante
        numeron a&ubreve; literon, a&ubreve; almovante la kursoron
        per sagoklavoj a&ubreve; muso.
      </dif>
      <trd lng="de">Men&uuml;</trd>
      <trd lng="hu">men&uuml;</trd>
      <trd lng="pl">menu</trd>
      <trd lng="pt">menu <klr>(inform.)</klr></trd>
      <trd lng="ro">menu</trd>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="repert.0o">repertuaro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_n;&c_ju;</trd>
  <trd lng="bo">&#xF41;&#xF0B;&#xF63;&#xF42;&#xF0B;&#xF50;&#xF7C;&#xF0B;</trd>
  <trd lng="ca">men&uacute;</trd>
  <trd lng="cs">j&iacute;deln&iacute; l&iacute;stek</trd>
  <trd lng="de">Men&uuml;</trd>
  <trd lng="en">menu</trd>
  <trd lng="es">men&uacute;</trd>
  <trd lng="fr">menu</trd>
  <trd lng="ja">&#x30E1;&#x30CB;&#x30E5;&#x30FC;  <pr>&#x3081;&#x306B;&#x3085;&#x30FC;</pr></trd>
  <trd lng="nl">menu</trd>
  <trd lng="pl">menu</trd>
  <trd lng="pt">menu</trd>
  <trd lng="ro">menu</trd>
  <trd lng="ru">&c_m;&c_je;&c_n;&c_ju;</trd>
  <trdgrp lng="sk">
    <trd>jed&aacute;lny l&iacute;stok</trd>,
    <trd>menu</trd>
  </trdgrp>
  <trd lng="uk">&#x43C;&c_je;&#x43D;&#x44E;</trd>
  <trd lng="zh">&#x83DC;&#x5355;<klr> [c&agrave;id&amacron;n]</klr></trd>
</drv>

<drv mrk="menu.0ero">
  <kap><tld/>ero</kap> <fnt><bib>KompLeks</bib></fnt>
  <snc mrk="menu.0ero.KOMUNE">
    <dif>
      Unuopa elemento en <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_u;&c_n;&c_k;&c_t; <ind>&c_m;&c_e;&c_n;&c_ju;</ind></trd>,
    <trd>&c_p;&c_a;&c_z;&c_y;&c_c;&c_y;&c_ja; <ind>&c_m;&c_e;&c_n;&c_ju;</ind></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>plat</trd>,
    <trd>opci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Men&uuml;item</trd>,
    <trd>Men&uuml;punkt</trd>
  </trdgrp>
  <trd lng="es">elemento del men&uacute;</trd>
  <trd lng="fr">&eacute;l&eacute;ment de menu</trd>
  <trd lng="hu">men&uuml;pont</trd>
  <trd lng="nl">menu-onderdeel</trd>
  <trdgrp lng="pl">
    <trd>pozycja z <ind>menu</ind></trd>,
    <trd>element <ind>menu</ind></trd>,
    <trd>danie z <ind>menu</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>item do <ind>menu</ind></trd>,
    <trd>op&ccedil;&atilde;o do <ind>menu</ind></trd>
  </trdgrp>
  <trd lng="ro">mas&abreve;</trd>
  <trdgrp lng="zh">
    <trd>&#x83DC;&#x5355;&#x9879; <pr>c&#xE0;id&#x101;nxi&#xE0;ng</pr></trd>,
    <trd>&#x83DC;&#x55AE;&#x9805; <pr>c&#xE0;id&#x101;nxi&#xE0;ng</pr></trd>
  </trdgrp>
</drv>
</art>
