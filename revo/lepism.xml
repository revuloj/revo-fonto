<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lepism.xml,v 1.4 2024/06/28 15:27:45 afido Exp $">
<kap>
    <rad>lepism</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="lepism.0o">
  <kap><tld/>o</kap>
  <snc mrk="lepism.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Maldika <ref tip="lst" lst="voko:insektoj" cel="insekt.0o.ZOO">insekto</ref>
      ar&gcirc;entkolora, &scirc;atanta malsekajn varmajn lokojn, vivanta en arbaroj kaj en domoj;
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">genro</ref>
      <trd lng="la">Lepisma</trd>:
      <ekz>
        <tld/>oj voregas balaa&jcirc;on
        <fnt>
          <aut>Old&rcaron;ich Arno&scaron;t Fischer</aut>
          <vrk><url
          ref="http://www.literatura.bucek.name/brno/fischer_insektoj-en-niaj-hejmoj_140330.pdf"
              >Insektoj en niaj hejmoj</url></vrk>
          <lok>Primitivaj insektoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi &jcirc;us elliti&gcirc;is por iri en necesejon, &scirc;altante la lumon, mi ekvidis <tld/>on
        <fnt>
          <aut>Trinuks'</aut>
          <vrk><url ref="https://esperanto.masto.host/@trinukso/108676489528943916">
          mi &jcirc;us...</url></vrk>
          <lok>esperanto.masto.host, 2022-07-20</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/8/84/LepismaSaccharina.jpg/640px-LepismaSaccharina.jpg"
      prm="CC BY-SA 3.0"
      ><tld/>o <klr>(Lepisma saccharina)</klr>, korplongo: 1 cm
        <fnt>
          <aut>Christian Fischer, propra verko</aut>
          <lok><url ref="&WCW;/File:LepismaSaccharina.jpg"
              >https://commons.wikimedia.org/wiki/File:LepismaSaccharina.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_c;&c_u;&c_k;&c_r;&c_o;&c_w;&c_n;&c_ib;&c_k;</trd>
  <trd lng="de">Silberfischchen</trd>
  <trd lng="en">silverfish</trd>
  <trd lng="fr">poisson d'argent</trd>
  <trd lng="ru">&c_o;&c_b;&c_y;&c_k;&c_n;&c_o;&c_v;&c_je;&c_n;&c_n;&c_a;&c_ja; &c_ch;&c_je;&c_sh;&c_u;&c_j;&c_n;&c_i;&c_c;&c_a;</trd>
</drv>
</art>

