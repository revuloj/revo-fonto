<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>8</ofc>
      <rad>animal</rad>/o
    </kap>

    <drv mrk="animal.0o">
      <kap><tld/>o</kap>
      <snc mrk="animal.0o.BIO">
	<uzo tip="fak">BIO</uzo>
	<dif>
	  <ref tip="sin" cel="viv.0ulo">Vivulo</ref>,
	  <ref tip="super" cel="viv.0ajxo">viva&jcirc;o</ref> sin nutranta
	  per organika materialo kaj kies
	  &ccirc;eloj ne estas fortikigitaj per celulozo. Plejparte
	  <tld/>oj kapablas senti kaj sin movi:
	  <ekz>
	    eble temas pri atavismo, reveno al tiu epoko, kiam
	    la praulo de l' homo ne estis ankora&ubreve; <tld/>o socia
	    kaj vivis izole en sia kaverno
	    <fnt>
	      <aut>A. &Ccirc;e&hcirc;ov, tr. M. Luka&scirc;</aut>: <vrk>Enujulo</vrk>,
	      <lok>La Nica literatura revuo, 3:1</lok>
	    </fnt>;
	  </ekz><ekz>
	    nun imagu ke malanta&ubreve; la muro homoj portadas &ccirc;iajn
	    objektojn kaj figurojn de homoj kaj aliaj <tld/>oj,
	    faritajn el &scirc;tono kaj ligno kaj &ccirc;ia materialo
	    <fnt>
	      <aut>Platono, tr. D. Broadribb</aut>:
	      <vrk>La Respubliko</vrk>.
	      <lok>Parto 7a</lok>;
	    </fnt>;
	  </ekz><ekz>
	    la nomaro de <tld/>oj kaj
	    <ref cel="veget.0ajxo">vegeta&jcirc;oj</ref>
	    <fnt><bib>TermKurs</bib>, <lok>&para;4.2</lok>.</fnt>.
	  </ekz>
	</dif>
	<refgrp tip="sub">
	  <ref cel="best.0o.nehomo">besto<sncref/></ref>,
	  <ref cel="hom.0o">homo</ref>
	</refgrp>
	<trd lng="ru">&c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_o;&c_je;</trd>
      </snc>
      <snc mrk="animal.0o.EVI">
	<uzo tip="stl">EVI</uzo>
	<dif>
	  = <refgrp>
	    <ref cel="best.0o">besto</ref>,
	    <ref cel="brut.0o">bruto</ref>
	  </refgrp>
	  <ekz>
	    &scirc;i rapide respondis al &jcirc;urnalistoj, ke &scirc;i amas la
	    <tld/>ojn, ne estis &scirc;i kiu mortigis la vulpojn,
	    la &ccirc;in&ccirc;ilojn, la mustelojn aux vizonojn cetere
	    breditajn de la homoj
	    <fnt>
	      <aut>Stefan MAUL</aut>: <vrk>Vivi plu: kial? kiel?</vrk>
	      <bib>Monato</bib>.
	    </fnt>.
	  </ekz>
	</dif>
      </snc>
      <trd lng="be">&c_zh;&c_y;&c_v;&c_jo;&c_l;&c_ib;&c_n;&c_a;</trd>
      <trd lng="de">Tier</trd>
      <trd lng="en">animal</trd>
      <trd lng="es">aninal</trd>
      <trd lng="fr">animal</trd>
      <trd lng="hu">&aacute;llat<klr tip="ind"> (ember is)</klr></trd>
      <trd lng="it">animale</trd>
      <trd lng="nl">dier<klr tip="ind"> (incl.mens)</klr></trd>
      <trd lng="sk">&zcaron;ivo&ccaron;&iacute;ch</trd>
    </drv>

    <drv mrk="animal.0a">
      <kap><tld/>a</kap>
      <snc>
	<dif>
	  Rilata al la <tld/>oj, plej ofte kontraste al
	  <ctl>vegeta&jcirc;a</ctl>:
	  <ekz>
	    <tld/>a regno;
	  </ekz><ekz>
	    <tld/>aj fosilioj;
	  </ekz><ekz>
	    <tld/>a graso (ekz-e butero), kontraste al vegeta&jcirc;a graso
	    (ekz-e oleo).
	  </ekz>
	</dif>
      </snc>
      <trd lng="ru">&c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_y;&c_j;</trd>
    </drv>

    <drv mrk="animal.0oj">
      <kap><tld/>oj</kap>
      <uzo tip="fak">ZOO</uzo>
      <snc>
	<dif>
	  Unu el la <ref tip="malprt" cel="regn.0o.BIO">regnoj</ref>
	  de la viva&jcirc;oj (<trd lng="la">Animalia</trd>).
	</dif>
      </snc>
      <trd lng="en">Animal kingdom</trd>
      <trd lng="fr">r&egrave;gne animal</trd>
      <trd lng="nl">dierenrijk</trd>
      <trd lng="hu">&aacute;llatvil&aacute;g</trd>
      <trdgrp lng="ru">
	<trd>&c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_y;e</trd>,
	<trd>&c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_y;&c_j; &c_m;&c_i;&c_r;</trd>
      </trdgrp>
    </drv>

  </art>
