<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>emboli</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="emboli.0o">
  <kap><tld/>o</kap>
  <snc mrk="emboli.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Angia &scirc;topo ka&ubreve;zita de ensanga koagula&jcirc;o
      a&ubreve; <ref tip="vid" cel="embol.0o.MED">embolo</ref>:
      <ekz>
        <tld/>o de pulma arterio povas ka&ubreve;zi la morton.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_m;&c_b;&c_a;&c_l;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_je;&c_m;&c_b;&c_o;&c_l;&c_i;&c_ja;</trd>
  <trdgrp lng="de">
     <trd>Gef&auml;&szlig;verschluss</trd>,
     <trd>Emboli <klr>[fak]</klr></trd>
  </trdgrp>
  <trd lng="es">embolia</trd>
  <trd lng="fr">embolie</trd>
  <trd lng="it">embolia</trd>
  <trdgrp lng="pl">
    <trd>zator krwi</trd>,
    <trd>embolia</trd>
  </trdgrp>
  <trd lng="pt">embolia</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_m;&c_b;&c_o;&c_l;&c_i;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_u;&c_p;&c_o;&c_r;&c_k;&c_a;
    &c_k;&c_r;&c_o;&c_v;&c_je;&c_n;&c_o;&c_s;&c_n;&c_y;&c_h;
    &c_s;&c_o;&c_s;&c_u;&c_d;&c_o;&c_v;</trd>
  </trdgrp>
</drv>
</art>
