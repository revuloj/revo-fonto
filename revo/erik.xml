<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>erik</rad>/o
</kap>
<drv mrk="erik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="erikac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Erica</trd>),
      arbedo a&ubreve; arbusto kun rozaj a&ubreve; blankaj floroj,
      kreskanta en sablaj teroj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Heide</trd>,
    <trd>Erika</trd>
  </trdgrp>
  <trd lng="sv">ljung</trd>
</drv>

</art>
