<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: erik.xml,v 1.28 2022/04/27 14:38:54 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>erik</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="erik.0o">
  <kap><tld/>o</kap>
  <snc mrk="erik.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbedo a&ubreve; arbusto kun rozaj a&ubreve; blankaj floroj,
      kreskanta en sablaj teroj, <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">genro</ref>
      el familio <ref tip="super" cel="erikac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Erica</trd>):
      <ekz>
        kiel <tld/>o aspektis la pinarbaroj sur la altaj rokoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        kabanoj <klr>[&#x2026;]</klr> kovritaj per <tld/>eja <ref cel="torf.0o">torfo</ref> kaj per tavolo da <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        tie kaj tie staris fasketoj da <tld/>o, vakcinio kaj mirtelo, kiuj trovis iom da
        humo kaj akvo en la kavetoj kaj fenda&jcirc;oj de la grundo
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="oksiko.0o">oksikoko</ref>,
      <ref cel="vakcin1.0o">vakcinio</ref>
    </refgrp>
    <ref tip="hom" cel="erik1.0o">Eriko <klr>(nomo)</klr></ref>
    <bld lok="&WCU;/thumb/d/d0/Erica_baccans_flower.JPG/320px-Erica_baccans_flower.JPG" prm="PD"><tld/>o, Tablomonto, Sudafriko
        <fnt>
          <aut>Andrew massyn, propra verko</aut>
          <lok><url ref="&WCW;/File:Erica_baccans_flower.JPG">&WCW;/File:Erica_baccans_flower.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_v;&c_je;&c_r;&c_a;&c_s;</trd>
  <trdgrp lng="ca">
    <trd>bruc</trd>,
    <trd>bruguera</trd>
  </trdgrp>
  <trd lng="cs">v&rcaron;esovec</trd>
  <trdgrp lng="de">
    <trd>Heidekraut</trd>,
    <trd>Erika</trd>
  </trdgrp>
  <trd lng="en">heath</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_r;&f_y;&f_k;&a_A;</trd>,
    <trd>&a_kh;&a_l;&a_n;&f_g;</trd>
  </trdgrp>
  <trd lng="fr">bruy&egrave;re<klr> (plante)</klr></trd>
  <trdgrp lng="hu">
    <trd>hanga</trd>,
    <trd>erika</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>heide</trd>,
    <trd>heidekruid</trd>,
    <trd>erica</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wrzos</trd>,
    <trd>wrzosiec</trd>
  </trdgrp>
  <trd lng="pt">urze</trd>
  <trd lng="ru">&c_v;&c_je;&c_r;&c_je;&c_s;&c_k;
  &c_d;&c_r;&c_je;&c_v;&c_o;&c_v;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">vresovec</trd>
  <trd lng="sv">ljung</trd>
  <trd lng="uk">&#x435;&#x440;&#x438;&#x43A;&#x430;</trd>
</drv>

<drv mrk="erik.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="erik.0ejo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Regiono, loko surkreskata de <tld/>oj:
      <ekz>
        pli ol unu vintro pelis la ne&gcirc;on super la <tld/>ejo kaj super la alta&jcirc;o
        <fnt>
          <bib>Fab3</bib>
          <lok>Ib kaj malgranda kristino</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i tie sur la <tld/>ejo ekzistis aliaj trezoroj, &ccirc;i tie estis plene de <tld/>o
        kaj fragoj kaj <ref tip="prt" cel="mirtel.0o.planto">mirtelo</ref>j
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni lo&gcirc;as sur la <tld/>ejo, sur la seka <tld/>ejo
        <fnt>
          <aut>A. Kivi, trad. H. Hall</aut>
          <vrk><url ref="&Gutenberg;files/28968/28968-h/28968-h.htm">
          La Botistoj</url></vrk>
          <lok>1919</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="herb.0ejo">herbejo</ref>,
       <ref cel="marcx.0o.GEOG">mar&ccirc;o<sncref/></ref>
    </refgrp>
    <bld lok="&WCU;/thumb/3/3c/Haltern_am_See%2C_Westruper_Heide_--_2015_--_7965-9.jpg/640px-Haltern_am_See%2C_Westruper_Heide_--_2015_--_7965-9.jpg" prm="CC BY-SA 4.0"><tld/>ejo apud Haltern am See, Germanujo
        <fnt>
          <aut>Dietmar Rabich, propra verko</aut>
          <lok><url ref="&WCW;/File:Haltern_am_See,_Westruper_Heide_--_2015_--_7965-9.jpg">&WCW;/File:Haltern_am_See,_Westruper_Heide_--_2015_--_7965-9.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="ca">bruguerar</trd>
  <trd lng="cs">v&rcaron;esovi&scaron;t&ecaron;</trd>
  <trd lng="de">Heide</trd>
  <trdgrp lng="en">
    <trd>heath</trd>,
    <trd>moor</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>brande</trd>,
    <trd>bruy&egrave;re<klr> (lieu)</klr></trd>,
    <trd>lande<klr> (&agrave; bruy&egrave;res)</klr></trd>
  </trdgrp>
  <trd lng="pl">wrzosowisko</trd>
  <trd lng="sk">vresovi&scaron;te</trd>
  <trd lng="uk">&#x432;&#x435;&#x440;&#x435;&#x441;&#x43E;&#x432;&#x438;&#x449;&#x435;</trd>
</drv>
</art>
