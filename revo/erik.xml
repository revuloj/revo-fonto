<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>erik</rad>/o
</kap>
<drv mrk="erik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super"
      cel="erikac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Erica</trd>),
      arbedo a&ubreve; arbusto kun rozaj a&ubreve; blankaj floroj,
      kreskanta en sablaj teroj.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_je;&c_r;&c_a;&c_s;</trd>
  <trdgrp lng="de">
    <trd>Heide</trd>,
    <trd>Erika</trd>
  </trdgrp>
  <trd lng="en">heath</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_r;&f_y;&f_k;&a_A;</trd>,
    <trd>&a_kh;&a_l;&a_n;&f_g;</trd>
  </trdgrp>
  <trd lng="fr">bruy&egrave;re<klr tip="ind"> (plante)</klr></trd>
  <trdgrp lng="hu">
    <trd>hanga</trd>,
    <trd>erika</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>heide</trd>,
    <trd>heidekruid</trd>,
    <trd>erica</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wrzos</trd>,
    <trd>wrzosiec</trd>
  </trdgrp>
  <trd lng="pt">urze</trd>
  <trd lng="ru">&c_v;&c_je;&c_r;&c_je;&c_s;&c_k;
  &c_d;&c_r;&c_je;&c_v;&c_o;&c_v;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>
  <trd lng="sv">ljung</trd>
</drv>

</art>
