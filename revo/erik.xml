<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>erik</rad>/o
</kap>
<drv mrk="erik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="erik.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Erica</trd>),
      arbedo a&ubreve; arbusto kun rozaj a&ubreve; blankaj floroj,
      kreskanta en sablaj teroj.   
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Heide</trd>,
    <trd>Erika</trd>
  </trdgrp>
</drv>
<drv mrk="erik.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj (<trd lng="la">Ericaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Heidekrautgew&auml;chse</trd>
</drv>
</art>
