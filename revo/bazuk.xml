<kap>
  <rad>bazuk</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="bazuk.0o">
  <kap><tld/>o</kap>
  <snc mrk="bazuk.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Tubforma <ref tip="lst" lst="voko:bataliloj" cel="paf.0ilo">pafilo</ref>, portebla de unuopa homo,
      por kontra&ubreve;tankaj raketoj:
      <ekz>
        dum Berlina operaco sovetaj <klr>[&#x2026;]</klr> tankoj uzis kiel anta&ubreve;stariga blendo
        kontra&ubreve; timataj germanaj <tld/>oj risortajn kradojn el litoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=36396">Bazuko</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x53CD;&#x5766;&#x514B;&#x6B65;&#x67AA; <pr>f&#x1CE;nt&#x1CE;nk&#xE8;b&#xF9;qi&#x101;ng</pr></trd>,
    <trd>&#x53CD;&#x5766;&#x514B;&#x706B;&#x7BAD; <pr>f&#x1CE;nt&#x1CE;nk&#xE8;hu&#x1D2;ji&#xE0;n</pr></trd>,
    <trd>&#x53CD;&#x5766;&#x514B;&#x624B;&#x96F7;&#x53D1;&#x5C04;&#x5668; <pr>f&#x1CE;nt&#x1CE;nk&#xE8;sh&#x1D2;ul&#xE9;if&#x101;sh&#xE8;q&#xEC;</pr></trd>,
    <trd>&#x706B;&#x7BAD;&#x7B52; <pr>hu&#x1D2;ji&#xE0;nt&#x1D2;ng</pr></trd>,
    <trd>&#x706B;&#x7BAD;&#x70AE; <pr>hu&#x1D2;ji&#xE0;np&#xE0;o</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_b;&c_a;&c_z;&c_u;&c_k;&c_a;</trd>
  <trd lng="bg">&c_b;&c_a;&c_z;&c_u;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>bazuka</trd>,
    <trd>panc&eacute;&rcaron;ovka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Panzerb&uuml;chse</trd>,
    <trd>Panzerfaust</trd>,
    <trd>Bazooka</trd>
  </trdgrp>
  <trd lng="en">bazooka</trd>
  <trd lng="fr">bazooka</trd>
  <trdgrp lng="hu">
    <trd>rak&eacute;tavet&odblac;</trd>,
    <trd>p&aacute;nc&eacute;l&ouml;k&ouml;l</trd>,
    <trd>bazooka</trd>
  </trdgrp>
  <trd lng="it">bazooka</trd>
  <trd lng="ja">&#x30D0;&#x30BA;&#x30FC;&#x30AB;&#x7832;  <pr>&#x30D0;&#x30BA;&#x30FC;&#x30AB;&#x307B;&#x3046;</pr></trd>
  <trdgrp lng="pl">
    <trd>bazooka<klr> (przest.)</klr></trd>,
    <trd>bazuka<klr> (przest.)</klr></trd>,
    <trd>r&eogonek;czna wyrzutnia rakiet</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_a;&c_z;&c_u;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>bazuka</trd>,
    <trd>pancierov&aacute; p&auml;s&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&#x431;&#x430;&#x437;&#x443;&#x43A;&#x430;</trd>
</drv>
