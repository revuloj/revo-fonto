<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>cim</rad>/o
</kap>
<drv mrk="cim.0o">
<kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="cim.0o.hemiptero">
    <dif>
      Iu el <ref tip="malprt" cel="hemipt.0oj">hemipteroj</ref>.
    </dif>
    <trd lng="de">Wanze</trd>
  </snc>
  <snc mrk="cim.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="cim.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Cimex</trd>). 
    </dif>
    <trd lng="de">Plattwanze</trd>
  </snc>
  <snc mrk="cim.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      &Jcirc;argona vorto por eraro, programa fu&scirc;o 
      (neevitebla tradukprunto el la angla).
    </dif> 
    <trd lng="de">Bug</trd>
    <trd lng="en">bug</trd>
    <trd lng="fr">bogue</trd>
  </snc>
</drv>
<drv mrk="cim.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="hemipt.0oj">hemipteroj</ref>
      (<trd lng="la">Cimidae</trd>). Ili estas su&ccirc;as sangon, estas
      tre plataj, brunaj kaj havas degenerintajn flugilojn.
    </dif>
  </snc>
  <trd lng="de">Plattwanzen</trd>
</drv>
<drv mrk="cim.lit0o">
  <kap>lit<tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Specio de <ref tip="super" cel="cim.0o.genro"><tld/>o</ref>
      (<trd lng="la">Cimex lectularius</trd>),
      kiu trovi&gcirc;as precipe en litoj, kaj su&ccirc;as la sangon de
      homoj.
    </dif>
  </snc>
  <trd lng="de">Bettwanze</trd>
</drv>
</art>
