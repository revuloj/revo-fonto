<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cim.xml,v 1.34 2023/10/31 13:37:03 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>cim</rad>/o
</kap>
<drv mrk="cim.0o">
<kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="cim.0o.hemiptero">
    <dif>
      Iu el <ref tip="malprt" cel="hemipt.0oj">hemipteroj</ref>:
      <ekz>
        vi estas plata kiel <tld/>o
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_l;&c_o;&c_p;</trd>
    <trd lng="ca">hem&iacute;pter</trd>
    <trd lng="de">Wanze</trd>
    <trd lng="en">hemipteran</trd>
    <trd lng="es">hem&iacute;ptero</trd>
    <trd lng="hu">poloska</trd>
    <trd lng="it">cimice</trd>
    <trd lng="pl">pluskwa</trd>
    <trd lng="ru">&c_k;&c_l;&c_o;&c_p;</trd>
    <trdgrp lng="zh">
    <trd>&#x58C1;&#x8671; <pr>b&#xEC;sh&#x12B;</pr></trd>,
    <trd>&#x7A83;&#x542C;&#x5668; <pr>qi&#xE8;t&#x12B;ngq&#xEC;</pr></trd>,
    <trd>&#x81ED;&#x866B; <pr>ch&#xF2;uch&#xF3;ng</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="cim.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="cimed.0oj"><tld/>edoj</ref>
      (<trd lng="la">Cimex</trd>) de du sangosu&ccirc;aj specioj, unu el ili
      estas la <ref tip="sub" cel="cim.lit0o">litcimo</ref>:
      <ekz>
        abomena &ccirc;ambro, kaj <tld/>oj tiaj, kiajn mi nenie vidis: ili
        mordas kiel hundoj
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_l;&c_o;&c_p;</trd>
    <trd lng="ca">xinxa</trd>
    <trd lng="de">Plattwanze</trd>
    <trdgrp lng="en">
      <trd>bedbug</trd>
    </trdgrp>
    <trd lng="es">chinche</trd>
    <trd lng="fr">punaise <klr tip="ind">(insecte)</klr></trd>
    <trd lng="hu">poloska</trd>
    <trd lng="it">cimici<klr> (ematofaghe)</klr></trd>
    <trd lng="pl">pluskwa</trd>
    <trd lng="pt">percevejo</trd>
    <trd lng="ru">&c_k;&c_l;&c_o;&c_p;</trd>
  </snc>
  <snc mrk="cim.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      &Jcirc;argona vorto por miso, <ref tip="super" cel="erar.0o">eraro</ref>,
      programa <ref tip="super" cel="fusx.0o">fu&scirc;o</ref>
      (neevitebla tradukprunto el la angla):
      <ekz>
        riparado de cimoj okazas sesfoje pli rapide &ccirc;e liberaj programoj
        ol &ccirc;e komercaj
        <fnt>
          <bib>Monato</bib>
          <aut>D. E. Rogers</aut>
          <vrk><url ref="&Monato;/2004/008269.html">Libera programaro: nova rimedo komerci</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        tio estas <tld/>o en la versio, kiun mi el&scirc;utis
        <fnt>
          <bib>Monato</bib>
          <aut>Edmund Grimley Evans</aut>
          <vrk><url ref="&Monato;/2010/010031.php">Knoppix</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_m;&c_y;&c_l;&c_k;&c_a; <klr>(&c_u;
      &c_p;&c_r;&c_a;&c_g;&c_r;&c_a;&c_m;&c_je;)</klr></trd>,
      <trd>&c_b;&c_l;&c_y;&c_h;&c_a;</trd>,
      <trd>&c_b;&c_a;&c_g;</trd>,
      <trd>&c_z;&c_b;&c_o;&c_j;</trd>,
      <trd>&c_g;&c_l;&c_ju;&c_k;</trd>
    </trdgrp>
    <trd lng="ca">error <klr>(de programari)</klr></trd>
    <trdgrp lng="de">
      <trd>Bug</trd>,
      <trd>Fehler</trd>
    </trdgrp>
    <trd lng="en">bug</trd>
    <trdgrp lng="es">
      <trd>error<klr> (de programaci&oacute;n)</klr></trd>,
      <trd>bug</trd>
    </trdgrp>
    <trd lng="fr">bogue</trd>
    <trdgrp lng="hu">
      <trd>programhiba</trd>,
      <trd>bug</trd>
    </trdgrp>
    <trd lng="it">baco<klr> (inf.)</klr></trd>
    <trdgrp lng="pl">
      <trd>bug</trd>,
      <trd>pluskwa</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_sh;&c_i;&c_b;&c_k;&c_a;</trd>,
      <trd>&c_s;&c_b;&c_o;&c_j;</trd>,
      <trd>&c_zh;&c_u;&c_k;</trd>,
      <trd>&c_g;&c_l;&c_ju;&c_k;</trd>
    </trdgrp>
    <trd lng="zh">&#x7D15;&#x7E46; <pr>p&#x12B;mi&#xF9;</pr></trd>
</snc>
  <trd lng="cs">&scaron;t&ecaron;nice</trd>
  <trd lng="he">&#x5E4;&#x5E9;&#x5E4;&#x5E9;</trd>
  <trd lng="ja">&#x30D0;&#x30B0;</trd>
  <trd lng="sk">plo&scaron;tica</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43B;&#x43E;&#x43F;</trd>,
    <trd>&#x431;&#x43B;&#x43E;&#x449;&#x438;&#x446;&#x44F;</trd>,
    <trd>&#x436;&#x430;&#x440;&#x491; (&#x456;&#x43D;&#x444;&#x43E;&#x440;&#x43C;.)</trd>
  </trdgrp>
</drv>

<drv mrk="cim.lit0o">
  <kap>lit<tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_specioj" cel="speci.0o.BIO">Specio</ref>
      de <ref tip="super" cel="cim.0o.genro"><tld/>o</ref>
      (<trd lng="la">Cimex lectularius</trd>),
      kiu trovi&gcirc;as precipe en litoj, kaj su&ccirc;as la sangon de
      homoj:
      <ekz>
        la lit<tld/>oj &gcirc;enerale aktivas nokte, ili trovas predon, homan
        a&ubreve; bestan, pere
        de &gcirc;iaj karbongaso kaj varmo, por nutri sin ili su&ccirc;as sangon,
        &gcirc;is sepoble
        sian propran pezon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=107390">Cimo (insekto)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/9/90/Cimex_lectularius_(bed_bug)_(5975917188).jpg/320px-Cimex_lectularius_(bed_bug)_(5975917188).jpg" prm="CC BY-SA 2.0">nimfo de lit<tld/>o sur vesperto (Cimex lectularius)
        <fnt>
          <aut>Gilles San Martin el Namur, Belgujo</aut>
          <lok><url ref="&WCW;/File:Cimex_lectularius_(bed_bug)_(5975917188).jpg">&WCW;/File:Cimex_lectularius_(bed_bug)_(5975917188).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_o;&c_p;</trd>
  <trd lng="ca">xinxa dom&egrave;stica</trd>
  <trd lng="de">Bettwanze</trd>
  <trd lng="en">bedbug</trd>
  <trd lng="es">chinche de las camas</trd>
  <trd lng="fr">punaise des lits</trd>
  <trdgrp lng="hu">
    <trd>h&aacute;zi poloska</trd>,
    <trd>&aacute;gyi poloska</trd>
  </trdgrp>
  <trd lng="it">cimice<klr> dei letti</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x30C8;&#x30B3;&#x30B8;&#x30E9;&#x30DF;</trd>,
    <trd>&#x30CA;&#x30F3;&#x30AD;&#x30F3;&#x30E0;&#x30B7;</trd>
  </trdgrp>
  <trd lng="pl">pluskwa domowa</trd>
  <trd lng="pt">perecevejo-de-cama</trd>
  <trd lng="ru">&c_k;&c_l;&c_o;&c_p;</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43B;&#x43E;&#x43F; &#x43F;&#x43E;&#x441;&#x442;&#x456;&#x43B;&#x44C;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x431;&#x43B;&#x43E;&#x449;&#x438;&#x446;&#x44F; &#x43B;&#x456;&#x436;&#x43A;&#x43E;&#x432;&#x430;</trd>
  </trdgrp>
</drv>
</art>
