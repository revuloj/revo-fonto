<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>cim</rad>/o
</kap>
<drv mrk="cim.0o">
<kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="cim.0o.hemiptero">
    <dif>
      Iu el <ref tip="malprt" cel="hemipt.0oj">hemipteroj</ref>.
    </dif>
    <trd lng="be">&c_k;&c_l;&c_o;&c_p;</trd>
    <trd lng="ca">hem&iacute;pter</trd>
    <trd lng="de">Wanze</trd>
    <trd lng="en">hemipteran</trd>
    <trd lng="hu">poloska</trd>
    <trd lng="it">cimice</trd>
    <trd lng="pl">pluskwa</trd>
    <trd lng="ru">&c_k;&c_l;&c_o;&c_p;</trd>
  </snc>
  <snc mrk="cim.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="cimed.0oj"><tld/>edoj</ref>
      (<trd lng="la">Cimex</trd>).
    </dif>
    <trd lng="be">&c_k;&c_l;&c_o;&c_p;</trd>
    <trd lng="ca">xinxa</trd>
    <trd lng="de">Plattwanze</trd>
    <trdgrp lng="en">
      <trd>bedbug</trd>
    </trdgrp>
    <trd lng="fr">punaise <klr tip="ind">(insecte)</klr></trd>
    <trd lng="hu">poloska</trd>
    <trd lng="it">cimici<klr> (ematofaghe)</klr></trd>
    <trd lng="pl">pluskwa</trd>
    <trd lng="pt">percevejo</trd>
    <trd lng="ru">&c_k;&c_l;&c_o;&c_p;</trd>
  </snc>
  <snc mrk="cim.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      &Jcirc;argona vorto por miso, eraro, programa fu&scirc;o
      (neevitebla tradukprunto el la angla).
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_m;&c_y;&c_l;&c_k;&c_a; <klr>(&c_u;
      &c_p;&c_r;&c_a;&c_g;&c_r;&c_a;&c_m;&c_je;)</klr></trd>,
      <trd>&c_b;&c_l;&c_y;&c_h;&c_a;</trd>,
      <trd>&c_b;&c_a;&c_g;</trd>,
      <trd>&c_z;&c_b;&c_o;&c_j;</trd>,
      <trd>&c_g;&c_l;&c_ju;&c_k;</trd>
    </trdgrp>
    <trd lng="ca">error <klr>(de programari)</klr></trd>
    <trdgrp lng="de">
      <trd>Bug</trd>,
      <trd>Fehler</trd>
    </trdgrp>
    <trd lng="en">bug</trd>
    <trd lng="fr">bogue</trd>
    <trdgrp lng="hu">
      <trd>programhiba</trd>,
      <trd>bug</trd>
    </trdgrp>
    <trd lng="it">baco<klr> (inf.)</klr></trd>
    <trdgrp lng="pl">
      <trd>bug</trd>,
      <trd>pluskwa</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_sh;&c_i;&c_b;&c_k;&c_a;</trd>,
      <trd>&c_s;&c_b;&c_o;&c_j;</trd>,
      <trd>&c_zh;&c_u;&c_k;</trd>,
      <trd>&c_g;&c_l;&c_ju;&c_k;</trd>
    </trdgrp>
  </snc>
  <trd lng="sk">plo≈°tica</trd>
</drv>

<drv mrk="cim.lit0o">
  <kap>lit<tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Specio de <ref tip="super" cel="cim.0o.genro"><tld/>o</ref>
      (<trd lng="la">Cimex lectularius</trd>),
      kiu trovi&gcirc;as precipe en litoj, kaj su&ccirc;as la sangon de
      homoj.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_o;&c_p;</trd>
  <trd lng="ca">xinxa dom&egrave;stica</trd>
  <trd lng="de">Bettwanze</trd>
  <trd lng="en">bedbug</trd>
  <trd lng="fr">punaise des lits</trd>
  <trdgrp lng="hu">
    <trd>h&aacute;zi poloska</trd>,
    <trd>&aacute;gyi poloska</trd>
  </trdgrp>
  <trd lng="it">cimice<klr> dei letti</klr></trd>
  <trd lng="pl">pluskwa domowa</trd>
  <trd lng="pt">perecevejo-de-cama</trd>
  <trd lng="ru">&c_k;&c_l;&c_o;&c_p;</trd>
</drv>
</art>
