<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rival.xml,v 1.20 2022/03/26 19:19:20 revo Exp $">
<kap>
  <rad>rival</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="rival.0o">
  <kap><tld/>o</kap>
  <snc mrk="rival.0o.KOMUNE">
    <ref tip="dif" cel="konkur">konkuranto</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_p;&c_je;&c_r;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_k;&c_u;&c_r;&c_e;&c_n;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_s;&c_malmol;&c_p;&c_je;&c_r;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">rival</trd>
  <trdgrp lng="cs">
    <trd>rival</trd>,
    <trd>sok</trd>,
    <trd>soupe&rcaron;</trd>
  </trdgrp>
  <trd lng="de">Rivale</trd>
  <trd lng="en">rival</trd>
  <trdgrp lng="fr">
  <trd>rival</trd>,
  <trd>concurrent</trd>
</trdgrp>
  <trdgrp lng="hu">
    <trd>vet&eacute;lyt&aacute;rs</trd>,
    <trd>riv&aacute;lis</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>mededinger</trd>,
    <trd>concurrent</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rywal</trd>,
    <trd>przeciwnik</trd>,
    <trd>wsp&oacute;&lstroke;zawodnik</trd>,
    <trd>konkurent</trd>
  </trdgrp>
  <trd lng="pt">rival</trd>
  <trd lng="ru">&c_s;&c_o;&c_p;&c_je;&c_r;&c_n;&c_i;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>rival</trd>,
    <trd>sok</trd>,
    <trd>s&uacute;per</trd>
  </trdgrp>
  <trd lng="sv">rival</trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x443;&#x43F;&#x435;&#x440;&#x43D;&#x438;&#x43A;</trd>,
    <trd>&#x43A;&#x43E;&#x43D;&#x43A;&#x443;&#x440;&#x435;&#x43D;&#x442;</trd>
  </trdgrp>
</drv>

<drv mrk="rival.0i">
  <kap><tld/>i</kap>
  <snc mrk="rival.0i.KOMUNE">
    <dif>
      <ref tip="vid" cel="konkur.0i.KOMUNE">Konkuri</ref>
      kontra&ubreve; <tld/>oj:
      <ekz>
        li saltis tre longe <klr>...</klr> e&ccirc; la &ccirc;ampiono 
        de Olimpikoj ne povis <tld/>i kontra&ubreve; li
        <fnt>
          <aut>red. Vejdo</aut>,
          <vrk><url ref="http://cindymckee.com/librejo/440FablojdeEzopo.pdf">
          440 Fabloj de Ezopo</url></vrk>,
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">soupe&rcaron;it <klr>(s n&ecaron;k&yacute;m)</klr></trd>
  <trdgrp lng="fr">
    <trd>concourir</trd>,
    <trd>rivaliser</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rywalizowa&cacute;</trd>,
    <trd>wsp&oacute;&lstroke;zawodniczy&cacute;</trd>,
    <trd>konkurowa&cacute;</trd>
  </trdgrp>
  <trd lng="sk">s&uacute;peri&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x431;&#x443;&#x442;&#x438; &#x441;&#x443;&#x43F;&#x435;&#x440;&#x43D;&#x438;&#x43A;&#x43E;&#x43C;</trd>,
    <trd>&#x441;&#x443;&#x43F;&#x435;&#x440;&#x43D;&#x438;&#x447;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x437;&#x43C;&#x430;&#x433;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
