<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>bast</rad>/o
</kap>
<drv mrk="bast.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <fnt><bib>KrED</bib></fnt>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Plej interna parto de la arbo&scirc;elo.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_u;&c_b;</trd>
  <trd lng="de">Bast</trd>
  <trd lng="en">heartwood</trd>
  <trdgrp lng="fr">
    <trd>phlo&egrave;me</trd>,
    <trd>liber</trd>,
    <trd>int&eacute;rieur de l'<ind>&eacute;corce</ind></trd>
  </trdgrp>
  <trd lng="hu">h&aacute;ncs</trd>
  <trd lng="ru">&c_l;&c_u;&c_b;</trd>
  <trd lng="sv">bast</trd>
</drv>

<drv mrk="bast.0fibro">
  <kap><tld/>fibro</kap>
  <fnt><bib>KrED</bib></fnt>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Fibro konsistanta el basto.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_u;&c_b;&c_ja;&c_n;&c_o;&c_je; &c_v;&c_a;&c_l;&c_a;&c_k;&c_n;&c_o;</trd>
  <trd lng="de">Bastfaser</trd>
  <trd lng="ru">&c_l;&c_u;&c_b;&c_ja;&c_n;&c_o;&c_je; &c_v;&c_o;&c_l;&c_o;&c_k;&c_n;&c_o;</trd>
</drv>

<drv mrk="bast.0korbo">
  <kap><tld/>korbo</kap>
  <fnt><bib>KrED</bib></fnt>
  <snc>
    <dif>
      Korbo farita el bastfibroj.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_u;&c_b;&c_ja;&c_n;&c_y; &c_k;&c_o;&c_r;&c_a;&c_b;</trd>
  <trd lng="de">Bastkorb</trd>
  <trd lng="ru">&c_l;&c_u;&c_b;&c_ja;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_r;&c_z;&c_i;&c_n;&c_a;</trd>
</drv>

<drv mrk="bast.0mato">
  <kap><tld/>mato</kap>
  <fnt><bib>KrED</bib></fnt>
  <snc>
    <dif>
      Mato farita el bastfibroj.
    </dif>
  </snc>
  <trd lng="de">Bastmatte</trd>
  <trd lng="ru">&c_c;&c_i;&c_n;&c_o;&c_v;&c_k;&c_a;</trd>
</drv>
</art>
