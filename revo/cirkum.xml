<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>9</ofc>
  <rad>cirkumcid</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="cirkum.0i">
  <kap><ofc>9</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">REL</uzo>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Fortran&ccirc;i <ref tip="vid" cel="penis.0o">penisan</ref> <ref
      tip="vid" cel="prepuc.0o.penisa">prepucion</ref>:
      <ekz>
        <tld/>i infaneton
        <fnt><bib>9OA</bib></fnt>;
      </ekz>
      <ekz>
        kompreneble vin <tld/>is la rabeno Kaheno
        <fnt>
          <aut>L. Beaucaire</aut>
          <vrk>Kruko kaj Baniko en Bervalo</vrk>
          <lok>Bonaj kalkuloj, bonaj amikoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Fortran&ccirc;i <ref tip="vid" cel="klitor.0o">klitoron</ref>, klitoran
      <ref tip="vid" cel="prepuc.0o.klitora">prepucion</ref> a&ubreve; la <ref
      tip="vid" cel="vulv.0o">vulvajn</ref> lipojn:
      <ekz>
        ne gravas scii, &ccirc;u oni povas <tld/>i virinon a&ubreve; ne, gravas la
        pacema batalo de viktimo kontra&ubreve; tiu grava mutilo 
        <fnt>
          <bib>Monato</bib>
          <aut>Bert BOON</aut>
          <vrk><url ref="http://www.monato.net/2006/008922.php">Bato sub
          la zono</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="mutil.0i">mutili</ref>
  </refgrp>
  <trd lng="ar">&a_kh;&a_t;&a_n;</trd>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_a;&c_b;&c_r;&c_a;&c_z;&c_a;&c_n;&c_mol;&c_n;&c_je;</ind></trd>,
    <trd>&c_a;&c_b;&c_r;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_r;&c_e;&c_z;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_o;&c_b;&c_r;&c_ja;&c_z;&c_v;&c_a;&c_m;</trd>
  <trd lng="ca">circumcidar</trd>
  <trd lng="cs">ob&rcaron;ezat</trd>
  <trd lng="da">omsk&aelig;re</trd>
  <trd lng="de">beschneiden</trd>
  <trd lng="el">&pi;&epsilon;&rho;&jota;&tau;&epsilon_ton;&my;&ny;&omega;</trd>
  <trd lng="en">circumcise</trd>
  <trd lng="es">circuncidar</trd>
  <trd lng="fa">&a_kh;&a_t;&a_n;&a_h; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fi">ymp&auml;rileikata</trd>
  <trd lng="fo">umskera</trd>
  <trd lng="fr">circoncire</trd>
  <trd lng="gl">circuncidar</trd>
  <trd lng="hi">&#2326;&#2364;&#2340;&#2344;&#2366; &#2325;&#2352;&#2344;&#2366;</trd>
  <trd lng="hu">k&ouml;r&uuml;lmet&eacute;l</trd>
  <trd lng="hy">&#1385;&#1388;&#1411;&#1377;&#1407;&#1381;&#1388;</trd>
  <trd lng="io">cirkoncizar</trd>
  <trd lng="is">umskera</trd>
  <trd lng="it">circoncidere</trd>
  <trd lng="ka">&#4332;&#4312;&#4316;&#4307;&#4304;&#4330;&#4309;&#4308;&#4311;&#4304;</trd>
  <trd lng="lt">apipjaustyti</trd>
  <trd lng="ms">mengkhatankan</trd>
  <trd lng="nl">besnijden</trd>
  <trd lng="oc">circomcir</trd>
  <trd lng="pl">obrzeza&cacute;</trd>
  <trd lng="pt">circuncidar</trd>
  <trd lng="ro">circumcide</trd>
  <trd lng="ru">&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; &c_o;&c_b;&c_r;&c_je;&c_z;&c_a;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">obreza&tcaron;</trd>
  <trd lng="sv">omsk&auml;ra</trd>
  <trd lng="tr">s&uuml;nnet etmek</trd>
  <trd lng="yi">&yod;&yod;&#1460;&dalet;&yod;&shin;&fnun;</trd>
</drv>

<drv mrk="cirkum.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">REL</uzo>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        kial halti &ccirc;e <tld/>o?
        <fnt>
          <aut>A. L&ouml;wenstein</aut>,
          <vrk>La &scirc;tona urbo</vrk>,
          <lok>1999</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ar">&a_kh;&a_t;&a_A;&a_n;</trd>
  <trd lng="ca">circumcisi&oacute;</trd>
  <trd lng="de">Beschneidung</trd>
  <trd lng="el">&pi;&epsilon;&rho;&jota;&tau;&omikron;&my;&eta_ton;</trd>
  <trd lng="en">circumcision</trd>
  <trd lng="es">circuncisi&oacute;n</trd>
  <trd lng="fa">&a_kh;&a_t;&a_n;&a_h;</trd>
  <trd lng="fr">circoncision</trd>
  <trd lng="fy">besnijenis</trd>
  <trd lng="he">&mem;&yod;&lamed;&he;</trd>
  <trd lng="hu">k&ouml;r&uuml;lmet&eacute;l&eacute;s</trd>
  <trd lng="io">cirkoncizo</trd>
  <trd lng="is">umskur&#240;ur</trd>
  <trd lng="it">circoncisione</trd>
  <trd lng="lt">apipjaustymas</trd>
  <trd lng="nl">besnijdenis</trd>
  <trd lng="pl">obrzezanie</trd>
  <trd lng="pt">circuncis&atilde;o</trd>
  <trd lng="ru">&c_o;&c_b;&c_r;&c_je;&c_z;&c_a;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">obriezka</trd>
  <trd lng="tr">s&uuml;nnet</trd>
</drv>
</art>
