<art mrk="$Id: mole.xml,v 1.21 2025/07/05 18:38:30 revo Exp $">
<kap>
   <rad>mole</rad>/o
</kap>
<drv mrk="mole.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">MAR</uzo>
   <snc>
     <dif>
       Ligna a&ubreve; &scirc;tona digo konstruita anta&ubreve; la eniro de <ref tip="malprt"
       cel="haven.0o.MAR">haveno</ref> por &scirc;irmi &gcirc;in kontra&ubreve; la ondoj
       kaj
       sendan&gcirc;erigi la &scirc;ipojn:
       <ekz>
         sur la <tld/>o staris viro, li fumis kaj kra&ccirc;adis en la maron
         <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 7a, p. 71a</lok></fnt>.
       </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>&c_m;&c_o;&c_l;</trd>,
       <trd>&c_d;&c_a;&c_m;&c_b;&c_a;</trd>
     </trdgrp>
     <trdgrp lng="en">
       <trd>breakwater</trd>,
       <trd>mole</trd>
     </trdgrp>
     <trdgrp lng="es">
       <trd>dique</trd>,
       <trd>muelle <klr tip="amb">(marino)</klr></trd>
       <trd>rompeolas</trd>
     </trdgrp>
   </snc>
  <trdgrp lng="cs">
    <trd>molo</trd>,
    <trd>p&rcaron;&iacute;stavn&iacute; hr&aacute;z</trd>
  </trdgrp>
  <trd lng="fr">m&ocirc;le</trd>
  <trdgrp lng="hu">
     <trd>m&oacute;l&oacute;</trd>,
     <trd>kik&ouml;t&odblac;g&aacute;t</trd>
   </trdgrp>
  <trdgrp lng="ja">
    <trd>&#38450;&#x6CE2;&#22564;  <pr>&#x307C;&#12358;&#x306F;&#12390;&#12356;</pr></trd>,
    <trd>&#x7A81;&#22564;  <pr>&#12392;&#12387;&#12390;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>havendam</trd>,
    <trd>pier<klr tip="ind"> (havendam)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>falochron</trd>,
    <trd>molo</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>molhe</trd>,
    <trd>mole <klr>(molhe)</klr></trd>,
    <trd>quebra-mar</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>mol <klr>(construc&#x21B;ie)</klr></trd>,
    <trd>dig</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_l;</trd>,
    <trd>&c_d;&c_a;&c_m;&c_b;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>m&oacute;lo</trd>,
    <trd>pr&iacute;stavn&aacute; hr&aacute;dza</trd>,
    <trd>vlnolam</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x43E;&#x43B;</trd>,
    <trd>&c_d;&c_a;&#x43C;&c_b;&c_a;</trd>,
    <trd>&#x43C;&#x43E;&#x43B;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x77F6; <pr>j&#x12B;</pr></trd>,
    <trd>&#x78EF; <pr>j&#x12B;</pr></trd>,
    <trd>&#38450;&#x6CE2;&#22564; <pr>f&#xE1;ngb&#x14D;d&#x12B;</pr></trd>
  </trdgrp>
</drv>
</art>
