<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mole</rad>/o
</kap>
<drv mrk="mole.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Ligna a&ubreve; &scirc;tona digo konstruita anta&ubreve; la eniro de la haveno por &scirc;irmi
      &gcirc;in kontra&ubreve; la ondoj kaj sendan&gcirc;erigi la &scirc;ipojn.
    </dif>
    <trdgrp lng="en">
      <trd>breakwater</trd>,
      <trd>mole</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="nl">
    <trd>havendam</trd>,
    <trd>pier<klr tip="ind"> (havendam)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_l;</trd>,
    <trd>&c_d;&c_a;&c_m;&c_b;&c_a;</trd>
  </trdgrp>
</drv>
</art>
