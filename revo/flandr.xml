<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>flandr</rad>/o
</kap>
<drv mrk="flandr.0o">
  <kap><tld/>o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Homo kun <tld/>a nacieco.
    </dif>
  </snc>
  <trd lng="de">Flame</trd>
  <trd lng="en">Flemish</trd>
  <trd lng="fr">Flamand</trd>
  <trd lng="nl">Vlaming</trd>
</drv>
<drv mrk="flandr.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj a&ubreve; ilia nacieco.
    </dif>
  </snc>
  <trd lng="de">fl&auml;misch</trd>
  <trd lng="en">Flemish</trd>
  <trd lng="fr">flamand</trd>
  <trd lng="nl">Vlaams</trd>
</drv>
<drv mrk="flandr.F0ujo">
  <kap><tld lit="F"/>ujo</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="flandr.F0ujo.reg">
    <dif>
      La norda nederlandlingva regiono de <ref tip="malprt" cel="belg.B0ujo">Belgujo</ref>,
      konsistanta el la provincoj Okcidenta <tld lit="F"/>ujo, Orienta <tld lit="F"/>ujo,
      Antverpeno, Limburgo kaj <tld lit="F"/>a Brabanto: <ekz>la a&ubreve;toro fami&gcirc;is en <tld lit="F"/>ujo
      &ccirc;efe pro granda nombro da stre&ccirc;aj aventurlibroj por la junularo, sed anka&ubreve; pro siaj
      erotikaj rakontoj <fnt><aut>Rejna DE JONG</aut>:
      <vrk><url ref="&Monato;/rec/bpet.html">Ripoziga libro por hasta mondo</url></vrk>,
      <bib>Monato</bib>, <lok>n-ro 04/1997</lok>
      </fnt>.</ekz>
    </dif>
  </snc>
  <snc mrk="flandr.F0ujo.prov">
    <dif>
      Nomo de du <ref tip="super" cel="provin.0o">provincoj</ref> en <ref 
      tip="malprt" cel="belg.B0ujo">Belgujo</ref> (Okcidenta kaj Orienta <tld lit="F"/>ujo),
      unu en <ref tip="malprt" cel="nederl.0o">Nederlando</ref> (Zelanda  <tld lit="F"/>ujo)
      kaj de unu regiono de <ref tip="malprt" cel="franc.F0ujo">Francujo</ref>.
    </dif>
  </snc>
  <snc mrk="flandr.F0ujo.geo">
    <dif>
      Parto de <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref>, la&ubreve;longe de la Norda
      Maro, inter la artezaj montetoj en Francujo kaj la skelda estuaro en Nederlando.
    </dif>
  </snc>
  <trd lng="de">Flandern</trd> 
  <trd lng="en">Flanders</trd> 
  <trd lng="es">Flandes</trd> 
  <trd lng="fr">Flandre</trd>
  <trd lng="nl">Vlaanderen</trd>
  <trd lng="pl">Flandria</trd>
</drv>
</art>


<!--
$Log$
-->
</vortaro>