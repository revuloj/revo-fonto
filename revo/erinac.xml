<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>erinac</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="erinac.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="erinac.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Malgranda mambesto, kies ha&ubreve;to estas kovrita de pikiloj, 
      genro el la familio <ref tip="super"
      cel="erinac1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Erinaceus</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_o;&c_zh;&c_y;&c_k;</trd>
  <trd lng="bg">&c_t;&c_a;&c_r;&c_a;&c_l;&c_je;&c_zh;</trd>
  <trd lng="de">Igel</trd>
  <trd lng="en">hedgehog</trd>
  <trd lng="fr">h&eacute;risson</trd>
  <trd lng="hu">s&uuml;ndiszn&oacute;</trd>
  <trdgrp lng="it">
    <trd>porcospino</trd>,
    <trd>riccio</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>ouri&ccedil;o</trd>,
    <trd>ouri&ccedil;o-cacheiro</trd>
  </trdgrp>
  <trd lng="ru">&c_jo;&c_zh;</trd>
</drv>

<drv mrk="erinac.mar0o">
  <kap>mar<tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="erinac.mar0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="kasxta.mar0o.ZOO">marka&scirc;tano</ref>
    <ekz>
      el la bordoj ni kaptis <klr>...</klr> mar<tld/>ojn, ostrojn kaj 
      &ccirc;iuspecajn konkojn
      <fnt>
        <aut>H. Bernardin de Saint-Pierre, trad. H. Hodler</aut>,
        <vrk><url ref="http://www.luin.se/inko/180-1.pdf">
        Pa&ubreve;lo kaj Virginio</url></vrk>,
        <lok>1905</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="ca">eri&ccedil;&oacute; de mar</trd>
  <trd lng="bg">&c_m;&c_o;&c_r;&c_s;&c_k;&c_i; &c_t;&c_a;&c_r;&c_a;&c_l;&c_je;&c_zh;</trd>
  <trd lng="de">Seeigel</trd>
  <trd lng="en">sea urchin</trd>
  <trd lng="es">erizo de mar</trd>
  <trd lng="fr">oursin</trd>
  <trd lng="hu">tengeri s&uuml;n</trd>
  <trd lng="it"><ind>riccio</ind> di mare</trd>
  <trd lng="pt">equino</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_s;&c_k;&c_o;&c_j; &c_k;&c_a;&c_sh;&c_t;&c_a;&c_n;</trd>
</drv>
</art>
