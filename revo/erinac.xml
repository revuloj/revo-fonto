<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>erinac</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="erinac.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="erinac.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Malgranda mambesto, kies ha&ubreve;to estas kovrita de 
      <ref tip="prt" cel="pik.0ilo.ZOO">pikiloj</ref>, 
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el la familio 
      <ref tip="malprt" cel="erinac1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Erinaceus</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_o;&c_zh;&c_y;&c_k;</trd>
  <trd lng="bg">&c_t;&c_a;&c_r;&c_a;&c_l;&c_je;&c_zh;</trd>
  <trd lng="de">Igel</trd>
  <trd lng="en">hedgehog</trd>
  <trdgrp lng="fa">
    <trd>&a_kh;&a_A;&a_r;&f_p;&a_sh;&a_t;</trd>,
    <trd>&a_j;&a_w;&a_j;&a_h;&zwnj;&a_t;&f_y;&a_gh;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">h&eacute;risson</trd>
  <trd lng="hu">s&uuml;ndiszn&oacute;</trd>
  <trdgrp lng="it">
    <trd>porcospino</trd>,
    <trd>riccio</trd>
  </trdgrp>
  <trd lng="pl">je&zdot;</trd>
  <trdgrp lng="pt">
    <trd>ouri&ccedil;o</trd>,
    <trd>ouri&ccedil;o-cacheiro</trd>
  </trdgrp>
  <trd lng="ru">&c_jo;&c_zh;</trd>
</drv>

<drv mrk="erinac.mar0o">
  <kap>mar<tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="erinac.mar0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="kasxta.mar0o.ZOO">marka&scirc;tano</ref>
    <ekz>
      el la bordoj ni kaptis <klr>...</klr> mar<tld/>ojn, ostrojn kaj 
      &ccirc;iuspecajn konkojn
      <fnt>
        <aut>H. Bernardin de Saint-Pierre, trad. H. Hodler</aut>,
        <vrk><url ref="http://www.luin.se/inko/180-1.pdf">
        Pa&ubreve;lo kaj Virginio</url></vrk>,
        <lok>1905</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="ca">eri&ccedil;&oacute; de mar</trd>
  <trd lng="bg">&c_m;&c_o;&c_r;&c_s;&c_k;&c_i; &c_t;&c_a;&c_r;&c_a;&c_l;&c_je;&c_zh;</trd>
  <trd lng="de">Seeigel</trd>
  <trd lng="en">sea urchin</trd>
  <trd lng="es">erizo de mar</trd>
  <trd lng="fa">&a_t;&a_w;&a_t;&f_y;&a_A;&f_y; &a_d;&a_r;&f_y;&a_A;&f_y;&f_y;</trd>
  <trd lng="fr">oursin</trd>
  <trd lng="hu">tengeri s&uuml;n</trd>
  <trd lng="it"><ind>riccio</ind> di mare</trd>
  <trd lng="pl">je&zdot;owiec</trd>
  <trd lng="pt">equino</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_s;&c_k;&c_o;&c_j; &c_k;&c_a;&c_sh;&c_t;&c_a;&c_n;</trd>
</drv>

<drv mrk="erinac.posx0o">
  <kap>po&scirc;<tld/>o</kap>
  <snc mrk="erinac.posx0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Malgranda mamulo kovrita de pikiloj,
      vivanta en A&ubreve;tralio kaj Novgvineo,
      perlange kaptanta termitojn,
      dementanta <ref cel="ov.0o">ovon</ref> kaj poste nutranta
      la idon en ventra <ref cel="posx.0o.saketo">po&scirc;o</ref>;
      familio ta&hcirc;iglosedoj
      (<trd lng="la">Tachyglossidae</trd>);
      <ref tip="sin" cel="ehxidn.0o.ZOO">e&hcirc;idno</ref>:
      <ekz>
        po&scirc;-<tld/>oj estas la solaj ankora&ubreve; vivantaj
        monotremoj krom la ornitorinko
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Ekidno">
          ekidno</url>, 2012-12-06</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">&eacute;chidn&eacute;</trd>
  <trd lng="pl">kolczatka</trd>
</drv>
</art>
