<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: erinac.xml,v 1.38 2023/11/07 20:34:11 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>erinac</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="erinac.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="erinac.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="mangx.insekto0ulo" lst="voko:insektoman&gcirc;uloj">Insektoman&gcirc;ulo</ref>, kies
      ha&ubreve;to estas
      kovrita de
      <ref tip="prt" cel="pik.0ilo.ZOO">pikiloj</ref>;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el la familio
      <ref tip="malprt" cel="erinac1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Erinaceus</trd>):
      <ekz>
        al <tld/>oj mankas ta&ubreve;gaj lokoj por dormi dum la vintro kaj nesti printempe
        <fnt>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2010/009971.php">Nestokestoj</url></vrk>
          <bib>Monato</bib>
          <lok>2010/03, p. 14</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia densa hararo similis al <tld/>aj pikiloj
        <fnt>
          <aut>Julian Modest</aut>
          <vrk><url ref="&Monato;/2008/009347.php">Nina kaj la knabo</url></vrk>
          <bib>Monato</bib>
          <lok>2008/05, p. 25</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/c/cb/Erinaceus_europaeus_(Linnaeus%2C_1758).jpg/620px-Erinaceus_europaeus_(Linnaeus%2C_1758).jpg" prm="CC BY-SA 3.0">E&ubreve;ropa <tld/>o (Erinaceus europaeus)
        <fnt>
          <aut>Michael G&auml;bler, propra verko</aut>
          <lok><url ref="&WCW;/File:Erinaceus_europaeus_(Linnaeus,_1758).jpg">&WCW;/File:Erinaceus_europaeus_(Linnaeus,_1758).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_v;&c_o;&c_zh;&c_y;&c_k;</trd>
  <trd lng="bg">&c_t;&c_a;&c_r;&c_a;&c_l;&c_je;&c_zh;</trd>
  <trd lng="cs">je&zcaron;ek</trd>
  <trd lng="de">Igel</trd>
  <trd lng="en">hedgehog</trd>
  <trdgrp lng="fa">
    <trd>&a_kh;&a_A;&a_r;&f_p;&a_sh;&a_t;</trd>,
    <trd>&a_j;&a_w;&a_j;&a_h;&#x200C;&a_t;&f_y;&a_gh;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">h&eacute;risson</trd>
  <trd lng="hu">s&uuml;ndiszn&oacute;</trd>
  <trd lng="id"><ind>landak</ind> susu</trd>
  <trdgrp lng="it">
    <trd>porcospino</trd>,
    <trd>riccio</trd>
  </trdgrp>
  <trd lng="ja">&#x30CF;&#x30EA;&#x30CD;&#x30BA;&#x30DF;</trd>
  <trd lng="nl">egel</trd>
  <trd lng="pl">je&zdot;</trd>
  <trdgrp lng="pt">
    <trd>ouri&ccedil;o</trd>,
    <trd>ouri&ccedil;o-cacheiro</trd>
  </trdgrp>
  <trd lng="ru">&c_jo;&c_zh;</trd>
  <trd lng="sk">je&zcaron;</trd>
  <trd lng="uk">&c_ji;&c_zh;&c_a;&#x43A;</trd>
  <trd lng="zh">&#x523A;&#x732C; <pr>c&#xEC;w&#xE8;i</pr></trd>
</drv>

<drv mrk="erinac.mar0o">
  <kap>mar<tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="erinac.mar0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="kasxta.mar0o.ZOO">Marka&scirc;tano</ref>:
      <ekz>
        el la bordoj ni kaptis <klr>[&#x2026;]</klr> mar<tld/>ojn, ostrojn kaj
        &ccirc;iuspecajn konkojn
        <fnt>
          <aut>H. Bernardin de Saint-Pierre, trad. H. Hodler</aut>,
          <vrk><url ref="http://www.luin.se/inko/180-1.pdf">
          Pa&ubreve;lo kaj Virginio</url></vrk>,
          <lok>1905</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_m;&c_o;&c_r;&c_s;&c_k;&c_i; &c_t;&c_a;&c_r;&c_a;&c_l;&c_je;&c_zh;</trd>
  <trd lng="ca">eri&ccedil;&oacute; de mar</trd>
  <trd lng="cs">je&zcaron;ovka mo&rcaron;sk&aacute;</trd>
  <trd lng="de">Seeigel</trd>
  <trd lng="en">sea urchin</trd>
  <trd lng="es">erizo de mar</trd>
  <trd lng="fa">&a_t;&a_w;&a_t;&f_y;&a_A;&f_y; &a_d;&a_r;&f_y;&a_A;&f_y;&f_y;</trd>
  <trd lng="fr">oursin</trd>
  <trd lng="he">&#x5E7;&#x5D9;&#x5E4;&#x5D5;&#x5D3; &#x5D9;&#x5DD;</trd>
  <trd lng="hu">tengeri s&uuml;n</trd>
  <trdgrp lng="id">
    <trd><ind>landak</ind> laut</trd>,
    <trd><ind>bulu</ind> babi</trd>
  </trdgrp>
  <trd lng="it"><ind>riccio</ind> di mare</trd>
  <trd lng="ja">&#x30A6;&#x30CB;</trd>
  <trd lng="nl">zee-egel</trd>
  <trd lng="pl">je&zdot;owiec</trd>
  <trd lng="pt">equino</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_s;&c_k;&c_o;&c_j; &c_k;&c_a;&c_sh;&c_t;&c_a;&c_n;</trd>
  <trd lng="sk">je&zcaron;ovka morsk&aacute;</trd>
  <trdgrp lng="zh">
    <trd>&#x6D77;&#x80C6;&#x7EB2; <pr>h&#x1CE;id&#x1CE;ng&#x101;ng</pr></trd>,
    <trd>&#x6D77;&#x80C6; <pr>h&#x1CE;id&#x1CE;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="erinac.posx0o">
  <kap>po&scirc;<tld/>o</kap>
  <snc mrk="erinac.posx0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Malgranda
      <ref tip="lst" cel="mam.pra0ulo" lst="voko:pramamuloj">pramamulo</ref>
      el familio ta&hcirc;iglosedoj
      (<trd lng="la">Tachyglossidae</trd>),
      kovrita de pikiloj, vivanta en A&ubreve;stralio kaj Novgvineo,
      perlange kaptanta termitojn,
      dementanta <ref cel="ov.0o">ovon</ref> kaj poste nutranta
      la idon en ventra <ref cel="posx.0o.saketo">po&scirc;o</ref>:
      <ekz>
        po&scirc;-<tld/>oj estas la solaj ankora&ubreve; vivantaj
        monotremoj krom la ornitorinko
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Ekidno">
          ekidno</url>, 2012-12-06</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="ehxidn.0o.ZOO">e&hcirc;idno</ref>
    <bld lok="&WCU;/thumb/c/c9/Wild_shortbeak_echidna.jpg/640px-Wild_shortbeak_echidna.jpg" prm="GFDL 1.2">Mallongbeka po&scirc;<tld/>o
        <fnt>
          <lok><url ref="&WCW;/File:Wild_shortbeak_echidna.jpg">&WCW;/File:Wild_shortbeak_echidna.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_ja;&c_h;&c_ib;&c_d;&c_n;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Ameisenigel</trd>,
    <trd>Schnabeligel</trd>
  </trdgrp>
  <trd lng="fr">&eacute;chidn&eacute;</trd>
  <trd lng="id">ekidna</trd>
  <trd lng="nl">mierenegel</trd>
  <trd lng="pl">kolczatka</trd>
</drv>
</art>
