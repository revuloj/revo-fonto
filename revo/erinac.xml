<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>erinac</rad>/o
</kap>
<drv mrk="erinac.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="erinac.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Erinaceus</trd>), kies ha&ubreve;to estas
      kovrita de pikiloj.
    </dif>
  </snc>
  <trd lng="de">Igel</trd>
  <trd lng="en">hedgehog</trd>
  <trd lng="fr">h&eacute;risson</trd>
</drv>
<drv mrk="erinac.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo 
      <ref tip="super" cel="mangx.insekto0uloj">insektoman&gcirc;uloj</ref>
      (<trd lng="la">Erinaceidae</trd>), kun mallongaj kruroj kaj longa
      bu&scirc;o.
    </dif>
  </snc>
  <trd lng="de">Igel <klr tip="ind">(Familie)</klr></trd>
  <trd lng="en">hedgehogs <klr tip="ind">(family)</klr></trd>
</drv>
</art>
