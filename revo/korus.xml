<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: korus.xml,v 1.27 2023/12/03 09:58:59 revo Exp $">
<kap>
  <rad>korus</rad>/o
  <fnt>V</fnt>
</kap>
<drv mrk="korus.0o">
  <kap><tld/>o</kap>
  <snc mrk="korus.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="dif" cel="hxor.0o.kantistaro">&Hcirc;oro</ref>:
      <ekz>
        ekleziaj <tld/>oj
        <fnt>
          <aut>Evgeni Georgiev</aut>
            <vrk><url ref="&Monato;/2000/007012.php">Jarmila festego ortodoksa</url></vrk>
            <bib>Monato</bib>
            <lok>2000/08, p. 22</lok>
          </fnt>;
      </ekz>
      <ekz>
        trivo&ccirc;a infan<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2011/010210.php">Junaj arbetoj post 70 jaroj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        montgrimpista <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2013/010944.php">Kanto de la montoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        milvo&ccirc;a <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Francisko Lorrain</aut>
          <vrk><url ref="&Monato;/2015/011537.php">Fa&ubreve;sto hodia&ubreve;</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        akademiaj <tld/>oj <klr>[&#x2026;]</klr> kvankam ili kantas tradicia&jcirc;ojn, sonas tro
        profesie kaj seke
        <fnt>
          <bib>Monato</bib>
          <aut>Anna kaj Mati Pentus</aut>
          <vrk><url ref="&Monato;/2005/008811.php">Katalunio, Okcitanio Kaj Tiel Plu</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
	japana virina <tld/>o parkere kantis hungarajn
	melodiojn<fnt><aut>Juh&aacute;sz K&aacute;roly</aut>: <vrk>Korusa konkurso</vrk>,
	<bib>Monato</bib><lok>jaro 2000a, numero 12a, p. 14a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_o;&c_r;</trd>
  <trd lng="cs">ch&oacute;r</trd>
  <trd lng="de">Chor</trd>
  <trdgrp lng="en">
    <trd>choir</trd>,
    <trd>chorus</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>choeur<klr> (troupe)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;nekkar</trd>,
    <trd>k&oacute;rus</trd>,
    <trd>kar <klr tip="ind">(sz&iacute;npadi)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5408;&#x5531;  <pr>&#x304C;&#x3063;&#x3057;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x5408;&#x5531;&#x56E3;  <pr>&#x304C;&#x3063;&#x3057;&#x3087;&#x3046;&#x3060;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">koor</trd>
  <trd lng="pl">ch&oacute;r</trd>
  <trd lng="pt">coro <klr tip="amb">(&ocirc;)</klr></trd>
  <trd lng="ru">&c_h;&c_o;&c_r;</trd>
  <trd lng="sk">ch&oacute;r</trd>
  <trd lng="sv">k&ouml;r<klr tip="ind"> (mus.)</klr></trd>
  <trdgrp lng="zh">
    <trd>&#x5408;&#x5531;&#x56E2; <pr>h&#xE9;ch&#xE0;ngtu&#xE1;n</pr></trd>,
    <trd>&#x5531;&#x8A69;&#x73ED; <pr>ch&#xE0;ngsh&#x12B;b&#x101;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="korus.0ano">
  <kap><tld/>ano</kap>
  <snc mrk="korus.0ano.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="dif" cel="hxor.0ano">&Hcirc;orano</ref>:
      <ekz>
	alvenis el 13 landoj &ccirc;. 1500 <tld/>anoj
        <fnt><aut>Juh&aacute;sz K&aacute;roly</aut>:
	<vrk>Korusa konkurso</vrk>, <bib>Monato</bib><lok>jaro 2000a,
	numero 12a, p. 14a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_r;&c_y;&c_s;&c_t;</trd>
  <trd lng="de">Chors&auml;nger</trd>
  <trd lng="en">choir singer</trd>
  <trdgrp lng="fr">
    <trd>choriste</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;nekkaros</trd>,
    <trd>kar&eacute;nekes</trd>,
    <trd>&eacute;nekkari tag</trd>,
    <trd>k&oacute;rustag</trd>
  </trdgrp>
  <trd lng="ja">&#x5408;&#x5531;&#x56E3;&#x54E1;  <pr>&#x304C;&#x3063;&#x3057;&#x3087;&#x3046;&#x3060;&#x3093;&#x3044;&#x3093;</pr></trd>
  <trd lng="nl">koorzanger</trd>
  <trd lng="pl">ch&oacute;rzysta</trd>
  <trd lng="ru">&c_h;&c_o;&c_r;&c_i;&c_s;&c_t;</trd>
  <trd lng="sv">k&ouml;rs&aring;ngare</trd>
</drv>
</art>
