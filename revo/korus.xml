<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>korus</rad>/o
  <fnt>V</fnt>
</kap>
<drv mrk="korus.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <dif>
      = <ref tip="dif" cel="hxor.0o.kantistaro">&hcirc;oro</ref>:
      <ekz>
	japana virina <tld/>o parkere kantis hungarajn
	melodiojn<fnt><aut>Juh&aacute;sz K&aacute;roly</aut>: <vrk>Korusa konkurso</vrk>,
	<bib>Monato</bib><lok>jaro 2000a, numero 12a, p. 14a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_o;&c_r;</trd>
  <trd lng="de">Chor</trd>
  <trdgrp lng="fr">
    <trd>choeur<klr> (troupe)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;nekkar</trd>,
    <trd>k&oacute;rus</trd>,
    <trd>kar <klr tip="ind">(sz&iacute;npadi)</klr></trd>
  </trdgrp>
  <trd lng="pt">coro <klr tip="amb">(&ocirc;)</klr></trd>
  <trd lng="ru">&c_h;&c_o;&c_r;</trd>
  <trd lng="sv">k&ouml;r<klr tip="ind"> (mus.)</klr></trd>
</drv>

<drv mrk="korus.0ano">
  <kap><tld/>ano</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      = <ref tip="dif" cel="hxor.0ano">&hcirc;orano</ref>:
      <ekz>
	Alvenis el 13 landoj &ccirc;. 1500 <tld/>anoj<fnt><aut>Juh&aacute;sz
	K&aacute;roly</aut>:
	<vrk>Korusa konkurso</vrk>, <bib>Monato</bib><lok>jaro 2000a,
	numero
	12a, p. 14a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Chors&auml;nger</trd>
  <trdgrp lng="fr">
    <trd>choriste</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;nekkaros</trd>,
    <trd>kar&eacute;nekes</trd>,
    <trd>&eacute;nekkari tag</trd>,
    <trd>k&oacute;rustag</trd>
  </trdgrp>
  <trd lng="ru">&c_h;&c_o;&c_r;&c_i;&c_s;&c_t;</trd>
  <trd lng="sv">k&ouml;rs&aring;ngare</trd>
</drv>
</art>
