<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: korus.xml,v 1.20 2019/10/13 16:10:17 revo Exp $">
<kap>
  <rad>korus</rad>/o
  <fnt>V</fnt>
</kap>
<drv mrk="korus.0o">
  <kap><tld/>o</kap>
  <snc mrk="korus.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="dif" cel="hxor.0o.kantistaro">&Hcirc;oro</ref>:
      <ekz>
        ekleziaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007012.html"
              >Grekio: Jarmila festego ortodoksa</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        trivo&ccirc;a infan<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="https://www.monato.be/2011/010210.php"
              >Junaj arbetoj post 70 jaroj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        montgrimpista <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="https://www.monato.be/2013/010944.php"
              >Kanto de la montoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        milvo&ccirc;a <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Francisko Lorrain</aut>
          <vrk><url ref="https://www.monato.be/2015/011537.php"
              >Fa&ubreve;sto hodia&ubreve;</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        akademiaj <tld/>oj <klr>[&#8230;]</klr> kvankam ili kantas tradicia&jcirc;ojn, sonas tro
        profesie kaj seke
        <fnt>
          <bib>Monato</bib>
          <aut>Anna kaj Mati Pentus</aut>
          <vrk><url ref="https://www.monato.be/2005/008811.php"
              >Katalunio, Okcitanio Kaj Tiel Plu</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
	japana virina <tld/>o parkere kantis hungarajn
	melodiojn<fnt><aut>Juh&aacute;sz K&aacute;roly</aut>: <vrk>Korusa konkurso</vrk>,
	<bib>Monato</bib><lok>jaro 2000a, numero 12a, p. 14a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_o;&c_r;</trd>
  <trd lng="cs">ch&oacute;r</trd>
  <trd lng="de">Chor</trd>
  <trdgrp lng="fr">
    <trd>choeur<klr> (troupe)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;nekkar</trd>,
    <trd>k&oacute;rus</trd>,
    <trd>kar <klr tip="ind">(sz&iacute;npadi)</klr></trd>
  </trdgrp>
  <trd lng="nl">koor</trd>
  <trd lng="pl">ch&oacute;r</trd>
  <trd lng="pt">coro <klr tip="amb">(&ocirc;)</klr></trd>
  <trd lng="ru">&c_h;&c_o;&c_r;</trd>
  <trd lng="sk">ch&oacute;r</trd>
  <trd lng="sv">k&ouml;r<klr tip="ind"> (mus.)</klr></trd>
</drv>

<drv mrk="korus.0ano">
  <kap><tld/>ano</kap>
  <snc mrk="korus.0ano.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="dif" cel="hxor.0ano">&Hcirc;orano</ref>:
      <ekz>
	alvenis el 13 landoj &ccirc;. 1500 <tld/>anoj
        <fnt><aut>Juh&aacute;sz K&aacute;roly</aut>:
	<vrk>Korusa konkurso</vrk>, <bib>Monato</bib><lok>jaro 2000a,
	numero 12a, p. 14a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Chors&auml;nger</trd>
  <trdgrp lng="fr">
    <trd>choriste</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;nekkaros</trd>,
    <trd>kar&eacute;nekes</trd>,
    <trd>&eacute;nekkari tag</trd>,
    <trd>k&oacute;rustag</trd>
  </trdgrp>
  <trd lng="nl">koorzanger</trd>
  <trd lng="pl">ch&oacute;rzysta</trd>
  <trd lng="ru">&c_h;&c_o;&c_r;&c_i;&c_s;&c_t;</trd>
  <trd lng="sv">k&ouml;rs&aring;ngare</trd>
</drv>
</art>
