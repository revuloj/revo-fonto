<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>korus</rad>/o
  <fnt>V</fnt>
</kap>
<drv mrk="korus.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <ref tip="dif" cel="hxor.0o">&hcirc;oro</ref>
    <ekz>
      japana virina <tld/>o parkere kantis hungarajn
      melodiojn<fnt><aut>Juh&aacute;sz K&aacute;roly</aut>: <vrk>Korusa konkurso</vrk>,
      <bib>Monato</bib><lok>jaro 2000a, numero 12a, p. 14a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="de">Chor</trd>
  <trdgrp lng="fr">
    <trd>choeur<klr> (troupe)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_h;&c_o;&c_r;</trd>
</drv>

<drv mrk="korus.0ano">
  <kap><tld/>ano</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <ref tip="dif" cel="hxor.0ano">&hcirc;orano</ref>
    <ekz>
      Alvenis el 13 landoj &ccirc;. 1500 <tld/>anoj<fnt><aut>Juh&aacute;sz K&aacute;roly</aut>:
      <vrk>Korusa konkurso</vrk>, <bib>Monato</bib><lok>jaro 2000a, numero
      12a, p. 14a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="fr">
    <trd>choriste</trd>
  </trdgrp>
</drv>
</art>
