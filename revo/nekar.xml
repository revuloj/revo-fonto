<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: nekar.xml,v 1.6 2024/01/29 15:21:35 revo Exp $">
    <kap>
      <rad>Nekar</rad>/o
    </kap>

<drv mrk="nekar.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="nekar.0o.rivero">
    <dif>
      <ref tip="lst" lst="voko:riveroj" cel="river.0o.GEOG">Rivero</ref>
      en <ref tip="malprt" cel="vurtemberg.Baden0o">Baden-Virtembergo</ref>
      trafluanta i.a. <ref cel="stutgart.0o">Stutgarto</ref>n enfluanta <ref cel="rejn.R0o">Rejno</ref>n
      &ccirc;e <ref cel="manhejm.0o">Manhejmo</ref>:
      <ekz>
        la ma&scirc;ino aldone kapablas transporti same vidvon a&ubreve; vidvinon al la loko de la
        eterna ripozo <klr>[&#x2026;]</klr>, se motorciklisto volas tiel rezervi sian lastan
        voja&gcirc;on, tiu estu ano de la germana regiono Rejno-<tld/>o kaj bonhavulo:
        kostoj komenci&gcirc;as &ccirc;e 1200 e&ubreve;roj
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2012/010723.php">Lastatikle motorcikle</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum la jarcentoj la akvovojoj estis pliampleks<klr>[ig]</klr>itaj trans la riveroj
        <klr>[&#x2026;]</klr> <tld/>o kaj Rejno &gcirc;is Nederlando
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=538204">Flosigado</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_N;&c_e;&c_k;&c_a;&c_r;</trd>
    <trd lng="de">Neckar</trd>
    <trd lng="en">Neckar</trd>
    <trd lng="fr">Neckar</trd>
    <trd lng="pl">Neckar</trd>
    <trd lng="ro">Neckar</trd>
    <trdgrp lng="zh">
    <trd>&#x5167;&#x5361;&#x6CB3; <pr>N&#xE8;ik&#x1CE;H&#xE9;</pr></trd>,
    <trd>&#x5185;&#x5361;&#x6CB3; <pr>N&#xE8;ik&#x1CE;H&#xE9;</pr></trd>,
    <trd>&#x5948;&#x5361;&#x6CB3; <pr>n&#xE0;ik&#x1CE;h&#xE9;</pr></trd>,
    <trd>&#x5976;&#x5361;&#x6CB3; <pr>n&#x1CE;ik&#x1CE;h&#xE9;</pr></trd>
  </trdgrp>
</snc>
</drv>

  </art>

