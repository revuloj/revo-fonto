<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: subalt.xml,v 1.22 2024/03/25 07:28:37 revo Exp $">
<kap>
  <rad>subaltern</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="subalt.0a">
  <kap><tld/>a</kap>
  <snc mrk="subalt.0a.KOMUNE">
    <dif>
      Dependanta de superulo kaj devigata obei ties ordonojn;
      <ref tip="super" cel="ord.sub0igi">subordigita</ref>:
      <ekz>
        instituto metinta fundamentojn por la moderna instrusistemo <tld/>a al la &scirc;tato
        <fnt>
          <vrk><url ref="http://pola-retradio.org/2018/10/e_elsendo-el-la-19-10-2018/">E-elsendo el
          la 19.10.2018</url></vrk>
          <lok>Pola Retradio en Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a konscio
        <fnt>
          <vrk><url ref="&Mondediplo;article1122.html">Le Monde diplomatique' en
          Esperanto]</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_p;&c_a;&c_r;&c_a;&c_d;&c_k;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_n;&c_a;&c_ch;&c_a;&c_l;&c_je;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pod&rcaron;&iacute;zen&yacute; <klr>(adj.)</klr></trd>,
    <trd>subaltern&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>subaltern</trd>,
    <trd>untergeordnet</trd>
  </trdgrp>
  <trd lng="fa">&a_z;&f_y;&a_r;&a_d;&a_s;&a_t;</trd>
  <trd lng="fr">subalterne</trd>
  <trdgrp lng="hu">
    <trd>al&aacute;rendelt</trd>,
    <trd>beosztott</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4E0B;&#x4F4D;&#x306E;  <pr>&#x304B;&#x3044;&#x306E;</pr></trd>,
    <trd>&#x4E0B;&#x7D1A;&#x306E;  <pr>&#x304B;&#x304D;&#x3085;&#x3046;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">ondergeschikt</trd>
  <trdgrp lng="pl">
    <trd>poddany</trd>,
    <trd>podw&lstroke;adny</trd>,
    <trd>zale&zdot;ny</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_ch;&c_i;&c_n;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>nesamostatn&yacute;</trd>,
    <trd>podriaden&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_ib;&c_d;&#x43B;&c_je;&c_g;&#x43B;&c_i;&c_j;</trd>,
    <trd>&#x43D;&c_i;&c_zh;&c_ch;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4E0B;&#x7D1A; <pr>xi&#xE0;j&#xED;</pr></trd>,
    <trd>&#x4E0B;&#x7EA7; <pr>xi&#xE0;j&#xED;</pr></trd>,
    <trd>&#x6B21;&#x7D1A; <pr>c&#xEC;j&#xED;</pr></trd>,
    <trd>&#x6B21;&#x7EA7; <pr>c&#xEC;j&#xED;</pr></trd>,
    <trd>&#x7B2C;&#x4E8C;&#x4F4D; <pr>d&#xEC;&#xE8;rw&#xE8;i</pr></trd>,
    <trd>&#x4E0B;&#x5C42; <pr>xi&#xE0;c&#xE9;ng</pr></trd>,
    <trd>&#x4E0B;&#x5C64; <pr>xi&#xE0;c&#xE9;ng</pr></trd>,
    <trd>&#x88E8; <pr>p&#xED;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="subalt.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="subalt.0ulo.KOMUNE">
    <dif>
      <ref tip="dif" cel="sub.0ulo.KOMUNE">Subulo</ref>:
      <ekz>
        &scirc;i plu parolis, apogante sin sur nomojn de
        <tld/>uloj, kiujn &scirc;i, pro cirkonstancoj, superregis
        <fnt><aut>S. Larbar</aut>, <vrk>Karuseloj</vrk>,
        <lok>1987</lok></fnt>.
      </ekz>
      <ekz>
        kiaj meditoj pasis en la kapo de la generalo anta&ubreve; ol li eniris en la domon de
        sia <tld/>ulo
        <fnt>
          <bib>Monato</bib>
          <aut>Anna kaj Mati Pentus</aut>
          <vrk><url ref="&Monato;/2004/008219.html">Penetrante en la profundon de la
          homanimo</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_n;&c_a;&c_ch;&c_a;&c_l;&c_je;&c_n;&c_y; <klr>(&c_n;&c_a;&c_z;.)</klr></trd>
  <trdgrp lng="cs">
    <trd>pod&rcaron;&iacute;zen&aacute; osoba</trd>,
    <trd>pod&rcaron;&iacute;zen&yacute;</trd>
  </trdgrp>
  <trd lng="de">Untergebener</trd>
  <trdgrp lng="fr">
    <trd>inf&eacute;rieur <klr tip="ind">(personne)</klr></trd>,
    <trd>subalterne <klr tip="ind">(personne)</klr></trd>,
    <trd>subordonn&eacute; <klr tip="ind">(personne)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>al&aacute;rendelt</trd>,
    <trd>beosztott</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x90E8;&#x4E0B;  <pr>&#x3076;&#x304B;</pr></trd>,
    <trd>&#x4E0B;&#x5F79;  <pr>&#x3057;&#x305F;&#x3084;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">ondergeschikte</trd>
  <trdgrp lng="pl">
    <trd>subaltern</trd>,
    <trd>poddany</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_ch;&c_i;&c_n;&c_jo;&c_n;&c_n;&c_y;&c_j; <klr>(&c_s;&c_u;&c_shch;.)</klr></trd>
  <trd lng="sk">podriaden&yacute;</trd>
  <trd lng="uk">&#x43F;&c_ib;&c_d;&#x43B;&c_je;&c_g;&#x43B;&c_i;&c_j; (&c_ib;&#x43C;.)</trd>
</drv>
</art>
