  <trdgrp lng="ja">
    <trd>&#x305B;&#x308A;&#x3082;&#x3061;&#x53D7;&#x3051;  <pr>&#x305B;&#x308A;&#x3082;&#x3061;&#x3046;&#x3051;</pr></trd>,
    <trd>&#x6A4B;&#x53F0;  <pr>&#x304D;&#x3087;&#x3046;&#x3060;&#x3044;</pr></trd>
