<vortaro>
<art mrk="$Id$">
<kap>
  <rad>abutment</rad>/o
</kap>
<drv mrk="abutme.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ARKI</uzo>
  <snc>
    <dif>
      Fortika masona&jcirc;o konstruita &ccirc;e amba&ubreve; ekstremoj de ponta
      arko por kontra&ubreve;stari &gcirc;ian pu&scirc;on.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Widerlager</trd>,
    <trd>St&uuml;tzpfeiler</trd>
  </trdgrp>
  <trd lng="en">abutment</trd>
  <trd lng="fr">contrefort</trd>
  <trd lng="es">contrafuerte</trd>
  <trd lng="nl">steunmuur</trd>
  <trdgrp lng="hu">
    <trd>h&iacute;df&odblac;</trd>,
    <trd>t&aacute;mpill&eacute;r</trd>
  </trdgrp>
  <trd lng="pt">peg&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_p;&c_o;&c_r;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_p;&c_o;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_b;&c_y;&c_k; <klr tip="amb">(&c_m;&c_o;&c_s;&c_t;&c_a;)</klr></trd>
  </trdgrp>
</drv>
</art>
