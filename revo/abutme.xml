  <trd lng="cs">op&ecaron;ra</trd>
  <trdgrp lng="de">
    <trd>Widerlager</trd>,
    <trd>St&uuml;tzpfeiler</trd>
  </trdgrp>
  <trd lng="en">abutment</trd>
  <trd lng="es">contrafuerte</trd>
  <trd lng="fa">&f_p;&a_A;&f_y;&a_h;&a_hamza_sure; &a_j;&a_n;&a_A;&a_H;&f_y;<klr tip="amb"> (&f_p;&a_l;)</klr></trd>
