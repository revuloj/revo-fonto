<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lantern</rad>/o
</kap>
<drv mrk="lanter.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Speco de skatolo kun travidaj flankoj, en kiun oni metas kandelon a&ubreve;
      alian lumilon, por &gcirc;in &scirc;irmi kontra&ubreve; vento:
      <ekz>
        <tld/>a kolono<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="lamp.0o">lampo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_h;&c_t;&c_a;&c_r;</trd>
  <trd lng="br">letern</trd>
  <trd lng="de">Laterne</trd>
  <trd lng="el">&phi;&alfa;&ny;&alfa_ton;&rho;&jota;</trd>
  <trd lng="en">lantern</trd>
  <trd lng="fr">lanterne</trd>
  <trdgrp lng="hu">
    <trd>l&aacute;mpa</trd>,
    <trd>l&aacute;mp&aacute;s</trd></trdgrp>
  <trd lng="nl">lantaarn</trd>
  <trd lng="pt">lanterna</trd>
  <trd lng="ru">&c_f;&c_o;&c_n;&c_a;&c_r;&c_mol;</trd>
  <trd lng="sv">lykta</trd>
</drv>

<drv mrk="lanter.magia0o">
  <kap>
    magia <tld/>o<fnt>Z</fnt>
  </kap>
  <snc>
    <dif>
      Primitiva projekcia <tld/>o.
    </dif>
    <ekz>
        En tiu tempo oni montris la formon de bakterioj per magia <tld/>o
        <fnt><bib>Lusin</bib>, <lok>Anta&ubreve;parolo de la 
        a&ubreve;toro</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="de">Laterna magica</trd>
  <trd lng="en">magic lantern</trd>
  <trd lng="fr">lanterne magique</trd>
  <trd lng="hu">lanterna magica</trd>
  <trd lng="nl">toverlantaarn</trd>
  <trd lng="pt">lanterna m&aacute;gica</trd>
  <trd lng="ru">&c_v;&c_o;&c_l;&c_sh;&c_je;&c_b;&c_n;&c_y;&c_j; &c_f;&c_o;&c_n;&c_a;&c_r;&c_mol; <klr>(&c_p;&c_r;&c_o;&c_je;&c_k;&c_t;&c_o;&c_r;)</klr></trd>
</drv>

<drv mrk="lanter.strata0o">
  <kap>
    strata <tld/>o
  </kap>
  <snc>
    <dif>
      Fiksa lanterno apud la strato por lumigi la vojon.
    </dif>
  </snc>
  <trd lng="br">kleuzeur-straed</trd>
  <trd lng="de">Stra&szlig;enlaterne</trd>
  <trd lng="fr">r&eacute;verb&egrave;re</trd>
  <trd lng="ru">&c_u;&c_l;&c_i;&c_ch;&c_n;&c_y;&c_j; <ind>&c_f;&c_o;&c_n;&c_a;&c_r;&c_mol;</ind></trd>
</drv>

<drv mrk="lanter.trajnfina0o">
  <kap>
    trajnfina <tld/>o
  </kap>
  <snc>
    <dif>
      Certa lanterno je la lasta vagono de vagonaro por indiki la finon de la vagonaro.
    </dif>
  </snc>
  <trd lng="de">Zugschlusslaterne</trd>
  <trd lng="ru">&c_b;&c_u;&c_f;&c_je;&c_r;&c_n;&c_y;&c_j; <ind>&c_f;&c_o;&c_n;&c_a;&c_r;&c_mol;</ind></trd>
</drv>

</art>
