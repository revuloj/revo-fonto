<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lantern</rad>/o
</kap>
<drv mrk="lanter.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Speco de skatolo kun travidaj flankoj, en kiun oni metas kandelon a&ubreve;
      alian lumilon, por &gcirc;in &scirc;irmi kontra&ubreve; vento:
      <ekz>
        <tld/>a kolono<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="lamp.0o">lampo</ref>.
    </refgrp>
  </snc>
  <trd lng="en">lantern</trd>
  <trdgrp lng="hu">
    <trd>l&aacute;mpa</trd>,
    <trd>l&aacute;mp&aacute;s</trd></trdgrp>
  <trd lng="nl">lantaarn</trd>
  <trd lng="ru">&c_f;&c_o;&c_n;&c_a;&c_r;&c_mol;</trd>
  <trd lng="sv">lykta</trd>
</drv>

<drv mrk="lanter.magia0o">
  <kap>
    magia <tld/>o<fnt>Z</fnt>
  </kap>
  <snc>
    <dif>
      Primitiva projekcia <tld/>o.
    </dif>
  </snc>
  <trd lng="en">magic lantern</trd>
  <trd lng="hu">lanterna magica</trd>
  <trd lng="nl">toverlantaarn</trd>
  <trd lng="ru">&c_v;&c_o;&c_l;&c_sh;&c_je;&c_b;&c_n;&c_y;&c_j; &c_f;&c_o;&c_n;&c_a;&c_r;&c_mol; <klr>(&c_p;&c_r;&c_o;&c_je;&c_k;&c_t;&c_o;&c_r;)</klr></trd>
</drv>
</art>
