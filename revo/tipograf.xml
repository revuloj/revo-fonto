<kap>
  <ofc>7</ofc><rad>tipograf</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tipograf.0o">
  <kap><ofc>7</ofc><tld/>o</kap>
  <snc mrk="tipograf.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref tip="dif" cel="tipogr.0a.TIP">Tipografiisto</ref>:
      <ekz>
        en la unuaj monatoj de mia estado en Parizo mi konati&gcirc;is kun 
        mia samlandano S-ro <nom>V. Polg&aacute;r</nom>, <tld/>o <klr>[&#x2026;]</klr> kaj 
        laboristo en la Presa Esperantista Societo
        <fnt>
          <aut>P. De Lengyel</aut>,
          <vrk>El miaj Rememoroj, 4</vrk>,
          <lok><url ref="http://www.tekstoj.nl/lm/lm23-1/index.html">
          Literatura Mondo, 1923-01</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis lernanto de la <tld lit="T"/>-Lernejo de la &Gcirc;enerala 
        Asocio pri Presarto
        <fnt>
          <aut>A. Marco, T. del Barrio</aut>,
          <vrk><url ref="http://lujimfer.spaces.live.com/Blog/cns!38DC6962F360FE23!3511.entry">
          Cayetano Redondo Ace&ntilde;a</url></vrk>,
          <lok>Bolet&iacute;n de Hispana Esperanto-Federacio, 2003-01 &gcirc;is 02, 
          numero 359a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_y;&c_p;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>,
    <trd>&c_d;&c_r;&c_u;&c_k;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="ca">tip&ograve;graf</trd>
  <trd lng="de">Setzer</trd>
  <trd lng="fr">typographe</trd>
  <trdgrp lng="ja">
    <trd>&#x6D3B;&#x7248;&#x5370;&#x5237;&#x6280;&#x8853;&#x8005;  [<ind>&#x304B;&#x3063;&#x3071;&#x3093;&#x3044;&#x3093;&#x3055;&#x3064;&#x304E;&#x3058;&#x3085;&#x3064;&#x3057;&#x3083;</ind>]</trd>,
    <trd>&#x30BF;&#x30A4;&#x30DD;&#x30B0;&#x30E9;&#x30D5;&#x30A1;&#x30FC;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>typograaf</trd>,
    <trd>zetter</trd>
  </trdgrp>
  <trd lng="ro">tipograf</trd>
  <trd lng="uk">&c_d;&c_r;&c_u;&#x43A;&c_a;&c_r;</trd>
</drv>
