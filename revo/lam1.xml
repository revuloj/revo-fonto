<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>lam</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="lam1.0o">
  <kap>
  <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="kameled.0oj">kameledoj</ref>
      (<trd lng="la">Lama</trd>), vivanta en Sudameriko:
      <ekz>
        la <ind>vikunja <tld/>o</ind> vivas en la altaj regionoj de Andoj;
        <trd lng="la">Lama vicugna</trd>
        <trd lng="de">Vikugna</trd>
        <trd lng="es">vicu&ntilde;a</trd>
        <trd lng="hu">vikunyna</trd>
      </ekz>
      <ekz>
        la <ref tip="sub" cel="alpak.0o">alpako</ref> estas
        hejma varia&jcirc;o de la <ind>guanaka <tld/>o</ind>.
        <trd lng="la">Lama guanicoe</trd>
        <trd lng="de">Guanako</trd>
        <trd lng="es">guanaco</trd>
        <trd lng="hu">guanako</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_m;&c_a; <klr 
tip="amb">(&c_zh;&c_y;&c_v;&c_jo;&c_l;&c_a;)</klr></trd>
  <trd lng="de">Lama</trd>
  <trd lng="en">llama</trd>
  <trd lng="es">llama</trd>
  <trd lng="fr">lama</trd>
  <trd lng="hu">l&aacute;ma</trd>
  <trd lng="nl">lama</trd>
  <trd lng="pt">lhama</trd>
  <trd lng="ru">&c_l;&c_a;&c_m;&c_a;
  <klr>(&c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_o;&c_je;)</klr></trd>
</drv>
</art>
