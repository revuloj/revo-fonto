<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>topor</rad>/o<fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="topor.0o">
  <kap><tld/>o</kap>
  <snc mrk="topor.0o.TEK">
  <uzo tip="fak">TEK</uzo>
    <dif>
      La plej komuna speco de
      <ref tip="super" cel="hak.0ilo">hakilo</ref>, kies klingo,
      lar&gcirc;a kaj plata, estas fiksita en la sama ebeno kiel la
      tenilo (kp <ref cel="adz.0o">adzo</ref>); tran&ccirc;a unurande,
      la klingo estas plata alirande, kio ebligas uzi la <tld/>on
      martele:
      <ekz>
        mi ludis rolon de <tld/>o en manoj de sorto
        <fnt><bib>Mary</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_ja;&c_k;&c_je;&c_r;&c_a;</trd>
  <trdgrp lng="bg">
    <trd>&c_t;&c_o;&c_p;&c_o;&c_r;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_d;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>destral</trd>,
    <trd>picassa</trd>
  </trdgrp>
  <trd lng="cs">dřevorubecká sekera</trd>
  <trdgrp lng="de">
    <trd>F&auml;llaxt</trd>,
    <trd>Holzhaueraxt</trd>,
    <trd>Schrotaxt</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ax</trd>,
    <trd>axe</trd>
  </trdgrp>
  <trd lng="fr">cogn&eacute;e<klr> (hache)</klr></trd>
  <trd lng="hu">balta</trd>
  <trd lng="nl">bijl</trd>
  <trd lng="pl">top&oacute;r</trd>
  <trdgrp lng="pt">
    <trd>machado</trd>,
    <trd>machadinha</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_o;&c_p;&c_o;&c_r;</trd>
  <trd lng="sk">drevorubačská sekera</trd>
</drv>
</art>
