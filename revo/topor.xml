<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
 
<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>topor</rad>/o
    </kap>
 
    <drv mrk="topor.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">TEK</uzo>
      <snc>
        <dif>
          La plej komuna speco de
          <ref tip="super" cel="hak.0ilo">hakilo</ref>, kies klingo,
          lar&gcirc;a kaj plata, estas fiksita en la sama ebeno kiel la
          tenilo (kp <ref cel="adz.0o">adzo</ref>); tran&ccirc;a unurande,
          la klingo estas plata alirande, kio ebligas uzi la <tld/>on
          martele.
        </dif>
      </snc>
      <trd lng="be">&c_s;&c_ja;&c_k;&c_je;&c_r;&c_a;</trd>
      <trd lng="fr">cogn&eacute;e<klr> (hache)</klr></trd>
      <trdgrp lng="de">
        <trd>F&auml;llaxt</trd>,
        <trd>Holzhaueraxt</trd>,
        <trd>Schrotaxt</trd>
      </trdgrp>
      <trd lng="nl">bijl</trd>
      <trd lng="ru">&c_t;&c_o;&c_p;&c_o;&c_r;</trd>
    </drv>
  </art>
