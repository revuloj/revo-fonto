<vortaro>
<art mrk="$Id$">
<kap>
  <rad>terminal</rad>/o
</kap>

<drv mrk="terminal1.0o">
  <kap><tld/>o</kap>
  <snc mrk="terminal1.0o.KOMUNE">
    <dif>
      Loko, konstrua&jcirc;aro aran&gcirc;ita por atendo,
      enveturili&gcirc;o a&ubreve; 
      elveturili&gcirc;o de voja&gcirc;antoj a&ubreve;
      transporta&jcirc;oj: 
    </dif>
    <subsnc mrk="terminal1.0o.AVI">
      <uzo tip="fak">AVI</uzo>
      <dif>
        Tia parto de <ref tip="malprt"
        cel="haven.flug0o.AVI">flughaveno</ref>:
        <ekz>
          parto de <tld lit="T"/>o 3 de la flughaveno disfalis je la
          28-a de septembro 2004
          <fnt>
            <bib>Viki</bib>,
            <lok>artikolo &leftquot;Dubaja Internacia
            Flughaveno&rightquot;, 2008-03-03</lok>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="hu">
        <trd>utasforgalmi &eacute;p&uuml;let</trd>,
        <trd>termin&aacute;l</trd>
      </trdgrp>
      <trd lng="pl">terminal<klr> lotniczy</klr></trd>
    </subsnc>
    <subsnc mrk="terminal1.0o.MAR">
      <uzo tip="fak">MAR</uzo>
      <dif>
        Tia parto de <ref tip="malprt" cel="haven.0o.MAR">haveno</ref>:
        <ekz>
          post tri horoj la &scirc;oforo liberigas la pasa&gcirc;erojn
          en la haveno 
          de Kalezo front&#8217; al la kompani<tld/>o
          <fnt>
            <aut>C. Pinardon</aut>,
            <vrk><url
            ref="http://horizonto.free.fr/rakontoj/cirilo/06-normandecanglio.html">
            Normandeca Anglio</url></vrk>,
            <lok>1996-09</lok>
          </fnt>
        </ekz>
      </dif>
      <trd lng="hu">fejkik&ouml;t&odblac;</trd>
      <trd lng="pl">terminal<klr> morski</klr></trd>
    </subsnc>
    <trd lng="de">Terminal<klr tip="amb"> (Empfangsgeb&auml;ude)</klr></trd>
    <trd lng="fr">terminal<klr> (portuaire)</klr></trd>
    <trd lng="pl">terminal</trd>
  </snc>
  <trd lng="ca">terminal</trd>
  <trd lng="it">terminal</trd>
  <trd lng="nl">terminal<klr tip="ind"> (v. lucht- of
  scheepvaartlijn)</klr></trd>
  <trd lng="sk">termin√°l</trd>
</drv>
