<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: toskani.xml,v 1.10 2023/10/21 14:37:42 revo Exp $">
<kap>
  <rad>Toskani</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="toskani.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="toskani.0o.toskanujo">
    <dif>
      <ref tip="dif" cel="toskan.0ujo">Toskanujo</ref>:
      <ekz>
        <tld/>o havas submediteranean moderan klimaton, kun pli oftaj
        pluvoj a&ubreve;tune kaj vintre kaj sekeco somera
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.uk2006.info/Toskanio.htm">Toskanio</url></vrk>,
          <lok>UK 2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        viziti <tld/>on for de la trivitaj itineroj (Florenco, Pizo, Sieno)
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2012/010845.php">Saturnia: Vintra magio</url></vrk>
          <lok>2012/11, p. 15</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_T;&c_a;&c_s;&c_k;&c_a;&c_n;&c_a;</trd>
  <trd lng="bg">&c_T;&c_o;&c_s;&c_k;&c_a;&c_n;&c_a;</trd>
  <trd lng="ca">la <ind>Toscana</ind></trd>
  <trd lng="de">Toskana</trd>
  <trd lng="en">Tuscany</trd>
  <trd lng="fr">Toscane</trd>
  <trd lng="it">Toscana</trd>
  <trd lng="ja">&#x30C8;&#x30B9;&#x30AB;&#x30CA;&#x5DDE;  <pr>&#x30C8;&#x30B9;&#x30AB;&#x30CA;&#x3057;&#x3085;&#x3046;</pr></trd>
  <trd lng="nl">Toscane</trd>
  <trd lng="pl">Toskania</trd>
  <trd lng="uk">&c_T;&#x43E;&c_s;&#x43A;&c_a;&#x43D;&c_a; (&#x43E;&c_b;&#x43B;&c_a;&c_s;&c_t;&#x44C;)</trd>
</drv>
</art>

