<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sibari.xml,v 1.17 2022/03/27 13:09:35 revo Exp $">
<kap>
  <rad>sibarit</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sibari.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="sibari.0o.MOR">
    <dif>
      Homo tro amanta la <ref cel="komfor.0o">komforto</ref>n:
      <ekz>
        la vivo en Marino fluis unutone: Arkadio vivis kiel <tld/>o, <nom>Bazarov</nom> laboris
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>;
      </ekz>
      <ekz>
        lifto lacigas kaj <tld/>igas
        <fnt>
          <bib>VaK</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dibocx.0ulo">dibo&ccirc;ulo</ref>,
      <ref cel="epikur.e0ano.KOMUNE">epikurano<sncref/></ref>,
      <ref cel="hedonist.0o.FIL">hedonisto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
     <trd>&c_s;&c_y;&c_b;&c_a;&c_r;&c_y;&c_t;</trd>, 
     <trd>&c_s;&c_p;&c_je;&c_sh;&c_ch;&c_a;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">sibarita<klr> (subst.)</klr></trd>
  <trdgrp lng="cs">
    <trd>po&zcaron;itk&aacute;&rcaron;</trd>,
    <trd>rozko&scaron;n&iacute;k</trd>,
    <trd>sybarita</trd>,
    <trd>zh&yacute;&ccaron;kanec</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Sybarit</trd>,
    <trd>Schlemmer</trd>
  </trdgrp>
  <trd lng="en">sybarite</trd>
  <trd lng="fr">sybarite</trd>
  <trdgrp lng="hu">
    <trd>szibarita</trd>,
    <trd>&eacute;lvhajh&aacute;sz</trd>,
    <trd>puh&aacute;ny</trd>
  </trdgrp>
  <trd lng="nl">sybariet</trd>
  <trd lng="ru">&c_s;&c_i;&c_b;&c_a;&c_r;&c_i;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>p&ocirc;&zcaron;itk&aacute;r</trd>,
    <trd>rozko&scaron;n&iacute;k</trd>,
    <trd>rozmaznanec</trd>
  </trdgrp>
  <trd lng="uk">&#x441;&#x438;&#x431;&#x430;&#x440;&#x438;&#x442;</trd>
</drv>

<drv mrk="sibari.0ismo">
  <kap><tld/>ismo</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="sibari.0ismo.MOR">
    <dif>
      &Gcirc;uisma <ref tip="super" cel="manier.viv0o">vivmaniero</ref> de <tld/>o:
      <ekz>
        unuflankaj estas &ccirc;iuj civilizoj de l&#x2019; Oriento,
        la &ccirc;ina kun sia izolismo, etiketo, sobreco kaj
        oficialismo <klr>[&#x2026;]</klr> la persa kun sia
        <tld/>ismo, komercemo, mesiismoj kaj cedemo
        al fremdaj influoj
        <fnt>
          <aut>T. Margul, trad. K. &Sacute;liwi&nacute;ski</aut>,
          <vrk><url ref="http://www.autodydaktyka.republika.pl/esperanto/ekkonado.htm">
          Kial Valoras Konadi Orienton?</url></vrk>,
          <lok>[vidita en 2012]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>sybaritstv&iacute;</trd>,
    <trd>vyhled&aacute;v&aacute;n&iacute; pohodln&eacute;ho &zcaron;ivota</trd>
  </trdgrp>
  <trd lng="de">Sy&#xAD;ba&#xAD;ri&#xAD;tis&#xAD;mus</trd>
  <trd lng="fr">sybaritisme</trd>
  <trd lng="sk">vyh&lcaron;ad&aacute;vanie pohodln&eacute;ho &zcaron;ivota</trd>
  <trd lng="uk">&#x441;&#x438;&#x431;&#x430;&#x440;&#x438;&#x442;&#x441;&#x442;&#x432;&#x43E;</trd>
</drv>
</art>
