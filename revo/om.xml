  </snc>
  <trd lng="be">&c_o;&c_m;</trd>
  <trd lng="de">Ohm</trd>
  <trd lng="nl">ohm</trd>
  <trd lng="ru">&c_o;&c_m;</trd>
  <trd lng="sv">ohm</trd>
</drv>
</art>
