<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Om</rad>/o
</kap>
<drv mrk="om.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Germanlingve: <trd lng="de">Georg Simon <ind>Ohm</ind></trd>,
      1787&dash;1854. Germana fizikisto, malkovrinta la le&gcirc;on
      pri proporcieco de elekra tensio kaj kurentintenso. Tiu le&gcirc;o
      nun estas nomata &leftquot;la le&gcirc;o de Omo&rightquot;.
    </dif>
  </snc>
  <trd lng="ru">&c_G;&c_je;&c_o;&c_r;&c_g; &c_S;&c_i;&c_m;&c_o;&c_n; <ind>&c_O;&c_m;</ind></trd>
</drv>

<drv mrk="om.o0o">
  <kap><tld lit="o"/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      Unuo de elektra <ref tip="vid" cel="rezist1.0o">rezistanco</ref>
      de kondukilo, kiu tralasas kurenton de
      unu <ref tip="vid" cel="amper.a0o">ampero</ref> sub tensio de unu
      <ref tip="vid" cel="volt.0o">volto</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_o;&c_m;</trd>
  <trd lng="de">Ohm</trd>
  <trd lng="hu">ohm</trd>
  <trd lng="nl">ohm</trd>
  <trd lng="ru">&c_o;&c_m;</trd>
  <trd lng="sv">ohm</trd>
</drv>
</art>
