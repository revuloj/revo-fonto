<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rap</rad>/o
</kap>
<drv mrk="rap.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="brasik.0o">brasiko</ref>
      (<trd lng="la">Brassica rapa</trd>), kun dika manĝebla radiko.
    </dif>
  </snc>
  <trd lng="be">рэпа</trd>
  <trd lng="de">Stoppelrübe</trd>
  <trdgrp lng="en">
    <trd>turnip</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tarlórépa</trd>,
    <trd>kerekrépa</trd>,
    <trd>fehérrépa</trd>
  </trdgrp>
  <trd lng="nl">raap</trd>
  <trd lng="pt">couve-rábano</trd>
  <trd lng="ru">репа</trd>
</drv>
</art>
