<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: makaro.xml,v 1.19 2022/03/23 14:01:05 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>makaron</rad>/o
</kap>
<drv mrk="makaro.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="makaro.0o.frandajxo">
    <dif>
      <ref tip="super" cel="frand.0ajxo">Franda&jcirc;o</ref> el pistitaj
      migdaloj miksitaj
      kun sukero kaj kirlita ovoblanko:
      <ekz>
        ofte mi al&scirc;ovadis al vi kuketojn, a&ubreve; biskvitojn, a&ubreve; <tld/>ojn
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_ib;&c_n;&c_d;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_je; &c_p;&c_je;&c_ch;&c_y;&c_v;&c_a;</trd>
    <trd lng="br">morlukenn</trd>
    <trd lng="ca">macarr&oacute;<klr> (pastisseria)</klr></trd>
    <trdgrp lng="de">
      <trd>Makrone</trd>,
      <trd>Mandelt&ouml;rtchen</trd>
    </trdgrp>                     
    <trd lng="en">macaroon</trd>
    <trd lng="fr">macaron</trd>
    <trdgrp lng="hu">
      <trd>mandul&aacute;s cs&oacute;k</trd>,
      <trd>mandul&aacute;s puszedli</trd>
    </trdgrp>  
    <trd lng="nl">bitterkoekje</trd>
    <trdgrp lng="pl">
      <trd>ciasteczko migda&lstroke;owe</trd>,
      <trd>makaronik</trd>
    </trdgrp>   
    <trd lng="pt">marzip&atilde;</trd>
    <trdgrp lng="ro">
      <trd>fursec de migdale</trd>,
      <trd>picromigdale</trd>
    </trdgrp>
    <trd lng="ru">&c_m;&c_i;&c_n;&c_d;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_je; &c_p;&c_je;&c_ch;&c_je;&c_n;&c_mol;&c_je;</trd>
  </snc>    
  <snc mrk="makaro.0o.nudelo">
    <fnt><bib>FK</bib></fnt>
    <dif>
      <ref tip="dif" cel="makaro1.0o">Makaronio</ref>:
      <ekz>
        grio, <tld/>oj, vermi&ccirc;eloj, pano, baka&jcirc;o kaj faruna&jcirc;o de
        diversaj specoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        jen estas faboj, kesto da rizo, tapioko kaj <tld/>oj
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Makkaroni</trd>
  </snc>                            
  <trd lng="uk">&#x43C;&#x438;&#x433;&#x434;&#x430;&#x43B;&#x44C;&#x43D;&#x435; &#x43F;&#x435;&#x447;&#x438;&#x432;&#x43E;</trd>
</drv>
</art>
