<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>makaron</rad>/o
</kap>
<drv mrk="makaro.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Franda&jcirc;o el pistitaj migdaloj miksitaj kun sukero kaj kirlita
      ovoblanko.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_n;&c_d;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_je; &c_p;&c_je;&c_ch;&c_y;&c_v;&c_a;</trd>
  <trdgrp lng="de">
      <trd>Makrone</trd>,
      <trd>Mandelt&ouml;rtchen</trd>
  </trdgrp>
  <trd lng="en">macaroon</trd>
  <trdgrp lng="hu">
      <trd>mandul&aacute;s cs&oacute;k</trd>,
      <trd>mandul&aacute;s puszedli</trd>
  </trdgrp>  
  <trd lng="nl">bitterkoekje</trd>
  <trd lng="ru">&c_m;&c_i;&c_n;&c_d;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_je; &c_p;&c_je;&c_ch;&c_je;&c_n;&c_mol;&c_je;</trd>
</drv>
</art>
