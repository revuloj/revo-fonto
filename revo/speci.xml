<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>speci</rad>/o
  <fnt>PIV</fnt>
</kap>
<drv mrk="speci.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      &Scirc;tupo de la botanika kaj zoologia
      klasifikoj (<trd lng="la">species</trd>).
      Membroj de <tld/>o povas interkruci&gcirc;i
      kaj heredigi siajn ecojn:
      <ekz>
        la botanikistoj asertas, ke post 50 jaroj 60 000
        plant<tld/>oj estos malaperintaj
        <fnt><aut>Robert Roze</aut>: <vrk>Vivisekcio de
        polenero</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero 11a, p.
        12a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="genr.0o.BIO">genro</ref>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_d; <klr tip="amb">(&c_b;&c_ib;&c_ja;&c_l;&c_ja;&c_g;&c_ib;&c_ch;&c_n;&c_y;)</klr></trd>
  <trd lng="de">Art</trd>
  <trd lng="en">species</trd>
  <trdgrp lng="fr">
    <trd>esp&egrave;ce<klr> (biol.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>faj</trd>,
    <trd>species</trd>
  </trdgrp>
  <trd lng="nl">soort</trd>
  <trd lng="ru">&c_v;&c_i;&c_d; <klr tip="amb">(&c_b;&c_i;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;)</klr></trd>
</drv>
</art>
