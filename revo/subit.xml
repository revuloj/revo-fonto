<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>subit</rad>/a
</kap>
<drv mrk="subit.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Tute rapide kaj neatendite okazanta:
      <ekz>
        <tld/>a teruro<fnt>Z</fnt>,
      </ekz>
      <ekz>
        morto;
      </ekz>
      <ekz>
        neniam atendita (io) ofte venas <tld/>e
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind><tld/>e</ind> fali;
        <trd lng="ru">&c_v;&c_n;&c_je;&c_z;&c_a;&c_p;&c_n;&c_o;</trd>
      </ekz>
      <ekz>
        <tld/>e venos la pereo<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="abrupt.0a">abrupta</ref>.
    </refgrp>
  </snc>
  <trd lng="de">pl&ouml;tzlich</trd>
  <trd lng="en">sudden</trd>
  <trd lng="nl">plotseling</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_n;&c_je;&c_z;&c_a;&c_p;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_o;&c_zh;&c_i;&c_d;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_k;&c_o;&c_r;&c_o;&c_p;&c_o;&c_s;&c_t;&c_i;&c_zh;&c_n;&c_y;&c_j; <klr>(pri morto)</klr></trd>
  </trdgrp>
</drv>
</art>
