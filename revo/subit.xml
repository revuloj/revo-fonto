<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>subit</rad>/a
</kap>
<drv mrk="subit.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Tute rapide kaj neatendite okazanta:
      <ekz>
        <tld/>a teruro<fnt>Z</fnt>,
      </ekz>
      <ekz>
        morto;
      </ekz>
      <ekz>
        neniam atendita (io) ofte venas <tld/>e
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind><tld/>e</ind> fali;
        <trdgrp lng="be">
          <trd>&c_r;&c_a;&c_p;&c_t;&c_a;&c_m;</trd>, 
          <trd>&c_r;&c_a;&c_p;&c_t;&c_o;&c_w;&c_n;&c_a;</trd>, 
          <trd>&c_z;&c_mol;&c_n;&c_ja;&c_n;&c_a;&c_c;&c_k;&c_u;</trd>, 
          <trd>&c_n;&c_je;&c_ch;&c_a;&c_k;&c_a;&c_n;&c_a;</trd>, 
          <trd>&c_n;&c_je;&c_s;&c_p;&c_a;&c_d;&c_z;&c_ja;&c_v;&c_a;&c_n;&c_a;</trd>, 
          <trd>&c_n;&c_je;&c_s;&c_p;&c_a;&c_d;&c_z;&c_je;&c_w;&c_n;&c_a;</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>hirtelen</trd>,
          <trd>v&aacute;ratlanul</trd>
        </trdgrp>
        <trd lng="ru">&c_v;&c_n;&c_je;&c_z;&c_a;&c_p;&c_n;&c_o;</trd>
      </ekz>
      <ekz>
        <tld/>e venos la pereo<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="abrupt.0a">abrupta</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_p;&c_t;&c_o;&c_w;&c_n;&c_y;</trd>, 
    <trd>&c_n;&c_je;&c_ch;&c_a;&c_k;&c_a;&c_n;&c_y;</trd>, 
    <trd>&c_n;&c_je;&c_s;&c_p;&c_a;&c_d;&c_z;&c_ja;&c_v;&c_a;&c_n;&c_y;</trd>, 
    <trd>&c_n;&c_je;&c_s;&c_p;&c_a;&c_d;&c_z;&c_je;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>sobtat</trd>, 
    <trd>sobt&oacute;s</trd> 
  </trdgrp>
  <trd lng="cs">náhlý</trd>
  <trd lng="de">pl&ouml;tzlich</trd>
  <trd lng="en">sudden</trd>
  <trdgrp lng="es">
    <trd>s&uacute;bito</trd>, 
    <trd>repentino</trd> 
  </trdgrp>
  <trd lng="fr">subit</trd>
  <trdgrp lng="hu">
    <trd>hirtelen</trd>,
    <trd>v&aacute;ratlan</trd>
  </trdgrp>
  <trd lng="nl">plotseling</trd>
  <trdgrp lng="pl">
    <trd>nag&lstroke;y</trd>,
    <trd>raptowny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_n;&c_je;&c_z;&c_a;&c_p;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_o;&c_zh;&c_i;&c_d;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_k;&c_o;&c_r;&c_o;&c_p;&c_o;&c_s;&c_t;&c_i;&c_zh;&c_n;&c_y;&c_j;
<klr>(pri morto)</klr></trd>
  </trdgrp>
  <trd lng="sk">náhly</trd>
</drv>
</art>
