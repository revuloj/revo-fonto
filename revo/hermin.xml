<art mrk="$Id$">
<kap>
  <rad>hermini</rad>/o
</kap>

<drv mrk="hermin.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zooligaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="noktue.0oj">noktuedoj</ref>
      (<trd lng="la">Herminia</trd>), nokta papilio.
    </dif>
  </snc>
  <trd lng="de">Herminiinae</trd>
  <trdgrp lng="nl">
    <trd>herminia</trd>,
    <trd>uiltje<klr tip="ind"> (nachtvlinder)</klr></trd>
  </trdgrp>
  <trd lng="pt">borboleta noturna</trd>
</drv>
</art>
