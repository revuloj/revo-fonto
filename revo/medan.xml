<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: medan.xml,v 1.4 2022/09/07 14:38:34 afido Exp $">
    <kap>
      <rad>Medan</rad>/o
    </kap>

<drv mrk="medan.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Granda <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      en la nordo de <ref tip="malprt" cel="sumatr.0o">Sumatro</ref>,
      <ref tip="malprt" cel="indonezi.I0o">Indonezio</ref>, &ccirc;. 300&#8239;km 
      nordokcidente de <ref cel="kualalumpur.0o">Kuala-Lumpuro</ref>:
      <ekz>
        <tld/>o <klr>[&#8230;kiel]</klr> vila&gcirc;o fondi&gcirc;is en la 1-an de januaro 1590
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Medano">Medano</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i lernis Esperanton anta&ubreve; nur kelkaj monatoj kaj decidis viziti la
        Esperanto-domon en la urbo <tld/>o
        <fnt>
          <vrk><url
          ref="http://kontakto.tejo.org/2015/05/akvo-la-oro-de-la-tria-jarmilo-kontakto.html">Akvo,
          la oro de la tria jarmilo</url></vrk>
          <lok>Kontakto 2015:2</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_M;&c_e;&c_d;&c_a;&c_n;</trd>
    <trd lng="de">Medan</trd>
    <trd lng="en">Medan</trd>
    <trd lng="fr">Medan</trd>
    <trd lng="id">Kota Medan</trd>
    <trd lng="pl">Medan</trd>     
    <trd lng="ro">Medan</trd>
  </snc>
</drv>

  </art>

