<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>fand</rad>/i
</kap>
<drv mrk="fand.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Fluecigi per varmo solidajn korpojn:
      <ekz>
        <tld/>i metalon<fnt>B</fnt>, vakson, buteron<fnt>B</fnt>, ne&gcirc;on;
      </ekz>
      <ekz>
        ar&gcirc;ento, purigita en tera <ind><tld/>ujo</ind> kaj sepfoje
        re<tld/>ita
        <fnt><bib>MT</bib>, <lok>&Psa; 12:6</lok></fnt>.
        <trd lng="fr">creuset</trd>
        <trd lng="hu">olvaszt&oacute;t&eacute;gely</trd>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_l;&c_a;&c_v;&c_ib;&c_c;&c_mol;</trd>,
      <trd>&c_t;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">schmelzen <klr tip="ind">(fl&uuml;ssig machen)</klr></trd>
    <trd lng="en">melt</trd>
    <trd lng="hu">olvaszt</trd>
    <trd lng="nl">doen <ind>smelten</ind></trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_t;&c_o;&c_p;&c_i;&c_t;&c_mol; <klr tip="ind">(&c_v;&c_o;&c_s;&c_k;, &c_m;&c_a;&c_s;&c_l;&c_o;)</klr></trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      <tld lit="F"/>e fabriki, <tld/>e muldi:
      <ekz>
        <tld/>i sonorilegon, kanonon, statuon;
      </ekz>
      <ekz>
        li <tld/>is por &gcirc;i kvar orajn ringojn sur &gcirc;iaj kvar anguloj
        <fnt><bib>MT</bib>, <lok>&Eli; 37:3</lok></fnt>;
      </ekz>
      <ekz>
        ili faris al si <tld/>itan bovidon kaj adorklini&gcirc;is al &gcirc;i
        <fnt><bib>MT</bib>, <lok>&Eli; 32:8</lok></fnt>;
      </ekz>
      <ekz>
        la skulptisto kura&gcirc;igas la <ind><tld/>isto</ind>n
        <fnt><bib>MT</bib>, <lok>&Jes; 41:7</lok></fnt>.
        <trd lng="fr">fondeur</trd>
        <trd lng="hu">olvaszt&aacute;r</trd>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_l;&c_ib;&c_c;&c_mol; <klr tip="amb">(&c_m;&c_e;&c_t;&c_a;&c_l;)</klr></trd>,
      <trd>&c_a;&c_d;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_m;&c_e;&c_t;&c_a;&c_l;)</klr></trd>
    </trdgrp>
    <trd lng="de">gie&szlig;en</trd>
    <trd lng="hu">&ouml;nt</trd>
    <trd lng="nl">gieten<klr tip="ind"> (v.metaal)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_l;&c_i;&c_t;&c_mol; <klr tip="ind">(&c_m;&c_je;&c_t;&c_a;&c_l;&c_l;)</klr></trd>,
      <trd>&c_o;&c_t;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol; <klr tip="ind">(&c_m;&c_je;&c_t;&c_a;&c_l;&c_l;)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="es">fundir</trd>
  <trdgrp lng="fr">
    <trd>fondre<klr tip="ind"> (du m&eacute;tal, un objet, etc.)</klr></trd>,
    <trd>liqu&eacute;fier</trd>,
    <trd>couler<klr tip="ind"> (du m&eacute;tal, un objet, etc.)</klr></trd>,
    <trd>mouler<klr tip="ind"> (couler)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="fand.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Flueci&gcirc;i per varmo:
      <ekz>
        vakso <tld/>i&gcirc;as de fajro
        <fnt><bib>MT</bib>, <lok>&Psa; 68:2</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;anta kandelo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="degel.0i">degeli</ref>,
      <ref cel="solv">solvi&gcirc;i</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_p;&c_l;&c_a;&c_v;&c_ib;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">schmelzen <klr tip="ind">(fl&uuml;ssig werden)</klr></trd>
    <trd lng="hu">megolvad</trd>
    <trdgrp lng="nl">
      <trd>smelten<klr tip="ind"> (vloeibaar worden)</klr></trd>,
      <trd>vloeibaar worden</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_a;&c_v;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_t;&c_a;&c_ja;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kormoli&gcirc;i, morale malforti&gcirc;i:
      <ekz>
        <tld/>i&gcirc;i pro <klr>(a&ubreve; de)</klr> kompato;
      </ekz>
      <ekz>
        &scirc;ia animo <tld/>i&gcirc;is pro plezurego;
      </ekz>
      <ekz>
        lia fiereco <tld/>i&gcirc;is en la humili&gcirc;oj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_s;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="hu">megenyh&uuml;l</trd>
    <trd lng="nl">smelten</trd>
    <trd lng="ru">&c_t;&c_a;&c_ja;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malaperi kunkonfuzi&gcirc;ante:
      <ekz>
        &ccirc;io <tld/>i&gcirc;is en pala krepusko
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 24a</lok></fnt>;
      </ekz>
      <ekz>
        la nuboj <tld/>i&gcirc;as kun la horizonto.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flu">forflui</ref>,
      <ref cel="perd">perdi&gcirc;i</ref>,
      <ref cel="neni">disnenii&gcirc;i</ref>.
    </refgrp>
    <trd lng="be">&c_r;&c_a;&c_s;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="hu">
      <trd>elmos&oacute;dik</trd>,
      <trd>&ouml;sszeolvad</trd>
    </trdgrp>
    <trd lng="nl">versmelten</trd>
    <trd lng="ru">&c_t;&c_a;&c_ja;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>fondre<klr tip="ind"> (intr)</klr></trd>,
    <trd>se <ind>fondre</ind></trd>,
    <trd>mollir<klr tip="ind"> (fig., s'attendrir)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="fand.dis0ifor0i">
  <kap>dis<tld/>i, for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malaperigi:
      <ekz>
        lumaj radioj dis<tld/>is la rigidecon de &scirc;iaj
        trajtoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la montoj tremas anta&ubreve; Li, la montetoj dis<tld/>i&gcirc;as
        <fnt><bib>MT</bib>, <lok>&Nah; 1:5</lok></fnt>;
      </ekz>
      <ekz>
        tia konduto for<tld/>is la anta&ubreve;ju&gcirc;ojn de la
        nobelaro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la tempo for<tld/>is tiun rigidan indiferentecon.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>faire <ind>fondre</ind><klr tip="ind"> (faire
      dispara&icirc;tre)</klr></trd>,
      <trd>dissiper</trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_a;&c_s;&c_t;&c_o;&c_p;&c_i;&c_t;&c_mol; <klr tip="ind">(&c_s;&c_n;&c_je;&c_g;, &c_h;&c_o;&c_l;&c_o;&c_d;&c_n;&c_o;&c_s;&c_t;&c_mol; &c_i;
    &c_t;.&c_p;.)</klr></trd>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_t;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="hu">
    <trd>eloszlat</trd>,
    <trd>szertefoszlat</trd>
  </trdgrp>
  <trd lng="nl">wegsmelten</trd>
</drv>

<drv mrk="fand.kun0i">
  <kap>kun<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fand.kun0i.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Miksi per <tld/>ado:
      <ekz>
        fero ne kun<tld/>i&gcirc;as kun argilo
        <fnt><bib>MT</bib>, <lok>&Dan; 2:43</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        pli a&ubreve; malpli frue la ideoj de la esperantismo kaj pacifismo tute
        kun<tld/>i&gcirc;os<fnt>Z</fnt>
        <klr>(sendiferenci&gcirc;os)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>kun<tld/>i&gcirc;o</ind> de nacioj en pli lar&gcirc;an unuon;
        <trd lng="fr">fusion<klr tip="ind"> (m&eacute;lange)</klr></trd>
        <trdgrp lng="hu">
          <trd>&ouml;sszeolvad&aacute;s</trd>,
          <trd>f&uacute;zi&oacute;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kun<tld/>i&gcirc;antaj <klr>(harmonie intermiksitaj)</klr> koloroj.
      </ekz>
    </dif>
    <ref tip="vid" cel="aloj.0o">alojo</ref>
    <trd lng="be">&c_s;&c_p;&c_l;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>
    <trdgrp lng="fr">
      <trd>fondre<klr tip="ind"> (fusionner)</klr></trd>,
      <trd>fusionner</trd>,
      <trd>m&eacute;langer</trd>
    </trdgrp>
    <trd lng="hu">&ouml;sszeolvaszt</trd>
    <trd lng="nl">samensmelten</trd>
    <trd lng="ru">&c_s;&c_p;&c_l;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol; <klr>(&c_m;&c_je;&c_t;&c_a;&c_l;&c_l;&c_y; &c_i; &c_t;.&c_p;.)</klr></trd>
  </snc>
  <snc mrk="fand.kun0i.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Kunigi plurajn ordigitajn datumarojn (listojn, dosierojn) tiel,
      ke la rezulto havu tian saman ordon.
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_b;'&c_ja;&c_d;&c_n;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_mol;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>merge</trd>,
      <trd>collate</trd>
    </trdgrp>
    <trd lng="hu">&ouml;sszef&eacute;s&uuml;l</trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_malmol;&c_je;&c_d;&c_i;&c_n;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="fand.kun0o">
  <kap>kun<tld/>o</kap>
  <uzo tip="fak">LIN</uzo>
  <snc mrk="fand.kun0o.LIN">
    <dif>
      &Ccirc;e <ref tip="malprt" cel="vort.0farado">vortfarado</ref>,
      tia unui&gcirc;o de kombinataj vortelementoj ke ne plu eblas
      indiki limon inter ili:
      <ekz>
        en la latina <ctl>rex</ctl> <em>x</em> prezentas kun<tld/>on de la
        radika konsonanto <em>-g</em> kun la nominativa fina&jcirc;o.
      </ekz>
    </dif>
    <trd lng="be">&c_f;&c_u;&c_z;&c_ib;&c_ja;</trd>
    <trd lng="en">fusion</trd>
    <trd lng="fr">fusion<klr tip="ind"> (ling.)</klr></trd>
    <trd lng="nl">fusie<klr tip="ind"> (ling.)</klr></trd>
    <trd lng="ru">&c_f;&c_u;&c_z;&c_i;&c_ja;</trd>
  </snc>
</drv>

<drv mrk="fand.kun0ema.lingvo">
  <kap>kun<tld/>ema lingvo</kap>
  <uzo tip="fak">LIN</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="lingv.0o.homa">Lingvo</ref> en kies
      <ref cel="fleksi.0i">fleksiado</ref> oftas
      <ref tip="prt" cel="fand.kun0o.LIN">kunfandoj</ref>..
    </dif>
    <adm>
      Cxu "kun~ema (p..p. lingvo)" ne estus pli bona derivajxo?
      <aut>MB</aut>
    </adm>
    <ref tip="ant" cel="agluti.0a">aglutina lingvo</ref>
    <trd lng="be">&c_f;&c_l;&c_je;&c_k;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>
    <trd lng="en">inflective language</trd>
    <trd lng="fr">langue flexionnelle</trd>
    <trd lng="hu">flekt&aacute;l&oacute; <ind>nyelv</ind></trd>
    <trd lng="ru">&c_f;&c_l;&c_je;&c_k;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
  </snc>
</drv>
</art>
