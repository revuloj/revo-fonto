<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>fand</rad>/i
    </kap>
    <drv mrk="fand.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Fluecigi per varmo solidajn korpojn:
          <ekz>
            <tld/>i metalon<fnt>B</fnt>, vakson, buteron<fnt>B</fnt>, ne&gcirc;on.
          </ekz>
        </dif>
        <trd lng="de">schmelzen <klr tip="ind">(fl&uuml;ssig machen)</klr></trd>
        <trd lng="en">melt</trd>
        <trdgrp lng="ru"><trd>&c_p;&c_l;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>,
          <trd>&c_t;&c_o;&c_p;&c_i;&c_t;&c_mol; <klr tip="ind">(&c_v;&c_o;&c_s;&c_k;, &c_m;&c_a;&c_s;&c_l;&c_o;)</klr></trd></trdgrp>
      </snc>
      <snc>
        <dif>
          <tld/>e fabriki, <tld/>e muldi:
          <ekz>
            <tld/>i sonorilegon, kanonon, statuon.
          </ekz>
        </dif>
        <trd lng="de">gie&szlig;en</trd>
        <trdgrp lng="ru"><trd>&c_l;&c_i;&c_t;&c_mol; <klr tip="ind">(&c_m;&c_je;&c_t;&c_a;&c_l;&c_l;)</klr></trd>,
          <trd>&c_o;&c_t;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol; <klr tip="ind">(&c_m;&c_je;&c_t;&c_a;&c_l;&c_l;)</klr></trd></trdgrp>
      </snc>
    </drv>
    <drv mrk="fand.0igxi">
      <kap><tld/>i&gcirc;i</kap>
      <snc>
        <dif>
          Flueci&gcirc;i per varmo:
          <ekz>
            vakso <tld/>i&gcirc;as de fajro
            <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
              <lok>Psalmaro 68, 2</lok>)</fnt>;
          </ekz>
          <ekz>
            <tld/>i&gcirc;anta kandelo
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="degel.0i">degeli</ref>,
          <ref cel="solv">solvi&gcirc;i</ref>.
        </refgrp>
        <trd lng="de">schmelzen <klr tip="ind">(fl&uuml;ssig werden)</klr></trd>
        <trdgrp lng="ru"><trd>&c_p;&c_l;&c_a;&c_v;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
          <trd>&c_t;&c_a;&c_ja;&c_t;&c_mol;</trd></trdgrp>
      </snc>
      <snc>
        <uzo tip="klr">(f)</uzo>
        <dif>
          Kormoli&gcirc;i, morale malforti&gcirc;i:
          <ekz>
            <tld/>i&gcirc;i pro <klr>(a&ubreve; de)</klr> kompato;
          </ekz><ekz>
            &scirc;ia animo <tld/>i&gcirc;is pro plezurego;
          </ekz><ekz>
            lia fiereco <tld/>i&gcirc;is en la humili&gcirc;oj<fnt>Z</fnt>.
          </ekz> 
        </dif>
        <trd lng="ru">&c_t;&c_a;&c_ja;&c_t;&c_mol;</trd>
      </snc>
      <snc>
        <uzo tip="klr">(f)</uzo>
        <dif>
          Malaperi kunkonfuzi&gcirc;ante:
          <ekz>
            &ccirc;io <tld/>i&gcirc;is en pala krepusko
            <fnt>(<aut>Prus, trad. Kabe</aut>: <vrk>La Faraono, 2a vol.</vrk>,
              <lok>&ccirc;ap. 24</lok>)</fnt>;
          </ekz><ekz>
            la nuboj <tld/>i&gcirc;as kun la horizonto;
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="flu">forflui</ref>,
          <ref cel="perd">perdi&gcirc;i</ref>,
          <ref cel="neni">disnenii&gcirc;i</ref>.
        </refgrp>
        <trd lng="ru">&c_t;&c_a;&c_ja;&c_t;&c_mol;</trd>
      </snc>
    </drv>
    <drv mrk="fand.dis0ifor0i">
      <kap>dis<tld/>i, for<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Malaperigi:
          <ekz>
            lumaj radioj dis<tld/>is la rigidecon de siaj
            trajtoj<fnt>Z</fnt>;
          </ekz>
          <ekz>
            tia konduto for<tld/>is la anta&ubreve;ju&gcirc;ojn de la
            nobelaro<fnt>Z</fnt>;
          </ekz>
          <ekz>
            la tempo for<tld/>is tiun rigidan indiferentecon.
          </ekz>
        </dif>
        <trd lng="ru">&c_r;&c_a;&c_s;&c_t;&c_o;&c_p;&c_i;&c_t;&c_mol;
          <klr tip="ind">(&c_s;&c_n;&c_je;&c_g;, &c_h;&c_o;&c_l;&c_o;&c_d;&c_n;&c_o;&c_s;&c_t;&c_mol; &c_i; &c_t;.&c_p;.)</klr></trd>
      </snc>
    </drv>
    <drv mrk="fand.kun0i">
      <kap>kun<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Miksi per <tld/>ado:
          <ekz>
            pli a&ubreve; malpli frue la ideoj de la esperantismo kaj pacifismo tute
            kun<tld/>i&gcirc;os<fnt>Z</fnt> 
            <klr>(sendiferenci&gcirc;os)</klr>;
          </ekz><ekz>
            kun<tld/>i&gcirc;o de nacioj en pli lar&gcirc;an unuon;
          </ekz><ekz>
            <tld/>i&gcirc;antaj <klr>(harmonie intermiksitaj)</klr> koloroj
          </ekz>
        </dif>
        <ref tip="prt" cel="aloj.0o">alojo</ref>
        <trd lng="ru">&c_s;&c_p;&c_l;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol; <klr>(&c_m;&c_je;&c_t;&c_a;&c_l;&c_l;&c_y; &c_i; &c_t;.&c_p;.)</klr></trd>
      </snc>
    </drv>
    <drv mrk="fand.kun0o">
      <kap>kun<tld/>o</kap>
      <snc mrk="fand.kun0o.LIN">
        <uzo tip="fak">LIN</uzo>
        <dif>
          &Ccirc;e <ref tip="malprt" cel="vort.0farado">vortfarado</ref>,
          tia unui&gcirc;o de kombinataj vortelementoj ke ne plu eblas
          indiki limon inter ili:
          <ekz>
            la angla <ctl>index</ctl> havas multenombron
            <ctl>indices</ctl>, en kiu la lasta silabo de la radiko
            kun<tld/>i&gcirc;as kun la fina&jcirc;o.
          </ekz>
        </dif>
        <trd lng="en">fusion</trd>
        <trd lng="ru">&c_f;&c_u;&c_z;&c_i;&c_ja;</trd>
      </snc>
    </drv>
    <drv mrk="fand.kun0ema.lingvo">
      <kap>kun<tld/>ema lingvo</kap>
      <snc>
        <uzo tip="fak">LIN</uzo>
        <dif>
          <ref tip="super" cel="lingv.0o.homa">Lingvo</ref> en kies
          <ref cel="fleksi.0i">fleksiado</ref> oftas
          <ref tip="prt" cel="fand.kun0o.LIN">kunfandoj</ref>.
        </dif>
        <ref tip="ant" cel="agluti.0a.lingvo">aglutina lingvo</ref>
        <trd lng="en">inflective language</trd>
        <trd lng="ru">&c_f;&c_l;&c_je;&c_k;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
      </snc>
    </drv>
  </art>
