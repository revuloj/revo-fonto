<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: fand.xml,v 1.41 2022/10/09 16:50:47 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>fand</rad>/i <fnt><bib>MT</bib></fnt>
    </kap>

<drv mrk="fand.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">TEK</uzo>
  <snc mrk="fand.0i.likvigi">
    <dif>
      Likvigi per varmo solidajn korpojn:
      <ekz>
        <tld/>i metalon, vakson, buteron, ne&gcirc;on;
      </ekz>
      <ekz>
        unu posttagmezon <klr>fulmo</klr> <tld/>is sur Kapitolo
        bronzan statuon de Cereso
        <fnt>
          <aut>H. Sienkewicz, trad. Lidia Zamenhof</aut>,
          <vrk>Quo vadis</vrk>, <lok>&ccirc;ap. 58a</lok>
        </fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>la suno</klr> <tld/>as la asfalton
        <fnt><bib>Metrop</bib></fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        mi ne scias, mi nur sentas, kaj turmento <tld/>as min
        <fnt>
          <aut>Catullus, trad. K. Kalocsay</aut>,
          <vrk>Elegioj</vrk>,
          <lok>Nica Literatura Revuo, 3:6, p. 221</lok>
        </fnt>.
      </ekz>        
    </dif>
    <rim>
      En unu loko <nom>Zamenhof</nom> uzas la verbon netransitive:      
      <ekz>
        aliaj <tld/>igis oron &ccirc;e malgrandaj flamoj super feraj tripiedoj
        <fnt>
          <bib>Marta</bib>
        </fnt>
      </ekz>, sed en multaj aliaj lokoj de la sama verko transitive:    
          
      <ekz>
        li simple ricevas kapturnon kaj la koro en li <tld/>i&gcirc;as
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </rim>
    <trdgrp lng="be">
      <trd>&c_p;&c_l;&c_a;&c_v;&c_ib;&c_c;&c_mol;</trd>,
      <trd>&c_t;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">schmelzen <klr tip="ind">(fl&uuml;ssig
    machen)</klr></trd>
    <trd lng="en">melt</trd>
    <trd lng="hu">olvaszt</trd>
    <trd lng="nl">doen <ind>smelten</ind></trd>
    <trd lng="no">smelte<klr tip="ind"> (gj&oslash;re til en
    v&aelig;ske)</klr></trd>
    <trdgrp lng="pl">
      <trd>topi&cacute;</trd>,
      <trd>roztapia&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_t;&c_o;&c_p;&c_i;&c_t;&c_mol;<klr tip="ind">
      (&c_v;&c_o;&c_s;&c_k;, &c_zh;&c_i;&c_r;)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="fand.0i.ekstrakti">
    <dif>
      <ref tip="dif" cel="fand.el0i.ekstrakti">El<tld/>i<sncref/></ref>:
      <ekz>
        el &scirc;tono oni <tld/>as la kupron
        <fnt><bib>MT</bib>, <lok>&Ijo; 28:2</lok></fnt>;
      </ekz>
      <ekz>
        ar&gcirc;ento, purigita en tera <ind><tld/>ujo</ind> kaj
        sepfoje re<tld/>ita
        <fnt><bib>MT</bib>, <lok>&Psa; 12:6</lok></fnt>.
        <trd lng="fr">creuset</trd>
        <trd lng="hu">olvaszt&oacute;t&eacute;gely</trd>
      </ekz>
    </dif>
    <trd lng="pl">wytapia&cacute;</trd>
    <trd lng="ru">&c_v;&c_y;&c_p;&c_l;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>
    <trd lng="zh">smelt</trd>
  </snc>
  <snc mrk="fand.0i.gisi">
    <dif>
      <ref tip="dif" cel="gis.0i.muldi">Gisi<sncref/></ref>:
      <ekz>
        li <tld/>is por &gcirc;i kvar orajn ringojn sur &gcirc;iaj
        kvar anguloj
        <fnt><bib>MT</bib>, <lok>&Eli; 37:3</lok></fnt>;
      </ekz><ekz>
        ili faris al si <tld/>itan bovidon kaj adorklini&gcirc;is al
        &gcirc;i
        <fnt><bib>MT</bib>, <lok>&Eli; 32:8</lok></fnt>;
      </ekz>      
      <ekz>
        formi per argilo la figuron <klr>[...]</klr> por ke &gcirc;i povu esti
        <tld/>ita el metalo
        <fnt>
          <bib>Fab4</bib>
          <lok>Malnova pre&gcirc;eja sonorilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum jarcentoj oni <tld/>is el sonoriloj kanonojn kaj re<tld/>is
        la kanonojn en sonorilojn
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://www.esperanto.be/fel/mon/007297.html">Kolekti
          sonorilojn</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      La&ubreve; UV kaj uzo Zamenhofa, <ekz>
        <tld/>i statuon</ekz>
      povas havi tute kontra&ubreve;ajn signifojn:
      krei (gisi) statuon; a&ubreve; malkrei (likvigi, dis<tld/>i)
      statuon. Tia konfuzo estas malakceptebla en racia lingvo
      kaj &scirc;oka por esperantistoj kies hejma lingvo neniam
      konfuzas tiujn kontra&ubreve;ojn. Tial jam en PV aperis pli
      preciza verbo <em>gisi</em> (prunto el la germana)
      &#8208; cetere, mise difinita.

      Fojfoje &Z; provis eviti tiun &gcirc;enon uzante la deriva&jcirc;on
      el<tld/>i<sncref ref="fand.el0i.gisi"/>; tio tamen prezentas
      alian problemon, &ccirc;ar la&ubreve; la lingva logiko tia formo devas
      havi la signifon <ctl>ekstrakti per <tld/>o</ctl> (kaj tia
      logika uzo estas fakte atestita).

      Menciindas, ke en UV la indikitan konfuzon prezentas la
      tradukoj franca, germana kaj angla; dum la tradukoj rusa kaj
      pola donas nur la &ccirc;efsencon <em>likvigi</em>.
      <aut>Sergio Pokrovskij</aut>
    </rim>
    <trdgrp lng="be">
      <trd>&c_l;&c_ib;&c_c;&c_mol; <klr tip="amb">(&c_m;&c_e;&c_t;&c_a;&c_l;)</klr></trd>,
      <trd>&c_a;&c_d;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol; <klr
      tip="amb">(&c_m;&c_e;&c_t;&c_a;&c_l;)</klr></trd>
    </trdgrp>
    <trd lng="de">gie&szlig;en</trd>
    <trd lng="hu">&ouml;nt</trd>
    <trd lng="nl">gieten<klr tip="ind"> (v.metaal)</klr></trd>
    <trdgrp lng="pl">
      <trd>wytapia&cacute;</trd>,
      <trd>odlewa&cacute;</trd>,
      <trd>la&cacute;<klr> (metal)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_l;&c_i;&c_t;&c_mol;<klr tip="ind">
      (&c_f;&c_o;&c_r;&c_m;&c_o;&c_v;&c_a;&c_t;&c_mol;)</klr></trd>,
      <trd>&c_o;&c_t;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;<klr tip="ind">
      (&c_f;&c_o;&c_r;&c_m;&c_o;&c_v;&c_a;&c_t;&c_mol;)</klr></trd>
    </trdgrp>
    <trd lng="zh">cast</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>rozpustit</trd>,
    <trd>roztavit</trd>,
    <trd>tavit <klr>(kov)</klr></trd>,
    <trd>&scaron;kva&rcaron;it kov ,, sl&eacute;vat kov</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>fundir<klr tip="ind"> (dos metales, un objeto,
        etc.)</klr></trd>,
    <trd>colicuar</trd>,
    <trd>colar<klr tip="ind"> (dos m&eacute;tales, un objeto,
        etc.)</klr></trd>,
    <trd>derretir</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fondre<klr tip="ind"> (du m&eacute;tal, un objet,
        etc.)</klr></trd>,
    <trd>liqu&eacute;fier</trd>,
    <trd>couler<klr tip="ind"> (du m&eacute;tal, un objet,
        etc.)</klr></trd>,
    <trd>mouler<klr tip="ind"> (couler)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>derreter</trd>,
    <trd>fundir</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rozpusti&tcaron;</trd>,
    <trd>tavi&tcaron; <klr>(kov)</klr></trd>,
    <trd>&scaron;kvari&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_l;&c_a;&c_v;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_t;&c_o;&c_p;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_t;&c_o;&c_p;&c_i;&c_t;&c_i; (&c_m;&c_je;&c_t;&c_a;&c_l;</trd>,
    <trd>&c_v;&c_ib;&c_s;&c_k;</trd>,
    <trd>&c_s;&c_n;&c_ib;&c_g;</trd>,
    <trd>&c_m;&c_a;&c_s;&c_l;&c_o;)</trd>,
    <trd>&c_l;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_l;&c_i;&c_v;&c_a;&c_t;&c_i; (&c_m;&c_je;&c_t;&c_a;&c_l;)</trd>,
    <trd>&c_v;&c_a;&c_r;&c_i;&c_t;&c_i; (&c_s;&c_k;&c_l;&c_o;)</trd>
  </trdgrp>
</drv>

<drv mrk="fand.0ado">
  <kap><tld/>ado, <var><kap><tld/>o</kap></var></kap>
  <uzo tip="fak">TEK</uzo>
  <snc mrk="fand.0ado.likvigo">
    <dif>
      La ago <tld/>i<sncref ref="fand.0i.likvigi"/> (likvigi):
      <ekz>
        la <tld/>o de plumbo ne postulas multe da varmo
        <fnt><bib>PIV1</bib>, <lok>art. fand</lok></fnt>;
      </ekz>      
      <ekz>
        la procezo de formado de vitroj ne liberigas varmon de <tld/>ado
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=8171">Kristalo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Schmelze</trd>
    <trdgrp lng="pl">
      <trd>roztapianie</trd>,
      <trd>wytop</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_t;&c_a;&c_p;&c_l;&c_i;&c_v;&c_a;&c_n;&c_i;&c_je;
      <klr tip="ind">(&c_l;&c_mol;&c_d;&c_a;,
      &c_zh;&c_i;&c_r;&c_a;)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="fand.0ado.ekstraktado">
    <dif>
      La ago <tld/>i<sncref ref="fand.0i.ekstrakti"/>,
      el<tld/>i<sncref ref="fand.el0i.ekstrakti"/>:      
      <ekz>
        oni minis precipe feron kaj oni okupi&gcirc;is anka&ubreve; pri <tld/>ado
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=235614">Gelnica</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Verh&uuml;ttung <klr>(von Metall)</klr></trd>
    <trdgrp lng="pl">
      <trd>wytop</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_y;&c_p;&c_l;&c_a;&c_v;&c_k;&c_a;</trd>,
      <trd>&c_v;&c_y;&c_t;&c_a;&c_p;&c_l;&c_i;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc mrk="fand.0ado.gisado">
    <dif>
      <ref tip="dif" cel="gis.0ado.mulda">Gisado<sncref/></ref>:      
      <ekz>
        gisado estas <tld/>ado de objektoj el metalo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=24279">Gisado</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de"><klr>(Metall-)</klr>guss</trd>
    <trdgrp lng="pl">
      <trd>odlew</trd>,
      <trd>lanie</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_t;&c_l;&c_i;&c_v;&c_k;&c_a;</trd>,
      <trd>&c_l;&c_i;&c_t;&c_mol;&c_jo;</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">tavba</trd>
  <trdgrp lng="en">
    <trd>melting</trd>,
    <trd>casting</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>fundici&oacute;n</trd>,
    <trd>derretimiento</trd>
  </trdgrp>
  <trd lng="sk">tavba</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_l;&c_a;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_l;&c_i;&c_t;&c_t;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="fand.0ejo">
  <kap>
    <tld/>ejo
</kap>
  <snc mrk="fand.0ejo.loko">
    <dif>
      Loko kie oni aran&gcirc;is <ref tip="prt" cel="forn.fand0o">fandfornon</ref>, 
      ilaron ktp. por <tld/>ado: 
      <ekz>
        ili vizitis <klr>[&#8230;]</klr> <tld/>ejojn, kie oni <tld/>is bronzon
        por vazoj kaj
        armiloj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz> 
      <ekz>
        vitro<tld/>ejo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni enver&scirc;is la ruba&jcirc;ojn en la <tld/>ejon
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2008/009431.php">Fero-infero</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">H&uuml;tte <klr>(Metallurgie)</klr></trd>
  <trd lng="en">foundry</trd>
  <trd lng="es">fundici&oacute;n</trd>
  <trd lng="fr">fonderie</trd>
  <trdgrp lng="uk">
    <trd>&c_l;&c_i;&c_v;&c_a;&c_r;&c_n;&c_ja;</trd>,
    <trd>&c_l;&c_i;&c_v;&c_a;&c_r;&c_n;&c_i;&c_j; &c_c;&c_je;&c_h;</trd>
  </trdgrp>
</drv>


<drv mrk="fand.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="fand.0igxi.per_varmo">
    <dif>
      Flueci&gcirc;i per varmo:
      <ekz>
        vakso <tld/>i&gcirc;as de fajro
        <fnt><bib>MT</bib>, <lok>&Psa; 68:2</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i&gcirc;i kiel vakso
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk>La Stranga Butiko</vrk>
          <lok>Epigramo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ne&gcirc;o <tld/>i&gcirc;is
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de la jaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la baza magmo konsistas &gcirc;is kvindek procentoj el silika acido
        kaj <tld/>i&gcirc;as je 1000 Celsius-gradoj
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="http://www.esperanto.be/fel/mon/007234.html">Vulkanoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="degel.0i">degeli</ref>,
      <ref cel="solv">solvi&gcirc;i</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_p;&c_l;&c_a;&c_v;&c_ib;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">schmelzen <klr tip="ind">(fl&uuml;ssig
    werden)</klr></trd>
    <trd lng="hu">megolvad</trd>
    <trdgrp lng="nl">
      <trd>smelten<klr tip="ind"> (vloeibaar worden)</klr></trd>,
      <trd>vloeibaar worden</trd>
    </trdgrp>
    <trd lng="no">smelte<klr tip="ind"> (bli
    v&aelig;ske)</klr></trd>
    <trdgrp lng="pl">
      <trd>roztapia&cacute; si&eogonek;</trd>,
      <trd>topi&cacute; si&eogonek;</trd>,
      <trd>taja&cacute;<klr> (o &sacute;niegu)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_a;&c_v;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_t;&c_a;&c_ja;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="fand.0igxi.anime">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kormoli&gcirc;i, morale malforti&gcirc;i:
      <ekz>
        <tld/>i&gcirc;i pro <klr>(a&ubreve; de)</klr> kompato;
      </ekz><ekz>
        &scirc;ia animo <tld/>i&gcirc;is pro plezurego;
      </ekz><ekz>
        &scirc;ia fiereco <klr>[&#8230;]</klr> <tld/>i&gcirc;is en la
        <klr>[&#8230;]</klr> humili&gcirc;oj
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_s;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="hu">megenyh&uuml;l</trd>
    <trd lng="nl">smelten</trd>
    <trd lng="no">smelte<klr tip="ind"> (emosjonelt)</klr></trd>
    <trdgrp lng="pl">
      <trd>topnie&cacute;</trd>,
      <trd>taja&cacute;</trd>
    </trdgrp>
    <trd lng="ru">&c_t;&c_a;&c_ja;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="fand.0igxi.malaperi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malaperi kunkonfuzi&gcirc;ante:
      <ekz>
        &ccirc;io <tld/>i&gcirc;is en pala krepusko
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 24a</lok></fnt>;
      </ekz><ekz>
        la nuboj <tld/>i&gcirc;as kun la horizonto.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flu">forflui</ref>,
      <ref cel="perd">perdi&gcirc;i</ref>,
      <ref cel="neni">disnenii&gcirc;i</ref>.
    </refgrp>
    <trd lng="be">&c_r;&c_a;&c_s;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="hu">
      <trd>elmos&oacute;dik</trd>,
      <trd>&ouml;sszeolvad</trd>
    </trdgrp>
    <trd lng="nl">versmelten</trd>
    <trdgrp lng="pl">
      <trd>stapia&cacute; si&eogonek;</trd>,
      <trd>roztapia&cacute; si&eogonek;</trd>
    </trdgrp>
    <trd lng="ru">&c_t;&c_a;&c_ja;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="en">
    <trd>melt</trd>,
    <trd>cast</trd>
  </trdgrp>
  <trd lng="es">fundirse</trd>
  <trdgrp lng="fr">
    <trd>fondre<klr tip="ind"> (intr)</klr></trd>,
    <trd>se <ind>fondre</ind></trd>,
    <trd>mollir<klr tip="ind"> (fig., s'attendrir)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_a;&c_n;&c_u;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_v;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_t;&c_o;&c_p;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
