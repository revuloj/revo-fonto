<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: konvent.xml,v 1.2 2021/01/28 19:10:32 revo Exp $">
<kap>
  <rad>konvent</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="konvent.0o">
  <kap><tld/>o</kap>
  <snc mrk="konvent.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Decida, le&gcirc;ofara
      <ref tip="super" cel="ven.kun0o">kunveno</ref>
      de &ccirc;iuj anoj de asocio:
       <ekz>
        en majo 1938 Lidia revenis al Ilinojso je la invito
        de la Nacia Spirita Asembleo por &ccirc;eesti la &ccirc;iujaran
        bahaan <tld/>on
        <fnt>
          <aut>W. Heller, trad. B. Westerhoff</aut>
          <vrk>Lidia, la Vivo de Lidia Zamenhof, Filino de Esperanto</vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="asembl1.0o">asembleo</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Hauptversammlung</trd>,
      <trd>Konvent</trd>
    </trdgrp>    
    <trd lng="en">convent</trd>
    <trdgrp lng="fr">
      <trd>assembl&eacute;e g&eacute;n&eacute;rale</trd>
      <trd>convent</trd>
    </trdgrp>    
  </snc>
</drv>
</art>

