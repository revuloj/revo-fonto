<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: auxked.xml,v 1.10 2019/02/26 15:10:20 revo Exp $">
<kap>
  <rad>a&ubreve;ked</rad>/oj
</kap>
<drv mrk="auxked.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="hxarad.0oformaj">&hcirc;aradrioformaj
      birdoj</ref> (<trd lng="la">Alcidae</trd>):
      <ekz>
        la <tld lit="A"/>oj <klr>[&#8230;]</klr> aspektas iom kiel pingveno pro iliaj blanka
        kaj nigra koloroj, la rekti&gcirc;a pozicio kaj pro kelkaj el iliaj kutimoj, tamen
        ili tute ne estas parencaj al pingvenoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18812"
              >A&ubreve;kedoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
       <ref cel="auxk.0o">a&ubreve;ko</ref>,
       <ref cel="frater.0o">fraterkulo</ref>
    </refgrp>
  </snc>
  <trd lng="de">Alken<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="en">auks</trd>
  <trd lng="fr">alcid&eacute;s</trd>
  <trd lng="hu">alkaf&eacute;l&eacute;k</trd>
  <trd lng="it">alcidi<klr> (zoo)</klr></trd>
  <trd lng="nl">alken<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="ru">&c_ch;&c_i;&c_s;&c_t;&c_i;&c_k;&c_o;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>

