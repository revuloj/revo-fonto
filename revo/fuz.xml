<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fuz.xml,v 1.14 2022/03/20 13:07:56 revo Exp $">
<kap>
  <rad>fuz</rad>/i
</kap>
<drv mrk="fuz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Longe flagradi sen eksplodo:
      <ekz>
        en la armilaron estis en tiu &ccirc;i periodo enkondukitaj batalaviadiloj <nom>Ilju&scirc;in
        Il-10</nom>, <tld/>-&ccirc;asaviadiloj <nom>Jakovlev Jak-17</nom>, <nom>Jakovlev Jak-23</nom>,
        <nom>MiG-15</nom> kaj <nom>MiG-17</nom> <tld/>-bombaviadilo <nom>Ilju&scirc;in Il-28</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=324089">&Ccirc;e&hcirc;oslovaka armeo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam la atakintoj turni&gcirc;is <klr>[&#x2026;]</klr> ilin renkontis kuna salva pafado de
        <tld/>iloj kaj kanonoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=583003">Batalo &ccirc;e Dobrini&ccirc;i</url></lok>
        </fnt>.
      </ekz> 
    </dif>
    <refgrp tip="vid">
       <ref cel="raket.0o">raketo</ref>,
       <ref cel="bazuk.0o">bazuko</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_r;&c_e;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_g;&c_a;&c_r;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_b;&c_ja;&c_z; &c_v;&c_y;&c_b;&c_u;&c_h;&c_u;)</klr></trd>
  </trdgrp>
  <trd lng="cs">ho&rcaron;et bez v&yacute;buchu</trd>
  <trd lng="de">abbrennen</trd>
  <trd lng="fr">fuser<klr tip="ind"> (en p. de la poudre)</klr></trd>
  <trdgrp lng="hu">
    <trd>&eacute;g<klr tip="ind"> (robban&aacute;s n&eacute;lk&uuml;l)</klr></trd>,
    <trd>izzik</trd>
  </trdgrp>
  <trd lng="nl">branden<klr tip="ind"> (zonder ontploffing)</klr></trd>
  <trd lng="pt">queimar <klr tip="amb">(foguete etc., sem explodir)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_g;&c_o;&c_r;&c_a;&c_t;&c_mol; &c_b;&c_je;&c_z; &c_v;&c_z;&c_r;&c_y;&c_v;&c_a;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_sh;&c_u;&c_m;&c_n;&c_o; &c_v;&c_o;&c_s;&c_p;&c_l;&c_a;&c_m;&c_je;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">horie&tcaron; bez v&yacute;buchu</trd>
  <trdgrp lng="uk">
    <trd>&#x437;&#x433;&#x43E;&#x440;&#x44F;&#x442;&#x438; &#x431;&#x435;&#x437; &#x432;&#x438;&#x431;&#x443;&#x445;&#x443;</trd>,
    <trd>&#x431;&#x435;&#x437;&#x448;&#x443;&#x43C;&#x43D;&#x43E; &#x437;&#x430;&#x43F;&#x430;&#x43B;&#x44E;&#x432;&#x430;&#x442;&#x438;&#x441;&#x44F; (&#x43F;&#x440;&#x43E; &#x43F;&#x43E;&#x440;&#x43E;&#x445;)</trd>
  </trdgrp>
</drv>
</art>
