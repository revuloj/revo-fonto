<art mrk="$Id$">
<kap>
  <rad>fuz</rad>/i
</kap>
<drv mrk="fuz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Longe flagradi sen eksplodo.  
    </dif>
  </snc>
  <trd lng="de">abbrennen</trd>
  <trd lng="fr">fuser<klr tip="ind"> (en p. de la poudre)</klr></trd>
  <trdgrp lng="hu">
    <trd>&eacute;g<klr tip="ind"> (robban&aacute;s n&eacute;lk&uuml;l)</klr></trd>,
    <trd>izzik</trd>
  </trdgrp>
  <trd lng="nl">branden<klr tip="ind"> (zonder ontploffing)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_g;&c_o;&c_r;&c_a;&c_t;&c_mol; &c_b;&c_je;&c_z; &c_v;&c_z;&c_r;&c_y;&c_v;&c_a;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_sh;&c_u;&c_m;&c_n;&c_o; &c_v;&c_o;&c_s;&c_p;&c_l;&c_a;&c_m;&c_je;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
