<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ume.xml,v 1.21 2024/05/10 21:21:40 revo Exp $">
<kap>
    <rad>ume</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="ume.0o">
    <kap><tld/>o</kap>
  <snc mrk="ume.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Frukto de <tld/>arbo, kiun ankora&ubreve; nematuran
      oni peklas por man&gcirc;i a&ubreve; marinas por fari
      brandon a&ubreve; vinagron:
      <ekz>
        peklitaj <tld/>oj
        <fnt>
          <vrk><url ref="http://esperantaretradio.blogspot.com/2013/08/la-japana-rapidmangajo-onigiri.html"
              >La japana rapidman&gcirc;a&jcirc;o Onigiri</url></vrk>
          <lok>Esperanta Retradio 2013-08-04</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj flavi&gcirc;is maturaj kaj falis unu post aliaj teren
        <fnt>
          <aut>Zhang Xiaotong</aut>
          <vrk><url ref="http://www.espero.com.cn/2015-02/27/content_34902810.htm"
              >Faladas umeoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/a/af/Fruits_of_Japanese_plum.jpg/640px-Fruits_of_Japanese_plum.jpg"
      prm="CC BY-SA 3.0"
      >bran&ccirc;o kun <tld/>oj
        <fnt>
          <aut>Sekiuchi, propra verko</aut>
          <lok><url ref="&WCW;/File:Fruits_of_Japanese_plum.jpg"
              >https://commons.wikimedia.org/wiki/File:Fruits_of_Japanese_plum.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="ca">albercoc japon&eacute;s</trd>
    <trd lng="de">Ume-Pflaume</trd>
    <trdgrp lng="en">
      <trd>ume</trd>,
      <trd>Japanese apricot</trd>,
      <trd>Japanese plum</trd>
    </trdgrp>
    <trdgrp lng="fi">
      <trd>ume</trd>,
      <trd>japaninaprikoosi</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>ume-pruim</trd>,
      <trd>Japanse <ind>abrikoos</ind></trd>
    </trdgrp>
    <trd lng="ru">&c_u;&c_m;&c_e;</trd>
    <trd lng="sv">ume-frukt</trd>
    <trd lng="zh">&#26757; <pr>m&eacute;i</pr></trd>
  </snc>
    <snc mrk="ume.0o.arbo">
      <ref tip="dif" cel="ume.0arbo"><tld/>arbo</ref>
      <trd lng="nl">ume-boom</trd>
      <trd lng="ca">albercoquer japon&eacute;s</trd>
    </snc>
  <trdgrp lng="cs">
    <trd>japonsk&aacute; meru&ncaron;ka</trd>,
    <trd>japonsk&aacute; &scaron;vestka</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12454;&#12513;&#12398;&#23455;  <pr>&#12358;&#12417;&#12398;&#12415;</pr></trd>,
    <trd>&#12454;&#12513;</trd>
  </trdgrp>
  <trd lng="pl">morela</trd>
  <trd lng="sk">slivka japonsk&aacute; <klr>(ovocie)</klr></trd>
  <trd lng="uk">&c_ja;&c_p;&c_o;&c_n;&c_s;&c_mol;&c_k;&c_a; &c_s;&c_l;&c_i;&c_v;&c_a;</trd>
</drv>

<drv mrk="ume.0arbo">
    <kap><tld/>arbo</kap>
    <snc mrk="ume.0arbo.BOT">
      <uzo tip="fak">BOT</uzo>
      <dif>
        Specio de <ref tip="super" cel="prunus.0o">prunuso</ref>
        (<trd lng="la">Prunus mume</trd>), &ccirc;in-devena, long-viva,
        kun kurbaj trunko kaj bran&ccirc;oj, frusezone blanke a&ubreve;
        ru&gcirc;ete floranta, ofte kultivata en Orienta Azio:
        <ekz>
          ru&gcirc;aj butonoj / &scirc;velas de <tld/>arbo /
          &ccirc;ielo bluas
          <fnt>
          <aut>Vastalto</aut>,
          <vrk><url ref="http://vastalto.com/hajko/hajko.cgi">Hajko en
          Esperanto</url></vrk>,
          <lok>2006-02-21</lok>
          </fnt>.
        </ekz>
      </dif>
    </snc>
    <trd lng="ca">albercoquer japon&eacute;s</trd>
    <trd lng="cs">strom japonsk&eacute;ho ovoce</trd>
    <trd lng="de">Ume-Baum</trd>
    <trdgrp lng="en">
      <trd>ume</trd>,
      <trd>Japanese apricot</trd>,
      <trd>Japanese plum</trd>
    </trdgrp>
    <trdgrp lng="fi">
      <trd>ume</trd>,
      <trd>ume-puu</trd>
    </trdgrp>
    <trd lng="fr">abricotier japonais</trd>
    <trd lng="hu">k&iacute;nai <ind>kajszi</ind></trd>
    <trd lng="ja">&#12454;&#12513;</trd>
    <trd lng="nl">ume-boom</trd>
    <trd lng="pl">morela japo&nacute;ska</trd>
    <trd lng="ru">&c_u;&c_m;&c_e;</trd>
    <trd lng="sk">slivka japonsk&aacute; <klr>(strom)</klr></trd>
    <trd lng="sv">ume-tr&auml;d</trd>
    <trd lng="uk">&c_ja;&c_p;&c_o;&c_n;&c_s;&c_mol;&c_k;&c_a; &c_s;&c_l;&c_i;&c_v;&c_a; (&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;)</trd>
    <trdgrp lng="zh">
    <trd>&#26757;&#23376; <pr>m&eacute;izi</pr></trd>,
    <trd>&#26757; <pr>m&eacute;i</pr></trd>,
    <trd>&#27089; <pr>m&eacute;i</pr></trd>
  </trdgrp>
</drv>
</art>
