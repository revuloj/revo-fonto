<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ume.xml,v 1.19 2024/02/20 01:21:38 revo Exp $">
<kap>
    <rad>ume</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="ume.0o">
    <kap><tld/>o</kap>
    <snc mrk="ume.0o.KUI">
      <uzo tip="fak">KUI</uzo>
      <dif>
        Frukto de <tld/>arbo, kiun ankora&ubreve; nematuran
        oni peklas por man&gcirc;i a&ubreve; marinas por fari
        brandon a&ubreve; vinagron.
      </dif>
      <trd lng="ca">albercoc japon&eacute;s</trd>
      <trd lng="de">Ume-Pflaume</trd>
      <trdgrp lng="en">
        <trd>ume</trd>,
        <trd>Japanese apricot</trd>,
        <trd>Japanese plum</trd>
      </trdgrp>
      <trdgrp lng="fi">
        <trd>ume</trd>,
        <trd>japaninaprikoosi</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>ume-pruim</trd>,
        <trd>Japanse <ind>abrikoos</ind></trd>
      </trdgrp>
      <trd lng="ru">&c_u;&c_m;&c_e;</trd>
      <trd lng="sv">ume-frukt</trd>
    </snc>
    <snc mrk="ume.0o.arbo">
      <ref tip="dif" cel="ume.0arbo"><tld/>arbo</ref>
      <trd lng="nl">ume-boom</trd>
      <trd lng="ca">albercoquer japon&eacute;s</trd>
    </snc>
  <trdgrp lng="cs">
    <trd>japonsk&#xE1; meru&ncaron;ka</trd>,
    <trd>japonsk&#xE1; &scaron;vestka</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30A6;&#x30E1;&#x306E;&#x5B9F;  <pr>&#12358;&#12417;&#x306E;&#x307F;</pr></trd>,
    <trd>&#x30A6;&#x30E1;</trd>
  </trdgrp>
  <trd lng="pl">morela</trd>
  <trd lng="sk">slivka japonsk&#xE1; <klr>(ovocie)</klr></trd>
  <trd lng="uk">&#x44F;&#x43F;&#x43E;&#x43D;&c_s;&#x44C;&#x43A;&c_a; &c_s;&#x43B;&c_i;&c_v;&c_a;</trd>
</drv>

<drv mrk="ume.0arbo">
    <kap><tld/>arbo</kap>
    <snc mrk="ume.0arbo.BOT">
      <uzo tip="fak">BOT</uzo>
      <dif>
        Specio de <ref tip="super" cel="prunus.0o">prunuso</ref>
        (<trd lng="la">Prunus mume</trd>), &ccirc;in-devena, long-viva,
        kun kurbaj trunko kaj bran&ccirc;oj, frusezone blanke a&ubreve;
        ru&gcirc;ete floranta, ofte kultivata en Orienta Azio:
        <ekz>
          ru&gcirc;aj butonoj / &scirc;velas de <tld/>arbo /
          &ccirc;ielo bluas
          <fnt>
          <aut>Vastalto</aut>,
          <vrk><url ref="http://vastalto.com/hajko/hajko.cgi">Hajko en
          Esperanto</url></vrk>,
          <lok>2006-02-21</lok>
          </fnt>
        </ekz>
      </dif>
    </snc>
  <trd lng="ca">albercoquer japon&eacute;s</trd>
  <trd lng="cs">strom japonsk&#xE9;ho ovoce</trd>
  <trd lng="de">Ume-Baum</trd>
  <trdgrp lng="en">
      <trd>ume</trd>,
      <trd>Japanese apricot</trd>,
      <trd>Japanese plum</trd>
    </trdgrp>
  <trdgrp lng="fi">
      <trd>ume</trd>,
      <trd>ume-puu</trd>
    </trdgrp>
  <trd lng="fr">abricotier japonais</trd>
  <trd lng="hu">k&iacute;nai <ind>kajszi</ind></trd>
  <trd lng="ja">&#x30A6;&#x30E1;</trd>
  <trd lng="nl">ume-boom</trd>
  <trd lng="pl">morela japo&nacute;ska</trd>
  <trd lng="ru">&c_u;&c_m;&c_e;</trd>
  <trd lng="sk">slivka japonsk&#xE1; <klr>(strom)</klr></trd>
  <trd lng="sv">ume-tr&auml;d</trd>
  <trd lng="uk">&#x44F;&#x43F;&#x43E;&#x43D;&c_s;&#x44C;&#x43A;&c_a; &c_s;&#x43B;&c_i;&c_v;&c_a; (&c_d;&c_je;&c_r;&c_je;&c_v;&#x43E;)</trd>
</drv>
</art>
