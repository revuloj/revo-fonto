<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ume</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="ume.0o">
  <kap><tld/>o</kap>
  <snc mrk="ume.0o.frukto">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Frukto de <tld/>arbo, kiun ankora&ubreve; nematuran
      oni peklas por man&gcirc;i a&ubreve; marinas por fari
      brandon a&ubreve; vinagron.
    </dif>
    <trd lng="de">Ume-Pflaume</trd>
    <trd lng="ru">&c_u;&c_m;&c_e;</trd>
  </snc>
  <snc mrk="ume.0o.arbo">
    <ref tip="dif" cel="ume.0arbo"><tld/>arbo</ref>
  </snc>
</drv>

<drv mrk="ume.0arbo">
  <kap><tld/>arbo</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      Specio de <ref tip="super" cel="prunus.0o">prunuso</ref>
      (<trd lng="la">Prunus Mume</trd>), &ccirc;in-devena, long-viva,
      kun kurbaj trunko kaj bran&ccirc;oj,
      frusezone blanke, helru&gcirc;e a&ubreve; purpur-ru&gcirc;e floranta,
      de kiu multaj subspecioj kultivatas en Orienta Azio.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="ume.0o.arbo"><tld/>o</ref>
  </refgrp>
  <trd lng="de">Ume-Baum</trd>
  <trd lng="hu">k&iacute;nai <ind>kajszi</ind></trd>
  <trd lng="ru">&c_u;&c_m;&c_e;</trd>
</drv>
</art>

