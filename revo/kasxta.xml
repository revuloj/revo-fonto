<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>ka&scirc;tan</rad>/o
</kap>
<drv mrk="kasxta.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Man&gcirc;ebla frukto de <tld/>arbo: 
      <ekz>
        <tld/>hara fra&ubreve;lino<fnt>Z</fnt> 
        <klr>(kun <tld/>koloraj haroj)</klr>.
      </ekz> 
    </dif>
    <trdgrp lng="de"><trd>Kastanie</trd>, <trd>Marone</trd></trdgrp>
    <trd lng="en">chestnut</trd>
    <trdgrp lng="fr"><trd>ch&acirc;taigne</trd>, <trd>marron</trd></trdgrp>
  </snc>
  <snc num="2">
    <uzo tip="klr">(populare)</uzo>
    <ref tip="dif" cel="hipoka.0o">hipoka&scirc;tano</ref>
    <trd lng="en">horse <ind>chestnut</ind></trd>
    <trd lng="fr">marron</trd>
  </snc>
</drv>
<drv mrk="kasxta.0arbo0ujo">
  <kap><tld/>arbo, <tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="fag.0acoj">fagacoj</ref>
      (<trd lng="la">Castanea</trd>).  
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>E&szlig;kastanie</trd>,
    <trd>Edelkastanie</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>chestnut</trd>,
    <trd>chestnut tree</trd>
  </trdgrp> 
  <trd lng="fr">ch&acirc;taignier</trd>
</drv>
</art>
