<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kasxta.xml,v 1.51 2023/12/03 08:00:24 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ka&scirc;tan</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kasxta.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kasxta.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;ebla <ref tip="super" cel="frukt.0o.BOT">frukto</ref>
      de <tld/>arbo:
      <ekz>
        apude ku&scirc;is kelke da salatfolioj kaj kelke da <tld/>oj, kaj ili
        servis al li kiel vesperman&gcirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/5/53/Esskastanie-.JPG/640px-Esskastanie-.JPG" prm="CC BY-SA 4.0">frukto de
    <tld/>ujo (Castanea sativa), insulo Thassos, Grekujo
         <fnt>
           <aut>Rosa-Maria Rinkl, propra verko</aut>
           <lok><url ref="&WCW;/File:Esskastanie-.JPG">&WCW;/File:Esskastanie-.JPG</url></lok>
         </fnt>
    </bld>  
    <trd lng="bg">&c_k;&c_je;&c_s;&c_t;&c_je;&c_n;</trd>
    <trd lng="br">kistinenn<klr tip="ind"> (frouezhenn)</klr></trd>
    <trdgrp lng="de">
      <trd>Kastanie</trd>, <trd>Marone</trd>
    </trdgrp>
    <trd lng="en">chestnut</trd>
    <trd lng="es">casta&ntilde;a <klr> (fruto del casta&ntilde;o)</klr></trd>
    <trdgrp lng="fr">
      <trd>ch&acirc;taigne</trd>,
      <trd>marron<klr tip="ind"> (ch&acirc;taigne)</klr></trd>
    </trdgrp>
    <trd lng="hu">gesztenye</trd>
    <trd lng="nl">kastanje</trd>
    <trd lng="ru">&c_k;&c_a;&c_sh;&c_t;&c_a;&c_n;
    <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
    <trd lng="tr">kestane</trd>
  </snc>
  <snc mrk="kasxta.0o.hipokasxtano">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Frukto de <ref tip="malprt" cel="hipoka.0o">hipoka&scirc;tano</ref>:   
      <ekz>
        ni amba&ubreve;, estante ankora&ubreve; infanoj, venis en &ccirc;i tiujn nordajn
        landojn <klr>[&#x2026;]</klr>
        en la rozarbeta&jcirc;oj brilis fajreru&gcirc;aj rozofruktoj, sur la
        sambukaj bran&ccirc;oj pendis grandaj, pezaj,
        nigre-brunaj beroj, la sova&gcirc;aj <tld/>oj maturaj elfalis el la
        malhele-verdaj &scirc;eloj
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de la jaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        foliarboj, kiuj nutris ursojn per abundaj nuksoj kiel glanoj kaj
        <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2014/011198.php">Ursoj
          mortigataj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_k;&c_o;&c_n;&c_s;&c_k;&c_i; <ind>&c_k;&c_je;&c_s;&c_t;&c_je;&c_n;</ind></trd>
    <trd lng="br">kistinenn-Indez<klr tip="ind">
    (frouezhenn)</klr></trd>
    <trd lng="en">horse <ind>chestnut</ind></trd>
    <trd lng="de">Rosskastanie</trd>
    <trd lng="es">falso casta&ntilde;o<klr> (de Indias)</klr></trd>
    <trd lng="fr">marron<klr tip="ind"> (d'Inde)</klr></trd>
    <trdgrp lng="hu">
      <trd>szel&iacute;dgesztenye</trd>,
      <trd>mar&oacute;ni</trd>
    </trdgrp>
    <trd lng="nl">paardekastanje</trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_sh;&c_t;&c_a;&c_n; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd> 
  <trd lng="fa">&a_sh;&a_A;&a_h;&#x200C;&a_b;&a_l;&a_w;&a_T;</trd>
  <trd lng="ja">&#x30AF;&#x30EA;  <pr>&#x304F;&#x308A;</pr></trd>
  <trd lng="pl">kasztan</trd>
  <trd lng="ro">castan&abreve;</trd>
  <trd lng="uk">&#x43A;&c_a;&c_sh;&c_t;&c_a;&#x43D;</trd>
</drv>

<drv mrk="kasxta.0a">
  <kap><tld/>a, <var><kap><tld/>kolora</kap></var></kap>
  <snc mrk="kasxta.0a.KOL">
    <dif>
      De sama, ru&gcirc;e
      <ref tip="vid" cel="brun.0a">bruna</ref>
      koloro, kiel la interna &scirc;elo de <tld/>oj:
      <ekz>
        hel<tld/>aj haroj
        <fnt><aut>C. Piron</aut>, <vrk>&Ccirc;u ni kunvenis 
        vane?</vrk>, <lok>1982</lok></fnt>;
      </ekz>
      <ekz>
        gaja junulino kun <tld/>koloraj haroj, malhelaj okuloj
        <fnt>
          <aut>Julian Modest</aut>
            <vrk><url ref="&Monato;/2001/007140.html">Vetiha</url></vrk>
            <bib>Monato</bib>
            <lok>2001/02, p. 26</lok>
          </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_sh;&c_t;&c_a;&c_n;&c_a;&c_v;&c_y;</trd>
    <trd lng="br">gell-kistin</trd>
    <trd lng="es">casta&ntilde;o<klr> (color)</klr></trd>
    <trd lng="fr">ch&acirc;tain</trd>
    <trd lng="nl">kastanjebruin</trd>
  </snc>
  <trd lng="ja">&#x6817;&#x8272;&#x306E;  <pr>&#x304F;&#x308A;&#x3044;&#x308D;&#x306E;</pr></trd>
  <trd lng="uk">&#x43A;&c_a;&c_sh;&c_t;&c_a;&#x43D;&#x43E;&c_v;&c_i;&c_j; (&#x43F;&c_r;&#x43E; &#x43A;&#x43E;&#x43B;&c_ib;&c_r;)</trd>
</drv>

<drv mrk="kasxta.0arbo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="kasxta.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio 
      <ref tip="malprt" cel="fagac.0oj">fagacoj</ref>
      (<trd lng="la">Castanea</trd>):
      <ekz>
        li sidis sur &scirc;tona benko en la &gcirc;ardeno de <nom>Sara</nom>,
        sub <tld/>arbo, kaj rigardis la &ccirc;irka&ubreve;antan lin pejza&gcirc;on
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malgrandaj birdoj sidis supre en la <tld/>arboj kaj pepadis:
        kvivit, kvivit!
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_sh;&c_t;&c_a;&c_n;
  <klr>(&c_d;&c_r;&c_e;&c_v;&c_a;)</klr></trd>   
  <trd lng="bg">&c_k;&c_je;&c_s;&c_t;&c_je;&c_n;&c_a;</trd>
  <trdgrp lng="br">
    <trd>gwezenn-gistin</trd>,
    <trd>kistinenn<klr tip="ind"> (gwezenn)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Esskastanie</trd>,
    <trd>Edelkastanie</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>chestnut</trd>,
    <trd>chestnut tree</trd>
  </trdgrp>
  <trd lng="es">casta&ntilde;o<klr> (&aacute;rbol)</klr></trd>
  <trd lng="fa">&a_d;&a_r;&a_kh;&a_t; <ind>&a_sh;&a_A;&a_h;&#x200C;&a_b;&a_l;&a_w;&a_T;</ind></trd>
  <trd lng="fr">ch&acirc;taignier</trd>
  <trd lng="hu">gesztenyefa</trd>
  <trd lng="ja">&#x30AF;&#x30EA;  <pr>&#x304F;&#x308A;</pr></trd>
  <trd lng="nl">kastanjeboom</trd>
  <trd lng="pl">kasztan</trd>
  <trd lng="ru">&c_k;&c_a;&c_sh;&c_t;&c_a;&c_n;
  <klr>(&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;)</klr></trd>
  <trd lng="tr">kestane a&gbreve;ac&inodot;</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_a;&c_sh;&c_t;&c_a;&#x43D;</trd>,
    <trd>&#x43A;&c_a;&c_sh;&c_t;&c_a;&#x43D;&#x43E;&c_v;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&#x43E;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6817; <pr>l&#xEC;</pr></trd>,
    <trd>&#x6817;&#x5B50; <pr>l&#xEC;zi</pr></trd>,
    <trd>&#x677F;&#x6817; <pr>b&#x1CE;nl&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kasxta.akvo0o">
  <kap>
    akvo<tld/>o
  </kap>
  <snc>
    <dif>
      La man&gcirc;eblaj tuberoj, kiuj kreskas fine de rizomoj &ccirc;e
      <ref tip="lst" lst="voko:botanikaj_specioj" cel="speci.0o.BIO">specio</ref> 
      el la familio <ref tip="malprt" cel="ciper.0oj">ciperacoj</ref> 
      (<trd lng="la">Eleocharis dulcis</trd>); tiu planto mem:
      <ekz>
        la akvo<tld/>o havas sian nomon pro la simileco de &gcirc;ia man&gcirc;ebla tubero kun la
        <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=556210">Akvoka&scirc;tano</url></lok>
        </fnt>.
      </ekz>
    </dif>
<ref tip="vid" cel="trap.0o.frukto">trapo<sncref/></ref>
    <bld lok="&WCU;/thumb/9/9e/Wasserkastanien.jpg/548px-Wasserkastanien.jpg" prm="PD">akvo<tld/>oj
        <fnt>
          <lok><url ref="&WCW;/File:Wasserkastanien.jpg">&WCW;/File:Wasserkastanien.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="de">Wasserkastanie</trd>
  <trd lng="fr">ch&acirc;taigne d'eau</trd>
  <trd lng="ja">&#x30D2;&#x30B7;&#x306E;&#x5B9F;  <pr>&#x30D2;&#x30B7;&#x306E;&#x307F;</pr></trd>
  <trdgrp lng="zh">
    <trd>&#x8308; <pr>c&#xED;</pr></trd>,
    <trd>&#x83F1;&#x89D2; <pr>l&#xED;ngji&#x1CE;o</pr></trd>,
    <trd>&#x83F1; <pr>l&#xED;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kasxta.mar0o">
  <kap>mar<tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="kasxta.mar0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Man&gcirc;ebla marbesto ekstere simila al <tld/>o: globa, kun
      pikiloj el <ref tip="super" cel="ehxinoderm.0oj.ZOO">e&hcirc;inodermoj</ref>:
      <ekz>
        mar<tld/>a&jcirc;o estas flavru&gcirc;a ovario de
        marka&scirc;tano
        <fnt>
          <aut>Vastalto</aut>,
          <vrk><url ref="http://www.vastalto.com/blog">Akvosfero</url></vrk>,
          <lok>2007-04-21</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/f/fd/ERIZOS_FRESCOS_%28Loxechinus_albus%29.jpg/320px-ERIZOS_FRESCOS_%28Loxechinus_albus%29.jpg" prm="CC BY-SA 4.0">mar<tld/>o, Puerto de San Antonio, &Ccirc;ilio
         <fnt>
           <aut>Tutoxic, propra verko</aut>
           <lok><url ref="&WCW;/File:ERIZOS_FRESCOS_(Loxechinus_albus).jpg">&WCW;/File:ERIZOS_FRESCOS_(Loxechinus_albus).jpg</url></lok>
         </fnt>
    </bld>
    <refgrp tip="sub">
      <ref cel="ehxin.0o">e&hcirc;ino, ekino</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_s;&c_k;&c_ib; &c_k;&c_a;&c_sh;&c_t;&c_a;&c_n;</trd>
  <trd lng="bg">&c_m;&c_o;&c_r;&c_s;&c_k;&c_i; &c_t;&c_a;&c_r;&c_a;&c_l;&c_je;&c_zh;</trd>
  <trd lng="ca">eri&ccedil;&oacute; de mar</trd>
  <trd lng="de">Seeigel</trd>
  <trd lng="en">sea urchin</trd>
  <trd lng="es">erizo de mar</trd>
  <trd lng="fa">&a_t;&a_w;&a_t;&f_y;&a_A;&f_y; &a_d;&a_r;&f_y;&a_A;&f_y;&f_y;</trd>
  <trd lng="fr">oursin</trd>
  <trd lng="hu">tengeri s&uuml;n</trd>
  <trd lng="ja">&#x30A6;&#x30CB;</trd>
  <trd lng="nl">zee-egel</trd>
  <trd lng="pl">je&zdot;owiec</trd>
  <trd lng="pt">equino</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_s;&c_k;&c_o;&c_j;
  &c_k;&c_a;&c_sh;&c_t;&c_a;&c_n;</trd>
  <trd lng="tr">deniz kestanesi</trd>
  <trd lng="uk">&#x43C;&#x43E;&c_r;&c_s;&#x44C;&#x43A;&c_i;&c_j; &c_ji;&c_zh;&c_a;&#x43A;</trd>
  <trdgrp lng="zh">
    <trd>&#x6D77;&#x80C6; <pr>h&#x1CE;id&#x1CE;n</pr></trd>,
    <trd>&#x6D77;&#x80C6;&#x7EB2; <pr>h&#x1CE;id&#x1CE;ng&#x101;ng</pr></trd>
  </trdgrp>
</drv>
</art>
