<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ka&scirc;tan</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kasxta.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kasxta.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;ebla frukto de <tld/>arbo:
      <ekz>
        <ind><tld/>kolora</ind>j haroj;   
        <trd lng="br">gell-kistin</trd>
        <trd lng="fr">ch&acirc;tain</trd>
        <trd lng="nl">kastanjebruin</trd>
      </ekz>
      <ekz>
        <tld/>hara fra&ubreve;lino<fnt>Z</fnt> .
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_sh;&c_t;&c_a;&c_n; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>   
    <trd lng="br">kistinenn<klr tip="ind"> (frouezhenn)</klr></trd>
    <trdgrp lng="de">
      <trd>Kastanie</trd>, <trd>Marone</trd>
    </trdgrp>
    <trd lng="en">chestnut</trd>
    <trdgrp lng="fr">
      <trd>ch&acirc;taigne</trd>,
      <trd>marron<klr tip="ind"> (ch&acirc;taigne)</klr></trd>
    </trdgrp>
    <trd lng="hu">gesztenye</trd>
    <trd lng="nl">kastanje</trd>
    <trd lng="ru">&c_k;&c_a;&c_sh;&c_t;&c_a;&c_n;
    <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  </snc>
  <snc mrk="kasxta.0o.hipokasxtano">
    <uzo tip="stl">KOMUNE</uzo>
    <ref tip="dif" cel="hipoka.0o">hipoka&scirc;tano</ref>   
    <trd lng="br">kistinenn-Indez<klr tip="ind">
    (frouezhenn)</klr></trd>
    <trd lng="en">horse <ind>chestnut</ind></trd>
    <trd lng="de">Rosskastanie</trd>
    <trd lng="fr">marron<klr tip="ind"> (d'Inde)</klr></trd>
    <trdgrp lng="hu">
      <trd>szel&iacute;dgesztenye</trd>,
      <trd>mar&oacute;ni</trd>
    </trdgrp>
    <trd lng="nl">paardekastanje</trd>
  </snc>
  <trd lng="pl">kasztan</trd>
</drv>

<drv mrk="kasxta.0arbo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="kasxta.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="fagac.0oj">fagacoj</ref>
      (<trd lng="la">Castanea</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_sh;&c_t;&c_a;&c_n; <klr>(&c_d;&c_r;&c_e;&c_v;&c_a;)</klr></trd>   
  <trdgrp lng="br">
    <trd>gwezenn-gistin</trd>,
    <trd>kistinenn<klr tip="ind"> (gwezenn)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Esskastanie</trd>,
    <trd>Edelkastanie</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>chestnut</trd>,
    <trd>chestnut tree</trd>
  </trdgrp>
  <trd lng="fr">ch&acirc;taignier</trd>
  <trd lng="hu">gesztenyefa</trd>
  <trd lng="nl">kastanjeboom</trd>
  <trd lng="pl">kasztan</trd>
  <trd lng="ru">&c_k;&c_a;&c_sh;&c_t;&c_a;&c_n;
  <klr>(&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;)</klr></trd>
</drv>

<drv mrk="kasxta.mar0o">
  <kap>mar<tld/>o</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="kasxta.mar0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Man&gcirc;ebla marbesto ekstere simila al <tld/>o: globa, kun pikiloj; 
      genro el <ref tip="super"
      cel="ehxinoderm.0oj.ZOO">e&hcirc;inodermoj</ref>
      (<trd lng="la">Echino</trd>):
      <ekz>
        mar<tld/>a&jcirc;o estas flavru&gcirc;a ovario de marka&scirc;tano
        <fnt>
          <aut>Vastalto</aut>,
          <vrk><url
          ref="http://www.vastalto.com/blog">Akvosfero</url></vrk>,
          <lok>2007-04-21</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">eri&ccedil;&oacute; de mar</trd>
  <trd lng="de">Seeigel</trd>
  <trd lng="en">sea urchin</trd>
  <trd lng="es">erizo de mar</trd>
  <trd lng="fr">oursin</trd>
  <trd lng="hu">tengeri s&uuml;n</trd>
  <trd lng="nl">zee-egel</trd>
  <trd lng="pt">equino</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_s;&c_k;&c_o;&c_j; &c_k;&c_a;&c_sh;&c_t;&c_a;&c_n;</trd>
</drv>
</art>
