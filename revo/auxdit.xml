<art mrk="$Id: auxdit.xml,v 1.22 2023/10/28 08:33:10 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>a&ubreve;ditori</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="auxdit.0o">
  <kap><tld/>o</kap>
  <snc mrk="auxdit.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">PED</uzo>
    <dif>
      <ref tip="super" cel="cxambr.0o">&Ccirc;ambrego</ref> por universitataj a&ubreve; publikaj prelegoj ks;
      <ref tip="sin" cel="preleg.0ejo">prelegejo</ref>:
      <ekz>
        kiam venis <nom>Agripo</nom> kaj <nom>Bernike</nom> kun grandioza parado <klr>[&#x2026;]</klr> ili
        eniris en
        la <tld/>on kun la &ccirc;efkapitanoj kaj la eminentuloj el la urbo
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 25:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        kune kun 300 aliaj homoj mi sidas en granda <tld/>o, a&ubreve;dante lekcion pri algebro
        <fnt>
          <bib>FSI</bib>
        </fnt>;
      </ekz>
      <ekz>
        eminenta <nom>Em&egrave;ric</nom>, &ccirc;u vi komplezos <tld/>i bukedon el miaj kompona&jcirc;oj
        <fnt>
          <bib>MMa</bib>
        </fnt>?
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="auxl.0o.ARKI">a&ubreve;lo</ref>,
      <ref cel="amfite.0o">amfiteatro</ref>,
      <ref cel="koncer1.0ejo">koncertejo</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>H&ouml;rsaal</trd>,
      <trd>Auditorium</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x5927;&#x5B78;&#x6559;&#x5BA4; <pr>d&#xE0;xu&#xE9;ji&#xE0;osh&#xEC;</pr></trd>,
    <trd>&#x5927;&#x6559;&#x5BA4; <pr>d&#xE0;ji&#xE0;osh&#xEC;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_w;&c_d;&c_y;&c_t;&c_o;&c_r;&c_y;&c_ja;</trd>
  <trd lng="ca">auditori</trd>
  <trd lng="es">auditorio</trd>
  <trd lng="en">auditorium</trd>
  <trd lng="fr">auditorium</trd>
  <trd lng="he">&alef;&vav;&#x5BC;&lamed;&#x5B8;&fmem;</trd>
  <trdgrp lng="hu">
    <trd>el&odblac;ad&oacute;terem</trd>,
    <trd>audit&oacute;rium</trd>
  </trdgrp>
   <trdgrp lng="it">
    <trd>auditorium</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>audytorium</trd>,
    <trd>sala</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_u;&c_d;&c_i;&c_t;&c_o;&c_r;&c_i;&c_ja;</trd>
  <trd lng="sk">posluch&aacute;re&ncaron;</trd>
  <trd lng="sv">f&ouml;rel&auml;sningssal</trd>
  <trd lng="uk">&c_a;&c_u;&c_d;&c_i;&c_t;&#x43E;&c_r;&c_ib;&#x44F;</trd>
</drv>
</art>
