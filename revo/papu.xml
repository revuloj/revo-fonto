<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>papu</rad>/o
</kap>

<drv mrk="papu.0o">
  <kap>
    <tld lit="P"/>o
  </kap>
  <snc mrk="papu.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Iama lando en la sudorienta parto de la insulo <ref tip="malprt" 
      cel="gvine.nov0o.GEOG">Nov Gvineo</ref>,
      teritorio dependa de Britujo kaj poste de A&ubreve;stralio.
      Nuntempe parto de <ref tip="malprt"
      cel="papu.0onovgvineo.sxtato">Papuo Nov Gvineo</ref>.
    </dif>
    <trdgrp lng="pl">
      <trd>Papua</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="papu.0onovgvineo">
  <kap>
    <tld lit="P"/>o-Nov-Gvineo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="papu.0onovgvineo.sxtato">
    <uzo tip="fak">POL</uzo>
    <dif>
      &Scirc;tato en la <ref tip="malprt" cel="oceani.O0o">Oceanio</ref>
      (&ccirc;efe en la insulo <ref tip="malprt"
      cel="gvine.nov0o.GEOG">Nov Gvineo</ref>)
      kun &ccirc;efurbo Port-Moresbo.
      ISO-3166 kodo: PG.
      &Gcirc;i limas kun
      <ref tip="vid" cel="indonezi.0o.GEOG">Indonezio</ref>.
    </dif>
    <trdgrp lng="pl">
      <trd>Papua-Nowa Gwinea</trd>,
      <trd>Niezale&zdot;ne Pa&nacute;stwo <ind>Papui-Nowej
      Gwinei</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

