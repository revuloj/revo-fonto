<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>klas</rad>/o
</kap>
<drv mrk="klas.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Divida&jcirc;o de popolo la&ubreve; iuj civilaj a&ubreve; politikaj
      kondi&ccirc;oj:
      <ekz>
        la tri <tld/>oj de l' patricioj, kavaliroj kaj plebanoj en la antikva
        Romo.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Aro da personoj havanta samajn ekonomiajn interesojn, morojn, kutimojn:
      <ekz>
        la reganta, la meza, la laborista <tld/>o;
      </ekz>
      <ekz>
        la <tld/>batalo;
      </ekz>
      <ekz>
        bon<tld/>a<fnt>Z</fnt>,
      </ekz>
      <ekz>
        alt<tld/>a homo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Aro el &ccirc;iuj lernantoj, kiuj sekvas iun gradon de studkurso:
      <ekz>
        la altaj, malaltaj <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <snc mrk="klas.0o.BIO">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      &Scirc;tupo de la botanika kaj zoologia klasifikoj
      (<trd lng="la">classis</trd>).
    </dif>
    <ref tip="malprt" cel="filum.0o">filumo</ref>
    <ref tip="prt" cel="ord.0o.BIO">ordo</ref>
  </snc>
  <snc>
    <dif>
      Aro da personoj a&ubreve; objektoj, kunigitaj la&ubreve; rango, kvalito
      a&ubreve; naturo:
      <ekz>
        tiu libro celas &ccirc;iujn <tld/>ojn da legantoj;
      </ekz>
      <ekz>
        tiuj varoj formas apartan <tld/>on;
      </ekz>
      <ekz>
        unua<tld/>a in&gcirc;eniero, vagono.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="spec.0o">speco</ref>,
      <ref cel="katego.0o">kategorio</ref>,
      <ref cel="grup.0o">grupo</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Kurso 1:
      <ekz>
        fari siajn klasikajn <tld/>ojn<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">t&rcaron;&iacute;da</trd>
  <trd lng="de">Klasse</trd>
  <trd lng="en">class</trd>
</drv>
<drv mrk="klas.en0igi">
  <kap>(en)<tld/>igi</kap>
  <snc>
    <dif>
      Klasifiki.
    </dif>
  </snc>
</drv>
<drv mrk="klas.ekvivalento0o">
  <kap>ekvivalento-<tld/>o, ekvivalent<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      (de elemento x la&ubreve;
      <ref cel="rilat.ekvivalento0o">ekvivalento-rilato</ref> <em>R</em>)
      <ref tip="super" cel="ar.0o.MAT">Aro</ref> de &ccirc;iuj tiaj elementoj y,
      ke x <em>R</em> y.
    </dif>
    <ref tip="malprt" cel="ar.kvocienta0o">kvocienta aro</ref>
  </snc>
  <trd lng="de">&Auml;quivalenzklasse</trd>
  <trd lng="en">equivalence <ind>class</ind></trd>
  <trd lng="fr">classe d'&eacute;quivalence</trd>
  <trd lng="ru">&c_k;&c_l;&c_a;&c_s;&c_s; &c_e;&c_k;&c_v;&c_i;&c_v;&c_a;&c_l;&c_je;&c_n;&c_t;&c_n;&c_o;&c_s;&c_t;&c_i;</trd>
</drv>
</art>
