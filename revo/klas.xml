<drv mrk="klas.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="dif" cel="klasif.0i">Klasifiki</ref>:
      <ekz>
        2000 lingvoj, <tld/>igitaj en 20 lingvistikajn familiojn
        <fnt>
          <bib>Monato</bib>
          <aut>Maritza Guti&eacute;rrez</aut>
          <vrk><url ref="&Monato;/2009/009768.php">Kia
          lingva ekologio por da&ubreve;ripova evoluo?</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        la perioda tabelo <klr>[&#8230;]</klr> de kemio donas ekstreme utilan
        referencon por <tld/>igi, sistemigi kaj kompari diversajn formojn de
        kemiaj ecoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3459">Perioda
          tabelo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="de">klassifizieren</trd>
  </snc>
  <snc mrk="klas.0igi.enklasigi">
    <dif>
      <ref tip="dif" cel="klas.en0igi">Enklasigi</ref>:
      <ekz>
        la korea <klr>...</klr> estas <klr>...</klr> malcerte
        <tld/>igita
        kiel ural-altaja lingvo
        <fnt><aut>B. Kendal</aut>, <vrk>La korea</vrk>,
        <bib>Monato</bib>, <lok>jaro 2002a, numero 3a, p.
        21-23a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">einstufen</trd>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_ja;&c_s;&c_y;&c_f;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">nechat set&rcaron;&iacute;dit</trd>
  <trdgrp lng="fr">
    <trd>classer</trd>,
    <trd>arranger</trd>,
    <trd>cat&eacute;goriser</trd>,
    <trd>classifier</trd>,
    <trd>ordonner</trd>,
    <trd>trier</trd>,
    <trd>ranger</trd>
  </trdgrp>
  <trd lng="hu">oszt&aacute;lyoz</trd>
  <trdgrp lng="id">
    <trd><baz>kelompok</baz>mengelompokkan</trd>,
    <trd><baz>sortir</baz>menyortir</trd>,
    <trd><baz>susun</baz>menyusun</trd>,
    <trd><baz>urut</baz>mengurutkan</trd>
  </trdgrp>
  <trd lng="nl">classificeren</trd>
  <trdgrp lng="pl">
