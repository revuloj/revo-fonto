<drv mrk="klas.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="dif" cel="klas.en0igi">Enklasigi</ref>:
      <ekz>
        se <tld/>i &ccirc;i poemojn la&ubreve; la terminologio de Auld, ili apartenus
        al la primitiva romantismo
        <fnt><aut>C. Minnaja</aut>, <vrk>Meritanta ne nur
        reeldonon</vrk>,
        <bib>Monato</bib>, <lok>jaro 2001a, numero 7a, p.
        28a</lok></fnt>;
      </ekz>
      <ekz>
        kovrildorsa kelklinia komento <klr>[&#8230;]</klr> <tld/>as &scirc;in
        &leftquot;vo&ccirc;eto modesta, minora&rightquot;, kun simpatio
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2003/008021.html">Suno kaj
          ombroj</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj objektoj, kiujn oni anta&ubreve;e <tld/>is kiel spiralajn
        nebulozojn, estas fakte vastaj aroj da steloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=7751">Galaksio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_ja;&c_s;&c_y;&c_f;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;ebrat</trd>,
    <trd>set&rcaron;&iacute;dit</trd>,
    <trd>t&rcaron;&iacute;dit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>einstufen</trd>,
    <trd>einer Klasse <ind>zuordnen</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>classer</trd>,
    <trd>arranger</trd>,
    <trd>cat&eacute;goriser</trd>,
    <trd>classifier</trd>,
    <trd>ordonner</trd>,
    <trd>trier</trd>,
    <trd>ranger</trd>
  </trdgrp>
  <trd lng="hu">oszt&aacute;lyoz</trd>
  <trdgrp lng="id">
    <trd><baz>kelompok</baz>mengelompokkan</trd>,
    <trd><baz>sortir</baz>menyortir</trd>,
    <trd><baz>susun</baz>menyusun</trd>,
    <trd><baz>urut</baz>mengurutkan</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7B49;&#x7D1A;&#12434;&#12388;&#12369;&#x308B;  <pr>&#12392;&#12358;&#x304D;&#12421;&#12358;&#12434;&#12388;&#12369;&#x308B;</pr></trd>,
    <trd>&#20998;&#x985E;&#12377;&#x308B;  <pr>&#12406;&#12435;&#x308B;&#12356;&#12377;&#x308B;</pr></trd>,
    <trd>&#x4ED5;&#20998;&#12369;&#x308B;  <pr>&#12375;&#x308F;&#12369;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">classificeren</trd>
