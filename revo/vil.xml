<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vil.xml,v 1.24 2023/10/21 16:50:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vil</rad>/a
</kap>
<drv mrk="vil.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="vil.0a.KOMUNE">
    <dif>
      Kovrita per densaj mallongaj, eble iom malordaj, <ref cel="har.0o">haro</ref>j:
      <ekz>
        gaje balancante la <tld/>an kapeton, la infano babiladis
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        prezentu al vi la nigre-brunan, sulkoplenan, <tld/>an virina&ccirc;on dancanta anta&ubreve;
        mi kaj &jcirc;urpetanta min per sia virgulina &ccirc;asteco...
        <fnt>
          <bib>Rabistoj</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> indi&gcirc;enoj <klr>[&#x2026;]</klr> en siaj <tld/>aj peltaj jakoj prezentis
        strangan vida&jcirc;on
        <fnt>
          <bib>Fab3</bib>
          <lok>&Ccirc;e la plej ekstrema maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la hundo, granda kaj <tld/>a <klr>[&#x2026;]</klr> staras anta&ubreve; mi
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio</lok>
        </fnt>;
      </ekz>
      <ekz>
        la uro <klr>[&#x2026;]</klr> kaj lia malpala, <tld/>a korpo tiel kuntiri&gcirc;is, ke li
        &scirc;ajnis simila al grandega globo
        <fnt>
          <bib>IKr</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frangx.0o">fran&gcirc;o</ref>,
      <ref cel="fel.0o">felo</ref>,
      <ref cel="hirt.0a.konfuze">hirta<sncref/></ref>,
      <ref cel="tauxz.0i.malordige">ta&ubreve;zi<sncref/></ref>,
      <ref cel="tuf.0o">tufo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_s;&c_m;&c_a;&c_t;&c_y;</trd>,
    <trd>&c_k;&c_a;&c_sh;&c_l;&c_a;&c_t;&c_y;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_m;&c_a;&c_t;&c_y;</trd>,
    <trd>&c_k;&c_u;&c_d;&c_l;&c_a;&c_v;&c_a;&c_t;&c_y;</trd>,
    <trd>&c_k;&c_u;&c_d;&c_l;&c_a;&c_t;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">vil&oacute;zn&iacute;</trd>
  <trdgrp lng="de">
    <trd>zottig</trd>,
    <trd>struppig</trd>,
    <trd>kraus</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>villous</trd>,
    <trd>shaggy</trd>,
    <trd>hairy</trd>,
    <trd>hirsute</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>peludo</trd>,
    <trd>peluda</trd>,
    <trd>velloso</trd>,
    <trd>velludo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>velu</trd>,
    <trd>villeux</trd>
  </trdgrp>
  <trd lng="hu">sz&odblac;r&odblac;s</trd>
  <trdgrp lng="ja">
    <trd>&#x6BDB;&#x3067;&#x8986;&#x308F;&#x308C;&#x305F;  <pr>&#x3051;&#x3067;&#x304A;&#x304A;&#x308F;&#x308C;&#x305F;</pr></trd>,
    <trd>&#x6BDB;&#x3080;&#x304F;&#x3058;&#x3083;&#x3089;&#x306E;  <pr>&#x3051;&#x3080;&#x304F;&#x3058;&#x3083;&#x3089;&#x306E;</pr></trd>,
    <trd>&#x3051;&#x3070;&#x7ACB;&#x3063;&#x305F;  <pr>&#x3051;&#x3070;&#x305F;&#x3063;&#x305F;</pr></trd>
  </trdgrp>
  <trd lng="nl">ruig</trd>
  <trdgrp lng="pl">
    <trd>kosmaty</trd>,
    <trd>kud&lstroke;aty</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_h;&c_n;&c_a;&c_t;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_o;&c_r;&c_s;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chlpat&yacute;</trd>,
    <trd>hu&ncaron;at&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_k;&c_o;&c_sh;&c_l;&c_a;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_k;&c_o;&c_s;&c_m;&c_a;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_k;&c_u;&c_d;&c_l;&c_a;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_a;&c_t;&c_l;&c_a;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_je;&c_l;&c_je;&c_h;&c_a;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_o;&c_l;&c_o;&c_h;&c_a;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_k;&c_r;&c_i;&c_t;&c_i;&c_j; &c_p;&c_u;&c_sh;&c_k;&c_o;&c_m;</trd>
  </trdgrp>
</drv>

<drv mrk="vil.0o">
  <kap><tld/>o</kap>
  <snc mrk="vil.0o.KOMUNE">
    <dif>
      Ta&ubreve;za&jcirc;o, lobeto, <ref cel="frangx.0o">fran&gcirc;o</ref> eli&gcirc;anta de io mola:
      <ekz>
        la vento furioze &scirc;iris <tld/>ojn de la nigra fumo
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        multkoloraj nubetoj kovris kvaza&ubreve; per siaj <tld/>oj la hele bluan &ccirc;ielon
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">ANA</uzo>
        la <tld/>oj de la <ref tip="malprt" cel="intest.0o">intesto</ref>, 
        de la <ref tip="malprt" cel="placen.0o">placento</ref>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_s;&c_m;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_s;&c_m;&c_y;&c_k;</trd>,
    <trd>&c_k;&c_o;&c_s;&c_m;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>anat. chlup</trd>,
    <trd>chlupy</trd>,
    <trd>chm&yacute;&rcaron;&iacute;</trd>,
    <trd>chum&aacute;&ccaron;</trd>,
    <trd>podsada srsti</trd>,
    <trd>srst</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Zottel</trd>,
    <trd>Zotte</trd>,
    <trd>Villus</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>villus</trd>,
    <trd>tuft of hair</trd>
  </trdgrp>
  <trd lng="es">vellosidad</trd>
  <trd lng="fr">villosit&eacute;</trd>
  <trdgrp lng="hu">
    <trd>serte</trd>,
    <trd>s&ouml;rte</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3051;&#x3070;</trd>,
    <trd>&#x77ED;&#x6BDB;  <pr>&#x305F;&#x3093;&#x3082;&#x3046;</pr></trd>,
    <trd>&#x3080;&#x304F;&#x6BDB;  <pr>&#x3080;&#x304F;&#x3052;</pr></trd>,
    <trd>&#x30D1;&#x30A4;&#x30EB;</trd>,
    <trd>&#x7D68;&#x6BDB;  <pr>&#x3058;&#x3085;&#x3046;&#x3082;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">haarbosje</trd>
  <trdgrp lng="pl">
    <trd>kosmyk</trd>,
    <trd>k&lstroke;ak</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_r;&c_s;</trd>,
    <trd>&c_k;&c_l;&c_o;&c_k;<klr tip="amb">
    (&c_sh;&c_je;&c_r;&c_s;&c_t;&c_i;,
    &c_v;&c_o;&c_l;&c_o;&c_s;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chlp</trd>,
    <trd>chum&aacute;&ccaron;</trd>,
    <trd>klk</trd>,
    <trd>srs&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_a;&c_t;&c_l;&c_a;</trd>,
    <trd>&c_k;&c_u;&c_d;&c_l;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_s;&c_m;&c_a;&c_k;</trd>,
    <trd>&c_p;&c_je;&c_l;&c_je;&c_h; (&c_v;&c_o;&c_v;&c_n;&c_i;</trd>,
    <trd>&c_v;&c_o;&c_l;&c_o;&c_s;&c_s;&c_ja;)</trd>,
    <trd>&c_p;&c_u;&c_sh;&c_o;&c_k; (&c_n;&c_a; &c_f;&c_r;&c_u;&c_k;&c_t;&c_a;&c_h; &c_ib; &c_t;.&c_p;.)</trd>,
    <trd>&c_v;&c_o;&c_r;&c_s;&c_i;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
