<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vil</rad>/a
</kap>
<drv mrk="vil.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Kovrita per densaj mallongaj haroj:
      <ekz>
        <tld/>a hundo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">zottig</trd>
  <trd lng="nl">ruig</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_h;&c_n;&c_a;&c_t;&c_y;&c_j;</trd>,
    <trd>&c_l;&c_o;&c_h;&c_m;&c_a;&c_t;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="vil.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Tufo da haroj &ccirc;e supra&jcirc;o de kelkaj membranoj:
      <ekz>
        la <tld/>oj de la stomako.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">haarbosje</trd>
</drv>
</art>
