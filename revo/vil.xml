<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vil.xml,v 1.18 2016/09/26 06:10:25 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vil</rad>/a
</kap>
<drv mrk="vil.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Kovrita per densaj mallongaj haroj:
      <ekz>
        <tld/>a hundo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_s;&c_m;&c_a;&c_t;&c_y;</trd>,
    <trd>&c_k;&c_a;&c_sh;&c_l;&c_a;&c_t;&c_y;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_m;&c_a;&c_t;&c_y;</trd>,
    <trd>&c_k;&c_u;&c_d;&c_l;&c_a;&c_v;&c_a;&c_t;&c_y;</trd>,
    <trd>&c_k;&c_u;&c_d;&c_l;&c_a;&c_t;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">vil&oacute;zn&iacute;</trd>
  <trd lng="de">zottig</trd>
  <trdgrp lng="en">
    <trd>villous</trd>,
    <trd>shaggy</trd>,
    <trd>hairy</trd>,
    <trd>hirsute</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>peludo</trd>,
    <trd>peluda</trd>,
    <trd>velloso</trd>,
    <trd>velludo</trd>
  </trdgrp>
  <trd lng="fr">villeux</trd>
  <trd lng="hu">sz&odblac;r&odblac;s</trd>
  <trd lng="nl">ruig</trd>
  <trdgrp lng="pl">
    <trd>kosmaty</trd>,
    <trd>kud&lstroke;aty</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_h;&c_n;&c_a;&c_t;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_o;&c_r;&c_s;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chlpat&yacute;</trd>,
    <trd>hu&ncaron;at&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="vil.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Tufo da haroj &ccirc;e supra&jcirc;o de kelkaj membranoj:
      <ekz>
        la <tld/>oj de la stomako.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_s;&c_m;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_s;&c_m;&c_y;&c_k;</trd>,
    <trd>&c_k;&c_o;&c_s;&c_m;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>anat. chlup</trd>,
    <trd>chlupy</trd>,
    <trd>chm&yacute;&rcaron;&iacute;</trd>,
    <trd>chum&aacute;&ccaron;</trd>,
    <trd>podsada srsti</trd>,
    <trd>srst</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>villus</trd>,
    <trd>tuft of hair</trd>
  </trdgrp>
  <trd lng="es">vellosidad</trd>
  <trd lng="fr">villosit&eacute;</trd>
  <trdgrp lng="hu">
    <trd>serte</trd>,
    <trd>s&ouml;rte</trd>
  </trdgrp>
  <trd lng="nl">haarbosje</trd>
  <trdgrp lng="pl">
    <trd>kosmyk</trd>,
    <trd>k&lstroke;ak</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_r;&c_s;</trd>,
    <trd>&c_k;&c_l;&c_o;&c_k;<klr tip="amb">
    (&c_sh;&c_je;&c_r;&c_s;&c_t;&c_i;,
    &c_v;&c_o;&c_l;&c_o;&c_s;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chlp</trd>,
    <trd>chum&aacute;&ccaron;</trd>,
    <trd>klk</trd>,
    <trd>srs&tcaron;</trd>
  </trdgrp>
</drv>
</art>
