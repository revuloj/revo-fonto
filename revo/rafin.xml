<vortaro>
<art mrk="$Id: rafin.xml,v 1.33 2025/07/15 06:00:21 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>rafin</rad>/i <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="rafin.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rafin.0i.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emie plipurigi substancon per iaj procedoj:
      <ekz>
	<tld/>i sukeron, petrolon, oleon;
      </ekz>
      <ekz>
        latuno <klr>[&#8230;]</klr> <tld/>ita en forno
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 1:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        pura kansukero duoble <tld/>ita
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne<tld/>ita sukero estis bruna
        <fnt>
          <bib>BonaLingvo</bib>
        </fnt>;
      </ekz>
      <ekz>
        benzino kaj dizela karbura&jcirc;o <tld/>ita el nafto
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2007/009177.php">Bone nutrataj a&ubreve;toj</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        parto de la heterogena&jcirc;o (miksa&jcirc;o de kupro kaj kobalto) estas <tld/>ata &ccirc;eloke
        de malgrandaj kompanioj
        <fnt>
          <bib>MD05</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_f;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_ch;&c_y;&c_sh;&c_ch;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="ca">refinar</trd>
    <trd lng="de">raffinieren</trd>
    <trd lng="hu">finom&iacute;t</trd>
    <trd lng="nl">raffineren</trd>
    <trd lng="pt">refinar</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_f;&c_i;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_ch;&c_i;&c_shch;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="rafin.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plidelikatigi, plisubtiligi:
      <ekz>
	<tld/>i sian stilon;
      </ekz>
      <ekz>
        je la vido de la lumoj, de hederornamitaj kru&ccirc;oj, de vinoj, glacitenataj en ne&gcirc;a
        lita&jcirc;o kaj de <tld/>itaj man&gcirc;oj, gaji&gcirc;is la koroj de la festenantoj
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la eleganta kremkolora robo ornamita de diskreta, sed <tld/>ita, bro&ccirc;o foliforma
        kunteksis &ccirc;arman silueton
        <fnt>
          <bib>CKv</bib>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;ambron anka&ubreve; nun obskurigis la vualo de bonaj cigaroj kaj <tld/>itaj lumoj
        <fnt>
          <bib>KKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiaj dialogoj estas la plej bona maniero por <tld/>i malnovajn konceptojn kaj e&ccirc;
        malkovri novajn
        <fnt>
          <bib>Ko11</bib>
          <lok>Blogo de diskutanto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="polur.0i">poluri</ref>
    </refgrp>
    <trd lng="be">&c_r;&c_a;&c_f;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="ca">refinar</trd>
    <trd lng="de">verfeinern</trd>
    <trdgrp lng="hu">
      <trd>finom&iacute;t</trd>,
      <trd>kifinom&iacute;t</trd>
    </trdgrp>
    <trd lng="nl">verfijnen</trd>
    <trd lng="ru">&c_r;&c_a;&c_f;&c_i;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="bg">&c_r;&c_a;&c_f;&c_i;&c_n;&c_i;&c_r;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>b&yacute;t mazan&yacute;</trd>,
    <trd>b&yacute;t rafinovan&yacute;</trd>,
    <trd>chytra&ccaron;it</trd>
  </trdgrp>
  <trd lng="en">refine</trd>
  <trd lng="es">refinar</trd>
  <trd lng="fr">raffiner</trd>
  <trdgrp lng="ja">
    <trd>&#31934;&#35069;&#12377;&#12427;  <pr>&#12379;&#12356;&#12379;&#12356;&#12377;&#12427;</pr></trd>,
    <trd>&#27927;&#32244;&#12377;&#12427;  <pr>&#12379;&#12435;&#12428;&#12435;&#12377;&#12427;</pr></trd>,
    <trd>&#32048;&#12363;&#12367;&#12377;&#12427;  <pr>&#12371;&#12414;&#12363;&#12367;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>czy&sacute;ci&cacute;  <klr>(chem.)</klr></trd>,
    <trd>rafinowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>by&tcaron; rafinovan&yacute;</trd>,
    <trd>chytr&aacute;&ccaron;i&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_a;&c_f;&c_ib;&c_n;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_ch;&c_i;&c_shch;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_b;&c_i;&c_t;&c_i; &c_b;&c_ib;&c_l;&c_mol;&c_sh; &c_v;&c_i;&c_t;&c_o;&c_n;&c_ch;&c_je;&c_n;&c_i;&c_m;</trd>,
    <trd>&c_v;&c_i;&c_sh;&c_u;&c_k;&c_a;&c_n;&c_i;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#28860; <pr>li&agrave;n</pr></trd>,
    <trd>&#37706; <pr>li&agrave;n</pr></trd>,
    <trd>&#28860;&#21046; <pr>li&agrave;nzh&igrave;</pr></trd>,
    <trd>&#29001;&#21046; <pr>li&agrave;nzh&igrave;</pr></trd>,
    <trd>&#28860;&#21270; <pr>li&agrave;nhu&agrave;</pr></trd>,
    <trd>&#29001;&#21270; <pr>li&agrave;nhu&agrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rafin.0ita">
  <kap><tld/>ita</kap>
  <snc mrk="rafin.0ita.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="krud.mal0a">Malkruda</ref>, tre
      <ref cel="delika.0a">delikata</ref>,
      pro granda prilaboriteco, lerte
      <ref tip="super" cel="inteli.0a">inteligenta</ref>:
      <ekz>
        <tld/>ita, politika kapo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ita patricio, kune <ref cel="subtil.0a.sente">subtila<sncref/></ref>
        kaj <ref cel="grandi.0a">grandioza</ref>
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        tre <tld/>itaj iroj sur la &scirc;aka tabulo
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        tio <klr>[&#8230;]</klr> similis al interesa ludo, pli <tld/>ita ol la ordinara
        hazardludo
        <fnt>
          <bib>HsT</bib>
        </fnt>;
      </ekz>
      <ekz>
        la dis&scirc;iro inter du eblaj vivoj &ndash; lukso, <tld/>ita konversacio kaj samseksema
        plezuro unuflanke, &#8208; politika lukto, virino ne tre klera, laborista vivo
        aliflanke
        <fnt>
          <bib>ChC</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="artifi.0a">artifika</ref>,
      <ref cel="sagac.0a">sagaca</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="lert.0a.menso">lerta<sncref/></ref>,
      <ref cel="ruz.0a">ruza</ref>,
      <ref cel="spert.0a">sperta</ref>
    </refgrp>
  <trdgrp lng="de">
    <trd>ausgekl&uuml;gelt</trd>,
    <trd>raffiniert</trd>
  </trdgrp></snc>
  <trd lng="be">&c_r;&c_a;&c_f;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
  <trd lng="bg">&c_r;&c_a;&c_f;&c_i;&c_n;&c_i;&c_r;&c_a;&c_n;</trd>
  <trd lng="ca">refinat</trd>
  <trdgrp lng="cs">
    <trd>chytr&yacute;</trd>,
    <trd>lstiv&yacute;</trd>,
    <trd>prohnan&yacute;</trd>,
    <trd>rafinovan&yacute;</trd>,
    <trd>v&scaron;emi mastmi mazan&yacute;</trd>,
    <trd>zchytral&yacute;</trd>
  </trdgrp>
  <trd lng="en">refined</trd>
  <trd lng="es">refinado</trd>
  <trd lng="fr">raffin&eacute;</trd>
  <trd lng="hu">kifinomult</trd>
  <trdgrp lng="ja">
    <trd>&#31934;&#35069;&#12373;&#12428;&#12383;  <pr>&#12379;&#12356;&#12379;&#12356;&#12373;&#12428;&#12383;</pr></trd>,
    <trd>&#27927;&#32244;&#12373;&#12428;&#12383;  <pr>&#12379;&#12435;&#12428;&#12435;&#12373;&#12428;&#12383;</pr></trd>,
    <trd>&#32251;&#23494;&#12394;  <pr>&#12385;&#12415;&#12388;&#12394;</pr></trd>
  </trdgrp>
  <trd lng="nl">geraffineerd</trd>
  <trdgrp lng="pl">
    <trd>rafinowany</trd>,
    <trd>oczyszczony</trd>,
    <trd>wyszukany</trd>,
    <trd>wymy&sacute;lny</trd>
  </trdgrp>
  <trd lng="pt">refinado</trd>
  <trd lng="ru">&c_r;&c_a;&c_f;&c_i;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>mazan&yacute;</trd>,
    <trd>rafinovan&yacute;</trd>,
    <trd>&lcaron;stiv&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_a;&c_f;&c_ib;&c_n;&c_o;&c_v;&c_a;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_o;&c_ch;&c_i;&c_shch;&c_je;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#31934;&#21046; <pr>j&#299;ngzh&igrave;</pr></trd>,
    <trd>&#31934;&#35069; <pr>j&#299;ngzh&igrave;</pr></trd>,
    <trd>&#31934;&#28860; <pr>j&#299;ngli&agrave;n</pr></trd>,
    <trd>&#31934;&#29001; <pr>j&#299;ngli&agrave;n</pr></trd>,
    <trd>&#20754;&#38597; <pr>r&uacute;y&#462;</pr></trd>,
    <trd>&#23092; <pr>xi&aacute;n</pr></trd>,
    <trd>&#23291; <pr>xi&aacute;n</pr></trd>,
    <trd>&#20856;&#38597; <pr>di&#462;ny&#462;</pr></trd>,
    <trd>&#24428;&#24428; <pr>b&#299;nb&#299;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rafin.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="rafin.0ejo.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Fabriko, kie oni &hcirc;emie <tld/>as:
      <ekz>
        <nom>Fushun Numero 1</nom> estis la unua <tld/>ejo de artefarita
        oleo kaj unu el la plej fruaj petrolaj <tld/>ejoj en &Ccirc;inio
        <fnt><aut>Wu Guojiang</aut>, <vrk><url ref="&Monato;/2008/009548.php">Rafinejo
        jubileas</url></vrk>, <bib>Monato</bib>, <lok>2008/08-09, p.
        22</lok></fnt>.
      </ekz>
      <ekz>
        <tld/>ejo de aluminia oksido
        <fnt>
          <bib>MD08</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili posedas preska&ubreve; tri kvaronojn el la etanol<tld/>ejoj de Minesoto
        <fnt>
          <bib>MD05</bib>
        </fnt>;
      </ekz>
      <ekz>
        mondvaste oni nun konstruas 70 pliajn alkohol-<tld/>ejojn
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2007/009177.php">Bone nutrataj a&ubreve;toj</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Raffinerie</trd>
  </snc>
  <trd
  lng="be">&c_n;&c_a;&c_f;&c_t;&c_a;&c_p;&c_je;&c_r;&c_a;&c_p;&c_r;&c_a;&c_c;&c_o;&c_w;&c_ch;&c_y;
  &c_z;&c_a;&c_v;&c_o;&c_d;</trd>
  <trd lng="bg">&c_r;&c_a;&c_f;&c_i;&c_n;&c_je;&c_r;&c_i;&c_ja;</trd>
  <trd lng="ca">refineria</trd>
  <trdgrp lng="cs">
    <trd>rafinerie</trd>,
    <trd>&ccaron;ist&iacute;rna</trd>
  </trdgrp>
  <trd lng="en">refinery</trd>
  <trd lng="es">refiner&iacute;a</trd>
  <trd lng="fr">raffinerie</trd>
  <trdgrp lng="ja">
    <trd>&#31934;&#35069;&#25152;  <pr>&#12379;&#12356;&#12379;&#12356;&#12376;&#12423;</pr></trd>,
    <trd>&#35069;&#27833;&#25152;  <pr>&#12379;&#12356;&#12422;&#12376;&#12423;</pr></trd>,
    <trd>&#35069;&#31958;&#25152;  <pr>&#12379;&#12356;&#12392;&#12358;&#12376;&#12423;</pr></trd>
  </trdgrp>
  <trd lng="nl">raffinaderij</trd>
  <trd lng="pl">rafineria</trd>
  <trd lng="pt">refinaria</trd>
  <trd lng="sk">rafin&eacute;ria</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_g;&c_ib;&c_n;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_f;&c_ib;&c_n;&c_u;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j; &c_z;&c_a;&c_v;&c_o;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#28860;&#27833;&#21378; <pr>li&agrave;ny&oacute;uch&#462;ng</pr></trd>,
    <trd>&#29001;&#27833;&#24288; <pr>li&agrave;ny&oacute;uch&#462;ng</pr></trd>
  </trdgrp>
</drv>
</art>
