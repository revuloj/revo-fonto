<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rafin</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="rafin.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rafin.0i.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emie plipurigi substancon per iaj procedoj:
      <ekz>
        <tld/>i sukeron, petrolon, oleon.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_f;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_ch;&c_y;&c_sh;&c_ch;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="ca">refinar</trd>
    <trd lng="de">raffinieren</trd>
    <trd lng="hu">finom&iacute;t</trd>    
    <trd lng="nl">raffineren</trd>
    <trd lng="pt">refinar</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_f;&c_i;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_ch;&c_i;&c_shch;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="rafin.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plidelikatigi, plisubtiligi:
      <ekz>
        <tld/>i sian stilon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="polur.0i">poluri</ref>
    </refgrp>
    <trd lng="be">&c_r;&c_a;&c_f;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="de">verfeinern</trd>
    <trdgrp lng="hu">
      <trd>finom&iacute;t</trd>,
      <trd>kifinom&iacute;t</trd>
    </trdgrp>
    <trd lng="nl">verfijnen</trd>
    <trd lng="ru">&c_r;&c_a;&c_f;&c_i;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="en">refine</trd>
  <trd lng="es">refinar</trd>
  <trd lng="fr">raffiner</trd>
</drv>

<drv mrk="rafin.0ita">
  <kap><tld/>ita</kap>
  <snc mrk="rafin.0ita.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malkruda, tre <ref tip="vid" cel="delika.0o">delikata</ref> pro 
      granda prilaboriteco:
      <ekz>
        <tld/>ita gusto;
      </ekz>
      <ekz>
        <tld/>itaj pensoj, manieroj;
      </ekz>
      <ekz>
        tre <tld/>itaj <klr>(subtile kombinitaj)</klr> iroj sur la &scirc;aka
        tabulo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="krud.0a">kruda</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_f;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
  <trd lng="fr">raffin&eacute;</trd>
  <trd lng="hu">kifinomult</trd>        
  <trd lng="ru">&c_r;&c_a;&c_f;&c_i;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
