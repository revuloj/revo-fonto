<drv mrk="klauxz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Speciala rezervo, kondi&ccirc;o, enskribita en kontrakto,
      traktato, interkonsento, le&gcirc;a akto:
      <ekz>
        tiuj kontraktoj devas enhavi anka&ubreve; <tld/>ojn por la
        estonta liberigo de servoj<fnt><aut>Evgeni Georgiev</aut>:
        <vrk>Interkonsento pri komerca liberigo</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 2a, p. 13a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Klausel</trd>
  <trd lng="ca">cl&agrave;usula</trd>
  <trd lng="en">clause</trd>
  <trd lng="es">cl&aacute;usula</trd>
  <trdgrp lng="fr">
    <trd>clause</trd>
  </trdgrp>
  <trd lng="nl">clausule</trd>
  <trdgrp lng="pl">
    <trd>klauzula</trd>,
    <trd>warunek</trd>,
    <trd>obostrzenie</trd>,
    <trd>zastrze&zdot;enie</trd>
  </trdgrp>
  <trd lng="ro">clauz&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_g;&c_o;&c_v;&c_o;&c_r;&c_k;&c_a; <klr>(&c_p;&c_u;&c_n;&c_k;&c_t; &c_z;&c_a;&c_k;&c_o;&c_n;&c_a;,
    &c_d;&c_o;&c_g;&c_o;&c_v;&c_o;&c_r;&c_a;, &c_z;&c_a;&c_v;&c_je;&c_shch;&c_a;&c_n;&c_i;&c_ja; &c_i; &c_t;.&c_p;.)</klr></trd>,
    <trd>&c_o;&c_g;&c_r;&c_a;&c_n;&c_i;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je; &c_u;&c_s;&c_l;&c_o;&c_v;&c_i;&c_je;</trd>,
    <trd>&c_k;&c_l;&c_a;&c_u;&c_z;&c_u;&c_l;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x437;&#x430;&#x441;&#x442;&#x435;&#x440;&#x435;&#x436;&#x435;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43E;&#x431;&#x43C;&#x435;&#x436;&#x443;&#x432;&#x430;&#x43B;&#x44C;&#x43D;&#x430; &#x443;&#x43C;&#x43E;&#x432;&#x430;</trd>
