<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>katalog</rad>/o
</kap>
<drv mrk="katalo.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Tabelo de objektoj ordigitaj la&ubreve; difinita ordo:
      <ekz><tld/>o de pentra&jcirc;oj, de libroj, de muzeo.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="list.0o">listo</ref>,
      <ref cel="tabel.0o">tabelo</ref>,
      <ref cel="biblio">bibliografio</ref>,
      <ref cel="repert.0o">repertuaro</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
      <trd>Katalog</trd>,
      <trd>Schlagwortregister</trd>
  </trdgrp>
  <trd lng="en">catalog</trd>
  <trd lng="nl">catalogus</trd>
  <trd lng="ru">&c_k;&c_a;&c_t;&c_a;&c_l;&c_o;&c_g;</trd>
</drv>
<drv mrk="katalo.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Registri en <tld/>o: <ekz><tld/>i bibliotekon.</ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
      <trd>katalogisieren</trd>,
      <trd>verschlagworten</trd>
  </trdgrp>
  <trd lng="en">catalog</trd>
  <trd lng="nl">catalogiseren</trd>
  <trd lng="ru">&c_k;&c_a;&c_t;&c_a;&c_l;&c_o;&c_g;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
