<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: katalo.xml,v 1.24 2023/12/03 08:00:25 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>katalog</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="katalo.0o">
  <kap><tld/>o</kap>
  <snc mrk="katalo.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="list.0o.KOMUNE">Listo</ref> de specoj de
      objektoj, venda&jcirc;oj, konserva&jcirc;oj en iu loko:
      <ekz>
        <tld/>o de pentra&jcirc;oj de muzeo;
      </ekz>
      <ekz>
        vi povos anka&ubreve; vidi en mia <tld/>o, ke <klr>...</klr>
        mi anka&ubreve; makleras pri &ccirc;iuj komerca&jcirc;oj de la urbo
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj,
        korespondado komerca, (de L. de Beaufront)</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="biblio.0o">bibliografio</ref>,
      <ref cel="invent1.0o.EKON">inventaro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_a;&c_l;&c_jo;&c_g;</trd>
  <trd lng="bg">&c_k;&c_a;&c_t;&c_a;&c_l;&c_o;&c_g;</trd>
  <trd lng="ca">cat&agrave;leg</trd>
  <trd lng="cs">katalog</trd>
  <trdgrp lng="de">
    <trd>Katalog</trd>,
    <trd>Schlagwortregister</trd>
  </trdgrp>
  <trd lng="en">catalog</trd>
  <trd lng="es">cat&aacute;logo</trd>
  <trdgrp lng="fr">
    <trd>catalogue</trd>,
    <trd>inventaire</trd>,
    <trd>r&eacute;pertoire</trd>
  </trdgrp>
  <trd lng="hu">katal&oacute;gus</trd>
  <trd lng="it">catalogo</trd>
  <trd lng="ja">&#x30C7;&#x30A3;&#x30EC;&#x30AF;&#x30C8;&#x30EA;&#x30FB;&#x30EA;&#x30B9;&#x30C8; 
  <pr>&#x30C7;&#x30A3;&#x30EC;&#x30AF;&#x30C8;&#x30EA;&#x30FB;&#x308A;&#x3059;&#x3068;</pr></trd>
  <trd lng="nl">catalogus</trd>
  <trd lng="pl">katalog</trd>
  <trd lng="ro">catalog</trd>
  <trd lng="ru">&c_k;&c_a;&c_t;&c_a;&c_l;&c_o;&c_g;</trd>
  <trd lng="sk">katal&oacute;g</trd>
  <trd lng="uk">&#x43A;&c_a;&c_t;&c_a;&#x43B;&#x43E;&c_g;</trd>
  <trdgrp lng="zh">
    <trd>&#x76EE;&#x5F55; <pr>m&#xF9;l&#xF9;</pr></trd>,
    <trd>&#x4E66;&#x76EE; <pr>sh&#x16B;m&#xF9;</pr></trd>,
    <trd>&#x7DE8;&#x76EE; <pr>bi&#x101;nm&#xF9;</pr></trd>,
    <trd>&#x578B;&#x5F55; <pr>x&#xED;ngl&#xF9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="katalo.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="katalo.0i.KOMUNE">
    <dif>
      Registri en <tld/>o:
      <ekz>
        <tld/>i bibliotekon;
      </ekz>
      <ekz>
        fascinaj estas viaj libroj, sed kiel oni <tld/>u ilin?
        <fnt>
          <aut>R. Nowakowski</aut>,
          <vrk><url ref="http://www.liberatorium.com/blog/blog_es.html">
          [blogo]</url></vrk>,
          <lok>2008-10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_k;&c_a;&c_t;&c_a;&c_l;&c_o;&c_g;&c_i;&c_z;&c_i;&c_r;&c_a;&c_m;</trd>
  <trd lng="cs">katalogizovat</trd>
  <trdgrp lng="de">
    <trd>katalogisieren</trd>,
    <trd>verschlagworten</trd>
  </trdgrp>
  <trd lng="en">catalog</trd>
  <trd lng="es">catalogar</trd>
  <trdgrp lng="fr">
    <trd>cataloguer</trd>,
    <trd>inventorier</trd>,
    <trd>r&eacute;pertorier</trd>
  </trdgrp>
  <trd lng="hu">katalogiz&aacute;l</trd>
  <trd lng="it">catalogare</trd>
  <trd lng="ja">&#xFF5E;<ind>&#x306E;&#x76EE;&#x9332;&#x3092;&#x4F5C;&#x308B; 
  </ind><pr>&#x306E;&#x3082;&#x304F;&#x308D;&#x304F;&#x3092;&#x3064;&#x304F;&#x308B;</pr></trd>
  <trd lng="nl">catalogiseren</trd>
  <trd lng="pl">katalogowa&cacute;</trd>
  <trd lng="ro">catalogare</trd>
  <trd lng="ru">&c_k;&c_a;&c_t;&c_a;&c_l;&c_o;&c_g;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">katalogizova&tcaron;</trd>
  <trd lng="uk">&#x43A;&c_a;&c_t;&c_a;&#x43B;&#x43E;&c_g;&c_ib;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>
</art>
