<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: geo.xml,v 1.4 2014/03/24 17:10:34 revo Exp $">
<kap>
  <rad>geo</rad>- <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="geo.0">
  <kap><tld/>-</kap>
  <gra><vspec>prefikso</vspec></gra>
  <snc mrk="geo.0.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Koncerna al esplorado de la Tero,
      <ref tip="vid" cel="geogra1.0o.SCI">geografio</ref>,  
      mapado,
      <ref tip="vid" cel="geolog1.0o.SCI">geologio</ref>:
      <ekz>
        la E&ubreve;ropa <tld lit="G"/>scienca Unio <klr>...</klr> disdonis 26
        
        premiojn &ccirc;i-jare
        <fnt>
          <aut>-</aut>,
          <vrk><url
          ref="http://www.espero.com.cn/z07-00a/07z-00-164.htm">
          &Ccirc;inaj sciencistoj gajnis premiojn</url></vrk>,
          <lok>[2007]</lok>
        </fnt>;
      </ekz>
      <ekz>
        estas <tld/>strategiaj kalkuloj en la politiko de NATO en eksa 
        jugoslavio
        <fnt>
          <aut>R. Platteau</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/34329/44731">
          Sendependeco de Kosovo</url></vrk>,
          <lok>2008-02</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="prefik.0pseuxdo">pse&ubreve;doprefikso</ref>
  </snc>
  <trd lng="bg">&c_g;&c_je;&c_o;-</trd>
  <trd lng="ca">geo-</trd>
  <trd lng="de">geo-</trd>
  <trd lng="fr">g&eacute;o-</trd>
</drv>
</art>

