<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tag</rad>/o
</kap>
<drv mrk="tag.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tag.0o.luma">
    <dif>
      La hela parto de <ref tip="malprt" cel="diurn.0o">diurno</ref>,
      la tempospaco dum kiu la suno, de sia levi&gcirc;o &gcirc;is sia
      subiro, prilumas koncernan parton de la tero:
      <ekz>
        mi veturis du <tld/>ojn kaj unu nokton
        <fnt><bib>F</bib>, <lok>&FE; 29</lok></fnt>;
      </ekz>
      <ekz>
        en la <tld/>o ni vidas la helan sunon
        <fnt><bib>F</bib>, <lok>&FE; 10</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>o <tld/>on sekvas, sed ne similas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        du fojojn en &ccirc;iu <tld/>o iri &ccirc;erpi akvon
        <fnt><bib>F</bib>, <lok>&FE; 13, la feino (da&ubreve;rigo)</lok>
        </fnt>;
      </ekz>
      <ekz>
        en vespero de <tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="tag.0o.12">
    <dif>
      Da&ubreve;ro de 12 horoj.
    </dif>
  </snc>
  <snc mrk="tag.0o.lumo">
    <dif>
      Suna lumo:
      <ekz>
        <tld/>i&gcirc;as.
      </ekz>
    </dif>
  </snc>
  <snc mrk="tag.0o.diurn">
    <dif>
      <ref tip="dif" cel="diurn.0o">Diurno</ref>,
      <tld/>nokto, terturno:
      <ekz>
        plimulti&gcirc;os viaj <tld/>oj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="he">&yod;&mem;&mem;&he;</trd>
    <trd lng="pl">doba</trd>
  </snc>
  <snc mrk="tag.0o.dato">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Kalendara tempopunkto, <ref tip="malprt" cel="dat.0o">dato</ref>:
      <ekz>
        la semajno havas sep <tld/>ojn;
      </ekz>
      <ekz>
        anta&ubreve; tri <tld/>oj mi vizitis vian kuzon
        <fnt><bib>F</bib>, <lok>&FE; 20</lok></fnt>;
      </ekz>
      <ekz>
        tiu &ccirc;i grava <tld/>o restos por mi &ccirc;iam
        memorinda
        <fnt><bib>F</bib>, <lok>&FE; 41</lok></fnt>;
      </ekz>
      <ekz>
        en <tld/>o de mizero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        reveni en la <tld/>o difinita;
      </ekz>
      <ekz>
        preparata por la <tld/>o de milito<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_je;&c_n;&c_mol;</trd>
  <trd lng="bg">&c_d;&c_je;&c_n;</trd>
  <trd lng="cs">den</trd>
  <trd lng="de">Tag</trd>
  <trd lng="en">day</trd>
  <trd lng="es">d&iacute;a</trd>
  <trd lng="fr">jour</trd>
  <trd lng="he">&yod;&vav;&fmem;</trd>
  <trd lng="hu">nap</trd>
  <trd lng="nl">dag</trd>
  <trd lng="pl">dzie&nacute;</trd>
  <trdgrp lng="pt">
   <trd>dia</trd>,
  </trdgrp>
  <trd lng="ro">zi<klr>(u&abreve;)</klr></trd>
  <trd lng="ru">&c_d;&c_je;&c_n;&c_mol;</trd>
  <trd lng="sv">dag</trd>
  <trd lng="vo">del</trd>
  <trdgrp lng="zh">
    <trd>&#22825;<klr> [ti&#257;n]</klr></trd>,
    <trd>&#26085;<klr> [r&igrave;]</klr></trd>
  </trdgrp>
  <trd lng="tr">g&uuml;n</trd>
</drv>

<drv mrk="tag.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Okazanta a&ubreve; farata dum la <tld/>lumo.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_jo;&c_n;&c_n;&c_y;</trd>
  <trdgrp lng="de">
    <trd>Tages<klr>-</klr></trd>,
    <trd>Tag<klr>-</klr></trd>,
    <trd>am <ind>Tag(e)</ind></trd>
  </trdgrp>
  <trd lng="en">daytime</trd>
  <trd lng="fr">diurne</trd>
  <trd lng="hu">nappali</trd>
  <trd lng="nl">dag-</trd>
  <trd lng="pl">dzienny</trd>
  <trd lng="ru">&c_d;&c_n;&c_je;&c_v;&c_n;&c_o;&c_j;</trd>
  <trd lng="tr">g&uuml;nd&uuml;z</trd>
</drv>

<drv mrk="tag.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Dum la <tld/>lumo:
      <ekz>
        la fi&scirc;kaptisto dormis bone kaj profunde, &ccirc;ar li
        multe kuris <tld/>e
        <fnt><bib>ElektFab</bib>, <lok>fi&scirc;kaptisto kaj lia
        edzino</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_d;&c_z;&c_je;&c_n;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>tags</trd>,
    <trd>am <ind>Tag(e)</ind></trd>
  </trdgrp>
  <trd lng="en">during the <ind>day</ind></trd>
  <trd lng="fr">de <ind>jour</ind></trd>
  <trd lng="he">&bet;&yod;&vav;&fmem;</trd>
  <trd lng="hu">nappal</trd>
  <trd lng="nl">overdag</trd>
  <trd lng="pl">w ci&aogonek;gu <ind>dnia</ind></trd>
  <trd lng="ro">zilnic</trd>
  <trd lng="ru">&c_d;&c_n;&c_jo;&c_m;</trd>
  <trd lng="sv">p&aring; <ind>dagen</ind></trd>
  <trd lng="sk">de&ncaron;</trd>
  <trd lng="tr">g&uuml;nd&uuml;zleyin</trd>
</drv>

<drv mrk="tag.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="tag.0igxi.KAL">
    <gra><vspec>sensubjekta</vspec></gra>
    <dif>
      Ekesti tago:
      <ekz>
        matene, tuj post kiam <tld/>i&gcirc;is
        <fnt>
          <aut>Mahbube Mirkadiri</aut>,
          <vrk>Donaco el voja&gcirc;o</vrk>,
          <bib>LOdE</bib>,
          <lok>2003:3 (101)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">le <ind>jour</ind> se l&egrave;ve</trd>
  <trd lng="nl">dag worden</trd>
  <trdgrp lng="pl">
    <trd>dnie&cacute;</trd>,
    <trd>&sacute;wita&cacute;</trd>
  </trdgrp>
  <trd lng="ro">&icirc;n zori</trd>
  <trd lng="tr">g&uuml;n a&gbreve;ar&inodot;yor</trd>
</drv>

<drv mrk="tag.antaux0o">
  <kap>anta&ubreve;<tld/>o, anta&ubreve;a <tld/>o</kap>
  <snc>
    <dif>
      La <tld/>o &gcirc;uste anta&ubreve;a al la koncerna:
      <ekz>
        foje, <ind>en anta&ubreve;<tld/>o</ind> de sabato &scirc;ia
        patro venis en la bienon
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 9a</lok></fnt>;
        <trd lng="ru">&c_n;&c_a;&c_k;&c_a;&c_n;&c_u;&c_n;&c_je;</trd>
      </ekz>
      <ekz>
        li <klr>...</klr> rakontis pri sia interparolo
        kun la viktimo en la anta&ubreve;a <tld/>o
        <fnt><bib>UrdHadda</bib>, <lok>p. 26a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="hierau.0o">hiera&ubreve;o</ref>
  </snc>
  <trdgrp lng="de">
    <trd>vorherige <ind>Tag</ind></trd>,
    <trd>Vortag</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>veille<klr tip="ind"> (jour
    pr&eacute;c&eacute;dent)</klr></trd>,
    <trd>jour pr&eacute;c&eacute;dent</trd>
  </trdgrp>
  <trd lng="nl">dag voordien</trd>
  <trd lng="pl">poprzedni <ind>dzie&nacute;</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_ch;&c_je;&c_r;&c_a;&c_sh;&c_n;&c_i;&c_j; 
&c_d;&c_je;&c_n;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_sh;&c_je;&c_d;&c_sh;&c_i;&c_j; 
&c_d;&c_je;&c_n;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="tag.cxi0e">
  <kap>&ccirc;i-<tld/>e</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="tag.cxi0e.hodiaux">
    <ref tip="dif" cel="hodiau.0">hodia&ubreve;</ref>
    <ekz>
      multajn pli gravajn aferojn oni diskutas &ccirc;i-<tld/>e en la
      nova&jcirc;grupo
      <fnt>
      <aut>F. Zenith</aut>,
      <vrk>Opera 7.03 finfine en esperanto!</vrk>,
      <lok>soc.culture.esperanto, 2003-03-24</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="de">heute</trd>
  <trd lng="fr">ce <ind>jour</ind>-ci</trd>
  <trd lng="nl">dezer <ind>dagen</ind></trd>
  <trd lng="pl">dzisiejszego <ind>dnia</ind></trd>
  <trd lng="ro">azi</trd>
  <trd lng="ru">&c_s;&c_je;&c_g;&c_o;&c_d;&c_n;&c_ja;</trd>
  <trd lng="tr">bu g&uuml;n</trd>
</drv>

<drv mrk="tag.cxiu0a">
  <kap>&ccirc;iu<tld/>a</kap>
  <snc>
    <dif>
      Okazanta a&ubreve; aperanta &ccirc;iun <tld/>on:
      <ekz>
        &ccirc;iu<tld/>a gazeto.
      </ekz>
    </dif>
    <trd lng="be">&c_sh;&c_t;&c_o;&c_d;&c_z;&c_jo;&c_n;&c_n;&c_y;</trd>
    <trd lng="de">t&auml;glich</trd>
    <trd lng="en">daily</trd>
    <trd lng="he">&yod;&vav;&mem;&yod;</trd>
    <trdgrp lng="hu">
      <trd>napi</trd>,
      <trd>mindennapi</trd>
    </trdgrp>
    <trd lng="nl">dagelijks</trd>
    <trd lng="pl">codzienny</trd>
    <trd lng="ro">zilnic</trd>
    <trd lng="ru">&c_je;&c_zh;&c_je;&c_d;&c_n;&c_je;&c_v;&c_n;&c_y;&c_j;
    </trd>
    <trd lng="sv">daglig</trd>
  </snc>
  <snc>
    <dif>
      Ordinara, banala:
      <ekz>
        la griza &ccirc;iu<tld/>eco de la vivo
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="proz.0a">proza</ref>
    </refgrp>
    <trd lng="de">allt&auml;glich</trd>
    <trdgrp lng="hu">
      <trd>mindennapos</trd>,
      <trd>h&eacute;tk&ouml;znapi</trd>
    </trdgrp>
    <trd lng="be">&c_a;&c_b;&c_y;&c_d;&c_z;&c_je;&c_n;&c_n;&c_y;</trd>
    <trdgrp lng="en">
      <trd>everyday</trd>,
      <trd>casual</trd>
    </trdgrp>
    <trd lng="he">&yod;&vav;&mem;&yod;&vav;&mem;&yod;</trd>
    <trd lng="nl">dagdagelijks</trd>
    <trdgrp lng="pl">
      <trd>szary <ind>dzie&nacute;</ind></trd>,
      <trd>zwyczajny <ind>dzie&nacute;</ind></trd>,
      <trd><ind>dzie&nacute;</ind> jak co dzie&nacute;</trd>,
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_y;&c_d;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>
      &c_k;&c_a;&c_zh;&c_d;&c_o;&c_d;&c_n;&c_je;&c_v;&c_n;&c_y;&c_j;
      </trd>
    </trdgrp>
    <trd lng="sv">vardaglig</trd>
  </snc>
  <trd lng="fr">quotidien</trd>
  <trd lng="tr">g&uuml;nl&uuml;k</trd>
</drv>

<drv mrk="tag.posta0o">
  <kap>posta <tld/>o</kap>
  <snc>
    <dif>
      La <tld/>o &gcirc;uste posta al la koncerna:
      <ekz>
        la postan <tld/>on iris mi vizite
        <fnt><aut>P. Peneter</aut>,
        <vrk>Sekretaj Sonetoj</vrk>, <lok>VII</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="morgau.0o">morga&ubreve;o</ref>
  </snc>
  <trdgrp lng="de">
    <trd>folgende <ind>Tag</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>lendemain</trd>,
    <trd>jour suivant</trd>
  </trdgrp>
  <trd lng="nl">volgende <ind>dag</ind></trd>
  <trdgrp lng="pl">
    <trd>przysz&lstroke;y <ind>dzie&nacute;</ind></trd>,
    <trd>nadchodz&aogonek;cy <ind>dzie&nacute;</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_v;&c_t;&c_r;&c_a;&c_sh;&c_n;&c_i;&c_j;
    &c_d;&c_je;&c_n;&c_mol;</trd>,
    <trd>&c_s;&c_l;&c_je;&c_d;&c_u;&c_ju;&c_shch;&c_i;&c_j;
    &c_d;&c_je;&c_n;&c_mol;</trd>
  </trdgrp>
  <trd lng="tr">ertesi g&uuml;n</trd>
</drv>

<drv mrk="tag.unu0a">
  <kap><ofc>*</ofc>unu<tld/>a</kap>
  <snc>
    <dif>
      Da&ubreve;ranta nur unu tagon.
    </dif>
    <refgrp tip="vid">
      <ref cel="efemer.0a">efemera</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_n;&c_a;&c_d;&c_z;&c_jo;&c_n;&c_n;&c_y;
  </trd>
  <trd lng="de">eint&auml;gig</trd>
  <trd lng="en">single-<ind>day</ind></trd>
  <trd lng="fr">eph&eacute;m&egrave;re<klr> (d'un jour)</klr></trd>
  <trdgrp lng="hu">
    <trd>egynapos</trd>,
    <trd>napos</trd>
  </trdgrp>
  <trd lng="nl">eendags-</trd>
  <trd lng="pl">jednodniowy</trd>
  <trd lng="ru">
  &c_o;&c_d;&c_n;&c_o;&c_d;&c_n;&c_je;&c_v;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="tag.0ekajnokte">
  <kap><tld/>e kaj nokte</kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc mrk="tag.0ekajnokte.cxiam">
    <dif>
      <ref tip="vid" cel="konsta.0e">Konstante</ref>, sen&ccirc;ese,
      persiste:
      <ekz>
        <tld/>e kaj nokte apud &gcirc;i staris gardostaranto
        <fnt><bib>Fab3</bib>, <lok>folio el la &ccirc;ielo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Tag und Nacht</trd>
  <trd lng="fr">nuit et jour</trd>
  <trd lng="nl">dag en nacht</trd>
  <trd lng="pl">dniem i noc&aogonek;</trd>
  <trd lng="ru">&c_d;&c_n;&c_jo;&c_m; &c_i; 
&c_n;&c_o;&c_ch;&c_mol;&c_ju;</trd>
<trd lng="tr">g&uuml;nd&uuml;z ve gece</trd>
</drv>
</art>
