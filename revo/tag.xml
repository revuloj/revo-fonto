<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>tag</rad>/o
    </kap>
    <drv mrk="tag.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="tag.0o.luma">
        <dif>
          Spaco da tempo dum kiu la suno, de sia levi&gcirc;o &gcirc;is sia
          subiro, lumigas difinitan parton de la tero;
          la hela parto de <ref tip="malprt" cel="diurn.0o">diurno</ref>:
          <ekz>
            en la <tld/>o ni vidas la helan sunon<fnt>Z</fnt>;
          </ekz><ekz><tld/>o <tld/>on sekvas, sed ne similas
<fnt><bib>PrV</bib></fnt>;</ekz><ekz>
            mi veturis du <tld/>ojn kaj unu nokton<fnt>Z</fnt>;
          </ekz><ekz>
            iri &ccirc;erpi akvon du fojojn en &ccirc;iu
<tld/>o<fnt>Z</fnt>;
          </ekz><ekz>
            en vespero de <tld/>o<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>man&gcirc;i
            <klr>(man&gcirc;i &ccirc;irka&ubreve; la mezo de la
<tld/>o)</klr>.
          </ekz> 
        </dif>
      </snc>
      <snc mrk="tag.0o.12">
        <dif>
          Da&ubreve;ro de 12 horoj. 
        </dif>
      </snc>
      <snc mrk="tag.0o.lumo">
        <dif>
          Suna lumo:
          <ekz>
            <tld/>i&gcirc;as.
          </ekz>
        </dif>
      </snc>
      <snc mrk="tag.0o.diurn">
        <dif>
          <ref tip="dif" cel="diurn.0o">Diurno</ref>,
          <tld/>nokto, terturno:
          <ekz>
            plimulti&gcirc;os viaj <tld/>oj<fnt>Z</fnt>.
          </ekz>
        </dif>
      </snc>
      <snc mrk="tag.0o.dato">
        <dif>
          Kalendara tempopunkto, <ref tip="malprt" cel="dat.0o">dato</ref>:
          <ekz>
            la semajno havas sep <tld/>ojn;
          </ekz><ekz>
            anta&ubreve; tri <tld/>oj mi vizitis vian kuzon<fnt>Z</fnt>;
          </ekz><ekz>
            tiu &ccirc;i grava <tld/>o restos por mi &ccirc;iam
memorinda<fnt>Z</fnt>;
          </ekz><ekz>
            en <tld/>o de mizero<fnt>Z</fnt>;
          </ekz><ekz>
            reveni en la <tld/>o difinita;
          </ekz><ekz>
            preparata por la <tld/>o de milito<fnt>Z</fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_d;&c_z;&c_je;&c_n;&c_mol;</trd>
      <trd lng="bg">&c_d;&c_je;&c_n;</trd>
      <trd lng="cs">den</trd>
      <trd lng="en">day</trd>
      <trd lng="de">Tag</trd>
      <trd lng="nl">dag</trd>
      <trd lng="fr">jour</trd>
      <trd lng="hu">nap</trd>
      <trd lng="pl">dzie&nacute;</trd>
      <trd lng="es">d&iacute;a</trd>
      <trd lng="sv">dag</trd>
    </drv>
    <drv mrk="tag.0a">
      <kap><tld/>a</kap>
      <snc>
        <dif>
          Okazanta a&ubreve; farata dum la <tld/>lumo. 
        </dif>
      </snc>
      <trd lng="nl">dag-</trd>
      <trd lng="hu">nappali</trd>
      <trd lng="be">&c_d;&c_z;&c_jo;&c_n;&c_n;&c_y;</trd>
    </drv>
    <drv mrk="tag.0e">
      <kap><tld/>e</kap>
      <snc>
        <dif>
          Dum la <tld/>lumo. 
        </dif>
      </snc>
      <trd lng="be">&c_u;&c_d;&c_z;&c_je;&c_n;&c_mol;</trd>
      <trd lng="hu">nappal</trd>
      <trd lng="nl">overdag</trd>
      <trd lng="sv">p&aring; <ind>dagen</ind></trd>
    </drv>
    <drv mrk="tag.cxiu0a">
      <kap>&ccirc;iu<tld/>a</kap>
      <snc>
        <dif>
          Okazanta a&ubreve; aperanta &ccirc;iun <tld/>on:
          <ekz>
            &ccirc;iu<tld/>a gazeto
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="jxurna.0o">&jcirc;urnalo</ref>.
        </refgrp>
        <trd lng="de">t&auml;glich</trd>
        <trdgrp lng="hu">
          <trd>napi</trd>,
          <trd>mindennapi</trd></trdgrp>
        <trd lng="nl">dagelijks</trd>
        <trd lng="sv">daglig</trd>
      </snc>
      <trd lng="be">&c_sh;&c_t;&c_o;&c_d;&c_z;&c_jo;&c_n;&c_n;&c_y;</trd>
      <snc>
        <dif>
          Ordinara, banala:
          <ekz>
            la griza &ccirc;iu<tld/>eco de la vivo
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="proz.0a">proza</ref>.
        </refgrp>
        <trd lng="de">allt&auml;glich</trd>
        <trdgrp lng="hu">
          <trd>mindennapos</trd>,
          <trd>h&eacute;tk&ouml;znapi</trd></trdgrp>
        <trd lng="nl">dagdagelijks</trd>
        <trd lng="sv">vardaglig</trd>
      </snc>
      <trd lng="be">&c_a;&c_b;&c_y;&c_d;&c_z;&c_je;&c_n;&c_n;&c_y;</trd>
    </drv>
    <drv mrk="tag.unu0a">
      <kap>unu<tld/>a</kap>
      <snc>
        <dif>
          Kiu da&ubreve;ras nur unu <tld/>on, sed ne 2 a&ubreve; 4.  
        </dif>
      </snc>
      <trdgrp lng="hu">
        <trd>egynapos</trd>,
        <trd>napos</trd></trdgrp>
      <trd lng="nl">eendags-</trd>
      <trd
lng="be">&c_a;&c_d;&c_n;&c_a;&c_d;&c_z;&c_jo;&c_n;&c_n;&c_y;</trd>
    </drv>
  </art>
