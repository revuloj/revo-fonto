<drv mrk="tag.cxiu0a">
  <kap>&ccirc;iu<tld/>a,
  <var><kap><tld/><tld/>a</kap></var></kap>
  <snc mrk="tag.cxiu0a.KOMUNE">
    <dif>
      Okazanta a&ubreve; aperanta &ccirc;iun <tld/>on:
      <ekz>
        &ccirc;iu<tld/>a gazeto;
      </ekz><ekz>
        pretigu vian &ccirc;iu<tld/>an laboron
        <fnt>
          <bib>MT</bib>
          <lok>&Eli; 5:13</lok>
        </fnt>;
      </ekz><ekz>
        estos duobla porcio kompare kun tio, kion ili kolektas
        &ccirc;iu<tld/>e
        <fnt>
          <bib>MT</bib>
          <lok>&Eli; 16:5</lok>
        </fnt>;
      </ekz><ekz>
        &ccirc;ar &scirc;i tedadis lin per siaj vortoj &ccirc;iu<tld/>e kaj turmentadis
        lin, lia animo morte laci&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>&Jug; 16:16</lok>
        </fnt>;
      </ekz><ekz>
        okupita de laboro por &ccirc;iu<tld/>a pano
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz><ekz>
        el la &ccirc;iu<tld/>a sperto ni &ccirc;iuj ja scias tre bone, ke
        <klr>[&#8230;]</klr>
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_sh;&c_t;&c_o;&c_d;&c_z;&c_jo;&c_n;&c_n;&c_y;</trd>
    <trd lng="de">t&auml;glich</trd>
    <trd lng="en">daily</trd>
    <trd lng="es">cada d&iacute;a</trd>
    <trd lng="he">&yod;&vav;&mem;&yod;</trd>
    <trdgrp lng="hu">
      <trd>napi</trd>,
      <trd>mindennapi</trd>
    </trdgrp>
    <trd lng="id">se<ind>tiap</ind> hari</trd>
    <trd lng="nl">dagelijks</trd>
    <trd lng="pl">codzienny</trd>
    <trd lng="ro">zilnic</trd>
    <trd lng="ru">&c_je;&c_zh;&c_je;&c_d;&c_n;&c_je;&c_v;&c_n;&c_y;&c_j;
    </trd>
    <trd lng="sv">daglig</trd>
  </snc>
  <snc mrk="tag.cxiu0a.banala">
    <dif>
      Ordinara, banala:
      <ekz>
       morga&ubreve; li jam estos <klr>[&#8230;]</klr> en la tropikoj, &gcirc;uos puran 
       aeron, puran akvon kaj neniam plu pensos pri sia <tld/><tld/>a 
       vivo <klr>[&#8230;]</klr> en malpura aero apud malsanaj homa&ccirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2004/008358.html">Kuracbaneje</url></vrk>
        </fnt>;
      </ekz><ekz>
        ne similu for&gcirc;istojn en la sen&gcirc;enaj &ccirc;iu<tld/>aj vestoj
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto unua</lok>
        </fnt>;
      </ekz><ekz>
        la griza &ccirc;iu<tld/>eco de la vivo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="proz.0a">proza</ref>
    </refgrp>
    <trd lng="de">allt&auml;glich</trd>
    <trdgrp lng="hu">
      <trd>mindennapos</trd>,
      <trd>h&eacute;tk&ouml;znapi</trd>
    </trdgrp>
    <trd lng="be">&c_a;&c_b;&c_y;&c_d;&c_z;&c_je;&c_n;&c_n;&c_y;</trd>
    <trdgrp lng="en">
      <trd>everyday</trd>,
      <trd>casual</trd>
    </trdgrp>
    <trd lng="he">&yod;&vav;&mem;&yod;&vav;&mem;&yod;</trd>
    <trd lng="nl">dagdagelijks</trd>
    <trdgrp lng="pl">
      <trd>szary <ind>dzie&nacute;</ind></trd>,
      <trd>zwyczajny <ind>dzie&nacute;</ind></trd>,
      <trd><ind>dzie&nacute;</ind> jak co dzie&nacute;</trd>,
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_y;&c_d;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_k;&c_a;&c_zh;&c_d;&c_o;&c_d;&c_n;&c_je;&c_v;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
    <trd lng="sv">vardaglig</trd>
  </snc>
  <trd lng="es">diario</trd>
  <trd lng="fr">quotidien</trd>
  <trd lng="tr">g&uuml;nl&uuml;k</trd>
  <trdgrp lng="uk">
    <trd>&c_shch;&c_o;&c_d;&c_je;&c_n;&c_n;&c_i;&c_j;</trd>,
    <trd>(&c_p;&c_r;&c_o; &c_h;&c_l;&c_ib;&c_b; &c_shch;&c_je;) &c_n;&c_a;&c_s;&c_u;&c_shch;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_v;&c_s;&c_ja;&c_k;&c_d;&c_je;&c_n;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_u;&c_d;&c_je;&c_n;&c_n;&c_i;&c_j;</trd>
