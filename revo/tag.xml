  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#19968;&#26085;&#12398;  <pr>&#12402;&#12392;&#12395;&#12385;&#12398;</pr></trd>,
    <trd>&#19968;&#26085;&#38480;&#12426;&#12398;
    <pr>&#12402;&#12392;&#12395;&#12385;&#12363;&#12366;&#12426;&#12398;</pr></trd>
  </trdgrp>
  <trd lng="nl">eendags-</trd>
  <trd lng="pl">jednodniowy</trd>
  <trd lng="ru">
