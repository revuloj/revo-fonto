<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>asperg</rad>/i
</kap>
<drv mrk="asperg.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1">
    <uzo tip="fak">REL</uzo>
    <dif>
      &Scirc;prucigi sanktan  flua&jcirc;on sur ion a&ubreve; iun:
      <ekz>
        <tld/>u ilin per akvo
        <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Nombroj 8, 7</lok>)</fnt>;
      </ekz>
      <ekz>
        <tld/>i per oleo<fnt>Z</fnt>.
      </ekz> 
    </dif>
    <trd lng="de">weihen</trd>
  </snc>
  <snc num="2">
    <dif>
      Delikate, pluvgutete sur&scirc;prucigi  ion a&ubreve; iun:
      <ekz>
        <tld/>i florojn.
      </ekz>
    </dif>
    <trd lng="de">gie&szlig;en</trd>
  </snc>
</drv>
<drv mrk="asperg.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Ilo por <tld/>i 1.  
    </dif>
  </snc>
  <trd lng="de">Gie&szlig;kanne</trd>
</drv>
</art>
