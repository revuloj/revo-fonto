<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>asperg</rad>/i
</kap>
<drv mrk="asperg.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc num="1">
     <uzo tip="fak">REL</uzo>
     <dif>
       &Scirc;prucigi sanktan  flua&jcirc;on sur ion a&ubreve; iun:
       <ekz>
         <tld/>u ilin per akvo
         <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
         <lok>Nombroj 8, 7</lok>)</fnt>;
       </ekz>
       <ekz>
         <tld/>i per oleo<fnt>Z</fnt>.
       </ekz>
     </dif>
     <trd lng="de">weihen</trd>
     <trd lng="fr">asperger (d'eau b&eacute;nite)</trd>
     <trdgrp lng="ru">
       <trd>&#1082;&#1088;&#1086;&#1087;&#1080;&#1090;&#1100;</trd>,
       <trd>&#1086;&#1082;&#1088;&#1086;&#1087;&#1083;&#1103;&#1090;&#1100;</trd>
     </trdgrp>
   </snc>
   <snc num="2">
     <dif>
       Delikate, pluvgutete sur&scirc;prucigi  ion a&ubreve; iun:
       <ekz>
         <tld/>i florojn.
       </ekz>
     </dif>
     <trd lng="de">gie&szlig;en</trd>
     <trdgrp lng="fr">
       <trd>vaporiser (des gouttelettes)</trd>,
       <trd>pulv&eacute;riser (un liquide)</trd></trdgrp>
     <trdgrp lng="ru">
       <trd>&#1086;&#1087;&#1088;&#1099;&#1089;&#1082;&#1080;&#1074;&#1072;&#1090;&#1100;</trd>,
       <trd>&#1086;&#1073;&#1088;&#1099;&#1079;&#1075;&#1080;&#1074;&#1072;&#1090;&#1100;</trd>
     </trdgrp>
   </snc>
</drv>
<drv mrk="asperg.0ilo">
   <kap><tld/>ilo</kap>
   <snc>
     <dif>
       Ilo por <tld/>i 1.
     </dif>
   </snc>
   <trd lng="de">Gie&szlig;kanne</trd>
   <trdgrp lng="fr">
     <trd>aspersoir</trd>,
     <trd>vaporisateur</trd>,
     <trd>pulv&eacute;risateur</trd></trdgrp>
   <trdgrp lng="ru">
     <trd>&#1082;&#1088;&#1086;&#1087;&#1080;&#1083;&#1086;</trd>,
     <trd>&#1083;&#1077;&#1081;&#1082;&#1072;</trd>
   </trdgrp>
</drv>
</art>
