<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ocel.xml,v 1.5 2024/02/08 21:19:46 revo Exp $">
<kap>
  <rad>ocel</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ocel.0o">
  <kap><tld/>o</kap>
  <snc mrk="ocel.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Primitiva, simpla <ref tip="super" cel="okul.0o.ANA">okulo</ref> trovebla &ccirc;e iuj artikuloj,
      kniduloj, e&hcirc;inodermoj, anelidoj:
      <ekz>
        la laboristoj de <ctl><nom>Myrmica</nom></ctl> ne havas e&ccirc; rudimentajn
        <tld/>ojn, kvankam la maskloj kaj fekundaj femaloj de &ccirc;i tiu
        genro
        havas bone formi&gcirc;intajn <tld/>ojn
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 7a</lok></fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/5/5a/Vespa_crabo_HC2.jpg/320px-Vespa_crabo_HC2.jpg"
      prm="CC BY-SA 3.0"
      >la tri fruntaj <tld/>oj de krabro
        <fnt>
          <aut>Holger Casselmann, propra verko</aut>
          <lok><url ref="&WCW;/File:Vespa_crabo_HC2.jpg"
              >https://commons.wikimedia.org/wiki/File:Vespa_crabo_HC2.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="de">
      <trd>Flachauge</trd>,
      <trd>Plattenauge</trd>,
      <trd>Punktauge</trd>,
      <trd>Ocellus</trd>
    </trdgrp>
  </snc>
  <snc mrk="ocel.0o.FORM">
    <dif>
      Pli-malpli ringoforma kolora <ref tip="super" cel="makul.0o.natura">makulo</ref>,
      iafoje okul-simila (ekz-e &ccirc;e papilioj vosto de pavo).
    </dif>
  <trd lng="de">Ozelle</trd>
  </snc>
  <trd lng="de">Augenfleck</trd>
  <trd lng="fr">ocelle</trd>
  <trd lng="pl">przyoczko</trd>
  <trd lng="ro">ochi</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_je; &c_o;&c_k;&c_o; (&c_k;&c_o;&c_m;&c_a;&c_h;&c_i;)</trd>,
    <trd>&c_o;&c_ch;&c_k;&c_o; (&c_n;&c_a; &c_k;&c_r;&c_i;&c_l;&c_a;&c_h; &c_m;&c_je;&c_t;&c_je;&c_l;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_ib;&c_r;&#8217;&c_ji; &c_p;&c_a;&c_v;&c_i; &c_ib; &c_t;.&c_p;.)</trd>
  </trdgrp>
</drv>
</art>

