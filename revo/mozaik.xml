<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mozaik.xml,v 1.26 2024/01/26 19:05:53 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>mozaik</rad>/o
</kap>
<drv mrk="mozaik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="bild.0o.KOMUNE">Bildo</ref> el koloraj &scirc;tonaj a&ubreve; vitraj kubetoj
      kuncementitaj, uzata por ornami grundon a&ubreve; muron:
      <ekz>
        sur la <tld/>a planko staris silente, en blankaj vestoj kaj nudpiede, la pastroj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        la juna viro <klr>[&#x2026;]</klr> komencis subakvi&gcirc;adi en la banujo, elpla&ubreve;dante
        abunde &ccirc;e tio la varmetan akvon sur la <tld/>on, prezentantan Heron en la
        momento, kiam &scirc;i petas Dormon pri endormi&gcirc;o de Ze&ubreve;so
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi travivis ravajn epizodojn, <klr>[&#x2026;]</klr> el tiuj <ind><tld/>ero</ind>j jam elformi&gcirc;as
        tute klara konvinki&gcirc;o
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>.
        <trd lng="de">Mosaikstein</trd>
        <trd lng="en">piece of mosaic</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li estas poeto de momentaj natureventoj, de subitaj, intuiciaj perceptoj
        <klr>[&#x2026;]</klr>, kie &ccirc;iu vorto estas nemalhavebla ero de la <tld/>a tuto
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Svislando estas <tld/>o de kulturoj
        <fnt>
          <bib>Monato</bib>
          <aut>Mireille Grosjean</aut>
          <vrk><url ref="&Monato;/2011/010267.php">Svislando: la arto interkompreni&gcirc;i</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>o de urboj kaj vila&gcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Marc Gassion</aut>
          <vrk><url ref="&Monato;/2003/007934.html">Aude</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kahel.0o">kahelo</ref>,
      <ref cel="pavim.0o">pavimo</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="kalejd.0o">kalejdoskopo</ref>,
      <ref cel="puzl.0o">puzlo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_z;&c_a;&c_ib;&c_k;&c_a;</trd>
  <trd lng="bg">&c_m;&c_o;&c_z;&c_a;&c_j;&c_k;&c_a;</trd>
  <trd lng="cs">mozaika</trd>
  <trd lng="de">Mosaik</trd>
  <trd lng="en">mosaic</trd>
  <trd lng="es">mosaico</trd>
  <trd lng="fa">&a_m;&a_ayn;&a_r;&a_q;&f_k;&a_A;&a_r;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>mosa&iuml;que</trd>
  </trdgrp>
  <trd lng="hu">mozaik</trd>
  <trdgrp lng="ja">
    <trd>&#x30E2;&#x30B6;&#x30A4;&#x30AF;  <pr>&#x3082;&#x3056;&#x3044;&#x304F;</pr></trd>,
    <trd>&#x5BC4;&#x305B;&#x96C6;&#x3081;  <pr>&#x3088;&#x305B;&#x3042;&#x3064;&#x3081;</pr></trd>,
    <trd>&#x30E2;&#x30B6;&#x30A4;&#x30AF;&#x75C5;  <pr>&#x30E2;&#x30B6;&#x30A4;&#x30AF;&#x3073;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">moza&iuml;ek</trd>
  <trd lng="pl">mozaika</trd>
  <trd lng="pt">mosaico <klr tip="amb">(artes, arq.)</klr></trd>
  <trd lng="ro">mozaic</trd>
  <trd lng="ru">&c_m;&c_o;&c_z;&c_a;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">mozaika</trd>
  <trd lng="uk">&c_m;&c_o;&c_z;&c_a;&c_ji;&c_k;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x99AC;&#x8CFD;&#x514B; <pr>m&#x1CE;s&#xE0;ik&#xE8;</pr></trd>,
    <trd>&#x9A6C;&#x8D5B;&#x514B; <pr>m&#x1CE;s&#xE0;ik&#xE8;</pr></trd>,
    <trd>&#x9B54;&#x8CFD;&#x514B; <pr>m&#xF3;s&#xE0;ik&#xE8;</pr></trd>,
    <trd>&#x9B54;&#x8D5B;&#x514B; <pr>m&#xF3;s&#xE0;ik&#xE8;</pr></trd>
  </trdgrp>
</drv>
</art>
