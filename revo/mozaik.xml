<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>mozaik</rad>/o
</kap>
<drv mrk="mozaik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Bildo el koloraj &scirc;tonaj a&ubreve; vitraj kubetoj
      kuncementitaj, uzata por ornami grundon a&ubreve; muron.
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la libro estas agrabla <tld/>o da rakontoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_z;&c_a;&c_ib;&c_k;&c_a;</trd>
  <trd lng="de">Mosaik</trd>
  <trd lng="en">mosaic</trd>
  <trdgrp lng="fr">
    <trd>mosa&iuml;que</trd>
  </trdgrp>
  <trd lng="hu">mozaik</trd>
  <trd lng="nl">moza&iuml;ek</trd>
  <trd lng="pt">mosaico <klr tip="amb">(artes, arq.)</klr></trd>
  <trd lng="ru">&c_m;&c_o;&c_z;&c_a;&c_i;&c_k;&c_a;</trd>
</drv>
</art>
