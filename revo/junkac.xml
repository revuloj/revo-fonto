  <trd lng="de">Binsengew&auml;chse</trd>
  <trd lng="es">junc&aacute;ceas</trd>
  <trd lng="fr">juncac&eacute;es</trd>
  <trd lng="hu">szitty&oacute;f&eacute;l&eacute;k</trd>
  <trd lng="it">giuncacee</trd>
  <trd lng="nl">russenfamilie</trd>
  <trd lng="pt">junc&aacute;ceas</trd>
  <trd lng="ru">&c_s;&c_i;&c_t;&c_n;&c_i;&c_k;&c_o;&c_v;&c_y;&c_je; 
<klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
