<art mrk="$Id: junkac.xml,v 1.14 2022/03/21 17:23:46 revo Exp $">
<kap>
  <rad>junkac</rad>/oj
</kap>
<drv mrk="junkac.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref>  de <ref tip="malprt" cel="kotile.unu0aj">unukotiledonaj</ref>
      plantoj (<trd lng="la">Juncaceae</trd>), herbsimilaj, kun rondaj,
      rektaj kaj flekseblaj trunketoj:
      <ekz>
        la <tld/>oj estas moltigaj herboj, similaj al poacoj, iliaj floroj redukti&gcirc;is al bruna a&ubreve; verda perianto, kaliko glumosimila
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=27225">Junkacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="junk.0o">junko</ref>
  </snc>
  <trd lng="de">Binsengew&auml;chse</trd>
  <trd lng="es">junc&aacute;ceas</trd>
  <trdgrp lng="en">
    <trd>Juncaceae</trd>,
    <trd>rush family</trd>, <trd>rushes</trd>
  </trdgrp>
  <trd lng="fr">juncac&eacute;es</trd>
  <trd lng="hu">szitty&oacute;f&eacute;l&eacute;k</trd>
  <trd lng="it">giuncacee</trd>
  <trd lng="nl">russenfamilie</trd>
  <trd lng="pl">sitowate</trd>
  <trd lng="pt">junc&aacute;ceas</trd>
  <trd lng="ru">&c_s;&c_i;&c_t;&c_n;&c_i;&c_k;&c_o;&c_v;&c_y;&c_je; 
<klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&#x441;&#x438;&#x442;&#x43D;&#x438;&#x43A;&#x43E;&#x432;&#x456;</trd>
</drv>
</art>
