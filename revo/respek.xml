<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>respekt</rad>/o
</kap>
<drv mrk="respek.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Sento de profunda estimo kaj submeti&gcirc;o, kiun oni sentas anta&ubreve; iu
      rangsuperulo a&ubreve; anta&ubreve; ia supereco.
    </dif>
  </snc>
  <snc>
    <dif>
      Eksteraj signoj, per kiuj oni aperigas tiun &ccirc;i senton.
    </dif>
  </snc>
  <trd lng="en">respect</trd>
  <trdgrp lng="nl">
    <trd>eerbied</trd>,
    <trd>respect</trd>
  </trdgrp>
  <trd lng="sv">respekt</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_ch;&c_t;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_u;&c_v;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="respek.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Havi senton de <tld/>o al iu a&ubreve; io
    </dif>
    <refgrp tip="sin">
      <ref cel="estim.0i">estimi</ref>,
      <ref cel="sxat.0i">&scirc;ati</ref>.
    </refgrp>
    <trd lng="de">achten</trd>
  </snc>
  <snc>
    <dif>
      Montri per siaj agoj, ke oni <tld/>as iun a&ubreve; ion:
      <ekz>
        <tld/>i <klr>(observi, obei)</klr> sian promeson<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i la bezonojn, la dezirojn de iu;
      </ekz>
      <ekz>
        li <tld/>is la silentan suferon de la virino<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="konsid.0i">konsideri</ref>,
      <ref cel="atent">atenti</ref>.
    </refgrp>
    <trd lng="de">respektieren</trd>
  </snc>
  <trdgrp lng="nl">
    <trd>eerbiedigen</trd>,
    <trd>respecteren</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_ch;&c_i;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_v;&c_a;&c_zh;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">respektera</trd>
</drv>

<drv mrk="respek.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="R"/>oplena, montranta <tld/>on.
    </dif>
  </snc>
  <trd lng="nl">eerbiedig</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_ch;&c_t;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_u;&c_v;&c_a;&c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
