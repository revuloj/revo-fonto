<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: astrag.xml,v 1.20 2021/03/04 17:56:54 revo Exp $">
<kap>
  <rad>astragal</rad>/o
</kap>
<drv mrk="astrag.0o">
  <kap><tld/>o</kap>
  <snc mrk="astrag.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Astragalus</trd>).
    </dif>
    <trd lng="be">&c_k;&c_u;&c_r;&c_a;&c_v;&c_a;&c_j;</trd>
    <trd lng="en">milk vetch</trd>
    <trd lng="de">Tragant</trd>
    <trd lng="fr">astragale<klr tip="amb"> (plante)</klr></trd>
    <trdgrp lng="hu">
      <trd>cs&uuml;df&udblac;</trd>,
      <trd>b&oacute;ka</trd>
    </trdgrp>
    <trd lng="pl">traganek</trd>
  </snc>
  <snc mrk="astrag.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="super" cel="modlur.0o.ARKI">Modluro</ref>, kiu &ccirc;irka&ubreve;as
      la
      supran ekstremon de <ref tip="malprt" cel="kolon.0o.ARKI">kolono</ref>:
      <ekz>
        <tld/>oj trovi&gcirc;as &ccirc;efe &ccirc;e romanordaj kolonoj.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_s;&c_t;&c_r;&c_a;&c_g;&c_a;&c_l;</trd>
    <trd lng="en">astragal</trd>
    <trd lng="fr">astragale<klr tip="amb"> (moulure)</klr></trd>
    <trd lng="hu">asztragalosz</trd>
  </snc>
  <snc mrk="astrag.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      La artikaj kalkaneaj surfacoj: malanta&ubreve;a kaj meza.
    </dif>
    <trd lng="fr">astragale<klr tip="amb"> (os)</klr></trd>
    <trdgrp lng="hu">
      <trd>bokacsont</trd>,
      <trd>ugr&oacute;csont</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">ozdoba na hlavici sloupu</trd>
  <trdgrp lng="he">
    <trd>&#x5DB;&#x5E8;&#x5DB;&#x5D5;&#x5D1;</trd>,
    <trd>&#x5E2;&#x5D9;&#x5D8;&#x5D5;&#x5E8; &#x5E9;&#x5DC; &#x5E2;&#x5DE;&#x5D5;&#x5D3;</trd>
  </trdgrp>
  <trd lng="it">astragalo</trd>
  <trd lng="pl">ko&sacute;&cacute; skokowa</trd>
  <trd lng="ru">&c_a;&c_s;&c_t;&c_r;&c_a;&c_g;&c_a;&c_l;</trd>
  <trd lng="sk">ozdobn&#xE1; obruba na hlavici st&#x13A;pu</trd>
</drv>
</art>
