<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>astragal</rad>/o
</kap>
<drv mrk="astrag.0o">
<kap><tld/>o</kap>
<snc mrk="astrag.0o.BOT">
   <uzo tip="fak">BOT</uzo>
   <dif>
     Genro el familio <ref tip="super" cel="fab.0acoj">fabacoj</ref>
     (<trd lng="la">Astragalus</trd>).
   </dif>
   <trd lng="de">Tragant</trd>
   <trd lng="fr">astragale (plante)</trd>
</snc>
<snc mrk="astrag.0o.ARKI">
   <uzo tip="fak">ARKI</uzo>
   <dif>
     Modlura ornamo, kiu  &ccirc;irka&ubreve;as la supran ekstremon de kolono:
     <ekz>
       <tld/>oj trovi&gcirc;as &ccirc;efe &ccirc;e romanordaj kolonoj.
     </ekz>
   </dif>
   <trd lng="fr">astragale (moulure)</trd>
</snc>
<snc mrk="astrag.0o.ANA">
   <uzo tip="fak">ANA</uzo>
   <dif>
     La artikaj kalkaneaj surfacoj: malanta&ubreve;a k meza.
   </dif>
</snc>
   <trd lng="fr">astragale (os)</trd>
<trd lng="ru">&#1072;&#1089;&#1090;&#1088;&#1072;&#1075;&#1072;&#1083;</trd>
</drv>
</art>
