<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cxokol.xml,v 1.37 2023/10/31 13:37:04 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&ccirc;okolad</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="cxokol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Bongusta nutra&jcirc;o farita el <ref tip="prt" cel="kaka.0o.KOMUNE">kakao</ref> kaj sukero:
      <ekz>
        museton el <tld/>o <klr>[&#x2026;]</klr> mia patrino donis <klr>[&#x2026;]</klr> al mi
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> prezentis al ili papersaketon kun <tld/>aj bombonoj de la plej
        bona speco
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi mem ordigus &ccirc;iun unuopan kuketon <klr>[&#x2026;]</klr> kun la <tld/>a flanko supre
        <fnt>
          <bib>OIP</bib>
          <lok>Ombro sur interna pejza&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la okcidentaj butikoj por solidara komerco, ne estas nuraj
        vendejoj de kafo, <tld/>o, teo, sukero, bananoj, mielo,
        infuza&jcirc;oj
        <fnt>
          <aut>Gianfranco Cazzaro</aut>:
          <vrk>Morale a&ccirc;eti, konscie subteni</vrk>,
          <bib>Monato</bib><lok>jaro 1994a, numero 6a, p. 11a</lok>
        </fnt>;
      </ekz>
      <ekz>
        diverslingvaj surskriboj informas pri <tld/>o kun kanabo &ndash; populara droga
        vegeta&jcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Antanas Grincevi&ccaron;ius</aut>
          <vrk><url ref="https://www.monato.be/2003/007879.html">&Ccirc;okolada konflikto</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        aktuale furoras &leftquot;altklasa kaj japaneca&rightquot; <tld/>o kun aromo de verda teo
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="https://www.monato.be/2006/008568.php">Februaraj festoj</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        lakto<tld/>o male perdigas parton de tiu efiko
        <fnt>
          <bib>Monato</bib>
          <aut>Paulo Sergio Viana</aut>
          <vrk><url ref="https://www.monato.be/2014/011354.php">&Ccirc;okolado, nia plej kora amiko (sed sen troigo!)</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Amelia</nom> pli ofte nutras sin per <tld/>biskvitoj
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="https://www.monato.be/2008/009326.php">Analoj de Bailenambeann</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        fama <tld/>ejo
        <fnt>
          <bib>LOdE</bib>
          <lok>2004, &#x2116; 2 (112)</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> ricevas etan premion <klr>[&#x2026;]</klr>, skatolon da <tld/>a&jcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="https://www.monato.be/2012/010647.php">Krimoj senviktimaj</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        Bastjano sopirante rememoris pri du tabuletoj da nukso<tld/>o,
        kiujn li hejme konservis en la nokto&scirc;ranketo
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Vo&ccirc;o de la Kvieto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="cxokol.0o.trinka">
    <dif>
      <ref tip="dif" cel="cxokol.trink0o">Trink<tld/>o</ref>:
      <ekz>
        maljuna markizino <klr>[&#x2026;]</klr> invitis lin al si por trinki <tld/>on
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        pure vestitaj junulinoj prezentis <tld/>on, kiun ili tre bone &scirc;a&ubreve;mkirlis
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXV</lok>
        </fnt>;
      </ekz>
      <ekz>
        tran&ccirc;i al mi la gor&gcirc;on? veneni min per vino a&ubreve; <tld/>o
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>?
      </ekz>
    </dif>
    <trd lng="de"><klr>(hei&szlig;e) </klr>Schokolade</trd>
  </snc>
  <trd lng="be">&c_sh;&c_a;&c_k;&c_a;&c_l;&c_ja;&c_d;</trd>
  <trd lng="ca">xocolata</trd>
  <trd lng="el">&sigma;&omikron;&kappa;&omikron;&lambda;&alfa_ton;&tau;&alfa;</trd>
  <trd lng="en">chocolate</trd>
  <trd lng="es">chocolate</trd>
  <trd lng="de">Schokolade</trd>
  <trd lng="fa">&a_sh;&f_k;&a_l;&a_A;&a_t;</trd>
  <trd lng="fr">chocolat</trd>
  <trd lng="he">&shin;&vav;&qof;&vav;&lamed;&dalet;</trd>
  <trd lng="hi">&#x91A;&#x949;&#x915;&#x932;&#x947;&#x91F; [<ind>c&ocirc;kl&#x113;&#x1E6D;</ind>]</trd>
  <trd lng="hu">csokol&aacute;d&eacute;</trd>
  <trd lng="id"><ind>cokelat</ind> (makanan)</trd>
  <trd lng="it">cioccolato</trd>
  <trdgrp lng="ja">
    <trd>&#x30C1;&#x30E7;&#x30B3;&#x30EC;&#x30FC;&#x30C8;</trd>,
    <trd>&#x30B3;&#x30B3;&#x30A2;  <pr>&#x3053;&#x3053;&#x3042;</pr></trd>
  </trdgrp>
  <trd lng="nl">chocolade</trd>
  <trd lng="pl">czekolada</trd>
  <trd lng="pt">chocolate</trd>
  <trd lng="ru">&c_sh;&c_o;&c_k;&c_o;&c_l;&c_a;&c_d;</trd>
  <trd lng="sk">&ccaron;okol&aacute;da</trd>
  <trd lng="sv">choklad</trd>
  <trd lng="tr">&ccedil;ikolata</trd>
  <trd lng="uk">&c_sh;&c_o;&c_k;&c_o;&c_l;&c_a;&c_d;</trd>
  <trd lng="vo">jokolad</trd>
  <trd lng="zh">&#x5DE7;&#x514B;&#x529B;<klr> [qi&#x1CE;ok&egrave;l&igrave;]</klr></trd>
</drv>

<drv mrk="cxokol.trink0o">
  <kap>trink<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="&Ccirc;"/>o preparita kiel plejofte varma
      <ref tip="lst" lst="voko:trinka&jcirc;oj" cel="trink.0ajxo">trinka&jcirc;o</ref>,
      mikse kun  akvo a&ubreve; lakto:
      <ekz>
        kvar tasoj da varmega trink<tld/>o<fnt><aut>Pejno Simono</aut>:
        <vrk>La lernemulo</vrk>, <bib>Monato</bib><lok>jaro 1999a,
        numero
        6a, p. 23a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="kaka.0o.KUI">kakao<sncref/></ref>
  </snc>
  <trd lng="be">&c_sh;&c_a;&c_k;&c_a;&c_l;&c_ja;&c_d;
  <klr>(&c_n;&c_a;&c_p;&c_ib;&c_t;&c_a;&c_k;)</klr></trd>
  <trd lng="ca"><ind>xocolata</ind> desfeta</trd>
  <trdgrp lng="en">
    <trd>chocolate milk</trd>,
    <trd>hot <ind>chocolate</ind></trd>
  </trdgrp>
  <trd lng="de">Trinkschokolade</trd>
  <trdgrp lng="fa">
    <trd>&f_k;&a_A;&f_k;&a_A;&a_y_hamza;&a_w;</trd>,
    <trd>&a_sh;&f_y;&a_r;&f_k;&a_A;&f_k;&a_A;&a_y_hamza;&a_w;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>chocolat<klr> (boisson)</klr></trd>,
    <trd>chocolat chaud</trd>
  </trdgrp>
  <trd lng="he">&shin;&vav;&qof;&vav;</trd>
  <trd lng="hu">csokol&aacute;d&eacute;ital</trd>
  <trdgrp lng="id">
    <trd><ind>cokelat</ind> panas</trd>,
    <trd><ind>susu</ind> cokelat</trd>
  </trdgrp>
  <trd lng="it">cioccolata</trd>
  <trdgrp lng="nl">
    <trd>cacao<klr tip="ind"> (om te drinken)</klr></trd>,
    <trd>chocolade<klr tip="ind"> (om te drinken)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>czekolada<klr tip="amb"> do picia</klr></trd>,
    <trd>gor&aogonek;ca <ind>czekolada</ind></trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x70ED;&#x5DE7;&#x514B;&#x529B; <pr>r&#xE8;qi&#x1CE;ok&#xE8;l&#xEC;</pr></trd>,
    <trd>&#x70ED;&#x5DE7;&#x514B; <pr>r&#xE8;qi&#x1CE;ok&#xE8;</pr></trd>
  </trdgrp>
</drv>
</art>
