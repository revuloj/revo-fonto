<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mercer</rad>/o
</kap>
<drv mrk="mercer.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Komerco per diversspecaj akcesora&jcirc;oj kaj objektoj, kiel ekz.:
      kudrilaro, butonoj, fadenoj, pingloj kaj aliaj ornama&jcirc;oj uzataj &ccirc;e
      kudr-, vest-, ornam- kaj virinlaboroj.
    </dif>
    <refgrp tip="sin">
      <ref cel="galant2.0o">galanterio</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Kurzwaren</trd>,
    <trd>Merceriewaren [CH]</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_v;&c_je;&c_j;&c_n;&c_a;&c_ja; &c_f;&c_u;&c_r;&c_n;&c_i;&c_t;&c_u;&c_r;&c_a;</trd>
  </trdgrp>
</drv>
</art>
