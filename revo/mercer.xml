<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mercer.xml,v 1.17 2024/10/22 10:33:26 revo Exp $">
<kap>
  <rad>mercer</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="mercer.0o">
  <kap><tld/>o</kap>
  <snc mrk="mercer.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="komerc.0o">Komerco</ref> per
      bezona&jcirc;oj de <ref tip="super" cel="kudr.0i">kudrado</ref>: kudrilaro, butonoj, fadenoj ktp:
      <ekz>
        en <tld/>ejo &scirc;i estis elektanta silkon por la salivtuketo
        <fnt>
          <aut>Karel &Ccaron;apek</aut>
          <vrk><url ref="http://www.literatura.bucek.name/capekk/capek-karel_posrakontoj.doc"
              >Rakontoj el unua po&scirc;o</url></vrk>
          <lok>Historio Kun Infano</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="galant2.0o">galanterio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_f;&c_u;&c_r;&c_n;&c_ib;&c_t;&c_u;&c_r;&c_a; <klr tip="amb">(&c_sh;&c_a;&c_v;&c_je;&c_c;&c_k;&c_a;&c_ja;)</klr></trd>
  <trd lng="ca">merceria</trd>
  <trd lng="cs">galantern&iacute; zbo&zcaron;&iacute;</trd>
  <trdgrp lng="de">
    <trd>Kurzwaren</trd>,
    <trd>Merceriewaren [CH]</trd>
  </trdgrp>
  <trd lng="fr">mercerie<klr> (activit&eacute;)</klr></trd>
  <trd lng="hu">r&ouml;vid&aacute;ru</trd>
  <trdgrp lng="ja">
    <trd>&#23567;&#38291;&#29289;  <pr>&#12371;&#12414;&#12418;&#12398;</pr></trd>,
    <trd>&#26381;&#39166;&#38609;&#36008;  <pr>&#12405;&#12367;&#12375;&#12423;&#12367;&#12374;&#12387;&#12363;</pr></trd>,
    <trd>&#25163;&#33464;&#29992;&#21697;  <pr>&#12375;&#12421;&#12370;&#12356;&#12424;&#12358;&#12402;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="pl">dodatki krawieckie {pl}</trd>
  <trd lng="ro">accesorii de &icirc;mbr&abreve;c&abreve;minte {pl}</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_v;&c_je;&c_j;&c_n;&c_a;&c_ja; &c_f;&c_u;&c_r;&c_n;&c_i;&c_t;&c_u;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">predaj &scaron;ijac&iacute;ch potrieb a&nbsp;ozd&ocirc;b</trd>
  <trd lng="uk">&c_gu;&c_a;&c_l;&c_a;&c_n;&c_t;&c_je;&c_r;&c_je;&c_ja;</trd>
</drv>

<drv mrk="mercer.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="mercer.0ajxo.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Materialo, a&jcirc;o por vestfarado:
      <ekz>
        &scirc;i havis grandan butikon kiu ofertis &ccirc;iuspecajn
        spicojn, tukojn, vi&scirc;ilojn, &ccirc;iajn ajn <tld/>a&jcirc;ojn
        <fnt>
          <aut>A. LeQuint</aut>,
          <vrk>Infantraviva&jcirc;oj</vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_l;&c_ja;&c_n;&c_t;&c_a;&c_r;&c_e;&c_j;&c_n;&c_y; <ind>&c_v;&c_y;&c_r;&c_a;&c_b;</ind></trd>,
    <trd>&c_g;&c_a;&c_l;&c_ja;&c_n;&c_t;&c_a;&c_r;&c_e;&c_ja;</trd>
  </trdgrp>
  <trd lng="ca">producte de <ind>merceria</ind></trd>
  <trd lng="de">N&auml;hutensil</trd>
  <trd lng="fr">article de <ind>mercerie</ind></trd>
  <trd lng="pl">pasmanteria</trd>
  <trd lng="ro">mercerie</trd>
  <trd lng="uk">&c_gu;&c_a;&c_l;&c_a;&c_n;&c_t;&c_je;&c_r;&c_je;&c_j;&c_n;&c_i;&c_j; &c_v;&c_i;&c_r;&c_ib;&c_b;</trd>
</drv>
</art>
