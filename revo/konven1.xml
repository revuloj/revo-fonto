<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>konvenci</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="konven1.0o">
  <kap><tld/>o</kap>
  <snc mrk="konven1.0o.gxenerale">
    <dif>
      Tio, kio estas starigita per esprimita a&ubreve; neesprimita
      konsento inter la homoj; interakceptitaj fiktiva&jcirc;oj:
      <ekz>
        sociaj <tld/>oj
        (<ref tip="vid" cel="dec">deco</ref>);
      </ekz>
      <ekz>
        teatraj <tld/>oj;
      </ekz>
      <ekz>
        ili baras al si la sanan naturon per sensencaj
        <tld/>oj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a valoro de la papermono;
      </ekz>
      <ekz>
        <tld/>a alfabeto, &jcirc;argono
      </ekz>
      <ref tip="vid" cel="kondic1.0a">kondi&ccirc;a</ref>.
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_a;&c_n;&c_v;&c_e;&c_n;&c_c;&c_y;&c_ja;</trd>,
      <trd>&c_p;&c_a;&c_g;&c_a;&c_d;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Konvention</trd>,
      <trd>&Uuml;bereinkunft</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>convention</trd>,
      <trd>general usage</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>koncenci&oacute;</trd>,
      <trd>bevett szok&aacute;s</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_o;&c_g;&c_l;&c_a;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_u;&c_s;&c_l;&c_o;&c_v;&c_n;&c_o;&c_s;&c_t;&c_mol;<klr tip="amb"> (&c_o;&c_b;&c_y;&c_ch;&c_n;&c_o; &c_v;&c_o; &c_m;&c_n;.&c_ch;.)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="konven1.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Interregna, internacia <ref tip="super" cel="kontra3.0o">kontrakto</ref>:
      <ekz>
        <tld/>o gardanta la a&ubreve;torajn rajtojn;
      </ekz>
      <ekz>
        la po&scirc;ta <tld/>o de Bern;
      </ekz>
      <ekz>
        <tld/>a kla&ubreve;zo.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="trakta.0o.JUR">traktato<sncref/></ref>,
      <ref cel="konsen">interkonsento</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_k;&c_a;&c_n;&c_v;&c_e;&c_n;&c_c;&c_y;&c_ja;</trd>,
      <trd>&c_p;&c_a;&c_g;&c_a;&c_d;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trd lng="de">Abkommen</trd>
    <trd lng="en">convention</trd>
    <trd lng="hu">&aacute;llamk&ouml;zi <ind>egyezm&eacute;ny</ind></trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_o;&c_g;&c_l;&c_a;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_k;&c_o;&c_n;&c_v;&c_je;&c_n;&c_c;&c_i;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc mrk="konven1.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Nacia eksterordinara asembleo:
      <ekz>
        la Franca <tld/>o (1792&ndash;1795);
      </ekz>
      <ekz>
        <tld/>ano.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_n;&c_v;&c_e;&c_n;&c_t;</trd>
    <trdgrp lng="de">
      <trd>Konvent</trd>,
      <trd>konstituierende Versammlung</trd>
    </trdgrp>
    <trd lng="en">convention</trd>
    <trdgrp lng="hu">
      <trd>konvent</trd>,
      <trd>alkotm&aacute;nyoz&oacute; nemzetgy&udblac;l&eacute;s</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_o;&c_n;&c_v;&c_je;&c_n;&c_t;</trd>
  </snc>
</drv>

<drv mrk="konven1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al a&ubreve; rezultanta el <tld/>o.
    </dif>
  </snc>
</drv>
</art>
