<vortaro>
  <art mrk="$Id: grenadin.xml,v 1.1 2024/10/12 15:26:52 afido Exp $">
    <kap>
      <ofc>10</ofc><rad>Grenadin</rad>/oj
    </kap>

<drv mrk="grenadin.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="grenadin.0oj.insularo">
    <dif>
      Insularo inter la karibaj insuloj Grenado kaj Sankta Vincento:
      <ekz>
        <tld/>oj <klr>[&#8230;]</klr> enhavas pli da 600 insuloj a&ubreve; insuletoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=227354"
              >Grenadinoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="vincent.Sankta_0o_kaj_Grenadinoj">Sankta Vincento kaj Grenadinoj</ref>
    <trd lng="de">Grenadinen</trd>
    <trd lng="en">Grenadines</trd>
    <trd lng="fr">Grenadines</trd>
  </snc>
</drv>

  </art>

