    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x4E0D;&#x53D8; <pr>b&#xF9;bi&#xE0;n</pr></trd>,
    <trd>&#x4E0D;&#x8B8A; <pr>b&#xF9;bi&#xE0;n</pr></trd>,
