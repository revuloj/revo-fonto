<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: arangx.xml,v 1.35 2020/12/10 15:10:32 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>aran&gcirc;</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="arangx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="arangx.0i.ordigi">
    <dif>
      Meti la&ubreve; &gcirc;usta ordo, <ref cel="ord.0igi" tip="sin">ordigi</ref>
      oportune:
      <ekz>
        <tld/>i &ccirc;ambron, liton;
      </ekz><ekz>
        <tld/>i librojn en biblioteko;
      </ekz><ekz>
        la du armeoj sin <tld/>is kontra&ubreve; la komuna
        malamiko<fnt>Z</fnt>;
      </ekz><ekz>
        kiel <tld/>i tiamaniere, ke la estraro vidu mian fervoron kaj
        estu kontenta
        <fnt><bib>Revizoro</bib>, <lok>akto 3a, sceno 5a</lok></fnt>?
      </ekz><ekz>
        ord<tld/>i
        <fnt><bib>Fab2</bib>, <lok>Malnova Domo</lok></fnt>;
      </ekz><ekz>
        vort<tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>anordnen</trd>,
      <trd>einrichten</trd>,
      <trd>ordnen</trd>,
      <trd>richten <klr>(ordnen)</klr></trd>,
      <trd>zurecht machen</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>ordenar</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>arranger</trd>,
      <trd>mettre en <ind>ordre</ind></trd>,
      <trd>ranger</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>arranjar</trd>,
      <trd>arrumar</trd>,
      <trd>ordenar</trd>
    </trdgrp>
  </snc>
  <snc mrk="arangx.0i.pretigi">
    <dif>
      <ref cel="pret.0igi">Pretigi</ref>, 
      <ref tip="sin" cel="organi2.0i">organizi</ref>, 
      <ref tip="sin" cel="prepar.0i">prepari</ref>, 
      <ref cel="zorg.pri0i">prizorgi</ref>,
      okazigi:
      <ekz>
        <tld/>i kongreson, feston,
        interkomuniki&gcirc;adon, belan akcepton;
      </ekz><ekz>
        li venis tien &ccirc;i nur por <tld/>i disputojn
        <fnt>
          <aut>L. Dilling, trad. E. Hall</aut>,
          <vrk>La Porcio da Glacia&jcirc;o</vrk>,
          <bib>FK</bib>
        </fnt>;
      </ekz><ekz>
        sin <tld/>i komforte;
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>vorbereiten</trd>,
      <trd>organisieren</trd>,
      <trd>veranstalten</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>arreglar</trd>,
      <trd>preparar<klr> (dejar las cosas listas y en orden)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>am&eacute;nager</trd>,
      <trd>arranger</trd>,
      <trd>r&eacute;gler</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>arranjar</trd>,
      <trd>preparar</trd>,
      <trd>organizar</trd>
    </trdgrp>
    <trd lng="sk">usporiada&tcaron;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_l;&c_a;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_l;&c_a;&c_d;&c_k;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_p;&c_a;&c_r;&c_a;&c_d;&c_k;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>adobar</trd>,
    <trd>arreglar</trd>
  </trdgrp>
  <trd lng="en">arrange</trd>
  <trdgrp lng="es">
    <trd>ordenar</trd>,
    <trd>arreglar<klr tip="amb"> (una habitaci&oacute;n)</klr></trd>,
    <trd>colocar<klr> (cosas en su sitio)</klr></trd>
  </trdgrp>
  <trd lng="oc">adobar</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_t;&c_r;&c_o;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_s;&c_t;&c_r;&c_a;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="arangx.0o">
  <kap><tld/>o</kap>
  <snc mrk="arangx.0o.KOMUNE">
    <dif>
      Io <tld/>ita, <ref tip="super" cel="manier.0o">maniero</ref>
      <tld/>i:
      <ekz>
        pla&ccirc;as al mi la <tld/>o de via salono;
      </ekz><ekz>
        jen estas la <tld/>o de la kandelabro: for&gcirc;ita el oro, de &gcirc;ia
        trunko &gcirc;is &gcirc;iaj floroj, for&gcirc;ita &gcirc;i estis
        <fnt><bib>MT</bib><lok>&Nom; 8:4.</lok></fnt>;
      </ekz><ekz>
        fari ta&ubreve;gan <tld/>on;
      </ekz><ekz>
        de lia vivo <tld/>o estas trinko kaj man&gcirc;o
         <fnt><bib>PrV</bib></fnt>;
      </ekz><ekz>
        brila <tld/>o de kongreso.
      </ekz>
    </dif>
    <ref tip="vid" cel="ord.0o">ordo</ref>
    <trdgrp lng="de">
      <trd>Anordnung</trd>,
      <trd>Einrichtung</trd>,
      <trd>Gestaltung</trd>
    </trdgrp>
  </snc>
    <trdgrp lng="fr">
      <trd>am&eacute;nagement</trd>,
      <trd>disposition</trd>,
      <trd>organisation</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_k;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_m;&c_je;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_u;&c_s;&c_t;&c_r;&c_o;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>
    </trdgrp>
  <snc mrk="arangx.0o.evento">
    <dif>
      Organizita publika a&ubreve; privata agado en formo de renkonti&gcirc;o,
      voja&gcirc;o, kongreso ktp:
      <ekz>
        kalendaro de Esperanto-<tld/>oj;
      </ekz><ekz>     
        li, kiel mastr&#8217;, ekskurson en kampojn preparis /
	kaj en domo <tld/>ojn por amuzo faris
        <fnt><aut>A. Mickiewicz tr. A. Grabowski</aut>:
          <vrk>Sinjoro Tadeo</vrk>, <lok>1:844&#173;845.</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="event.0o">evento</ref>
    </refgrp>
    <trd lng="be">&c_m;&c_je;&c_r;&c_a;&c_p;&c_r;&c_y;&c_je;&c_m;&c_s;&c_t;&c_v;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Veranstaltung</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>distribuci&oacute;n</trd>,
      <trd>disposici&oacute;n</trd>,
      <trd>organizaci&oacute;n</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>am&eacute;nagement</trd>,
      <trd>disposition</trd>,
      <trd>organisation</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>arranjo</trd>,
      <trd>arruma&ccedil;&atilde;o</trd>,
      <trd>disposi&ccedil;&atilde;o</trd>,
      <trd>ordem</trd>
    </trdgrp>
    <trd lng="ru">&c_m;&c_je;&c_r;&c_o;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="arangx.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Maniero <tld/>i datumojn sur datumportilo;
      <ekz>
        sur paperfolio oni povas &gcirc;israndigi la tekston dekstren,
        a&ubreve; uzi alian tekst<tld/>on.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_s;&c_k;&c_l;&c_a;&c_d;</trd>,
      <trd>&c_l;&c_a;&c_d;</trd>,
      <trd>&c_ch;&c_y;&c_n;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Layout</trd>,
      <trd>Gestaltung</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>layout</trd>, <trd>format</trd>; <trd>alignment</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>alineaci&oacute;n</trd>,
      <trd>formato</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>disposer</trd>,
      <trd>mettre en <ind>page</ind></trd>,
      <trd>formater<klr tip="ind"> (un texte)</klr></trd>
    </trdgrp>
    <trd lng="pt">formata&ccedil;&atilde;o</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_m;&c_je;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_k;&c_o;&c_m;&c_p;&c_o;&c_n;&c_o;&c_v;&c_k;&c_a;</trd>,
      <trd>&c_f;&c_o;&c_r;&c_m;&c_a;&c_t;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="arangx.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="arangx.0ajxo.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 14</lok></fnt>
    <dif>
      <klr>(de <frm am="p">p</frm> el <frm am="n">n</frm>,
      de <frm am="n">n</frm> po <frm am="p">p</frm>)</klr>
      &Ccirc;iu el la diversaj manieroj vicigi <frm am="p">p</frm> objektojn
      el <frm><k>n</k></frm>-elementa aro:
      <ekz>
        la nombron de &ccirc;iuj <tld/>a&jcirc;oj de <frm am="n">n</frm> po
        <frm am="p">p</frm> oni signas per
        <frm
        am="A_n^p=n(n-1)(n-2)...(n-p+2)(n-p+1)">A<sup>p</sup><sub>n</sub>
        = n(n-1)(n-2)...(n-p+2)(n-p+1)</frm>;
      </ekz><ekz>
        la ses <tld/>a&jcirc;oj de du elementoj el la aro
        <frm am="{a,b,c}">{a,b,c}</frm> estas
        <frm am="ab">ab</frm>, <frm am="ac">ac</frm>,
        <frm am="bc">bc</frm>, <frm am="ba">ba</frm>,
        <frm am="ca">ca</frm>, <frm am="cb">cb</frm>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kombin.0ajxo.MAT">kombina&jcirc;o</ref>,
      <ref cel="permut.0ajxo">permuta&jcirc;o</ref>
    </refgrp>
    <trd
    lng="be">&c_r;&c_a;&c_z;&c_mol;&c_m;&c_ja;&c_sh;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
    <trd lng="de">Variation<klr tip="amb"> (von n Elementen zur p-ten
    Klasse)</klr></trd>
    <trd lng="en">permutation<klr tip="amb"> (of n things taken p at a
    time)</klr></trd>
    <trdgrp lng="es">
      <trd>permutaci&oacute;n<klr tip="amb"> (de n elementos tomados de
      p en p)</klr></trd>,
      <trd>combinaci&oacute;n<klr tip="amb"> (de n elementos tomados de
      p en p)</klr></trd>
    </trdgrp>
    <trd lng="fr">arrangement<klr tip="amb"> (de n &eacute;l&eacute;ments pris
    p &agrave; p)</klr></trd>
    <trd lng="pl">wariacja<klr tip="ind"> (p-elementowa zbioru
    n-elementowego)</klr></trd>
    <trdgrp lng="pt">
      <trd>permuta&ccedil;&atilde;o</trd>,
      <trd>arranjo</trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_m;&c_je;&c_shch;&c_je;&c_n;&c_i;&c_je;<klr tip="amb"> (&c_i;&c_z; n &c_p;&c_o; p)</klr></trd>
    <trd lng="sk">usporiadanie</trd>
  </snc>
  <snc mrk="arangx.0ajxo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Ilo, aparato a&ubreve; organo <tld/>ita por plenumi funkcion
      a&ubreve; laboron.
    </dif>
    <trd lng="be">&c_p;&c_r;&c_y;&c_l;&c_a;&c_d;&c_a;</trd>
    <trd lng="de">Ger&auml;t</trd>
    <trd lng="en">device</trd>
    <trd lng="es">dispositivo</trd>
    <trd lng="fr">dispositif</trd>
    <trd lng="pt">dispositivo</trd>
    <trd lng="ru">&c_u;&c_s;&c_t;&c_r;&c_o;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>
  </snc>
</drv>

<drv mrk="arangx.0ema">
  <kap><tld/>ema<fnt>Z</fnt></kap>
  <snc mrk="arangx.0ema.KOMUNE">
    <dif>
      Elturni&gcirc;ema, elpensema, eltrovema,
      <ref tip="vid" cel="ingxeni.0a">in&gcirc;enia</ref>.
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd>ingenioso</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>astucieux</trd>,
    <trd>ing&eacute;nieux</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_h;&c_o;&c_d;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>
</drv>

<drv mrk="arangx.al0i">
  <kap>al<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="arangx.al0i.KOMUNE">
    <dif>
      <ref tip="vid" cel="adapt.0i">Adapti</ref>,
      bone kunmeti kun:
      <ekz>
        juna fra&ubreve;lino <klr>...</klr> en tiu momento estis al<tld/>anta
        multekostan plumon de struto al velura &ccirc;apelo
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">anpassen</trd>
  <trdgrp lng="es">
    <trd>adaptar</trd>,
    <trd>ajustar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>adapter</trd>,
    <trd>ajuster</trd>
  </trdgrp>
  <trd lng="pt">adaptar</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_l;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_l;&c_a;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="arangx.antaux0i">
  <kap>anta&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Anticipe <tld/>i:
      <ekz>
        ni estas Lia farita&jcirc;o, kreitaj en Kristo Jesuo por bonaj faroj,
        kiujn anta&ubreve;<tld/>is Dio, por ke ni iradu en ili
        <fnt><bib>NT</bib>, <lok>&Efe; 2:10</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd>organizar anticipadamente</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>arranger d'avance</trd>,
    <trd>disposer<klr tip="amb"> (en vue de)</klr></trd>,
    <trd>organiser d'avance</trd>
  </trdgrp>
</drv>

<drv mrk="arangx.har0o">
  <kap>har<tld/>o</kap>
  <snc mrk="arangx.har0o.FRIZ">
    <dif>
      Arta meto de haroj, precipe de la kapharoj:
      <ekz>
        la har<tld/>o de la virinoj multe &scirc;an&gcirc;i&gcirc;is de kelkaj jaroj.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Frisur</trd>
  <trd lng="es">peinado</trd>
  <trd lng="fr">coiffure<klr tip="ind"> (mani&egrave;re de se
  coiffer)</klr></trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_ch;&c_jo;&c_s;&c_k;&c_a;</trd>
</drv>

<drv mrk="arangx.kun0o">
  <kap>kun<tld/>o</kap>
  <snc mrk="arangx.kun0o.KOMUNE">
    <dif>
      Kunordigo.
    </dif>
  </snc>
  <trd lng="es">ordenaci&oacute;n</trd>
  <trd lng="fr">ordonnancement</trd>
  <trd lng="pt">ordena&ccedil;&atilde;o</trd>
</drv>

<drv mrk="arangx.mal0i">
  <kap>mal<tld/>i</kap>
  <snc mrk="arangx.mal0i.KOMUNE">
    <dif>
      Malordigi.
    </dif>
    <refgrp tip="vid">
      <ref cel="konfuz.0i">konfuzi</ref>,
      <ref cel="help.mal0i">malhelpi</ref>,
      <ref cel="pertur.0i">perturbi</ref>,
      <ref cel="renver.0i">renversi</ref>,
      <ref cel="ruin.0igi">ruinigi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_l;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="es">
    <trd>desordenar</trd>,
    <trd>desorganizar</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;ranger</trd>
  <trdgrp lng="pt">
    <trd>desarranjar</trd>,
    <trd>desarrumar</trd>,
    <trd>bagun&ccedil;ar</trd>,
    <trd>desordenar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_v;&c_je;&c_s;&c_t;&c_i; &c_v; <ind>&c_b;&c_je;&c_s;&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_k;</ind></trd>,
    <trd>&c_r;&c_a;&c_z;&c_l;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_s;&c_t;&c_r;&c_o;&c_i;&c_t;&c_mol;<klr tip="amb"> (&c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>
  </trdgrp>
</drv>
</art>
