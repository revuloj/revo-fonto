<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>aran&gcirc;</rad>/i
</kap>
<drv mrk="arangx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="arangx.0i.ordigi">
    <dif>
      Meti la&ubreve; &gcirc;usta ordo, ordigi:
      <ekz>
        <tld/>i &ccirc;ambron, liton;
      </ekz>
      <ekz>
        <tld/>i librojn en biblioteko;
      </ekz>
      <ekz>
        la du armeoj sin <tld/>is kontra&ubreve; la komuna
        malamiko<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ord<tld/>i
        <fnt><bib>Fab2</bib>, <lok>Malnova domo</lok></fnt>;
      </ekz>
      <ekz>
        vort<tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>arranger</trd>,
      <trd>mettre en <ind>ordre</ind></trd>,
      <trd>ranger</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>arranjar</trd>,
      <trd>arrumar</trd>,
      <trd>ordenar</trd>
    </trdgrp>
  </snc>
  <snc mrk="arangx.0i.pretigi">
    <dif>
      Pretigi, organizi, prepari, prizorgi:
      <ekz>
        <tld/>i kongreson, feston,
        interkomuniki&gcirc;adon, belan akcepton;
      </ekz>
      <ekz>
        sin <tld/>i komforte;
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>arranger</trd>,
      <trd>organiser</trd>,
      <trd>r&eacute;gler</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>arranjar</trd>,
      <trd>preparar</trd>,
      <trd>organizar</trd>
    </trdgrp>  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_l;&c_a;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>, 
    <trd>&c_u;&c_l;&c_a;&c_d;&c_k;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>, 
    <trd>&c_u;&c_p;&c_a;&c_r;&c_a;&c_d;&c_k;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">arangieren</trd>
  <trd lng="en">arrange</trd>
  <trd lng="ru">&c_u;&c_s;&c_t;&c_r;&c_a;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="arangx.0o">
  <kap><tld/>o</kap>
  <snc mrk="arangx.0o.IoAuManiero">
    <dif>
      Io <tld/>ita, maniero <tld/>i:
      <ekz>
        kalendaro de Esperanto-<tld/>oj,
      </ekz>
      <ekz>
        pla&ccirc;as al mi la <tld/>o de via salono,
      </ekz>
      <ekz>
        fari ta&ubreve;gan <tld/>on.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_je;&c_r;&c_a;&c_p;&c_r;&c_y;&c_je;&c_m;&c_s;&c_t;&c_v;&c_a;</trd>
    <trdgrp lng="fr">
      <trd>arrangement</trd>,
      <trd>disposition</trd>,
      <trd>manifestation (&eacute;v&eacute;nement organis&eacute;)</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>arranjo</trd>,
      <trd>arruma&ccedil;&atilde;o</trd>,
      <trd>disposi&ccedil;&atilde;o</trd>,
      <trd>ordem</trd>
    </trdgrp>    
   <trd lng="ru">&c_m;&c_je;&c_r;&c_o;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="arangx.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Maniero <tld/>i datumojn sur datumportilo;
      <ekz>
        sur paperfolio oni povas &gcirc;israndigi la tekston dekstren,
        a&ubreve; uzi alian teksto<tld/>on.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_s;&c_k;&c_l;&c_a;&c_d;</trd>, 
      <trd>&c_l;&c_a;&c_d;</trd>, 
      <trd>&c_ch;&c_y;&c_n;</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>layout</trd>, <trd>format</trd>; <trd>alignment</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>disposer</trd>,
      <trd>mettre en <ind>page</ind></trd>,
      <trd>formater<klr tip="ind"> (un texte)</klr></trd>
    </trdgrp>
    <trd lng="pt">formata&ccedil;&atilde;o</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_m;&c_je;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_k;&c_o;&c_m;&c_p;&c_o;&c_n;&c_o;&c_v;&c_k;&c_a;</trd>,
      <trd>&c_f;&c_o;&c_r;&c_m;&c_a;&c_t;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="arangx.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="arangx.0ajxo.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 14</lok></fnt>
    <dif>
      <klr>(de <frm><k>p</k></frm> el <frm><k>n</k></frm>, 
      de <frm><k>n</k></frm> po <frm><k>p</k></frm>)</klr>
      &Ccirc;iu el la diversaj manieroj vicigi <frm><k>p</k></frm> objektojn
      el <frm><k>n</k></frm>-elementa aro:
      <ekz>
        la nombron de &ccirc;iuj <tld/>a&jcirc;oj de <frm><k>n</k></frm> po 
        <frm><k>p</k></frm> oni signas per 
        <frm>A<sup><k>p</k></sup><sub><k>n</k></sub> = 
        <k>n</k>(<k>n</k>-1)(<k>n</k>-2)...
        (<k>n</k>-<k>p</k>+2)(<k>n</k>-<k>p</k>+1)</frm>;
      </ekz>
      <ekz>
        la ses <tld/>a&jcirc;oj de du elementoj el la aro 
        <frm>{<k>a</k>,<k>b</k>,<k>c</k>}</frm> estas
        <frm><k>ab</k></frm>, <frm><k>ac</k></frm>, 
        <frm><k>bc</k></frm>, <frm><k>ba</k></frm>, 
        <frm><k>ca</k></frm>, <frm><k>cb</k></frm>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kombin.0ajxo.MAT">kombina&jcirc;o</ref>,
      <ref cel="permut.0ajxo">permuta&jcirc;o</ref>.
    </refgrp>
    <trd lng="be">&c_r;&c_a;&c_z;&c_mol;&c_m;&c_ja;&c_sh;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
    <trd lng="de">Variation<klr tip="amb"> (von n Elementen zur p-ten Klasse)</klr></trd>
    <trd lng="en">permutation<klr tip="amb"> (of n things taken p at a time)</klr></trd>
    <trd lng="pl">wariacja<klr tip="ind"> (p-elementowa zbioru n-elementowego)</klr></trd>
    <trdgrp lng="pt">
      <trd>permuta&ccedil;&atilde;o</trd>,
      <trd>arranjo</trd>
    </trdgrp>
    <trd lng="fr">arrangement<klr tip="amb"> (de n &eacute;l&eacute;ments pris p &agrave; p)</klr></trd>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_m;&c_je;&c_shch;&c_je;&c_n;&c_i;&c_je;<klr tip="amb"> (&c_i;&c_z; n &c_p;&c_o; p)</klr></trd>
  </snc>
  <snc mrk="arangx.0ajxo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Ilo, aparato a&ubreve; organo <tld/>ita por plenumi funkcion
      a&ubreve; laboron.
    </dif>
    <trd lng="be">&c_p;&c_r;&c_y;&c_l;&c_a;&c_d;&c_a;</trd>
    <trd lng="de">Ger&auml;t</trd>
    <trd lng="en">device</trd>
    <trd lng="fr">dispositif</trd>
    <trd lng="pt">dispositivo</trd>
    <trd lng="ru">&c_u;&c_s;&c_t;&c_r;&c_o;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>
  </snc>
</drv>

<drv mrk="arangx.0ema">
  <kap><tld/>ema<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Elturni&gcirc;ema, elpensema, eltrovema.
    </dif>
  </snc>
</drv>

<drv mrk="arangx.al0i">
  <kap>al<tld/>i<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Adapti.
    </dif>
  </snc>
  <trd lng="pt">adaptar</trd>
</drv>

<drv mrk="arangx.kun0o">
  <kap>kun<tld/>o</kap>
  <snc>
    <dif>
      Kunordigo.
    </dif>
  </snc>
  <trd lng="pt">ordena&ccedil;&atilde;o</trd>
</drv>

<drv mrk="arangx.mal0i">
  <kap>mal<tld/>i</kap>
  <snc>
    <dif>
      Malordigi.
    </dif>
    <refgrp tip="sin">
      <ref cel="konfuz.0i">konfuzi</ref>,
      <ref cel="help.mal0i">malhelpi</ref>,
      <ref cel="pertur.0i">perturbi</ref>,
      <ref cel="renver.0i">renversi</ref>,
      <ref cel="ruin">ruinigi</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_l;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="fr">d&eacute;ranger</trd>
  <trdgrp lng="pt">
      <trd>desarranjar</trd>,
      <trd>desarrumar</trd>,
      <trd>bagun&ccedil;ar</trd>,
      <trd>desordenar</trd>
   </trdgrp>
</drv>
</art>
