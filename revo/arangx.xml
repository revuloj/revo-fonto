<drv mrk="arangx.al0i">
  <kap>al<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="arangx.al0i.KOMUNE">
    <dif>
      <ref tip="vid" cel="adapt.0i">Adapti</ref>,
      bone kunmeti kun:
      <ekz>
        juna fra&ubreve;lino <klr>[&#x2026;]</klr> en tiu momento estis al<tld/>anta
        multekostan plumon de struto al velura &ccirc;apelo
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">anpassen</trd>
  <trdgrp lng="es">
    <trd>adaptar</trd>,
    <trd>ajustar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>adapter</trd>,
    <trd>ajuster</trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5D0;&#x5E8;&#x5D2;&#x5DF;</trd>
  <trd lng="pt">adaptar</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_l;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_l;&c_a;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="arangx.antaux0i">
  <kap>anta&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="arangx.antaux0i.anticipe">
    <dif>
      Anticipe <tld/>i:
      <ekz>
        ni estas Lia farita&jcirc;o, kreitaj en Kristo Jesuo por bonaj faroj,
        kiujn anta&ubreve;<tld/>is Dio, por ke ni iradu en ili
        <fnt><bib>NT</bib>, <lok>&Efe; 2:10</lok></fnt>;
      </ekz>
      <ekz>
        apena&ubreve; dekdujara li edzi&gcirc;as per anta&ubreve;<tld/>ita geedzi&gcirc;o, la&ubreve; la hinduisma
        tradicio
        <fnt>
          <vrk><url ref="http://www.ueci.it/kongresoj/kng_2012/prelego_3.htm">Prelego 3/2012 - ueci.it</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        konkurso estas anta&ubreve;<tld/>ita batalo inter konkurantoj la&ubreve; difinitaj reguloj
        <fnt>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Konkurso">Konkurso - Vikipedio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">arrangieren</trd>
  <trdgrp lng="es">
    <trd>organizar anticipadamente</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>arranger d'avance</trd>,
    <trd>disposer<klr tip="amb"> (en vue de)</klr></trd>,
    <trd>organiser d'avance</trd>
  </trdgrp>
</drv>

<drv mrk="arangx.har0o">
  <kap>har<tld/>o</kap>
  <snc mrk="arangx.har0o.FRIZ">
    <dif>
      Arta meto de haroj, precipe de la kapharoj:
      <ekz>
        &ccirc;iam nete vestite en belaj vesta&jcirc;oj, &ccirc;arma har<tld/>o, &scirc;i konstante dum tiuj
        kunvenoj estis leganta iujn librojn
        <fnt>
          <bib>Monato</bib>
          <aut>Dmitrij Cibulevskij</aut>
          <vrk><url ref="https://www.monato.be/2010/010054.php">Turmentoj</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili argumentis, ke la har<tld/>o konsistigas la &ccirc;efajn propra&jcirc;ojn de ilia ineco
        <fnt>
          <bib>Monato</bib>
          <aut>Sagbadjelou Kokou</aut>
          <vrk><url ref="https://www.monato.be/2006/008900.php">Disputo neniel harfenda</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u oni &scirc;an&gcirc;u eklezion, je la unua malfacila&jcirc;o, kvaza&ubreve; estus <klr>[&#x2026;]</klr>
        hararan&gcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.be/2010/010098.php">Paradoksa aserto</url></vrk>
          <lok>2010</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Frisur</trd>
  <trd lng="es">peinado</trd>
  <trd lng="fr">coiffure<klr tip="ind"> (mani&egrave;re de se
  coiffer)</klr></trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_ch;&c_jo;&c_s;&c_k;&c_a;</trd>
</drv>

<drv mrk="arangx.kun0o">
  <kap>kun<tld/>o</kap>
  <snc mrk="arangx.kun0o.KOMUNE">
    <dif>
      Kunordigo:
      <ekz>
        aspiro al <klr>[&#x2026;]</klr> superiro de la diversaj bariloj kaj al kun<tld/>o de la
        konfliktoj
        <fnt>
          <vrk><url ref="https://esperokatolika.org/ek19861990/ek1988_06.htm">Espero Katolika - 1988_06</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ni devas konservi la disvolvan ideon de novigo, kun<tld/>o, verdeco, malfermo
        <fnt>
          <vrk><url ref="http://esperanto.cri.cn/audio/news/4191/20201111/576239.html">Nova&jcirc;oj (Novembro 11) - esperanto.cri.cn</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">koordiniieren</trd>
  <trd lng="es">ordenaci&oacute;n</trd>
  <trd lng="fr">ordonnancement</trd>
  <trd lng="pt">ordena&ccedil;&atilde;o</trd>
</drv>

<drv mrk="arangx.mal0i">
  <kap>mal<tld/>i</kap>
  <snc mrk="arangx.mal0i.KOMUNE">
    <dif>
      <ref tip="dif" cel="ord.mal0igi">Malordigi</ref>, dismeti:
      <ekz>
        edzi&gcirc;o mal<tld/>ita
        <fnt>
          <vrk><url ref="https://neciklopedio.org/wiki/Humuro">Humuro - Neciklopedio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="er.dis0igi">diserigi</ref>,
      <ref cel="konfuz.0i">konfuzi</ref>,
      <ref cel="help.mal0i">malhelpi</ref>,
      <ref cel="konstr.mal0i">malkonstrui</ref>,
      <ref cel="munt.mal0i">malmunti</ref>,
      <ref cel="pertur.0i">perturbi</ref>,
      <ref cel="renver.0i">renversi</ref>,
      <ref cel="ruin.0igi">ruinigi</ref>
