      <trd>&c_u;&c_l;&c_a;&c_d;&c_k;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Anordnung</trd>,
      <trd>Aufbau</trd>,
      <trd>Einrichtung</trd>,
      <trd>Gestaltung</trd>
    </trdgrp>
    <trdgrp lng="en">
