<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
   <art mrk="$Id$">
     <kap>
       <ofc>*</ofc>
       <rad>aran&gcirc;</rad>/i
     </kap>
     <drv mrk="arangx.0i">
       <kap><ofc>*</ofc><tld/>i</kap>
       <gra><vspec>tr</vspec></gra>
       <snc>
         <dif>
           Meti la&ubreve; &gcirc;usta ordo, ordigi:
           <ekz>
             <tld/>i &ccirc;ambron, liton;
           </ekz><ekz>
             <tld/>i librojn en biblioteko;
           </ekz><ekz>
             la du armeoj sin <tld/>is kontra&ubreve; la komuna
             malamiko<fnt>Z</fnt>;
           </ekz><ekz>
             ord<tld/>i
             <fnt>(<aut>Zamenhof</aut>: <vrk>Fabeloj de Andersen,
                 Malnova domo</vrk>)</fnt>;
           </ekz><ekz>
             vort<tld/>o.
           </ekz>
         </dif>
         <trdgrp lng="fr">
           <trd>arranger</trd>,
           <trd>mettre en <ind>ordre</ind></trd>,
           <trd>ranger</trd></trdgrp>
       </snc>
       <snc>
         <dif>
           Pretigi, organizi, prepari, prizorgi:
           <ekz>
             <tld/>i kongreson, feston,
             interkomuniki&gcirc;adon, belan akcepton;
           </ekz><ekz>
             sin <tld/>i komforte;
           </ekz>
         </dif>
         <trdgrp lng="fr">
           <trd>arranger</trd>,
           <trd>organiser</trd>,
           <trd>r&eacute;gler</trd></trdgrp>
       </snc>
       <trd lng="de">arangieren</trd>
       <trd lng="en">arrange</trd>
       <trd lng="ru">&c_u;&c_s;&c_t;&c_r;&c_a;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
     </drv>
     <drv mrk="arangx.0o">
       <kap><tld/>o</kap>
       <snc>
         <dif>
           Io <tld/>ita, maniero <tld/>i:
           <ekz>
             kalendaro de Esperanto-<tld/>oj,
           </ekz><ekz>
             pla&ccirc;as al mi la <tld/>o de via salono,
           </ekz><ekz>
             fari ta&ubreve;gan <tld/>on.
           </ekz>
         </dif>
         <trdgrp lng="fr">
           <trd>arrangement</trd>,
           <trd>disposition</trd>,
           <trd>manifestation (&eacute;v&eacute;nement organis&eacute;)</trd></trdgrp>
         <trd lng="ru">&c_m;&c_je;&c_r;&c_o;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_i;&c_je;</trd>
       </snc>
       <snc>
         <uzo tip="fak">KOMP</uzo>
         <dif>
           Maniero <tld/>i datumojn sur datumportilop;
           <ekz>sur paperfolio oni povas &gcirc;israndigi la tekston dekstren,
             a&ubreve; uzi alian teksto<tld/>on.</ekz>
         </dif>
         <trdgrp lng="en">
           <trd>layout</trd>, <trd>format</trd>; <trd>alignment</trd>
         </trdgrp>
         <trdgrp lng="fr">
           <trd>disposer</trd>,
           <trd>mettre en <ind>page</ind></trd>,
           <trd>formater (un texte)</trd></trdgrp>
         <trdgrp lng="ru">
           <trd>&c_r;&c_a;&c_z;&c_m;&c_je;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>,
           <trd>&c_k;&c_o;&c_m;&c_p;&c_o;&c_n;&c_o;&c_v;&c_k;&c_a;</trd>,
           <trd>&c_f;&c_o;&c_r;&c_m;&c_a;&c_t;</trd>
         </trdgrp>
       </snc>
     </drv>
     <drv mrk="arangx.0ajxo">
       <kap><tld/>a&jcirc;o</kap>
       <snc mrk="arangx.0ajxo.mat">
         <uzo tip="fak">MAT</uzo>
         <dif>
           En kombinatoriko, <ctl><tld/>a&jcirc;o de <em>m</em> elementoj el
           <em>n</em> elementoj</ctl> estas &ccirc;iu el la diversaj manieroj
           vicigi <em>m</em> objektojn el <em>n</em>-elementa aro (do,
           konsiderante anka&ubreve; la lokon de &ccirc;iu elektita objekto).  La
           <ctl>nombron de &ccirc;iuj <tld/>a&jcirc;oj el <em>n</em> po <em>m</em></ctl>
           esprimas la formulo:

           A<sup>m</sup><sub>n</sub> = n(n-1)(n-2)...(n-m+2)(n-m+1)
         </dif>
         <ref tip="super" cel="kombinrk.0ajxo">kombinatorika&jcirc;o</ref>
         <refgrp tip="vid">
           <ref cel="kombin.0ajxo.mat">kombina&jcirc;o</ref>,
           <ref cel="permut.0ajxo">permuta&jcirc;o</ref>
         </refgrp>
         <trd lng="de">Variationen n-ter Klasse</trd>
         <trd lng="en">arrangement</trd>
         <trd lng="fr">arrangement de p objets pris parmi n</trd>
         <trd lng="ru">&c_r;&c_a;&c_z;&c_m;&c_je;&c_shch;&c_je;&c_n;&c_i;&c_ja; &c_i;&c_z; n &c_p;&c_o; m</trd>
       </snc>
       <snc mrk="arangx.0ajxo.tek">
         <uzo tip="fak">TEK</uzo>
         <dif>Ilo, aparato a&ubreve; organo <tld/>ita por plenumi funkcion 
          a&ubreve; laboron.
         </dif>
         <trd lng="de">Ger&auml;t</trd>
         <trd lng="en">device</trd>
         <trd lng="fr">dispositif</trd>
         <trd lng="ru">&c_u;&c_s;&c_t;&c_r;&c_o;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>
       </snc>
     </drv>
     <drv mrk="arangx.0ema">
       <kap><tld/>ema<fnt>Z</fnt></kap>
       <snc>
         <dif>
           Elturni&gcirc;ema, elpensema, eltrovema.
         </dif>
       </snc>
     </drv>
     <drv mrk="arangx.al0i">
       <kap>al<tld/>i<fnt>Z</fnt></kap>
       <snc>
         <dif>
           Adapti.
         </dif>
       </snc>
     </drv>
     <drv mrk="arangx.kun0o">
       <kap>kun<tld/>o</kap>
       <snc>
         <dif>
           Kunordigo.
         </dif>
       </snc>
     </drv>
     <drv mrk="arangx.mal0i">
       <kap>mal<tld/>i</kap>
       <snc>
         <dif>
           Malordigi.
         </dif>
         <refgrp tip="sin">
           <ref cel="konfuz.0i">konfuzi</ref>,
           <ref cel="help.mal0i">malhelpi</ref>,
           <ref cel="pertur.0i">perturbi</ref>,
           <ref cel="renver.0i">renversi</ref>,
           <ref cel="ruin">ruinigi</ref>.
         </refgrp>
       </snc>
       <trd lng="fr">d&eacute;ranger</trd>
     </drv>
   </art>
