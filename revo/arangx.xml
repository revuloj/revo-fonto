  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_l;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="de"><ind>durcheinander</ind> bringen</trd>
  <trdgrp lng="es">
    <trd>desordenar</trd>,
    <trd>desorganizar</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;ranger</trd>
  <trdgrp lng="pt">
    <trd>desarranjar</trd>,
    <trd>desarrumar</trd>,
    <trd>bagun&ccedil;ar</trd>,
    <trd>desordenar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_v;&c_je;&c_s;&c_t;&c_i; &c_v; <ind>&c_b;&c_je;&c_s;&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_k;</ind></trd>,
    <trd>&c_r;&c_a;&c_z;&c_l;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_s;&c_t;&c_r;&c_o;&c_i;&c_t;&c_mol;<klr tip="amb"> (&c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>
  </trdgrp>
</drv>
