<drv mrk="arangx.antaux0i">
  <kap>anta&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="arangx.antaux0i.anticipe">
    <dif>
      Anticipe <tld/>i:
      <ekz>
        ni estas Lia farita&jcirc;o, kreitaj en Kristo Jesuo por bonaj faroj,
        kiujn anta&ubreve;<tld/>is Dio, por ke ni iradu en ili
        <fnt><bib>NT</bib>, <lok>&Efe; 2:10</lok></fnt>;
      </ekz>
      <ekz>
        apena&ubreve; dekdujara li edzi&gcirc;as per anta&ubreve;<tld/>ita geedzi&gcirc;o, la&ubreve; la hinduisma
        tradicio
        <fnt>
          <vrk><url ref="http://www.ueci.it/kongresoj/kng_2012/prelego_3.htm">Prelego 3/2012 -
          ueci.it</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        konkurso estas anta&ubreve;<tld/>ita batalo inter konkurantoj la&ubreve; difinitaj reguloj
        <fnt>
          <vrk><url ref="&Viki;/Konkurso">Konkurso - Vikipedio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
