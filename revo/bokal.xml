<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>bokal</rad>/o
</kap>
<drv mrk="bokal.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="vaz.0o">Vazo</ref>, plej ofte vitra,
      kun lar&gcirc;a malferma&jcirc;o kaj tre mallonga kolo, ofte
      kun kovrilo; destinita por konservado de firmaj a&ubreve; 
      duonfluaj materialoj:
      <ekz>
        <tld/>o por marmelado, mielo, kompoto.
      </ekz>
    </dif>
  </snc>
  <trd lng="de"><klr>(Kompott-, Marmeladen-)</klr>Glas</trd>
  <trd lng="fr">bocal</trd>
  <trd lng="nl">bokaal</trd>
  <trd lng="ru">&c_b;&c_a;&c_n;&c_k;&c_a;
  <klr>(&c_s;&c_t;&c_je;&c_k;&c_l;&c_ja;&c_n;&c_n;&c_a;&c_ja; &c_i;
  &c_t;.&c_p;.)</klr></trd>
</drv>
</art>
