      <trd>monding</trd>
    </trdgrp>
    <trd lng="pl">gard&lstroke;o</trd>
    <trd lng="ro">orificiu</trd>
    <trdgrp lng="ru">
      <trd>&c_u;&c_s;&c_t;&c_mol;&c_je; <klr>(&c_r;&c_je;&c_k;&c_i;,
      &c_sh;&c_a;&c_h;&c_t;&c_y;)</klr></trd>,
