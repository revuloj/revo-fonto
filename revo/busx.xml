    <trd lng="pl">gard&lstroke;o</trd>
    <trd lng="ro">orificiu</trd>
    <trdgrp lng="ru">
      <trd>&c_u;&c_s;&c_t;&c_mol;&c_je; <klr>(&c_r;&c_je;&c_k;&c_i;,
      &c_sh;&c_a;&c_h;&c_t;&c_y;)</klr></trd>,
      <trd>&c_d;&c_u;&c_l;&c_o; <klr>(&c_r;&c_u;&c_zh;&c_mol;&c_ja;,
      &c_p;&c_u;&c_sh;&c_k;&c_i;)</klr></trd>,
      <trd>&c_zh;&c_je;&c_r;&c_l;&c_o;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#21475;&#40763; <pr>k&#466;ub&iacute;</pr></trd>,
    <trd>&#28846;&#21475; <pr>p&agrave;ok&#466;u</pr></trd>,
    <trd>&#26538;&#21475; <pr>qi&amacron;ngk&#466;u</pr></trd>,
