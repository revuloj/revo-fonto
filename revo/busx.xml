<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>bu&scirc;</rad>/o</kap>
<drv mrk="busx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="busx.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Kava parto en la kapo de homo a&ubreve; besto, per kiu ili sin nutras:
      <ekz>
        <tld/>o de homo, bovo, leono, hundo, rato;
      </ekz>
      <ekz>
        &ccirc;e &ccirc;iu vorto, kiun vi diros, el via <tld/>o eliros a&ubreve; floro a&ubreve; 
        multekosta &scirc;tono
        <fnt><bib>F</bib>, <lok>&FE; 15</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_o;&c_t;</trd>,
      <trd>&c_v;&c_u;&c_s;&c_n;&c_y;</trd>,
      <trd>&c_p;&c_a;&c_sh;&c_ch;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Mund <klr>(&ccirc;e homo)</klr></trd>,
      <trd>Maul <klr>(&ccirc;e bestoj)</klr></trd>
    </trdgrp>
    <trd lng="hu">sz&aacute;j</trd>
    <trdgrp lng="nl">
      <trd>mond</trd>,
      <trd>muil</trd>
    </trdgrp>
    <trd lng="pl">usta</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_o;&c_t; <klr>(&ccirc;e homo)</klr></trd>,
      <trd>&c_p;&c_a;&c_s;&c_t;&c_mol; <klr>(&ccirc;e bestoj)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="busx.0o.parolilo">
    <dif>
      Sama parto de la homa kapo, konsiderata kiel servanta por paroli:
      <ekz>
        en <tld/>o Biblio, en koro malpio
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ne movi la <tld/>on
        <klr>(tute silenti)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        &scirc;topi, fermi al iu la <tld/>on
        <klr>(silentigi)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lang.0o">lango</ref>,
      <ref cel="lip.0o">lipo</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_r;&c_o;&c_t;</trd>,
      <trd>&c_v;&c_u;&c_s;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="hu">sz&aacute;j</trd>
    <trd lng="nl">mond</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_o;&c_t;</trd>,
      <trd>&c_u;&c_s;&c_t;&c_a; <klr>(&c_p;&c_o;&c_e;&c_t;.)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="busx.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Enira a&ubreve; elira aperturo:
      <ekz>
        <tld/>o de forno, de rivero en la maro
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flu.en0ejo">enfluejo</ref>,
      <ref cel="fauxk.0o">fa&ubreve;ko</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_a;&c_d;&c_t;&c_u;&c_l;&c_ib;&c_n;&c_a;</trd>,
      <trd>&c_zh;&c_a;&c_r;&c_a;&c_l;&c_o;</trd>,
      <trd>&c_v;&c_u;&c_s;&c_mol;&c_c;&c_je;</trd>,
      <trd>&c_ch;&c_a;&c_l;&c_je;&c_s;&c_mol;&c_n;&c_ib;&c_k;&c_ib; <klr>(de forno)</klr></trd>
    </trdgrp>
    <trd lng="de">M&uuml;ndung</trd>
    <trdgrp lng="hu">
      <trd>sz&aacute;j</trd>,
      <trd>torkolat</trd>,
      <trd>ny&iacute;l&aacute;s</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>mond</trd>,
      <trd>monding</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_u;&c_s;&c_t;&c_mol;&c_je; <klr>(&c_r;&c_je;&c_k;&c_i;,
      &c_sh;&c_a;&c_h;&c_t;&c_y;)</klr></trd>,
      <trd>&c_d;&c_u;&c_l;&c_o; <klr>(&c_r;&c_u;&c_zh;&c_mol;&c_ja;,
      &c_p;&c_u;&c_sh;&c_k;&c_i;)</klr></trd>,
      <trd>&c_zh;&c_je;&c_r;&c_l;&c_o;</trd>
    </trdgrp>
  </snc>
  <trd lng="br">geno&ugrave;</trd>
  <trd lng="cs">&uacute;sta</trd>
  <trd lng="en">mouth</trd>
  <trd lng="es">boca</trd>
  <trdgrp lng="fr">
    <trd>bouche</trd>,
    <trd>embouchure</trd>,
    <trd>gueule<klr tip="ind"> (d'un animal)</klr></trd>
  </trdgrp>
  <trd lng="he">&pe;&he;</trd>
  <trd lng="sk">&uacute;sta</trd>
  <trd lng="tr">a&gbreve;&inodot;z</trd>
  <trd lng="vo">mud</trd>
</drv>

<drv mrk="busx.0a">
  <kap><tld/>a</kap>
  <snc mrk="busx.0a.ANA">
    <dif>
      De la <tld/>o.
    </dif>
    <trd lng="be">&c_r;&c_o;&c_t;&c_a;&c_v;&c_y;</trd>
    <trd lng="br">-<ind>geno&ugrave;</ind></trd>
    <trd lng="es">bucal</trd>
    <trd lng="hu">sz&aacute;j-</trd>
    <trd lng="nl">mond-</trd>
    <trd lng="pl">ustny</trd>
    <trd lng="ru">&c_r;&c_o;&c_t;&c_o;&c_v;&c_o;&c_j;</trd>
    <trd lng="vo">mudik</trd>
  </snc>
  <snc mrk="busx.0a.parola">
    <dif>
      Parola, vo&ccirc;a:
      <ekz>
        <tld/>a interparolado, literaturo<fnt>Z</fnt>
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_u;&c_s;&c_n;&c_y;</trd>
    <trd lng="br">dre gomz</trd>
    <trd lng="de">m&uuml;ndlich</trd>
    <trd lng="es">oral</trd>
    <trd lng="hu">sz&oacute;beli</trd>
    <trd lng="nl">mondeling</trd>
    <trd lng="ru">&c_u;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="busx.0a.FON">
    <uzo tip="fak">FON</uzo>
    <dif>
      Dum kies prononco la velo estas levita kaj baras la pasejon inter la
      <tld/>o kaj la naza kava&jcirc;o:
      <ekz>
        <tld/>a vokalo
        <trd lng="hu">or&aacute;lis mag&aacute;nhangz&oacute;</trd>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="naz">naza</ref>.
    </refgrp>
    <trd lng="br">-<ind>geno&ugrave;</ind><klr tip="ind"> (soniadoniezh)</klr></trd>
    <trd lng="es">oral</trd>
    <trdgrp lng="hu">
      <trd>sz&aacute;j-</trd>,
      <trd>or&aacute;lis</trd>
    </trdgrp>
  </snc>
  <trd lng="en">oral</trd>
  <trdgrp lng="fr">
    <trd>buccal</trd>,
    <trd>oral</trd>
  </trdgrp>
  <trd lng="nl">oraal</trd>
  <trd lng="sk">&uacute;stny</trd>
</drv>

<drv mrk="busx.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="busx.0ego.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <tld lit="B"/>o de grandaj bestoj.
    </dif>
    <ref tip="sin" cel="fauxk.0o">fa&ubreve;ko</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_sh;&c_ch;&c_a;</trd>
  <trd lng="br">geol</trd>
  <trd lng="de">Schnauze</trd>
  <trdgrp lng="en">
    <trd>maw</trd>,
    <trd>gullet</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gueule <klr tip="ind">(d'un animal)</klr></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&lamed;&vav;&ayin;</trd>,
    <trd>&mem;&lamed;&tav;&ayin;&vav;&tav;</trd>
  </trdgrp>
  <trd lng="hu">pofa</trd>
  <trd lng="nl">muil</trd>
  <trd lng="pl">paszcza</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">papu&lcaron;a</trd>
  <trd lng="vo">lemud</trd>
</drv>

<drv mrk="busx.0umo">
  <kap><ofc>*</ofc><tld/>umo</kap>
  <snc mrk="busx.0umo.KOMUNE">
    <dif>
      Ilo por teni fermita la <tld/>on de bestoj kaj
      malhelpi la mordojn:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la gazetaro portas <tld/>umon
        <klr>(ne povas libere paroli)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_m;&c_o;&c_r;&c_d;&c_n;&c_ib;&c_k;</trd>
  <trd lng="br">minwal</trd>
  <trd lng="de">Maulkorb</trd>
  <trd lng="en">muzzle</trd>
  <trd lng="fr">museli&egrave;re</trd>
  <trd lng="he">&resh;&samekh;&fnun;</trd>
  <trd lng="hu">sz&aacute;jkos&aacute;r</trd>
  <trd lng="nl">muilkorf</trd>
  <trd lng="pl">kaganiec</trd>
  <trd lng="ru">&c_n;&c_a;&c_m;&c_o;&c_r;&c_d;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">n&aacute;hubok</trd>
</drv>

<drv mrk="busx.en0ajxo">
  <kap>en<tld/>a&jcirc;o</kap>
  <snc mrk="busx.en0ajxo.RAJ">
    <dif>
      Transversa stangeto, kiun oni &scirc;ovas en la <tld/>on
      de &ccirc;evalo por direkti &gcirc;in per alfiksitaj bridoj.
    </dif>
  </snc>
  <trd lng="br">gweskenn</trd>
  <trd lng="en"><klr>bridle </klr>bit</trd>
  <trd lng="fr">mors</trd>
  <trd lng="nl">mondstuk</trd>
  <trd lng="hu">zabla</trd>
  <trd lng="pl">w&eogonek;dzid&lstroke;o</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_d;&c_i;&c_l;&c_a;</trd>,
    <trd>&c_m;&c_u;&c_n;&c_d;&c_sh;&c_t;&c_u;&c_k;<klr tip="amb"> (&c_u; &c_l;&c_o;&c_sh;&c_a;&c_d;&c_i;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="busx.el0igi">
  <kap>el<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="busx.el0igi.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <ref tip="dif" cel="dir.0i.KOMUNE">diri</ref>
      <ekz>
        mi neniam povis el<tld/>igi a&ubreve; &leftquot;so&rightquot; post &leftquot;po&rightquot;, a&ubreve; &leftquot;co&rightquot; post 
        &leftquot;so&rightquot;
        <fnt>
          <aut>D. K. Jordan</aut>,
          <vrk>Kna&hcirc;o&hcirc;&scirc;ve&ubreve;psceg</vrk>,
          <lok><url ref="http://esperanto.ca/kea/j-enkonduko.html">La 
          Fabeloj de Jordan</url>, [vidita en 2006-05-24]</lok>
        </fnt>.
      </ekz>
  </snc>
  <trdgrp lng="fr">
    <trd>causer</trd>,
    <trd>prononcer</trd>
  </trdgrp>
</drv>

<drv mrk="busx.post0o">
  <kap>post<tld/>o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="busx.post0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Parto de la <tld/>o proksima al la
      <ref cel="faring.0o">faringo</ref>.
    </dif>
  </snc>
  <adm>
    Fakulo bv kontroli la difinon. La difino en
    PIV1 estas "faringo" kaj en PIV2 "busxa
    parto de faringo" (supra parto de faringo??)
    <aut>MB</aut>
  </adm>
  <trd lng="fr">arri&egrave;re-bouche</trd>
  <trd lng="de">hintere Mundh&ouml;hle</trd>
  <trd lng="ru">&c_z;&c_je;&c_v;</trd>
</drv>


<drv mrk="busx.plen0o">
  <kap>plen<tld/>o</kap>
  <snc mrk="busx.plen0o.KOMUNE">
    <dif>
      Kvanto, kiun povas enteni la <tld/>o.
    </dif>
  </snc>
  <trd lng="br">genaouad</trd>
  <trd lng="en">mouthful</trd>
  <trdgrp lng="fr">
    <trd>bouch&eacute;e</trd>,
    <trd>lamp&eacute;e</trd>
  </trdgrp>
  <trd lng="he">&mem;&lamed;&vav;&alef; &he;&pe;&he;</trd>
  <trd lng="hu">falat</trd>
  <trd lng="nl">mondvol</trd>
  <trd lng="de">Mundvoll</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_l;&c_n;&c_y;&c_j; &c_r;&c_o;&c_t;<klr tip="amb"> (&c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>,
    <trd>&c_k;&c_u;&c_s;&c_o;&c_k;</trd>,
    <trd>&c_g;&c_l;&c_o;&c_t;&c_o;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="busx.rond0uloj">
  <kap>rond<tld/>uloj</kap>
  <snc mrk="busx.rond0uloj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Klaso de <ref tip="super" cel="verteb.0uloj">vertebruloj</ref>, 
      serpento- a&ubreve; vermo-formaj senmakzelaj fi&scirc;oj (<trd 
      lng="la">Cyclostomata</trd>).
    </dif>
  </snc>
  <trd lng="de">Rundm&auml;uler</trd>
  <trd lng="en">cyclostomes</trd>
  <trd lng="fr">cyclostomes</trd>
  <trd lng="he">&ayin;&gimel;&vav;&lamed;&yod; &he;&pe;&he;</trd>
  <trd lng="hu">k&ouml;rsz&aacute;j&uacute;ak</trd>
  <trd lng="nl">rondbekken</trd>
  <trd lng="ru">&c_k;&c_r;&c_u;&c_g;&c_l;&c_o;&c_r;&c_o;&c_t;&c_y;&c_je; <klr tip="amb">(&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>
</drv>
</art>
