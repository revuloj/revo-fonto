<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>bu&scirc;</rad>/o
</kap>
<drv mrk="busx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="busx.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Kava parto en la kapo de homo a&ubreve; besto, per kiu ili sin nutras:
      <ekz>
        <tld/>o de homo, bovo, leono, hundo, rato.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Mund <klr>(&ccirc;e homo)</klr></trd>,
      <trd>Maul <klr>(&ccirc;e bestoj)</klr></trd>
    </trdgrp>
    <trd lng="hu">sz&aacute;j</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_o;&c_t; <klr>(&ccirc;e homo)</klr></trd>,
      <trd>&c_p;&c_a;&c_s;&c_t;&c_mol; <klr>(&ccirc;e bestoj)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="busx.0o.parolilo">
    <dif>
      Sama parto de la homa kapo, konsiderata kiel servanta por paroli:
      <ekz>
        en <tld/>o Biblio, en koro malpio<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ne movi la <tld/>on
        <klr>(tute silenti)</klr><fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        &scirc;topi, fermi al iu la <tld/>on
        <klr>(silentigi)</klr>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lip.0o">lipo</ref>,
      <ref cel="lang.0o">lango</ref>.
    </refgrp>
    <trd lng="hu">sz&aacute;j</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_o;&c_t;</trd>,
      <trd>&c_u;&c_s;&c_t;&c_a; <klr>(&c_p;&c_o;&c_e;&c_t;.)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="busx.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Enira a&ubreve; elira aperturo:
      <ekz>
        <tld/>o de forno, de rivero en la maro
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fauxk.0o">fa&ubreve;ko</ref>,
      <ref cel="flu.en0ejo">enfluejo</ref>.
    </refgrp>
    <trd lng="de">M&uuml;ndung</trd>
    <trdgrp lng="hu">
      <trd>sz&aacute;j</trd>,
      <trd>torkolat</trd>,
      <trd>ny&iacute;l&aacute;s</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_u;&c_s;&c_t;&c_mol;&c_je; <klr>(&c_r;&c_je;&c_k;&c_i;,
&c_sh;&c_a;&c_h;&c_t;&c_y;)</klr></trd>,
      <trd>&c_d;&c_u;&c_l;&c_o; <klr>(&c_r;&c_u;&c_zh;&c_mol;&c_ja;,
&c_p;&c_u;&c_sh;&c_k;&c_i;)</klr></trd>,
      <trd>&c_zh;&c_je;&c_r;&c_l;&c_o;</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">&uacute;sta</trd>
  <trd lng="en">mouth</trd>
  <trd lng="es">boca</trd>
  <trdgrp lng="fr">
    <trd>bouche</trd>,
    <trd>gueule <klr tip="ind">(d'un animal)</klr></trd>,
    <trd>embouchure</trd>
  </trdgrp>
  <trd lng="he">&pe;&he;</trd>
  <trd lng="sk">&uacute;sta</trd>
  <trd lng="tr">a&gbreve;&inodot;z</trd>
  <trd lng="vo">mud</trd>
</drv>

<drv mrk="busx.0a">
  <kap><tld/>a</kap>
  <snc mrk="busx.0a.ANA">
    <dif>
      De la <tld/>o.
    </dif>
    <trd lng="es">bucal</trd>
    <trd lng="hu">sz&aacute;j-</trd>
    <trd lng="ru">&c_r;&c_o;&c_t;&c_o;&c_v;&c_o;&c_j;</trd>
    <trd lng="vo">mudik</trd>
  </snc>
  <snc mrk="busx.0a.parola">
    <dif>
      Parola, vo&ccirc;a:
      <ekz>
        <tld/>a interparolado, literaturo<fnt>Z</fnt>
      </ekz>
    </dif>
    <trd lng="de">m&uuml;ndlich</trd>
    <trd lng="es">oral</trd>
    <trd lng="hu">sz&oacute;beli</trd>
    <trd lng="ru">&c_u;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="busx.0a.FON">
    <uzo tip="fak">FON</uzo>
    <dif>
      Dum kies prononco la velo estas levita kaj baras la pasejon inter la
      <tld/>o kaj la naza kava&jcirc;o:
      <ekz>
        <tld/>a vokalo
        <trd lng="hu">or&aacute;lis mag&aacute;nhangz&oacute;</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="naz">naza</ref>.
    </refgrp>
    <trd lng="es">oral</trd>
    <trdgrp lng="hu">
      <trd>sz&aacute;j-</trd>,
      <trd>or&aacute;lis</trd></trdgrp>
  </snc>
  <trd lng="en">oral</trd>
  <trdgrp lng="fr">
    <trd>buccal</trd>,
    <trd>oral</trd>
  </trdgrp>
  <trd lng="sk">&uacute;stny</trd>
</drv>

<drv mrk="busx.0ego">
  <kap><tld/>ego</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <tld lit="B"/>o de grandaj bestoj.
    </dif>
    <ref tip="sin" cel="fauxk.0o">fa&ubreve;ko.</ref>
  </snc>
  <trd lng="de">Schnauze</trd>
  <trdgrp lng="fr">
    <trd>gueule <klr tip="ind">(d'un animal)</klr></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&lamed;&vav;&ayin;</trd>,
    <trd>&mem;&lamed;&tav;&ayin;&vav;&tav;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>maw</trd>,
    <trd>gullet</trd>
  </trdgrp>
  <trd lng="hu">pofa</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">papu&lcaron;a</trd>
  <trd lng="vo">lemud</trd>
</drv>

<drv mrk="busx.0umo">
  <kap><ofc>*</ofc><tld/>umo</kap>
  <snc>
    <dif>
      Ilo por teni fermita la <tld/>on de bestoj kaj
      malhelpi la mordojn:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la gazetaro portas <tld/>umon
        <klr>(ne povas libere paroli)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Maulkorb</trd>
  <trd lng="en">muzzle</trd>
  <trd lng="fr">museli&egrave;re</trd>
  <trd lng="he">&resh;&samekh;&fnun;</trd>
  <trd lng="hu">sz&aacute;jkos&aacute;r</trd>
  <trd lng="ru">&c_n;&c_a;&c_m;&c_o;&c_r;&c_d;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">n&aacute;hubok</trd>
</drv>

<drv mrk="busx.en0ajxo">
  <kap>en<tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Transversa stangeto, kiun oni &scirc;ovas en la <tld/>on
      de &ccirc;evalo por direkti &gcirc;in per alfiksitaj bridoj.
    </dif>
  </snc>
  <trd lng="en"><klr>bridle </klr>bit</trd>
  <trd lng="fr">mors</trd>
  <trd lng="hu">zabla</trd>
</drv>

<drv mrk="busx.plen0o">
  <kap>plen<tld/>o</kap>
  <snc>
    <dif>
      Kvanto, kiun povas enteni la <tld/>o.
    </dif>
  </snc>
  <trd lng="en">mouthful</trd>
  <trd lng="fr">bouch&eacute;e</trd>
  <trd lng="he">&mem;&lamed;&vav;&alef; &he;&pe;&he;</trd>
  <trd lng="hu">falat</trd>
</drv>

<drv mrk="busx.rond0uloj">
  <kap>rond<tld/>uloj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Klaso de <ref tip="super" cel="verteb.0uloj">vertebruloj</ref>
      (<trd lng="la">Cyclostomata</trd>), serpento- a&ubreve;
      vermoformaj senmakzelaj fi&scirc;oj.
    </dif>
  </snc>
  <trd lng="de">Rundm&auml;uler</trd>
  <trd lng="en">cyclostomes</trd>
  <trd lng="fr">cyclostomes</trd>
  <trd lng="he">&ayin;&gimel;&vav;&lamed;&yod; &he;&pe;&he;</trd>
  <trd lng="hu">k&ouml;rsz&aacute;j&uacute;ak</trd>
  <trd lng="ru">&c_k;&c_r;&c_u;&c_g;&c_l;&c_o;&c_r;&c_o;&c_t;&c_y;&c_je;
<klr tip="amb">(&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>
</drv>
</art>
