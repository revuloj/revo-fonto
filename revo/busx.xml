<drv mrk="busx.0a">
  <kap><tld/>a</kap>
  <snc mrk="busx.0a.ANA">
    <dif>
      De la <tld/>o<sncref ref="busx.0o.ANA"/>:
      <ekz>
        la <tld/>a kavo;
      </ekz>
      <ekz>
        &gcirc;i konvulsiigas lin kun <tld/>a &scirc;a&ubreve;mo
        <fnt>
          <bib>NT</bib>
          <lok>&Luk; 9:39</lok>
        </fnt>;
      </ekz>
      <ekz>
        elkra&ccirc;itaj <tld/>a&jcirc;oj
        <fnt>
          <aut>Garbhan MacAoidh</aut>
            <vrk><url ref="&Monato;/2000/007076.php">Ho, tempoj! Ho, moroj!</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 19</lok>
          </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_o;&c_t;&c_a;&c_v;&c_y;</trd>
    <trd lng="br">-<ind>geno&ugrave;</ind></trd>
    <trd lng="es">bucal</trd>
    <trd lng="fa">&a_d;&a_h;&a_A;&a_n;&f_y;</trd>
    <trd lng="hu">sz&aacute;j-</trd>
    <trd lng="id">oral</trd>
    <trdgrp lng="it">
     <trd>orale</trd>, <trd>boccale</trd>
    </trdgrp>
    <trd lng="nl">mond-</trd>
    <trd lng="pl">ustny</trd>
    <trd lng="ru">&c_r;&c_o;&c_t;&c_o;&c_v;&c_o;&c_j;</trd>
    <trd lng="vo">mudik</trd>
  </snc>
  <snc mrk="busx.0a.parola">
    <dif>
      Parola, vo&ccirc;a:
      <ekz>
        <tld/>aj interparoladoj
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        tio estas nur <tld/>a babilado
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 36:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i akceptos inviton <tld/>an
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;parmonujo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi pli bone povos doni al vi <tld/>an klarigon
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        esprimi &gcirc;in per <tld/>a vorto
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        havantaj literaturon skriban pli frue ol literaturon <tld/>an kaj orelan
        <fnt>
          <bib>LR</bib>
          <lok>4. Fonetiko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>aj lingvoj
        <fnt>
       <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2000/006880.php">Ne &ccirc;iuj lingvoj estas langaj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 14</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj informoj
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2003/008043.html">Seksa sklaveco: &ccirc;u defio al la 21a
          jarcento?</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi faris mian <tld/>an ekzamenon de la franca
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2005/008422.php">Disleksio</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj tradicioj (eposoj, legendoj, rakontoj ktp), transdonitaj dum multaj
        jarcentoj, de generacio al generacio
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2005/008718.php">La gaela romanarto</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_u;&c_s;&c_n;&c_y;</trd>
    <trd lng="br">dre gomz</trd>
    <trd lng="de">m&uuml;ndlich</trd>
    <trd lng="es">oral</trd>
    <trdgrp lng="fa">
      <trd>&a_sh;&a_f;&a_A;&a_h;&f_y;</trd>,
