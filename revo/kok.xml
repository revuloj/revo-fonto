<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kok.xml,v 1.62 2022/05/01 08:09:24 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kok</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kok.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kok.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Subspecio (<trd lng="la">Gallus gallus domesticus</trd>) el la 
      genro <ref tip="super" cel="kok.0o.ZOO"><tld/>o</ref>,
      bredata kiel <ref tip="super" cel="bird.korto0o">kortobirdo</ref> en multaj rasoj pro la
      viando kaj la ovoj:
      <ekz>
        anta&ubreve; ol krios <tld/>o, vi trifoje malkonfesos min
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 26:34</lok>
        </fnt>;
      </ekz>
      <ekz>
        la unua krio de <tld/>o <klr>(en frua mateno)</klr>
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        putraj ovoj kaj rostitaj <tld/>okrestoj estis la &ccirc;efaj man&gcirc;a&jcirc;oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o fiere mar&scirc;as kaj gratante ser&ccirc;as man&gcirc;a&jcirc;on por siaj <tld/>inoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        e&ubreve;ropaj bredistoj de <tld/>oj, anseroj kaj meleagroj kutime posedas amason da
        birdoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2006/008913.php">Paniko</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u la malsano ne jam transsaltis anka&ubreve; al porkoj, <tld/>oj, meleagroj, anseroj
        <fnt>
          <bib>Monato</bib>
          <aut>S. Maul</aut>
          <lok><url
          ref="&Steloj;monato2/007332.html">&Steloj;monato2/007332.html</url></lok>
        </fnt>?
      </ekz>
      <ekz>
        al feli&ccirc;ulo e&ccirc; <tld/>o donas ovojn
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        bovo prenita, <tld/>o donita kaj &dash; kvita
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <uzo tip="stl">RAR</uzo>
        trompita <tld/>o <klr>(trompita edzo)</klr>;
        <klr>(vd <ref tip="vid" cel="kokr.0i">kokri</ref>)</klr>
      </ekz>
      <ekz>
        <ctl><tld lit="K"/>o krias jam!</ctl>
        <klr>(novelaro de <nom>Ferenc Szil&aacute;gyi</nom>)</klr>
        <fnt>
          <aut>Szil&aacute;gyi F.</aut>
          <vrk>Koko krias jam!</vrk>
          <lok>1955</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="meleag.0o">meleagro</ref>,
      <ref cel="numid.0o">numido</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_p;&c_je;&c_v;&c_je;&c_n;&c_mol;</trd>
    </trdgrp>   
    <trd lng="bg">&c_p;&c_je;&c_t;&c_je;&c_l;</trd>
    <trdgrp lng="br">  
      <trd>kilhog</trd>,
      <trd>kog</trd>
    </trdgrp>
    <trd lng="de">Hahn</trd>
    <trd lng="fr">coq<klr tip="ind"> (domestique)</klr></trd>
    <trd lng="it">gallo <klr tip="ind">(domestico)</klr></trd>
    <trd lng="pl">kura domowa</trd>
    <trd lng="ro">g&abreve;in&abreve;</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_je;&c_t;&c_u;&c_h;</trd>
    </trdgrp>
    <trd lng="tr">horoz</trd>
  </snc>
  <snc mrk="kok.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>      
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el 
      familio <ref tip="super" cel="fazane.0oj">fazanedoj</ref>
      (<trd lng="la">Gallus</trd>), <ref tip="lst" cel="bird.0o" lst="voko:birdoj">birdo</ref>
      kun ru&gcirc;a kresto sur la kapo.
    </dif>
    <bld lok="../bld/kok.jpg">
      vira <tld/>o
    </bld>
    <refgrp tip="vid">
       <ref cel="lagop.0o">lagopo</ref>,
       <ref cel="tetr.0o">tetro</ref>,
       <ref cel="urogal.0o">urogalo</ref>
    </refgrp>
    <trd lng="be">&c_k;&c_u;&c_r;&c_y;&c_c;&c_a;
    <klr>(&c_v;&c_ib;&c_d;)</klr></trd>
    <trd lng="de">Huhn</trd>
    <trd lng="es">Gallo (vira koko), gallina (por kokino)</trd>
    <trd lng="it">gallo <klr tip="ind">(selvatico)</klr></trd>
    <trd lng="pl">kura</trd>     
    <trd lng="ro">g&abreve;in&abreve;</trd>
    <trd lng="ru">&c_k;&c_u;&c_r;&c_i;&c_c;&c_a;</trd>
    <trd lng="tr">tavuk</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>kohout</trd>,
    <trd>kokus</trd>,
    <trd>kulovit&aacute; bakterie</trd>
  </trdgrp>
  <trd lng="en">chicken</trd>
  <trd lng="hu">ty&uacute;k</trd>
  <trd lng="id">ayam</trd>
  <trd lng="io">hano</trd>
  <trd lng="it">gallo</trd>
  <trd lng="nl">hoen</trd>
  <trdgrp lng="sk">
    <trd>gu&lcaron;ovit&aacute; bakt&eacute;ria</trd>,
    <trd>kokus</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_ib;&c_v;&c_je;&#x43D;&#x44C;</trd>,
    <trd>&#x43A;&c_u;&c_r;&#x43A;&c_a;</trd>
  </trdgrp>
  <trd lng="vo">gok</trd>
  <trdgrp lng="zh">
    <trd>&#x9E21;</trd>,
    <trd><klr>tr. </klr>&#x96DE; [<ind>j&imacron;</ind>]</trd>
  </trdgrp>
</drv>

<drv mrk="kok.0ido">
  <kap><tld/>ido</kap>
  <snc mrk="kok.0ido.AGR">
    <dif>
      Ido de <tld/>o:
      <ekz>
        la maljuna korta <tld/>ino <klr>[&#8230;]</klr> kvin fojojn elkovis <tld/>idojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj malgrandaj <tld/>idoj forte tremis
        <fnt>
          <bib>Fab4</bib>
          <lok>Korta koko kaj ventkoko</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni rostas hodia&ubreve; junajn <tld/>idojn
        <fnt>
          <bib>Fab3</bib>
          <lok>Johan&ccirc;jo-malsa&gcirc;ulo</lok>
        </fnt>;
      </ekz>
      <ekz>
        estas malsa&gcirc;e kalkuli la <tld/>idojn anta&ubreve; la elkovi&gcirc;o
        <fnt>
          <aut>J. Merchant</aut>
          <vrk>Kompatinda Klem</vrk>
          <lok>1931</lok>
        </fnt>;
      </ekz>
      <ekz>
        se vulpo pentofaras, gardu la <tld/>idojn
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_r;&c_a;&c_n;&c_ja;</trd>
  <trd lng="bg">&c_p;&c_i;&c_l;&c_je;&c_n;&c_c;&c_je;</trd>
  <trd lng="bo">&t_ba;&t_yata;&t_tsheg;&t_sa;&t_data;&t_gigu;&t_tsheg;</trd>
  <trd lng="br">po&ntilde;sin</trd>
  <trd lng="cs">ku&rcaron;ice</trd>
  <trd lng="de">K&uuml;ken</trd>
  <trdgrp lng="es">
    <trd>pollo</trd>,
    <trd>polluelo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>poussin</trd>,
    <trd>poulet</trd>
  </trdgrp>
  <trd lng="it">pulcino</trd>
  <trd lng="nl">kuiken</trd>
  <trdgrp lng="pl">
    <trd>kurcz&eogonek;</trd>,
    <trd>kurczak</trd>
  </trdgrp>
  <trd lng="ro">pui</trd>
  <trd lng="ru">&c_c;&c_y;&c_p;&c_l;&c_jo;&c_n;&c_o;&c_k;</trd>
  <trd lng="sk">sliepka</trd>
  <trd lng="uk">&#x43A;&c_u;&c_r;&c_ch;&c_a;</trd>
</drv>

<drv mrk="kok.0ino">
  <kap><ofc>*</ofc><tld/>ino
  <fnt><bib>F</bib>, <lok>Universala Vortaro, -in'</lok></fnt></kap>
  <snc mrk="kok.0ino.AGR">
    <dif>
      Inseksa <tld/>o<sncref ref="kok.0o.AGR"/>:
      <ekz>
        <tld/>inejo
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la korto staras <tld/>o kun tri <tld/>inoj
        <fnt>
          <bib>F</bib>
          <vrk>Ekzercaro</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>inoj kaj <tld/>idoj kuris sur la sterkamason, ilin sekvis per kavaliraj
        pa&scirc;oj la <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Korta koko kaj ventkoko</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi volis kolekti viajn infanojn, kiel <tld/>ino kolektas sian idaron sub la
        flugilojn
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 23:37</lok>
        </fnt>;
      </ekz>
      <ekz>
        al <tld/>ino la ovo lecionojn ne donu
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; blinda <tld/>ino povas trovi grajnon
        <fnt>
          <bib>PrV</bib>
        </fnt>.
        <trd lng="de">auch ein blindes Huhn findet manchmal ein Korn</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_u;&c_r;&c_y;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_k;&c_o;&c_k;&c_o;&c_sh;&c_k;&c_a;</trd>
  <trd lng="br">yar</trd>
  <trd lng="cs">coquina</trd>
  <trd lng="de">Huhn</trd>
  <trd lng="es">gallina</trd>
  <trd lng="fr">poule</trd>
  <trd lng="io">hanino</trd>
  <trd lng="it">gallina</trd>
  <trd lng="nl">hen<klr tip="ind"> (zn.)</klr></trd>
  <trd lng="pl">kura</trd>
  <trd lng="ro">g&abreve;in&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_r;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">coquina</trd>
  <trd lng="uk">&#x43A;&c_u;&c_r;&#x43A;&c_a;</trd>
  <trd lng="vo">jigok</trd>
</drv>

<drv mrk="kok.vir0o">
  <kap>vir<tld/>o</kap>
  <snc mrk="kok.vir0o.AGR">
    <dif>
      Virseksa <tld/>o<sncref ref="kok.0o.AGR"/>:
      <ekz>
        <klr>[ili]</klr> estas bonaj soldatoj, sed ankora&ubreve; pli bonaj vir<tld/>oj
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ni incitas la noblajn bumbukojn unu kontra&ubreve; la alian, kiel la vir<tld/>ojn en
        areno
        <fnt>
          <bib>VaK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_v;&c_je;&c_n;&c_mol;</trd>
  <trd lng="de">Hahn</trd>
  <trdgrp lng="en">
    <trd>cock</trd>,
    <trd>rooster</trd>
  </trdgrp>
  <trd lng="es">gallo</trd>
  <trd lng="fr">coq</trd>
  <trd lng="io">hanulo</trd>
  <trd lng="it">gallo</trd>
  <trd lng="nl">haan</trd>
  <trd lng="pl">kogut</trd>
  <trd lng="ro">coco&#537;</trd>
  <trd lng="ru">&c_p;&c_je;&c_t;&c_u;&c_h;</trd>
  <trd lng="uk">&#x43F;&c_ib;&c_v;&c_je;&#x43D;&#x44C;</trd>
  <trd lng="vo">higok</trd>
</drv>

<drv mrk="kok.0oformaj">
  <kap><tld/>oformaj</kap>
  <snc mrk="kok.0oformaj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref> el la klaso <ref
      tip="malprt" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Galliformes</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_r;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja;
  <klr tip="amb">(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trdgrp lng="cs">
    <trd>hrabav&iacute;</trd>,
    <trd>kurovit&iacute;</trd>
  </trdgrp>
  <trd lng="de">H&uuml;hnerv&ouml;gel</trd>
  <trd lng="es">galliformes</trd>
  <trd lng="fr">galliformes</trd>
  <trd lng="hu">ty&uacute;kalak&uacute;ak</trd>
  <trd lng="it">galliformi</trd>
  <trd lng="nl">hoenderachtigen</trd>
  <trdgrp lng="pl">
    <trd>grzebi&aogonek;ce</trd>,
    <trd>kuraki</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>zg&acirc;rierea</trd>,
    <trd>galliformes</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_u;&c_r;&c_i;&c_n;&c_y;&c_je; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trd lng="sk">kurovit&iacute;</trd>
</drv>

<drv mrk="kok.akvo0o">
  <kap>akvo<tld/>o,
  <var><kap>mar&ccirc;o<tld/>o</kap></var></kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="kok.akvo0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="fulik.0o">Fuliko</ref>,
      <ref tip="dif" cel="galinu.0o">galinolo</ref>
      a&ubreve; simila birdo:
      <ekz>
        <nom>&Hcirc;ufu</nom> <klr>[&#8230;]</klr> ordonas dekapigon de <klr>[&#8230;]</klr> akvo<tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=374796">Papiruso Westcar</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_y;&c_s;&c_k;&c_a;</trd>
  <trd lng="br">duanenn</trd>
  <trdgrp lng="cs">
    <trd>lyska</trd>,
    <trd>sl&iacute;pka zelenonoh&aacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Blessralle</trd>,
    <trd>Teichralle</trd>,
    <trd>Teichhuhn</trd>
  </trdgrp>
  <trd lng="en">coot</trd>
  <trdgrp lng="fr">
    <trd>foulque</trd>,
    <trd>poule d'eau</trd>
  </trdgrp>
  <trd lng="hu">sz&aacute;rcsa</trd>
  <trd lng="it">folaga</trd>
  <trd lng="nl">koet</trd>
  <trd lng="pl">&lstroke;yska</trd>
  <trd lng="ro">li&#537;i&#x21B;&abreve;</trd>
  <trd lng="ru">&c_l;&c_y;&c_s;&c_u;&c_h;&c_a;</trd>
  <trd lng="sk">sliepka zelenonoh&aacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&#x43C;&c_i;&c_sh;&#x43D;&c_i;&c_c;&#x44F;</trd>,
    <trd>&c_v;&#x43E;&c_d;&#x44F;&#x43D;&c_a; &#x43A;&c_u;&c_r;&#x43E;&c_ch;&#x43A;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="kok.vento0o">
  <kap>vento<tld/>o,
  <var><kap>vent<tld/>o</kap></var></kap>
  <snc mrk="kok.vento0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Turni&gcirc;anta koko-silueta <ref tip="super" cel="montr.vent0ilo">ventomontrilo</ref>
      ordinare starigita sur firsto...:
      <ekz>
        la tegoloj falis malsupren, sed la vent<tld/>o sidis firme, &gcirc;i e&ccirc; ne turnis sin
        <fnt>
          <bib>Fab4</bib>
          <lok>Korta koko kaj ventkoko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li vento<tld/>as la&ubreve; la politikaj ventoj interne de sia partio
        <fnt>
          <aut>Izumi Y.</aut>,
          <vrk>Nova kabineto de Tanaka-Nakasone<url ref=""/></vrk>
          <bib>Monato</bib>
          <lok>1983:1, p. 22a-23a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_n;&c_mol;&c_d;&c_z;&c_je;&c_r;&c_a;</trd>,
    <trd>&c_f;&c_l;&c_ju;&c_g;&c_je;&c_r; <klr>(&c_u; &c_v;&c_y;&c_g;&c_l;&c_ja;&c_d;&c_z;&c_je; &c_p;&c_je;&c_w;&c_n;&c_ja;)</klr></trd>
  </trdgrp>
  <trd lng="de">Wetterhahn</trd>
  <trd lng="fr">girouette<klr> (en forme de coq)</klr></trd>
  <trd lng="uk">&c_f;&#x43B;&#x44E;&c_gu;&c_je;&c_r;</trd>
</drv>
</art>
