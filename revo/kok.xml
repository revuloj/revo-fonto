<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kok</rad>/o
</kap>
<drv mrk="kok.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kok.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="fazane.0oj">fazanedoj</ref>
      (<trd lng="la">Gallus</trd>), kun ru&gcirc;a kresto sur la kapo.
    </dif>
    <bld lok="../bld/kok.jpg">
      vira <tld/>o
    </bld>
    <trd lng="de">Huhn</trd>
    <trd lng="es">Gallo (vira koko), gallina (por kokino)</trd>
    <trd lng="ru">&c_k;&c_u;&c_r;&c_i;&c_c;&c_a;</trd>
    <trd lng="tr">tavuk</trd>
  </snc>
  <snc mrk="kok.0o.hejmbesto">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Specio el la genro <ref tip="super" cel="kok.0o.genro"><tld/>o</ref>,
      bredata kiel kortbirdo en multaj rasoj pro la viando kaj la ovoj;
      precipe ties viro:
      <ekz>
        al feli&ccirc;ulo e&ccirc; <tld/>o donas
        ovojn<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        vivi kiel <tld/>o en butero<fnt>Z</fnt>
        <klr>(tre komforte, kiel grasigita <tld/>o)</klr>;
      </ekz>
      <ekz>
        trompita <tld/>o <klr>(trompita edzo)</klr>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kokr.0i">kokri</ref>
    <trd lng="de">Hahn</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_u;&c_r;&c_i;&c_c;&c_a; <klr>(kokino)</klr></trd>,
      <trd>&c_p;&c_je;&c_t;&c_u;&c_h; <klr>(virkoko)</klr></trd>
    </trdgrp>
    <trd lng="tr">horoz</trd>
  </snc>
  <trd lng="cs">kohout</trd>
  <trd lng="en">chicken</trd>
  <trdgrp lng="fr">
    <trd>poulet <klr>(por kuirado)</klr></trd>,
    <trd>poule <klr>(kokino)</klr></trd>,
    <trd>coq</trd>
  </trdgrp>
  <trd lng="hu">ty&uacute;k</trd>
  <trd lng="nl">hoen</trd>
</drv>

<drv mrk="kok.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Galliformes</trd>).
    </dif>
  </snc>
  <trd lng="de">H&uuml;hnerv&ouml;gel</trd>
  <trd lng="hu">ty&uacute;kalak&uacute;ak</trd>
  <trd lng="nl">hoenderachtigen</trd>
  <trd lng="ru">&c_k;&c_u;&c_r;&c_i;&c_n;&c_y;&c_je; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
</drv>

<drv mrk="kok.akvo0o">
  <kap>akvo<tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <uzo tip="stl">KOMUNE</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="fulik.0o">Fuliko</ref>.
    </dif>
  </snc>
  <trd lng="ru">&c_l;&c_y;&c_s;&c_u;&c_h;&c_a;</trd>
</drv>
</art>
