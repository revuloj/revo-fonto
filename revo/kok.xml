<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kok</rad>/o
</kap>
<drv mrk="kok.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kok.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="fazane.0oj">fazanedoj</ref>
      (<trd lng="la">Gallus</trd>), kun ru&gcirc;a kresto sur la kapo.
    </dif>
    <bld lok="../bld/kok.jpg">
      vira <tld/>o
    </bld>
    <trd lng="be">&c_k;&c_u;&c_r;&c_y;&c_c;&c_a; <klr>(&c_v;&c_ib;&c_d;)</klr></trd>
    <trd lng="de">Huhn</trd>
    <trd lng="es">Gallo (vira koko), gallina (por kokino)</trd>
    <trd lng="ru">&c_k;&c_u;&c_r;&c_i;&c_c;&c_a;</trd>
    <trd lng="tr">tavuk</trd>
  </snc>
  <snc mrk="kok.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Specio el la genro <ref tip="super" cel="kok.0o.ZOO"><tld/>o</ref>,
      bredata kiel kortbirdo en multaj rasoj pro la viando kaj la ovoj;
      precipe ties virseksulo:
      <ekz>
        al feli&ccirc;ulo e&ccirc; <tld/>o donas ovojn
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <uzo tip="stl">RAR</uzo>
        trompita <tld/>o <klr>(trompita edzo)</klr>;
      </ekz>
      <ekz>
        <ctl><tld lit="K"/>o krias jam!</ctl>
        <klr>(novelaro de Szil&aacute;gyi Ferenc)</klr>
        <fnt><aut>Szil&aacute;gyi Ferenc</aut>, <vrk>Koko krias jam! - La Laguna:
        J. R&eacute;gulo, 1955</vrk></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kokr.0i">kokri</ref>
    <trdgrp lng="be">
      <trd>&c_p;&c_je;&c_v;&c_je;&c_n;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">Hahn</trd>
    <trd lng="fr">coq<klr tip="ind"> (domestique)</klr></trd>
    <trd lng="pl">kura domowa</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_je;&c_t;&c_u;&c_h;</trd>
    </trdgrp>
    <trd lng="tr">horoz</trd>
  </snc>
  <trd lng="cs">kohout</trd>
  <trd lng="en">chicken</trd>
  <trd lng="hu">ty&uacute;k</trd>
  <trd lng="nl">hoen</trd>
  <trdgrp lng="zh">
    <trd>&#40481;</trd>,
    <trd><klr>tr. </klr>&#38622;<klr> (j&#299;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="kok.0ido">
  <kap><tld/>id(in)o</kap>
  <snc>
    <dif>
      Ido de <tld/>o.
    </dif>
  </snc>
  <trd lng="fr"><ind>poulet</ind></trd>
  <trd lng="pl">kurcz&eogonek;</trd>
  <trd lng="ru">&c_c;&c_y;&c_p;&c_l;&c_jo;&c_n;&c_o;&c_k;</trd>
</drv>

<drv mrk="kok.0ino">
  <kap><tld/>ino</kap>
  <snc>
    <dif>
      Inseksa <tld/>o <sncref ref="kok.0o.AGR"/>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_u;&c_r;&c_y;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="fr">poule</trd>
  <trd lng="pl">kura</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_r;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="kok.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="kok.0oformaj.ZOO">
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Galliformes</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_r;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja; <klr tip="amb">(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="de">H&uuml;hnerv&ouml;gel</trd>
  <trd lng="fr">galliformes</trd>
  <trd lng="hu">ty&uacute;kalak&uacute;ak</trd>
  <trd lng="nl">hoenderachtigen</trd>
  <trd lng="ru">&c_k;&c_u;&c_r;&c_i;&c_n;&c_y;&c_je; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
</drv>

<drv mrk="kok.akvo0o">
  <kap>akvo<tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="kok.akvo0o.ZOO">
    <ref tip="dif" cel="fulik.0o">fuliko</ref>
  </snc>
  <trd lng="be">&c_l;&c_y;&c_s;&c_k;&c_a;</trd>
  <trd lng="cs">lyska</trd>
  <trd lng="de">Blessralle</trd>
  <trd lng="en">coot</trd>
  <trd lng="fr">foulque</trd>
  <trd lng="nl">koet</trd>
  <trd lng="hu">sz&aacute;rcsa</trd>
  <trd lng="pl">&lstroke;yska</trd>
  <trd lng="ru">&c_l;&c_y;&c_s;&c_u;&c_h;&c_a;</trd>
</drv>
</art>
