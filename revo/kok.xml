<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kok</rad>/o
</kap>
<drv mrk="kok.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="fazane.0oj">fazanedoj</ref>
      (<trd lng="la">Gallus</trd>), kun ru&gcirc;a kresto sur la kapo.
      Unu specio estas bredata kiel kortbirdo en multaj rasoj pro la
      viando kaj la ovoj:
      <ekz>
        al feli&ccirc;ulo e&ccirc; <tld/>o donas ovojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        vivi kiel <tld/>o en butero<fnt>Z</fnt>
        <klr>(tre komforte, kiel grasigita <tld/>o)</klr>;
      </ekz>
      <ekz>
        trompita <tld/>o <klr>(trompita edzo)</klr>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kokr.0i">kokri</ref>
  </snc>
  <trd lng="cs">kohout</trd>
  <trdgrp lng="de">
    <trd>Huhn</trd>,
    <trd>Hahn <klr>(masklo)</klr></trd>
  </trdgrp>
  <trd lng="en">chicken</trd>
  <trdgrp lng="fr">
    <trd>poulet <klr>(por kuirado)</klr></trd>,
    <trd>poule <klr>(kokino)</klr></trd>,
    <trd>coq</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_t;&c_u;&c_h; <klr>(masklo)</klr></trd>,
    <trd>&c_k;&c_u;&c_r;&c_i;&c_c;&c_a; <klr>(kokino, &gcirc;enerala nomo)</klr></trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>tavuk</trd>,
    <trd>horoz <klr>(masklo)</klr></trd>
  </trdgrp>
</drv>
<drv mrk="kok.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Galliformes</trd>).
    </dif>
  </snc>
  <trd lng="de">H&uuml;hnerv&ouml;gel</trd>
  <trd lng="ru">&c_k;&c_u;&c_r;&c_i;&c_n;&c_y;&c_je;</trd>
</drv>
</art>
