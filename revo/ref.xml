<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ref.xml,v 1.14 2022/03/26 18:10:07 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>ref</rad>/o
</kap>
<drv mrk="ref.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SHI</uzo>
  <snc>
    <dif>
      Parto de velo, kiun oni faldas por malgrandigi la supra&jcirc;on
      allasitan
      al vento.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_y;&c_f; <klr tip="amb">(&c_v;&c_je;&c_t;&c_r;&c_a;&c_z;&c_ja;)</klr></trd>
  <trdgrp lng="cs">
    <trd>shrnovac&iacute; plachta</trd>,
    <trd>skas&aacute;vac&iacute; pruh plachty</trd>
  </trdgrp>
  <trd lng="de">Reff</trd>
  <trd lng="en">reef<klr tip="amb"> (sail)</klr></trd>
  <trdgrp lng="hu">
    <trd>kurt&iacute;t&aacute;s<klr tip="ind"> (vitorl&aacute;&eacute;)</klr></trd>,
    <trd>harmadol&aacute;s<klr tip="ind"> (vitorl&aacute;&eacute;)</klr></trd>,
    <trd>fog&aacute;s<klr tip="ind"> (vitorl&aacute;&eacute;)</klr></trd>,
    <trd>reff</trd>
  </trdgrp>
  <trd lng="nl">reef</trd>
  <trd lng="ru">&c_r;&c_i;&c_f; <klr tip="amb">(&c_ch;&c_a;&c_s;&c_t;&c_mol;
  &c_p;&c_a;&c_r;&c_u;&c_s;&c_a;)</klr></trd>
  <trd lng="sk">zhrnovacia plachta</trd>
  <trd lng="uk">&#x440;&#x438;&#x444; (&#x447;&#x430;&#x441;&#x442;&#x438;&#x43D;&#x430; &#x432;&#x456;&#x442;&#x440;&#x438;&#x43B;&#x430;),</trd>
</drv>

<drv mrk="ref.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Faldi <tld/>on:
      <ekz>
        por <tld/>i, paki a&ubreve; malpaki la velon sufi&ccirc;a nombro da homoj staras sur &scirc;nurego
        sub la jardo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=265956">Jardo (&scirc;ipero)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_a;&c_c;&c_mol; &c_r;&c_y;&c_f;</trd>
  <trdgrp lng="cs">
    <trd>shrnout</trd>,
    <trd>skasat</trd>
  </trdgrp>
  <trd lng="de">reffen</trd>
  <trd lng="en">reef<klr tip="amb"> (sail)</klr></trd>
  <trdgrp lng="hu">
    <trd>kurt&iacute;t<klr tip="ind"> (vitorl&aacute;t)</klr></trd>,
    <trd>harmadol&aacute;s<klr tip="ind"> (vitorl&aacute;t)</klr></trd>,
    <trd>fog&aacute;st bek&ouml;t<klr tip="ind"> (vitorl&aacute;n&aacute;l)</klr></trd>,
    <trd>reffel</trd>
  </trdgrp>
  <trd lng="nl">reven</trd>
  <trd lng="ru">&c_b;&c_r;&c_a;&c_t;&c_mol; &c_r;&c_i;&c_f;</trd>
  <trd lng="sk">zhrn&uacute;&tcaron; plachtu</trd>
  <trdgrp lng="uk">
    <trd>&#x431;&#x440;&#x430;&#x442;&#x438;/&#x432;&#x437;&#x44F;&#x442;&#x438; &#x440;&#x438;&#x444;</trd>,
    <trd>&#x440;&#x438;&#x444;&#x438;&#x442;&#x438;</trd>
  </trdgrp>
</drv>
</art>
