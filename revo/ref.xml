<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>ref</rad>/o
</kap>
<drv mrk="ref.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Parto de velo, kiun oni faldas por malgrandigi la supra&jcirc;on allasitan
      al vento.
    </dif>
  </snc>
  <trd lng="de">Reff</trd>
  <trd lng="nl">reef</trd>
  <trd lng="ru">&c_r;&c_i;&c_f; <klr tip="amb">(&c_ch;&c_a;&c_s;&c_t;&c_mol; &c_p;&c_a;&c_r;&c_u;&c_s;&c_a;)</klr></trd>
</drv>

<drv mrk="ref.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Faldi <tld/>on.
    </dif>
  </snc>
  <trd lng="de">reffen</trd>
  <trd lng="nl">reven</trd>
  <trd lng="ru">&c_b;&c_r;&c_a;&c_t;&c_mol; &c_r;&c_i;&c_f;</trd>
</drv>
</art>
