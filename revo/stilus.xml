<art mrk="$Id: stilus.xml,v 1.17 2023/10/24 21:23:00 revo Exp $">
<kap>
  <rad>stilus</rad>/o
</kap>
<drv mrk="stilus.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      Parto de la <ref tip="malprt" cel="pistil.0o">pistilo</ref> portanta la stigmon,
      sed ofte mankanta:
      <ekz>
        fuksio <klr>[&#8230;]</klr> havas ok longajn stamenojn kaj longan pinte kvar-fanditan <tld/>on
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=20164">Fuksio</url></lok>
        </fnt>.
      </ekz>
    </dif>

  </snc>
  <trd lng="be">&c_sh;&c_y;&c_j;&c_k;&c_a; <klr>(&c_s;&c_l;&c_u;&c_p;&c_k;&c_a; &c_k;&c_v;&c_je;&c_t;&c_k;&c_ib;)</klr></trd>
  <trdgrp lng="cs">
    <trd>stylus</trd>,
    <trd>&ccaron;n&ecaron;lka</trd>
  </trdgrp>
  <trd lng="de">Griffel</trd>
  <trd lng="fr">style</trd>
  <trd lng="hu">bibesz&aacute;l</trd>
  <trd lng="ja">&#x82B1;&#x67F1;  <pr>&#x304B;&#12385;&#12421;&#12358;</pr></trd>
  <trd lng="nl">stijl<klr tip="ind"> (v..bloem)</klr></trd>
  <trd lng="pl">szyjka<klr>  (s&lstroke;upka)</klr></trd>
  <trd lng="ru">&c_s;&c_t;&c_o;&c_l;&c_b;&c_i;&c_k; <klr tip="amb">(&c_ch;&c_a;&c_s;&c_t;&c_mol; &c_p;&c_je;&c_s;&c_t;&c_i;&c_k;&c_a;)</klr></trd>
  <trd lng="sk">&ccaron;nelka</trd>
  <trd lng="uk">&c_s;&c_t;&#x43E;&c_v;&#x43F;&c_ch;&c_i;&#x43A; (&#x43A;&c_v;&c_ib;&c_t;&#x43A;&#x43E;&c_v;&#x43E;&c_ji;
  &#x43C;&c_a;&c_t;&#x43E;&c_ch;&#x43A;&c_i;)</trd>
</drv>
</art>
