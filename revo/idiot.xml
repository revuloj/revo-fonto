<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>idiot</rad>/o
</kap>
<drv mrk="idiot.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Homo senigita je inteligenteco pro cerbaj difektoj:
      <ekz>alkoholismo &ccirc;e la patro igas ofte la infanojn <tld/>oj.</ekz>
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Homo tute ne inteligenta; malsa&gcirc;ega. 
    </dif>
    <trd lng="de">Idiot</trd>
    <trd lng="sv">idiot</trd>
    <trd lng="ru">&c_b;&c_o;&c_l;&c_v;&c_a;&c_n;</trd>
  </snc>
  <trd lng="nl">idioot</trd>
  <trd lng="ru">&c_i;&c_d;&c_i;&c_o;&c_t;</trd>
</drv>
<drv mrk="idiot.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Stato de <tld/>o. 
    </dif>
  </snc>
  <trd lng="nl">idiotie</trd>
  <trd lng="ru">&c_i;&c_d;&c_i;&c_o;&c_t;&c_i;&c_z;&c_m;</trd>
</drv>
<drv mrk="idiot.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi iun <tld/>o:
      <ekz>la kutima ebrieco <tld/>igas.</ekz>
    </dif>
  </snc>
</drv>
</art>
