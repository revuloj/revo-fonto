<drv mrk="idiot.0o">
  <kap><tld/>o</kap>
  <snc mrk="idiot.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Homo senigita je inteligenteco pro cerbaj difektoj:
      <ekz>alkoholismo &ccirc;e la patro igas ofte la infanojn
<tld/>oj.</ekz>
    </dif>
  <trdgrp lng="hu">
    <trd>gyengeelm&eacute;j&udblac;</trd>,
    <trd>idi&oacute;ta</trd>
  </trdgrp>
  <trd lng="pl">idiota</trd>
  </snc>
  <snc mrk="idiot.0o.KOMUNE">
    <dif>
      Homo tute ne inteligenta, malsa&gcirc;ega.
    </dif>
    <ref tip="vid" cel="stult.0a">stulta.</ref>
  <trdgrp lng="hu">
    <trd>h&uuml;lye</trd>,
    <trd>ostoba</trd>
  </trdgrp>
    <trd lng="sv">idiot</trd>
    <trd lng="ru">&c_b;&c_o;&c_l;&c_v;&c_a;&c_n;</trd>
  </snc>
  <ref tip="vid" cel="kreten.0o">kreteno.</ref>
  <trd lng="be">&c_ib;&c_d;&c_y;&c_jo;&c_t;</trd>
  <trd lng="bg">&c_i;&c_d;&c_i;&c_o;&c_t;</trd>
  <trd lng="de">Idiot</trd>
  <trd lng="en">idiot</trd>
  <trd lng="es">idiota</trd>
  <trdgrp lng="fr">
    <trd>idiot</trd>,
    <trd>abruti</trd>,
    <trd>imb&eacute;cile</trd>,
    <trd>cr&eacute;tin</trd></trdgrp>
  <trdgrp lng="id">
    <trd>idiot</trd>,
    <trd>tunagrahita</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x91CD;&#x5EA6;&#x77E5;&#x7684;&#x969C;&#x5BB3;&#x8005;  <pr>&#x3058;&#x3085;&#x3046;&#x3069;&#x3061;&#x3066;&#x304D;&#x3057;&#x3087;&#x3046;&#x304C;&#x3044;&#x3057;&#x3083;</pr></trd>,
    <trd>&#x767D;&#x75F4;  <pr>&#x305F;&#x308F;&#x3051;</pr></trd>,
    <trd>&#x611A;&#x304B;&#x8005;  <pr>&#x304A;&#x308D;&#x304B;&#x3082;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">idioot</trd>
  <trdgrp lng="pl">
    <trd>idiota</trd>,
    <trd>g&lstroke;upek</trd>
  </trdgrp>
  <trd lng="pt">idiota<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="ru">&c_i;&c_d;&c_i;&c_o;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>hlup&#xE1;k</trd>,
    <trd>idiot</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>blbec</trd>,
    <trd>idiot</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x456;&#x434;&#x456;&#x43E;&#x442;</trd>,
    <trd>&#x449;&#x435; &#x439;&#x43E;&#x43B;&#x43E;&#x43F;</trd>,
    <trd>&#x431;&#x43E;&#x432;&#x434;&#x443;&#x440;</trd>,
    <trd>&#x431;&#x435;&#x432;&#x437;&#x44C;</trd>,
    <trd>&#x442;&#x435;&#x43B;&#x435;&#x43F;&#x435;&#x43D;&#x44C;</trd>,
    <trd>&#x43B;&#x43E;&#x431;&#x443;&#x440;</trd>,
    <trd>&#x434;&#x443;&#x440;&#x435;&#x43D;&#x44C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x50BB;&#x5B50; <pr>sh&#x1CE;zi</pr></trd>,
    <trd>&#x50BB;&#x74DC; <pr>sh&#x1CE;gu&#x101;</pr></trd>,
