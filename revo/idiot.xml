<drv mrk="idiot.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="idiot.0eco.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">Malsano</ref> de <tld/>o <sncref ref="idiot.0o.MED"/>.
    </dif>
    <adm>
    Kontroli trd nl. La antauxa difino tekstis nur
    "stato de ~o". Mi sxovis la tradukojn al la medicina
    senco, sed ne certas. <aut>MB</aut>
    </adm>
    <trd lng="de">Idiotismus</trd>
    <trdgrp lng="hu">
      <trd>gyengeelm&eacute;j&udblac;s&eacute;g</trd>,
      <trd>idi&oacute;tizmus</trd>
    </trdgrp>
    <trd lng="nl">idiotie</trd>
    <trd lng="pl">idiotyzm</trd>
  </snc>
  <snc mrk="idiot.0eco.KOMUNE">
    <dif>
      Stato de <tld/>o <sncref ref="idiot.0o.KOMUNE"/>.
    </dif>
    <trd lng="de">Idiotie</trd>
    <trdgrp lng="hu">
      <trd>h&uuml;lyes&eacute;g</trd>,
      <trd>ostobas&aacute;g</trd>
    </trdgrp>
  <trdgrp lng="pl">
    <trd>idiotyzm</trd>,
    <trd>g&lstroke;upota</trd>
  </trdgrp>
  </snc>
  <trd lng="be">&c_ib;&c_d;&c_y;&c_ja;&c_t;&c_y;&c_z;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>blbost</trd>,
    <trd>choromyslnost</trd>,
    <trd>du&#x161;evn&#xED; zaostalost</trd>,
    <trd>idiocie</trd>,
    <trd>idiotie</trd>,
    <trd>idiotstv&#xED;</trd>,
    <trd>slabomyslnost</trd>
  </trdgrp>
  <trd lng="en">idiocy</trd>
  <trd lng="es">idiotez</trd>
  <trd lng="fr">idiotie<klr tip="ind"> (&eacute;tat, maladie)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x91CD;&#x5EA6;&#x306E;&#x77E5;&#x7684;&#x969C;&#x5BB3;  <pr>&#x3058;&#x3085;&#x3046;&#x3069;&#x306E;&#x3061;&#x3066;&#x304D;&#x3057;&#x3087;&#x3046;&#x304C;&#x3044;</pr></trd>,
    <trd>&#x767D;&#x75F4;  <pr>&#x305F;&#x308F;&#x3051;</pr></trd>
  </trdgrp>
  <trd lng="pt">idiotice<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="ru">&c_i;&c_d;&c_i;&c_o;&c_t;&c_i;&c_z;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>idiotstvo</trd>,
    <trd>slabomyse&#x13E;nos&#x165;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x456;&#x434;&#x456;&#x43E;&#x442;&#x438;&#x437;&#x43C;</trd>,
    <trd>&#x43D;&#x435;&#x434;&#x43E;&#x443;&#x43C;&#x43A;&#x443;&#x432;&#x430;&#x442;&#x456;&#x441;&#x442;&#x44C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x767D;&#x75F4; <pr>b&#xE1;ich&#x12B;</pr></trd>,
    <trd>&#x767D;&#x7661; <pr>b&#xE1;ich&#x12B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="idiot.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi iun <tld/>o:
      <ekz>la kutima ebrieco <tld/>igas.</ekz>
    </dif>
  </snc>
  <trd lng="cs">zblbnout <klr>(n&#x11B;koho)</klr></trd>
  <trd lng="de">verbl&ouml;den</trd>
  <trd lng="es">idiotizar</trd>
  <trdgrp lng="fr">
    <trd>rendre <ind>idiot</ind></trd>,
    <trd>abrutir</trd>,
    <trd>cr&eacute;tiniser</trd>
  </trdgrp>
  <trd lng="ja">&#x611A;&#x304B;&#x306B;&#x3059;&#x308B;  <pr>&#x304A;&#x308D;&#x304B;&#x306B;&#x3059;&#x308B;</pr></trd>
  <trd lng="pl">og&lstroke;upia&cacute;</trd>
  <trd lng="pt">idiotizar</trd>
  <trd lng="sk">zblbn&#xFA;&#x165; <klr>(niekoho)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x440;&#x43E;&#x431;&#x438;&#x442;&#x438; &#x456;&#x434;&#x456;&#x43E;&#x442;&#x43E;&#x43C;</trd>,
    <trd>&#x43F;&#x440;&#x438;&#x437;&#x432;&#x43E;&#x434;&#x438;&#x442;&#x438; &#x434;&#x43E; &#x456;&#x434;&#x456;&#x43E;&#x442;&#x438;&#x437;&#x43C;&#x443;</trd>
  </trdgrp>
</drv>
