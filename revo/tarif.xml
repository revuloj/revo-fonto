<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>tarif</rad>/o
</kap>
<drv mrk="tarif.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Difinita prezaro pri komerca&jcirc;oj, laboroj, servoj, impostoj:
      <ekz>
        <tld/>oj de publika transporto pli rapide kreskis ol la kostoj de
        propra veturilo<fnt><aut>Roland Rotsaert</aut>: <vrk>Medio-indulgo
        de la &ccirc;efaj ekonomiaj sektoroj</vrk>, <bib>Monato</bib><lok>jaro
        2001a, numero 3a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Tarif</trd>,
    <trd>Satz</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>tarif</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tarifa</trd>,
    <trd>d&iacute;jszab&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">tarief</trd>
  <trd lng="ru">&c_t;&c_a;&c_r;&c_i;&c_f;</trd>
</drv>

<drv mrk="tarif.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
  <uzo tip="fak">EKON</uzo>
    <dif>
      Difini <tld/>on pri io.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>tarifieren</trd>,
    <trd>Tarife beschlie&szlig;en</trd>,
    <trd>S&auml;tze festlegen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fixer les <ind>prix</ind></trd>,
    <trd>fixer le <ind>tarif</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_i;&c_t;&c_mol; <ind>&c_t;&c_a;&c_r;&c_i;&c_f;</ind></trd>
</drv>
</art>
