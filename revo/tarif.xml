<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>tarif</rad>/o
</kap>
<drv mrk="tarif.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Difinita prezaro pri komerca&jcirc;oj, laboroj, servoj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Tarif</trd>,
    <trd>Satz</trd>
  </trdgrp>
  <trd lng="nl">tarief</trd>
  <trd lng="ru">&c_t;&c_a;&c_r;&c_i;&c_f;</trd>
</drv>

<drv mrk="tarif.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Difini <tld/>on pri io.
    </dif>
  </snc>
  <trd lng="ru">&c_u;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_i;&c_t;&c_mol; <ind>&c_t;&c_a;&c_r;&c_i;&c_f;</ind></trd>
</drv>
</art>
