<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>stras</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="stras.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Juvel-imita&jcirc;o el senvalora materialo.
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="fals.0i">falsi</ref>,
    <ref cel="tombak.0o">tombako</ref>
  </refgrp>
  <trdgrp lng="fr">
    <trd>faux bijou</trd>,
    <trd>strass</trd>,
    <trd>toc<klr> (subst., faux)</klr></trd>,
    <trd>verroterie</trd>
  </trdgrp>
</drv>

<drv mrk="stras.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      El malvera valora materialo:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la &ccirc;efo <klr>...</klr> taksas la planon &leftquot;<tld/>a labora&ccirc;o&rightquot;
        <fnt>
        <aut>Irl</aut>,
        <vrk>&Ccirc;u polica plano nelica?</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 1999a, numero 11a, p. 6a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>factice</trd>,
    <trd>faux<klr> (adj., bijou, m&eacute;tal)</klr></trd>,
    <trd>tape-&agrave;-l'oeil</trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
