<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>teza&ubreve;r</rad>/o
    </kap>
    <uzo tip="stl">NEO</uzo>
    <drv mrk="tezaux.0o">
      <kap><tld/>o</kap>
      <snc>
        <uzo tip="fak">SCI</uzo>
        <dif>
          Sistemo de nocioj de scienco, te&hcirc;nika fako a&ubreve; lingvo.
        </dif>
        <refgrp tip="sub"><ref cel="termin.0aro">terminaro</ref>,
          <ref cel="klasif.0o">klasifiko</ref></refgrp>
        <trd lng="ru">&#x442;&#x435;&#x437;&#x430;&#x443;&#x440;&#x443;&#x441;</trd>
      </snc>
      <snc>
        <uzo tip="fak">LIN</uzo>
        <dif>
          Vortaro prezentanta sistemon de nocioj de fako a&ubreve; lingvo,
          precipe la genra-specajn rilatojn, sinonimojn kaj
          antonimojn, ebliganta trovi vorton la&ubreve; senco.
        </dif>
        <ref tip="super" cel="vort.0aro">vortaro</ref>
      </snc>
      <trdgrp lng="en"><trd>thesaurus</trd>,
        <trd>semantic dictionary</trd></trdgrp>
      <trdgrp lng="ru"><trd>&#x438;&#x434;&#x435;&#x43e;&#x433;&#x440;&#x430;&#x444;&#x438;&#x447;&#x435;&#x441;&#x43a;&#x438;&#x439; &#x441;&#x43b;&#x43e;&#x432;&#x430;&#x440;&#x44c;</trd>,
        <trd>&#x442;&#x435;&#x437;&#x430;&#x443;&#x440;&#x443;&#x441;</trd></trdgrp>
    </drv>
  </art>

