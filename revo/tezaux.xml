<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>teza&ubreve;r</rad>/o
</kap>
<uzo tip="stl">NEO</uzo>

<drv mrk="tezaux.0o">
  <kap><tld/>o</kap>
  <snc mrk="tezaux.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Sistemo de nocioj de scienco, te&hcirc;nika fako a&ubreve; lingvo.
    </dif>
    <refgrp tip="sub">
      <ref cel="termin.0aro">terminaro</ref>,
      <ref cel="klasif.0o">klasifiko</ref>
    </refgrp>
    <trd lng="ru">&c_t;&c_je;&c_z;&c_a;&c_u;&c_r;&c_u;&c_s;</trd>
  </snc>
  <snc mrk="tezaux.0o.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      Vortaro prezentanta sistemon de nocioj de fako a&ubreve; lingvo,
      precipe la genra-specajn rilatojn, sinonimojn kaj
      antonimojn, ebliganta trovi vorton la&ubreve; senco.
    </dif>
    <ref tip="super" cel="vort.0aro">vortaro</ref>
    <trdgrp lng="ru">
      <trd>&c_i;&c_d;&c_je;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; &c_s;&c_l;&c_o;&c_v;&c_a;&c_r;&c_mol;</trd>,
      <trd>&c_t;&c_je;&c_z;&c_a;&c_u;&c_r;&c_u;&c_s;</trd></trdgrp>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_z;&c_a;&c_w;&c_r;&c_u;&c_s;</trd>
  <trd lng="de">Thesaurus</trd>
  <trdgrp lng="en"><trd>thesaurus</trd>,
    <trd>semantic dictionary</trd></trdgrp>
  <trd lng="hu">tezaurusz</trd>
  <trdgrp lng="nl">
    <trd>thesaurus</trd>,
    <trd>semantisch woordenboek</trd>
  </trdgrp>
</drv>
</art>

