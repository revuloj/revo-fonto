<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gamel.xml,v 1.21 2023/10/15 06:07:00 revo Exp $">
<kap>
  <rad>gamel</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="gamel.0o">
  <kap><tld/>o</kap>
  <snc mrk="gamel.0o.MIL">
    <uzo tip="fak">MAR</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Soldata a&ubreve; &scirc;ipana man&gcirc;o<ref tip="super" cel="vaz.0o">vazo</ref> ordinare metala:
      <ekz>
        unutone tintas <tld/>o, kro&ccirc;ita al dorsosako
        <fnt>
          <aut>Vladimir Glazunov</aut>,
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Noveloj/Cxarmo.html">La
          &ccirc;armo de &Ccirc;ielmontaro</url></vrk>,
          <lok>Fonto, numero 37a</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi devis purigi la <tld/>ojn, do la klabitajn lavpelvojn, en kiuj oni alportis
        la man&gcirc;on el la kuirejo
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 27</lok>
        </fnt>;
      </ekz>
      <ekz>
        la poto enhavis <tld/>ojn, kiuj dividis la spacon kaj entenis la diversajn
        man&gcirc;a&jcirc;ojn
        <fnt>
          <bib>KrM</bib>
          <lok>3. Ne Gutas Mielo El La &Ccirc;ielo.</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="mangx.0ilo">man&gcirc;ilo</ref>
    <refgrp tip="vid">
      <ref cel="pot.0o">poto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_c;&c_ja;&c_l;&c_o;&c_k; <klr>(&c_n;&c_a;&c_p;&c_r;., &c_zh;&c_a;&c_w;&c_n;&c_je;&c_r;&c_a;)</klr></trd>
  <trd lng="ca">carmanyola</trd>
  <trdgrp lng="cs">
    <trd>e&scaron;us</trd>,
    <trd>j&iacute;del. miska</trd>,
    <trd>spole&ccaron;n&aacute; m&iacute;sa</trd>,
    <trd>&scaron;&aacute;lek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Essgeschirr</trd>,
    <trd>Kochgeschirr</trd>,
    <trd>Gamelle</trd>,
    <trd>Blechnapf</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>billy can</trd>,
    <trd>dixie</trd>,
    <trd>pannikin</trd>,
    <trd>mess tin</trd>
  </trdgrp>
  <trd lng="fr">gamelle</trd>
  <trd lng="hu">csajka</trd>
  <trdgrp lng="it">
    <trd>gamella</trd>,
    <trd>gavetta</trd>
  </trdgrp>
  <trd lng="ja">&#x98EF;&#x76D2;  <pr>&#x306F;&#x3093;&#x3054;&#x3046;</pr></trd>
  <trdgrp lng="nl">
    <trd>gamel</trd>,
    <trd>etensbak</trd>,
    <trd>eetketeltje</trd>
  </trdgrp>
  <trd lng="pt">marmita</trd>
  <trd lng="ru">&c_k;&c_o;&c_t;&c_je;&c_l;&c_o;&c_k;<klr> (&c_p;&c_o;&c_h;&c_o;&c_d;&c_n;&c_a;&c_ja; &c_p;&c_o;&c_s;&c_u;&c_d;&c_a;)</klr></trd>
  <trdgrp lng="sk">
    <trd>e&scaron;us</trd>,
    <trd>vojensk&aacute; &scaron;&aacute;lka</trd>
  </trdgrp>
  <trd lng="uk">&c_k;&c_a;&c_z;&c_a;&c_n;&c_o;&c_k;&nbsp;(&c_p;&c_o;&c_h;&c_ib;&c_d;&c_n;&c_i;&c_j;)</trd>
</drv>
</art>
