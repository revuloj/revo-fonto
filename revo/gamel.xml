<kap>
  <rad>gamel</rad>/o
</kap>
<drv mrk="gamel.0o">
  <kap><tld/>o</kap>
  <snc mrk="gamel.0o.MIL">
    <uzo tip="fak">MAR</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Soldata a&ubreve; &scirc;ipana man&gcirc;ovazo ordinare metala:
      <ekz>
        unutone tintas <tld/>o, kro&ccirc;ita al dorsosako
        <fnt>
        <aut>Vladimir Glazunov</aut>,
        <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Noveloj/Cxarmo.html">La
        &ccirc;armo de &Ccirc;ielmontaro</url></vrk>,
        <lok>Fonto, numero 37a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_c;&c_ja;&c_l;&c_o;&c_k; <klr>(&c_n;&c_a;&c_p;&c_r;., &c_zh;&c_a;&c_w;&c_n;&c_je;&c_r;&c_a;)</klr></trd>
  <trd lng="ca">carmanyola</trd>
  <trdgrp lng="cs">
    <trd>e&#x161;us</trd>,
    <trd>j&#xED;del. miska</trd>,
    <trd>spole&#x10D;n&#xE1; m&#xED;sa</trd>,
    <trd>&#x161;&#xE1;lek</trd>
  </trdgrp>
  <trd lng="de">Essgeschirr</trd>
  <trdgrp lng="en">
    <trd>billy can</trd>,
    <trd>dixie</trd>,
    <trd>pannikin</trd>,
    <trd>mess tin</trd>
  </trdgrp>
  <trd lng="fr">gamelle</trd>
  <trd lng="hu">csajka</trd>
  <trdgrp lng="it">
    <trd>gamella</trd>,
    <trd>gavetta</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gamel</trd>,
    <trd>etensbak</trd>,
    <trd>eetketeltje</trd>
  </trdgrp>
  <trd lng="pt">marmita</trd>
  <trd lng="ru">&c_k;&c_o;&c_t;&c_je;&c_l;&c_o;&c_k;<klr> (&c_p;&c_o;&c_h;&c_o;&c_d;&c_n;&c_a;&c_ja; &c_p;&c_o;&c_s;&c_u;&c_d;&c_a;)</klr></trd>
  <trdgrp lng="sk">
    <trd>e&#x161;us</trd>,
    <trd>vojensk&#xE1; &#x161;&#xE1;lka</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&#x430;&#x437;&#x430;&#x43D;&#x43E;&#x43A;&#xA0;(&#x43F;&#x43E;&#x445;&#x456;&#x434;&#x43D;&#x438;&#x439;)</trd>
</drv>
