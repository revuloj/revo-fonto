<kap>
  <ofc>1</ofc>
  <rad>revolver</rad>/o
</kap>
<drv mrk="revolv.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Plurpafa <ref tip="super" cel="pistol.0o">pistolo</ref>, kun
      cilindra turni&gcirc;anta &scirc;argilo:
      <ekz>
        oni prenas <tld/>on, en kiun oni &scirc;argis unusolan kuglon, oni ruligas
        la cilindron, metas la bu&scirc;on de la <tld/>o al sia kapo, kaj pafas
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2006/009002.php">Memoroj neniam perdas sian mal&gcirc;ojon</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li supreniris &gcirc;is la dua eta&gcirc;o kaj, portante du <tld/>ojn, ekpafis
        <fnt>
          <bib>Monato</bib>
          <aut>Jozefo Lej&ccirc;</aut>
          <vrk><url ref="&Monato;/2011/010359.php">Tragedio en lernejo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_v;&c_a;&c_l;&c_mol;&c_v;&c_e;&c_r;</trd>
  <trd lng="bg">&c_r;&c_je;&c_v;&c_o;&c_l;&c_v;&c_je;&c_r;</trd>
  <trd lng="ca">rev&ograve;lver</trd>
  <trdgrp lng="cs">
    <trd>oto&ccaron;. z&aacute;sobn&iacute;k</trd>,
    <trd>revolver</trd>
  </trdgrp>
  <trd lng="de">Revolver</trd>
  <trd lng="en">revolver</trd>
  <trd lng="es">rev&oacute;lver</trd>
  <trd lng="fa">&a_t;&f_p;&a_A;&a_n;&f_ch;&a_h;</trd>
  <trd lng="fr">revolver</trd>
  <trd lng="hu">revolver</trd>
  <trdgrp lng="it">
    <trd>revolver</trd>,
    <trd><ind>pistola</ind> a tamburo</trd>
  </trdgrp>
  <trd lng="ja">&#x30EA;&#x30DC;&#x30EB;&#x30D0;&#x30FC;</trd>
  <trd lng="nl">revolver</trd>
  <trd lng="pl">rewolwer</trd>
  <trd lng="pt">rev&oacute;lver</trd>
  <trd lng="ro">revolver</trd>
  <trd lng="ru">&c_r;&c_je;&c_v;&c_o;&c_l;&c_mol;&c_v;&c_je;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>revolver</trd>,
    <trd>z&aacute;sobn&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_je;&c_v;&#x43E;&#x43B;&#x44C;&c_v;&c_je;&c_r;</trd>,
    <trd>&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&#x43D;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5DE6;&#x8F2A;&#x624B;&#x69CD; <pr>zu&#x1D2;l&#xFA;nsh&#x1D2;uqi&#x101;ng</pr></trd>,
    <trd>&#x5DE6;&#x8F6E;&#x624B;&#x67AA; <pr>zu&#x1D2;l&#xFA;nsh&#x1D2;uqi&#x101;ng</pr></trd>,
    <trd>&#x8F49;&#x8F2A;&#x624B;&#x69CD; <pr>zhu&#xE0;nl&#xFA;nsh&#x1D2;uqi&#x101;ng</pr></trd>,
    <trd>&#x8F6C;&#x8F6E;&#x624B;&#x67AA; <pr>zhu&#xE0;nl&#xFA;nsh&#x1D2;uqi&#x101;ng</pr></trd>
  </trdgrp>
