<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <ofc>*</ofc>
    <rad>pork</rad>/o
  </kap>

  <drv mrk="pork.0o">
    <kap><ofc>*</ofc><tld/>o</kap>
    <snc mrk="pork.0o.ZOO">
      <uzo tip="fak">ZOO</uzo>
      <dif>
        <ref tip="lst" cel="genr.0o.BIO"
        lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
        tip="malprt"
        cel="porked.0oj"><tld/>edoj</ref>
        (<trd lng="la">Sus</trd>), kun mallongaj kruroj kaj rostreto.
        Unu specio estas en iuj kulturoj hejme bredata pro viando kaj
        ledo:
        <ekz>
          avarulo kaj <tld/>o estas bonaj post la
          morto <fnt><bib>PrV</bib></fnt>;
        </ekz>
        <ekz>
          <ind>&jcirc;eti perlojn anta&ubreve; la <tld/>ojn</ind>
          <fnt><bib>PrV</bib></fnt>
          <klr> (esprimi altajn ideojn anta&ubreve; malkleruloj)</klr>;
          <trd lng="de">Perlen vor die <ind>S&auml;ue</ind> werfen</trd>
          <trd lng="hu">gy&ouml;ngy&ouml;t sz&oacute;r a diszn&oacute;k
          el&eacute;</trd>
          <trd lng="nl"><ind>paarlen</ind> voor de zwijnen werpen</trd>
          <trd lng="pl">rzuca&cacute; per&lstroke;y mi&eogonek;dzy
          <ind>&sacute;winie</ind></trd>
          <trd lng="ru">&c_m;&c_je;&c_t;&c_a;&c_t;&c_mol;
          &c_b;&c_i;&c_s;&c_je;&c_r; &c_p;&c_je;&c_r;&c_je;&c_d;
          &c_s;&c_v;&c_i;&c_n;&c_mol;&c_ja;&c_m;&c_i;</trd>
          
        </ekz>
        <ekz>
          paperujo el <tld/>ledo.
        </ekz>
      </dif>
      <ref tip="sub" cel="apr.0o.ZOO">apro</ref>
      <trdgrp lng="be">
        <trd>&c_k;&c_a;&c_b;&c_a;&c_n;</trd>,
        <trd>&c_p;&c_a;&c_r;&c_s;&c_ju;&c_k;</trd>,
        <trd>&c_s;&c_mol;&c_v;&c_ib;&c_n;&c_mol;&c_n;&c_ja;</trd>
      </trdgrp>
      <trdgrp lng="bg">
        <trd>&c_s;&c_v;&c_i;&c_n;&c_ja;</trd>,
        <trd>&c_p;&c_r;&c_a;&c_s;&c_je;</trd>
      </trdgrp>
      <trd lng="ca">porc</trd>
      <trdgrp lng="cs">
        <trd>vep&rcaron;</trd>,
        <trd>prase</trd>
      </trdgrp>
      <trd lng="de">Schwein</trd>
      <trd lng="fr">porc</trd>
      <trd lng="en">pig</trd>
      <trdgrp lng="hu">
        <trd>sert&eacute;s</trd>,
        <trd>diszn&oacute;</trd>
      </trdgrp>
      <trd lng="id">babi</trd>
      <trdgrp lng="nl">
        <trd>zwijn</trd>,
        <trd>varken</trd>
      </trdgrp>
      <trd lng="pl">&sacute;winia</trd>
      <trdgrp lng="ru">
        <trd>&c_k;&c_a;&c_b;&c_a;&c_n; <klr>(sova&gcirc;a)</klr></trd>,
        <trd>&c_s;&c_v;&c_i;&c_n;&c_mol;&c_ja; <klr>(hejma)</klr></trd>
      </trdgrp>
      <trd lng="sv">svin</trd>
      <trdgrp lng="pt">
        <trd>porco</trd>,
        <trd>su&iacute;no</trd>
      </trdgrp>
      <trd lng="tr">domuz</trd>
      <trd lng="vo">svin</trd>
    </snc>
  </drv>

  <drv mrk="pork.vir0o">
    <kap>
      vir<tld/>o
    </kap>
    <uzo tip="fak">ZOO</uzo>
    <snc mrk="pork.vir0o.ZOO">
      <dif>
        Virseksa <tld/>o.
      </dif>
    </snc>
    <trd lng="ca">verro</trd>
    <trdgrp lng="pl">
      <trd>knur <klr>(rozp&lstroke;odowy)</klr></trd>,
      <trd>wieprz <klr>(wykastrowany)</klr></trd>
    </trdgrp>
  </drv>

  <drv mrk="pork.0ino">
    <kap>
      <tld/>ino
    </kap>
    <uzo tip="fak">ZOO</uzo>
    <snc mrk="pork.0ino.ZOO">
      <dif>
        Inseksa <tld/>o.
      </dif>
      <trd lng="ca">truja</trd>
      <trdgrp lng="pl">
        <trd>locha</trd>,
        <trd>maciora</trd>
      </trdgrp>
    </snc>
  </drv>

  <drv mrk="pork.0ido">
    <kap>
      <tld/>ido
    </kap>
    <uzo tip="fak">ZOO</uzo>
    <snc mrk="pork.0ido.ZOO">
      <dif>
        Ido de <tld/>o.
      </dif>
      <trdgrp lng="ca">
        <trd>porcell</trd>,
        <trd>garr&iacute;</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>warchlak</trd>
        <trd>prosi&eogonek;</trd>
      </trdgrp>
    </snc>
  </drv>

  <drv mrk="pork.0ajxo">
    <kap>
      <tld/>a&jcirc;o
    </kap>
    <snc mrk="pork.0ajxo.KUI">
      <uzo tip="fak">KUI</uzo>
      <dif>
        Viando de <tld/>o.
      </dif>
      <trd lng="ca">carn de porc</trd>
      <trd lng="pl">wieprzowina</trd>
    </snc>
  </drv>

</art>
