<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kupr.xml,v 1.50 2025/04/12 22:26:43 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kupr</rad>/o
<fnt><bib>UV</bib></fnt></kap>

<drv mrk="kupr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <mlg kod="IUPAC">Cu</mlg>
  <snc mrk="kupr.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="29">elemento</ref>
      kun atomnumero 29, atompezo 63,546; bruneru&gcirc;a peza
      transir<ref tip="super" cel="metal.0o">metalo</ref>, pli fandebla
      ol oro kaj malpli ol <ref tip="vid" cel="argxen.0o">ar&gcirc;ento</ref>:
      <ekz>
        <tld/>o estas uzata precipe por elektraj kondukiloj, kaj en kunfanda&jcirc;oj kun
        <ref tip="vid" cel="stan.0o">stano</ref> (t.e. <ref tip="malprt" cel="bronz.0o">bronzo</ref>) a&ubreve;
        <ref tip="vid" cel="zink.0o">zinko</ref>
        (t.e. <ref tip="malprt" cel="latun.0o">latuno</ref>);
      </ekz>
      <ekz>
        for&gcirc;anto de diversaj majstra&jcirc;oj el <tld/>o kaj fero
        <fnt>
          <bib>MT</bib>
          <lok>&Gen; 4:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        faru stangojn por la altaro, stangojn el akacia ligno, kaj tegu ilin per <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>&Eli; 27:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; la plej lerta po&scirc;o&scirc;telisto ne plu trovus <tld/>eron
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_je;&c_d;&c_z;&c_mol;</trd>
    <trd lng="bg">&c_m;&c_je;&c_d;<klr tip="amb"> (&c_je;&c_l;&c_je;&c_m;&c_je;&c_n;&c_t;)</klr></trd>
    <trd lng="br">kouevr</trd>
    <trd lng="de">Kupfer</trd>
    <trd lng="en">copper</trd>
    <trd lng="es">cobre</trd>
    <trd lng="fr">cuivre</trd>
    <trd lng="hu">r&eacute;z</trd>
    <trd lng="id">tembaga</trd>
    <trd lng="it">rame</trd>
    <trd lng="nl">koper<klr tip="ind"> (metaal)</klr></trd>
    <trd lng="pl">mied&zacute;</trd>
    <trd lng="pt">cobre</trd>
    <trd lng="ru">&c_m;&c_je;&c_d;&c_mol;</trd>
    <trd lng="sv">koppar</trd>
    <trd lng="vi">&#273;&#7891;ng</trd>
    <trdgrp lng="zh">
    <trd>&#37509; <pr>t&oacute;ng</pr></trd>,
    <trd>&#38108; <pr>t&oacute;ng</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="kupr.0o.bronzo">
    <uzo tip="stl">ARK</uzo>
    <uzo tip="fak">BIB</uzo>
    <dif>
      <ref tip="dif" cel="bronz.0o">Bronzo</ref>:
      <ekz>
        jen estas la oferdono <klr>[&#8230;]</klr>: oro kaj ar&gcirc;ento kaj <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 25:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        plej perfekte artiste farita porko el <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        sonorilo <klr>[&#8230;]</klr> fandita el <tld/>o, malmola kaj peza
        <fnt>
          <bib>Fab4</bib>
          <lok>Sonorila profundo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_r;&c_o;&c_n;&c_z;&c_a;</trd>
    <trd lng="bg">&c_b;&c_r;&c_o;&c_n;&c_z;</trd>
    <trd lng="hu">bronz<klr tip="ind"> (elavult)</klr></trd>
    <trd lng="id">perunggu</trd>
    <trd lng="pl">br&aogonek;z</trd>
    <trd lng="ru">&c_b;&c_r;&c_o;&c_n;&c_z;&c_a;</trd>
  </snc>
    <refgrp tip="vid">
      <ref cel="verdig.0o">verdigro</ref>
    </refgrp>
  <trd lng="bo">&t_za;&t_nga;&t_sa;&t_tsheg;</trd>
  <trd lng="cs">m&ecaron;&dcaron;</trd>
  <trd lng="ja">&#37509;  <pr>&#12393;&#12358;</pr></trd>
  <trd lng="sk">me&dcaron;</trd>
  <trd lng="uk">&c_l;&c_a;&c_t;&c_u;&c_n;&c_mol;</trd>
</drv>

<drv mrk="kupr.0a">
<kap><tld/>a</kap>
  <snc mrk="kupr.0a.elkupro">
    <dif>
      Farita el <tld/>o:
      <ekz>
        faru kvindek <tld/>ajn hoketojn
        <fnt>
          <bib>MT</bib>
          <lok>&Eli; 26:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a reto <klr>[kun]</klr> <tld/>aj ringoj sur &gcirc;iaj kvar anguloj
        <fnt>
          <bib>MT</bib>
          <lok>&Eli; 27:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a lavujo
        <fnt>
          <bib>MT</bib>
          <lok>&Eli; 30:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne havanta en la sako e&ccirc; <tld/>an <ref cel="uten.0o">uteno</ref>n
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro 9a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a monero
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="fr">cuivr&eacute;</trd>
  </snc>
  <snc mrk="kupr.0a.kolore">
    <dif>
      Havanta <ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">koloron</ref> de <tld/>o,
      brilan, brune
      <ref tip="super" cel="rugx.0a">ru&gcirc;an</ref>:
      <ekz>
        <tld/>a viza&gcirc;o
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj makuloj super la brovoj kaj sur la vangoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro 16a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="har.0aro.FRIZ">hararo</ref>
  <trd lng="fr">auburn</trd>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_d;&c_n;&c_y;</trd>
  <trdgrp lng="br">
    <trd>kouevr</trd>,
    <trd>kouevrek</trd>
  </trdgrp>
  <trd lng="cs">m&ecaron;d&ecaron;n&eacute; barvy</trd>
  <trd lng="de">kupfern</trd>
  <trd lng="en">copper</trd>
  <trd lng="es">de cobre</trd>
  <trd lng="hu">r&eacute;z-</trd>
  <trdgrp lng="ja">
    <trd>&#37509;&#35069;&#12398;  <pr>&#12393;&#12358;&#12379;&#12356;&#12398;</pr></trd>,
    <trd>&#36196;&#37509;&#33394;&#12398;  <pr>&#12375;&#12419;&#12367;&#12393;&#12358;&#12356;&#12429;&#12398;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>koperen</trd>,
    <trd>uit <ind>koper</ind></trd>
  </trdgrp>
  <trd lng="pl">miedziany</trd>
  <trd lng="ru">&c_m;&c_je;&c_d;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">medenej farby</trd>
  <trd lng="uk">&c_m;&c_ib;&c_d;&c_n;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#32043;&#37509; <pr>z&#464;t&oacute;ng</pr></trd>,
    <trd>&#32043;&#38108; <pr>z&#464;t&oacute;ng</pr></trd>
  </trdgrp>
</drv>

