<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kupr</rad>/o
</kap>

<drv mrk="kupr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <mlg>Cu</mlg>
  <snc>
  <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref>kun 
      atomnumero 29, atompezo 63,546, simbolo <frm>Cu</frm>. Bruneru&gcirc;a
      peza 
      transir<ref tip="super" cel="metal.0o">metalo</ref>, pli fandebla 
      ol oro kaj malpli 
      ol <ref tip="vid" cel="argxen.0o">ar&gcirc;ento</ref>, 
      uzata precipe por elektraj kondukiloj, kaj en kunfanda&jcirc;oj kun 
      <ref tip="vid" cel="stan.0o">stano</ref> (= <ref tip="malprt" 
      cel="bronz.0o">bronzo</ref>) kaj kun 
      <ref tip="vid" cel="zink.0o">zinko</ref> 
      (= <ref tip="malprt" cel="latun.0o">latuno</ref>):
      <ekz>
        orita <tld/>o;
      </ekz>
      <ekz>
        <tld/>a koloro de ha&ubreve;to.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_je;&c_d;&c_z;&c_mol;</trd>
    <trd lng="br">kouevr</trd>
    <trd lng="de">Kupfer</trd>
    <trd lng="en">copper</trd>
    <trd lng="es">cobre</trd>
    <trd lng="hu">r&eacute;z</trd>
    <trd lng="it">rame</trd>
    <trd lng="nl">koper<klr tip="ind"> (metaal)</klr></trd>
    <trd lng="pl">mied&zacute;</trd>
    <trd lng="pt">cobre</trd>
    <trd lng="ru">&c_m;&c_je;&c_d;&c_mol;</trd>
    <trd lng="sv">koppar</trd>
  </snc>
  <snc>
  <uzo tip="fak">BIB</uzo>
    <dif>
      Arkaika por <ref tip="sin" cel="bronz.0o">bronzo</ref>.
    </dif>
    <trd lng="be">&c_b;&c_r;&c_o;&c_n;&c_z;&c_a;</trd>
    <trd lng="de">Bronze</trd>
    <trd lng="en">bronze</trd>
    <trd lng="hu">bronz<klr tip="ind"> (elavult)</klr></trd>
    <trd lng="nl">brons<klr tip="ind"> (arch.)</klr></trd>
    <trd lng="pl">br&aogonek;z</trd>
    <trd lng="ru">&c_b;&c_r;&c_o;&c_n;&c_z;&c_a;</trd>
  </snc>
  <trd lng="fr">cuivre</trd>
</drv>

<drv mrk="kupr.0a">
<kap><tld/>a</kap>
  <snc>
    <dif>
      Farita el <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_d;&c_n;&c_y;</trd>
  <trdgrp lng="br">
    <trd>kouevr</trd>,
    <trd>kouevrek</trd>
  </trdgrp>
  <trd lng="de">kupfern</trd>
  <trd lng="en">copper</trd>
  <trd lng="es">de cobre</trd>
  <trd lng="hu">r&eacute;z-</trd>
  <trdgrp lng="nl">
    <trd>koperen</trd>,
    <trd>uit <ind>koper</ind></trd>
  </trdgrp>
  <trd lng="pl">miedziany</trd>
  <trd lng="ru">&c_m;&c_je;&c_d;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="kupr.0i">
<kap><tld/>i</kap>
<gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Tegi per maldika tavolo el <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_k;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol; 
&c_m;&c_je;&c_d;&c_z;&c_mol;&c_d;&c_z;&c_ju;</trd>
  <trd lng="br">kouevra&ntilde;</trd>
  <trd lng="de">verkupfern</trd>
  <trdgrp lng="en">
    <trd>copperplate</trd>,
    <trd>copper</trd>
  </trdgrp>
  <trd lng="fr">cuivrer</trd>
  <trdgrp lng="hu">
    <trd>rezez</trd>,
    <trd>r&eacute;zzel bevon</trd>
  </trdgrp>
  <trd lng="nl">verkoperen</trd>
  <trd lng="pl">miedziowa&cacute;</trd>
  <trd lng="ru">&c_p;&c_o;&c_k;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol; 
&c_m;&c_je;&c_d;&c_mol;&c_ju;</trd>
</drv>

<drv mrk="kupr.0epoko">
<kap><tld/>oepoko</kap>
  <snc>
    <dif>
      Anta&ubreve;historia tempo kiam la homo jam sciis utiligi kupron
      por produkti ilojn el &gcirc;i.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_d;&c_n;&c_y; &c_v;&c_je;&c_k;</trd>
  <trd lng="br">oadvezh an <ind>arem</ind></trd>
  <trd lng="de">Kupferzeit</trd>
  <trd lng="en">copper age</trd>
  <trd lng="hu">r&eacute;zkorszak</trd>
  <trdgrp lng="nl">
    <trd>kopertijd</trd>,
    <trd>kopersteentijd</trd>
  </trdgrp>
  <trd lng="pl">epoka miedzi</trd>
  <trd lng="ru">&c_m;&c_je;&c_d;&c_n;&c_y;&c_j; &c_v;&c_je;&c_k;</trd>
</drv>

<drv mrk="kupr.0erco">
<kap><tld/>oerco</kap>
  <snc>
    <dif>
      Erco liveranta kupron.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_d;&c_n;&c_a;&c_ja; &c_r;&c_u;&c_d;&c_a;</trd>
  <trd lng="de">Kupfererz</trd>
  <trd lng="hu">r&eacute;z&eacute;rc</trd>
  <trd lng="nl">kopererts</trd>
  <trd lng="pl">ruda miedzi</trd>
  <trd lng="ru">&c_m;&c_je;&c_d;&c_n;&c_a;&c_ja; &c_r;&c_u;&c_d;&c_a;</trd>
</drv>

<drv mrk="kupr.0isto">
<kap><tld/>isto</kap>
  <snc>
    <dif>
      Metiisto kiu faras a&jcirc;ojn, ekz., kaldronojn, el <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_d;&c_n;&c_ib;&c_k;</trd>
  <trd lng="br">kouevrour</trd>
  <trdgrp lng="de">
    <trd>K&uuml;fer</trd>,
    <trd>Kupferschmied</trd>
  </trdgrp>
  <trd lng="en">coppersmith</trd>
  <trd lng="hu">r&eacute;zm&udblac;ves</trd>
  <trd lng="nl">koperslager</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_d;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_k;&c_o;&c_t;&c_je;&c_l;&c_mol;&c_shch;&c_i;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="kupr.0gravuristo">
<kap><tld/>ogravuristo</kap>
  <snc>
    <dif>
      Gravuristo uzanta kuprajn tavolojn.
    </dif>
  </snc>
  <trd lng="de">Kupferstecher</trd>
  <trd lng="hu">r&eacute;zmetsz&odblac;</trd>
  <trd lng="nl">kopergraveur</trd>
  <trd lng="pl">miedziorytnik</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_v;&c_jo;&c_r; &c_n;&c_a; 
&c_m;&c_je;&c_d;&c_i;</trd>
</drv>
</art>

