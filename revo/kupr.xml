<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kupr.xml,v 1.39 2021/05/29 10:57:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kupr</rad>/o
</kap>

<drv mrk="kupr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kupr.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <mlg kod="IUPAC">Cu</mlg>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="29">elemento</ref>kun 
      atomnumero 29, atompezo 63,546; bruneru&gcirc;a peza 
      transir<ref tip="super" cel="metal.0o">metalo</ref>, pli fandebla 
      ol oro kaj malpli 
      ol <ref tip="vid" cel="argxen.0o">ar&gcirc;ento</ref>, 
      uzata precipe por elektraj kondukiloj, kaj en kunfanda&jcirc;oj kun 
      <ref tip="vid" cel="stan.0o">stano</ref> (= <ref tip="malprt" cel="bronz.0o">bronzo</ref>) kaj kun 
      <ref tip="vid" cel="zink.0o">zinko</ref> 
      (= <ref tip="malprt" cel="latun.0o">latuno</ref>):
      <ekz>
        orita <tld/>o;
      </ekz>
      <ekz>
        <tld/>a koloro de ha&ubreve;to.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_je;&c_d;&c_z;&c_mol;</trd>
    <trd lng="bg">&c_m;&c_je;&c_d;<klr tip="amb"> (&c_je;&c_l;&c_je;&c_m;&c_je;&c_n;&c_t;)</klr></trd>
    <trd lng="br">kouevr</trd>
    <trd lng="de">Kupfer</trd>
    <trd lng="en">copper</trd>
    <trd lng="es">cobre</trd>
    <trd lng="fr">cuivre</trd>
    <trd lng="hu">r&eacute;z</trd>
    <trd lng="id">tembaga</trd>
    <trd lng="it">rame</trd>
    <trd lng="nl">koper<klr tip="ind"> (metaal)</klr></trd>
    <trd lng="pl">mied&zacute;</trd>
    <trd lng="pt">cobre</trd>
    <trd lng="ru">&c_m;&c_je;&c_d;&c_mol;</trd>
    <trd lng="sv">koppar</trd>
    <trd lng="vi">&#x111;&#x1ED3;ng</trd>
  </snc>
  <snc mrk="kupr.0o.bronzo">
    <uzo tip="stl">ARK</uzo>
    <uzo tip="fak">BIB</uzo>
    <ref tip="dif" cel="bronz.0o">bronzo</ref>
    <trd lng="be">&c_b;&c_r;&c_o;&c_n;&c_z;&c_a;</trd>
    <trd lng="bg">&c_b;&c_r;&c_o;&c_n;&c_z;</trd>
    <trd lng="hu">bronz<klr tip="ind"> (elavult)</klr></trd>
    <trd lng="id">perunggu</trd>
    <trd lng="pl">br&aogonek;z</trd>
    <trd lng="ru">&c_b;&c_r;&c_o;&c_n;&c_z;&c_a;</trd>
  </snc>
  <trd lng="bo">&#xF5F;&#xF44;&#xF66;&#xF0B;</trd>
  <trd lng="cs">m&ecaron;&dcaron;</trd>
  <trd lng="sk">me&dcaron;</trd>
</drv>

<drv mrk="kupr.flava0o">
<kap><ofc>*</ofc> flava <tld/>o <fnt><bib>F</bib>, <lok>Universala
Vortaro, <url ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5074">flav'a
kupr'o</url></lok></fnt></kap>
  <snc>
  <uzo tip="stl">ARK</uzo>
    <dif>
      <sup>1</sup><ref tip="sin" cel="latun.0o">Latuno</ref>, flava
      alojo el
      kupro kaj zinko.
    </dif>
  <trd lng="fr">laiton</trd>  
  </snc>
</drv>

<drv mrk="kupr.0a">
<kap><tld/>a</kap>
  <snc mrk="kupr.0a.elkupro">
    <dif>
      Farita el <tld/>o.
    </dif>
  <trd lng="fr">cuivr&eacute;</trd>  
  </snc>
  <snc mrk="kupr.0a.kolore">
    <dif>
      Havanta <ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">koloron</ref> de <tld/>o, brilan, brune 
      <ref tip="super" cel="rugx.0a">ru&gcirc;an</ref>.
    </dif>
    <ref tip="vid" cel="har.0aro.FRIZ">hararo</ref>
  <trd lng="fr">auburn</trd>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_d;&c_n;&c_y;</trd>
  <trdgrp lng="br">
    <trd>kouevr</trd>,
    <trd>kouevrek</trd>
  </trdgrp>
  <trd lng="cs">m&ecaron;d&ecaron;n&eacute; barvy</trd>
  <trd lng="de">kupfern</trd>
  <trd lng="en">copper</trd>
  <trd lng="es">de cobre</trd>
  <trd lng="hu">r&eacute;z-</trd>
  <trdgrp lng="nl">
    <trd>koperen</trd>,
    <trd>uit <ind>koper</ind></trd>
  </trdgrp>
  <trd lng="pl">miedziany</trd>
  <trd lng="ru">&c_m;&c_je;&c_d;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">medenej farby</trd>
</drv>

<drv mrk="kupr.0i">
<kap><tld/>i</kap>
<gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Tegi per maldika tavolo el <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_k;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol; 
&c_m;&c_je;&c_d;&c_z;&c_mol;&c_d;&c_z;&c_ju;</trd>
  <trd lng="br">kouevra&ntilde;</trd>
  <trd lng="cs">pom&ecaron;dit</trd>
  <trd lng="de">verkupfern</trd>
  <trdgrp lng="en">
    <trd>copperplate</trd>,
    <trd>copper</trd>
  </trdgrp>
  <trd lng="fr">cuivrer</trd>
  <trdgrp lng="hu">
    <trd>rezez</trd>,
    <trd>r&eacute;zzel bevon</trd>
  </trdgrp>
  <trd lng="nl">verkoperen</trd>
  <trd lng="pl">miedziowa&cacute;</trd>
  <trd lng="ru">&c_p;&c_o;&c_k;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol; 
&c_m;&c_je;&c_d;&c_mol;&c_ju;</trd>
  <trd lng="sk">pomedi&tcaron;</trd>
  <trd lng="sv">f&ouml;rkoppra</trd>
</drv>

<drv mrk="kupr.0epoko">
<kap><tld/>oepoko</kap>
  <snc>
    <dif>
      Anta&ubreve;historia tempo kiam la homo jam sciis utiligi kupron
      por produkti ilojn el &gcirc;i.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_d;&c_n;&c_y; &c_v;&c_je;&c_k;</trd>
  <trd lng="br">oadvezh an <ind>arem</ind></trd>
  <trd lng="cs">doba m&ecaron;d&ecaron;n&aacute;</trd>
  <trd lng="de">Kupferzeit</trd>
  <trd lng="en">copper age</trd>
  <trd lng="fr">&acirc;ge du cuivre</trd>
  <trd lng="hu">r&eacute;zkorszak</trd>
  <trdgrp lng="nl">
    <trd>kopertijd</trd>,
    <trd>kopersteentijd</trd>
  </trdgrp>
  <trd lng="pl">epoka miedzi</trd>
  <trd lng="ru">&c_m;&c_je;&c_d;&c_n;&c_y;&c_j; &c_v;&c_je;&c_k;</trd>
  <trd lng="sk">doba meden&aacute;</trd>
</drv>

<drv mrk="kupr.0erco">
<kap><tld/>oerco</kap>
  <snc>
    <dif>
      Erco liveranta kupron.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_d;&c_n;&c_a;&c_ja; &c_r;&c_u;&c_d;&c_a;</trd>
  <trd lng="cs">m&ecaron;d&ecaron;n&aacute; ruda</trd>
  <trd lng="de">Kupfererz</trd>
  <trd lng="hu">r&eacute;z&eacute;rc</trd>
  <trd lng="nl">kopererts</trd>
  <trd lng="pl">ruda miedzi</trd>
  <trd lng="ru">&c_m;&c_je;&c_d;&c_n;&c_a;&c_ja; &c_r;&c_u;&c_d;&c_a;</trd>
  <trd lng="sk">meden&aacute; ruda</trd>
</drv>

<drv mrk="kupr.0isto">
<kap><tld/>isto</kap>
  <snc>
    <dif>
      Metiisto kiu faras a&jcirc;ojn, ekz., kaldronojn, el <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_d;&c_n;&c_ib;&c_k;</trd>
  <trd lng="br">kouevrour</trd>
  <trdgrp lng="de">
    <trd>K&uuml;fer</trd>,
    <trd>Kupferschmied</trd>
  </trdgrp>
  <trd lng="en">coppersmith</trd>
  <trd lng="fr">chaudronnier</trd>
  <trd lng="hu">r&eacute;zm&udblac;ves</trd>
  <trd lng="nl">koperslager</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_d;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_k;&c_o;&c_t;&c_je;&c_l;&c_mol;&c_shch;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sv">kopparslagare</trd>
</drv>

<drv mrk="kupr.0gravuristo">
<kap><tld/>ogravuristo</kap>
  <snc>
    <dif>
      Gravuristo uzanta kuprajn tavolojn.
    </dif>
  </snc>
  <trd lng="cs">m&ecaron;dirytec</trd>
  <trd lng="de">Kupferstecher</trd>
  <trd lng="fr">graveur sur cuivre</trd>
  <trd lng="hu">r&eacute;zmetsz&odblac;</trd>
  <trd lng="nl">kopergraveur</trd>
  <trd lng="pl">miedziorytnik</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_v;&c_jo;&c_r; &c_n;&c_a; 
&c_m;&c_je;&c_d;&c_i;</trd>
  <trd lng="sk">medirytec</trd>
</drv>
</art>

