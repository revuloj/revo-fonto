<drv mrk="kupr.0isto">
<kap><tld/>isto</kap>
  <snc>
    <dif>
      Metiisto kiu faras a&jcirc;ojn, ekzemple kaldronojn, el <tld/>o:
      <ekz>
        lia patro estis <klr>[&#8230;]</klr> <tld/>isto, <klr>[&#8230;]</klr> plena de sa&gcirc;eco,
        kompetenteco, kaj sciado, por fari &ccirc;ian laboron el <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>&Re1; 7:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        Aleksandro, la <tld/>isto, faris al mi multe da malbono
        <fnt>
          <bib>NT</bib>
          <lok>&Ti2; 4:14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_d;&c_n;&c_ib;&c_k;</trd>
  <trd lng="br">kouevrour</trd>
  <trdgrp lng="de">
    <trd>K&uuml;fer</trd>,
    <trd>Kupferschmied</trd>
  </trdgrp>
  <trd lng="en">coppersmith</trd>
  <trd lng="fr">chaudronnier</trd>
  <trd lng="hu">r&eacute;zm&udblac;ves</trd>
  <trd lng="ja">&#37619;&#25499;&#12369;&#23627;  <pr>&#12356;&#12363;&#12369;&#12420;</pr></trd>
  <trd lng="nl">koperslager</trd>
  <trd lng="pl">miedziownik</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_d;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_k;&c_o;&c_t;&c_je;&c_l;&c_mol;&c_shch;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sv">kopparslagare</trd>
  <trd lng="uk">&c_m;&c_ib;&c_d;&c_n;&c_i;&c_k;</trd>
  <trdgrp lng="zh">
    <trd>&#37509;&#21280; <pr>t&oacute;ngjiang</pr></trd>,
    <trd>&#38108;&#21280; <pr>t&oacute;ngjiang</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kupr.flava0o">
<kap>
  <ofc>*</ofc>flava <tld/>o
    <fnt><bib>F</bib>,
      <lok>Universala Vortaro,
      <url ref="&AdE;/fundamento/universala_vortaro.html#av-5074">flav'a kupr'o</url></lok>
    </fnt>
  </kap>
  <snc>
  <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="sin" cel="latun.0o">Latuno</ref>, flava
      alojo el kupro kaj zinko:
      <ekz>
        la kaldrono estis el flava <tld/>o, sed brilis kiel oro
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino kaj Rozulino</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">brass <klr>(alloy)</klr></trd>
  <trd lng="fr">laiton</trd>
  <trdgrp lng="ja">
    <trd>&#30495;&#37742;  <pr>&#12375;&#12435;&#12385;&#12421;&#12358;</pr></trd>,
    <trd>&#40644;&#37509;  <pr>&#12362;&#12358;&#12393;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="pl">mosi&aogonek;dz</trd>
</drv>

<drv mrk="kupr.0erco">
<kap><tld/>oerco</kap>
  <snc>
    <dif>
      Erco liveranta <tld/>on:
      <ekz>
        juna virino, laboranta por la prizorgado de la kemia&jcirc;oj uzataj por tranformi&gcirc;o
        de <tld/>oerco
        <fnt>
          <bib>MD08</bib>
          <vrk>Investo de Barato en Suda Afriko</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="hxalkogen.0o">&hcirc;alkogeno</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_d;&c_n;&c_a;&c_ja; &c_r;&c_u;&c_d;&c_a;</trd>
  <trd lng="cs">m&ecaron;d&ecaron;n&aacute; ruda</trd>
  <trd lng="de">Kupfererz</trd>
  <trd lng="en"><ind>copper</ind> ore</trd>
  <trd lng="fr">minerai de <ind>cuivre</ind></trd>
  <trd lng="hu">r&eacute;z&eacute;rc</trd>
  <trd lng="nl">kopererts</trd>
  <trd lng="pl">ruda <ind>miedzi</ind></trd>
  <trd lng="ru">&c_m;&c_je;&c_d;&c_n;&c_a;&c_ja; &c_r;&c_u;&c_d;&c_a;</trd>
  <trd lng="sk">meden&aacute; ruda</trd>
  <trd lng="uk">&c_m;&c_ib;&c_d;&c_n;&c_a; &c_r;&c_u;&c_d;&c_a;</trd>
</drv>
</art>

