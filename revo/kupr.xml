<drv mrk="kupr.0erco">
<kap><tld/>oerco</kap>
  <snc>
    <dif>
      Erco liveranta <tld/>on:
      <ekz>
        juna virino, laboranta por la prizorgado de la kemia&jcirc;oj uzataj por tranformi&gcirc;o
        de <tld/>oerco
        <fnt>
          <bib>MD08</bib>
          <vrk>Investo de Barato en Suda Afriko</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="hxalkogen.0o">&hcirc;alkogeno</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_d;&c_n;&c_a;&c_ja; &c_r;&c_u;&c_d;&c_a;</trd>
  <trd lng="cs">m&ecaron;d&ecaron;n&aacute; ruda</trd>
  <trd lng="de">Kupfererz</trd>
  <trd lng="hu">r&eacute;z&eacute;rc</trd>
  <trd lng="nl">kopererts</trd>
  <trd lng="pl">ruda <ind>miedzi</ind></trd>
  <trd lng="ru">&c_m;&c_je;&c_d;&c_n;&c_a;&c_ja; &c_r;&c_u;&c_d;&c_a;</trd>
  <trd lng="sk">meden&aacute; ruda</trd>
  <trd lng="uk">&#x43C;&c_ib;&c_d;&#x43D;&c_a; &c_r;&c_u;&c_d;&c_a;</trd>
</drv>

<drv mrk="kupr.0isto">
<kap><tld/>isto</kap>
  <snc>
    <dif>
      Metiisto kiu faras a&jcirc;ojn, ekz-e kaldronojn, el <tld/>o:
      <ekz>
        lia patro estis <klr>[&#8230;]</klr> <tld/>isto, <klr>[&#8230;]</klr> plena de sa&gcirc;eco,
        kompetenteco, kaj sciado, por fari &ccirc;ian laboron el <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 7:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        Aleksandro, la <tld/>isto, faris al mi multe da malbono
        <fnt>
          <bib>NT</bib>
          <lok>II. Timoteo 4:14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_d;&c_n;&c_ib;&c_k;</trd>
  <trd lng="br">kouevrour</trd>
  <trdgrp lng="de">
    <trd>K&uuml;fer</trd>,
    <trd>Kupferschmied</trd>
  </trdgrp>
  <trd lng="en">coppersmith</trd>
  <trd lng="fr">chaudronnier</trd>
  <trd lng="hu">r&eacute;zm&udblac;ves</trd>
  <trd lng="ja">&#x92F3;&#x639B;&#12369;&#x5C4B;  <pr>&#12356;&#x304B;&#12369;&#12420;</pr></trd>
  <trd lng="nl">koperslager</trd>
  <trd lng="pl">miedziownik</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_d;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_k;&c_o;&c_t;&c_je;&c_l;&c_mol;&c_shch;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sv">kopparslagare</trd>
  <trd lng="uk">&#x43C;&c_ib;&c_d;&#x43D;&c_i;&#x43A;</trd>
  <trdgrp lng="zh">
    <trd>&#37509;&#21280; <pr>t&#xF3;ngjiang</pr></trd>,
    <trd>&#x94DC;&#21280; <pr>t&#xF3;ngjiang</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kupr.0gravuristo">
<kap><tld/>ogravuristo</kap>
  <snc>
    <dif>
      Gravuristo uzanta <tld/>ajn tabulojn:
      <ekz>
        nepo de Rentsch estis la saksa kortegopentristo kaj <tld/>ogravuristo Christian
        Wilhelm Ernst Dietrich
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=343155">Johann Ernst Rentsch (la pli
          maljuna)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">m&ecaron;dirytec</trd>
  <trd lng="de">Kupferstecher</trd>
  <trd lng="fr">graveur sur cuivre</trd>
  <trd lng="hu">r&eacute;zmetsz&odblac;</trd>
  <trd lng="nl">kopergraveur</trd>
  <trd lng="pl">miedziorytnik</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_v;&c_jo;&c_r; &c_n;&c_a;
&c_m;&c_je;&c_d;&c_i;</trd>
  <trd lng="sk">medirytec</trd>
  <trdgrp lng="zh">
    <trd>&#37509;&#29256;&#x5F6B;&#x523B;&#x6A5F; <pr>t&#xF3;ngb&#x1CE;ndi&amacron;ok&#xE8;j&#x12B;</pr></trd>,
    <trd>&#x94DC;&#29256;&#x96D5;&#x523B;&#x673A; <pr>t&#xF3;ngb&#x1CE;ndi&amacron;ok&#xE8;j&#x12B;</pr></trd>
  </trdgrp>
</drv>
</art>

