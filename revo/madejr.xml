<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: madejr.xml,v 1.16 2023/09/02 06:04:07 revo Exp $">
<kap>
  <rad>Madejr</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="madejr.M0o">
  <kap><tld/>o</kap>
  <snc mrk="madejr.M0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" cel="insul.0o.GEOG" lst="voko:insuloj">Insulo</ref>
      en 
      <ref tip="malprt" cel="atlant.A0o.GEOG">Atlantiko</ref>,
      okcidente de Maroko, posedata de 
      <ref tip="malprt" cel="portugal.P0ujo">Portugalujo</ref>:
      <ekz>
        <tld/>on oni malkovris kaj eklo&gcirc;is &ccirc;irka&ubreve; 1425
        <fnt>
          <aut>Juan R&eacute;gulo P&eacute;rez</aut>,
          <vrk>dracenoj &ccirc;e pentristoj de la 15a jarcento</vrk>,
          <lok>norda prismo, 1957, numero 3a, p. 123-128a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">Madeira</trd>
  <trd lng="de">Madeira</trd>
  <trd lng="es">Madeira<klr tip="amb"> (archipi&eacute;lago
  Portugu&eacute;s)</klr></trd>
  <trd lng="fa">&a_m;&a_A;&a_d;&f_y;&a_r;&a_A;</trd>
  <trd lng="fr">Mad&egrave;re</trd>
  <trd lng="hu">Madeira-szigetek</trd>
  <trd lng="id">Madeira</trd>
  <trd lng="nl">Madeira</trd>
  <trd lng="pl">Madera</trd>
  <trd lng="pt">Madeira <klr>(ilha da)</klr></trd>
</drv>

<drv mrk="madejr.0o">
  <kap><tld lit="m"/>o</kap>
  <snc mrk="madejr.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld/>o<ref tip="super" cel="vin.0o.KUI">vino</ref> kaj
      komunlingve
      &ccirc;ia simila vinbera vino, al kiu oksidigo en barelo donis
      specialan guston:
      <ekz>
        ni havas gubernian <tld/>on: &gcirc;i ne imponas per 
        ekstera&jcirc;o, sed e&ccirc; elefanton &gcirc;i renversos
        <fnt><bib>Revizoro</bib>, <lok>akto 2a, sceno 10a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">madeira</trd>
  <trdgrp lng="de">
    <trd>Madeirawein</trd>,
    <trd>Madeira</trd>
  </trdgrp>
  <trd lng="fa">&a_sh;&a_r;&a_A;&a_b; <ind>&a_m;&a_A;&a_d;&f_y;&a_r;&a_A;</ind></trd>
  <trd lng="fr">mad&egrave;re<klr tip="amb"> (vin)</klr></trd>
  <trd lng="hu">madeira<klr tip="amb"> (bor)</klr></trd>
  <trd lng="ja">&#x30DE;&#x30C7;&#x30A4;&#x30E9;&#x5CF6;  [<ind>&#x30DE;&#x30C7;&#x30A4;&#x30E9;&#x3057;&#x307E;</ind>]</trd>
  <trd lng="nl">madeira<klr tip="ind"> (wijn)</klr></trd>
  <trd lng="pl">madera<klr tip="amb"> (wino)</klr></trd>
  <trd lng="pt">madeira <klr>(vinho)</klr></trd>
  <trd lng="uk">&#x41C;&#x430;&#x434;&#x435;&#x439;&#x440;&#x430;</trd>
</drv>
</art>
