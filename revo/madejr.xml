<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Madejr</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="madejr.M0o">
  <kap><tld/>o</kap>
  <snc mrk="madejr.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Insulo en <ref tip="malprt" cel="atlant.0o.GEOG">Atlantiko</ref>,
      okcidente de Maroko, posedata de Portugalujo.
    </dif>
  </snc>
  <trd lng="fr">Mad&egrave;re</trd>
  <trd lng="hu">Madeira-szigetek</trd>  
</drv>

<drv mrk="madejr.0o">
  <kap><tld lit="m"/>o</kap>
  <snc mrk="madejr.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld/>o<ref tip="super" cel="vin.0o.KUI">vino</ref> kaj komunlingve
      &ccirc;ia simila vinbera vino, al kiu oksidigo en barelo donis
      specialan guston:
      <ekz>
        ni havas gubernian <tld/>on: &gcirc;i ne imponas per ekstera&jcirc;o, sed e&ccirc;
        elefanton &gcirc;i renversos
        <fnt><bib>Revizoro</bib>, <lok>akto 2a, sceno 10a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">mad&egrave;re<klr tip="amb"> (vin)</klr></trd>
  <trd lng="hu">madeira<klr tip="amb"> (bor)</klr></trd>
</drv>
</art>

