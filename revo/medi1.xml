<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: medi1.xml,v 1.9 2022/01/09 17:38:30 revo Exp $">
<kap>
  <rad>Medi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="medi1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="med.0ujo">Medujo</ref>:
      <ekz>
        <tld/>o estas regiono de nordokcidenta Irano, kiu estis la politika kaj kultura
        bazo de la medoj kaj de aliaj antikvaj iranaj popoloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=537706"
              >Medio (regiono)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim> 
    Sen kuntekso oni povus konfuzi la vorton kun  
    <refgrp tip="hom"><ref cel="medi.0o">medio</ref></refgrp> (&ccirc;irka&ubreve;a&jcirc;o, cirkonstancaro).
  </rim>
  <trd lng="be">&c_M;&c_ib;&c_d;&c_y;&c_ja;</trd>
  <trd lng="ca">M&egrave;dia</trd>
  <trd lng="de">Medien <klr>(westasiat. Region)</klr></trd>
  <trd lng="fr">M&eacute;die</trd>
  <trd lng="hu">M&eacute;dia</trd>
  <trd lng="pl">Media <klr>(kraina)</klr></trd>
  <trd lng="pt">M&eacute;dia <klr tip="amb">(hist.)</klr></trd>
  <trd lng="ru">&c_M;&c_i;&c_d;&c_i;&c_ja;</trd>
</drv>
</art>

