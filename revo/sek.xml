<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>sek</rad>/a
    </kap>

    <drv mrk="sek.0a">
      <kap><ofc>*</ofc><tld/>a</kap>
      <snc mrk="sek.0a.senakva">
        <dif>
          Enhavanta neniom a&ubreve; malmulte da akvo, flua&jcirc;o, suko
          a&ubreve; graso.
        </dif>
        <subsnc mrk="sek.0a.nesorbinta">
          <dif>
            Ne sorbinta flua&jcirc;on (sub la pluvo, en akvujo ktp):
            <ekz>
              <tld/>a &ccirc;ifono, vi&scirc;tuko, subvesto;
            </ekz><ekz>
              transiri <klr>(rivereton)</klr> kun <tld/>aj
              piedoj<fnt>Z</fnt>;
            </ekz><ekz>
              ne restas e&ccirc; unu <tld/>a fadeno sur mi<fnt>Z</fnt>.
            </ekz>
          </dif>
        </subsnc>
        <subsnc mrk="sek.0a.perdinta">
          <dif>
            Perdinta sian naturan akvon pro elvapori&gcirc;o a&ubreve; <tld/>igado:
            <ekz>
              lia lango estis <tld/>a;
            </ekz><ekz>
              <tld/>aj <klr>(velkintaj kaj mortintaj)</klr> folioj;
            </ekz><ekz>
              <tld/>a <klr>(<tld/>igita)</klr> vinbero;
            </ekz><ekz>
              <uzo tip="stl">FIG</uzo>
              <tld/>a <klr>(malgrasa)</klr> mano de
              maljunulo<fnt>B</fnt>.
            </ekz>
          </dif>
        </subsnc>
        <subsnc mrk="sek.0a.pere">
          <dif>
            Rilata al malesto a&ubreve; malmulto da akvo a&ubreve; flua&jcirc;o
            &ccirc;e fabrikado, esti&gcirc;o, funkciado de io:
            <ekz>
              <uzo tip="fak">MED</uzo>
              <tld/>a tuso <klr>(sen muka&jcirc;o)</klr>;
            </ekz><ekz>
              <uzo tip="fak">TEK</uzo>
              <tld/>a riparo
              <klr>(en <tld/>a
                <ref cel="dok.0o.riparejo">doko<sncref/></ref>)</klr>;
            </ekz><ekz>
              <tld/>a vojo <klr>(kontraste al akva vojo)</klr>;
            </ekz><ekz>
              <tld/>a baterio, pilo;
            </ekz><ekz>
              <tld/>a pezo, maso;
            </ekz><ekz>
              <tld/>a kompreso;
            </ekz><ekz>
              <tld/>a vaporo <klr>(sen akvogutetoj)</klr>;
            </ekz><ekz>
              <tld/>a vetero<fnt>Z</fnt>
              <klr>(sen pluvo)</klr>.
            </ekz>
          </dif>
        </subsnc>
        <trd lng="fr">sec</trd>
      </snc>
      <snc mrk="sek.0a.senornama">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Senornama, senfantazia, sendetala:
          <ekz>
            <tld/>a formo<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>a artikolo<fnt>Z</fnt>,
          </ekz><ekz>
            statistiko, skizo;
          </ekz><ekz>
            iom <tld/>a priskribo, rakonto, stilo;
          </ekz><ekz>
            <tld/>a <klr>(seninteresa)</klr> lernado de izolaj vortoj;
          </ekz><ekz>
            <tld/>aj <klr>(seninteresaj)</klr> faktoj kaj datoj;
          </ekz><ekz>
            <tld/>e teoria logikeco<fnt>Z</fnt>
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="sobr.0a">sobra</ref>,
          <ref cel="proz.0a">proza</ref>,
          <ref cel="scienc">scienca</ref>,
          <ref cel="metafo">senmetafora</ref>,
          <ref cel="nud.0a">nuda</ref>.
        </refgrp>
      </snc>
      <snc mrk="sek.0a.malsentimentala">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Senkompata, sensenta, severa, ordonema:
          <ekz>
            <tld/>a tono;
          </ekz><ekz>
            tiu kortu&scirc;a poemo plorigus la plej <tld/>an koron.
          </ekz>
        </dif>
        <trd lng="fr">sec</trd>
      </snc>
      <snc mrk="sek.0a.sono">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Pri sonoj sensonoraj, plataj, nemelodiaj:
          <ekz>
            mallonga <tld/>a rido<fnt>B</fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_s;&c_u;&c_h;&c_ib;</trd>
      <trd lng="ca">sec</trd>
      <trd lng="de">trocken</trd>
      <trd lng="en">dry</trd>
      <trd lng="es">seco</trd>
      <trd lng="he">&yod;&bet;&shin;</trd>
      <trd lng="hu">sz&aacute;raz</trd>
      <trd lng="nl">droog</trd>
      <trd lng="ru">&c_s;&c_u;&c_h;&c_o;&c_j;</trd>
      <trd lng="sv">torr</trd>
      <trd lng="tr">kuru</trd>
    </drv>

    <drv mrk="sek.0ajxo">
      <kap><ofc>*</ofc><tld/>a&jcirc;o</kap>
      <snc mrk="sek.0ajxo.generale">
        <dif>
          Io <tld/>a:
          <ekz>
            la patrino iras <klr>...</klr> en la &ccirc;ambreton
            al la etulo, kiu &jcirc;us ekkriis, metas lin en
            <tld/>a&jcirc;on, donas al li botelon
            <fnt>
              <aut>Moa Martinson</aut>:
              <vrk>Vespera ru&gcirc;o anoncas ventegon</vrk>.
              <bib>Monato</bib>
            </fnt>.
          </ekz>
        </dif>
        <trd lng="ru">&c_s;&c_u;&c_h;&c_o;&c_je;<klr> (&c_s;&c_u;&c_shch;.)</klr></trd>
      </snc>
      <snc mrk="sek.0ajxo.GEOG">
        <uzo tip="fak">GEOG</uzo>
        <dif>
          La <tld/>a tero, kontraste al la
          <ref tip="ant" cel="ocean.0o">oceano</ref> kaj
          <ref tip="ant" cel="mar.0o">maro</ref> kaj ceteraj akvujoj:
          <ekz>
            kaj Dio nomis la <tld/>a&jcirc;on Tero, kaj la kolekti&gcirc;ojn
            de la akvo Li nomis Maroj
	    <fnt><bib>MT</bib>, <lok>&Gen; 1:10</lok></fnt>;
          </ekz><ekz>
            la <tld/>a&jcirc;o okupas proksimume trionon da tersurfaco, kies
            reston kovras la akvo;
          </ekz><ekz>
            la kulivata tero okupas malpli ol 14% de la tuta <tld/>a&jcirc;o.
          </ekz>
        </dif>
        <trdgrp lng="en">
          <trd>land</trd>, <trd>mainland</trd>, <trd>dry land</trd>
        </trdgrp>
        <trd lng="ru">&c_s;&c_u;&c_sh;&c_a;</trd>
      </snc>
    </drv>

    <drv mrk="sek.0igi">
      <kap><tld/>igi</kap>
      <snc>
        <dif>
          Igi <tld/>a
        </dif>
        <refgrp tip="sin">
          <ref cel="akv">senakvigi</ref>,
          <ref cel="dren.0i">dreni</ref>.
        </refgrp>
      </snc>
      <trd lng="be">&c_s;&c_u;&c_sh;&c_y;&c_c;&c_mol;</trd>
      <trd lng="ca">assecar</trd>
      <trd lng="de">trocknen</trd>
      <trd lng="es">secar</trd>
      <trd lng="fr">s&eacute;cher</trd>
      <trd lng="he">&lamed;<ind>&yod;&bet;&shin;</ind></trd>
      <trd lng="hu">sz&aacute;r&iacute;t</trd>
      <trd lng="nl">drogen<klr tip="ind"> (doen drogen)</klr></trd>
      <trdgrp lng="ru">
        <trd>&c_s;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>,
        <trd>&c_v;&c_y;&c_s;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>
      </trdgrp>
      <trd lng="sv">torka</trd>
      <trd lng="tr">kurutmak</trd>
    </drv>

    <drv mrk="sek.0igxi">
      <kap><tld/>i&gcirc;i</kap>
      <snc>
        <dif>
          I&gcirc;i senakva, senabunda, malri&ccirc;a:
          <ekz>
            pro varmo la tero rapide <tld/>i&gcirc;is;
          </ekz><ekz>
            velk<tld/>i&gcirc;intaj bran&ccirc;oj<fnt>Z</fnt>;
          </ekz><ekz>
            la fonto <tld/>i&gcirc;is;
          </ekz><ekz>
            tiel la &scirc;tato akiros fonton de enspezaj neniam
            <tld/>i&gcirc;antan<fnt>B</fnt>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            mia forto el<tld/>i&gcirc;is<fnt>Z</fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_s;&c_o;&c_h;&c_n;&c_u;&c_c;&c_mol;</trd>
      <trd lng="de">trocknen</trd>
      <trd lng="es">secarse</trd>
      <trd lng="he">&lamed;&he;&tav;<ind>&yod;&bet;&shin;</ind></trd>
      <trd lng="hu">sz&aacute;rad</trd>
      <trd lng="nl">drogen<klr tip="ind"> (droog worden)</klr></trd>
      <trdgrp lng="ru">
        <trd>&c_s;&c_o;&c_h;&c_n;&c_u;&c_t;&c_mol;</trd>,
        <trd>&c_v;&c_y;&c_s;&c_y;&c_h;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_v;&c_y;&c_s;&c_o;&c_h;&c_n;&c_u;&c_t;&c_mol;</trd>
      </trdgrp>
      <trdgrp lng="fr"><trd>s&eacute;cher</trd><trd>se
          <ind>d&eacute;ssecher</ind></trd></trdgrp>
      <trdgrp lng="sv">
        <trd>torkas</trd>,
        <trd>torka ut</trd>
      </trdgrp>
      <trd lng="tr">kurumak</trd>
    </drv>

    <drv mrk="sek.mal0a">
      <kap>mal<tld/>a</kap>
      <snc mrk="sek.mal0a.sorbinta">
        <dif>
          Enhavanta multe da akvo a&ubreve; flua&jcirc;o:
          <ekz>
            mal<tld/>a &ccirc;ifono, vesto, vi&scirc;tuko;
          </ekz><ekz>
            mal<tld/>aj haroj;
          </ekz><ekz>
            la mantelo estas mal<tld/>a, sed la vesto
            sub &gcirc;i &dash; <tld/>a
            <fnt><bib>Metrop</bib></fnt>;
          </ekz><ekz>
            larme mal<tld/>aj okuloj<fnt>B</fnt>;
          </ekz><ekz>
            mal<tld/>a ne&gcirc;o
            <klr>(ne&gcirc;o enhavanta multe da likva akvo)</klr>;
          </ekz><ekz>
            regis kruda, mal<tld/>a vetero<fnt>Z</fnt>;
          </ekz><ekz>
            per miaj larmoj mi mal<tld/>igas mian ku&scirc;ejon<fnt>Z</fnt>.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="pluv.0o">pluvo</ref>,
          <ref cel="ros.0o">roso</ref>.
        </refgrp>
        <trd lng="ru">&c_m;&c_o;&c_k;&c_r;&c_y;&c_j;</trd>
      </snc>
      <snc mrk="sek.mal0a.humida">
        <dif>
          <klr>(Pri gaso, atmosfero:)</klr>
          Entenanta percepteble multe da gasstata akvo:
          <ekz>
            densa ondo da mucida, mal<tld/>a aero frapis
            la viza&gcirc;ojn de amba&ubreve; enirantaj virinoj
            <fnt><bib>Marta</bib></fnt>;
          </ekz><ekz>
            &gcirc;ardenoj, kies arboj dank' al la mal<tld/>a blovo
            de l' maro kreskis
            <fnt><bib>Far2</bib>, <lok>&Ccirc;ap. 2a</lok></fnt>;
          </ekz><ekz>
            mal<tld/>a vento, klimato.
          </ekz>
        </dif>
        <ref tip="sin" cel="humid.0a">humida</ref>
        <trdgrp lng="ru">
          <trd>&c_v;&c_l;&c_a;&c_zh;&c_n;&c_y;&c_j;</trd>,
          <trd>&c_s;&c_y;&c_r;&c_o;&c_j;</trd>
        </trdgrp>
      </snc>
      <trdgrp lng="be">
        <trd>&c_m;&c_o;&c_k;&c_r;&c_y;</trd>,
        <trd>&c_v;&c_ib;&c_l;&c_mol;&c_g;&c_o;&c_t;&c_n;&c_y;</trd>
      </trdgrp>
      <trd lng="ca">mullat</trd>
      <trd lng="de">nass</trd>
      <trd lng="en">wet</trd>
      <trd lng="es">mojado</trd>
      <trd lng="he">&resh;&tet;&vav;&bet;</trd>
      <trd lng="hu">nedves</trd>
      <trd lng="nl">nat</trd>
      <trd lng="sv">bl&ouml;t</trd>
      <trdgrp lng="fr">
        <trd>humide</trd>,
        <trd>mouill&eacute;</trd>
      </trdgrp>
      <trdgrp lng="tr">
        <trd>&inodot;slak</trd>,
        <trd>nemli</trd>
      </trdgrp>
    </drv>

    <drv mrk="sek.mal0eta">
      <kap>mal<tld/>eta</kap>
      <snc>
        <dif>
          Modere, ne tre multe mal<tld/>a:
          <ekz>
            sablomuldilo <klr>(estas)</klr> ludilo por formi
            figuron el mal<tld/>eta sablo
            <fnt><bib>HejmVort</bib></fnt>;
          </ekz><ekz>
            la ronda viza&gcirc;o estis ru&gcirc;eta kaj &scirc;i iom
            &scirc;vitis; la okuloj brilis mal<tld/>ete
            <fnt>
              <aut>Marjorie BOULTON</aut>:
              <vrk>Ebrivirgeco</vrk>.
              <lok>La nica literatura revuo, 3:3</lok>.
            </fnt>;
          </ekz><ekz>
            mal<tld/>eta &ccirc;ambro, muro, kelo.
          </ekz>
        </dif>
      </snc>
      <trd lng="ca">humit</trd>
      <trd lng="es">h&uacute;medo</trd>
      <trdgrp lng="hu">
        <trd>nyirkos</trd>,
        <trd>p&aacute;r&aacute;s</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_v;&c_l;&c_a;&c_zh;&c_n;&c_y;&c_j;</trd>,
        <trd>&c_s;&c_y;&c_r;&c_o;&c_j;</trd>
      </trdgrp>
      <trd lng="sv">fuktig</trd>
    </drv>
  </art>
