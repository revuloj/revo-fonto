  <trd lng="es">secar</trd>
  <trd lng="fr">s&eacute;cher</trd>
  <trd lng="he">&lamed;<ind>&yod;&bet;&shin;</ind></trd>
  <trd lng="hu">sz&aacute;r&iacute;t</trd>
  <trd lng="id"><baz>kering</baz> mengeringkan</trd>
  <trd lng="io">sikigar</trd>
  <trdgrp lng="it">
    <trd>seccare</trd>,
    <trd>essiccare</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4E7E;&#x304B;&#x3059;  <pr>&#x304B;&#x308F;&#x304B;&#x3059;</pr></trd>,
    <trd>&#x5E72;&#x3059;  <pr>&#x307B;&#x3059;</pr></trd>
  </trdgrp>
  <trd lng="nl">drogen<klr tip="ind"> (doen drogen)</klr></trd>
