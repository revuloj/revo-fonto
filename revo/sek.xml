  <trd lng="en">wet</trd>
  <trd lng="es">mojado</trd>
  <trd lng="fr">mouill&eacute;</trd>
  <trd lng="fy">wiet</trd>
  <trd lng="he">&resh;&tet;&vav;&bet;</trd>
  <trd lng="hu">nedves</trd>
  <trd lng="ia">humide</trd>
  <trd lng="id">basah</trd>
  <trd lng="io">humida</trd>
  <trdgrp lng="ja">
    <trd>&#x306C;&#x308C;&#x305F;</trd>,
    <trd>&#x6E7F;&#x3063;&#x305F;  <pr>&#x3057;&#x3081;&#x3063;&#x305F;</pr></trd>,
    <trd>&#x6E7F;&#x6C17;&#x306E;&#x591A;&#x3044;  <pr>&#x3057;&#x3063;&#x3051;&#x306E;&#x304A;&#x304A;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="jbo">cilmo</trd>
