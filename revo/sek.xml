    <trd>mokry</trd>,
    <trd>nawil&zdot;aj&aogonek;cy</trd>,
    <trd>wilgotny</trd>
  </trdgrp>
  <trd lng="sk">mokr&yacute;</trd>
  <trd lng="sv">bl&ouml;t</trd>
  <trdgrp lng="tr">
    <trd>&inodot;slak</trd>,
    <trd>nemli</trd>
  </trdgrp>
  <trd lng="tok">telo</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_o;&c_k;&c_r;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_o;&c_l;&c_o;&c_g;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="vo">luimik</trd>
