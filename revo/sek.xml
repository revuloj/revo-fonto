<drv mrk="sek.mal0igxi">
  <kap>mal<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sek.mal0igxi.KOMUNE">
    <dif>
      I&gcirc;i mal<tld/>a:
      <ekz>
        ili ne havis fajron, <klr>[do]</klr> la lignaj pecetoj <klr>[&#x2026;]</klr> mal<tld/>i&gcirc;is
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj vangoj ru&gcirc;i&gcirc;is <klr>[kaj &scirc;iaj]</klr> okuloj mal<tld/>i&gcirc;is
        <fnt>
          <bib>FK</bib>
          <lok>Nur unu vorton!</lok>
        </fnt>;
      </ekz>
      <ekz>
        li mal<tld/>i&gcirc;is, kiel spongo, kaj tremis pro malvarmo
        <fnt>
          <aut>Ivan &Scirc;irjaev</aut>
          <vrk><url ref="https://eo.wikisource.org/wiki/Tra_la_loko_ensor&ccirc;ita">Tra la loko ensor&ccirc;ita</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="tremp.0igxi">trempi&gcirc;i</ref>
  </snc>
  <trd lng="be">&c_m;&c_o;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_n;&c_a;&c_m;&c_o;&c_k;&c_r;&c_ja;&c_m; &c_s;&c_je;</trd>
  <trdgrp lng="de">
