<drv mrk="sek.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sek.0igxi.KOMUNE">
    <dif>
      I&gcirc;i senakva, senabunda, malri&ccirc;a:
      <ekz>
        sidi&gcirc;u &ccirc;e la fajro, por ke viaj vestoj <tld/>i&gcirc;u
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        post kelka tempo la torento el<tld/>i&gcirc;is, &ccirc;ar ne estis pluvo en la lando
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 17:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        for<tld/>i&gcirc;is la akvo sur la tero
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 8:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia forto el<tld/>i&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 22:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        alte sur la monto <klr>[&#x2026;]</klr> la potencaj kverkoj <klr>[&#x2026;]</klr>
        velk<tld/>i&gcirc;is
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bretoj tiom <tld/>i&gcirc;is, ke multloke estis grandaj fendoj
        <fnt>
          <bib>HsT</bib>
          <lok>Feli&ccirc;a homo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> <tld/>i&gcirc;as de melankolio
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i tie jam <tld/>i&gcirc;is la gor&gcirc;oj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        naski&gcirc;is plano <klr>[por akiri]</klr> fonton de enspezoj, neniam <tld/>i&gcirc;antan
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        larmo virina balda&ubreve; <tld/>i&gcirc;as
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_o;&c_h;&c_n;&c_u;&c_c;&c_mol;</trd>
  <trdgrp lng="bg">
    <trd>&c_s;&c_u;&c_sh;&c_a; &c_s;&c_je;</trd>,
    <trd>&c_s;&c_malmol;&c_h;&c_n;&c_a;</trd>,
    <trd>&c_i;&c_z;&c_s;&c_u;&c_sh;&c_a;&c_v;&c_a;&c_m; &c_s;&c_je;</trd>,
    <trd>&c_i;&c_z;&c_s;&c_malmol;&c_h;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="ca">assecar-se</trd>
