<drv mrk="sek.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="sek.mal0a.sorbinta">
    <dif>
      Enhavanta multe da akvo a&ubreve; flua&jcirc;o:
      <ekz>
        mal<tld/>a &ccirc;ifono, vesto, vi&scirc;tuko;
      </ekz>
      <ekz>
        li estis tute mal<tld/>a, kaj la akvo fluis el liaj longaj blondaj haroj
        <fnt>
          <bib>Fab1</bib>
          <lok>Malbonkonduta knabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la mantelo estas mal<tld/>a, sed la vesto
        sub &gcirc;i &ndash; <tld/>a
        <fnt><bib>Metrop</bib></fnt>;
      </ekz>
      <ekz>
        larme mal<tld/>aj okuloj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili sentis sin bone en la mal<tld/>a elemento
        <klr>(akvo)</klr>
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> glitfaladis sur la mal<tld/>a herbo
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la mal<tld/>a argila grundo oni povis vidi la postesignojn de iliaj
        piedoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Najbaraj familioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni trenis &scirc;in en malluman, mal<tld/>an malliberejon
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        miaj fingroj estis mal<tld/>aj de mirha fluida&jcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 5:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>a ne&gcirc;o
        <klr>(ne&gcirc;o enhavanta multe da likva akvo)</klr>
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        regis kruda, mal<tld/>a <klr>[&#x2026;]</klr> vetero
        <fnt>
          <bib>Fab4</bib>
          <lok>Anneto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pluv.0o">pluvo</ref>,
      <ref cel="ros.0o">roso</ref>
    </refgrp>
    <trd lng="bg">&c_m;&c_o;&c_k;&c_malmol;&c_r;</trd>
    <trdgrp lng="ca">
      <trd>mullat</trd>,
      <trd>banyat</trd>
    </trdgrp>
    <trd lng="de">nass</trd>
    <trd lng="it">bagnato</trd>
    <trd lng="ru">&c_m;&c_o;&c_k;&c_r;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="sek.mal0a.humida">
    <dif>
      <klr>(pri gaso, atmosfero:)</klr>
      Entenanta percepteble multe da gasstata akvo;
      <ref tip="sin" cel="humid.0a.malseka">humida<sncref/></ref>:
      <ekz>
        densa ondo da mucida, mal<tld/>a aero frapis
        la viza&gcirc;ojn de amba&ubreve; enirantaj virinoj
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        &gcirc;ardenoj, kies arboj dank' al la mal<tld/>a blovo
        de l' maro kreskis
        <fnt><bib>Far2</bib>, <lok>&Ccirc;ap. 2a</lok></fnt>;
      </ekz>
      <ekz>
        mal<tld/>a frida klimato
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 22</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_v;&c_l;&c_a;&c_zh;&c_je;&c_n;</trd>
    <trd lng="ca">humit</trd>
    <trdgrp lng="de">
      <trd>feucht</trd>,
      <trd>humid</trd>
    </trdgrp>
    <trd lng="it">umido</trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_l;&c_a;&c_zh;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_y;&c_r;&c_o;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_o;&c_k;&c_r;&c_y;</trd>,
    <trd>&c_v;&c_ib;&c_l;&c_mol;&c_g;&c_o;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="af">nat</trd>
  <trd lng="bo">&t_ra;&t_lata;&t_naro;&t_na;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trd lng="ca">mullat</trd>
