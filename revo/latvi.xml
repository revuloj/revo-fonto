<art mrk="$Id: latvi.xml,v 1.19 2024/01/15 16:14:08 revo Exp $">
<kap>
  <rad>Latvi</rad>/o
</kap>

<drv mrk="latvi.L0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="latv.L0ujo.GEOG">Latvujo</ref>:
      <ekz>
        <tld lit="L"/>o estas lando en nordoriento de E&ubreve;ropo.
        <fnt><bib>Viki</bib>, <lok><url ref="&Viki;/Latvio">Latvio</url></lok></fnt>;
      </ekz>
      <ekz>
        en la najbaraj baltaj landoj <tld/>o kaj Estonio la inflacia indico iom superis
        tiun de Litovio
        <fnt>
               <aut>LAST</aut>
          <vrk><url ref="&Monato;/2000/006854.php">Eta recesio</url></vrk>
          <bib>Monato</bib>
          <lok>2000/05, p. 7</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_L;&c_a;&c_t;&c_v;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_L;&c_a;&c_t;&c_v;&c_i;&c_ja;</trd>
  <trd lng="cs">Loty&scaron;sko</trd>
  <trd lng="de">Lettland</trd>
  <trd lng="en">Latvia</trd>
  <trd lng="es">Letonia</trd>
  <trd lng="fr">Lettonie</trd>
  <trd lng="hu">Lettorsz&aacute;g</trd>
  <trd lng="id">Latvia</trd>
  <trd lng="ja">&#x30E9;&#x30C8;&#x30D3;&#x30A2;</trd>
  <trd lng="nl">Letland</trd>
  <trd lng="pl">&Lstroke;otwa</trd>
  <trd lng="sk">Loty&scaron;sko</trd>
  <trd lng="uk">&#x41B;&c_a;&c_t;&c_v;&c_ib;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x62C9;&#x812B;&#x7DAD;&#x4E9E; <pr>L&#x101;tu&#x14D;w&#xE9;iy&#xE0;</pr></trd>,
    <trd>&#x62C9;&#x8131;&#x7EF4;&#x4E9A; <pr>L&#x101;tu&#x14D;w&#xE9;iy&#xE0;</pr></trd>
  </trdgrp>
</drv>
