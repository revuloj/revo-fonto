<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: latvi.xml,v 1.13 2019/09/11 22:10:23 revo Exp $">
<kap>
  <rad>Latvi</rad>/o
</kap>

<drv mrk="latvi.L0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="latv.L0ujo.GEOG">Latvujo</ref>:
      <ekz>
        <tld lit="L"/>o estas lando en nordoriento de E&ubreve;ropo.
        <fnt><bib>Viki</bib>, <lok><url 
        ref="http://eo.wikipedia.org/wiki/Latvio">Latvio</url></lok></fnt>;
      </ekz>
      <ekz>
        en la najbaraj baltaj landoj <tld/>o kaj Estonio la inflacia indico iom superis
        tiun de Litovio
        <fnt>
          <bib>Monato</bib>
          <aut>Last</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006854.html"
              >Litovio: Eta recesio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_L;&c_a;&c_t;&c_v;&c_i;&c_ja;</trd>
  <trd lng="cs">Loty&scaron;sko</trd>
  <trd lng="de">Lettland</trd>
  <trd lng="en">Latvia</trd>
  <trd lng="es">Letonia</trd>
  <trd lng="fr">Lettonie</trd>
  <trd lng="hu">Lettorsz&aacute;g</trd>
  <trd lng="id">Latvia</trd>
  <trd lng="nl">Letland</trd>
  <trd lng="pl">&Lstroke;otwa</trd>
  <trd lng="sk">Loty&scaron;sko</trd>
</drv>
</art>

