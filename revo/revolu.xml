<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc><rad>revoluci</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="revolu.0o">
  <kap><tld/>o</kap>
  <snc mrk="revolu.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Subita perforta &scirc;an&gcirc;o en politika re&gcirc;imo de iu lando.
    </dif>
    <refgrp tip="sin">
      <ref cel="renver">&scirc;tatrenverso</ref>
    </refgrp>
    <trd lng="nl">staatsgreep</trd>
  </snc>
  <snc mrk="revolu.0o.KOMUNE">
    <dif>
      Subita grava &scirc;an&gcirc;i&gcirc;o en irado de ia da&ubreve;ranta afero:
      <ekz>
        <tld/>o efektivi&gcirc;is en la prezoj.
      </ekz>
    </dif>
    <trdgrp lng="nl">
      <trd>revolutie</trd>,
      <trd>omwenteling</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_v;&c_a;&c_l;&c_ju;&c_c;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_r;&c_je;&c_v;&c_o;&c_l;&c_ju;&c_c;&c_i;&c_ja;</trd>
  <trd lng="ca">Revoluci&oacute;</trd>
  <trd lng="cs">revoluce</trd>
  <trd lng="de">Revolution</trd>
  <trd lng="en">revolution</trd>
  <trd lng="fr">r&eacute;volution</trd>
  <trd lng="hu">forradalom</trd>
  <trd lng="pt">revolu&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_r;&c_je;&c_v;&c_o;&c_l;&c_ju;&c_c;&c_i;&c_ja;</trd>
  <trd lng="zh">&#38761;&#21629;<klr> [g&eacute;m&igrave;ng]</klr></trd>
</drv>

<drv mrk="revolu.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="revolu.0igi.POL">
    <dif>
      Fari <tld/>on en.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; &c_r;&c_e;&c_v;&c_a;&c_l;&c_ju;&c_c;&c_y;&c_ju;</trd>
  <trd lng="ca">revolucionar</trd>
  <trd lng="de">revolutionieren</trd>
  <trd lng="fr">r&eacute;volutionner</trd>
  <trd lng="hu">forradalmas&iacute;t</trd>
  <trdgrp lng="nl">
    <trd>een <ind>revolutie</ind> tot stand brengen</trd>,
    <trd>een <ind>omwenteling</ind> veroorzaken</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_s;&c_t;&c_r;&c_o;&c_i;&c_t;&c_mol; <ind>&c_r;&c_je;&c_v;&c_o;&c_l;&c_ju;&c_c;&c_i;&c_ju;</ind> <klr>(&c_v;
  &c_ch;&c_jo;&c_m;-&c_l;.)</klr></trd>
</drv>

<drv mrk="revolu.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="revolu.0ulo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Ano de <tld/>a movado:
      <ekz>
        ili ne plu povis ligi kontakton al la aktivaj <tld/>uloj
        <fnt>
          <aut>J. London, trad. D. Kaiser, F. Vohla, H.G. Kaiser</aut>,
          <vrk>La meksikano</vrk>,
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">revolucionari <klr>(subst.)</klr></trd>
  <trd lng="fr">r&eacute;volutionnaire<klr> (subst.)</klr></trd>
</drv>

<drv mrk="revolu.antaux0a">
  <kap>anta&ubreve;<tld/>a</kap>
  <snc mrk="revolu.antaux0a.KAL">
    <dif>
      Rilata al tempo anta&ubreve;anta <tld/>on, okazinta
      anta&ubreve; &gcirc;i:
      <ekz>
        astrakana malnovre&gcirc;ima anta&ubreve;<tld/>a &ccirc;apo
        <fnt><bib>Metrop</bib><lok>&ccirc;ap. 2</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>pre-revolucionari</trd>,
    <trd>d'abans de la revoluci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>vorrevolution&auml;r</trd>
    <trd>prerevolution&auml;r</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d'avant la <ind>r&eacute;volution</ind></trd>,
    <trd>pr&eacute;-r&eacute;volutionnaire</trd>
  </trdgrp>
  <trd lng="hu">forradalom el&odblac;tti</trd>
  <trdgrp lng="nl">
    <trd>prerevolutionair</trd>,
    <trd>van voor de <ind>revolutie</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_o;&c_r;&c_je;&c_v;&c_o;&c_l;&c_ju;&c_c;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="revolu.industria0o">
  <kap>industria <tld/>o</kap>
  <snc mrk="revolu.industria0o.HIS">
    <uzo tip="fak">EKON</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Te&hcirc;nika kaj ekonomia &scirc;an&gcirc;o ekinta en la 19a jarcento en
      Anglujo kun ekuzo de industriaj ma&scirc;inoj,
      vasti&gcirc;inta al la mondo, kun
      da&ubreve;raj sekvoj en &ccirc;iuj aspektoj de homaj socioj:
      <ekz>
        la produktado ludis malgrandan rolon en ekonomiaj tekstoj anta&ubreve;
        la industria <tld/>o
        <fnt>
        <aut>Bo Sandelin</aut>,
        <vrk>Anta&ubreve;merkantilisma ekonomia pensado</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2001a, numero 2a, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_i;&c_n;&c_d;&c_u;&c_s;&c_t;&c_r;&c_i;&c_a;&c_l;&c_n;&c_a; &c_r;&c_je;&c_v;&c_o;&c_l;&c_ju;&c_c;&c_i;&c_ja;</trd>
  <trd lng="ca">revoluci&oacute; industrial</trd>
  <trd lng="de">industrielle <ind>Revolution</ind></trd>
  <trd lng="fr">r&eacute;volution industrielle</trd>
  <trd lng="hu">ipari <ind>forradalom</ind></trd>
  <trd lng="nl">industri&euml;le <ind>revolutie</ind></trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_m;&c_y;&c_sh;&c_l;&c_je;&c_n;&c_n;&c_a;&c_ja; &c_r;&c_je;&c_v;&c_o;&c_l;&c_ju;&c_c;&c_i;&c_ja;</trd>
</drv>

<drv mrk="revolu.kontraux0o">
  <kap>kontra&ubreve;<tld/>o</kap>
  <snc mrk="revolu.kontraux0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Reakcia renverso, kiu provas restarigi
      anta&ubreve;<tld/>an situacion:
      <ekz>
        la venko de la kontra&ubreve;<tld/>o <klr>...</klr>
        ne sukcesis stimuli lin je ven&gcirc;okrioj
        <fnt>
          <aut>J. Baghy</aut>,
          <vrk>Sur Sanga Tero</vrk>,
          <lok>1933</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>contrarevoluci&oacute;</trd>,
    <trd>reacci&oacute;</trd>
  </trdgrp>
  <trd lng="fr">contre-r&eacute;volution</trd>
</drv>

<drv mrk="revolu.post0a">
  <kap>post<tld/>a</kap>
  <snc mrk="revolu.post0a.KAL">
    <dif>
      Rilata al tempo sekvanta la <tld/>on, okazanta
      post &gcirc;i:
      <ekz>
        la eduka SAT-laboro estas anta&ubreve;- kaj
        post<tld/>a, ne dum<tld/>a
        <fnt><bib>Lanti</bib>, <lok>Pensoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">postrevolucionari</trd>
  <trdgrp lng="fr">
    <trd>d'apr&egrave;s la <ind>r&eacute;volution</ind></trd>,
    <trd>post-r&eacute;volutionnaire</trd>
  </trdgrp>
  <trd lng="hu">forradalom ut&aacute;ni</trd>
  <trdgrp lng="de">
    <trd>nachrevolution&auml;r</trd>
    <trd>postrevolution&auml;r</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>postrevolutionair</trd>,
    <trd>van na de <ind>revolutie</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_l;&c_je;&c_r;&c_je;&c_v;&c_o;&c_l;&c_ju;&c_c;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
