  <trd lng="uk">&c_d;&c_o;&c_r;&c_je;&c_v;&c_o;&c_l;&c_ju;&c_c;&c_ib;&c_j;&c_n;&c_i;&c_j;</trd>
</drv>

<drv mrk="revolu.industria0o">
  <kap>industria <tld/>o</kap>
  <snc mrk="revolu.industria0o.HIS">
    <uzo tip="fak">EKON</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Te&hcirc;nika kaj ekonomia
      <ref tip="lst" cel="sxangx.0o" lst="voko:eventoj">&scirc;an&gcirc;o</ref>
      ekinta en la 19a jarcento en
      Anglujo kun ekuzo de industriaj ma&scirc;inoj,
      vasti&gcirc;inta al la mondo, kun
      da&ubreve;raj sekvoj en &ccirc;iuj aspektoj de homaj socioj:
      <ekz>
        ma&scirc;inismo venigis la industrian <tld/>on, kiu perfektigis
        homekspluatadon kaj pravigis la aktualan kapitalismon
        <fnt><aut>Gilbert Ledon</aut>,
        <vrk><url ref="&Monato;/2000/006916.php">Bugra bug-insekto</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/05, p. 5</lok>
        </fnt>;
      </ekz>
      <ekz>
        la produktado ludis malgrandan rolon en ekonomiaj tekstoj
        anta&ubreve;
        la industria <tld/>o
        <fnt>
        <aut>Bo Sandelin</aut>,
        <vrk>Anta&ubreve;merkantilisma ekonomia pensado</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2001a, numero 2a, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
