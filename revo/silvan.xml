    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>Silvain</trd>,
    <trd>Sylvain</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Silvan</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>Silv&aacute;nus</trd>,
    <trd>Szilv&aacute;nusz</trd>
  </trdgrp>
</drv>
</art>

