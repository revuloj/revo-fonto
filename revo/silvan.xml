<art mrk="$Id$">
<kap>
  <rad>Silvan</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="silvan.0o">
  <kap><tld/>o</kap>
  <snc mrk="silvan.0o.nom">
    <uzo tip="fak">NOM</uzo>
