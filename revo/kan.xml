<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kan</rad>/o
</kap>
<drv mrk="kan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Alta herbo el fam. <ref tip="super" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Phragmites</trd>),
      kies trunketoj i&gcirc;as per seki&gcirc;ado
      fortikaj, kaj estas uzataj kiel bastonoj, iloj a&ubreve; meblomaterialo:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sin apogi sur rompita <tld/>o
        <klr>(iluzie kalkuli pri neefektiva helpo, vane fidi senpovan
        helponton)</klr>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>cane</trd>,
    <trd>reed</trd>
  </trdgrp>
  <trd lng="hu">n&aacute;d</trd>
  <trd lng="nl">riet</trd>
  <trd lng="ru">&c_t;&c_r;&c_o;&c_s;&c_t;&c_n;&c_i;&c_k;</trd>
</drv>

<drv mrk="kan.suker0o">
  <kap>suker<tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Speco de <tld/>o (<trd lng="la">Saccharum</trd>),
      kies suko donas sukeron.
    </dif>
  </snc>
  <trd lng="en">sugar <ind>cane</ind></trd>
  <trd lng="hu">cukorn&aacute;d</trd>
  <trd lng="nl">suikerriet</trd>
  <trd lng="ru">&c_s;&c_a;&c_h;&c_a;&c_r;&c_n;&c_y;&c_j; &c_t;&c_r;&c_o;&c_s;&c_t;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
