<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vagabond.xml,v 1.4 2022/03/28 11:08:54 revo Exp $">
<kap>
  <rad>vagabond</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="vagabond.0o">
  <kap><tld/>o</kap>
  <snc mrk="vagabond.0o.MOR">
    <dif>
      Malri&ccirc;a
      <ref tip="vid" cel="hejm.sen0ulo">senhejmulo</ref>,
      vagulo, kiu konstante vojiras:
      <ekz>
        <ctl>La <tld/>o kantas</ctl>
        <klr>(poemaro de Julio Baghy)</klr>
        <fnt>
          <aut>J. Baghy</aut>,
          <vrk>La vagabondo kantas</vrk>,
          <lok>1933</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tramp.0o.MOR">trampo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>pobuda</trd>,
    <trd>tul&#xE1;k</trd>,
    <trd>vagabund</trd>,
    <trd>vandr&#xE1;k</trd>
  </trdgrp>
  <trd lng="fr">vagabond</trd>
  <trdgrp lng="sk">
    <trd>tul&#xE1;k</trd>,
    <trd>vagabund</trd>,
    <trd>vandr&#xE1;k</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x431;&#x440;&#x43E;&#x434;&#x44F;&#x433;&#x430;</trd>,
    <trd>&#x432;&#x43E;&#x43B;&#x43E;&#x446;&#x44E;&#x433;&#x430;</trd>
  </trdgrp>
</drv>

<drv mrk="vagabond.0i">
  <kap><tld/>i</kap>
  <snc mrk="vagabond.0i.MOR">
    <dif>
      <ref tip="vid" cel="vag.0i">Vagadi</ref>,
      voja&gcirc;i kiel <tld/>o:
      <ekz>
        en tiu tempo li <tld/>is en A&ubreve;stralio
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>potulovat se</trd>,
    <trd>vagabundovat</trd>,
    <trd>&#x17E;&#xED;t vandr&#xE1;ck&#xFD;m &#x17E;ivotem</trd>
  </trdgrp>
  <trd lng="fr">vagabonder</trd>
  <trd lng="sk">potulova&#x165; sa</trd>
  <trdgrp lng="uk">
    <trd>&#x431;&#x440;&#x43E;&#x434;&#x438;&#x442;&#x438;</trd>,
    <trd>&#x431;&#x43B;&#x443;&#x43A;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x431;&#x440;&#x43E;&#x434;&#x44F;&#x436;&#x438;&#x442;&#x438;</trd>
  </trdgrp>
</drv>
</art>

