<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vagabond.xml,v 1.9 2024/04/10 23:24:10 revo Exp $">
<kap>
  <rad>vagabond</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="vagabond.0o">
  <kap><tld/>o</kap>
  <snc mrk="vagabond.0o.MOR">
    <dif>
      Malri&ccirc;a
      <ref tip="vid" cel="hejm.sen0ulo">senhejmulo</ref>,
      vagulo, kiu konstante vojiras:
      <ekz>
        <ctl>La <tld/>o kantas</ctl>
        <klr>(poemaro de Julio Baghy)</klr>
        <fnt>
          <aut>J. Baghy</aut>,
          <vrk>La vagabondo kantas</vrk>,
          <lok>1933</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tramp.0o.MOR">trampo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_d;&c_z;&c_ja;&c_g;&c_a;</trd>,
    <trd>&c_v;&c_a;&c_l;&c_a;&c_c;&c_u;&c_g;&c_a;</trd>,
    <trd>&c_b;&c_ja;&c_z;&c_d;&c_o;&c_m;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pobuda</trd>,
    <trd>tul&#xE1;k</trd>,
    <trd>vagabund</trd>,
    <trd>vandr&#xE1;k</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Vagabund</trd>,
    <trd>Landstreicher</trd>
    <trd>Nichtsesshafter</trd>
  </trdgrp>
  <trd lng="fr">vagabond</trd>
  <trdgrp lng="ja">
    <trd>&#x6D6E;&#x6D6A;&#32773;  <pr>&#12405;&#x308D;&#12358;&#12375;&#12419;</pr></trd>,
    <trd>&#x653E;&#x6D6A;&#32773;  <pr>&#x307B;&#12358;&#x308D;&#12358;&#12375;&#12419;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wagabunda</trd>,
    <trd>w&lstroke;&oacute;cz&eogonek;ga</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>tul&#xE1;k</trd>,
    <trd>vagabund</trd>,
    <trd>vandr&#xE1;k</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_r;&#x43E;&c_d;&#x44F;&c_g;&c_a;</trd>,
    <trd>&c_v;&#x43E;&#x43B;&#x43E;&c_c;&#x44E;&c_g;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="vagabond.0i">
  <kap><tld/>i</kap>
  <snc mrk="vagabond.0i.MOR">
    <dif>
      <ref tip="vid" cel="vag.0i">Vagadi</ref>,
      voja&gcirc;i kiel <tld/>o:
      <ekz>
        en tiu tempo li <tld/>is en A&ubreve;stralio
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_d;&c_z;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_je;&c_s;&c_mol;&c_c;&c_ib; <ind>&c_b;&c_a;&c_d;&c_z;&c_ja;&c_zh;&c_n;&c_a;&c_je;</ind> &c_zh;&c_y;&c_c;&c_mol;&c_c;&c_jo;</trd>,
    <trd>&c_v;&c_a;&c_l;&c_a;&c_c;&c_u;&c_zh;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>potulovat se</trd>,
    <trd>vagabundovat</trd>,
    <trd>&#x17E;&#xED;t vandr&#xE1;ck&#xFD;m &#x17E;ivotem</trd>
  </trdgrp>
  <trd lng="fr">vagabonder</trd>
  <trd lng="pl">w&lstroke;&oacute;czy&cacute; si&eogonek;</trd>
  <trd lng="sk">potulova&tcaron; sa</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_r;&#x43E;&c_d;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_b;&#x43B;&c_u;&#x43A;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_r;&#x43E;&c_d;&#x44F;&c_zh;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
</art>

