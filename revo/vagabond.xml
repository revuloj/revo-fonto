<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vagabond.xml,v 1.11 2025/04/08 19:13:10 revo Exp $">
<kap>
  <rad>vagabond</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="vagabond.0o">
  <kap><tld/>o</kap>
  <snc mrk="vagabond.0o.MOR">
    <dif>
      Malri&ccirc;a
      <ref tip="vid" cel="hejm.sen0ulo">senhejmulo</ref>,
      vagulo, kiu konstante vojiras:
      <ekz>
        <ctl>La <tld/>o kantas</ctl>
        <klr>(poemaro de Julio Baghy)</klr>
        <fnt>
          <aut>J. Baghy</aut>,
          <vrk>La vagabondo kantas</vrk>,
          <lok>1933</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tramp.0o.MOR">trampo</ref>
    </refgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Anka&#365; eblas diri <ke>vagulo</ke>, <ke>vaganto</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_d;&c_z;&c_ja;&c_g;&c_a;</trd>,
    <trd>&c_v;&c_a;&c_l;&c_a;&c_c;&c_u;&c_g;&c_a;</trd>,
    <trd>&c_b;&c_ja;&c_z;&c_d;&c_o;&c_m;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pobuda</trd>,
    <trd>tul&#225;k</trd>,
    <trd>vagabund</trd>,
    <trd>vandr&#225;k</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Vagabund</trd>,
    <trd>Landstreicher</trd>
    <trd>Nichtsesshafter</trd>
  </trdgrp>
  <trd lng="fr">vagabond</trd>
  <trdgrp lng="ja">
    <trd>&#28014;&#28010;&#32773;  <pr>&#12405;&#12429;&#12358;&#12375;&#12419;</pr></trd>,
    <trd>&#25918;&#28010;&#32773;  <pr>&#12411;&#12358;&#12429;&#12358;&#12375;&#12419;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wagabunda</trd>,
    <trd>w&lstroke;&oacute;cz&eogonek;ga</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>tul&#225;k</trd>,
    <trd>vagabund</trd>,
    <trd>vandr&#225;k</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_r;&#1086;&c_d;&#1103;&c_g;&c_a;</trd>,
    <trd>&c_v;&#1086;&#1083;&#1086;&c_c;&#1102;&c_g;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#27969;&#28010;&#27721; <pr>li&#250;l&#224;ngh&#224;n</pr></trd>,
    <trd>&#27969;&#28010;&#28450; <pr>li&#250;l&#224;ngh&#224;n</pr></trd>,
    <trd>&#28418;&#27850; <pr>pi&#257;ob&#243;</pr></trd>,
    <trd>&#28216;&#27665; <pr>y&#243;um&#237;n</pr></trd>,
    <trd>&#36938;&#27665; <pr>y&#243;um&#237;n</pr></trd>,
    <trd>&#28010;&#20154; <pr>l&#224;ngr&#233;n</pr></trd>,
    <trd>&#21513;&#26222;&#36093; <pr>j&#237;p&#468;s&#224;i</pr></trd>,
    <trd>&#21513;&#26222;&#36187; <pr>j&#237;p&#468;s&#224;i</pr></trd>,
    <trd>&#27969;&#28010;&#32773; <pr>li&#250;l&#224;ngzh&#283;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="vagabond.0i">
  <kap><tld/>i</kap>
  <snc mrk="vagabond.0i.MOR">
    <dif>
      <ref tip="vid" cel="vag.0i">Vagadi</ref>,
      voja&gcirc;i kiel <tld/>o:
      <ekz>
        en tiu tempo li <tld/>is en A&ubreve;stralio
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_d;&c_z;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_je;&c_s;&c_mol;&c_c;&c_ib; <ind>&c_b;&c_a;&c_d;&c_z;&c_ja;&c_zh;&c_n;&c_a;&c_je;</ind> &c_zh;&c_y;&c_c;&c_mol;&c_c;&c_jo;</trd>,
    <trd>&c_v;&c_a;&c_l;&c_a;&c_c;&c_u;&c_zh;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>potulovat se</trd>,
    <trd>vagabundovat</trd>,
    <trd>&#382;&#237;t vandr&#225;ck&#253;m &#382;ivotem</trd>
  </trdgrp>
  <trd lng="fr">vagabonder</trd>
  <trd lng="pl">w&lstroke;&oacute;czy&cacute; si&eogonek;</trd>
  <trd lng="sk">potulova&tcaron; sa</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_r;&#1086;&c_d;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_b;&#1083;&c_u;&#1082;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_r;&#1086;&c_d;&#1103;&c_zh;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#27969;&#28010; <pr>li&#250;l&#224;ng</pr></trd>
</drv>
</art>

