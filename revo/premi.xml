<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>premi</rad>/o
</kap>
<drv mrk="premi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Rekompenco, donacata al la plej indaj:
      <ekz>
        disdoni <tld/>ojn al la la&ubreve;reatoj;
      </ekz>
      <ekz>
        anonci <tld/>on por la kapo de krimulo.
      </ekz>
    </dif>
    <trd lng="de">Preis</trd>
    <trd lng="nl">prijs</trd>
  </snc>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Donaco proponata al a&ccirc;etantoj a&ubreve; abonantoj, por stimuli eblajn
      klientojn.
    </dif>
    <trd lng="de">Pr&auml;mie</trd>
    <trd lng="nl">premie</trd>
  </snc>
  <trd lng="en">prize</trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_m;&c_i;&c_ja;</trd>
</drv>

<drv mrk="premi.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Rekompenci per <tld/>o: (f)
      <ekz>
        oftaj rideksplodoj kaj apla&ubreve;da uragano <tld/>is la majstran
        publikan lecionon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>pr&auml;mieren</trd>,
    <trd>pr&auml;miieren</trd>,
    <trd>auszeichnen</trd>
  </trdgrp>
  <trd lng="nl">bekronen</trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
