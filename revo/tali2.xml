<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>tali</rad>/o<fnt>PIV2</fnt>
    </kap>

    <drv mrk="tali2.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">KEM</uzo>
      <mlg kod="IUPAC">Tl</mlg>
      
      <ref tip="dif" cel="talium.0o">taliumo</ref>
      <ref tip="hom" cel="tali.0o">talio</ref>
      
      <trd lng="be">&c_t;&c_a;&c_l;&c_ib;&c_j;</trd>
      <trd lng="br">taliom</trd>
      <trd lng="de">Thallium</trd>
      <trd lng="fr">thallium</trd>
      <trd lng="hu">tallium</trd>
      <trd lng="la">Thallium</trd>
      <trd lng="nl">thallium</trd>
      <trd lng="pl">tal</trd>
      <trdgrp lng="pt">
        <trd>t&aacute;lio</trd>
      </trdgrp>
      <trd lng="ru">&c_t;&c_a;&c_l;&c_l;&c_i;&c_j;</trd>
      <trd lng="sv">tallium</trd>
      <trdgrp lng="sk">
    <trd>driek</trd>,
    <trd>pás</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pas <klr>(těla)</klr></trd>,
    <trd>život</trd>
  </trdgrp>
</drv>
  </art>
