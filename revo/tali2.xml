<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tali2.xml,v 1.24 2023/10/21 13:02:26 revo Exp $">
<kap>
  <rad>tali</rad>/o <fnt>PIV2</fnt>,
  <var><kap><rad var="um">talium</rad>/o <fnt>Z</fnt></kap></var>
</kap>

<drv mrk="tali2.0o">
  <kap><tld/>o,
    <var><kap><tld var="um"/>o</kap></var>
  </kap>
  <uzo tip="fak">KEM</uzo>
  <mlg kod="IUPAC">Tl</mlg>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="81">elemento</ref>
      kun la atomnumero 81, atompezo 204,37;
      malofta, malpeza kaj mola posttransira 
      <ref tip="super" cel="metal.0o">metalo</ref>:
      <ekz>
        <tld var="um"/>o (a&ubreve; <tld/>o) estas kemia elemento <klr>[kiun]</klr> malkovris 
        la brita fizikisto kaj kemiisto <nom>William Crookes</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=27276">Taliumo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        pluraj elementoj, kiel heliumo, <tld var="um"/>o, kaj cerio estis unue malkovritaj per
        iliaj spektraj linioj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=557352">Spektra linio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      Oni nomas la korpan regionon super la kokso <ref tip="hom" cel="tali.0o">talio</ref>.
    </rim>
  </snc>  
  <trd lng="be">&c_t;&c_a;&c_l;&c_ib;&c_j;</trd>
  <trd lng="bo">&#xF66;&#xF90;&#xF7A;&#xF51;&#xF0B;&#xF54;&#xF0B;</trd>
  <trd lng="br">taliom</trd>
  <trd lng="de">Thallium</trd>
  <trd lng="en">thallium</trd>
  <trd lng="fr">thallium</trd>
  <trd lng="hu">tallium</trd>
  <trdgrp lng="ja">
    <trd>&#x80F4;  <pr>&#x3069;&#x3046;</pr></trd>,
    <trd>&#x80F4;&#x4F53;  <pr>&#x3069;&#x3046;&#x305F;&#x3044;</pr></trd>,
    <trd>&#x8170;&#x56DE;&#x308A;  <pr>&#x3053;&#x3057;&#x307E;&#x308F;&#x308A;</pr></trd>,
    <trd>&#x8170;  <pr>&#x3053;&#x3057;</pr></trd>,
    <trd>&#x30A6;&#x30A8;&#x30B9;&#x30C8;  <pr>&#x3046;&#x3048;&#x3059;&#x3068;</pr></trd>
  </trdgrp>
  <trd lng="la">Thallium</trd>
  <trd lng="nl">thallium</trd>
  <trd lng="pl">tal</trd>
  <trdgrp lng="pt">
    <trd>t&aacute;lio</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_l;&c_l;&c_i;&c_j;</trd>
  <trd lng="sv">tallium</trd>
  <trdgrp lng="sk">
    <trd>driek</trd>,
    <trd>p&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pas <klr>(t&ecaron;la)</klr></trd>,
    <trd>&zcaron;ivot</trd>
  </trdgrp>
  <trd lng="uk">&#x442;&#x430;&#x43B;&#x456;&#x439;</trd>
</drv>
  </art>
