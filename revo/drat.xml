<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>drat</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="drat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Tre maldika, fleksebla kajlonga filamento a&ubreve; stangeto el metalo
      a&ubreve; alia materio, ordinare kun ronda sekcio.
    </dif>
  </snc>
  <trd lng="de">Draht</trd>
  <trd lng="en">wire</trd>
</drv>
<drv mrk="drat.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Provizi per <tld/>oj. 
    </dif>
  </snc>
  <trd lng="de">verdrahten</trd>
  <trd lng="en">wire</trd>
</drv>
<drv mrk="drat.pikil0o">
  <kap>pikil<tld/>o</kap>
  <snc>
    <dif>
      Metalfadeno  garnita per pikiloj, k servanta por bariloj.  
    </dif>
  </snc>
  <trd lng="de">Stacheldraht</trd>
  <trd lng="en">barbed wire</trd>
</drv>
</art>
