          <aut>PBworks</aut>,
          <vrk><url ref="http://fotokolekto.pbworks.com/lumbestetoj">
          lumbestetoj</url></vrk>, 
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_y; <ind>&c_d;&c_r;&c_o;&c_t;</ind></trd>,
    <trd>&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_y; <ind>&c_k;&c_a;&c_b;&c_e;&c_l;&c_mol;</ind></trd>
