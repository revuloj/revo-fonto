<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>8</ofc>
<rad>drat</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="drat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Tre maldika, fleksebla kaj longa filamento a&ubreve; stangeto el metalo
      a&ubreve; alia materio, ordinare kun ronda sekcio.
    </dif>
    <refgrp tip="vid">
      <ref cel="faden.0o">fadeno</ref>,
      <ref cel="kabl.0o">kablo</ref>,
      <ref cel="stang.0o">stango</ref>,
      <ref cel="sxnur.0o">&scirc;nuro</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Draht</trd>
  <trd lng="fr">fil<klr tip="amb"> (m&eacute;tallique)</klr></trd>
  <trd lng="en">wire</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_v;&c_o;&c_l;&c_o;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;</trd>
  </trdgrp>
</drv>
<drv mrk="drat.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Provizi per <tld/>oj. 
    </dif>
  </snc>
  <trd lng="de">verdrahten</trd>
  <trd lng="en">wire</trd>
</drv>
<drv mrk="drat.pikil0o">
  <kap>pikil<tld/>o</kap>
  <snc>
    <dif>
      Metalfadeno, garnita per pikiloj k servanta por bariloj.  
    </dif>
  </snc>
  <trd lng="de">Stacheldraht</trd>
  <trd lng="en">barbed wire</trd>
  <trdgrp lng="fr">
    <trd>barbel&eacute;</trd>,
    <trd>fil de fer barbel&eacute;</trd></trdgrp> 
  <trd lng="ru">&c_k;&c_o;&c_l;&c_ju;&c_ch;&c_a;&c_ja; &c_p;&c_r;&c_o;&c_v;&c_o;&c_l;&c_o;&c_k;&c_a;</trd>
</drv>
</art>
