<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>sep</rad></kap>
<drv mrk="sep.0">
<kap><ofc>*</ofc><tld/></kap>
   <snc>
   <dif>
     Ses kaj unu. Matematika simbolo 7:
     <ekz>
       anka&ubreve; Meze&ubreve;ropo estas &scirc;losita per <tld/>
       sigeloj
       <fnt><aut>Spomenka &Scaron;timec</aut>:
         <vrk>Hejmo en Meze&ubreve;ropo</vrk>,
         <bib>Monato</bib>
         <lok>jaro 1996a, numero 1a, p. 20a</lok>
       </fnt>.
     </ekz>
   </dif>
   </snc>
   <trd lng="be">&c_s;&c_je;&c_m;</trd>
   <trd lng="br">seizh</trd>
   <trd lng="bg">&c_s;&c_je;&c_d;&c_je;&c_m;</trd>
   <trd lng="cs">sedm</trd>
   <trd lng="de">sieben</trd>
   <trd lng="en">seven</trd>
   <trd lng="es">siete</trd>
   <trd lng="fi">seitsem&auml;n</trd>
   <trd lng="fr">sept</trd>
   <trd lng="he">&shin;&bet;&ayin;</trd>
   <trd lng="hu">h&eacute;t</trd>
   <trd lng="ja">&#19971; [<ind>&#12375;&#12385;</ind>]
   	[<ind>&#12394;&#12394;</ind>]</trd>
   <trd lng="ku">heft</trd>
   <trd lng="nl">zeven</trd>
   <trd lng="pl">siedem</trd>
   <trd lng="ru">&c_s;&c_je;&c_m;&c_mol;</trd>
   <trd lng="sv">sju</trd>
   <trd lng="tr">yedi</trd>
   <trd lng="vo">vel</trd>
   <trd lng="zh">&#19971;<klr> [q&#299;]</klr></trd>
</drv>

<drv mrk="sep.0a">
<kap><tld/>a</kap>
   <snc>
   <dif>
     <tld lit="S"/>aranga, venanta post ses anta&ubreve;aj:
     <ekz>
       dum ses tagoj laboru kaj faru &ccirc;iujn viajn
       aferojn; sed la <tld/>a tago estas sabato de la
       Eternulo via Dio
       <fnt><bib>MT</bib>, <lok>&Eli; 20:9-10</lok></fnt>.
     </ekz>
   </dif>
   </snc>
   <trd lng="be">&c_s;&c_jo;&c_m;&c_y;</trd>
   <trd lng="br">seizhvet</trd>
   <trd lng="de">siebter</trd>
   <trd lng="fi">seitsem&auml;s</trd>
   <trdgrp lng="fr">
   <trd>septi&egrave;me</trd>
   </trdgrp>
   <trd lng="hu">hetedik</trd>
   <trd lng="nl">zevende</trd>
   <trd lng="ru">&c_s;&c_je;&c_d;&c_mol;&c_m;&c_o;&c_j;</trd>
   <trd lng="zh">&#31532;&#19971;<klr> [d&igrave;q&#299;]</klr></trd>
</drv>

<drv mrk="sep.0e">
<kap><tld/>e</kap>
   <snc>
   <dif>
     <tld lit="S"/>arange, post ses anta&ubreve;aj.
   </dif>
   </snc>
   <trd lng="br">da <ind>seizhvet</ind></trd>
   <trd lng="fi">seitsem&auml;nneksi</trd>
   <trdgrp lng="fr">
     <trd>septi&egrave;mement</trd>
   </trdgrp>
   <trd lng="hu">hetedszer<klr tip="ind"> (felsorol&aacute;sban)</klr></trd>
   <trd lng="nl">ten <ind>zevende</ind></trd>
   <trd lng="de">siebtens</trd>
   <trd lng="ru">&c_v;-&c_s;&c_je;&c_d;&c_mol;&c_m;&c_y;&c_h;</trd>
  </drv>

</art>

