<drv mrk="amas.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="amas.0igxi.KOMUNE">
    <dif>
      Kuni&gcirc;i en <tld/>o:
      <ekz>
        pli ol 2.500 homoj <tld/>i&gcirc;is en konstrua&jcirc;o
        en kiu jam kvinono estus tro
        <fnt>
          <aut>T. del Barrio</aut>
          <vrk><url
          ref="http://www.liberafolio.org/2013/esperantisto-gvidis-fugon-el-prizono-de-franco">
          Esperantisto gvidis fu&gcirc;on el prizono de
          Franco</url></vrk>
          <lok>libera folio, 2013-05-26</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_r;&c_u;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_mol;&c_b;&c_ib;&c_r;&c_a;&c_c;&c_c;&c_a; <klr>(&c_u; &c_v;&c_ja;&c_l;&c_ib;&c_k;&c_a;&c_j; &c_k;&c_o;&c_l;&c_mol;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sich anh&auml;ufen</trd>,
    <trd>sich ansammeln</trd>
  </trdgrp>
  <trdgrp lng="en"><trd>to amass</trd>, <trd>mass</trd></trdgrp>
  <trdgrp lng="es">
    <trd><ind>reunir</ind>se</trd>,
    <trd><ind>congregar</ind>se</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>amasser</ind></trd>,
    <trd>s'<ind>entasser</ind></trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>kumpul</baz>berkumpul</trd>,
    <trd><baz>himpun</baz>berhimpun</trd>,
    <trd><baz>gabung</baz>bergabung</trd>,
    <trd><baz>akumulasi</baz>berakumulasi</trd>
  </trdgrp>
  <trd lng="nl">zich <ind>ophopen</ind></trd>
  <trdgrp lng="pl">
    <trd>gromadzi&cacute; si&eogonek;</trd>,
    <trd>skupia&cacute; si&eogonek;</trd>,
    <trd>g&eogonek;stnie&cacute;<klr> (o t&lstroke;umie)</klr></trd>
  </trdgrp>
</drv>

