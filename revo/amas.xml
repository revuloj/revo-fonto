<drv mrk="amas.0a">
  <kap><tld/>a</kap>
  <snc mrk="amas.0a.KOMUNE">
    <dif>
      Mult-era, densa, ara:
      <ekz>
        dum festaj semajnoj <klr>[okazis]</klr> <tld/>a promenado
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a ma&scirc;ina produktado
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        granda, <tld/>a popolkunveno
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj revizioj kaj arestoj
        <fnt>
          <bib>SrT</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e dissemataj mensogoj kaj kalumnioj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Dua Kongreso Esperantista en Gen&egrave;ve en la 28a de a&ubreve;gusto 1906</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="grand.po0e">pogranda</ref>
    </refgrp>
    <trd lng="zh">&#20805;&#x6C9B; <pr>ch&omacron;ngp&egrave;i</pr></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_s;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_m;&c_a;&c_s;&c_y;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_g;&c_r;&c_a;&c_m;&c_a;&c_d;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hromadn&yacute;</trd>,
    <trd>masov&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>massenhaft</trd>,
    <trd>Massen-</trd>
  </trdgrp>
  <trd lng="en">mass</trd>
  <trdgrp lng="es">
    <trd>masiva</trd>,
    <trd>de <ind>masas</ind></trd>,
    <trd>en <ind>masa</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>de <ind>masse</ind></trd>,
    <trd>massif</trd>
  </trdgrp>
  <trd lng="id">massal</trd>
  <trd lng="ja">&#22823;&#x91CF;&#x306E;  <pr>&#x305F;&#12356;&#x308A;&#12423;&#12358;&#x306E;</pr></trd>
  <trd lng="nl">massa-</trd>
  <trdgrp lng="pl">
    <trd>masowy</trd>,
    <trd>hurtowy</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hromadn&yacute;</trd>,
    <trd>masov&yacute;</trd>
  </trdgrp>
