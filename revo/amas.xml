<drv mrk="amas.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="amas.0igi.KOMUNE">
    <dif>
      <ref tip="sin" cel="kolekt.0i.kunmeti">Kolekti</ref> por fari <tld/>on:
      <ekz>
        kie mi povas <tld/>igi miajn fruktojn
        <fnt>
          <bib>NT</bib>
          <lok>Luko 12:17</lok>
        </fnt>?
      </ekz>
      <ekz>
        ili <tld/>igas monon <klr>[&#8230;]</klr> kaj pasigas la vivon en &gcirc;uoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        la soluleco, en kiu &scirc;i pasigis la tutan matenon, <tld/>igis en &scirc;ia kapeto
        provizon da pensoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>igis en angulon muzikajn kajerojn, pastelajn skatolojn, pentrotola&jcirc;ojn
        kaj skizojn
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        post &ccirc;iuj <tld/>igitaj pruvoj, dubi ankora&ubreve; estas <klr>[&#8230;]</klr> nur obstineco
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Oka</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akapar.0i">akapari</ref>,
      <ref cel="akumul.0i">akumuli</ref>,
      <ref cel="aglome.0i">aglomeri</ref>,
      <ref cel="agluti.0i">aglutini</ref>.
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#x5C6F;&#x805A; <pr>t&uacute;nj&ugrave;</pr></trd>,
    <trd>&#x5C6F;&#x96C6; <pr>t&uacute;nj&iacute;</pr></trd>,
    <trd>&#x579C; <pr>du&ograve;</pr></trd>,
    <trd>&#22534;&#21046; <pr>du&imacron;zh&igrave;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_g;&c_u;&c_r;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>reunir</trd>,
    <trd>acumular</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hromadit</trd>,
    <trd>kumulovat</trd>,
    <trd>nakupit</trd>,
    <trd>shrnout na hromadu</trd>,
    <trd>sn&aacute;&scaron;et na hromadu</trd>,
    <trd>st&rcaron;&aacute;dat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>anh&auml;ufen</trd>,
    <trd>ansammeln</trd>,
    <trd>massieren <klr>(zusammenziehen, bes.
    milit&auml;risch)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
