<drv mrk="amas.0a">
  <kap><tld/>a</kap>
  <snc mrk="amas.0a.KOMUNE">
    <dif>
      Mult-era, densa, ara:
      <ekz>
        <tld/>e dissemataj mensogoj kaj kalumnioj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a bu&ccirc;ado;
      </ekz>
      <ekz>
        <tld/>a kunveno.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>hromadn&yacute;</trd>,
    <trd>masov&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
