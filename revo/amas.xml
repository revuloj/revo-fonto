<drv mrk="amas.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="amas.0igxi.KOMUNE">
    <dif>
      <ref cel="kun.0igxi">Kuni&gcirc;i</ref> en <tld/>o:
      <ekz>
        la tuta popolo <tld/>i&gcirc;is &ccirc;irka&ubreve; <klr>[&#8230;]</klr> la domo
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 26:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;is teruraj fulmotondraj nuboj
        <fnt>
          <bib>Fab2</bib>
          <lok>Poligono</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj pekoj <tld/>i&gcirc;is &gcirc;is la &ccirc;ielo
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 18:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        pli ol 2&#x202F;500 homoj <tld/>i&gcirc;is en konstrua&jcirc;o
        en kiu jam kvinono estus tro
        <fnt>
          <aut>T. del Barrio</aut>
          <vrk><url
          ref="http://www.liberafolio.org/2013/esperantisto-gvidis-fugon-el-prizono-de-franco">
          Esperantisto gvidis fu&gcirc;on el prizono de
          Franco</url></vrk>
