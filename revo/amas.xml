<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>amas</rad>/o
</kap>
<drv mrk="amas.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc>
     <dif>
       Multnombra, &gcirc;enerale senorda kolekto, multo da:
       <ekz>
         la unuaj piramidoj estis nur <tld/>o da &scirc;tonoj;
       </ekz>
       <ekz>
         la popol<tld/>o atakis la re&gcirc;an palacon;
       </ekz>
       <ekz>
         <tld/>o da mono kaj titolo de barono
         <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
       </ekz>
       <ekz>
         <tld/>o da &scirc;uldoj<fnt>Z</fnt>;
       </ekz>
       <ekz>
         liaj proponoj estis faritaj al ni en granda
         <tld/>o<fnt>Z</fnt> <klr>(tre multaj)</klr>;
       </ekz>
       <ekz>
         en <tld/>o <klr>(popol<tld/>o)</klr> e&ccirc; morto estas pli
         gaja
         <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
       </ekz>
       <ekz>
         al kuko k kaso &ccirc;iam  venas <tld/>o
         <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>
         <klr>(<ref tip="vid" cel="miel.0o">mielo</ref>)</klr>;
       </ekz>
       <ekz>
         <tld/>e dissemataj mensogoj kaj kalumumnioj<fnt>Z</fnt>;
       </ekz>
       <ekz>
         <tld/>a bu&ccirc;ado;
       </ekz>
       <ekz>
         <tld/>a kunveno.
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="flu.al0o">alflui</ref>,
       <ref cel="svarm.0i">svarmi</ref>,
       <ref cel="abund.0a">abunda</ref>,
       <ref cel="dens.0a">densa</ref>,
       <ref cel="kolekt1.0a">kolektiva</ref>.
     </refgrp>
     <refgrp tip="vid">
       <ref cel="mont.0o">monto</ref>,
       <ref cel="mar.0o">maro</ref>.
     </refgrp>
   </snc>
   <trd lng="cs">hromada</trd>
   <trdgrp lng="de"><trd>Masse</trd>, <trd>Menge</trd></trdgrp>
   <trd lng="en">mass</trd>
   <trdgrp lng="fr">
     <trd>masse</trd>,
     <trd>multitude</trd>,
     <trd>foule</trd></trdgrp>
   <trdgrp lng="nl">
   <trd>massa</trd>,
   <trd>menigte</trd>
       </trdgrp>
<trd lng="it">massa</trd>
   <trdgrp lng="ru"><trd>&#1090;&#1086;&#1083;&#1087;&#1072;</trd>, 
<trd>&#1089;&#1082;&#1086;&#1087;&#1083;&#1077;&#1085;&#1080;&#1077;</trd>, 
<trd>&#1082;&#1091;&#1095;&#1072;</trd></trdgrp>
</drv>
<drv mrk="amas.0igi">
   <kap><tld/>igi</kap>
   <snc>
     <dif>
       Kolekti por fari <tld/>on:
       <ekz>
         <tld/>igi scii&gcirc;ojn;
       </ekz>
       <ekz>
         <tld/>igi aron da kura&gcirc;uloj<fnt>Z</fnt>
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="akapar.0i">akapari</ref>,
       <ref cel="akumul.0i">akumuli</ref>,
       <ref cel="aglome.0i">aglomeri</ref>,
       <ref cel="agluti.0i">aglutini</ref>.
     </refgrp>
   </snc>
   <trd lng="de">anh&auml;fen</trd>
   <trdgrp lng="fr">
     <trd>amasser</trd>,
     <trd>entasser</trd></trdgrp>
   <trd lng="nl">ophopen</trd>
</drv>
</art>
