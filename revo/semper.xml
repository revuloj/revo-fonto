<vortaro>
<art mrk="$Id$">
<kap>
  <rad>semperviv</rad>/o
</kap>
<drv mrk="semper.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="krasul1.0oj">krasulacoj</ref>
      (<trd lng="la">Sempervivum</trd>), kreskanta en montaro, sur
      tegmentoj kaj malnovaj muroj.
    </dif>
  </snc>
  <trd lng="de">Hauswurz</trd>
  <trd lng="nl">huislook</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_l;&c_o;&c_d;&c_i;&c_l;&c_o;</trd>,
    <trd>&c_zh;&c_i;&c_v;&c_u;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
