<drv mrk="najbar.0a">
  <kap><tld/>a</kap>
  <snc>
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Rilata al apuda lo&gcirc;anto:
      <ekz>
        <tld/>aj familioj
        <fnt>
          <bib>Fab2</bib>
          <lok>Najbaraj familioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a malpaco
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la re&gcirc;ido devis <klr>[&#x2026;]</klr> ricevi kiel edzinon la filinon de la <tld/>a re&gcirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        pri havo <tld/>a oni estas malavara
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>nachbarschaftlich</trd>,
      <trd>Nachbarschafts-</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>neighboring</trd>,
      <trd>neighbouring</trd>,
      <trd>adjoining</trd>,
      <trd>adjacent</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>vecino</trd>,
      <trd>pr&oacute;ximo</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>s&aogonek;siedni</trd>,
      <trd>o&sacute;cienny</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>vizinho</trd>,
      <trd>pr&oacute;ximo</trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>vecin</trd>,
      <trd>al&abreve;turat</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="klr">(io)</uzo>
    <dif>
      Estanta proksime de io, <ref tip="sin" cel="apud.0a.KOMUNE">apuda</ref>:
      <ekz>
        la tegmento de unu <tld/>a domo kuntu&scirc;i&gcirc;is kun la tegmento de la alia domo
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a lando
        <fnt>
          <bib>Fab2</bib>
          <lok>Dano Holger</lok>
        </fnt>;
      </ekz>
      <ekz>
        vidante, ke en la <tld/>a &ccirc;ambro estas korteganoj, li diris per malla&ubreve;ta vo&ccirc;o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        tri <tld/>aj nomesoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a insulo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i rimarkis en la <tld/>a parko tiel belan ludon de koloroj
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        aleo, <tld/>a al la &gcirc;ardeno
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro V</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Nachbar-</trd>,
      <trd>angrenzend</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>neighboring</trd>,
      <trd>neighbouring</trd>,
      <trd>adjoining</trd>,
      <trd>adjacent</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>vecino</trd>,
      <trd>pr&oacute;ximo</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>okoliczny</trd>,
      <trd>przystaj&aogonek;cy</trd>,
      <trd>pobliski</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>vizinho</trd>,
      <trd>pr&oacute;ximo</trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>&icirc;nconjur&abreve;tor</trd>,
      <trd>congruent</trd>,
      <trd>din apropiere</trd>
    </trdgrp>
  </snc>
  <snc mrk="najbar.0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="najbar.0a.en_orda_aro">
      <fnt><bib>EKV</bib><lok>&para;288</lok></fnt>
      <dif>
        <klr>(p.p. elementoj <frm am="a"><k>a</k></frm> kaj
        <frm am="b"><k>b</k></frm> de
        <ref cel="ar.orda0o">orda aro</ref>
        <frm am="(bb E,le)">(<g>E</g>,&#x2264;)</frm>)</klr>
        Tiaj, ke ne ekzistas tia elemento <frm am="c"><k>c</k></frm>,
        ke <frm am="a lt c lt b"><k>a</k> &lt; <k>c</k> &lt;
        <k>b</k></frm> a&ubreve;
        <frm am="b lt c lt a"><k>b</k> &lt; <k>c</k> &lt;
        <k>a</k></frm>:
        <ekz>
          entjero <frm am="n"><k>n</k></frm> estas <tld/>a
          nur kun <frm am="n+1"><k>n</k>+1</frm> kaj
          <frm am="n-1"><k>n</k>-1</frm>;
        </ekz>
        <ekz>
          du racionaloj neniam povas esti <tld/>aj.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="antaux.0anto.MAT">anta&ubreve;anto</ref>,
        <ref cel="post.0anto.MAT">postanto</ref>.
      </refgrp>
      <trd lng="de">benachbart</trd>
      <trdgrp lng="en">
        <trd>neighbouring</trd>,
        <trd>consecutive</trd>,
        <trd>adjacent</trd>
      </trdgrp>
      <trdgrp lng="es">
        <trd>adyacente</trd>,
        <trd>consecutivo</trd>,
        <trd>contiguo<klr tip="ind"> (adj., math.)</klr></trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>adjacent</trd>,
        <trd>cons&eacute;cutif</trd>,
        <trd>voisin<klr tip="ind"> (adj., math.)</klr></trd>
      </trdgrp>
      <trd lng="pl">s&aogonek;siedni</trd>
      <trdgrp lng="ru">
        <trd>&c_s;&c_o;&c_s;&c_je;&c_d;&c_n;&c_i;&c_j;</trd>,
        <trd>&c_p;&c_o;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
      </trdgrp>
      <trdgrp lng="tr">
        <trd>biti&scedil;ik</trd>,
        <trd>kom&scedil;u a&ccedil;&inodot;lar<klr>(matematikte)</klr></trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="najbar.0a.grafee">
      <fnt><bib>KompLeks</bib></fnt>
      <dif>
        <klr>(p.p. pri du verticoj en
        <ref cel="grafe.0o.strukturo">grafeo</ref>)</klr>
        Tiaj, ke ekzistas e&gcirc;o, kies
        <ref cel="rand.0o.grafea_egxo">randoj</ref> ili estas;
        <klr>(p.p. pri du e&gcirc;oj en
        <ref cel="grafe.0o.strukturo">grafeo</ref>)</klr>
        Havantaj komunan randon.
      </dif>
      <trdgrp lng="de">
        <trd>benachbart</trd>,
        <trd>adjazent</trd>
