          <aut>A. Pu&scirc;kin, trad. A. Kor&jcirc;enkov</aut>,
          <vrk><url ref="&LOdE;Libroj/V_bibl01.pdf">
          La pika damo</url></vrk>,
          <lok>2003-2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_y;&c_r;&c_v;&c_o;&c_n;&c_je;&c_c;</trd>
  <trd lng="en">chervonets</trd>
  <trd lng="fr">tchervonets</trd>
  <trd lng="uk">&#x447;&#x435;&#x440;&#x432;&#x456;&#x43D;&#x435;&#x446;&#x44C;</trd>
</drv>
