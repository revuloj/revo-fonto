<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aspir.xml,v 1.34 2023/10/28 08:33:09 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>aspir</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="aspir.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="aspir.0i.KOMUNE">
    <dif>
      Forte <ref tip="super" cel="dezir.0i">deziri</ref>, 
      fervore <ref tip="super" cel="cel.0i.por_atingi">celi</ref>:
      <ekz>
        la peko ku&scirc;os &ccirc;e la pordo, kaj vin &gcirc;i <tld/>os
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 4:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> havas la permeson <tld/>i pri edzi&gcirc;o kun &scirc;i <klr>[&#x2026;]</klr>, sed
        li devas diveni tri aferojn, pri kiuj &scirc;i demandas
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        la homoj <tld/>is kaj klopodis kaj estis sa&gcirc;aj kaj malsa&gcirc;aj dum la paso de
        jarcentoj
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        amon &scirc;i &gcirc;uis sufi&ccirc;e &ccirc;e la du unuaj edzoj, kaj &ccirc;e la tria &scirc;i <tld/>as ion alian
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi timas tion, kion mi anta&ubreve;e tiom arde <tld/>is
        <fnt>
          <bib>FSI</bib>
          <lok>I</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu &ccirc;i rakonto tute ne <tld/>as ian biografian precizecon
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni <klr>[&#x2026;]</klr> sciigis min, ke multaj <tld/>as la postenon kaj ke la firmao
        donos al mi respondon la sekvontan tagon
        <fnt>
          <bib>KrM</bib>
          <lok>3. Ne Gutas Mielo El La &Ccirc;ielo.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>anto</ind> de la Moskva Komunista Akademio
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
        <trd lng="de">Bewerber</trd>
      </ekz>
      <ekz>
        en la aferon de &scirc;iaj <tld/>antoj li ne volas sin enmiksi
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        konkurantoj <klr>[&#x2026;]</klr> <tld/>is fari procesorojn ne multe malpli rapidajn ol
        tiuj de <nom>Intel</nom>
        <fnt>
          <aut>Franck ARNAUD</aut>
            <vrk><url ref="&Monato;/2000/007114.html">Intel alfrontas konkurencon</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 16</lok>
          </fnt>;
      </ekz>
      <ekz>
        rigardi kaj <tld/>i ne devigas akiri
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ambici.0i">ambicii</ref>,
      <ref cel="avid.0i">avidi</ref>,
      <ref cel="kandid.0i">kandidati</ref>,
      <ref cel="klopod.0i">klopodi</ref>,
      <ref cel="preten.0i">pretendi</ref>,
      <ref cel="sopir.0i.dezirajxon">sopiri<sncref/></ref>,
      <ref cel="streb.0i.KOMUNE">strebi</ref>
    </refgrp>
    <rim>
      Oni <em>deziras</em> a&ubreve; <em>celas</em> ion ajn,
      oni <em>aspiras</em> al io bona, agrabla, supera,
      oni <em>ambicias</em> ion altan, superan kaj malfacile atingeblan,
      oni <em>sopiras</em> pri io perdita.
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_ib;&c_m;&c_k;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_s;&c_t;&c_r;&c_je;&c_m;&c_ja; &c_s;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_s;&c_l;&c_je;&c_d;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>aspirar</trd>,
    <trd>anhelar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>aspirovat <klr>(na n&ecaron;co)</klr></trd>,
    <trd>dychtit</trd>,
    <trd>m&iacute;t cti&zcaron;&aacute;dost</trd>,
    <trd>sna&zcaron;it se</trd>,
    <trd>uch&aacute;zet se</trd>,
    <trd>&zcaron;&aacute;dat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>erstreben</trd>,
    <trd>bewerben</trd>,
    <trd>trachten</trd>,
    <trd>begehren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>aspire (to/after)</trd>,
    <trd>be <ind>ambitious of</ind></trd>,
    <trd>be <ind>eager</ind> for</trd>,
    <trd>desire earnestly</trd>,
    <trd>seek after</trd>
  </trdgrp>
  <trd lng="es"><ind>aspirar</ind> a</trd>
  <trd lng="fr">aspirer &agrave;</trd>
  <trdgrp lng="he">
    <trd>&lamed;&shin;&alef;&vav;&fpe; &lamed;</trd>,
    <trd>&lamed;&he;&shin;&tav;&vav;&qof;&qof;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>aspir&aacute;l</trd>,
    <trd>v&aacute;gyik</trd>,
    <trd>t&ouml;rekszik</trd>,
    <trd>p&aacute;ly&aacute;zik</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>aspirare<klr> (anelare)</klr></trd>,
    <trd>anelare</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x71B1;&#x671B;&#x3059;&#x308B;  <pr>&#x306D;&#x3064;&#x307C;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x3042;&#x3053;&#x304C;&#x308C;&#x308B;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd><ind>streven</ind> naar</trd>,
    <trd>nastreven</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>aspirowa&cacute;</trd>,
    <trd>d&aogonek;&zdot;y&cacute; <klr>do upragnionego celu</klr></trd>,
    <trd>pragn&aogonek;&cacute; <klr>czego&sacute;</klr></trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>dori</trd>,
    <trd>urm&abreve;re&#x219;te scopul dorit</trd>,
    <trd>s&abreve; doresc ceva</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_r;&c_je;&c_m;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_zh;&c_je;&c_l;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>uch&aacute;dza&tcaron; sa</trd>,
    <trd>usilova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_a;&c_g;&c_n;&c_u;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_i;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_b;&c_a;&c_zh;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x52AA; <pr>n&#x1D4;</pr></trd>,
    <trd>&#x529B;&#x6C42; <pr>l&#xEC;qi&#xFA;</pr></trd>,
    <trd>&#x6112; <pr>k&#xE0;i</pr></trd>,
    <trd>&#x55DC;&#x597D; <pr>sh&#xEC;h&#xE0;o</pr></trd>,
    <trd>&#x610F;&#x613F; <pr>y&#xEC;yu&#xE0;n</pr></trd>,
    <trd>&#x610F;&#x9858; <pr>y&#xEC;yu&#xE0;n</pr></trd>,
    <trd>&#x5E94;&#x8058; <pr>y&#xEC;ngp&#xEC;n</pr></trd>,
    <trd>&#x61C9;&#x8058; <pr>y&#xEC;ngp&#xEC;n</pr></trd>,
    <trd>&#x627E;&#x5DE5;&#x4F5C; <pr>zh&#x1CE;og&#x14D;ngzu&#xF2;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="aspir.0o">
  <kap><tld/>o</kap>
  <snc mrk="aspir.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="dezir.0o">Dezirego</ref> por iu alta celo:
      <ekz>
        arda <tld/>o al malproksima feli&ccirc;o
        <fnt>
          <aut>R. Rokero, trad. R. Rokero</aut>,
          <vrk><url ref="">
          La Malbeno de la Praktikismo</url></vrk>,
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        kunigis ilin multe da komunaj <tld/>oj kaj sopiroj, similaj traviva&jcirc;oj kaj
        mondproblemoj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        amikoj, kiujn ligas la sama ideo, la samaj <tld/>oj kaj esperoj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvina Kongreso Esperantista en Barcelona en la 6a de septembro
          1909</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> ne povas esti respondaj pri miaj personaj ideoj kaj <tld/>oj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Oka Kongreso Esperantista en Krakow en la 11a de a&ubreve;gusto 1912</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>on lian nomas vi minaco
        <fnt>
          <bib>Ifigenio</bib>
          <lok>Ifigenio en Ta&ubreve;rido</lok>
        </fnt>?
      </ekz>
      <ekz>
        iel utopia <tld/>o kelkfoje pelas homojn al fanatikaj ekscesoj
        <fnt>
          <aut>Werner Schad</aut>
            <vrk><url ref="&Monato;/2000/007074.php">Al pli justa mondo</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 7</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_a;&c_d;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_k;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_ib;&c_m;&c_k;&c_n;&c_je;&c_n;&c_n;&c_je;</trd>,
    <trd>&c_c;&c_ja;&c_g;&c_a; <klr>(&c_d;&c_a; &c_ch;&c_a;&c_g;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>aspirace</trd>,
    <trd>dycht&ecaron;n&iacute;</trd>,
    <trd>uch&aacute;zen&iacute; se</trd>,
    <trd>&uacute;sil&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bestrebung</trd>,
    <trd>Begehren</trd>,
    <trd>Ziel <klr>(ideell)</klr></trd>
  </trdgrp>
  <trd lng="fr">aspiration</trd>
  <trdgrp lng="ja">
    <trd>&#x71B1;&#x671B;  <pr>&#x306D;&#x3064;&#x307C;&#x3046;</pr></trd>,
    <trd>&#x5FD7;&#x9858;  <pr>&#x3057;&#x304C;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>aspiracja</trd>,
    <trd>d&aogonek;&zdot;enie</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>dorin&#x21B;a</trd>,
    <trd>tentativ&abreve;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>a&scaron;pir&aacute;cia</trd>,
    <trd>&uacute;silie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_a;&c_g;&c_n;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_i;&c_v;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_i;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
