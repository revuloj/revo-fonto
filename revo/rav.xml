<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rav.xml,v 1.25 2026/02/27 16:54:59 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>rav</rad>/i
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="rav.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="cxarm.0i">&Ccirc;armegi</ref> estigante
      entuziasman <ref cel="admir.0o">admiro</ref>n:
      <ekz>
        Salomono solvis al &scirc;i &ccirc;iujn &scirc;iajn demandojn <klr>[&#x2026;]</klr> kaj &scirc;i estis tute
        <tld/>ita
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 10:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;ojas mia koro, <tld/>i&gcirc;as mia animo
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 16:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;oju kaj <tld/>i&gcirc;u, &ccirc;ar via rekompenco estos granda en la &ccirc;ielo
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 5:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam &scirc;i staris en sia tuta ornamo, &scirc;i estis tiel <tld/>ante bela, ke la kortego
        <klr>[&#x2026;]</klr> profunde klini&gcirc;is anta&ubreve; &scirc;i
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia koro forte batis de <tld/>o
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fidela Johano</lok>
        </fnt>;
      </ekz>
      <ekz>
        pro <tld/>iteco li faris transkapi&gcirc;an salton
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        subite ekbruis la muziko, forte kaj <tld/>ante bele
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        neniam anta&ubreve;e tiel <tld/>antaj tonoj tu&scirc;is &scirc;ian orelon
        <fnt>
          <bib>Fab4</bib>
          <lok>Infano en la tombo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la plej malgrandaj idoj estas <tld/>ataj, kiam oni diras al ili nur <esc>krible,
        krable, plurremurre</esc>
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>!
      </ekz>
      <ekz>
        nin &ccirc;iam <tld/>as, kion ni ne havas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="amuz.0i">amuzi</ref>,
       <ref cel="delekt.0i">delekti</ref>,
       <ref cel="entuzi.0igi">entuziasmigi</ref>,
       <ref cel="gxoj.0igi">&gcirc;ojigi</ref>,
       <ref cel="plezur.0igi">plezurigi</ref>
     </refgrp>
   </snc>
   <trdgrp lng="be">
     <trd>&c_z;&c_a;&c_h;&c_a;&c_p;&c_l;&c_ja;&c_c;&c_mol;</trd>,
     <trd>&c_z;&c_a;&c_ch;&c_a;&c_r;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_p;&c_r;&c_y;&c_v;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol; &c_u;
     <ind>&c_z;&c_a;&c_h;&c_a;&c_p;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</ind></trd>
   </trdgrp>
  <trd lng="ca">encantar</trd>
  <trdgrp lng="cs">
    <trd>nadchnout</trd>,
    <trd>okouzlit</trd>,
    <trd>uchv&aacute;tit</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>bezaubern</trd>,
     <trd>entz&uuml;cken</trd>,
     <trd>hinrei&szlig;en</trd>,
     <trd>mitrei&szlig;en</trd>
   </trdgrp>
  <trd lng="en">delight</trd>
  <trd lng="es">encantar</trd>
  <trdgrp lng="fr">
     <trd>ravir</trd>,
     <trd>enchanter</trd>
   </trdgrp>
  <trdgrp lng="hu">
    <trd>elb&udblac;v&ouml;l</trd>,
    <trd>elb&aacute;jol</trd>
  </trdgrp>
  <trdgrp lng="it">
     <trd>deliziare</trd>,
     <trd>rallegrare</trd>
   </trdgrp>
   <trdgrp lng="ja">
    <trd>&#x3046;&#x3063;&#x3068;&#x308A;&#x3055;&#x305B;&#x308B;</trd>,
    <trd>&#x5FC3;&#x3092;&#x596A;&#x3046;  <pr>&#x3053;&#x3053;&#x308D;&#x3092;&#x3046;&#x3070;&#x3046;</pr></trd>,
    <trd>&#x6709;&#x9802;&#x5929;&#x306B;&#x3055;&#x305B;&#x308B;  <pr>&#x3046;&#x3061;&#x3087;&#x3046;&#x3066;&#x3093;&#x306B;&#x3055;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">verrukken</trd>
  <trdgrp lng="pl">
    <trd>zachwyca&cacute;</trd>,
    <trd>zachwyci&cacute;</trd>,
    <trd>oczarowywa&cacute;</trd>,
    <trd>oczarowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>encantar</trd>,
    <trd>extasiar</trd>,
    <trd>arrebatar</trd>,
    <trd>embevecer</trd>,
    <trd>enlear</trd>,
    <trd>maravilhar</trd>
  </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_v;&c_o;&c_s;&c_h;&c_i;&c_t;&c_i;&c_t;&c_mol;</trd>,
     <trd>&c_v;&c_o;&c_s;&c_h;&c_i;&c_shch;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_o;&c_ch;&c_a;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_p;&c_r;&c_i;&c_v;&c_je;&c_s;&c_t;&c_i; &c_v;
     &c_v;&c_o;&c_s;&c_t;&c_o;&c_r;&c_g;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>nadchn&uacute;&tcaron;</trd>,
    <trd>o&ccaron;ari&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_h;&c_o;&c_p;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_ch;&c_a;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_ch;&c_a;&c_r;&c_o;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_v;&c_o;&c_d;&c_i;&c_t;&c_i; &c_v; &c_z;&c_a;&c_h;&c_v;&c_a;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="rav.0a">
  <kap>
    <tld/>a
  </kap>
  <snc mrk="rav.0a.ravanta">
    <dif>
      Tia, ke &gcirc;i <tld/>as:
      <ekz>
        <tld/>a, belega, mirinda! ripetadis &ccirc;iuj
        <fnt>
          <bib>Fab1</bib>
          <lok>La novaj vestoj de la re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        vidi la <tld/>ajn bela&jcirc;ojn de la mondo
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> ludas violone la plej <tld/>ajn, sor&ccirc;ajn improviza&jcirc;ojn
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="admir.0inda">admirinda</ref>,
      <ref cel="cxarm.0a">&ccirc;arma</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>entz&uuml;ckend</trd>,
      <trd>bezaubernd</trd>
    </trdgrp>
    <trd lng="en">delightful</trd>
  </snc>
  <snc mrk="rav.0a.ravita">
    <dif>
      Montri admiron; <tld/>ita:
      <ekz>
        <tld/>a entuziasmo
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a sento pri preska&ubreve; sankta trankvilo
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> eligas <tld/>an ekkrion
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a rigardo &ccirc;irka&ubreve;prenante la graciajn krurojn de <nom>Alice</nom>
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">entz&uuml;ckt</trd>
    <trd lng="en">delighted</trd>
  </snc>
</drv>

<drv mrk="rav.0igxi">
  <kap>
    <tld/>i&gcirc;i
  </kap>
  <snc>
    <dif>
      Ekesti <tld/>ita:
      <ekz>
        &gcirc;ojas mia koro, <tld/>i&gcirc;as mia animo
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 16:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi komplete <tld/>i&gcirc;is pro lia belsona parolado
        <fnt>
          <bib>Monato</bib>
          <aut>Sen Rodin (Filippo Franceschi)</aut>
          <vrk><url ref="&Monato;/2007/009131.php"
              >Tri diskoj kun ses paroladoj de Ivo Lapenna</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i e&ccirc; plurfoje la&ubreve;tigis sian <tld/>i&gcirc;on pro la bonegaj man&gcirc;a&jcirc;oj
        <fnt>
          <bib>VaK</bib>
          <lok>Dekkvina &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="entuzi.0igxi">entuziasmi&gcirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="de">sich entz&uuml;cken</trd>
  <trd lng="en">get <ind>delighted</ind></trd>
</drv>

<drv mrk="rav.0iteco">
  <kap>
    <tld/>iteco
  </kap>
  <snc>
    <dif>
      Stato de iu <tld/>ita:
      <ekz>
        pro <tld/>iteco li faris transkapi&gcirc;an salton
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        senti <tld/>itecon pro la belegeco de la meblaro, kaj la poezieco de la ornama&jcirc;oj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        li staris <klr>[&#x2026;]</klr> dum momento, kun levitaj manoj, senmove pro <tld/>iteco
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekstaz.0o">ekstazo</ref>,
      <ref cel="entuzi.0o">entuziasmo</ref>
    </refgrp>
  </snc>
  <trd lng="de">Entz&uuml;ckung</trd>
  <trd lng="en">delight</trd>
  <trd lng="fr">enchantement </trd>
</drv>

</art>
