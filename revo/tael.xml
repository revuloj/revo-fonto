<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tael</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tael.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Antikva &ccirc;ina <ref tip="lst" 
      cel="unu.mezur0o" lst="voko:mezurunuoj">mezurunuo</ref> de pezo,
      37,8 gramojn 
      valora, kaj <ref tip="lst" cel="unu.mon0o"
      lst="voko:monunuoj">monunuo</ref>:
      <ekz>
        &ccirc;iu el ni ricevas du <tld/>ojn monate
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 56a, p. 237a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">Tael</trd>
  <trdgrp lng="de">
     <trd>Tael</trd>,
     <trd>Liang</trd>,
     <trd>Ta&euml;l</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>tael</trd>
  </trdgrp>
  <trd lng="hu">tael</trd>
  <trdgrp lng="pt">
   <trd>tael</trd>,
  </trdgrp>
</drv>
</art>

