<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tael</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tael.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Antikva &ccirc;ina pezunuo (37,8 g) kaj <ref tip="super"
      cel="unu.mon0o">mon-unuo</ref>:
      <ekz>
        &ccirc;iu el ni ricevas du <tld/>ojn monate
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 56a, p. 237a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Tael</trd>,
     <trd>Ta&euml;l</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>tael</trd>
  </trdgrp>
  <trd lng="hu">tael</trd>
</drv>
</art>

