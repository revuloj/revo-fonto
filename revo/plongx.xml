<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>plon&gcirc;</rad>/i
</kap>
<drv mrk="plongx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="plongx.0i.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Lerte sin &jcirc;eti en akvon por na&gcirc;i.
    </dif>
    <trdgrp lng="ru">
      <trd>&c_n;&c_y;&c_r;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_n;&c_y;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="plongx.0i.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      Abrupte kaj krute malsupreni&gcirc;i, direktante malsupren la nazon
      de <ref tip="vid" cel="aviad.0ilo">aviadilo</ref>:
      <ekz><tld/>bombi.</ekz>
    </dif>
    <trd lng="ru">&c_p;&c_i;&c_k;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="en">dive</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>