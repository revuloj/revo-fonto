<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pelv</rad>/o
</kap>
<drv mrk="pelv.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pelv.0o.vazo">
    <dif>
      Portebla malprofunda kaj lar&gcirc;a vazo por enteni flua&jcirc;on:
      <ekz>
        lavo<tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="basen.0o">baseno</ref>.
    </refgrp>                                           
    <trd lng="be">&c_t;&c_a;&c_z; <klr tip="amb">(&c_p;&c_o;&c_s;&c_u;&c_d;)</klr></trd>
    <trd lng="br">basin</trd>
    <trd lng="de">Sch&uuml;ssel</trd>
    <trdgrp lng="en">
      <trd>basin</trd>,
      <trd>bowl</trd>,
    </trdgrp>
    <trd lng="hu">mosd&oacute;t&aacute;l</trd>    
    <trdgrp lng="nl">
      <trd>kom</trd>,
      <trd>bekken</trd>
    </trdgrp>
    <trd lng="pl">miednica</trd>   
  </snc>
  <snc mrk="pelv.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Speco de osta zono, konsistiganta la bazon de la trunko kaj
      kuniganta &gcirc;in kun la kruroj.
    </dif>
    <trdgrp lng="be">
      <trd>&c_t;&c_a;&c_z; <klr tip="amb">(&c_ch;&c_a;&c_s;&c_t;&c_k;&c_a; &c_sh;&c_k;&c_ib;&c_l;&c_je;&c_t;&c_a;)</klr></trd>,
      <trd>&c_l;&c_a;&c_g;&c_v;&c_o;</trd>
    </trdgrp>                  
    <trd lng="br">divgroazell</trd>
    <trd lng="de">Becken</trd>
    <trd lng="en">pelvis</trd>
  </snc>                     
  <trd lng="fr">bassin</trd>
  <trd lng="hu">medence(csont)</trd>  
  <trd lng="nl">bekken</trd>
  <trd lng="pl">miednica</trd>   
  <trd lng="ru">&c_t;&c_a;&c_z;</trd>
</drv>

<drv mrk="pelv.0eto">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pelv.0eto.ujo">
    <dif>
      Malgranda <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Schale</trd>
</drv>

</art>
