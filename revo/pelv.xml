<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pelv</rad>/o
</kap>
<drv mrk="pelv.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pelv.0o.vazo">
    <dif>
      Portebla malprofunda kaj lar&gcirc;a vazo por enteni flua&jcirc;on:
      <ekz>
        <ind>lavo<tld/>o</ind><fnt>Z</fnt>;
        <trd lng="ca">rentamans</trd>
      </ekz>
      <ekz>
        donu la <tld/>on kun la terpomoj &ccirc;i tien
        <fnt>
          <aut>E. Kumi&ccaron;i&cacute;, trad. S. Rukelj</aut>,
          <vrk><url
          ref="http://free-vz.t-com.hr/Stanko/Surprizitaj.pdf">
          Surprizitaj nuptofestantoj</url></vrk>,
          <lok>inko, 2001</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_a;&c_z; <klr tip="amb">(&c_p;&c_o;&c_s;&c_u;&c_d;)</klr></trd>
    <trd lng="bg">&c_l;&c_je;&c_g;&c_je;&c_n;</trd>
    <trd lng="br">basin</trd>
    <trdgrp lng="ca">
      <trd>gibrell</trd>,
      <trd>conca</trd>
    </trdgrp>
    <trd lng="de">Sch&uuml;ssel</trd>
    <trdgrp lng="en">
      <trd>basin</trd>,
      <trd>bowl</trd>,
    </trdgrp>
    <trd lng="es">fuente<klr tip="amb"> (para servir a la
    mesa)</klr></trd>
    <trd lng="fa">&a_l;&f_g;&a_n;</trd>
    <trdgrp lng="fr">
      <trd>bassin</trd>,
      <trd>bassine</trd>
    </trdgrp>
    <trd lng="hu">mosd&oacute;t&aacute;l</trd>   
    <trd lng="id"><ind>tulang</ind> panggul</trd>
    <trdgrp lng="it">
      <trd>catino</trd>,
      <trd>bacile</trd>,
      <trd>bacinella</trd>
    </trdgrp> 
    <trdgrp lng="nl">
      <trd>kom</trd>,
      <trd>bekken</trd>
    </trdgrp>
    <trd lng="pl">miednica</trd>   
  </snc>
  <snc mrk="pelv.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Speco de osta zono, konsistiganta la bazon de la trunko kaj
      kuniganta &gcirc;in kun la kruroj.
    </dif>
    <trdgrp lng="be">
      <trd>&c_t;&c_a;&c_z; <klr tip="amb">(&c_ch;&c_a;&c_s;&c_t;&c_k;&c_a;
      &c_sh;&c_k;&c_ib;&c_l;&c_je;&c_t;&c_a;)</klr></trd>,
      <trd>&c_l;&c_a;&c_g;&c_v;&c_o;</trd>
    </trdgrp>                  
    <trd lng="bg">&c_t;&c_a;&c_z;</trd>
    <trd lng="br">divgroazell</trd>
    <trd lng="ca">pelvis</trd>
    <trd lng="de">Becken</trd>
    <trd lng="en">pelvis</trd>
    <trd lng="fa">&a_l;&f_g;&a_n; &a_kh;&a_A;&a_S;&a_r;&a_h;</trd>
    <trd lng="fr">pelvis</trd>
    <trdgrp lng="it">
      <trd>bacino<klr> (anat.)</klr></trd>,
      <trd>pelvi</trd>
    </trdgrp>
    <trd lng="nl">bekken</trd>
  </snc>
  <trd lng="cs">mísa</trd>
  <trd lng="hu">medence(csont)</trd>
  <trd lng="pl">miednica</trd>
  <trd lng="ru">&c_t;&c_a;&c_z;</trd>
  <trdgrp lng="sk">
    <trd>misa</trd>,
    <trd>plytká nádoba</trd>
  </trdgrp>
</drv>

<drv mrk="pelv.0eto">
  <kap><ofc>*</ofc><tld/>eto</kap>
  <snc mrk="pelv.0eto.ujo">
    <dif>
      Malgranda <tld/>o.
    </dif>
  </snc>
  <trd lng="bg">&c_l;&c_je;&c_g;&c_je;&c_n;&c_ch;&c_je;</trd>
  <trdgrp lng="ca">
    <trd>escudella</trd>,
    <trd>bol</trd>
  </trdgrp>
  <trd lng="cs">miska</trd>
  <trd lng="de">Schale</trd>
  <trd lng="fa">&a_l;&f_g;&a_n;&f_ch;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>cuvette</trd>,
    <trd>&eacute;cuelle</trd>,
    <trd>jatte</trd>
  </trdgrp>
  <trdgrp lng="it">
      <trd>catinella</trd>,
      <trd>bacinella</trd>
    </trdgrp>
  <trd lng="nl">schaal</trd>
  <trd lng="sk">miska</trd>
</drv>

</art>
