<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>vigvam</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="vigvam.0o">
  <kap><tld/>o</kap>
  <snc mrk="vigvam.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Duonsfera <ref tip="super" cel="kaban.0o">kabano</ref> iam uzata de
      nordorientamerikaj indianoj, el framo de kurbigitaj stangoj kaj
      arbo&scirc;ela tega&jcirc;o:
      <ekz>
        Jaguarpiedul, indiano, sidadis anta&ubreve; sia <tld/>o
        <fnt><bib>SatirRak</bib>, <lok>la guarbo</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="logx.0ejo">lo&gcirc;ejo</ref>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_g;&c_v;&c_a;&c_m;</trd>
  <trd lng="fr">wigwam</trd>
  <trd lng="hu">vigvam</trd>
  <trd lng="ru">&c_v;&c_i;&c_g;&c_v;&c_a;&c_m;</trd>
</drv>
</art>

