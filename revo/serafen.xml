<vortaro>
<art mrk="$Id: serafen.xml,v 1.4 2025/05/26 20:32:18 revo Exp $">
<kap>
  <rad>Serafen</rad>/o
</kap>

<drv mrk="serafen.0o">
  <kap><tld/>o</kap>
  <snc mrk="serafen.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> hebredevena:
      <ekz>
        li lo&gcirc;as en la domo de pastro <tld/>o
        <fnt>
          <aut>I. Silone, trad. D. Mistretta</aut>
          <vrk><url
          ref="http://ekladata.com/XiKWRW1ETT4zBOHrQZm16PxBQmc/silone-la-sekreto-de-luka.pdf">
          La sekreto de Luka</url></vrk>
          <lok>[vidita en 2018]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_S;&c_e;&c_r;&c_a;&c_f;&c_ib;&c_n;</trd>,
    <trd>&c_S;&c_e;&c_r;&c_a;&c_f;&c_ib;&c_m;</trd>
  </trdgrp>
  <trd lng="de">Serafin</trd>
  <trd lng="fr">S&eacute;raphin</trd>
  <trd lng="pl">Serafin</trd>
</drv>
</art>

