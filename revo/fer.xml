<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fer.xml,v 1.81 2021/06/18 20:41:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fer</rad>/o
</kap>

<drv mrk="fer.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fer.0o.KEM">
  <uzo tip="fak">KEM</uzo>
  <mlg kod="IUPAC">Fe</mlg>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="26">elemento</ref>
      kun atomnumero 26, atompezo 55,847; malmola for&gcirc;ebla
      <ref tip="super" cel="metal.0o">metalo</ref>, kiu estas tre 
      uzata en multaj industrioj:
      <ekz>
        <ind>for&gcirc;u <tld/>on dum &gcirc;i estas varmega</ind>
        <klr>(ne ellasu la okazon)</klr>
        <trd lng="fr">il faut battre le <ind>fer</ind> quand il est
        chaud</trd>
        <trd lng="hu">addig &uuml;sd a <ind>vasat</ind>, am&iacute;g
        meleg</trd>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">TEK</uzo>
        <ind>stanita <tld/>o</ind>.
        <trdgrp lng="fr">
          <trd>fer &eacute;tam&eacute;</trd>,
          <trd>fer-blanc</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>&oacute;nozott <ind>vas</ind></trd>,
          <trd>feh&eacute;rb&aacute;dog</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="af">yster</trd>
  <trd lng="am">&#x1265;&#x1228;&#x1275;</trd>
  <trd lng="ar">&#x62D;&#x62F;&#x64A;&#x62F;</trd>
  <trd lng="az">d&#x259;mir</trd>
  <trd lng="be">&c_zh;&c_a;&c_l;&c_je;&c_z;&c_a;</trd>
  <trd lng="bg">&c_zh;&c_je;&c_l;&c_ja;&c_z;&c_o;</trd>
  <trd lng="bn">&#x9B2;&#x9CB;&#x9B9;&#x9BE;</trd>
  <trd lng="bo">&#xF63;&#xF95;&#xF42;&#xF66;&#xF0B;</trd>
  <trd lng="br">houarn</trd>
  <trd lng="bs">&#x17E;eljeza</trd>
  <trd lng="co">ferru</trd>
  <trdgrp lng="cs">
    <trd>&zcaron;elezo</trd>,
    <trd>&zcaron;elezo zn. Fe</trd>
  </trdgrp>
  <trd lng="da">jern</trd>
  <trd lng="de">Eisen</trd>
  <trd lng="en">iron</trd>
  <trd lng="es">hierro</trd>
  <trd lng="et">raud</trd>
  <trd lng="eu">burdina</trd>
  <trd lng="fr">fer<klr tip="ind"> (m&eacute;tal)</klr></trd>
  <trd lng="fy">izer</trd>
  <trd lng="gl">ferro</trd>
  <trd lng="gu">&#xAB2;&#xACB;&#xA96;&#xA82;&#xAA1;</trd>
  <trd lng="ha">ba&#x199;in &#x199;arfe</trd>
  <trd lng="hi">&#x932;&#x94B;&#x939;&#x93E;</trd>
  <trd lng="hr">&#x17E;eljezo</trd>
  <trd lng="hu">vas</trd>
  <trd lng="hy">&#x565;&#x580;&#x56F;&#x561;&#x569;</trd>
  <trd lng="id">besi</trd>
  <trd lng="is">j&#xE1;rn</trd>
  <trd lng="it">ferro</trd>
  <trd lng="ja">&#x9244;</trd>
  <trd lng="jw">wesi</trd>
  <trd lng="ka">&#x10E0;&#x10D9;&#x10D8;&#x10DC;&#x10D8;&#x10E1;</trd>
  <trd lng="km">&#x1787;&#x17B6;&#x178F;&#x17B7;&#x178A;&#x17C2;&#x1780;</trd>
  <trd lng="ko">&#xCCA0;</trd>
  <trd lng="lo">&#xE97;&#xEB2;&#xE94;&#xEC0;&#xEAB;&#xEBC;&#xEB1;&#xE81;</trd>
  <trd lng="ml">&#xD07;&#xD30;&#xD41;&#xD2E;&#xD4D;&#xD2A;&#xD4D;</trd>
  <trd lng="mr">&#x932;&#x94B;&#x916;&#x902;&#x921;</trd>
  <trd lng="my">&#x101E;&#x1036;</trd>
  <trd lng="ne">&#x92B;&#x932;&#x93E;&#x92E;</trd>
  <trd lng="nl">ijzer</trd>
  <trd lng="ny">chitsulo</trd>
  <trd lng="pa">&#xA32;&#xA4B;&#xA39;&#xA47;</trd>
  <trd lng="pl">&zdot;elazo</trd>
  <trd lng="ps">&#x62F; &#x627;&#x648;&#x633;&#x67E;&#x646;&#x6D0;</trd>
  <trd lng="pt">ferro</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_je;&c_z;&c_o;</trd>
  <trd lng="sd">&#x644;&#x648;&#x6BE;</trd>
  <trd lng="si">&#xDBA;&#xD9A;&#xDA9;</trd>
  <trd lng="sk">&zcaron;elezo <klr>(Fe)</klr></trd>
  <trd lng="sq">hekuri</trd>
  <trd lng="st">tsepe</trd>
  <trd lng="sv">j&auml;rn</trd>
  <trd lng="ta">&#xB87;&#xBB0;&#xBC1;&#xBAE;&#xBCD;&#xBAA;&#xBC1;</trd>
  <trd lng="te">&#xC07;&#xC28;&#xC41;&#xC2E;&#xC41;</trd>
  <trd lng="th">&#xE40;&#xE2B;&#xE25;&#xE47;&#xE01;</trd>
  <trd lng="tl">bakal</trd>
  <trd lng="ur">&#x644;&#x648;&#x6C1;&#x6D2;</trd>
  <trd lng="vi">s&#x1EAF;t</trd>
  <trd lng="vo">lel</trd>
  <trd lng="yi">&#x5E4;&#x5BC;&#x5E8;&#x5E2;&#x5E1;&#x5DF;</trd>
  <trd lng="zu">nensimbi</trd>
</drv>

<drv mrk="fer.0a">
  <kap><tld/>a</kap>
  <snc mrk="fer.0a.KEM">
    <dif>
      De <tld/>o, el <tld/>o.
    </dif>
    <trd lng="bg">&c_zh;&c_je;&c_l;&c_je;&c_z;&c_je;&c_n;</trd>
    <trdgrp lng="br">
      <trd>-<ind>houarn</ind></trd>,
      <trd>en <ind>houarn</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>vas-</trd>,
      <trd>vasb&oacute;l val&oacute;</trd>
    </trdgrp>
    <trd lng="nl">ijzer-</trd>
    <trd lng="pl">&zdot;elazny</trd>
  </snc>
  <snc mrk="fer.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senceda, severa, neindulga:
      <ekz>
        <tld/>a volo, konstanteco;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>teni iun per <tld/>a mano</ind>
        <fnt><bib>PrV</bib></fnt>
        <klr>(<ref cel="buton.0umi">butonumi</ref>,
        <ref cel="konduk.0ilo.rimeno">kondukilo</ref>)</klr>;
        <trd lng="fr">diriger qn d'une <ind>main</ind> de fer</trd>
      </ekz>
      <ekz>
        <tld/>a disciplino, le&gcirc;o, realo;
      </ekz>
      <ekz>
        <tld/>a <klr>(nelacigebla)</klr> pacienco.
      </ekz>
    </dif>
    <trd lng="hu">vas-</trd>
    <trd lng="nl">ijzersterk</trd>
    <trdgrp lng="pl">
      <trd>&zdot;elazny<klr> (np. o dyscyplinie)</klr></trd>,
      <trd>twardy<klr> (np. o prawie, wychowaniu)</klr></trd>,
      <trd>nieugi&eogonek;ty<klr> (np. o czyjej&sacute; woli)</klr></trd>,
      <trd>anielski<klr> (o cierpliwo&sacute;ci)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_l;&c_je;&c_z;&c_n;&c_y;</trd>
  <trdgrp lng="br">
    <trd>dir<klr tip="ind"> (db. an dud)</klr></trd>,
    <trd>divrall<klr tip="ind"> (db. an dud)</klr></trd>,
    <trd>strizh<klr tip="ind"> (db. an dud)</klr></trd>
  </trdgrp>
  <trd lng="cs">&zcaron;elezn&yacute;</trd>
  <trd lng="de">eisern</trd>
  <trd lng="en">iron</trd>
  <trd lng="es">de <ind>hierro</ind></trd>
  <trdgrp lng="fr">
    <trd>de <ind>fer</ind></trd>,
    <trd>&eacute;nergique</trd>,
    <trd>implacable</trd>,
    <trd>strict</trd>,
    <trd>vigoureux</trd>
  </trdgrp>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_je;&c_z;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">&zcaron;elezn&yacute;</trd>
  <trd lng="vo">lelik</trd>
</drv>

<drv mrk="fer.0eca">
  <kap><tld/>eca, <var><kap><tld/>hava</kap></var></kap>
  <snc mrk="fer.0eca.KEM">
    <dif>
      Enhavanta <tld/>ajn substancojn:
      <ekz>
        <tld/>eca tero, akvo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_l;&c_je;&c_z;&c_ib;&c_s;&c_t;&c_y;</trd>
  <trd lng="br">houarnek</trd>
  <trdgrp lng="cs">
    <trd>obsahuj&iacute;c&iacute; &zcaron;elezo</trd>,
    <trd>&zcaron;elezit&yacute;</trd>
  </trdgrp>
  <trd lng="de">eisenhaltig</trd>
  <trdgrp lng="es">
    <trd>ferruginoso</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ferreux</trd>,
    <trd>ferrugineux</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vasas</trd>,
    <trd>vastartalm&uacute;</trd>
  </trdgrp>
  <trd lng="nl">ijzerhoudend</trd>
  <trd lng="pl">&zdot;elazisty</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_je;&c_z;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>&zcaron;elezit&yacute;</trd>,
    <trd>&zcaron;eleznat&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="fer.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="fer.0ajxo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Objekto el <tld/>o:
      <ekz>
        malnovaj <tld/>a&jcirc;oj;
      </ekz>
      <ekz>
        <ind>huf<tld/>o</ind>.
        <trd lng="br">houarn marc'h</trd>
        <trd lng="es">herradura</trd>
        <trd lng="fr"><ind>fer</ind>-&agrave;-cheval</trd>
        <trd lng="hu">patk&oacute;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_l;&c_ja;&c_z;&c_ja;&c_k;&c_a;</trd>
  <trdgrp lng="br">
    <trd>houarn<klr tip="ind"> (traezenn)</klr></trd>,
    <trd>houarnaj</trd>
  </trdgrp>
  <trd lng="de">Eisen</trd>
  <trdgrp lng="es">
    <trd>forja</trd>,
    <trd>herraje</trd>
  </trdgrp>
  <trd lng="fr">fer<klr tip="ind"> (objet en fer)</klr></trd>
  <trd lng="hu">vas <klr tip="ind">(t&aacute;rgy)</klr></trd>
  <trd lng="nl">ijzerwerk</trd>
  <trd lng="pl">element <ind>&zdot;elazny</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_je;&c_l;&c_je;&c_z;&c_k;&c_a;</trd>,
    <trd>&c_zh;&c_je;&c_l;&c_je;&c_z;&c_ja;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="fer.0ajxisto">
  <kap><tld/>a&jcirc;isto</kap>
  <snc mrk="fer.0ajxisto.KEM">
    <dif>
      Metiisto, kiu faras a&ubreve; vendas <tld/>ajn objektojn.
    </dif>
  </snc>
  <trd lng="br">houarnajer</trd>
  <trd lng="es">chatarrero</trd>
  <trd lng="fr">ferrailleur</trd>
  <trdgrp lng="hu">
    <trd>vaskeresked&odblac;</trd>,
    <trd>kov&aacute;cs</trd>
  </trdgrp>
  <trd lng="nl">ijzerhandelaar</trd>
</drv>

<drv mrk="fer.0lado">
  <kap><tld/>lado</kap>
  <snc>
    <dif>
      <tld lit="F"/>a plato.
    </dif>
    <trd lng="hu">vaslemez</trd>
  </snc>
  <snc>
    <dif>
      Stanita <tld/>a plato.
    </dif>
    <trd lng="hu">b&aacute;dog</trd>
  </snc>
  <trd lng="be">&c_b;&c_l;&c_ja;&c_h;&c_a;</trd>
  <trd lng="br">tolenn houarn</trd>
  <trd lng="cs">&zcaron;elezn&yacute; plech</trd>
  <trd lng="de">Eisenblech</trd>
  <trd lng="es">chapa</trd>
  <trd lng="fr">t&ocirc;le<klr tip="ind"> (de fer, de fer-blanc)</klr></trd>
  <trd lng="pl">blacha</trd>
  <trd lng="ru">&c_zh;&c_je;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">&zcaron;elezn&yacute; plech</trd>
</drv>

<drv mrk="fer.0rubo">
  <kap><tld/>rubo</kap>
  <snc>
    <dif>
      <ref tip="super" cel="rub.0o.forjxetajxoj">Rubo</ref>
      konsistanta precipe el <ref tip="prt" cel="fer.0ajxo">fera&jcirc;oj</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_t;&c_a;&c_l;&c_a;&c_l;&c_o;&c_m;</trd>
  <trd lng="br">kozh-houarn</trd>
  <trdgrp lng="cs">
    <trd>star&eacute; &zcaron;elezo</trd>,
    <trd>&zcaron;elezn&yacute; &scaron;rot</trd>
  </trdgrp>
  <trd lng="de">Schrott</trd>
  <trd lng="es">chatarra</trd>
  <trd lng="fr">ferraille</trd>
  <trd lng="hu">vashullad&eacute;k</trd>
  <trd lng="nl">schroot</trd>
  <trd lng="pl">z&lstroke;om<klr> &zdot;elazny</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_je;&c_l;&c_je;&c_z;&c_n;&c_y;&c_j; &c_l;&c_o;&c_m;</trd>,
    <trd>&c_m;&c_je;&c_t;&c_a;&c_l;&c_l;&c_o;&c_l;&c_o;&c_m;</trd>
  </trdgrp>
  <trd lng="sk">&zcaron;elezn&yacute; &scaron;rot</trd>
  <trd lng="sv">j&auml;rnskrot</trd>
</drv>
</art>
