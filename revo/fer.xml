<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fer</rad>/o
</kap>
<drv mrk="fer.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 26, atompezo 55,847, simbolo Fe. Malmola for&gcirc;ebla <ref
      tip="super" cel="metal.0o">metalo</ref>, tre uzata en multaj industrioj:
      <ekz>
        for&gcirc;u <tld/>on dum &gcirc;i estas varmega
        <klr>(ne ellasu la okazon)</klr><fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">TEK</uzo>
        <ind>stanita <tld/>o</ind>.
        <trdgrp lng="fr">
          <trd>fer &eacute;tam&eacute;</trd>,
          <trd>fer-blanc</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_l;&c_je;&c_z;&c_a;</trd>
  <trd lng="cs">&zcaron;elezo</trd>
  <trd lng="de">Eisen</trd>
  <trd lng="hu">vas</trd>
  <trd lng="nl">ijzer</trd>
  <trd lng="fr">fer<klr tip="ind"> (m&eacute;tal)</klr></trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_je;&c_z;&c_o;</trd>
  <trd lng="vo">lel</trd>
</drv>

<drv mrk="fer.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      De <tld/>o, el <tld/>o.
    </dif>
    <trdgrp lng="hu">
      <trd>vas-</trd>,
      <trd>vasb&oacute;l val&oacute;</trd>
    </trdgrp>
    <trd lng="nl">ijzer-</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senceda, severa, neindulga:
      <ekz>
        <tld/>a volo, konstanteco;
      </ekz>
      <ekz>
        teni iun per <tld/>a mano
        (<ref tip="sin"
        cel="buton.0umi">butonumi</ref>)<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>a disciplino, le&gcirc;o, realo;
      </ekz>
      <ekz>
        <tld/>a <klr>(nelacigebla)</klr> pacienco.
      </ekz>
    </dif>
    <trd lng="hu">vas-</trd>
    <trd lng="nl">ijzersterk</trd>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_l;&c_je;&c_z;&c_n;&c_y;</trd>
  <trd lng="de">eisern</trd>
  <trd lng="en">iron</trd>
  <trdgrp lng="fr">
    <trd>de <ind>fer</ind></trd>,
    <trd>&eacute;nergique</trd>,
    <trd>implacable</trd>,
    <trd>strict</trd>,
    <trd>vigoureux</trd>
  </trdgrp>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_je;&c_z;&c_n;&c_y;&c_j;</trd>
  <trd lng="vo">lelik</trd>
</drv>

<drv mrk="fer.0eca0hava">
  <kap><tld/>eca, <tld/>hava</kap>
  <snc>
    <dif>
      Enhavanta <tld/>ajn substancojn:
      <ekz>
        <tld/>eca tero, akvo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_l;&c_je;&c_z;&c_ib;&c_s;&c_t;&c_y;</trd>
  <trd lng="de">eisenhaltig</trd>
  <trdgrp lng="fr">
    <trd>ferreux</trd>,
    <trd>ferrugineux</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vasas</trd>,
    <trd>vastartalm&uacute;</trd>
  </trdgrp>
  <trd lng="nl">ijzerhoudend</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_je;&c_z;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>
</drv>

<drv mrk="fer.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Objekto el <tld/>o:
      <ekz>
        malnovaj <tld/>a&jcirc;oj;
      </ekz>
      <ekz>
        <ind>huf<tld/>o</ind>.
        <trd lng="fr">fer-&agrave;-cheval</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_l;&c_ja;&c_z;&c_ja;&c_k;&c_a;</trd>
  <trd lng="de">Eisen</trd>
  <trd lng="fr">fer<klr tip="ind"> (objet en fer)</klr></trd>
  <trd lng="nl">ijzerwerk</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_je;&c_l;&c_je;&c_z;&c_k;&c_a;</trd>,
    <trd>&c_zh;&c_je;&c_l;&c_je;&c_z;&c_ja;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="fer.0ajxisto">
  <kap><tld/>a&jcirc;isto</kap>
  <snc>
    <dif>
      Metiisto, kiu faras a&ubreve; vendas <tld/>ajn objektojn.
    </dif>
  </snc>
  <trd lng="nl">ijzerhandelaar</trd>
  <trd lng="fr">ferrailleur</trd>
</drv>

<drv mrk="fer.0lado">
  <kap><tld/>lado</kap>
  <snc>
    <dif>
      <tld lit="F"/>a plato.
    </dif>
  </snc>
  <snc>
    <dif>
      Stanita <tld/>a plato.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_l;&c_ja;&c_h;&c_a;</trd>
  <trd lng="fr">t&ocirc;le<klr tip="ind"> (de fer, de fer-blanc)</klr></trd>
  <trd lng="ru">&c_zh;&c_je;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="fer.0rubo">
  <kap><tld/>rubo</kap>
  <snc>
    <dif>
      <ref tip="super" cel="rub.0o.forjxetajxoj">Rubo</ref>
      konsistanta precipe el <ref tip="prt" cel="fer.0ajxo">fera&jcirc;oj</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_t;&c_a;&c_l;&c_a;&c_l;&c_o;&c_m;</trd>
  <trd lng="de">Schrott</trd>
  <trd lng="fr">ferraille</trd>
  <trd lng="hu">vashullad&eacute;k</trd>
  <trd lng="nl">schroot</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_je;&c_l;&c_je;&c_z;&c_n;&c_y;&c_j; &c_l;&c_o;&c_m;</trd>,
    <trd>&c_m;&c_je;&c_t;&c_a;&c_l;&c_l;&c_o;&c_l;&c_o;&c_m;</trd>
  </trdgrp>
</drv>

<drv mrk="fer.0vojo">
  <kap><ofc>*</ofc><tld/>vojo</kap>
  <uzo tip="fak">FER</uzo>
  <snc mrk="fer.0vojo.relvojo">
    <dif>
      <ref tip="prt" cel="rel.0o">Rel</ref>vojo por vagonoj, tramveturiloj:
      <ekz>
        unu tagon promenante apud la reloj de <tld/>vojo 
        li falis sub la radojn de veturanta vagonaro 
        <fnt><bib>F</bib>, <lok>Ekzercaro, &para; 39</lok></fnt>;
      </ekz>
      <ekz>
        la gladilo <klr>[...]</klr> imagis al si ke &gcirc;i estas 
        vaporma&scirc;ina kaldrono, kiu devas iri sur la <tld/>vojon por 
        tiri vagonojn
        <fnt><bib>Fab3</bib>, <lok>Historio de unu patrino</lok></fnt>; 
      </ekz>
      <ekz>
        jen tie malsupre ku&scirc;as Germanujo kiu iam estis kovrita de 
        plej densa reto de <tld/>vojoj kaj kanaloj
        <fnt><bib>Fab3</bib>, <lok>Historio de unu patrino</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Gleis</trd>,
      <trd>Bahnlinie</trd>.
    </trdgrp>
    <trd lng="fr">voie ferr&eacute;e</trd>
    <trd lng="nl">spoor</trd>
  </snc>
  <snc mrk="fer.0vojo.transportilo">
    <dif>
      Sistemo por transporti homojn kaj varojn per relvojo kaj
      <ref tip="prt" cel="vagon.0aro">vagonaroj</ref>:
      <ekz>
        la <ind><tld/>voja</ind>j kompanioj;
        <trd lng="fr">ferroviaire</trd>
        <trd lng="nl">spoor-</trd>
      </ekz>
      <ekz>
        estas <klr>[...]</klr> malsa&gcirc;a&jcirc;o <klr>[...]</klr>sendi 
        ion el Parizo Peterburgon per &ccirc;evaloj, kiam ni havas la eblon 
        fari tion &ccirc;i per <tld/>vojo
        <fnt><bib>EE</bib>, <lok>&ccirc;ap. 5</lok></fnt>;
      </ekz>
      <ekz>
        tage kaj nokte &gcirc;i kuris, &gcirc;i uzis paka&jcirc;vagonaron 
        de <tld/>vojo kaj tamen &gcirc;i preska&ubreve; malfrui&gcirc;is
        <fnt><bib>Fab3</bib>, <lok>Historio de unu patrino</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>chemin de fer</trd>,
      <trd>train<klr tip="ind"> (mode de transport)</klr></trd>
    </trdgrp>
    <trd lng="de">Eisenbahn</trd>
    <trd lng="nl">spoorweg</trd>
  </snc>
  <snc mrk="fer.0vojo.organizajxo">
    <dif>
      La tuta organiza&jcirc;o konstruiganta kaj administranta la
      transportsistemon per vagonaroj sur relvojoj en iu lando:
      <ekz>
        la svisa <tld/>vojo.
      </ekz>
    </dif>
    <trd lng="de">Eisenbahngesellschaft</trd>
  </snc>
  <trd lng="be">&c_ch;&c_y;&c_g;&c_u;&c_n;&c_k;&c_a;</trd>
  <trd lng="hu">vas&uacute;t</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_je;&c_z;&c_n;&c_a;&c_ja;
  &c_d;&c_o;&c_r;&c_o;&c_g;&c_a;</trd>
</drv>
</art>
