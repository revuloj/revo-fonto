<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bulten.xml,v 1.22 2024/01/23 09:23:02 revo Exp $">
<kap>
  <ofc>5</ofc>
  <rad>bulten</rad>/o
  <fnt><bib>PVEF</bib></fnt>
</kap>

<drv mrk="bulten.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TIP</uzo>
  <snc mrk="bulten.0o.informilo">
    <dif>
      Perioda, pli a&ubreve; malpli regula, <ref tip="super" cel="inform.0ilo">informilo</ref> de societo
      a&ubreve; publika institucio:
      <ekz>
        la oficiala ekzamenregulo estis publikigita en la <tld lit="B"/>o de la
        Instruministerio
        <fnt>
          <bib>EeP</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        televidaj nova&jcirc;-<tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2011/010471.php"
              >Koruptado demisiigas prezidanton</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        multaj naciaj naturistaj asocioj eldonas membro-<tld/>ojn kaj perioda&jcirc;ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Leif Heilberg</aut>
          <vrk><url ref="&Monato;/2005/008615.php"
              >Menso sana en korpo nuda</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        legi la &ccirc;iutagajn meteorologiajn <tld/>ojn
        <fnt>
          <bib>SatirRak</bib>
          <lok>La fluidumo de la &ccirc;iovido</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o <ctl>Esperanto en Azio</ctl>
        <fnt><aut>Julian
        Modest</aut>: <vrk>Libroteko Tokio esee jubileas (Japana
        esearo)</vrk>, <bib>Monato</bib><lok>jaro 1995a, numero 6a, p.
        26a</lok></fnt>;
      </ekz>
      <ekz>
        la&ubreve;necese la Akademio havas la rajton eldoni propran <tld/>on: Oficiala <tld
        lit="B"/>o
        <fnt>
          <bib>EeP</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi metis anoncon en la notaria ser&ccirc;a <tld/>o
        <fnt>
          <bib>ChV</bib>
          <lok>23</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cirkul1.0o">cirkulero</ref>,
      <ref cel="jxurna.0o.INF">&jcirc;urnalo<sncref/></ref>,
      <ref cel="period.0ajxo">perioda&jcirc;o</ref>,
      <ref cel="raport.0o.dokumenta">raporto<sncref/></ref>,
      <ref cel="revu.0o.INF_gazeto">revuo<sncref/></ref>,
      <ref cel="kronik1.0o.TIP">kroniko<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ju;&c_l;&c_je;&c_t;&c_e;&c_n;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_v;&c_je;&c_s;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_b;&c_ju;&c_l;&c_je;&c_t;&c_i;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>bulletin</trd>,
    <trd>list</trd>,
    <trd>v&ecaron;stn&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bulletin</trd>,
    <trd>Bericht</trd>
  </trdgrp>
  <trd lng="en">bulletin</trd>
  <trd lng="es">bolet&iacute;n</trd>
  <trd lng="fr">bulletin</trd>
  <trd lng="he">&ayin;&#1464;&lamed;&vav;&#1465;&fnun;</trd>
  <trdgrp lng="hu">
    <trd>&eacute;rtes&iacute;t&odblac;</trd>,
    <trd>k&ouml;zl&ouml;ny</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#20250;&#22577;  <pr>&#12363;&#12356;&#12411;&#12358;</pr></trd>,
    <trd>&#32000;&#35201;  <pr>&#12365;&#12424;&#12358;</pr></trd>,
    <trd>&#22577;&#21578;&#26360;  <pr>&#12411;&#12358;&#12371;&#12367;&#12375;&#12423;</pr></trd>
  </trdgrp>
  <trd lng="pl">biuletyn</trd>
  <trd lng="pt">boletim</trd>
  <trd lng="ru">&c_b;&c_ju;&c_l;&c_l;&c_je;&c_t;&c_je;&c_n;&c_mol;
  <klr>(&c_p;&c_je;&c_r;&c_i;&c_o;&c_d;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je;
  &c_i;&c_z;&c_d;&c_a;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">vestn&iacute;k</trd>
  <trd lng="uk">&c_b;&c_ju;&c_l;&c_je;&c_t;&c_je;&c_n;&c_mol;</trd>
</drv>
</art>
