<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ezoked.xml,v 1.11 2022/03/17 13:47:19 revo Exp $">
<kap>
  <rad>ezoked</rad>/oj
</kap>
<drv mrk="ezoked.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="ezok.0oformaj">ezokoformaj fi&scirc;oj</ref>
      (<trd lng="la">Esocidae</trd>):
      <ekz>
        la <ref tip="prt" cel="ezok.0o">ezoko</ref>
        estas fi&scirc;o el la <tld/>a familio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=102742">Ezoko</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_ch;&c_u;&c_p;&c_a;&c_k;&c_ib; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">es&ograve;cids</trd>
  <trd lng="de">Hechte <klr tip="ind">(Familie)</klr></trd>
  <trd lng="fr">&eacute;socid&eacute;s</trd>
  <trd lng="hu">csukaf&eacute;l&eacute;k</trd>
  <trd lng="pt">esoc&iacute;deos</trd>
  <trdgrp lng="ru">
    <trd>&c_shch;&c_u;&c_k;&c_o;&c_v;&c_y;&c_je;</trd>,
    <trd>&c_shch;&c_u;&c_k;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="uk">&#x449;&#x443;&#x43A;&#x43E;&#x432;&#x456;</trd>
</drv>
</art>
