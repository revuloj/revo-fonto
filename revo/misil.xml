<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: misil.xml,v 1.9 2024/01/26 17:40:06 afido Exp $">
    <kap>
      <rad>misil</rad>/o
    </kap>

<drv mrk="misil.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="paf.0ajxo">Pafa&jcirc;o</ref> radiofonie a&ubreve; memstire 
      navigata al sia celo:
      <ekz>
        <klr>[ili]</klr> vidis <tld/>on alteri&gcirc;i dek pa&scirc;ojn anta&ubreve; ili kaj mortigi patron
        <fnt>
          <aut>Hamza Baksi&cacute;</aut>
          <vrk><url ref="&Monato;/1993/A931001.php">La fino de la mondo komenci&gcirc;as en
          Sarajevo</url></vrk>
          <bib>Monato</bib>
          <lok>1993/10, p. 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        la soldatoj de NATO restis spektantoj de terura masakrado fare de la usonaj
        <tld/>oj
        <fnt>
       <aut>Gilbert Ledon</aut>
          <vrk><url ref="&Monato;/2000/006861.php">Milito kaj paco</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 5</lok>
        </fnt>;
      </ekz>
      <ekz>
        tie trovi&gcirc;is subtera militbazo, en kiu staris kvar nukleaj <tld/>oj, direktitaj
        al okcidenta E&ubreve;ropo
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2013/010882.php">Muzeo pri la malvarma milito</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni alportis amason da komplikaj aparatoj, radio- kaj elektro-stacioj kaj
        aran&gcirc;is manipulcentron de <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2013/010882.php">Muzeo pri la malvarma milito</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu <ind>kroz<tld/>o</ind> kostas 800&#x202F;000 e&ubreve;rojn
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2003/008109.html">Taglibro de l' milito ... el la lando de
          &Scirc;ekspiro (3)</url></vrk>
          <lok>2003</lok>
        </fnt>.
        <trd lng="de">Marschflugk&ouml;rper</trd>
        <trd lng="en">cruise missile</trd>
        <trd lng="fr">missile de croisi&egrave;re</trd>
      </ekz>
    </dif>
    <rim>
      Nestiratan, do balistike flugantan, mempelatan pafa&jcirc;on oni nomas <ref cel="raket.0o.AVI">raketo</ref>, 
      faligatan <ref cel="bomb.0o.falanta">bombo</ref>.
    </rim>
    <trd lng="be">&c_r;&c_a;&c_k;&c_je;&c_t;&c_a; <klr>(&c_z;&c_b;&c_r;&c_o;&c_j;&c_n;&c_a;&c_ja;)</klr></trd>
    <trd lng="de">Lenkflugk&ouml;rper</trd>
    <trd lng="en">missile</trd>
    <trd lng="fr">missile</trd>
  </snc>
  <trd lng="ja">&#x30DF;&#x30B5;&#x30A4;&#x30EB;  <pr>&#x307F;&#x3055;&#x3044;&#x308B;</pr></trd>
  <trd lng="uk">&c_b;&c_a;&#x43B;&c_ib;&c_s;&c_t;&c_i;&c_ch;&#x43D;&c_a; &c_r;&c_a;&#x43A;&c_je;&c_t;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x5BFC;&#x5F39; <pr>d&#x1CE;od&#xE0;n</pr></trd>,
    <trd>&#x5C0E;&#x5F48; <pr>d&#x1CE;od&#xE0;n</pr></trd>,
    <trd>&#x98DB;&#x5F48; <pr>f&#x113;id&#xE0;n</pr></trd>,
    <trd>&#x98DE;&#x5F39; <pr>f&#x113;id&#xE0;n</pr></trd>
  </trdgrp>
</drv>

  </art>

