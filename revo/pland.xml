<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pland</rad>/o
</kap>
<drv mrk="pland.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pland.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Suba plata parto de la <ref tip="malprt"
      cel="pied.0o.ANA">piedo</ref>, kiu tu&scirc;as la teron,
      kiam oni pa&scirc;as a&ubreve; staras:
      <ekz>
        Dio metis liajn malamikojn sub la <tld/>ojn de liaj
        piedoj<fnt>Z</fnt>
        <klr>(humiligis kaj subigis)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la vojo brulas sub liaj <tld/>oj
        <klr>(li tre deziras alveni balda&ubreve;.
        <ref tip="vid" cel="pingl.0o">pinglo</ref>)</klr>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_d;&c_e;&c_sh;&c_v;&c_a;
<klr>(&c_n;&c_a;&c_g;&c_ib;)</klr></trd>
    <trd lng="en">sole</trd>
    <trd lng="fr">plante du pied</trd>
    <trd lng="hu">talp</trd>
    <trdgrp lng="nl">
      <trd>zool</trd>,
      <trd>voetzool</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_o;&c_d;&c_o;&c_sh;&c_v;&c_a; <klr
tip="amb">(&c_n;&c_o;&c_g;&c_i;)</klr></trd>
  </snc>
  <snc mrk="pland.0o.plandumo">
    <ref tip="dif" cel="pland.0umo">plandumo</ref>
    <ekz>
      <uzo tip="stl">FIG</uzo>
      meti ion sub la <tld/>on de siaj &scirc;uoj
      <klr>(tute mal&scirc;ati ion)</klr>.
    </ekz>
  </snc>
</drv>

<drv mrk="pland.0umo">
  <kap><tld/>umo</kap>
  <snc mrk="pland.0umo.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Suba plata parto de <ref tip="malprt" cel="sxu.0o">&scirc;uo</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_e;&c_sh;&c_v;&c_a;
<klr>(&c_a;&c_b;&c_u;&c_t;&c_k;&c_u;)</klr></trd>,
    <trd>&c_p;&c_a;&c_d;&c_n;&c_o;&c_s;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="en">sole</trd>
  <trd lng="fr">semelle</trd>
  <trd lng="hu">cip&odblac;talp</trd>
  <trdgrp lng="nl">
    <trd>zool</trd>,
    <trd>schoenzool</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_o;&c_sh;&c_v;&c_a; <klr
tip="amb">(&c_o;&c_b;&c_u;&c_v;&c_i;)</klr></trd>,
    <trd>&c_p;&c_o;&c_d;&c_m;&c_jo;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="pland.re0umi">
  <kap>re<tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pland.re0umi.TEKS">
    <dif>
      &Scirc;an&gcirc;i foruzitan <tld/>umon de &scirc;uo al pli nova:
      <ekz>
        re<tld/>umi botojn.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">ressemeler</trd>
  <trd lng="hu">megsarkal</trd>
  <trd lng="nl">verzolen</trd>
</drv>
</art>
