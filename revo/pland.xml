<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pland.xml,v 1.38 2024/02/17 07:52:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pland</rad>/o
</kap>

<drv mrk="pland.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pland.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Suba plata parto de la <ref tip="malprt" cel="pied.0o.ANA">piedo</ref>, kiu tu&scirc;as la teron,
      kiam oni pa&scirc;as a&ubreve; staras:
      <ekz>
        bestoj kvarpiedaj <klr>[&#x2026;]</klr>, kiuj iras sur siaj <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 11:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>de la <tld/>o de via piedo &gcirc;is via verto</ind>
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 28:35</lok>
        </fnt>;
        <trd lng="de">vom SCheitel bis zur Sohle</trd>
      </ekz>
      <ekz>
        mi ne donos al vi el ilia lando e&ccirc; lar&gcirc;econ de <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 2:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la Eternulo metis ilin sub la <tld/>ojn de liaj piedoj
        <klr>(humiligis kaj subigis)</klr>
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 5:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la vojo brulas sub liaj <tld/>oj
        <klr>(li tre deziras alveni balda&ubreve;,
        <ref tip="vid" cel="pingl.0o">pinglo</ref>)</klr>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_d;&c_e;&c_sh;&c_v;&c_a;
    <klr>(&c_n;&c_a;&c_g;&c_ib;)</klr></trd>
    <trd lng="de">Fu&szlig;sohle</trd>
    <trd lng="en">sole</trd>
    <trd lng="es">planta del pie</trd>
    <trd lng="fr">plante du pied</trd>
    <trd lng="hu">talp</trd>
    <trd lng="id"><ind>telapak</ind> kaki</trd>
    <trdgrp lng="nl">
      <trd>zool<klr tip="ind"> (v.voet)</klr></trd>,
      <trd>voetzool</trd>
    </trdgrp>
    <trd lng="it"><ind>pianta</ind> del piede</trd>
    <trd lng="pl">podeszwa <klr>(stopy)</klr></trd>
    <trd lng="pt">planta do <klr tip="amb">p&eacute;</klr></trd>
    <trd lng="ru">&c_p;&c_o;&c_d;&c_o;&c_sh;&c_v;&c_a; <klr tip="amb">(&c_n;&c_o;&c_g;&c_i;)</klr></trd>
    <trd lng="sv">fotsula</trd>
    <trdgrp lng="zh">
    <trd>&#x8E60; <pr>zh&#xED;</pr></trd>,
    <trd>&#x811A;&#x5E95; <pr>ji&#x1CE;od&#x1D0;</pr></trd>,
    <trd>&#x8173;&#x5E95; <pr>ji&#x1CE;od&#x1D0;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="pland.0o.plandumo">
    <dif>
      <ref tip="dif" cel="pland.0umo">Plandumo</ref>:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        meti ion sub la <tld/>on de siaj &scirc;uoj
        <klr>(tute mal&scirc;ati ion)</klr>;
      </ekz>
      <ekz>
        perdu vi la &scirc;u<tld/>ojn kaj kuru nudpiedaj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino kaj Rozulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        feli&ccirc;e libera, se e&ccirc; sen <tld/>oj &ccirc;e la &scirc;uoj kaj kun flikita &ccirc;emizo
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>.
      </ekz>
    </dif>
      <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_e;&c_sh;&c_v;&c_a; <klr>(&c_a;&c_b;&c_u;&c_t;&c_k;&c_u;)</klr></trd>,
    <trd>&c_p;&c_a;&c_d;&c_n;&c_o;&c_s;&c_a;&c_k;</trd>
  </trdgrp>
    <trd lng="id">sol</trd>
    <trd lng="it">suola</trd>
    <trd lng="nl">zool<klr tip="ind"> (v.schoen)</klr></trd>
    <trdgrp lng="pl">
      <trd>zel&oacute;wka</trd>,
      <trd>podeszwa <klr>buta</klr></trd>
    </trdgrp>
    <trd lng="sv">sula</trd>
  </snc>
  <trd lng="cs">chodidlo</trd>
  <trd lng="ja">&#x8DB3;&#x306E;&#x88CF;  <pr>&#x3042;&#x3057;&#x306E;&#x3046;&#x3089;</pr></trd>
  <trd lng="nl">zool</trd>
  <trdgrp lng="sk">
    <trd>chodidlo</trd>,
    <trd>podo&scaron;va</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_ib;&c_d;&#x43E;&c_sh;&c_v;&c_a; &#x43D;&#x43E;&c_g;&c_i;</trd>,
    <trd>&#x43F;&c_ib;&c_d;&#x43E;&c_sh;&c_v;&c_a; (&c_v;&c_z;&c_u;&c_t;&c_t;&#x44F;</trd>,
    <trd>&c_s;&#x43F;&c_ib;&c_d;&#x43D;&#x44F; &c_ch;&c_a;&c_s;&c_t;&c_i;&#x43D;&c_a; &c_d;&c_je;&c_t;&c_a;&#x43B;&c_ib;</trd>,
    <trd>&c_s;&#x43F;&#x43E;&c_r;&c_u;&c_d;&c_i;)</trd>,
    <trd>&#x43F;&c_ib;&c_d;&#x43C;&c_je;&c_t;&#x43A;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="pland.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="pland.0.SON">
    <dif>
      <ref tip="lst" cel="son.0o" lst="voko:sonoj">Sono</ref>
      de <tld/>oj irantaj sur grundo:
      <ekz>
        nokte ili rampas eksteren &dash; <tld/> &dash; <tld/>, <tld/> &dash; <tld/>
        ...
        <fnt><bib>Salamandroj</bib>, <lok>libro 1a, &ccirc;apitro
        5a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">schlurf</trd>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_p;-&c_t;&c_u;&c_p;</trd>
  <trdgrp lng="en">
    <trd>shuffle<klr> (if not lifting feet)</klr></trd>,
    <trd>plod<klr> (if moving slowly and heavily)</klr></trd>
  </trdgrp>
  <trd lng="fr">pat-pat<klr> (bruit de pas)</klr></trd>
  <trd lng="it">tap-tap<klr> (rumore di passi)</klr></trd>
  <trdgrp lng="pl">
    <trd>tup-tup</trd>,
    <trd>cz&lstroke;ap-cz&lstroke;ap</trd>
  </trdgrp>
</drv>

<drv mrk="pland.0i">
  <kap><tld/>i, <var><kap><tld/>umi</kap></var></kap>
  <snc mrk="pland.0i.KOMUNE">
    <dif>
      <ref tip="sin" cel="pasx.0i">Pa&scirc;i</ref>, premante la
      <tld/>ojn al la grundo:
      <ekz>
        kiel eble plej rapide li <tld/>is la&ubreve; la vojo supren
        <fnt>
        <aut>J. R. R. Tolkien, trad. W. Auld</aut>,
        <vrk>La Mastro de l' Ringoj, 3</vrk>,
        <lok>1997</lok>
        </fnt>;
      </ekz>
      <ekz>
        nun kion fari? &dash; <tld/>umis <nom>Elder </nom>malpacience
        <fnt>
          <vrk><url ref="https://nemaloknig.com/read-229264/?page=5">Sub kvaranteno la
          Grand-Hotelo...</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">schlurfen</trd>
    <trdgrp lng="zh">
    <trd>&#x62D6;&#x7740;&#x6B65;&#x8D70; <pr>tu&#x14D;zhu&#xF3;b&#xF9;z&#x1D2;u</pr></trd>,
    <trd>&#x62D6;&#x8457;&#x6B65;&#x8D70; <pr>tu&#x14D;zhu&#xF3;b&#xF9;z&#x1D2;u</pr></trd>,
    <trd>&#x66F3;&#x6B65; <pr>y&#xEC;b&#xF9;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_u;&c_p;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_t;&c_u;&c_p;&c_a;&c_c;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>plaho&ccaron;it se</trd>,
    <trd>vl&eacute;ci se</trd>,
    <trd>&scaron;ourat se</trd>
  </trdgrp>
  <trd lng="nl">schuifelen</trd>
  <trdgrp lng="pl">
    <trd>tupa&cacute;</trd>,
    <trd>cz&lstroke;apa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>plaho&ccaron;i&tcaron; sa</trd>,
    <trd>vliec&tcaron; sa</trd>
  </trdgrp>
</drv>

<drv mrk="pland.0umo">
  <kap><tld/>umo</kap>
  <snc mrk="pland.0umo.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Suba plata parto de <ref tip="malprt" cel="sxu.0o">&scirc;uo</ref>:
      <ekz>
        &scirc;uoj kun gumaj <tld/>umoj
        <fnt>
          <bib>Monato</bib>
          <aut>Dimitar Ha&gcirc;iev</aut>
          <vrk><url ref="&Monato;/2007/009286.php">La tri turoj de MKC (6)</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ser&ccirc;is &scirc;uiston, kiu povus altigi la <tld/>umojn
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2006/008678.php">Esti&gcirc;i la plej alta</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiamaniere forigi ma&ccirc;gumon de sur <tld/>umo
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2011/010337.php">Konsiloj avinaj kaj neavinaj</url></vrk>
          <lok>2011</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_e;&c_sh;&c_v;&c_a; <klr>(&c_a;&c_b;&c_u;&c_t;&c_k;&c_u;)</klr></trd>,
    <trd>&c_p;&c_a;&c_d;&c_n;&c_o;&c_s;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="cs">podr&aacute;&zcaron;ka</trd>
  <trd lng="de">Sohle</trd>
  <trd lng="en">sole</trd>
  <trd lng="fr">semelle</trd>
  <trd lng="hu">cip&odblac;talp</trd>
  <trd lng="id">sol</trd>
  <trd lng="it">suola</trd>
  <trd lng="ja">&#x9774;&#x5E95;  <pr>&#x304F;&#x3064;&#x305E;&#x3053;</pr></trd>
  <trdgrp lng="nl">
    <trd>zool<klr tip="ind"> (v.schoen)</klr></trd>,
    <trd>schoenzool</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zel&oacute;wka</trd>,
    <trd>podeszwa <klr>buta</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_o;&c_sh;&c_v;&c_a; <klr tip="amb">(&c_o;&c_b;&c_u;&c_v;&c_i;)</klr></trd>,
    <trd>&c_p;&c_o;&c_d;&c_m;&c_jo;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">podr&aacute;&zcaron;ka</trd>
  <trdgrp lng="sv">
    <trd>sula</trd>,
    <trd>skosula</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_ib;&c_d;&#x43E;&c_sh;&c_v;&c_a; (&c_v;&c_z;&c_u;&c_t;&c_t;&#x44F;</trd>,
    <trd>&c_s;&#x43F;&c_ib;&c_d;&#x43D;&#x44F; &c_ch;&c_a;&c_s;&c_t;&c_i;&#x43D;&c_a; &c_d;&c_je;&c_t;&c_a;&#x43B;&c_ib;</trd>,
    <trd>&c_s;&#x43F;&#x43E;&c_r;&c_u;&c_d;&c_i;)</trd>,
    <trd>&#x43F;&c_ib;&c_d;&#x43C;&c_je;&c_t;&#x43A;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x978B;&#x5E95; <pr>xi&#xE9;d&#x1D0;</pr></trd>,
    <trd>&#x8DB3;&#x5E95; <pr>z&#xFA;d&#x1D0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pland.re0umi">
  <kap>re<tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pland.re0umi.TEKS">
    <dif>
      &Scirc;an&gcirc;i foruzitan <tld/>umon de &scirc;uo per pli nova:
      <ekz>
        re<tld/>umi botojn.
      </ekz>
    </dif>
    <trd lng="de">neu <ind>besohlen</ind></trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_b;&c_ib;&c_v;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_a;&c_b;&c_u;&c_t;&c_a;&c_k;)</klr></trd>
  <trd lng="en">to resole</trd>
  <trd lng="cs">podrazit <klr>(boty)</klr></trd>
  <trd lng="fr">ressemeler</trd>
  <trd lng="hu">megtalpal</trd>
  <trd lng="it">risuolare</trd>
  <trd lng="ja">&#x5E95;&#x3092;&#x5F35;&#x308A;&#x66FF;&#x3048;&#x308B; 
  <pr>&#x305D;&#x3053;&#x3092;&#x306F;&#x308A;&#x304B;&#x3048;&#x308B;</pr></trd>
  <trd lng="nl">verzolen</trd>
  <trd lng="pl">podzelowa&cacute; </trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_b;&c_i;&c_t;&c_mol;
  &c_n;&c_o;&c_v;&c_y;&c_je;
  &c_p;&c_o;&c_d;&c_m;&c_jo;&c_t;&c_k;&c_i;</trd>
  <trd lng="sk">podrazi&tcaron; <klr>(top&aacute;nku)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_ib;&c_d;&c_b;&c_i;&c_v;&c_a;&c_t;&c_i; &c_v;&c_z;&c_u;&c_t;&c_t;&#x44F;</trd>,
    <trd>&#x43C;&c_ib;&#x43D;&#x44F;&c_t;&c_i; &#x43F;&c_ib;&c_d;&#x43E;&c_sh;&c_v;&c_u;/&#x43F;&c_ib;&c_d;&#x43C;&c_je;&c_t;&#x43A;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7DD4; <pr>sh&#xE0;ng</pr></trd>,
    <trd>&#x7EF1; <pr>sh&#xE0;ng</pr></trd>
  </trdgrp>
</drv>
</art>
