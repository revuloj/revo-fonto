<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pland</rad>/o
</kap>
<drv mrk="pland.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Suba plata parto de la piedo, kiu tu&scirc;as la teron, kiam oni mar&scirc;as a&ubreve;
      staras:
      <ekz>
        Dio metis liajn malamikojn sub la <tld/>ojn de liaj
        piedoj<fnt>Z</fnt>
        <klr>(humiligis kaj subigis)</klr>;
      </ekz>
      <ekz>
        la vojo brulas sub liaj <tld/>oj
        <klr>(li tre deziregas alveni kiel eble plej rapide.
        <ref tip="vid" cel="pingl.0o">pinglo</ref>)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">plante du pied</trd>
  <trdgrp lng="nl">
    <trd>zool</trd>,
    <trd>voetzool</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_o;&c_sh;&c_v;&c_a; <klr tip="amb">(&c_n;&c_o;&c_g;&c_i;)</klr></trd>
</drv>

<drv mrk="pland.0umo">
  <kap><tld/>(um)o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Suba plata parto de <ref tip="malprt" cel="sxu.0o">&scirc;uo</ref>:
      <ekz>
        meti ion sub la <tld/>on de siaj &scirc;uoj
        <klr>(f: tute mal&scirc;ati ion)</klr>;
      </ekz>
      <ekz>
        <ind>re<tld/>umi</ind> botojn.
        <trd lng="nl">verzolen</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">semelle</trd>
  <trdgrp lng="nl">
    <trd>zool</trd>,
    <trd>schoenzool</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_o;&c_sh;&c_v;&c_a; <klr tip="amb">(&c_o;&c_b;&c_u;&c_v;&c_i;)</klr></trd>,
    <trd>&c_p;&c_o;&c_d;&c_m;&c_jo;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
