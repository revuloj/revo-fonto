<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>tavol</rad>/o
</kap>
<drv mrk="tavol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Maldika etendita&jcirc;o da iu substanco sur io alia:
      <ekz>
        dika <tld/>o da fojno, da ne&gcirc;o, da nuboj;
      </ekz>
      <ekz>
        maldika <tld/>o da akvo, da butero;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        glacia <tld/>o da anta&ubreve;ju&gcirc;oj kontra&ubreve;staris al
        ni<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="stern.0ajxo">sterna&jcirc;o</ref>,
      <ref cel="sedime.0o">sedimento</ref>,
      <ref cel="tapisx.0o">tapi&scirc;o</ref>,
      <ref cel="teg.0o">tego</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Parto de la socio konsiderata kompare kun pli superaj kaj pli
      malsuperaj partoj:
      <ekz>
        la diversaj <tld/>oj de la socio;
      </ekz>
      <ekz>
        la registaro subpremas la laborulajn <tld/>ojn.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="klas.0o">klaso</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Schicht</trd>
  <trd lng="fr">couche</trd>
  <trd lng="hu">r&eacute;teg</trd>
  <trd lng="nl">laag</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_o;&c_j;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_s;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="tavol.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Dividi la&ubreve; <tld/>oj.
    </dif>
  </snc>
  <trd lng="hu">r&eacute;tegez</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_s;&c_l;&c_a;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_s;&c_l;&c_o;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="tavol.glob0o">
  <kap>glob<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      Parto de <ref tip="malprt" cel="sfer.0o.solido">sfera solido</ref>,
      situanta inter du paralelaj ebenoj, sekcantaj la sferon.
    </dif>
    <ref tip="sin" cel="disk.sfer0o">sferdisko.</ref>
    <rim>
      Kiam unu el la ebenoj tan&gcirc;as la sferon, oni ricevas
      <ref tip="sub" cel="segmen.0o.sfero">segmenton (de sfero)</ref>.
      Tial tiu figuro en iuj naciaj lingvoj nomi&gcirc;as
      <ctl>dubaza sfera segmento</ctl>.
    </rim>
  </snc>
  <trd lng="de">Kugelschicht</trd>
  <trd lng="en">spherical <ind>layer</ind></trd>
  <trd lng="fr">segment sph&eacute;rique &agrave; deux bases</trd>
  <trd lng="hu">g&ouml;mbr&eacute;teg</trd>
  <trd lng="pl">warstwa kuli</trd>
  <trd lng="ru">&c_sh;&c_a;&c_r;&c_o;&c_v;&c_o;&c_j;
  <ind>&c_s;&c_l;&c_o;&c_j;</ind></trd>
</drv>
</art>
