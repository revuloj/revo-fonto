<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gatj.xml,v 1.7 2023/11/27 09:27:52 revo Exp $">
<kap>
  <rad>gatj</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="gatj.0o">
  <kap><tld/>o</kap>
  <snc mrk="gatj.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Tradicia hungara <ref tip="super" cel="pantal.0o">pantalono</ref>
      plejofte lina, kun ampleksaj krurumoj malsupre lar&gcirc;i&gcirc;antaj
      la&ubreve; jupa aspekto:
      <ekz>
        tataroj <klr>[&#8230;]</klr> /
        la <tld/>ojn vestis, kriis, /
        kaj venis al Rusuj'
        <fnt>
         <aut>A. K. Tolstoj, trad. G. Arosev</aut>,
         <vrk><url ref="&Sezonoj;2013/01/219poemo/">
         Historio de la Rusia Regno ekde Gostomysl &gcirc;is
         Tima&scirc;ev</url></vrk>,
         <lok>2013-01-14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_ch;&c_y; <klr tip="amb">(&c_v;&c_u;&c_g;&c_o;&c_r;&c_s;&c_k;&c_ib;&c_ja; &c_n;&c_a;&c_g;&c_a;&c_v;&c_ib;&c_c;&c_y;)</klr></trd>
  <trd lng="cs">gatya</trd>
  <trd lng="de">Gatja <klr>(ung. Kniebundhose, Unterhose)</klr></trd>
  <trd lng="en">breeches</trd>
  <trd lng="fr">gatya</trd>
  <trd lng="hu">gatya</trd>
  <trd lng="sk">gatya</trd>
  <trdgrp lng="uk">
    <trd>&c_gu;&c_a;&c_ch;&c_ib;&nbsp;(&c_sh;&c_t;&c_a;&c_n;&c_i;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_mol;&c_s;&c_o;&c_n;&c_i;)</trd>
  </trdgrp>
</drv>
</art>

