<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gatj.xml,v 1.3 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>gatj</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="gatj.0o">
  <kap><tld/>o</kap>
  <snc mrk="gatj.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Tradicia hungara pantalono plejofte lina,
      kun ampleksaj krurumoj malsupre lar&gcirc;i&gcirc;antaj
      la&ubreve; jupa aspekto:
      <ekz>
        tataroj <klr>...</klr> /
        la <tld/>ojn vestis, kriis, /
        kaj venis al Rusuj'
        <fnt>
         <aut>A. K. Tolstoj, trad. G. Arosev</aut>,
         <vrk><url ref="http://sezonoj.ru/2013/01/219poemo/">
         Historio de la Rusia Regno ekde Gostomysl &gcirc;is
         Tima&scirc;ev</url></vrk>,
         <lok>2013-01-14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">gatya</trd>
  <trd lng="fr">gatya</trd>
  <trd lng="sk">gatya</trd>
</drv>
</art>

