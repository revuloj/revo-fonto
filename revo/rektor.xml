<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rektor.xml,v 1.26 2025/10/29 08:41:53 revo Exp $">
<kap>
   <ofc>2</ofc>
   <rad>rektor</rad>/o
<fnt><bib>JED</bib></fnt></kap>
<drv mrk="rektor.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">PED</uzo>
   <snc mrk="rektor.0o.universitata">
     <dif>
       <ref tip="super" cel="direkt1.0o">Direktoro</ref> de universitato:
       <ekz>
        <tld/>o de la Vilna Akademio
        <fnt>
          <bib>SrT</bib>
          <lok>Libro VIII.</lok>
        </fnt>;
      </ekz>
       <ekz>
         <nom>Inazo Nitobe</nom> <klr>[&#8230;]</klr> estis japana senatano kaj
         eks<tld/>o
         de la Universitato en Tokio
         <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro 41a, p.
         95a</lok></fnt>;
       </ekz>
       <ekz>
        Johano <nom>Hus</nom> <klr>[estis]</klr> en la jaroj 1409-10 <tld/>o de de la praga
        universitato
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk>Katolikismo: Johano Hus -- &ccirc;u ne plu herezulo?</vrk>
        </fnt>.
      </ekz>
     </dif>
     <trd lng="ca">rector</trd>
     <trd lng="en">chancellor</trd>
     <trd lng="fa">&a_r;&a_y_hamza;&f_y;&a_s; &a_d;&a_A;&a_n;&a_sh;&f_g;&a_A;&a_h;</trd>
     <trd lng="fr">pr&eacute;sident d'universit&eacute;<klr> (France)</klr></trd>
     <trd lng="hu">rektor</trd>
     <trd lng="nl">rector magnificus</trd>
     <trd lng="pt">reitor</trd>
   </snc>
   <snc mrk="rektor.0o.gimnazia">
     <dif>
       Direktoro de gimnazio.
     </dif>
     <trd lng="ca">director <klr>(d'institut)</klr></trd>
     <trd lng="fa">&a_m;&a_d;&f_y;&a_r; &a_d;&a_b;&f_y;&a_r;&a_s;&a_t;&a_A;&a_n;</trd>
     <trd lng="fr">proviseur<klr> (France)</klr></trd>
     <trd lng="nl">rector</trd>
   </snc>
  <trd lng="be">&c_r;&c_e;&c_k;&c_t;&c_a;&c_r;</trd>
  <trd lng="cs">rektor</trd>
  <trd lng="de">Rektor</trd>
  <trd lng="en">rector</trd>
  <trd lng="hu">gimn&aacute;ziumi <ind>igazgat&oacute;</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x5B66;&#38263;  <pr>&#x304C;&#x304F;&#12385;&#12423;&#12358;</pr></trd>,
    <trd>&#x7DCF;&#38263;  <pr>&#x305D;&#12358;&#12385;&#12423;&#12358;</pr></trd>,
    <trd>&#x5B66;&#22290;&#38263;  <pr>&#x304C;&#x304F;&#12360;&#12435;&#12385;&#12423;&#12358;</pr></trd>,
    <trd>&#26657;&#38263;  <pr>&#12371;&#12358;&#12385;&#12423;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="pl">rektor</trd>
  <trd lng="ro">rector</trd>
  <trd lng="ru">&c_r;&c_je;&c_k;&c_t;&c_o;&c_r;</trd>
  <trd lng="sk">rektor</trd>
  <trd lng="sv">rektor</trd>
  <trd lng="uk">&c_r;&c_je;&c_k;&c_t;&c_o;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#25945;&#x533A;&#x957F; <pr>ji&agrave;oq&umacron;ch&aacute;ng</pr></trd>,
    <trd>&#25945;&#21312;&#38263; <pr>ji&agrave;oq&umacron;ch&aacute;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rektor.0ejo">
  <kap>
    <tld/>ejo
  </kap>
  <snc>
    <dif>
      Oficejo de <tld/>o:
      <ekz>
        la adreso de la <tld/>ejo
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 2 (64)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_k;&c_t;&c_a;&c_r;&c_a;&c_t;</trd>
  <trd lng="de">Rektorat</trd>
</drv>

</art>
