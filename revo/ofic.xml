<drv mrk="ofic.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ofic.0i.POL">
    <dif>
      Plenumi <tld/>on:
      <ekz>
        informi&gcirc;i pri iu en tia domo estis treege malfacile, precipe, 
        se &ccirc;e la pordego ne <tld/>is pordisto
        <fnt>
          <aut>H. Sienkiewicz, trad Lidia Zamenhof</aut>
          <vrk>Quo Vadis?</vrk>
          <lok>1933</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la vilhelma kastelo <klr>[&#x2026;]</klr> kiel gvidanto <tld/>as maljuna imperiestra
        pordisto
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni malpermesis al vi <tld/>i, esti membro de l&#x2019; partio
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; ol &ccirc;e la nuna notario mi <tld/>is &ccirc;e advokato
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        sur <klr>[la &scirc;ipo]</klr> <tld/>as preska&ubreve; nur finnoj
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
          <lok>En Peterburgo</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la Festo de Britio <tld/>is nombro da interpretistoj anka&ubreve; por Esperanto
        <fnt>
          <bib>EeP</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_j;&c_m;&c_a;&c_c;&c_mol; &c_p;&c_a;&c_s;&c_a;&c_d;&c_u;</trd>,
    <trd>&c_s;&c_l;&c_u;&c_zh;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>&uacute;&rcaron;adovat</trd>,
    <trd>&uacute;&rcaron;adovat</trd>
  </trdgrp>
  <trd lng="de">ein <ind>Amt</ind> bekleiden</trd>
  <trd lng="en">hold office</trd>
  <trd lng="es">oficiar</trd>
  <trdgrp lng="fr">
    <trd>remplir son <ind>office</ind></trd>,
    <trd>travailler</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hivatalban van</trd>,
    <trd>hivatalt bet&ouml;lt</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x57F7;&#x52D9;&#x3059;&#x308B;  <pr>&#x3057;&#x3064;&#x3080;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x4E8B;&#x52D9;&#x3092;&#x57F7;&#x308B;  <pr>&#x3058;&#x3080;&#x3092;&#x3068;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>een <ind>ambt</ind> vervullen</trd>,
    <trd>een <ind>betrekking</ind> vervullen</trd>
  </trdgrp>
  <trd lng="pl">urz&eogonek;dowa&cacute;</trd>
  <trd lng="ro">functiona</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_u;&c_zh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;
    &c_d;&c_o;&c_l;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">&uacute;radova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43B;&c_u;&c_zh;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_j;&#x43C;&c_a;&c_t;&c_i; &#x43F;&#x43E;&c_s;&c_a;&c_d;&c_u;</trd>,
    <trd>&c_b;&c_u;&c_t;&c_i; &#x43D;&c_a; &#x43F;&#x43E;&c_s;&c_a;&c_d;&c_ib;</trd>
  </trdgrp>
