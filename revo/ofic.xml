<drv mrk="ofic.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="ofic.0o.POL">
    <dif>
      <ref tip="super" cel="okup.0o">Okupo</ref>, 
      <ref tip="super" cel="profes.0o">profesio</ref>, 
      konsistanta en plenumo de publika a&ubreve; privata servado:
      <ekz>
        <tld/>o de kasisto, kontisto, po&scirc;tisto, urbestro, ministro;
      </ekz>
      <ekz>
        <tld/>o de rabeno, de kantoro
        <fnt>
          <bib>RBa</bib>
        </fnt>;
      </ekz>
      <ekz>
        de la a&gcirc;o de kvindek jaroj ili retiri&gcirc;os el la <tld/>o 
        <klr>[&#x2026;]</klr> kaj ne servos plu
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 8:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        post tri tagoj Faraono levos vian kapon kaj redonos al vi vian <tld/>on
        <klr>[kiel]</klr> anta&ubreve;e, kiam vi estis lia vinisto
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 40:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        kandidatinoj por <tld/>o de instruistino
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi ricevos <tld/>on en la <tld/>ejo de la ar&hcirc;itekturisto de la urbo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        en siaj bienoj li facile trovos por vi <tld/>on malpli lacigan
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni nomis vezirojn, kies <tld/>o estis ju&gcirc;i pri diversaj &scirc;tataferoj
        <fnt>
          <bib>Kandid</bib>
        </fnt>;
      </ekz>
      <ekz>
        Emilo ricevis <tld/>on de pastro
        <fnt>
          <bib>Fab4</bib>
          <lok>Peco da perlovico</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi havas sekretarian <tld/>on kaj akompanis mian mastron eksterlande
        <fnt>
          <bib>ChR</bib>
        </fnt>;
      </ekz>
      <ekz>
        eksigo el <tld/>o
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> malbone plenumis <tld/>on al ili konfiditan de la Asocio
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        granda <tld/>o, grandaj zorgoj
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>Dio donis <tld/>on, Dio donas kapablon</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
        <trd lng="de">man w&auml;chst mit seinen Aufgaben</trd>
      </ekz>
      <ekz>
        <ind>anta&ubreve; &ccirc;io zorgu <tld/>on, plezuro atendos sian vicon</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="de">erst die <ind>Arbeit</ind> dann das Vergn&uuml;gen</trd>
      </ekz>
    </dif>
    <ref tip="sub" cel="posten.0o.ofico">posteno<sncref/></ref>
    <refgrp tip="vid">
      <ref cel="fak.0o">fako</ref>,
      <ref cel="funkci.0o">funkcio</ref>,
      <ref cel="meti.0o">metio</ref>,
      <ref cel="profes.0o">profesio</ref>,
      <ref cel="rol.0o">rolo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
