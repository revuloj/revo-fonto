    <trd>hivatalba helyez</trd>,
    <trd>munk&aacute;ba &aacute;ll&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4EFB;&#x3058;&#x308B;  <pr>&#x306B;&#x3093;&#x3058;&#x308B;</pr></trd>,
    <trd>&#x4EFB;&#x547D;&#x3059;&#x308B;  <pr>&#x306B;&#x3093;&#x3081;&#x3044;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">aanstellen</trd>
  <trdgrp lng="pl">
    <trd>wprowadzi&cacute; na urz&aogonek;d</trd>,
