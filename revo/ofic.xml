<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>ofic</rad>/o
    </kap>

    <drv mrk="ofic.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc>
        <dif>
          Okupo, profesio, konsistanta en plenumo de publika a&ubreve; privata
          servado:
          <ekz>
            <tld/>o de kasisto, kontisto, po&scirc;tisto, urbestro, ministro;
          </ekz><ekz>
            havi la <tld/>on de rabeno<fnt>Z</fnt>;
          </ekz><ekz>
            plenumadi la <tld/>on de distran&ccirc;isto<fnt>Z</fnt>;
          </ekz><ekz>
            anta&ubreve; &ccirc;io zorgu <tld/>on, plezuro atendos sian vicon
            <fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            granda <tld/>o, grandaj zorgoj
            <fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            senlabora <tld/>o;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            por tiel malvasta <tld/>o ne estas necese enkonduki specialan
            sufikson;
          </ekz><ekz>
            <tld/>aj vestoj, devoj.
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="meti.0o">metio</ref>,
          <ref cel="profes.0o">profesio</ref>,
          <ref cel="funkci.0o">funkcio</ref>,
          <ref cel="fak.0o">fako</ref>,
          <ref cel="rol.0o">rolo</ref>.
        </refgrp>
      </snc>
      <trdgrp lng="be">
        <trd>&c_p;&c_a;&c_s;&c_a;&c_d;&c_a;</trd>,
        <trd>&c_s;&c_l;&c_u;&c_zh;&c_b;&c_a;</trd>
      </trdgrp>
      <trd lng="cs">&uacute;&rcaron;ad</trd>
      <trd lng="de">Amt</trd>
      <trd lng="en">office</trd>
      <trd lng="es">oficio</trd>
      <trdgrp lng="hu">
        <trd>hivatal</trd>,
        <trd>tiszts&eacute;g</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>ambt</trd>,
        <trd>betrekking</trd>
      </trdgrp>
      <trd lng="pt">of&iacute;cio</trd>
      <trdgrp lng="ru">
        <trd>&c_d;&c_o;&c_l;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
        <trd>&c_s;&c_l;&c_u;&c_zh;&c_b;&c_a;</trd>
      </trdgrp>
      <trdgrp lng="sv">
        <trd>tj&auml;nst</trd>,
        <trd>anst&auml;llning</trd>,
        <trd>plats</trd>
      </trdgrp>
      <trd lng="vo">cal</trd>
    </drv>

    <drv mrk="ofic.0i">
      <kap><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
        <dif>
          Plenumi <tld/>on.
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_z;&c_a;&c_j;&c_m;&c_a;&c_c;&c_mol; &c_p;&c_a;&c_s;&c_a;&c_d;&c_u;</trd>,
        <trd>&c_s;&c_l;&c_u;&c_zh;&c_y;&c_c;&c_mol;</trd>
      </trdgrp>
      <trd lng="cs">&uacute;&rcaron;adovat</trd>
      <trd lng="de">ein <ind>Amt</ind> begleiten</trd>
      <trd lng="en">hold office</trd>
      <trdgrp lng="hu">
        <trd>hivatalban van</trd>,
        <trd>hivatalt bet&ouml;lt</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>een <ind>ambt</ind> vervullen</trd>,
        <trd>een <ind>betrekking</ind> vervullen</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_s;&c_l;&c_u;&c_zh;&c_i;&c_t;&c_mol;</trd>,
        <trd>&c_z;&c_a;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;
          &c_d;&c_o;&c_l;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
      </trdgrp>
    </drv>

    <drv mrk="ofic.0ejo">
      <kap><tld/>ejo</kap>
      <snc>
        <dif>
          Loko, kie oni plenumas <tld/>on.
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_m;&c_je;&c_s;&c_c;&c_a; &c_s;&c_l;&c_u;&c_zh;&c_b;&c_y;</trd>,
        <trd>&c_o;&c_f;&c_ib;&c_s;</trd>,
        <trd>&c_b;&c_ju;&c_r;&c_o;</trd>,
        <trd>&c_k;&c_a;&c_n;&c_t;&c_o;&c_r;&c_a;</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Amt</trd>,
        <trd>B&uuml;ro</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>office</trd>,
        <trd>office space</trd>
      </trdgrp>
      <trd lng="hu">hivatal</trd>
      <trdgrp lng="nl">
        <trd>kantoor</trd>,
        <trd>bureau</trd>
      </trdgrp>
      <trdgrp lng="pt">
        <trd>oficina</trd>,
        <trd>escrit&oacute;rio</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_m;&c_je;&c_s;&c_t;&c_o; &c_s;&c_l;&c_u;&c_zh;&c_b;&c_y;</trd>,
        <trd>&c_k;&c_a;&c_n;&c_c;&c_je;&c_l;&c_ja;&c_r;&c_i;&c_ja;</trd>,
        <trd>&c_k;&c_o;&c_n;&c_t;&c_o;&c_r;&c_a;</trd>,
        <trd>&c_o;&c_f;&c_i;&c_s;</trd>
      </trdgrp>
      <trd lng="sv">kontor</trd>
    </drv>

    <drv mrk="ofic.0isto">
      <kap><tld/>isto</kap>
      <snc>
        <dif>
          Homo, kiu profesie plenumas <tld/>on:
          <ekz>
            banka, komerca, publika, regna, &scirc;tata, urba, dogana, polica
            <tld/>isto.
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="funkci.0ulo">funkciulo</ref>.
        </refgrp>
      </snc>
      <trdgrp lng="be">
        <trd>&c_s;&c_l;&c_u;&c_zh;&c_b;&c_o;&c_v;&c_je;&c_c;</trd>,
        <trd>&c_k;&c_a;&c_n;&c_t;&c_o;&c_r;&c_ch;&c_sh;&c_y;&c_k;</trd>,
        <trd>&c_k;&c_l;&c_je;&c_r;&c_k;</trd>
      </trdgrp>
      <trd lng="de">Beamter</trd>
      <trd lng="en">officer</trd>
      <trdgrp lng="hu">
        <trd>hivatalnok</trd>,
        <trd>tisztvisel&odblac;</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>ambtenaar</trd>,
        <trd>bediende</trd>
      </trdgrp>
      <trd lng="pt">oficial</trd>
      <trdgrp lng="ru">
        <trd>&c_s;&c_l;&c_u;&c_zh;&c_a;&c_shch;&c_i;&c_j;</trd>,
        <trd>&c_k;&c_l;&c_je;&c_r;&c_k;</trd>
      </trdgrp>
      <trd lng="sv">tj&auml;nsteman</trd>
    </drv>

    <drv mrk="ofic.en0igi">
      <kap>en<tld/>igi</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Per formala ago (ceremonio, rito, prezento) rajtigi a&ubreve;
          devigi iun plenumi koncernan <tld/>on:
          <ekz>
            ili ne en<tld/>igas re&gcirc;on super la lando
            <fnt>
              <aut>Tr. Gerrit Berveling</aut>:
              <vrk>Letero de Jeremia</vrk>.
              <lok><url ref="http://www.esperanto.gr/esp/grek/els/let.htm"
                  >La Biblio (duakanonaj libroj)</url></lok>
            </fnt>;
          </ekz><ekz>
            la 15an de decembro en Makedonio en<tld/>i&gcirc;is nova
            prezidento
            <fnt>
              <aut>Evgeni GEORGIEV</aut>:
              <vrk>Nova &scirc;anco por &ccirc;iuj etnoj</vrk>. <bib>Monato</bib>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <refgrp tip="vid">
        <ref cel="posten.en0igi">enpostenigi</ref>,
        <ref cel="instal.0i.enpostenigi">instali<sncref/></ref>,
        <ref cel="nom.0umi">nomumi</ref>
      </refgrp>
      <trdgrp lng="be">
        <trd>&c_n;&c_a;&c_z;&c_n;&c_a;&c_ch;&c_y;&c_c;&c_mol; &c_n;&c_a;
          &c_p;&c_a;&c_s;&c_a;&c_d;&c_u;</trd>,
        <trd>&c_d;&c_a;&c_c;&c_mol; &c_p;&c_a;&c_s;&c_a;&c_d;&c_u;</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>commission</trd>,
        <trd>inaugurate</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd><klr>mit einer </klr>Aufgabe <ind>betrauen</ind></trd>,
        <trd><klr>ein </klr>Amt <ind>geben</ind></trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>beiktat</trd>,
        <trd>hivatalba helyez</trd>,
        <trd>munk&aacute;ba &aacute;ll&iacute;t</trd>
      </trdgrp>
      <trd lng="nl">aanstellen</trd>
      <trd lng="pt">oficiar</trd>
      <trd lng="ru">&c_v;&c_v;&c_je;&c_s;&c_t;&c_i; &c_v;
        &c_d;&c_o;&c_l;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
    </drv>
  </art>
