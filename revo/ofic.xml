<drv mrk="ofic.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="ofic.0o.POL">
    <dif>
      Okupo, profesio, konsistanta en plenumo de publika a&ubreve; privata
      servado:
      <ekz>
        <tld/>o de kasisto, kontisto, po&scirc;tisto, urbestro, ministro;
      </ekz>
      <ekz>
        havi la <tld/>on de rabeno<fnt>Z</fnt>;
      </ekz>
      <ekz>
        plenumadi la <tld/>on de distran&ccirc;isto<fnt>Z</fnt>;
      </ekz>
      <ekz>
        anta&ubreve; &ccirc;io zorgu <tld/>on, plezuro atendos sian vicon
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        granda <tld/>o, grandaj zorgoj
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        senlabora <tld/>o;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        por tiel malvasta <tld/>o ne estas necese enkonduki specialan
        sufikson;
      </ekz>
      <ekz>
        <tld/>aj vestoj, devoj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fak.0o">fako</ref>,
      <ref cel="funkci.0o">funkcio</ref>,
      <ref cel="meti.0o">metio</ref>,
      <ref cel="profes.0o">profesio</ref>,
      <ref cel="rol.0o">rolo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_s;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_s;&c_l;&c_u;&c_zh;&c_b;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>&uacute;&rcaron;ad</trd>,
    <trd>&uacute;&rcaron;ad</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Amt</trd>,
    <trd>Stellung</trd>
  </trdgrp>
  <trd lng="en">office</trd>
  <trdgrp lng="es">
    <trd>cargo</trd>,
    <trd>empleo</trd>,
    <trd>oficio</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>emploi</trd>,
    <trd>fonction<klr> (emploi)</klr></trd>,
    <trd>office</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hivatal</trd>,
    <trd>tiszts&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ambt</trd>,
    <trd>betrekking</trd>
  </trdgrp>
  <trd lng="pt">of&iacute;cio</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_l;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_l;&c_u;&c_zh;&c_b;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>funkcia</trd>,
    <trd>postavenie; povolanie</trd>,
    <trd>&uacute;rad</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>tj&auml;nst</trd>,
    <trd>anst&auml;llning</trd>,
    <trd>plats</trd>
  </trdgrp>
  <trd lng="vo">cal</trd>
</drv>

<drv mrk="ofic.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ofic.0i.POL">
    <dif>
      Plenumi <tld/>on:
      <ekz>
        informi&gcirc;i pri iu en tia domo estis treege malfacile, precipe, se &ccirc;e la pordego
        ne <tld/>is pordisto
        <fnt>
          <aut>H. Sienkiewicz, trad Lidia Zamenhof</aut>
          <vrk>Quo Vadis?</vrk>
          <lok>1933</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_j;&c_m;&c_a;&c_c;&c_mol; &c_p;&c_a;&c_s;&c_a;&c_d;&c_u;</trd>,
    <trd>&c_s;&c_l;&c_u;&c_zh;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>&uacute;&rcaron;adovat</trd>,
    <trd>&uacute;&rcaron;adovat</trd>
  </trdgrp>
  <trd lng="de">ein <ind>Amt</ind> bekleiden</trd>
  <trd lng="en">hold office</trd>
  <trd lng="es">oficiar</trd>
  <trdgrp lng="fr">
    <trd>remplir son <ind>office</ind></trd>,
    <trd>travailler</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hivatalban van</trd>,
    <trd>hivatalt bet&ouml;lt</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>een <ind>ambt</ind> vervullen</trd>,
    <trd>een <ind>betrekking</ind> vervullen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_u;&c_zh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;
    &c_d;&c_o;&c_l;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">&uacute;radova&tcaron;</trd>
</drv>

