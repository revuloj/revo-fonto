<drv mrk="ofic.en0igi">
  <kap>en<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ofic.en0igi.POL">
    <dif>
      Per formala ago (ceremonio, rito, prezento) rajtigi a&ubreve;
      devigi iun plenumi koncernan <tld/>on:
      <ekz>
        &ccirc;iu &ccirc;efpastro estas en<tld/>igita por oferi donacojn kaj oferojn
        <fnt>
          <bib>NT</bib>
          <lok>Hebreoj 8:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili ne en<tld/>igas re&gcirc;on super la lando
        <fnt>
          <aut>trad. G. Berveling</aut>,
          <vrk>Letero de Jeremia</vrk>,
          <lok>en: <url ref="http://www.esperanto.gr/esp/grek/els/let.htm">La Biblio (duakanonaj
          libroj)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la postan jaron, tamen, estis akceptita la statuto, 
        kaj en<tld/>igita nova prezidanto
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi anta&ubreve; la&ubreve;le&gcirc;e en<tld/>igita pola konsulo en ekstera lando 
        <klr>[&#x2026;]</klr> persone protokoligis, ke vi volas esti plu pola &scirc;tatano?
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="posten.en0igi">enpostenigi</ref>,
      <ref cel="instal.0i.enpostenigi">instali<sncref/></ref>,
      <ref cel="nom.0umi">nomumi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_z;&c_n;&c_a;&c_ch;&c_y;&c_c;&c_mol; &c_n;&c_a; &c_p;&c_a;&c_s;&c_a;&c_d;&c_u;</trd>,
    <trd>&c_d;&c_a;&c_c;&c_mol; &c_p;&c_a;&c_s;&c_a;&c_d;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>commission</trd>,
    <trd>inaugurate</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><klr>mit einer </klr>Aufgabe <ind>betrauen</ind></trd>,
    <trd><klr>ein </klr>Amt <ind>geben</ind></trd>
  </trdgrp>
  <trd lng="es">tomar posesi&oacute;n de un cargo</trd>
  <trd lng="fr">installer dans ses <ind>fonctions</ind></trd>
  <trdgrp lng="hu">
