<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>hal</rad>/o
</kap>
<drv mrk="hal.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Malfermita tegmentita vendejo:
      <ekz>
        vin<tld/>o, drap<tld/>o
        <klr>(analogie)</klr>;
      </ekz>
      <ekz>
        <tld/>o de stacidomo.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      &Ccirc;ambrego:
      <ekz>
        fest<tld/>o, honor<tld/>o;
      </ekz>
      <ekz>
        granda <tld/>o de hotelo.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="salon.0o">salono</ref>,
      <ref cel="solen">solenejo</ref>,
      <ref cel="auxl.0o">a&ubreve;lo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Halle</trd>
  <trd lng="nl">hal</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_l;</trd>,
    <trd>&c_h;&c_o;&c_l;&c_l;</trd>
  </trdgrp>
</drv>
</art>
