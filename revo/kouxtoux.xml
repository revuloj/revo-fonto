<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kouxtoux.xml,v 1.9 2023/12/03 09:59:00 revo Exp $">
<kap>
  <rad>ko&ubreve;to&ubreve;</rad>/i
</kap>
<drv mrk="kouxtoux.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kouxtoux.0i.MOR">
    <dif>
      Saluti per genuado kaj sinklino &gcirc;is kapo-grunda tu&scirc;o:
      <ekz>
        Jaspa pretis fali sur la genuojn por <tld/>i
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 3a, volumo 1a, p. 
        34a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="klin.ador0igxi">adorklini&gcirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_a;&c_n;&c_ja;&c_c;&c_c;&c_a; <klr>(&c_u; &c_n;&c_o;&c_g;&c_ib;, &c_d;&c_a; &c_z;&c_ja;&c_m;&c_l;&c_ib;)</klr></trd>
  <trdgrp lng="de">
    <trd><klr>eine </klr>Verbeugung machen</trd>,
    <trd><klr>sich </klr>verbeugen</trd>,
    <trd>einen Diener machen<klr> [ark]</klr></trd>,
    <trd>einen Kotau machen<klr> [nur rilate al aziuloj]</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>postrar</trd>,
    <trd>postrarse</trd>
  </trdgrp>
  <trd lng="fr">se prosterner</trd>
  <trd lng="hu">f&ouml;ldre borul</trd>
  <trd lng="pl">pok&lstroke;oni&cacute; si&eogonek; <klr>(do ziemi)</klr></trd>
  <trd lng="pt">prosternar-se</trd>
  <trd lng="ro">arc &icirc;n jos <klr>(a sol)</klr></trd>
    <trd lng="zh">&#x78D5;&#x5934; <pr>k&#x113;t&#xF3;u</pr></trd>
</drv>
</art>

