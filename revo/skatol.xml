<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>skatol</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="skatol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="skatol.0o.KOMUNE">
    <dif>
      Ujeto diversforma, fermebla kaj malfermebla,
      el solida substanco kiaj ligno, kartono, plasto, metalo...:
      <ekz>
        <tld/>o, en kiu oni tenas plumojn, estas plumujo
        <fnt><bib>F</bib>, <lok>&FE; 40</lok></fnt>;
      </ekz>
      <ekz>
        vi trovos leterpaperon en la <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_r;&c_y;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_u;&c_f;&c_l;&c_ja;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_u;&c_f;&c_e;&c_r;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">caixa</trd>
  <trd lng="cs">krabice</trd>
  <trd lng="de">Schachtel</trd>
  <trd lng="en">box</trd>
  <trd lng="es">caja</trd>
  <trdgrp lng="fr">
    <trd>bo&icirc;te</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>doboz</trd>,
    <trd>skatulya</trd>
  </trdgrp>
  <trd lng="it">scatola</trd>
  <trd lng="nl">doos</trd>
  <trdgrp lng="ru">
    <trd>&c_ja;&c_shch;&c_i;&c_k;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_o;&c_b;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_k;&c_a;&c_t;&c_u;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_l;&c_a;&c_r;&c_je;&c_c;</trd>
  </trdgrp>
</drv>

<drv mrk="skatol.muzik0o">
  <kap>muzik<tld/>o</kap>
  <snc mrk="skatol.muzik0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Amuzilo el <tld/>o entenanta tintan muzika&ubreve;tomaton:
      <ekz>
        eksterordinaraj ludiloj: a&ubreve;tomataj dancistinoj, muzik-<tld/>oj,
        
        akrobataj simioj
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>,
        <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>...
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">boite &agrave; musique</trd>
  <trd lng="it">carillon</trd>
</drv>

<drv mrk="skatol.nigra0o">
  <kap>nigra <tld/>o</kap>
  <snc mrk="skatol.nigra0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <ref tip="dif" cel="kest.nigra0o.TEK">nigra kesto <sncref/></ref>
    <trdgrp lng="de">
      <trd>Blackbox</trd>
    </trdgrp>
  </snc>
  <snc mrk="skatol.nigra0o.AVI">
    <uzo tip="fak">AVI</uzo>
    <uzo tip="stl">EVI</uzo>
    <ref tip="dif" cel="kest.nigra0o.AVI">nigra kesto <sncref/></ref>
    <trdgrp lng="de">
      <trd>Flugschreiber</trd>,
      <trd>Blackbox</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Blackbox</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>boite noire</trd>,
    <trd>bo&icirc;te noire</trd>
  </trdgrp>
  <trd lng="it"><ind>scatola</ind> nera</trd>
</drv>

<drv mrk="skatol.son0o">
  <kap>son<tld/>o</kap> <fnt><bib>MuzTerm</bib></fnt>
  <snc mrk="skatol.son0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <ref tip="dif" cel="kest.son0o">sonkesto</ref>
  </snc>
  <trd lng="de">Klangk&ouml;rper</trd>
  <trd lng="fr">caisse de r&eacute;sonance</trd>
  <trd lng="hu">hangszekr&eacute;ny</trd>
  <trdgrp lng="it">
    <trd><ind>cassa</ind> di risonanza</trd>,
    <trd><ind>cassa</ind> armonica</trd>
  </trdgrp>
</drv>
</art>
