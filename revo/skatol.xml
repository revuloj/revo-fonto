  <trd lng="sk">rozvodn&aacute; skrinka</trd>
  <trd lng="cs">rozvodn&aacute; krabice</trd>
<trdgrp lng="de">
  <trd>Steckdose</trd>,
  <trd>Wallbox</trd>,
  <trd>Ladestation</trd>
</trdgrp>
</drv>

