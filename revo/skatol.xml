    <trd>&#x7BB1;  <pr>&#x306F;&#12371;</pr></trd>,
    <trd>&#x7B52;  <pr>&#x30D4;&#x30F3;</pr></trd>,
    <trd>&#x7F36;  <pr>&#x307B;&#12392;&#x304E;</pr></trd>,
    <trd>&#x5BB9;&#22120;  <pr>&#12424;&#12358;&#x304D;</pr></trd>,
    <trd>&#x30B1;&#x30FC;&#x30B9;  <pr>&#12369;&#x30FC;&#12377;</pr></trd>
  </trdgrp>
  <trd lng="nl">doos</trd>
  <trdgrp lng="oc">
