<drv mrk="skatol.karton0o">
  <kap>
    karton<tld/>o, <var><kap>paper<tld/>o</kap></var>
  </kap>
  <snc>
    <dif>
      <tld lit="S"/>o el kartono a&ubreve; fortika papero:
      <ekz>
        <klr>[li]</klr> revenis kun karton<tld/>o plena je skriba&jcirc;oj
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sepa</lok>
        </fnt>;
      </ekz>
      <ekz>
        malvarmiga &scirc;ranko, interne paper<tld/>oj, en ili diversaj kompaktaj man&gcirc;a&jcirc;oj
        <fnt>
          <bib>VaK</bib>
          <lok>Kvara &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_d;&c_o;&c_n;&c_n;&c_a;&c_ja; <ind>&c_s;&c_k;&c_r;&c_y;&c_n;&c_k;&c_a;</ind></trd>
  <trdgrp lng="de">
    <trd>Pappschachtel</trd>,
    <trd>Karton <klr>(Schachtel)</klr></trd>,
    <trd>Pappkarton</trd>
  </trdgrp>
  <trd lng="en">cardboard box</trd>
  <trd lng="fr">bo&icirc;te en carton</trd>
  <trdgrp lng="ja">
    <trd>&#x7D19;&#x7BB1;  <pr>&#x304B;&#x307F;&#12400;&#12371;</pr></trd>,
    <trd>&#x30DC;&#x30FC;&#x30EB;&#x7BB1;  <pr>&#x30DC;&#x30FC;&#x30EB;&#12400;&#12371;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>karton</trd>,
    <trd>pude&lstroke;ko kartonowe</trd>
  </trdgrp>
</drv>

<drv mrk="skatol.kolor0o">
