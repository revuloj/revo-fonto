<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>fran&gcirc;</rad>/o</kap>
<drv mrk="frangx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Ornama&jcirc;o el pendantaj fadenoj por borderi randon de vesto, meblo kc.
    </dif>
    <refgrp tip="vid">
      <ref cel="kvast.0o">kvasto</ref>,
      <ref cel="penik.0o">peniko</ref>,
      <ref cel="galon.0o">galono</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_h;&c_r;&c_y;</trd>
  <trd lng="fr">frange<klr tip="ind"> (ornement)</klr></trd>
  <trd lng="hu">rojt</trd>
  <trd lng="nl">franje</trd>
  <trd lng="ru">&c_b;&c_a;&c_h;&c_r;&c_o;&c_m;&c_a;</trd>
</drv>

<drv mrk="frangx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ornami per <tld/>oj.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_z;&c_d;&c_a;&c_b;&c_l;&c_ja;&c_c;&c_mol; <ind>&c_m;&c_a;&c_h;&c_r;&c_a;&c_m;&c_ib;</ind></trd>
  <trd lng="fr">franger</trd>
  <trd lng="hu">rojtoz</trd>  
  <trd lng="nl">van een <ind>franje</ind> voorzien</trd>
  <trd lng="ru">&c_u;&c_k;&c_r;&c_a;&c_sh;&c_a;&c_t;&c_mol; &c_b;&c_a;&c_h;&c_r;&c_o;&c_m;&c_o;&c_j;</trd>
</drv>
</art>
