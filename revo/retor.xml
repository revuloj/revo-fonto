<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>retor</rad>/o
</kap>
<drv mrk="retor.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;e la antikvuloj: homo instruanta la arton trafe kaj elokvente
      paroli.
    </dif>
    <trd lng="be">&c_r;&c_y;&c_t;&c_a;&c_r;</trd>
    <trdgrp lng="de">
      <trd>Rhetor</trd>,
      <trd>Redner</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>r&eacute;tor</trd>,
      <trd>besz&eacute;doktat&oacute;</trd>
    </trdgrp>
    <trd lng="nl">retor</trd>
    <trd lng="ru">&c_r;&c_i;&c_t;&c_o;&c_r;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Oratoro, kies parolado estas afekta, bombasta kaj senenhava.
    </dif>
    <trd lng="be">&c_n;&c_a;&c_p;&c_y;&c_sh;&c_l;&c_ib;&c_v;&c_y; &c_p;&c_r;&c_a;&c_m;&c_o;&c_w;&c_c;&c_a;</trd>
    <trd lng="de">Sch&ouml;nredner</trd>
    <trd lng="hu">dag&aacute;lyos sz&oacute;nok</trd>    
    <trd lng="nl">schoonprater</trd>
    <trd lng="ru">&c_n;&c_a;&c_p;&c_y;&c_shch;&c_je;&c_n;&c_n;&c_y;&c_j; &c_o;&c_r;&c_a;&c_t;&c_o;&c_r;</trd>
  </snc>
</drv>
</art>
