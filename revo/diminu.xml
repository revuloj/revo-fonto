<art mrk="$Id: diminu.xml,v 1.18 2022/03/15 15:50:55 revo Exp $">
<kap>
  <rad>diminutiv</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="diminu.0o">
  <kap><tld/>o</kap>
  <snc mrk="diminu.0o.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      Vorto derivita de alia per sufikso, kiu malgrandigas a&ubreve;
      malgravigas
      la enhavatan ideon:
      <ekz>
      <ind><tld/>a sufikso</ind> en Esperanto estas <ctl>-et</ctl>.
      <trd lng="be">&c_p;&c_a;&c_m;&c_ja;&c_n;&c_sh;&c_a;&c_l;&c_mol;&c_n;&c_y; &c_s;&c_u;&c_f;&c_ib;&c_k;&c_s;</trd>
      <trd lng="hu">kicsiny&iacute;t&odblac;k&eacute;pz&odblac;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_m;&c_ja;&c_n;&c_sh;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_f;&c_o;&c_r;&c_m;&c_a;</trd>
  <trd lng="cs">zdrobn&ecaron;lina</trd>
  <trdgrp lng="de">
     <trd>Verkleinerungswort</trd>,
     <trd>Verkleinerungsform</trd>,
     <trd>Diminutiv</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kicsiny&iacute;t&odblac; alak</trd>,
    <trd>kicsiny&iacute;t&odblac;k&eacute;pz&odblac;s forma</trd>,
    <trd>diminutivum</trd>
  </trdgrp>
  <trd lng="en">diminutive</trd>
  <trd lng="es">diminutivo</trd>
  <trd lng="fr">diminutif</trd>
  <trd lng="he">&#x5E6;&#x5D5;&#x5BC;&#x5E8;&#x5B7;&#x5EA;
  &#x5D4;&#x5B7;&#x5E7;&#x5D8;&#x5B8;&#x5E0;&#x5B8;&#x5D4;</trd>
  <trdgrp lng="nl">
    <trd>diminitief</trd>,
    <trd>verkleinvorm</trd>
  </trdgrp>
  <trd lng="pt">diminutivo</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_m;&c_je;&c_n;&c_mol;&c_sh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_f;&c_o;&c_r;&c_m;&c_a; &c_s;&c_l;&c_o;&c_v;&c_a;</trd>,
    <trd>&c_d;&c_i;&c_m;&c_i;&c_n;&c_u;&c_t;&c_i;&c_v;</trd>
  </trdgrp>
  <trd lng="sk">zdrobnenina</trd>
  <trdgrp lng="uk">
    <trd>&#x434;&#x435;&#x43C;&#x456;&#x43D;&#x443;&#x442;&#x438;&#x432;</trd>,
    <trd>&#x437;&#x434;&#x440;&#x456;&#x431;&#x43D;&#x456;&#x43B;&#x430; &#x444;&#x43E;&#x440;&#x43C;&#x430;</trd>
  </trdgrp>
</drv>
</art>
