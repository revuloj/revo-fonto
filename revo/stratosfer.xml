<kap>
  <rad>stratosfer</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="stratosfer.0o">
  <kap><tld/>o</kap>
  <snc mrk="stratosfer.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Tavolo de la tera
      <ref tip="malprt" cel="atmosf.0o.AST">atmosfero</ref>
      mezume inter 12 kaj 50 kilometrojn alta super la maro,
      sen ventoj, kirloj a&ubreve; nuboj:
      <ekz>
        tiuj malaltaj temperaturoj estas atingataj nur en la polusa 
        <tld/>o
        <fnt>
          <aut>A. Oberndorfer</aut>,
          <vrk><url ref="http://esperantomovado.blogspot.fr/2012/04/la-ozona-tavolo-rekreigas.html">
          La ozona tavolo rekrei&gcirc;as</url></vrk>,
          <lok>2012-04-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_r;&c_a;&c_t;&c_a;&c_s;&c_f;&c_e;&c_r;&c_a;</trd>
  <trd lng="bg">&c_s;&c_t;&c_r;&c_a;&c_t;&c_o;&c_s;&c_f;&c_je;&c_r;&c_a;</trd>
  <trd lng="cs">stratosf&#xE9;ra</trd>
  <trd lng="de">Stratosph&auml;re</trd>
  <trd lng="fr">stratosph&egrave;re</trd>
  <trd lng="ja">&#x6210;&#x5C64;&#x570F;  <pr>&#x305B;&#x3044;&#x305D;&#x3046;&#x3051;&#x3093;</pr></trd>
  <trd lng="pl">stratosfera</trd>
  <trd lng="sk">stratosf&#xE9;ra</trd>
  <trd lng="uk">&#x441;&#x442;&#x440;&#x430;&#x442;&#x43E;&#x441;&#x444;&#x435;&#x440;&#x430;</trd>
  <trdgrp lng="zh">
    <trd>&#x540C;&#x6E29;&#x5C42; <pr>t&#xF3;ngw&#x113;nc&#xE9;ng</pr></trd>,
    <trd>&#x540C;&#x6EAB;&#x5C64; <pr>t&#xF3;ngw&#x113;nc&#xE9;ng</pr></trd>,
    <trd>&#x6700;&#x4E0A;&#x5C42; <pr>zu&#xEC;sh&#xE0;ngc&#xE9;ng</pr></trd>,
    <trd>&#x6700;&#x4E0A;&#x5C64; <pr>zu&#xEC;sh&#xE0;ngc&#xE9;ng</pr></trd>,
    <trd>&#x6700;&#x9AD8;&#x9636;&#x6BB5; <pr>zu&#xEC;g&#x101;oji&#x113;du&#xE0;n</pr></trd>,
    <trd>&#x6700;&#x9AD8;&#x968E;&#x6BB5; <pr>zu&#xEC;g&#x101;oji&#x113;du&#xE0;n</pr></trd>,
    <trd>&#x5E73;&#x6D41;&#x5C42; <pr>p&#xED;ngli&#xFA;c&#xE9;ng</pr></trd>,
    <trd>&#x5E73;&#x6D41;&#x5C64; <pr>p&#xED;ngli&#xFA;c&#xE9;ng</pr></trd>
  </trdgrp>
</drv>
</art>

