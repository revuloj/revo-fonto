<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>klif</rad>/o <fnt>Y</fnt>
</kap>
<drv mrk="klif.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Mar<ref tip="super" cel="bord.0o">bord</ref>a alta kruta&jcirc;o:
      <ekz>
        tie estas granda diverseco de birdoj sur la bretsimilaj tavoloj de
        la krutaj <tld/>oj<fnt><aut>W. H. Simcock</aut>: <vrk>Vizito al la
        nordaj insuloj</vrk>, <bib>Monato</bib><lok>jaro 1999a, numero 9a,
        p. 18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_c;&c_jo;&c_s;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_o;&c_m;&c_k;&c_ib; &c_b;&c_je;&c_r;&c_a;&c_g;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kliff</trd>,
    <trd>Steilk&uuml;ste</trd>,
    <trd>Klippe</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cliff</trd>,
    <trd>crag</trd>,
    <trd>headland</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>falaise</trd>
  </trdgrp>
  <trd lng="nl">klip</trd>
  <trd lng="ru">&c_u;&c_t;&c_jo;&c_s;</trd>
</drv>
</art>
