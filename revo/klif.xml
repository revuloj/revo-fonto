<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: klif.xml,v 1.21 2025/11/20 18:42:58 revo Exp $">
<kap>
  <rad>klif</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="klif.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Mar<ref tip="super" cel="bord.0o">bord</ref>a alta kruta&jcirc;o:
      <ekz>
        vojeto paralelis la randon de la <tld/>forma alta&jcirc;o tiel proksime al la abismo,
        ke multaj certe sentus kapturni&gcirc;on
        <fnt>
          <bib>ChV</bib>
          <lok>5</lok>
        </fnt>;
      </ekz>
      <ekz>
        rokforma&jcirc;oj <tld/>e levi&gcirc;as &ccirc;ielen, kvaza&ubreve; muroj okdekmetre altaj
        <fnt>
          <bib>ChR</bib>
          <lok>Tien kaj reen</lok>
        </fnt>;
      </ekz>
      <ekz>
        nek rifoj nek <tld/>oj lin haltigos
        <fnt>
          <bib>Monato</bib>
          <aut>Raita Pyh&auml;l&auml;</aut>
          <vrk><url ref="&Monato;/2005/008685.php"
              >Kesto de viveroj</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        tie estas granda diverseco de birdoj sur la bretsimilaj tavoloj
        de la krutaj <tld/>oj
        <fnt><aut>W. H. Simcock</aut>: <vrk>Vizito al
        la nordaj insuloj</vrk>, <bib>Monato</bib><lok>jaro 1999a, numero
        9a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_c;&c_jo;&c_s;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_o;&c_m;&c_k;&c_ib; &c_b;&c_je;&c_r;&c_a;&c_g;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_t;&c_r;&c_malmol;&c_m;&c_n;&c_a; &c_s;&c_k;&c_a;&c_l;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>fal&eacute;za</trd>,
    <trd>klif</trd>,
    <trd>pob&rcaron;e&zcaron;n&iacute; sr&aacute;z</trd>,
    <trd>&uacute;tes</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kliff</trd>,
    <trd>Steilk&uuml;ste</trd>,
    <trd>Klippe</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cliff</trd>,
    <trd>crag</trd>,
    <trd>headland</trd>
  </trdgrp>
  <trd lng="es">acantilado</trd>
  <trdgrp lng="fr">
    <trd>falaise</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7D76;&#x58C1;  <pr>&#x305C;&#x3063;&#x307A;&#x304D;</pr></trd>,
    <trd>&#x304C;&#x3051;</trd>,
    <trd>&#x65AD;&#x5D16;  <pr>&#x3060;&#x3093;&#x304C;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">klip</trd>
  <trd lng="pl">klif</trd>
  <trdgrp lng="ro">
    <trd>pant&abreve;klifo</trd>,
    <trd>st&acirc;nc&abreve;</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_t;&c_jo;&c_s;</trd>
  <trd lng="sk">morsk&yacute; &uacute;tes</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_r;&c_ib;&c_m;&c_ch;&c_a;&c_k;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_k;&c_i;&c_d;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_ib;&c_m;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_j; &c_b;&c_je;&c_r;&c_je;&c_g;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_b;&c_je;&c_r;&c_je;&c_zh;&c_n;&c_ib; &c_s;&c_t;&c_r;&c_ib;&c_m;&c_k;&c_ib; &c_s;&c_k;&c_je;&c_l;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5C71;&#x5D16; <pr>sh&amacron;ny&aacute;</pr></trd>,
    <trd>&#x58C1; <pr>b&igrave;</pr></trd>,
    <trd>&#x5888; <pr>k&agrave;n</pr></trd>,
    <trd>&#x5CED;&#x58C1; <pr>qi&agrave;ob&igrave;</pr></trd>,
    <trd>&#x5D16; <pr>y&aacute;</pr></trd>,
    <trd>&#x5D3F; <pr>&egrave;</pr></trd>,
    <trd>&#x60AC;&#x5D16; <pr>xu&aacute;ny&aacute;</pr></trd>,
    <trd>&#x78F4; <pr>d&egrave;ng</pr></trd>
  </trdgrp>
</drv>
</art>
