<drv mrk="precip.0e">
  <kap><ofc>*</ofc><tld/>e</kap>
  <snc>
    <dif>
      Adverbo, signifanta &leftquot;anta&ubreve; &ccirc;io, &ccirc;efe, pli ol &ccirc;io a&ubreve;
      &ccirc;iu alia&rightquot;:
      <ekz>
        sur tiuj &ccirc;i vastaj kaj herbori&ccirc;aj kampoj pa&scirc;tas sin grandaj brutaroj, <tld/>e
        aroj da bellanaj &scirc;afoj
        <fnt>
          <bib>F</bib>
          <lok>&FE; 34</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi elkondukis lin anta&ubreve; vin &ccirc;iujn, kaj <tld/>e anta&ubreve; vin, re&gcirc;o Agripo
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 25:26</lok>
        </fnt>;
      </ekz>
      <ekz>
        alportu <klr>[&#8230;]</klr> la librojn, <tld/>e la pergamenojn
        <fnt>
          <bib>NT</bib>
          <lok>II. Timoteo 4:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e &scirc;i amis la rozojn, kaj tial &scirc;i havis la plej diversajn specojn de ili
        <fnt>
          <bib>Fab3</bib>
          <lok>Plej bela rozo de la mondo</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio estis stranga maskarado, kaj <tld/>e estis rimarkinde vidi, kiel ili &ccirc;iuj
        sub siaj vestoj zorge ion ka&scirc;as
        <fnt>
          <bib>Fab3</bib>
          <lok>En la lasta tago</lok>
        </fnt>;
      </ekz>
      <ekz>
        la luno lumis <tld/>e sur unu lokon, kie staris arbo
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu listo ne estis kompleta, kaj enhavis <tld/>e individuajn nomojn
        <fnt>
          <bib>ChC</bib>
          <lok>4</lok>
        </fnt>;
      </ekz>
      <ekz>
        la oponantoj <tld/>e emfazadis la financajn &scirc;ar&gcirc;ojn
        <fnt>
          <bib>EeP</bib>
          <lok>Unua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="apart.0a">aparte</ref>,
      <ref cel="cxef.0e">&ccirc;efe</ref>,
      <ref cel="specia.0e">speciale</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_s;&c_a;&c_b;&c_l;&c_ib;&c_v;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_v;&c_a;&c_zh;&c_n;&c_a;</trd>,
    <trd>&c_g;&c_a;&c_l;&c_o;&c_w;&c_n;&c_y;&c_m; &c_ch;&c_y;&c_n;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="ca">principalment</trd>
  <trdgrp lng="cs">
