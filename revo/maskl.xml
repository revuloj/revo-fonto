<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>maskl</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="maskl.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="maskl.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <ref tip="dif" cel="vir.0o.animalo">viro <sncref/></ref>
    <ekz>
      la grandaj kolharaj <tld/>oj montris al Saba la dentegojn
      <fnt><aut>H. Sienkiewicz, trad. M. Sygnarski</aut>, <vrk><url
      ref="http://esperanto.pl/page.php?tid=200000">Tra dezerto kaj
      praarbaro</url></vrk>, <lok>Varsovio, 1978</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="fr">m&acirc;le<klr> (subst.)</klr></trd>
</drv>

<drv mrk="maskl.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="maskl.0a.BIO">
    <ref tip="dif" cel="vir.0a.virula">vira <sncref/></ref>
    <ekz>
      &leftquot;&ccirc;u vi kredas, ke inter la dek mu&scirc;oj &jcirc;us de mi mortigitaj estas 
      kvar <tld/>aj kaj ses femalaj?&rightquot;
      <fnt>
        <aut>-</aut>,
        <vrk><url ref="http://www.chinareport.com.cn/02l/02l-11.htm">Humura&jcirc;oj, 11</url></vrk>,
        <lok>[2003-04]</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="fr">m&acirc;le<klr> (adj.)</klr></trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>