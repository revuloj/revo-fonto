<drv mrk="abomen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Sento de tiu, kiu <tld/>as:
      <ekz>
        vi sentos <tld/>on anta&ubreve; vi mem kontra&ubreve; &ccirc;iuj malbonagoj, kiujn vi faris.
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 20:43</lok>
        </fnt>;
      </ekz>
      <ekz>
        birdo, malbela kaj vekanta <tld/>on
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        sin skuante de <tld/>o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia abomeno al tiu knabino komencas malforti&gcirc;i
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        kortu&scirc;ate de kompato pli ol de <tld/>o &ccirc;e la vido de tiu terura mizerula&ccirc;o
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi kvaza&ubreve; &scirc;velis de <tld/>o kaj timo, sed mi ne povis krii, nek movi&gcirc;i
        <fnt>
          <bib>FdO</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i sentis e&ccirc; ian <tld/>on kontra&ubreve; man&gcirc;ado
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="estim.mal0o">malestimo</ref>,
      <ref cel="nauxz.0o">na&ubreve;zo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_g;&c_ib;&c_d;&c_a;</trd>
  <trdgrp lng="br">
    <trd>heug</trd>,
    <trd>do&ntilde;jer</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>aversi&oacute;</trd>,
    <trd>repulsi&oacute;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>averze</trd>,
    <trd>hnus</trd>,
    <trd>odpor</trd>,
    <trd>odpudivost</trd>
  </trdgrp>
  <trd lng="da">afsky</trd>
  <trd lng="de">Abscheu</trd>
  <trdgrp lng="en">
    <trd>abomination</trd>,
    <trd>loathing</trd>
  </trdgrp>
  <trd lng="es">aversi&oacute;n</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&f_y;&a_z;&a_A;&a_r;&f_y;</trd>,
    <trd>&a_n;&a_f;&a_r;&a_t;</trd>,
    <trd>&a_A;&a_n;&a_z;&a_j;&a_A;&a_r;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>abomination<klr tip="ind"> (d&eacute;go&ucirc;t)</klr></trd>,
    <trd>aversion</trd>,
    <trd>d&eacute;go&ucirc;t</trd>,
    <trd>d&eacute;testation</trd>,
    <trd>r&eacute;pulsion</trd>
  </trdgrp>
  <trd lng="he">&tav;&vav;&#x5B9;&ayin;&#x5B5;&bet;&#x5B8;&he;</trd>
  <trd lng="hu">ut&aacute;lat</trd>
  <trdgrp lng="ja">
    <trd>&#x5ACC;&#x60AA;  [<ind>&#x3051;&#x3093;&#x304A;</ind>]</trd>,
    <trd>&#x53CD;&#x611F;  [<ind>&#x306F;&#x3093;&#x304B;&#x3093;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>odraza</trd>,
    <trd>wstr&eogonek;t</trd>
  </trdgrp>
  <trd lng="ro">detestare</trd>
  <trd lng="ru">&c_o;&c_t;&c_v;&c_r;&c_a;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>hnus</trd>,
    <trd>odpor</trd>,
    <trd>o&scaron;klivos&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">avsky</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_r;&c_a;&c_z;&c_a;</trd>,
    <trd>&c_o;&c_g;&c_i;&c_d;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="abomen.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Ka&ubreve;zanta <tld/>on:
      <ekz>
        via vino estas nur ia <tld/>a acida&jcirc;o
        <fnt>
          <bib>F</bib>
          <lok>&FE; 35</lok>
        </fnt>;
      </ekz>
      <ekz>
        kia <tld/>a urbeto
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>!
      </ekz>
      <ekz>
        rigardu, kia <tld/>a spektaklo, duone dispremita vermo, kiu ankora&ubreve; tordi&gcirc;as
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XXVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        kia <tld/>a faro! mortigi la &ccirc;evalon, sur kiu la re&gcirc;o estis rajdonta
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fidela Johano</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="nauxz.0a">na&ubreve;za</ref>
