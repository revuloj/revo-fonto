<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>abomen</rad>/i
</kap>
<drv mrk="abomen.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malamegi pro fizike na&ubreve;zaj a&ubreve; morale malpla&ccirc;egaj
      ecoj:
      <ekz>
        <tld/>i odoron, la ostrojn, la mensogon, la militon;
      </ekz>
      <ekz>
        <tld/>i trinki akvon
        <fnt><bib>MT</bib>, <lok>&Eli; 7:18</lok></fnt>;
      </ekz>
      <ekz>
        ni deziras forigi de ni tion, kion ni <tld/>as.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_c;&c_mol; <ind>&c_a;&c_g;
&c_ib;&c_d;&c_u;</ind></trd>
  <trdgrp lng="br">
    <trd>heugi&ntilde;</trd>,
    <trd>do&ntilde;jeri&ntilde;</trd>
  </trdgrp>
  <trd lng="de">verabscheuen</trd>
  <trd lng="hu">ut&aacute;l</trd>
  <trdgrp lng="nl">
    <trd>verafschuwen</trd>,
    <trd>verfoeien</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>abominate</trd>,
    <trd>abhor</trd>,
    <trd>loathe</trd>
  </trdgrp>
  <trd lng="es">abominar</trd>
  <trdgrp lng="fr">
    <trd>abominer</trd>,
    <trd>abhorrer</trd>,
    <trd>d&eacute;tester</trd>,
    <trd>ex&eacute;crer</trd>,
    <trd>avoir en <ind>horreur</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_i;&c_t;&c_a;&c_t;&c_mol; <ind>&c_o;&c_t;&c_v;&c_r;
&c_a;&c_shch;&c_je;&c_n;&c_i;&c_je;</ind></trd>
  <trdgrp lng="sk">
    <trd>hnusi&tcaron; si</trd>,
    <trd>pova&zcaron;ova&tcaron; za <ind>odporn&eacute;</ind></trd>
  </trdgrp>
  <trd lng="sv">avsky</trd>
  <trd lng="pt">abominar</trd>
</drv>

<drv mrk="abomen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Sento de tiu, kiu <tld/>as.
      <ekz>
        senti <tld/>on al, anta&ubreve;, kontra&ubreve; iu, io.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_g;&c_ib;&c_d;&c_a;</trd>
  <trdgrp lng="br">
    <trd>heug</trd>,
    <trd>do&ntilde;jer</trd>
  </trdgrp>
  <trd lng="de">Abscheu</trd>
  <trdgrp lng="fr">
    <trd>abomination<klr tip="ind"> (d&eacute;go&ucirc;t)</klr></trd>,
    <trd>aversion</trd>,
    <trd>d&eacute;go&ucirc;t</trd>,
    <trd>d&eacute;testation</trd>,
    <trd>r&eacute;pulsion</trd>
  </trdgrp>
  <trd lng="hu">ut&aacute;lat</trd>  
  <trd lng="ru">&c_o;&c_t;&c_v;&c_r;&c_a;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="abomen.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Ka&ubreve;zanta <tld/>on:
      <ekz>
        kia <tld/>a urbeto!<fnt>Z</fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_g;&c_ib;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_m;&c_ja;&c_r;&c_z;&c_o;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>heugus</trd>,
    <trd>do&ntilde;jerus</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>scheu&szlig;lich</trd>,
    <trd>abscheulich</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>abominable</trd>,
    <trd>detestable</trd>,
    <trd>loathsome</trd>,
    <trd>odious</trd>,
    <trd>revolting</trd>
    <trd lng="es">abominable</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>undok</trd>,
    <trd>ut&aacute;latos</trd>,
    <trd>ocsm&aacute;ny</trd>
  </trdgrp>
  <trd lng="nl">afschuwelijk</trd>
  <trdgrp lng="fr">
    <trd>abominable</trd>,
    <trd>d&eacute;go&ucirc;tant</trd>,
    <trd>d&eacute;testable</trd>,
    <trd>horrible</trd>,
    <trd>r&eacute;pugnant</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_t;&c_v;&c_r;&c_a;&c_t;&c_i;&c_t;&c_je;&c_l;&c_mol;
&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>odporn&yacute;</trd>,
    <trd>hnusn&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="abomen.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Io <tld/>a.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>abomination<klr tip="ind"> (chose abominable)</klr></trd>,
    <trd>atrocit&eacute;</trd>,
    <trd>honte<klr tip="ind"> (chose qui r&eacute;vulse)</klr></trd>,
    <trd>horreur<klr tip="ind"> (chose qui r&eacute;vulse)</klr></trd>,
    <trd>ignominie<klr tip="ind"> (chose qui r&eacute;vulse)</klr></trd>,
    <trd>scandale<klr tip="ind"> (chose qui r&eacute;vulse)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>undoks&aacute;g</trd>,
    <trd>ocsm&aacute;nys&aacute;g</trd>
  </trdgrp>
</drv>

<drv mrk="abomen.0inda">
  <kap><tld/>inda</kap>
  <snc>
    <dif>
      Inda je <tld/>o; estanta tiel fia, ke oni devas &gcirc;in <tld/>i:
      <ekz>
        lepro kovris mian korpon per <tld/>inda &scirc;elo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        vi <tld/>indigis vian belecon
        <fnt><bib>MT</bib>, <lok>&Jeh; 16:25</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="antipa.0o">antipatio</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_g;&c_ib;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_m;&c_ja;&c_r;&c_z;&c_o;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="hu">ut&aacute;lnival&oacute;</trd>
  <trdgrp lng="nl">
    <trd>afschuwelijk</trd>,
    <trd>ijselijk</trd>,
    <trd>afgrijselijk</trd>,
    <trd>verfoeilijk</trd>
  </trdgrp>
  <trd lng="es">abominable</trd>
  <trdgrp lng="de">
    <trd>verabscheuensw&uuml;rdig</trd>,
    <trd>verabscheuenswert</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>abominable</trd>,
    <trd>d&eacute;go&ucirc;tant</trd>,
    <trd>d&eacute;testable</trd>,
    <trd>horrible</trd>,
    <trd>r&eacute;pugnant</trd>
  </trdgrp>
  <trd lng="it">abominevole</trd>
  <trd lng="ru">&c_o;&c_t;&c_v;&c_r;&c_a;&c_t;&c_i;&c_t;&c_je;&c_l;&c_mol;
&c_n;&c_y;&c_j;</trd>
  <trd lng="sv">avskyv&auml;rd</trd>
  <trd lng="pt">abomin&aacute;vel</trd>
</drv>
</art>
