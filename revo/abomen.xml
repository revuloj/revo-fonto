    <trd>&#x5ACC;&#x60AA;&#x3059;&#x308B;  [<ind>&#x3051;&#x3093;&#x304A;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verafschuwen</trd>,
    <trd>verfoeien</trd>
  </trdgrp>
  <trdgrp lng="pl">
