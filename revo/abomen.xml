<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>abomen</rad>/i
</kap>
<drv mrk="abomen.0i">
    <kap><ofc>*</ofc><tld/>i</kap>
    <gra><vspec>tr</vspec></gra>
    <snc>
      <dif>
        Malamegi pro fizike na&ubreve;zaj a&ubreve; morale  malpla&ccirc;egaj
        ecoj:
        <ekz>
          <tld/>i odoron, la ostrojn, la mensogon, la militon;
        </ekz>
        <ekz>
          <tld/>i trinki akvon
          <fnt>
            (<aut>Zamenhof</aut>:
            <vrk>Biblio</vrk>,
            <lok>Eliro 7, 18</lok>)
          </fnt>;
        </ekz>
        <ekz>
          ni deziras forigi de ni tion, kion ni <tld/>as;
        </ekz>
        <ekz>
          senti <ind><tld/>o</ind>n al, anta&ubreve;, kontra&ubreve; iu, io.
          <trdgrp lng="fr">
            <trd>d&eacute;go&ucirc;t</trd>,
            <trd>aversion</trd>,
            <trd>r&eacute;pulsion</trd></trdgrp> 
        </ekz>
      </dif>
    </snc>
    <trd lng="de">verabscheuen</trd>
    <trd lng="hu">ut&aacute;l</trd>
    <trdgrp lng="nl">
      <trd>verafschuwen</trd>,
      <trd>verfoeien</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>abominate</trd>,
      <trd>abhor</trd>,
      <trd>loathe</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>abominer</trd>,
      <trd>avoir en <ind>horreur</ind></trd></trdgrp>
    <trd lng="ru">&#1087;&#1080;&#1090;&#1072;&#1090;&#1100;
&#1086;&#1090;&#1074;&#1088;&#1072;&#1097;&#1077;&#1085;&#1080;&#1077;</trd>
    <trd lng="sv">avsky</trd>
</drv>
<drv mrk="abomen.0a">
    <kap><tld/>a<fnt>Z</fnt></kap>
    <snc>
      <dif>
        Tia, ke &gcirc;i, li, &scirc;i ka&ubreve;zas <tld/>on:
        <ekz>
          kia <tld/>a urbeto!<fnt>Z</fnt>
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="de">
      <trd>scheu&szlig;lich</trd>,
      <trd>abscheulich</trd></trdgrp>
    <trdgrp lng="en">
      <trd>abominable</trd>,
      <trd>detestable</trd>,
      <trd>loathsome</trd>,
      <trd>odious</trd>,
      <trd>revolting</trd>
    </trdgrp>
    <trd lng="hu">ut&aacute;l</trd>
    <trd lng="nl">afschuwelijk</trd>
    <trdgrp lng="fr">
      <trd>abominable</trd>,
      <trd>d&eacute;go&ucirc;tant</trd>,
      <trd>horrible</trd></trdgrp>
    <trd
lng="ru">&#1086;&#1090;&#1074;&#1088;&#1072;&#1090;&#1080;&#1090;&#1077;&#1083;&#1100;&#1085;&#1099;&#1081;</trd>
</drv>
<drv mrk="abomen.0inda">
    <kap><tld/>inda<fnt>Z</fnt></kap>
    <snc>
      <dif>
        Inda je <tld/>o; estanta tiel fia, ke oni devas &gcirc;in <tld/>i:
        <ekz>
          lepro kovris mian korpon per <tld/>inda &scirc;elo<fnt>Z</fnt>;
        </ekz>
        <ekz>
          vi <tld/>indigis vian belecon
          <fnt>
            (<aut>Zamenhof</aut>:
            <vrk>Biblio</vrk>,
            <lok>Je&hcirc;ezkel 16, 25</lok>)
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="antipa.0o">antipatio</ref>.
      </refgrp>
    </snc>
    <trd lng="hu">ut&aacute;lnival&oacute;</trd>
    <trdgrp lng="nl">
      <trd>afschuwelijk</trd>,
      <trd>ijselijk</trd>,
      <trd>afgrijselijk</trd>,
      <trd>verfoeilijk</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>verabscheuensw&uuml;rdig</trd>,
      <trd>verabscheuenswert</trd></trdgrp>
    <trd lng="fr">abominable</trd>
    <trd lng="it">abominevole</trd>
    <trd
lng="ru">&#1086;&#1090;&#1074;&#1088;&#1072;&#1090;&#1080;&#1090;&#1077;&#1083;&#1100;&#1085;&#1099;&#1081;</trd>
    <trd lng="sv">avskyv&auml;rd</trd>
</drv>
</art>
