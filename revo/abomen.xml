<drv mrk="abomen.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malamegi pro fizike na&ubreve;zaj a&ubreve; morale
      malpla&ccirc;egaj ecoj:
      <ekz>
        <tld/>i odoron, la mensogon, la militon;
      </ekz>
      <ekz>
        <tld/>i trinki akvon el la rivero
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 7:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>as min, malproksimi&gcirc;as de mi, ne timas kra&ccirc;i sur mian viza&gcirc;on
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 30:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu homo <tld/>as sian staton kaj estas la plej malfeli&ccirc;a
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        homo iafoje <tld/>as sin mem
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i posedis tiun fierecon, kiu <tld/>as timon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mia animo <tld/>as la subtilajn intrigojn kaj la malnoblajn konspirojn
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni perfidon prenas, sed perfidulon <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="estim.mal0i">malestimi</ref>,
      <ref cel="respek.mal0i">malrespekti</ref>,
      <ref cel="sxat.mal0i">mal&scirc;ati</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_c;&c_mol;
  <ind>&c_a;&c_g;&c_ib;&c_d;&c_u;</ind></trd>
  <trdgrp lng="bg">
    <trd>&c_m;&c_r;&c_a;&c_z;&c_ja;</trd>,
    <trd>&c_n;&c_je;&c_n;&c_a;&c_v;&c_i;&c_zh;&c_d;&c_a;&c_m;</trd>,
    <trd>&c_o;&c_t;&c_v;&c_r;&c_a;&c_shch;&c_a;&c_v;&c_a;&c_m;
    &c_s;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>heugi&ntilde;</trd>,
    <trd>do&ntilde;jeri&ntilde;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>abominar</trd>,
    <trd>execrar</trd>,
    <trd>detestar</trd>,
    <trd>odiar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hnusit si</trd>,
    <trd>protivit si n&ecaron;co</trd>
  </trdgrp>
  <trd lng="da">afsky</trd>
  <trd lng="de">verabscheuen</trd>
  <trdgrp lng="en">
    <trd>abominate</trd>,
    <trd>abhor</trd>,
    <trd>loathe</trd>
  </trdgrp>
  <trd lng="es">abominar</trd>
  <trdgrp lng="fa">
    <trd>&a_n;&a_f;&a_r;&a_t; &a_d;&a_A;&a_sh;&a_t;&a_n;</trd>,
    <trd>&a_m;&a_t;&a_n;&a_f;&a_r; &a_b;&a_w;&a_d;&a_n;</trd>,
    <trd>&a_b;&f_y;&a_z;&a_A;&a_r; &a_b;&a_w;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
