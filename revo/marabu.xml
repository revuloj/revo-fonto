<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>marabu</rad>/o
</kap>
<drv mrk="marabu.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="cikoni.0edoj">cikoniedoj</ref>
      (<trd lng="la">Leptoptilos</trd>),
      kun senhara kapo kaj ha&ubreve;ta gor&gcirc;sako,
      kaj kun po unu specio en Afriko, Hindujo kaj Sunda-insuloj.
    </dif>
  </snc>
  <trd lng="de">Marabu</trd>
  <trd lng="en">marabou stork</trd>
</drv>
</art>
