<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>marabu</rad>/o
</kap>
<drv mrk="marabu.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="cikoni1.0oj">cikoniedoj</ref>
      (<trd lng="la">Leptoptilos</trd>),
      kun senhara kapo kaj ha&ubreve;ta gor&gcirc;sako,
      kaj kun po unu specio en Afriko, Hindujo kaj Sunda-insuloj.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_a;&c_b;&c_u;</trd>
  <trd lng="de">Marabu</trd>
  <trd lng="en">marabou stork</trd>
  <trd lng="hu">marabu</trd>
  <trd lng="nl">maraboe</trd>
  <trdgrp lng="pt">
    <trd>marabu</trd>,
    <trd>cordufal</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_r;&c_a;&c_b;&c_u;</trd>,
    <trd>&c_z;&c_o;&c_b;&c_a;&c_t;&c_y;&c_j; &c_a;&c_i;&c_s;&c_t;</trd>
  </trdgrp>
</drv>
</art>
