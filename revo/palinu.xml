<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>palinur</rad>/o
</kap>
<drv mrk="palinu.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="palinu.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Palinurus</trd>), man&gcirc;ebla krustulo.
    </dif>
    <ref tip="sin" cel="langus.0o">langusto</ref>
  </snc>
  <trd lng="de">Languste</trd>
</drv>
<drv mrk="palinu.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="krust.dekpiedaj0uloj">dekpiedaj 
      krustuloj</ref> (<trd lng="la">Palinuridae</trd>),
      sen pin&ccirc;iloj kaj kun dika, malmola karapaco.
    </dif>
  </snc>
</drv>
</art>
