<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: palinu.xml,v 1.26 2023/09/03 12:45:03 revo Exp $">
<kap>
  <ofc>2</ofc><rad>palinur</rad>/o
</kap>

<drv mrk="palinu.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="palinu1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Palinurus</trd>), man&gcirc;ebla <ref tip="lst" cel="krust.0ulo" lst="voko:krustuloj">krustulo</ref>:
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> ne havas pin&ccirc;ilojn, &gcirc;i estas man&gcirc;ebla kaj
        tre bongusta, &gcirc;i vivas en Mediteraneo kaj en Atlantiko.
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=56128">Palinuro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="langus.0o">langusto</ref>
    <bld lok="&WCU;/thumb/6/63/Palinurus_vulgaris_Latreille%2C_1804.jpg/464px-Palinurus_vulgaris_Latreille%2C_1804.jpg" prm="CC0"><tld/>o (Palinurus vulgaris)
        <fnt>
          <aut>Parent G&eacute;ry, propra verko</aut>
          <lok><url ref="&WCW;/File:Palinurus_vulgaris_Latreille,_1804.jpg">&WCW;/File:Palinurus_vulgaris_Latreille,_1804.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_n;&c_g;&c_u;&c_s;&c_t;&c_a;</trd>
  <trd lng="bg">&c_l;&c_a;&c_n;&c_g;&c_u;&c_s;&c_t;&c_a;</trd>
  <trd lng="cs">langusta</trd>
  <trd lng="de">Languste</trd>
  <trdgrp lng="en">
    <trd>spiny lobster</trd>,
    <trd>rock lobster</trd>,
    <trd>sea crayfish</trd>,
    <trd>langouste</trd>
  </trdgrp>
  <trd lng="hu">languszta</trd>
  <trd lng="ja">&#x30A4;&#x30BB;&#x30A8;&#x30D3;</trd>
  <trd lng="nl">langoest</trd>
  <trd lng="pl">langusta</trd>
  <trdgrp lng="pt">
    <trd>lagosta</trd>,
    <trd>palinuro</trd>
  </trdgrp>
  <trd lng="ro">langust&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_n;&c_g;&c_u;&c_s;&c_t;</trd>,
    <trd>&c_l;&c_a;&c_n;&c_g;&c_u;&c_s;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">langusta</trd>
  <trd lng="uk">&#x43B;&#x430;&#x43D;&#x433;&#x443;&#x441;&#x442;(&#x430;)</trd>
</drv>
</art>
