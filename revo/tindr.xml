<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>tindr</rad>/o
</kap>
<drv mrk="tindr.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Spongeca, tre brulema substanco, devenanta de iuj fungoj. 
      <ekz>
        Trovinte per palpado silikon kaj <tld/>on, li bruligis kenan tor&ccirc;on kaj prilumis per &gcirc;i la akvujon.
        <fnt><bib>Lusin</bib>, <lok>El Malnovaj legendoj nove redaktitaj, For&gcirc;ado de spado</lok></fnt>.
      </ekz>
    </dif>
  </snc> 
  <trdgrp lng="be">
    <trd>&c_c;&c_e;&c_r;&c_a;</trd>,
    <trd>&c_g;&c_u;&c_b;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Zunder</trd>
  <trd lng="fr">amadou</trd>
  <trd lng="hu">tapl&oacute;</trd>
  <trd lng="nl">tondelzwam</trd>
  <trdgrp lng="pt">
    <trd>isca</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_u;&c_t;</trd>
</drv>
</art>
