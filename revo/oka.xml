<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Oka</rad>/o
    </kap>
    <drv mrk="oka.O0o">
      <kap><tld/>o</kap>
      <snc mrk="oka.O0o.Euxropa">
	<uzo tip="fak">GEOG</uzo>
	<dif>
	  <ref tip="super" cel="river.0o">Rivero</ref> en e&ubreve;ropa
          <ref tip="malprt" cel="rus.R0ujo">Rusujo</ref>,
	  1480 km longa, la plej granda dekstra alfluanto de
	  <ref cel="volg.0o">Volgo</ref>.
	</dif>
	<trd lng="de">Oka</trd>
	<trd lng="en">Oka</trd>
	<trd lng="hu">Oka</trd>
        <trd lng="pt">Oka</trd>
      </snc>
      <snc mrk="oka.O0o.Siberia">
	<uzo tip="fak">GEOG</uzo>
	<dif>
	  <ref tip="super" cel="river.0o">Rivero</ref> en sudorienta
	  <ref tip="malprt" cel="siberi.S0o">Siberio</ref>, 630 km longa,
	  maldekstra alfluanto de <ref cel="angar.A0o">Angaro</ref>.
	</dif>
   	  <trd lng="de">Oka</trd>
          <trd lng="hu">Oka</trd>
      </snc>
      <trd lng="be">&c_A;&c_k;&c_a;</trd>
      <trd lng="nl">Oka</trd>
      <trd lng="ru">&c_O;&c_k;&c_a;</trd>
    </drv>

  </art>

