  </trdgrp>
  <trdgrp lng="en">
    <trd>stay</trd>,
    <trd>guy</trd>
  </trdgrp>
  <trd lng="fr">hauban</trd>
  <trdgrp lng="de">
    <trd>Stag</trd>,
    <trd>Halteseil</trd>,
    <trd>Abspannseil</trd>,
    <trd>Wanten</trd>
