<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>staj</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="staj.0o">
  <kap><tld/>o</kap>
  <snc mrk="staj.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      &Scirc;nurego, subtenanta
      <ref tip="vid" cel="mast.0o.MAR">mastojn</ref>
      flanke, anta&ubreve;e a&ubreve; malanta&ubreve;e, kaj fiksata al 
      &scirc;iprandoj:
      <ekz>
        li staris tenante sin per la <tld/>oj, parolis al si mem kaj 
        &gcirc;emis la&ubreve;te kiam ajn la &scirc;ipo marteladis sur la rokon
        <fnt><aut>R. L. Stevenson, trad. A. Goodheir</aut>, 
        <vrk>Forkaptita</vrk>, <lok>Kardo, 1976</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Stag</trd>,
    <trd>Halteseil</trd>,
    <trd>Abspannseil</trd>,
    <trd>Wanten</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>stay</trd>,
    <trd>guy</trd>
  </trdgrp>
  <trd lng="fr">hauban</trd>
  <trd lng="pl">sztag</trd>
  <trdgrp lng="pt">
    <trd>estai</trd>,
    <trd>tirante</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_t;&c_ja;&c_zh;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_ch;&c_a;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_a;&c_n;&c_t;&c_a;</trd>,
    <trd>&c_sh;&c_t;&c_a;&c_g;</trd>
  </trdgrp>
</drv>
</art>

