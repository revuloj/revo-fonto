<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: staj.xml,v 1.16 2025/05/29 20:00:21 revo Exp $">
<kap>
  <rad>staj</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="staj.0o">
  <kap><tld/>o</kap>
  <snc mrk="staj.0o.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      &Scirc;nurego, subtenanta
      <ref tip="vid" cel="mast.0o.MAR">mastojn</ref>
      flanke, anta&ubreve;e a&ubreve; malanta&ubreve;e, kaj fiksata al
      &scirc;iprandoj; <ref tip="sin" cel="vant1.0o.SHI">vanto</ref>:
      <ekz>
        li staris tenante sin per la <tld/>oj, parolis al si mem kaj
        &gcirc;emis la&ubreve;te kiam ajn la &scirc;ipo marteladis sur la rokon
        <fnt><aut>R. L. Stevenson, trad. A. Goodheir</aut>,
        <vrk>Forkaptita</vrk>, <lok>1976</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_zh;&c_k;&c_a; <klr>(&c_p;&c_a;&c_m;&c_ib;&c_zh;
    &c_sh;&c_ch;&c_o;&c_g;&c_l;&c_a;&c_m;&c_ib;)</klr></trd>,
    <trd>&c_v;&c_a;&c_n;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">estai</trd>
  <trdgrp lng="cs">
    <trd>nap&iacute;nac&iacute; lano sto&zcaron;&aacute;ru</trd>,
    <trd>v&yacute;ztu&zcaron;n&eacute; lano</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Stag</trd>,
    <trd>Halteseil</trd>,
    <trd>Abspannseil</trd>,
    <trd>Wanten</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>stay</trd>,
    <trd>guy</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>tirante</trd>,
    <trd>estay</trd>
  </trdgrp>
  <trd lng="fr">hauban</trd>
  <trdgrp lng="ja">
    <trd>&#25903;&#32034;  <pr>&#12375;&#12373;&#12367;</pr></trd>,
    <trd>&#12473;&#12486;&#12540;</trd>
  </trdgrp>
  <trd lng="pl">sztag</trd>
  <trdgrp lng="pt">
    <trd>estai</trd>,
    <trd>tirante</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_t;&c_ja;&c_zh;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_ch;&c_a;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_a;&c_n;&c_t;&c_a;</trd>,
    <trd>&c_sh;&c_t;&c_a;&c_g;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nap&iacute;nacie lano</trd>,
    <trd>v&yacute;stu&zcaron;n&eacute; lano</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_o;&c_p;&c_o;&c_r;&c_a;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_p;&c_ib;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_t;&c_ja;&c_zh;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_ib;&c_p;&c_a;</trd>,
    <trd>&c_l;&c_ju;&c_n;&c_je;&c_t;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_s;</trd>,
    <trd>&c_v;&c_a;&c_n;&c_t;&c_a;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_ch;&c_a;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_p;&c_o;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#23433;&#20840;&#32034; <pr>&#257;nqu&aacute;nsu&#466;</pr></trd>
</drv>
