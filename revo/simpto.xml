    <trd>&#30149;&#24449; <pr>b&igrave;ngzh&emacron;ng</pr></trd>,
    <trd>&#30149;&#24501; <pr>b&igrave;ngzh&emacron;ng</pr></trd>,
    <trd>&#30149;&#29366; <pr>b&igrave;ngzhu&agrave;ng</pr></trd>,
