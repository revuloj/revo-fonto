<drv mrk="simpto.manko0o">
  <kap>
    manko<tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="S"/>o kiu aperas pro manko de iu nutra&jcirc;o:
      <ekz>
        kvantodiferencoj de tiuj mikroelementoj rezultas en manko<tld/>oj a&ubreve;
        troabund-<tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=520871"
              >Nutra&jcirc;oj (de planto)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_m;&c_p;&c_t;&c_o;&c_m; &c_n;&c_ja;&c_s;&c_t;&c_a;&c_ch;&c_y; <klr>(&c_p;&c_a;&c_zh;&c_y;&c_w;&c_n;&c_y;&c_h; &c_r;&c_e;&c_ch;&c_y;&c_v;&c_a;&c_w;)</klr></trd>
  <trd lng="de">Mangelerscheinung</trd>
  <trd lng="en">deficiency symptom</trd>
</drv>

