<drv mrk="simpto.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="simpto.0aro.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Tuto de <tld/>oj karakterizaj de iu malsano:
      <ekz>
        priinteligenta handikapulo nek povas, ofte, alporti nin siajn
        detalojn biografiajn, nek scipovas rakonti al ni detale sian
        <tld/>aron
        <fnt>
          <aut>J. Pina Tuells</aut>,
          <vrk><url ref="http://thor.prohosting.com/jesuo/k44novaj.htm">Nova
          konceptoj kaj anta&ubreve;ju&gcirc;oj...</url></vrk>,
          <lok>Kajeroj el la Sudo, 1999-10, numero 44a</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_k;&c_u;&c_p;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol; &c_s;&c_y;&c_m;&c_p;&c_t;&c_o;&c_m;&c_a;&c_w;</trd>,
    <trd>&c_s;&c_y;&c_n;&c_d;&c_r;&c_o;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>soubor p&rcaron;&iacute;znak&uring;</trd>,
    <trd>syndrom</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Syndrom</trd>,
    <trd>Krankheitsbild</trd>
  </trdgrp>
  <trd lng="es">s&iacute;ndrome</trd>
  <trdgrp lng="fa">
    <trd>&a_n;&a_sh;&a_A;&a_n;&f_g;&a_A;&a_n;</trd>,
    <trd>&a_s;&a_fatha;&a_n;&a_d;&a_r;&a_w;&a_m;</trd>
