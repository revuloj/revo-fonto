<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>he&gcirc;</rad>/o
</kap>
<drv mrk="hegx.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Barilo konsistanta el vivantaj arbetoj, kies bran&ccirc;oj interplekti&gcirc;as.
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_y;&c_v;&c_a;&c_ja; &c_z;&c_a;&c_g;&c_a;&c_r;&c_a;&c_d;&c_z;&c_mol;</trd>
  <trd lng="de">Hecke</trd>
  <trd lng="en">hedge</trd>
  <trd lng="fr">haie</trd>
  <trdgrp lng="hu">
    <trd>&eacute;l&odblac; <ind>s&ouml;v&eacute;ny</ind></trd>,
    <trd>s&ouml;v&eacute;nyker&iacute;t&eacute;s</trd>
  </trdgrp>
  <trd lng="nl">haag</trd>
  <trd lng="pt">cerca viva</trd>
  <trd lng="ru">&c_zh;&c_i;&c_v;&c_a;&c_ja; &c_i;&c_z;&c_g;&c_o;&c_r;&c_o;&c_d;&c_mol;</trd>
</drv>

<drv mrk="hegx.0ado">
  <kap><tld/>ado</kap>
  <fnt><bib>IKEV</bib></fnt>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Kontraktado de transakcioj sur la templima merkato kun la celo elimini la riskon de prezfluktuoj.
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_e;&c_d;&c_zh;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="en">hedging</trd>
  <trdgrp lng="de">
     <trd>Deckungsgesch&auml;ft</trd>,
     <trd>Hedgegesch&auml;ft</trd>,
     <trd>Hedging</trd>
  </trdgrp>
  <trd lng="hu">&aacute;rfolyam-fedezeti &uuml;gylet k&ouml;t&eacute;se</trd>
  <trd lng="it">riporto</trd>
</drv>

<drv mrk="hegx.0anto">
  <kap><tld/>anto</kap>
  <fnt><bib>IKEV</bib></fnt>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Persono kiu faras he&gcirc;adon.
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_e;&c_d;&c_zh;&c_e;&c_r;</trd>
  <trd lng="en">hedger</trd>
  <trdgrp lng="de">
     <trd>Absicherer</trd>,
     <trd>Hedger</trd>
  </trdgrp>
  <trd lng="hu">fedezeti &uuml;gyletk&ouml;t&odblac;</trd>
  <trd lng="it">riportatore</trd>
</drv>

</art>
