<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>he&gcirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="hegx.0o">
  <kap><tld/>o</kap>
  <snc mrk="hegx.0o.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      <ref tip="super" cel="bar.0ilo.HOR">Barilo</ref> konsistanta el
      vivantaj arbetoj, kies bran&ccirc;oj interplekti&gcirc;as.
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_y;&c_v;&c_a;&c_ja;
  &c_z;&c_a;&c_g;&c_a;&c_r;&c_a;&c_d;&c_z;&c_mol;</trd>
  <trd lng="bg">&c_zh;&c_i;&c_v; &c_p;&c_l;&c_je;&c_t;</trd>
  <trd lng="br">garzh</trd>
  <trd lng="de">Hecke</trd>
  <trd lng="en">hedge</trd>
  <trd lng="es">seto</trd>
  <trdgrp lng="fa">
    <trd>&f_p;&a_r;&f_ch;&f_y;&a_n;</trd>,
    <trd>&f_ch;&f_p;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">haie</trd>
  <trdgrp lng="hu">
    <trd>&eacute;l&odblac; <ind>s&ouml;v&eacute;ny</ind></trd>,
    <trd>s&ouml;v&eacute;nyker&iacute;t&eacute;s</trd>
  </trdgrp>
  <trd lng="it">siepe</trd>
  <trd lng="nl">haag</trd>
  <trd lng="pl">&zdot;ywop&lstroke;ot</trd>
  <trd lng="pt">cerca viva</trd>
  <trd lng="ru">&c_zh;&c_i;&c_v;&c_a;&c_ja;
  &c_i;&c_z;&c_g;&c_o;&c_r;&c_o;&c_d;&c_mol;</trd>
</drv>

<drv mrk="hegx.0ado">
  <kap><tld/>ado</kap>
  <fnt><bib>IKEV</bib></fnt>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Kontraktado de transakcioj sur la templima merkato kun la celo
      elimini la riskon de prezfluktuoj.
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_e;&c_d;&c_zh;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="de">
    <trd>Deckungsgesch&auml;ft</trd>,
    <trd>Hedgegesch&auml;ft</trd>,
    <trd>Hedging</trd>
  </trdgrp>
  <trd lng="en">hedging</trd>
  <trd lng="fa">&a_m;&a_ayn;&a_A;&a_m;&a_l;&a_h;&a_hamza_sure; &a_t;&a_A_hamza_sure;&a_m;&f_y;&a_n;&f_y;</trd>
  <trd lng="hu">&aacute;rfolyam-fedezeti &uuml;gylet
  k&ouml;t&eacute;se</trd>
  <trd lng="it">hedging</trd>
</drv>

<drv mrk="hegx.0anto">
  <kap><tld/>anto</kap>
  <fnt><bib>IKEV</bib></fnt>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Persono kiu faras he&gcirc;adon.
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_e;&c_d;&c_zh;&c_e;&c_r;</trd>
  <trd lng="en">hedger</trd>
  <trdgrp lng="de">
    <trd>Absicherer</trd>,
    <trd>Hedger</trd>
  </trdgrp>
  <trd lng="hu">fedezeti &uuml;gyletk&ouml;t&odblac;</trd>
</drv>
</art>
