<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hegx.xml,v 1.33 2023/11/15 16:45:18 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>he&gcirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="hegx.0o">
  <kap><tld/>o</kap>
  <snc mrk="hegx.0o.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      Densa vico de <ref tip="prt" cel="arb.0eto">arbetoj</ref>, regule tondata,
      uzata kiel <ref tip="super" cel="bar.0ilo.HOR">barilo</ref>:
      <ekz>
        ka&scirc;ita malanta&ubreve; densa <tld/>o trovi&gcirc;as belega &gcirc;ardeno
        <fnt>
          <bib>ChR</bib>
          <lok>La fantoma bovino</lok>
        </fnt>;
      </ekz>
      <ekz>
        lar&gcirc;a marmora &scirc;tuparo inter la ebriige aromaj roz<tld/>oj
        <fnt>
          <bib>MkM</bib>
          <lok>2</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;ardeno kun <tld/>oj perfekte tonditaj
        <fnt>
          <bib>StU</bib>
          <lok>Al Romo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sambuko <klr>[estas]</klr> tre ofte videbla arbo en la kampar<tld/>oj
        <fnt>
          <aut>M&aacute;ire NicAoidh</aut>
          <vrk><url ref="&Monato;/2000/006878.php">&Gcirc;ardenumi</url></vrk>
          <bib>Monato</bib>
          <lok>2000/04, p. 20</lok>
          </fnt>;
      </ekz>
      <ekz>
         turfalkino<klr>[n]</klr>, kiu rompetis al si flugilon, ni trovis <klr>[&#x2026;]</klr> en <tld/>o
         de nia &gcirc;ardeno
         <fnt>
          <bib>Monato</bib>
          <aut>Walter, Renate, Irene, Stefan Klag</aut>
          <vrk><url ref="&Monato;/2007/009167.php">Birdo de la jaro</url></vrk>
         </fnt>;
      </ekz>
      <ekz>
        hodia&ubreve; ni parolis trans la <tld/>o, kiam ni amba&ubreve; estis en la &gcirc;ardeno
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2009/009682.php">La Pendantaj &Gcirc;ardenoj estis son&gcirc;o</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la ne&gcirc;o ku&scirc;is genuprofunde, kaj de la roz&gcirc;ardeno restis nur nigraj dorn<tld/>oj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Sinjorino Ajuolo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="arb.0etaro">arbetaro</ref>,
      <ref cel="bosk.0o">bosko</ref>,
      <ref cel="vepr.0o">vepro</ref>
    </refgrp>
    <bld lok="&WCU;/thumb/7/70/A_wide_hedge_-_geograph.org.uk_-_2362142.jpg/463px-A_wide_hedge_-_geograph.org.uk_-_2362142.jpg" prm="CC BY-SA 2.0">lar&gcirc;a he&gcirc;o inter du pa&scirc;tejoj
        <fnt>
          <aut>David Lally, Geograph Britain and Ireland&nbsp;</aut>
          <lok><url ref="&WCW;/File:A_wide_hedge_-_geograph.org.uk_-_2362142.jpg">&WCW;/File:A_wide_hedge_-_geograph.org.uk_-_2362142.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_zh;&c_y;&c_v;&c_a;&c_ja;
  &c_z;&c_a;&c_g;&c_a;&c_r;&c_a;&c_d;&c_z;&c_mol;</trd>
  <trd lng="bg">&c_zh;&c_i;&c_v; &c_p;&c_l;&c_je;&c_t;</trd>
  <trd lng="br">garzh</trd>
  <trd lng="de">Hecke</trd>
  <trd lng="en">hedge</trd>
  <trd lng="es">seto</trd>
  <trdgrp lng="fa">
    <trd>&f_p;&a_r;&f_ch;&f_y;&a_n;</trd>,
    <trd>&f_ch;&f_p;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">haie</trd>
  <trdgrp lng="hu">
    <trd>&eacute;l&odblac; <ind>s&ouml;v&eacute;ny</ind></trd>,
    <trd>s&ouml;v&eacute;nyker&iacute;t&eacute;s</trd>
  </trdgrp>
  <trd lng="it">siepe</trd>
  <trd lng="ja">&#x751F;&#x3051;&#x57A3;  <pr>&#x3044;&#x3051;&#x304C;&#x304D;</pr></trd>
  <trd lng="nl">haag</trd>
  <trd lng="pl">&zdot;ywop&lstroke;ot</trd>
  <trd lng="pt">cerca viva</trd>
  <trd lng="ru">&c_zh;&c_i;&c_v;&c_a;&c_ja;
  &c_i;&c_z;&c_g;&c_o;&c_r;&c_o;&c_d;&c_mol;</trd>
  <trd lng="uk">&c_zh;&c_i;&c_v;&c_o;&c_p;&c_l;&c_ib;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#x6835; <pr>l&#xEC;</pr></trd>,
    <trd>&#x6811;&#x7BF1; <pr>sh&#xF9;l&#xED;</pr></trd>,
    <trd>&#x67F5;&#x7C6C; <pr>zh&#xE0;l&#xED;</pr></trd>,
    <trd>&#x7BF1;&#x57A3; <pr>l&#xED;yu&#xE1;n</pr></trd>
  </trdgrp>
</drv>


</art>
