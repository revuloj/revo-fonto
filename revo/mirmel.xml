<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>mirmeleon</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="mirmel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="mirmel.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Myrmeleon</trd>).
      La larvoj de iu specio (<trd lng="la">Myrmeleon formicarius</trd>)
      konstruas en sablo funelojn por kapti formikojn.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Ameisenjungfer</trd>,
    <trd>Ameisenl&ouml;we <klr>(larvo)</klr></trd>
  </trdgrp>
</drv>
<drv mrk="mirmel.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo
      <ref tip="super" cel="flug.ret0iluloj">retflugiluloj</ref>
      (<trd lng="la">Myrmeleonidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Ameisenjungfern <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>
