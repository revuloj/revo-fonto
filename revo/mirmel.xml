<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mirmeleon</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="mirmel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="mirmel2.0oj"><tld/>edoj</ref>
      (<trd lng="la">Myrmeleon</trd>).
      La larvoj de iu specio (<trd lng="la">Myrmeleon formicarius</trd>)
      konstruas en sablo funelojn por kapti formikojn.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Ameisenjungfer</trd>,
    <trd>Ameisenl&ouml;we <klr>(larvo)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd> ant lion</trd>,
    <trd>doodle bug</trd>
  </trdgrp>
  <trd lng="nl">mierenleeuw</trd>
  <trd lng="ru">&c_m;&c_u;&c_r;&c_a;&c_v;&c_mol;&c_i;&c_n;&c_y;&c_j; &c_l;&c_je;&c_v;</trd>
</drv>
</art>
