<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mirmeleon</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="mirmel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="mirmel2.0oj"><tld/>edoj</ref>
      (<trd lng="la">Myrmeleon</trd>).
      La larvoj de iu specio (<trd lng="la">Myrmeleon formicarius</trd>)
      konstruas en sablo funelojn por kapti formikojn.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_r;&c_a;&c_sh;&c_y;&c_n;&c_y; <ind>&c_l;&c_je;&c_w;</ind></trd>
  <trdgrp lng="de">
    <trd>Ameisenjungfer</trd>,
    <trd>Ameisenl&ouml;we <klr>(larvo)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd> ant lion</trd>,
    <trd>doodle bug</trd>
  </trdgrp>
  <trd lng="es">hormiga le&oacute;n</trd>
  <trd lng="fr">fourmi-lion</trd>
  <trd lng="hu">hangyales&oacute;</trd>
  <trd lng="nl">mierenleeuw</trd>
  <trd lng="pt">formiga-le&atilde;o</trd>
  <trd lng="ru">&c_m;&c_u;&c_r;&c_a;&c_v;&c_mol;&c_i;&c_n;&c_y;&c_j; &c_l;&c_je;&c_v;</trd>
  <trd lng="sk">mravcolev</trd>
</drv>
</art>
