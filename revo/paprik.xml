<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>paprik</rad>/o<fnt>Z</fnt></kap>
<drv mrk="paprik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc mrk="paprik.0o.frukto">
    <dif>
      Frukto de <ref tip="malprt" cel="kapsik.0o">kapsiko</ref>,
      uzata kiel legomo a&ubreve; spica&jcirc;o.
    </dif>
    <trdgrp lng="en">
      <trd>chili</trd>,
      <trd>chilli</trd>,
      <trd>chilie</trd>,
      <trd>pepper</trd>    
    </trdgrp>
    <trd lng="sv">paprika</trd>
  </snc>
  <snc mrk="paprik.0o.pulvoro">
    <dif>
      Pulvoro farita el sekigita <tld/>o <sncref ref="paprik.0o.frukto"/>
    </dif>
    <refgrp tip="sin">
      <ref cel="pipr.rugxa0o">ru&gcirc;a pipro</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>paprika</trd>
  </trdgrp>
  <trd lng="de">Paprika</trd>
  <trdgrp lng="en">
    <trd>red pepper</trd>,
    <trd>paprika</trd>
  </trdgrp>
  <trd lng="hu">paprika</trd>
  <trd lng="nl">paprika</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_p;&c_r;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_s;&c_n;&c_y;&c_j; <ind>&c_p;&c_je;&c_r;&c_je;&c_c;</ind></trd>
  </trdgrp>
</drv>
</art>
