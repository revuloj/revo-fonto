<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>9</ofc> <rad>paprik</rad>/o <fnt>Z</fnt></kap>
<drv mrk="paprik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc mrk="paprik.0o.pulvoro">
    <dif>
      <ref cel="spic.0o.KUI">Spico</ref> (ru&gcirc;a, bruna a&ubreve; flava)
      preparata el sekigita kaj pulvorigita frukto de iuj specoj de
      <ref cel="kapsik.0o.BOT">kapsiko</ref>:
      <ekz>hungara papriko</ekz>;
      <ekz>dol&ccirc;a papriko</ekz>;
      <ekz>akra papriko</ekz>.
    </dif>
    <ref tip="sin" cel="pipr.rugxa0o">ru&gcirc;a pipro</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_p;&c_r;&c_y;&c_k;&c_a;</trd>,
    <trd>&c_ch;&c_y;&c_r;&c_v;&c_o;&c_n;&c_y;
    <ind>&c_p;&c_je;&c_r;&c_a;&c_c;</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>paprika</trd>
  </trdgrp>
  <trd lng="de">Paprika</trd>
  <trdgrp lng="en">
    <trd>red pepper</trd>,
    <trd>paprika</trd>
  </trdgrp>
  <trd lng="hu">paprika</trd>
  <trd lng="nl">paprika</trd>
  <trd lng="pl">papryka</trd>
  <trdgrp lng="pt">
    <trd>p&aacute;prica</trd>,
    <trd>colorau</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_p;&c_r;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_s;&c_n;&c_y;&c_j;
    <ind>&c_p;&c_je;&c_r;&c_je;&c_c;</ind></trd>
  </trdgrp>
</drv>
</art>
