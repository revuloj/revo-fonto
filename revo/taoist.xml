<art mrk="$Id: taoist.xml,v 1.19 2023/10/21 13:02:26 revo Exp $">
<kap>
   <rad>taoist</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="taoist.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">REL</uzo>
   <snc>
     <dif>
      <ref tip="dif" cel="daux.0isto">Da&ubreve;isto</ref>:
      <ekz>
        vivante pli ol 100 jarojn kiel <tld/>o, li spertis tri malsamajn re&gcirc;imojn: la
        <nom>Qing</nom>-an imperion, la Respublikon de &Ccirc;inio kaj la Popolan Respublikon de &Ccirc;inio
        <fnt>
          <aut>Sabeo Fu</aut>
            <vrk><url ref="&Monato;/2000/007127.php">Escepto-recepto</url></vrk>
            <bib>Monato</bib>
            <lok>2000/11, p. 14</lok>
          </fnt>.
      </ekz>
    </dif>
   </snc>
   <rim>
     &leftquot;Tao&rightquot; &leftquot;taoismo&rightquot; kaj &leftquot;<ref tip="hom" cel="daux.0isto">taoisto</ref>&rightquot; 
     estas pli uzataj, sed malpli regulaj formoj ol &leftquot;da&ubreve;o&rightquot;, &leftquot;da&ubreve;ismo&rightquot;, &leftquot;da&ubreve;isto.
     <aut>&jcirc;v</aut>
   </rim>
   <trd lng="ca">taoista</trd>
   <trdgrp lng="de">
    <trd>Daoist</trd>,
    <trd>Taoist</trd>
  </trdgrp>
  <trd lng="fr">tao&iuml;ste</trd>
   <trd lng="hu">taoista</trd>
   <trd lng="ja">&#x9053;&#x58EB;  <pr>&#x3069;&#x3046;&#x3057;</pr></trd>
  <trd lng="pl">taoista</trd>
   <trd lng="pt">taoista</trd>
   <trd lng="ru">&c_d;&c_a;&c_o;&c_s;</trd>
  <trd lng="sk">taoista</trd>
  <trd lng="cs">taoista</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_a;&#x43E;&c_s;&c_i;&c_s;&c_t;</trd>,
    <trd>&c_d;&c_a;&#x43E;&c_ji;&c_s;&c_t;</trd>
  </trdgrp>
</drv>
</art>
