<art mrk="$Id: rodezi.xml,v 1.9 2024/04/14 05:47:06 revo Exp $">
<kap>
    <rad>Rodezi</rad>/o <fnt><bib>PIV3</bib></fnt>
</kap>
<drv mrk="rodezi.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
     Suda parto de brita <ref tip="super" cel="koloni.0o.lando">kolonio</ref>,
     kiu 1980 fari&gcirc;is <ref tip="dif" cel="zimbabv.Z0o">Zimbabvo</ref>.
     Norda <tld/>o jam 1964 liberi&gcirc;is kiel Respubliko <ref cel="zambi.Z0o">Zambio</ref>:
      <ekz>
        post longaj intertraktadoj kaj diversaj premoj, Afriko celebris en 1980 la
        liberi&gcirc;on de Suda <tld/>o, kiu fari&gcirc;is Zimbabvo
        <fnt>
          <bib>Monato</bib>
          <aut>Princo Henriko Oguinye</aut>
          <vrk><url ref="&Monato;/2005/008764.php">Dronanta diktatoro</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_R;&c_a;&c_d;&c_e;&c_z;&c_ib;&c_ja;</trd>
  <trd lng="de">Rhodesien</trd>
  <trd lng="fr">Rhod&eacute;sie</trd>
  <trd lng="ja">&#x30ED;&#x30FC;&#x30C7;&#x30B7;&#x30A2;</trd>
  <trd lng="pl">Rodezja</trd>
  <trd lng="ro">Rhodesia</trd>
  <trd lng="uk">&c_R;&#x43E;&c_d;&c_je;&c_z;&c_ib;&#x44F; (&c_ch;&c_a;&c_s;&c_t;&c_i;&#x43D;&c_a; &c_A;&c_f;&c_r;&c_i;&#x43A;&c_i;)</trd>
  <trdgrp lng="zh">
    <trd>&#x7F57;&#x5FB7;&#x897F;&#x4E9A; <pr>lu&#xF3;d&#xE9;x&#x12B;y&#xE0;</pr></trd>,
    <trd>&#x7F85;&#x5FB7;&#x897F;&#x4E9E; <pr>lu&#xF3;d&#xE9;x&#x12B;y&#xE0;</pr></trd>
  </trdgrp>
</drv>
</art>

