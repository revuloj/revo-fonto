<kap>
  <ofc>*</ofc>
  <rad>&ccirc;ambelan</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="cxambe.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="cxambe.0o.KOMUNE">
    <dif>
      Oficisto zorganta pri la interna lo&gcirc;ejo de suvereno:
      <ekz>
        la re&gcirc;o tuj ordonis al siaj <tld/>oj, ke ili alportu plej
        belajn vestojn
        <fnt>
          <aut>C. Perrault, trad. P. Sarpy</aut>,
          <vrk>La Majstro Kato</vrk>,
          <lok>
            <url ref="http://eo.wikibooks.org/wiki/Rakontoj_pri_Feinoj">
            Rakontoj pri feinoj</url>, Hachette, 1904
          </lok>
        </fnt>.
      </ekz>
    </dif>
    
    <trd lng="be">&c_k;&c_a;&c_m;&c_e;&c_r;&c_g;&c_je;&c_r;</trd>
    <trdgrp lng="de">
      <trd>Kammerherr</trd>,
      <trd>K&auml;mmerer</trd>
    </trdgrp>
    <trd lng="en">chamberlain</trd>
    <trd lng="fr">chambellan</trd>
    <trd lng="hu">kamar&aacute;s</trd>
    <trd lng="it">ciambellano</trd>
    <trd lng="pl">szambelan</trd>
    <trd lng="pt">camarista</trd>
    <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_r;&c_g;&c_je;&c_r;</trd>
    <trdgrp lng="zh">
    <trd>&#x5167;&#x81E3; <pr>n&#xE8;ich&#xE9;n</pr></trd>,
    <trd>&#x4F8D;&#x4ECE;&#x5B98; <pr>sh&#xEC;c&#xF3;nggu&#x101;n</pr></trd>,
    <trd>&#x5BAB;&#x5973; <pr>g&#x14D;ngn&#x1DA;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="he">
    <trd>&#x5D7;&#x5D3;&#x5E8;&#x5DF;</trd>,
    <trd>&#x5DE;&#x5E9;&#x5E8;&#x5EA;</trd>
  </trdgrp>
  <trdgrp lng="ja">
