     <trd>&c_b;&c_a;&c_z;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
     <trd>&c_g;&c_r;&c_u;&c_n;&c_t;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
   </trdgrp>
   <trd lng="de">sich <ind>gr&uuml;nden</ind> (auf)</trd>
