<drv mrk="baz.sur0ede">
   <kap>sur<tld/>e de,
   <var><kap><tld/>e de</kap></var></kap>
   <gra><vspec>prepozicia&jcirc;o</vspec></gra>
   <snc mrk="baz.sur0ede.KOMUNE">
     <dif>
       <ref tip="super" cel="laux.0.referencajxo">La&ubreve;<sncref/></ref>,
       uzante kiel <tld/>on, kiel modelon:
       <ekz>
         sur<tld/>e de tiu &ccirc;i dokumento, oni povus <klr>...</klr>
         elverki  specifajn planojn
         <fnt><vrk>Strategia Plano de Universala Esperanto-Asocio</vrk>,
         <bib>Esperanto</bib>, <lok>1988:10 (994), p.
         168a</lok></fnt>;
       </ekz>
      <ekz>
        <tld/>e de &ccirc;i tiuj konsideroj <klr>...</klr>
        <fnt>
          <bib>OriginoDeSpecioj</bib>,
          <lok>Enkonduko</lok>
        </fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_n;&c_a; <ind>&c_b;&c_a;&c_z;&c_je;</ind></trd>,
     <trd>&c_n;&c_a; <ind>&c_a;&c_s;&c_n;&c_o;&c_v;&c_je;</ind></trd>
   </trdgrp>
  <trd lng="cs">na z&aacute;klad&ecaron; <klr>(&ccaron;eho)</klr></trd>
  <trdgrp lng="fr">
     <trd>&agrave; partir de<klr> (en prenant pour base)</klr></trd>,
     <trd>sur la base de</trd>
   </trdgrp>
  <trd lng="sk">na z&aacute;klade</trd>
</drv>

<drv mrk="baz.duala0o">
   <kap>duala <tld/>o</kap>
   <uzo tip="fak">MAT</uzo>
   <snc>
     <dif>
       <klr>(de bazo <frm am="(e_i)">(<g>e</g><sub><k>i</k></sub>)</frm>
       de <ref cel="spac.vektora0o">vektora spaco</ref>
       <frm><g>E</g></frm>)</klr>
       Tia <ref cel="baz.0o.vektoraspaco"><tld/>o <sncref/></ref>
       <frm am="(e^i)">(<g>e</g><sup><k>i</k></sup>)</frm> de la
       <ref cel="dual.0o.MAT">dualo</ref> de <frm am="bb        E"><g>E</g></frm>, ke <frm am="bb        e^i(bb e_j)"><g>e</g><sup><k>i</k></sup>(<g>e</g><sub><k>j</k></sub>)</frm>
       egalas al <frm>1</frm>, se <frm am="i=j"><k>i</k> = <k>j</k></frm>, kaj al <frm>0</frm>
       aliokaze.
     </dif>
   </snc>
   <adm>
     Fonto trovenda. <aut>MB</aut>
   </adm>
   <trd lng="de">duale <ind>Basis</ind></trd>
   <trd lng="en">dual <ind>basis</ind></trd>
