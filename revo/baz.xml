</drv>

<drv mrk="baz.0a">
  <kap><tld/>a</kap>
  <snc mrk="baz.0a.SNC">
    <dif>
      Rilata al bazo:
      <ekz>
        la baza tavolo de epidermo
        <fnt>
          <aut>Vikipedio</aut>,
          <vrk><url ref="http://eo.wikipedia.org/wiki/Efelido">Efelido</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="fr">basal</trd>
  </snc>
  <snc mrk="baz.0a.KOMUNE">
    <dif>
      Kiu konsistigas <tld/>on, unuan &scirc;tupon,
      kiu estas <ref tip="vid" cel="elemen.0a">elementa</ref>:
      <ekz>
        la bazaj principoj de la lingvo
        <fnt>
          <aut>Ivo Lapenna</aut>,
          <vrk><url ref="http://www.angelfire.com/ny2/ts/lapenna.html">Por pli
          efika informado</url></vrk>,
          <bib/>,
          <lok/>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="fr">basique</trd>
  </snc>
