       Al kiu mankas <tld/>o,
       fundamento, pravigo:
       <ekz>
         sen<tld/>aj teorioj, pretendoj.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="aer.0a">aera</ref>,
       <ref cel="prav">nepravigita</ref>,
       <ref cel="serioz">neserioza</ref>
     </refgrp>
   </snc>
   <trd lng="be">&c_b;&c_je;&c_s;&c_p;&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
