<drv mrk="baz.duala0o">
   <kap>duala <tld/>o</kap>
   <uzo tip="fak">MAT</uzo>
   <snc>
     <dif>
       <klr>(de bazo <frm am="(e_i)">(<g>e</g><sub><k>i</k></sub>)</frm>
       de <ref cel="spac.vektora0o">vektora spaco</ref>
       <frm><g>E</g></frm>)</klr>
       Tia <ref cel="baz.0o.vektoraspaco"><tld/>o <sncref/></ref>
       <frm am="(e^i)">(<g>e</g><sup><k>i</k></sup>)</frm> de la
       <ref cel="dual.0o.MAT">dualo</ref> de <frm am="bb
       E"><g>E</g></frm>, ke <frm am="bb e^i(bb e_j)"
       ><g>e</g><sup><k>i</k></sup>(<g>e</g><sub><k>j</k></sub>)</frm>
       egalas al <frm>1</frm>, se <frm am="i=j"
       ><k>i</k> = <k>j</k></frm>, kaj al <frm>0</frm> aliokaze.
     </dif>
   </snc>
   <adm>
     Fonto trovenda. <aut>MB</aut>
   </adm>
   <trd lng="de">duale <ind>Basis</ind></trd>
   <trd lng="en">dual <ind>basis</ind></trd>
