    <trd>&c_a;&c_s;&c_n;&c_o;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_k;&c_l;&c_ju;&c_ch;&c_o;&c_v;&c_y;</trd>
  </trdgrp>
