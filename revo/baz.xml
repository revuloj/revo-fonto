   <trd lng="he">&#x5DC;&#x5D4;&#x5EA;&#x5D1;&#x5E1;&#x5E1;</trd>
  <trd lng="hu">alapul</trd>
   <trdgrp lng="pl">
     <trd>bazowa&cacute;<klr> (na czym&sacute;)</klr></trd>,
     <trd>opiera&cacute;<klr> si&eogonek; (co&sacute; na czym&sacute;)</klr></trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_o;&c_s;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
