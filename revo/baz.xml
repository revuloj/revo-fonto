     <trd>&c_d;&c_v;&c_o;&c_j;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j; <ind>&c_b;&c_a;&c_z;&c_i;&c_s;</ind></trd>,
     <trd>&c_s;&c_o;&c_p;&c_r;&c_ja;&c_zh;&c_jo;&c_n;&c_n;&c_y;&c_j; [&c_v;&c_z;&c_a;&c_i;&c_m;&c_n;&c_y;&c_j;] <ind>&c_b;&c_a;&c_z;&c_i;&c_s;</ind></trd>
   </trdgrp>
</drv>

<drv mrk="baz.kanona0o">
   <kap>kanona <tld/>o</kap>
