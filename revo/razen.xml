<kap>
  <rad>razen</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="razen.0o">
  <kap><tld/>o</kap>
  <snc mrk="razen.0o.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      <ref tip="dif" cel="gazon.0o">Gazono</ref>:
      <ekz>
        granda amaso staris &ccirc;e la limoj de la institutaj <tld/>oj 
        <klr>[&#x2026;]</klr> por vidi la elirantojn
        <fnt><aut>J. Francis</aut>, <vrk>Misio sen Alveno</vrk>, 
        <lok>tk-stafeto, 1982</lok></fnt>;
      </ekz>
      <ekz>
        amasoj da infanoj kaj junuloj <klr>[&#x2026;]</klr> kuni&gcirc;as en parkoj, sur <tld/>oj
        <fnt>
          <aut>Garbhan MacAoidh</aut>
            <vrk><url ref="&Monato;/2000/007076.php">Ho, tempoj! Ho, moroj!</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 19</lok>
          </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="tapisx.herbo0o">herbotapi&scirc;o</ref>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_z;&c_o;&c_n;</trd>
  <trd lng="ca">gespa</trd>
  <trdgrp lng="cs">
    <trd>pa&zcaron;it</trd>,
    <trd>p&ecaron;st&ecaron;n&yacute; tr&aacute;vn&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Rasen</trd>,
    <trd>Wiese</trd>
  </trdgrp>
  <trd lng="en">lawn</trd>
  <trd lng="es">c&eacute;sped, grama</trd>
  <trdgrp lng="fr">
    <trd>gazon</trd>,
    <trd>pelouse</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>p&aacute;zsit</trd>,
    <trd>gyep</trd>
  </trdgrp>
  <trd lng="ja">&#x829D;&#x751F;  <pr>&#x3057;&#x3070;&#x3075;</pr></trd>
  <trdgrp lng="nl">
    <trd>gazon</trd>,
    <trd>grasperk</trd>
  </trdgrp>
  <trd lng="pl">trawnik</trd>
  <trd lng="pt">gramado</trd>
  <trd lng="ro">gazon</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_z;&c_o;&c_n;</trd>,
    <trd>&c_l;&c_u;&c_zh;&c_a;&c_j;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">pestovan&yacute; tr&aacute;vnik</trd>
  <trd lng="sv">gr&auml;smatta</trd>
</drv>
