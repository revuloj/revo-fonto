<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>salomon</rad>/oj
</kap>

<drv mrk="salomon1.0oj">
  <kap>
    <tld lit="S"/>oj
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="salomon1.0oj.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Insularo en la sudorienta <ref tip="malprt"
      cel="oceani.O0o">Oceanio</ref>.
      Teritorio de Respubliko Salomonoj
      kaj <ref tip="vid" cel="papu.0onovgvineo.sxtato">Papuo Nov
      Gvineo</ref>.
    </dif>
    <trdgrp lng="pl">
      <trd><klr>archipelag </klr>Wysp Salomona</trd>
    </trdgrp>
  </snc>
  <snc mrk="salomon1.0oj.sxtato">
    <uzo tip="fak">POL</uzo>
    <dif>
      &Scirc;tato en la orienta parto de la samnoma insularo kun
      &ccirc;efurbo Honiaro.
      Respubliko Salomonoj.
      ISO-3166 kodo: SB.
    </dif>
    <trdgrp lng="pl">
      <trd>Wyspy Salomona</trd>
    </trdgrp>
  </snc>
</drv>

</art>

<!--
$Log$
-->
</vortaro>