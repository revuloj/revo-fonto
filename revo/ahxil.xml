<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>A&hcirc;il</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ahxil.0o">
  <kap><tld/>o</kap>
  <snc mrk="ahxil.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira anta&ubreve;nomo grekdevena.
    </dif>
    <trd lng="hu">Elek</trd>    
  </snc>
  <snc mrk="ahxil.0o.PERS">
    <uzo tip="fak">PERS</uzo>
    <dif>
      Re&gcirc;o kaj fortega militisto en <ref tip="vid" cel="iliad.0o">Iliado</ref>:
      <ekz>
        anta&ubreve;en, idoj de <tld/>o
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 4a</lok></fnt>!
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>Akhilleusz</trd>,
      <trd>Achilles</trd>
    </trdgrp>
  </snc>                      
  <trd lng="br">Ac'hilles</trd>
  <trd lng="fr">Achille</trd>
  <trdgrp lng="ru">
    <trd>&c_A;&c_h;&c_i;&c_l;&c_l;</trd>,
    <trd>&c_A;&c_h;&c_i;&c_l;&c_l;&c_je;&c_s;</trd>
  </trdgrp>
</drv>
</art>

