<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>konval</rad>/o
</kap>
<drv mrk="konval.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Genro familio <ref tip="super" cel="liliac.0oj">liliacoj</ref>
      (<trd lng="la">Convallaria</trd>), kun
      bonodoraj sonorilformaj floretoj, venena.
    </dif>
    <ref tip="sin" cel="maj.0floro">majfloro</ref>
  </snc>
  <bld lok="../bld/konval.jpg">
    <tld/>o <klr>(Convallaria majalis)</klr>
  </bld>
  <trd lng="be">&c_l;&c_a;&c_n;&c_d;&c_y;&c_sh;</trd>
  <trd lng="de">Maigl&ouml;ckchen</trd>
  <trd lng="en">lilly of the valley</trd>
  <trd lng="fr">muguet</trd>
  <trd lng="hu">gy&ouml;ngyvir&aacute;g</trd>
  <trd lng="nl">meiklokje</trd>
  <trd lng="pl">konwalia</trd>
  <trd lng="ru">&c_l;&c_a;&c_n;&c_d;&c_y;&c_sh;</trd>
</drv>
</art>
