<kap>
  <ofc>*</ofc>
  <rad>iniciat</rad>/i
</kap>
<drv mrk="inicia.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Esti la unua, kiu proponas a&ubreve; faras ion:
      <ekz>
        <tld/>i novan metodon, eksperimentojn, kuracilon;
      </ekz>
      <ekz>
        li maltime <tld/>is en, pri tiu afero;
      </ekz>
      <ekz>
        la sinjorino <tld/>is al la sinjoro prokriman proceson, li forkuris
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;uste mi <tld/>is la esperantan korespondon
        kun la ru&gcirc;armeanoj
        <fnt><bib>Metrop</bib><lok>&ccirc;ap. 10</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;u &scirc;i efektive <tld/>is la tutan konspiradon,
        tion li ne povis konkludi el la leteroj, sed tio ne estis
        elbarita
        <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 12</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i trovos manieron ta&ubreve;ge <tld/>i kontakton kun li
        <fnt>
          <bib>CKv</bib>
          <lok>12</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pusx.antauxen0i">anta&ubreve;enpu&scirc;i</ref>,
      <ref cel="pasx.el0i.iniciati">elpa&scirc;i<sncref/></ref>,
      <ref cel="entrep.0i">entrepreni</ref>,
      <ref cel="debut.0i">debuti</ref>,
      <ref cel="fond.0i">fondi</ref>,
      <ref cel="instig.0i">instigi</ref>,
      <ref cel="komenc.0i">komenci</ref>,
      <ref cel="lancx.0i.sociajxon">lan&ccirc;i<sncref/></ref>,
      <ref cel="lev.0i.komenci">levi<sncref/></ref>,
      <ref cel="promoc.0i.EKON">promocii<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_n;&c_ib;&c_c;&c_y;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_ch;&c_y;&c_n;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_ch;&c_y;&c_n;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>d&aacute;t podnet</trd>,
    <trd>d&aacute;t popud</trd>,
    <trd>iniciovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>initiieren</trd>,
    <trd>einf&uuml;hren</trd>,
    <trd>ansto&szlig;en <klr>(initiieren)</klr></trd>,
    <trd>anregen</trd>,
    <trd>den <ind>Ansto&szlig;</ind> geben</trd>
  </trdgrp>
  <trd lng="en">initiate</trd>
  <trd lng="es">iniciar</trd>
  <trdgrp lng="fr">
    <trd>initier<klr tip="ind"> (amorcer)</klr></trd>,
    <trd>amorcer<klr tip="ind"> (lancer)</klr></trd>,
    <trd>engager<klr tip="ind"> (lancer)</klr></trd>,
    <trd>entamer<klr tip="ind"> (lancer)</klr></trd>,
    <trd>entreprendre<klr tip="ind"> (lancer)</klr></trd>,
    <trd>lancer<klr tip="ind"> (un projet, etc.)</klr></trd>
  </trdgrp>
  <trd lng="hu">kezdem&eacute;nyez</trd>
  <trdgrp lng="ja">
    <trd>&#x59CB;&#x3081;&#x308B;  [<ind>&#x306F;&#x3058;&#x3081;&#x308B;</ind>]</trd>,
    <trd>&#x767A;&#x8D77;&#x3059;&#x308B;  [<ind>&#x307B;&#x3063;&#x304D;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x5275;&#x59CB;&#x3059;&#x308B;  [<ind>&#x305D;&#x3046;&#x3057;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>iniciar<klr tip="amb"> (tomar iniciativa)</klr></trd>, 
   <trd>tomar <ind>iniciativa</ind></trd>,
   <trd>lan&ccedil;ar<klr tip="amb"> (um projeto etc)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol;
    <ind>&c_p;&c_o;&c_ch;&c_i;&c_n;</ind></trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol;
    <ind>&c_i;&c_n;&c_i;&c_c;&c_i;&c_a;&c_t;&c_o;&c_r;&c_o;&c_m;</ind><klr>
    (&c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>,
  </trdgrp>
  <trdgrp lng="sk">
    <trd>da&tcaron; popud</trd>,
    <trd>d&aacute;va&tcaron; podnet</trd>,
    <trd>iniciova&tcaron;</trd>,
    <trd>zosnova&tcaron;</trd>
  </trdgrp>
  <trd lng="th">&#xE23;&#xE34;&#xE40;&#xE23;&#xE34;&#xE48;&#xE21;</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_b;&c_i;&c_t;&c_i; &c_p;&c_o;&c_ch;&c_i;&c_n;</trd>,
    <trd>&c_v;&c_i;&c_ja;&c_v;&c_l;&c_ja;&c_t;&c_i; &c_ib;&c_n;&c_ib;&c_c;&c_ib;&c_a;&c_t;&c_i;&c_v;&c_u;</trd>,
    <trd>&c_ib;&c_n;&c_ib;&c_c;&c_ib;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_m;&c_i;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_u;&c_s;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_b;&c_u;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="inicia.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago de tiu, kiu <tld/>as:
      <ekz>
        li en tio &ccirc;i venis al nenio, krom senfruktaj <tld/>oj
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun la sama komuna <tld/>o en la sama momento <klr>[&#x2026;]</klr> ili interkisi&gcirc;is
        <fnt>
          <bib>LGA</bib>
          <lok>III.</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o ne trovi&gcirc;as en viaj manoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>!
      </ekz>
      <ekz>
        la redaktoro <klr>[&#x2026;]</klr> <ind>la&ubreve; propra <tld/>o</ind> entreprenis plurajn forstrekojn
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
          <lok>&Ccirc;e Lev Tolstoj en Jasnaja Poljana</lok>
        </fnt>.
        <trd lng="de">in <ind>Eigeninitiative</ind></trd>
      </ekz>
    </dif>
    <ref tip="sin" cel="inicia1.0o.iniciato">iniciativo<sncref/></ref>
    <refgrp tip="vid">
      <ref cel="aktiv.0eco">aktiveco</ref>,
      <ref cel="ofensi.0o">ofensivo</ref>,
      <ref cel="promoc.0o.EKON">promocio<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_n;&c_ib;&c_c;&c_y;&c_ja;&c_t;&c_y;&c_v;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_ch;&c_y;&c_n;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>iniciativa</trd>,
    <trd>podnet</trd>,
    <trd>popud</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Initiative</trd>,
    <trd>Ansto&szlig;</trd>,
    <trd>Anregung</trd>,
  </trdgrp>
  <trd lng="en">initiative</trd>
  <trd lng="es">iniciativa</trd>
  <trdgrp lng="fr">
    <trd>initiative<klr tip="ind"> (fait de lancer un
    projet)</klr></trd>,
    <trd>entreprise<klr tip="ind"> (initiative)</klr></trd>,
    <trd>lancement<klr tip="ind"> (initiative)</klr></trd>
  </trdgrp>
  <trd lng="hu">kezdem&eacute;nyez&eacute;s</trd>
  <trdgrp lng="ja">
    <trd>&#x5275;&#x59CB;  [<ind>&#x305D;&#x3046;&#x3057;</ind>]</trd>,
    <trd>&#x767A;&#x8D77;  [<ind>&#x307B;&#x3063;&#x304D;</ind>]</trd>,
    <trd>&#x7387;&#x5148;&#x3057;&#x305F;&#x884C;&#x52D5;  [<ind>&#x305D;&#x3063;&#x305B;&#x3093;&#x3057;&#x305F;&#x3053;&#x3046;&#x3069;&#x3046;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">initiatief</trd>
  <trd lng="pt">iniciativa<klr tip="amb">
  (a&ccedil;&atilde;o)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_ch;&c_i;&c_n;</trd>,
    <trd>&c_n;&c_a;&c_ch;&c_i;&c_n;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_i;&c_n;&c_i;&c_c;&c_i;&c_a;&c_t;&c_i;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>iniciat&iacute;va</trd>,
    <trd>podnet</trd>,
    <trd>popud</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ib;&c_n;&c_ib;&c_c;&c_ib;&c_a;&c_t;&c_i;&c_v;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_ch;&c_i;&c_n;</trd>,
    <trd>&c_p;&c_o;&c_ch;&c_i;&c_n;&c_a;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="inicia.0emo">
  <kap><tld/>emo</kap>
  <snc>
    <dif>
      Kapablo de tiu, kiu emas <tld/>i:
      <ekz>
        sen li mankos la <ref cel="energi.0o.forto">energio</ref> kaj <tld/>emo
        <fnt>
          <bib>ChB</bib>
          <lok>4</lok>
        </fnt>;
      </ekz>
      <ekz>
        pioniroj, kiuj feli&ccirc;e kompletigis la entuziasmon &gcirc;enerale komunan al la unuaj
        esperantistoj per la <tld/>emo kaj laborforto propraj al gejunuloj
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>ema</ind> direktorino
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2010/010085.php">Kura&gcirc;u malobei!</url></vrk>
          <lok>2010</lok>
        </fnt>;
        <trdgrp lng="de">
          <trd>unternehmungslustig</trd>,
          <trd>agil</trd>,
          <trd>dynamisch</trd>,
          <trd>entschlossen</trd>
        </trdgrp>
        <trdgrp lng="es">
          <trd>emprendedor</trd>,
          <trd>con iniciativa</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>entreprenant<klr tip="ind"> (plein
          d'initiative)</klr></trd>,
          <trd>dynamique<klr tip="ind"> (plein
          d'initiative)</klr></trd>,
          <trd>audacieux<klr tip="ind"> (plein d'initiative)</klr></trd>
        </trdgrp>
        <trd lng="hu">kezdem&eacute;nyez&odblac;k&eacute;sz</trd>
      </ekz>
      <ekz>
        manko de imago kaj de <tld/>emo
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2010/009989.php">Voja&gcirc;o unudirekta</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="inicia1.0o.iniciatemo">iniciativo<sncref/></ref>
    <refgrp tip="ant">
      <ref cel="apati.0o">apatio</ref>,
      <ref cel="letarg.0o.FIG">letargio<sncref/></ref>,
      <ref cel="pigr.0o">pigro</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="vol.0o">volo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_ib;&c_c;&c_y;&c_ja;&c_t;&c_y;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>Unternehmungslust</trd>,
    <trd>Unternehmungsgeist</trd>,
    <trd>Entschlusskraft</trd>,
    <trd>Agilit&auml;t</trd>
  </trdgrp>
  <trd lng="en">enterprising</trd>
  <trd lng="es">iniciativa</trd>
  <trdgrp lng="fr">
    <trd>initiative<klr tip="ind"> (qualit&eacute;)</klr></trd>,
    <trd>esprit d'<ind>initiative</ind></trd>,
    <trd>esprit d'<ind>entreprise</ind></trd>
  </trdgrp>
  <trd lng="hu">kezdem&eacute;nyez&odblac;k&eacute;szs&eacute;g</trd>
  <trdgrp lng="ja">
    <trd>&#x7387;&#x5148;&#x6027;  [<ind>&#x305D;&#x3063;&#x305B;&#x3093;&#x305B;&#x3044;</ind>]</trd>,
    <trd>&#x4E3B;&#x5C0E;&#x6027;  [<ind>&#x3057;&#x3085;&#x3069;&#x3046;&#x305B;&#x3044;</ind>]</trd>,
    <trd>&#x9032;&#x53D6;&#x306E;&#x6C17;&#x6027;  [<ind>&#x3057;&#x3093;&#x3057;&#x3085;&#x306E;&#x304D;&#x3057;&#x3087;&#x3046;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ondernemingsgeest</trd>,
    <trd>proactiviteit</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>iniciativa<klr tip="amb"> (capacidade)</klr></trd>, 
   <trd>esp&iacute;rito de <ind>iniciativa</ind></trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_i;&c_c;&c_i;&c_a;&c_t;&c_i;&c_v;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_p;&c_r;&c_i;&c_i;&c_m;&c_ch;&c_i;&c_v;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="uk">&c_ib;&c_n;&c_ib;&c_c;&c_ib;&c_a;&c_t;&c_i;&c_v;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="inicia.0into">
  <kap><tld/>into</kap>
  <snc>
    <dif>
      La unua, kiu faris a&ubreve; aran&gcirc;is aferon:
      <ekz>
        mi tamen asertas, ke <nom>Zamenhof</nom> fakte <ctl>kreis</ctl> la
        fundamenton de nia lingvo <klr>[...]</klr>, sed prave oni anka&ubreve; povas
        argumenti, ke li estis nur &gcirc;ia <tld/>into
        <fnt><bib>Lanti</bib><lok>&Ccirc;u kreo a&ubreve; nur
        iniciato?</lok></fnt>;
      </ekz>
      <ekz>
        unu <nom>Lemuel</nom>, <klr>[...]</klr> fanatika malamiko de niaj dioj, kiu
        estis intimulo de <nom>Omar</nom>, kaj kun<tld/>into de ribelo kontra&ubreve;
        ili, jam pagis per la vivo sian blasfemadon
        <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 19</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="inicia2.0o">iniciatoro</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_n;&c_ib;&c_c;&c_y;&c_ja;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_n;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>inici&aacute;tor</trd>,
    <trd>p&uring;vodce</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Initiator</trd>,
    <trd>Ansto&szlig;geber</trd>,
    <trd>Urheber</trd>
  </trdgrp>
  <trd lng="en">initiator</trd>
  <trd lng="es">iniciador</trd>
  <trdgrp lng="fr">
    <trd>initiateur<klr tip="ind"> (promoteur)</klr></trd>,
    <trd>pionnier<klr tip="ind"> (initiateur)</klr></trd>,
    <trd>auteur<klr tip="ind"> (initiateur)</klr></trd>,
    <trd>cr&eacute;ateur<klr tip="ind"> (initiateur)</klr></trd>,
    <trd>promoteur<klr tip="ind"> (initiateur)</klr></trd>
  </trdgrp>
  <trd lng="hu">kezdem&eacute;nyez&odblac;</trd>
  <trdgrp lng="ja">
    <trd>&#x5275;&#x59CB;&#x8005;  [<ind>&#x305D;&#x3046;&#x3057;&#x3057;&#x3083;</ind>]</trd>,
    <trd>&#x5148;&#x899A;&#x8005;  [<ind>&#x305B;&#x3093;&#x304B;&#x304F;&#x3057;&#x3083;</ind>]</trd>,
    <trd>&#x767A;&#x8D77;&#x4EBA;  [<ind>&#x307B;&#x3063;&#x304D;&#x306B;&#x3093;</ind>]</trd>
  </trdgrp>
  <trd lng="pt">iniciador</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_i;&c_c;&c_i;&c_a;&c_t;&c_o;&c_r;</trd>,
    <trd>&c_z;&c_a;&c_ch;&c_i;&c_n;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_r;&c_o;&c_d;&c_o;&c_n;&c_a;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_o;&c_s;&c_n;&c_o;&c_v;&c_o;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">p&ocirc;vodca</trd>
  <trd lng="th">&#xE1C;&#xE39;&#xE49;&#xE23;&#xE34;&#xE40;&#xE23;&#xE34;&#xE48;&#xE21;</trd>
  <trd lng="uk">&c_ib;&c_n;&c_ib;&c_c;&c_ib;&c_a;&c_t;&c_o;&c_r;</trd>
</drv>
