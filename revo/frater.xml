<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>fraterkul</rad>/o
</kap>
<drv mrk="frater.0o">
  <kap><tld/>o</kap>
  <snc mrk="frater.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="auxked.0oj">a&ubreve;kedoj</ref>
      (<trd lng="la">Fratercula</trd>), norda na&gcirc;birdo kun plumaro
      kaj beko multkoloraj, iom similaj al tiuj de papago.
    </dif>
  </snc>
  <trd lng="de">Papageitaucher</trd>
  <trd lng="en">puffin</trd>
  <trd lng="fr">macareux</trd>
  <trdgrp lng="hu">
    <trd>lunda</trd>,
    <trd>papag&aacute;lycs&odblac;r&udblac; <ind>alka</ind></trd>
  </trdgrp>
  <trd lng="pl">maskonur</trd>
  <trd lng="pt">papagaio-do-mar</trd>
  <trd lng="ru">&c_t;&c_u;&c_p;&c_i;&c_k; <klr>(&c_p;&c_t;&c_i;&c_c;&c_a;)</klr></trd>
</drv>
</art>
