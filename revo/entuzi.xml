<drv mrk="entuzi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="entuzi.0o.PSI">
    <dif>
      Admira a&ubreve; espera <ref tip="super" cel="fervor.0o">fervorego</ref>:
      <ekz>
        aperis popolaj amasoj kun krioj, floroj kaj muzikistoj <klr>[&#x2026;]</klr>, proksime
        de l' urbo la <tld/>o atingis la supron
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        virino kun menso de infano, kun <tld/>o kaj kredo, &dash; la standardon &scirc;i portas
        anta&ubreve; la batalanta militistaro
        <fnt>
          <bib>Fab3</bib>
          <lok>Dorna vojo de la honoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i rimarkis en la najbara parko tiel belan ludon de koloroj, ke &scirc;i ekkriis kun
        <tld/>o
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve;ta ekkrio montranta <tld/>on de juna brusto
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de la publiko por la nova ideo estis ankora&ubreve; tute fre&scirc;a
        <fnt>
          <bib>EE</bib>
          <lok>VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        nenio estas pli dan&gcirc;era ol blinda <tld/>o
        <fnt>
          <bib>Lanti</bib>,
          <lok>1925-07-09</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="agit.0igxo">agiti&gcirc;o</ref>,
      <ref cel="pasi.0o">pasio</ref>,
      <ref cel="simpat.0o">simpatio</ref>,
      <ref cel="verv.0o">vervo</ref>,
      <ref cel="vibr.0i.FIG">vibri<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_n;&c_t;&c_u;&c_z;&c_ib;&c_ja;&c_z;&c_m;</trd>,
    <trd>&c_n;&c_a;&c_t;&c_h;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_z;&c_a;&c_h;&c_a;&c_p;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="bg">&c_je;&c_n;&c_t;&c_u;&c_s;&c_i;&c_a;&c_z;&c_malmol;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>el&aacute;n</trd>,
    <trd>entuziazmus</trd>,
    <trd>nad&scaron;en&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Begeisterung</trd>,
     <trd>Enthusiasmus</trd>
  </trdgrp>
  <trd lng="es">entusiasmo</trd>
  <trd lng="fr">enthousiasme</trd>
  <trdgrp lng="ja">
    <trd>&#x71B1;&#x72C2;  [<ind>&#x306D;&#x3063;&#x304D;&#x3087;&#x3046;</ind>]</trd>,
    <trd>&#x71B1;&#x4E2D;  [<ind>&#x306D;&#x3063;&#x3061;&#x3085;&#x3046;</ind>]</trd>,
    <trd>&#x60C5;&#x71B1;  [<ind>&#x3058;&#x3087;&#x3046;&#x306D;&#x3064;</ind>]</trd>,
    <trd>&#x72C2;&#x559C;  [<ind>&#x304D;&#x3087;&#x3046;&#x304D;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>entuzjazm</trd>,
    <trd>zapa&lstroke;</trd>,
    <trd>furora</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>entusiasmo</trd>,
    <trd>&acirc;nimo</trd>,
    <trd>vida</trd>,
    <trd>furor</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_n;&c_t;&c_u;&c_z;&c_i;&c_a;&c_z;&c_m;</trd>,
    <trd>&c_v;&c_o;&c_o;&c_d;&c_u;&c_sh;&c_je;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_u;&c_v;&c_l;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_a;&c_z;&c_a;&c_r;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nad&scaron;enie</trd>,
    <trd>odu&scaron;evnenie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_je;&c_n;&c_t;&c_u;&c_z;&c_ib;&c_a;&c_z;&c_m;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_n;&c_a;&c_g;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;</trd>,
    <trd>&c_a;&c_z;&c_a;&c_r;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="entuzi.0a">
  <kap><tld/>a</kap>
  <snc mrk="entuzi.0a.PSI">
    <dif>
      Plena je <tld/>o:
      <ekz>
        konante la <tld/>an kaj kavaliran karakteron de Fernando, mi ne estus tre
        mirigita
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        amba&ubreve; sublevite de sama <tld/>a instigo, sin brakpremis kun multaj montroj de
        reciproka amemo
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekkvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        entuziasmaj apla&ubreve;doj <klr>[&#x2026;]</klr> sekvis post lia parolado
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        niaj kongresoj, ne sole la universala, sed anka&ubreve; la lokaj,
        tiamaniere edukas <klr>[&#x2026;]</klr> <tld/>ajn batalantojn
        por nia afero
        <fnt>
          <bib>Paroloj</bib>,
          <lok>&ccirc;e la Solena Malfermo de la Prepara Konferenco
          de Ruslandaj Esperantistoj en St. Peterburg, 1910</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="fervor.mal0a">malfervora</ref>,
      <ref cel="rezerv.0i.pordiskute">rezervita</ref>,
      <ref cel="skepti.0a">skeptika</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_n;&c_t;&c_u;&c_z;&c_ib;&c_ja;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_a;&c_t;&c_h;&c_n;&c_jo;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_a;&c_h;&c_o;&c_p;&c_l;&c_je;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nad&scaron;en&yacute;</trd>,
    <trd>zan&iacute;cen&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>begeistert</trd>,
    <trd>enthusiastisch</trd>
  </trdgrp>
  <trd lng="fr">enthousiaste<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="he">&bet;&he;&#x5B4;&tav;&lamed;&#x5B7;&he;&#x5B2;&bet;&vav;&#x5BC;&tav;</trd>
  <trdgrp lng="ja">
    <trd>&#x71B1;&#x70C8;&#x306A;  [<ind>&#x306D;&#x3064;&#x308C;&#x3064;&#x306A;</ind>]</trd>,
    <trd>&#x71B1;&#x72C2;&#x7684;&#x306A;  [<ind>&#x306D;&#x3063;&#x304D;&#x3087;&#x3046;&#x3066;&#x304D;&#x306A;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>entuzjastyczny</trd>,
    <trd>zapalony</trd>,
    <trd>napalony<klr> (na co&sacute;)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_o;&c_d;&c_u;&c_sh;&c_je;&c_v;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>nad&scaron;en&yacute;</trd>,
    <trd>zanieten&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_v;&c_n;&c_i;&c_j; &c_je;&c_n;&c_t;&c_u;&c_z;&c_ib;&c_a;&c_z;&c_m;&c_u;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_s;&c_t;&c_r;&c_a;&c_s;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_a;&c_t;&c_h;&c_n;&c_je;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="entuzi.0i">
  <kap><tld/>i</kap>
  <snc mrk="entuzi.0i.PSI">
    <dif>
      Senti, elmontri <tld/>on:
      <ekz>
        mi hejmeniru enliti&gcirc;i por ke neniam, kun neniu mi parolu, ne amu, ne <tld/>u
        <fnt>
          <bib>VaK</bib>
          <lok>Oka &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        li tro <tld/>as pri krimromanoj
        <fnt>
          <bib>ChB</bib>
          <lok>20</lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;belega lando!&rightquot; <tld/>is la sekretariino
        <fnt>
          <bib>CKv</bib>
          <lok>21</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Miguel Fern&aacute;ndez</nom> ne tuj <tld/>is, kiam la <ctl>mirindaj 
        frenezuloj de UEA</ctl> petis lin estri la Belartajn Konkursojn
        <fnt>
          <aut>[Redakcio]</aut>,
          <vrk><url ref="http://www.liberafolio.org/2017/01/28/belartaj-konkursoj-estu-pordo-por-novuloj/">
          Belartaj Konkursoj estu pordo por novuloj</url></vrk>,
          <lok>libera folio, 2017-01-28</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de"><ind>begeistert</ind> sein</trd>
  <trd lng="fr">&ecirc;tre <ind>enthousiaste</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x71B1;&#x72C2;&#x3057;&#x3066;&#x3044;&#x308B;  [<ind>&#x306D;&#x3063;&#x304D;&#x3087;&#x3046;&#x3057;&#x3066;&#x3044;&#x308B;</ind>]</trd>,
    <trd>&#x71B1;&#x4E2D;&#x3057;&#x3066;&#x3044;&#x308B;  [<ind>&#x306D;&#x3063;&#x3061;&#x3085;&#x3046;&#x3057;&#x3066;&#x3044;&#x308B;</ind>]</trd>
  </trdgrp>
</drv>

<drv mrk="entuzi.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="entuzi.0igi.PSI">
    <dif>
      Plenigi je <tld/>o:
      <ekz>
        li <tld/>igas ilin per sia flameco kaj elokventeco
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kio do tiel <tld/>igis la membrojn de la kongreso
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Dua Kongreso Esperantista en Gen&egrave;ve en la 28a de a&ubreve;gusto 1906</vrk>
        </fnt>?
      </ekz>
      <ekz>
        tio &ccirc;i enblovos fajron en viajn vejnojn, se nenio alia vin plu <tld/>igas
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="agit.0i.MENS">agiti<sncref/></ref>,
      <ref cel="ekscit.0i">eksciti</ref>,
      <ref cel="emoci.0i">emocii</ref>,
      <ref cel="fervor.0igi">fervorigi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_t;&c_h;&c_n;&c_ja;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>nadchnout</trd>,
    <trd>poblouznit</trd>,
    <trd>uv&eacute;st v nad&scaron;en&iacute;</trd>,
    <trd>zap&aacute;lit</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>begeistern</trd>
  </trdgrp>
  <trd lng="fr">enthousiasmer</trd>
  <trd lng="he">&lamed;&he;&lamed;&he;&yod;&bet;</trd>
  <trdgrp lng="ja">
    <trd>&#x71B1;&#x72C2;&#x3055;&#x305B;&#x308B;  [<ind>&#x306D;&#x3063;&#x304D;&#x3087;&#x3046;&#x3055;&#x305B;&#x308B;</ind>]</trd>,
    <trd>&#x5922;&#x4E2D;&#x306B;&#x3055;&#x305B;&#x308B;  [<ind>&#x3080;&#x3061;&#x3085;&#x3046;&#x306B;&#x3055;&#x305B;&#x308B;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rozentuzjazmowywa&cacute;</trd>,
    <trd>rozpala&cacute;<klr> (zapa&lstroke;)</klr></trd>,
    <trd>wywo&lstroke;ywa&cacute; <ind>furor&eogonek;</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_o;&c_d;&c_u;&c_sh;&c_je;&c_v;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_v;&c_l;&c_je;&c_ch;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nadchn&uacute;&tcaron;</trd>,
    <trd>odu&scaron;evni&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_a;&c_p;&c_o;&c_v;&c_n;&c_ju;&c_v;&c_a;&c_t;&c_i; &c_je;&c_n;&c_t;&c_u;&c_z;&c_ib;&c_a;&c_z;&c_m;&c_o;&c_m;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_i;&c_h;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="entuzi.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="entuzi.0igxi.PSI">
    <dif>
      I&gcirc;i <tld/>a, eksenti <tld/>on:
      <ekz>
        homoj, kiuj havas la inklinon <tld/>i&gcirc;i kaj dedi&ccirc;i sin al pionireco, oferas sian
        tempon
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Ajna</nom> <tld/>i&gcirc;is, sed Alda estis skeptika
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sola, kiu ne <tld/>i&gcirc;is pri la propono, estis <nom>Montmorency</nom>
        <fnt>
          <aut>J. K. Jerome, trad. G. Badash</aut>,
          <vrk><url ref="http://www.luin.se/inko/065-1.pdf">
          Tri viroj en boato</url></vrk>,
          <lok>1934</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">sich <ind>begeistern</ind></trd>
  <trd lng="fr">s'<ind>enthousiasmer</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x71B1;&#x72C2;&#x3059;&#x308B;  [<ind>&#x306D;&#x3063;&#x304D;&#x3087;&#x3046;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x5922;&#x4E2D;&#x306B;&#x306A;&#x308B;  [<ind>&#x3080;&#x3061;&#x3085;&#x3046;&#x306B;&#x306A;&#x308B;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_a;&c_b;&c_i;&c_r;&c_a;&c_t;&c_i;&c_s;&c_ja; &c_je;&c_n;&c_t;&c_u;&c_z;&c_ib;&c_a;&c_z;&c_m;&c_u;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="entuzi.0ulo">
  <kap><tld/>ulo, <var><kap><tld/>emulo</kap></var></kap>
  <snc mrk="entuzi.0ulo.PSI">
    <dif>
      Facile <tld/>i&gcirc;anta homo:
      <ekz>
        la nombro de la <tld/>uloj atingos certan ciferon kaj haltos
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili efemere aperas sub la plumo de nur malmultaj <tld/>uloj, kaj malaperas
        senspure
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        skii sen ne&gcirc;o &dash; tion provis 80 <tld/>uloj pri vintra sporto meze de la somero en
        la litova &ccirc;efurbo
        <fnt>
          <aut>Laimius Stra&zcaron;nickas</aut>
            <vrk><url ref="&Monato;/2000/007067.php">Skii sen ne&gcirc;o</url></vrk>
            <bib>Monato</bib>
            <lok>2000/11, p. 15</lok>
          </fnt>;
      </ekz>
      <ekz>
        la menciitan karton iniciatis skotaj <tld/>uloj en la jaro 1987
        <fnt>
          <bib>Monato</bib>
          <aut>Marija Belo&scaron;evi&cacute;</aut>
          <vrk><url ref="&Monato;/2003/007901.html">Turismi kun la karto EURO&lt;26 en la po&scirc;o</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fan.0o">fano</ref>,
      <ref cel="fervor.0ulo">fervorulo</ref>,
      <ref cel="zelot.0o">zeloto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_n;&c_t;&c_u;&c_z;&c_ib;&c_ja;&c_s;&c_t;</trd>
  <trd lng="cs">nad&scaron;enec</trd>
  <trdgrp lng="de">
    <trd>Begeisterter</trd>,
    <trd>Schw&auml;rmer</trd>,
    <trd>Enthusiast</trd>
  </trdgrp>
  <trd lng="fr">enthousiaste<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="he">&nun;&lamed;&he;&bet;</trd>
  <trdgrp lng="ja">
    <trd>&#x71B1;&#x4E2D;&#x3057;&#x3084;&#x3059;&#x3044;&#x4EBA;  [<ind>&#x306D;&#x3063;&#x3061;&#x3085;&#x3046;&#x3057;&#x3084;&#x3059;&#x3044;&#x3058;&#x3093;</ind>]</trd>,
    <trd>&#x71B1;&#x72C2;&#x8005;  [<ind>&#x306D;&#x3063;&#x304D;&#x3087;&#x3046;&#x3057;&#x3083;</ind>]</trd>,
    <trd>&#x30D5;&#x30A1;&#x30F3;  [<ind>&#x3075;&#x3041;&#x3093;</ind>]</trd>,
    <trd>&#x60C5;&#x71B1;&#x5BB6;  [<ind>&#x3058;&#x3087;&#x3046;&#x306D;&#x3064;&#x304B;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>entuzjasta</trd>,
    <trd>zapaleniec</trd>,
    <trd>napaleniec</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_n;&c_t;&c_u;&c_z;&c_i;&c_a;&c_s;&c_t;</trd>
  <trd lng="sk">nad&scaron;enec</trd>
  <trd lng="uk">&c_je;&c_n;&c_t;&c_u;&c_z;&c_ib;&c_a;&c_s;&c_t;</trd>
</drv>
</art>
