<drv mrk="entuzi.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="entuzi.0igxi.PSI">
    <dif>
      I&gcirc;i <tld/>a, eksenti <tld/>on:
      <ekz>
        homoj, kiuj havas la inklinon <tld/>i&gcirc;i kaj dedi&ccirc;i sin al pionireco, oferas sian
        tempon
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Ajna</nom> <tld/>i&gcirc;is, sed Alda estis skeptika
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sola, kiu ne <tld/>i&gcirc;is pri la propono, estis <nom>Montmorency</nom>
        <fnt>
          <aut>J. K. Jerome, trad. G. Badash</aut>,
          <vrk><url ref="http://www.luin.se/inko/065-1.pdf">
          Tri viroj en boato</url></vrk>,
          <lok>1934</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_p;&c_a;&c_w;&c_n;&c_ja;&c_c;&c_c;&c_a; <ind>&c_e;&c_n;&c_t;&c_u;&c_z;&c_ib;&c_ja;&c_z;&c_m;&c_a;&c_m;</ind></trd>,
    <trd>&c_n;&c_a;&c_h;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de">sich <ind>begeistern</ind></trd>
  <trd lng="fr">s'<ind>enthousiasmer</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x71B1;&#x72C2;&#12377;&#x308B;  <pr>&#x306D;&#12387;&#x304D;&#12423;&#12358;&#12377;&#x308B;</pr></trd>,
    <trd>&#22818;&#x4E2D;&#x306B;&#x306A;&#x308B;  <pr>&#12416;&#12385;&#12421;&#12358;&#x306B;&#x306A;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_a;&c_b;&c_i;&c_r;&c_a;&c_t;&c_i;&c_s;&c_ja; &c_je;&c_n;&c_t;&c_u;&c_z;&c_ib;&c_a;&c_z;&c_m;&c_u;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
