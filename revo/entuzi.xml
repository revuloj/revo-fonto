  <trdgrp lng="uk">
    <trd>&c_n;&c_a;&c_p;&c_o;&c_v;&c_n;&c_ju;&c_v;&c_a;&c_t;&c_i; &c_je;&c_n;&c_t;&c_u;&c_z;&c_ib;&c_a;&c_z;&c_m;&c_o;&c_m;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_i;&c_h;&c_a;&c_t;&c_i;</trd>
