<drv mrk="entuzi.0a">
  <kap><tld/>a</kap>
  <snc mrk="entuzi.0a.PSI">
    <dif>
      Plena je <tld/>o:
      <ekz>
        konante la <tld/>an kaj kavaliran karakteron de Fernando, mi ne estus tre
        mirigita
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        amba&ubreve; sublevite de sama <tld/>a instigo, sin brakpremis kun multaj montroj de
        reciproka amemo
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekkvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        entuziasmaj apla&ubreve;doj <klr>[&#8230;]</klr> sekvis post lia parolado
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        niaj kongresoj, ne sole la universala, sed anka&ubreve; la lokaj,
        tiamaniere edukas <klr>[&#8230;]</klr> <tld/>ajn batalantojn
        por nia afero
        <fnt>
          <bib>Paroloj</bib>,
          <lok>&ccirc;e la Solena Malfermo de la Prepara Konferenco
          de Ruslandaj Esperantistoj en St. Peterburg, 1910</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="fervor.mal0a">malfervora</ref>,
      <ref cel="rezerv.0i.pordiskute">rezervita</ref>,
      <ref cel="skepti.0a">skeptika</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_n;&c_t;&c_u;&c_z;&c_ib;&c_ja;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_a;&c_t;&c_h;&c_n;&c_jo;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_a;&c_h;&c_o;&c_p;&c_l;&c_je;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nad&scaron;en&yacute;</trd>,
    <trd>zan&iacute;cen&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>begeistert</trd>,
    <trd>enthusiastisch</trd>
  </trdgrp>
  <trd lng="fr">enthousiaste<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="he">&bet;&he;&#x5B4;&tav;&lamed;&#x5B7;&he;&#x5B2;&bet;&vav;&#x5BC;&tav;</trd>
  <trdgrp lng="ja">
