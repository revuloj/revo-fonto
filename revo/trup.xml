<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>trup</rad>/o
</kap>
<drv mrk="trup.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="stl">ARK</uzo>
    <dif>
      Aro da artistoj ludantaj en sama teatro a&ubreve; cirko:
      <ekz>
        opera <tld/>o<fnt>B</fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_t;&c_r;&c_u;&c_p;&c_p;&c_a;</trd>
  </snc>
  <snc>
    <dif>
      Iu ajn aro da soldatoj; armeparto.
    </dif>
    <trdgrp lng="ru">
      <trd>&c_o;&c_t;&c_r;&c_ja;&c_d;</trd>,
      <trd>&c_v;&c_o;&c_i;&c_n;&c_s;&c_k;&c_a;&c_ja; &c_ch;&c_a;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_o;&c_j;&c_s;&c_k;&c_a; <klr>(plurale)</klr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>mu&zcaron;stvo</trd>,
    <trd>dru&zcaron;stvo</trd>
  </trdgrp>
  <trd lng="de">Truppe</trd>
  <trd lng="nl">troep</trd>
</drv>
</art>
