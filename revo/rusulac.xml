<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rusulac.xml,v 1.3 2019/02/24 14:10:22 revo Exp $">
<kap>
    <rad>rusulac</rad>/oj
</kap>
<drv mrk="rusulac.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="rusulac.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" 
      lst="voko:botanikaj_familioj">Familio</ref>
      de <ref tip="malprt" cel="fung.klab0oj">klabfungoj</ref>
      (<trd lng="la">Russulaceae</trd>):
      <ekz>
        <ref tip="lst" lst="voko:urboj" cel="rusul.0o">rusulo</ref>
        estas genro de fungoj en la familio <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=336893"
              >Rusulo</url></lok>
        </fnt>.
      </ekz>
     </dif>
  </snc>
  <trd lng="de">T&auml;ublingsverwandte</trd>
</drv>
</art>

