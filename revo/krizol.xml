<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: krizol.xml,v 1.17 2018/10/03 16:10:18 revo Exp $">
<kap>
  <rad>krizolit</rad>/o <fnt>MT</fnt>
</kap>
<drv mrk="krizol.0o">
  <kap><tld/>o</kap>
  <snc mrk="krizol.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Multekosta &scirc;tono, plej perfekta <ref tip="super" cel="krista.0o.MIN">kristalo</ref>
      de <ref tip="super" cel="peridot.0o.MIN">peridoto</ref>,
      bele verdeta, travidebla:
      <ekz>
        la kvara vico: <tld/>o, kaj onikso, kaj berilo
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 28:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kvina, sardonikso, la sesa, sardio, la sepa, <tld/>o, la oka,
        berilo, la na&ubreve;a, topazo, la deka, krizopraso, la dek-unua, jacinto, la dek-dua, ametisto  
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 21:20</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_r;&c_y;&c_z;&c_a;&c_l;&c_ib;&c_t;</trd>
  <trd lng="ca">cris&ograve;lit</trd>
  <trd lng="cs">chryzolit</trd>
  <trd lng="de">Chrysolith</trd>
  <trdgrp lng="en">
    <trd>chrysolite</trd>,
    <trd>olivine</trd>,
    <trd>peridot</trd>
  </trdgrp>
  <trd lng="fr">chrysolite</trd>
  <trd lng="hu">krizolit</trd>
  <trd lng="nl">chrysoliet</trd>
  <trdgrp lng="pl">
    <trd>perydot</trd>,
    <trd>chryzolit</trd>
  </trdgrp> 
  <trd lng="pt">cris&oacute;lita</trd>
  <trd lng="ro">olivin&abreve;</trd>
  <trd lng="ru">&c_h;&c_r;&c_i;&c_z;&c_o;&c_l;&c_i;&c_t;</trd>
  <trd lng="sk">chryzolit</trd>
</drv>
</art>

