<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>krizolit</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="krizol.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Multekosta &scirc;tono, plej perfekta kristalo de peridoto, bele verdeta,
      travidebla.
    </dif>
    <refgrp tip="sin">
      <ref cel="peridot.0o">peridoto</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Chrysolith</trd>
  <trd lng="ru">&c_h;&c_r;&c_i;&c_z;&c_o;&c_l;&c_i;&c_t;</trd>
</drv>
</art>

