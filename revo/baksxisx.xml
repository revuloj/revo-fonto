<drv mrk="baksxisx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc mrk="baksxisx.0o.gratifiko">
    <dif>
      <ref tip="dif" cel="gratif.0o">Gratifiko</ref>:
      <ekz>
        oni donu la unuagradan <tld/>on al la senditoj
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 56a, p.
        247a</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i servu en kupeo per trinka&jcirc;o, 10 juanojn al &scirc;i por unu horo, plus <tld/>o
        <fnt>
          <aut>Xu Jing</aut>
          <vrk><url ref="&CRI;221/2007/03/30/1@56936.htm">Kelnerino</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="bonus.0o.EKON">bonuso</ref>,
       <ref cel="mon.trink0o">trinkmono</ref>
    </refgrp>
    <trdgrp lng="zh">
      <trd>&#22870;&#36175; <pr>ji&#462;ngsh&#462;ng</pr></trd>,
      <trd>&#29305;&#21029;&#36027;&#29992; <pr>t&#232;bi&#233;f&#232;iy&#242;ng</pr></trd>,
      <trd>&#19987;&#29992;&#27454;&#39033; <pr>zhu&#257;ny&#242;ngku&#462;nxi&#224;ng</pr></trd>
    </trdgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Anka&#365; eblas diri <ke>&#349;mirmono</ke>, <ke>dankmono</ke>, <ke>trinkmono</ke>.
  </rim>

  <trd lng="be">&c_b;&c_a;&c_k;&c_sh;&c_y;&c_sh;</trd>
  <trd lng="bg">&c_b;&c_a;&c_k;&c_sh;&c_i;&c_sh;</trd>
  <trdgrp lng="ca">
    <trd>gratificaci&oacute;</trd>,
    <trd>propina</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Sonderzuwendung</trd>,
    <trd>Sonderzahlung</trd>,
    <trd>freiwillige <ind>Zahlung</ind></trd>,
    <trd>Gratifikation</trd>,
    <trd>Pr&auml;mie</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>gratificaci&oacute;n</trd>
    <trd>propina</trd>
  </trdgrp>
  <trdgrp lng="fr">
