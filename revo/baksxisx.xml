<kap>
  <rad>bak&scirc;i&scirc;</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="baksxisx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc mrk="baksxisx.0o.gratifiko">
    <dif>
      <ref tip="dif" cel="gratif.0o">Gratifiko</ref>:
      <ekz>
        oni donu la unuagradan <tld/>on al la senditoj
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 56a, p.
        247a</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i servu en kupeo per trinka&jcirc;o, 10 juanojn al &scirc;i por unu horo, plus <tld/>o
        <fnt>
          <aut>Xu Jing</aut>
          <vrk><url ref="&CRI;221/2007/03/30/1@56936.htm">Kelnerino</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="bonus.0o.EKON">bonuso</ref>,
       <ref cel="mon.trink0o">trinkmono</ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#x5956;&#x8D4F; <pr>ji&#x1CE;ngsh&#x1CE;ng</pr></trd>,
    <trd>&#x7279;&#x5225;&#x8CBB;&#x7528; <pr>t&#xE8;bi&#xE9;f&#xE8;iy&#xF2;ng</pr></trd>,
    <trd>&#x4E13;&#x7528;&#x6B3E;&#x9879; <pr>zhu&#x101;ny&#xF2;ngku&#x1CE;nxi&#xE0;ng</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_b;&c_a;&c_k;&c_sh;&c_y;&c_sh;</trd>
  <trd lng="bg">&c_b;&c_a;&c_k;&c_sh;&c_i;&c_sh;</trd>
  <trdgrp lng="ca">
    <trd>gratificaci&oacute;</trd>,
    <trd>propina</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Sonderzuwendung</trd>,
    <trd>Sonderzahlung</trd>,
    <trd>freiwillige <ind>Zahlung</ind></trd>,
    <trd>Gratifikation</trd>,
    <trd>Pr&auml;mie</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>gratificaci&oacute;n</trd>
    <trd>propina</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bakchich</trd>,
    <trd>gratification<klr tip="ind"> (r&eacute;compense
    financi&egrave;re)</klr></trd>,
    <trd>pourboire</trd>
  </trdgrp>
  <trd lng="he">&#x5E9;&#x5D5;&#x5D7;&#x5D3;</trd>
  <trdgrp lng="hu">
    <trd>baksis</trd>,
    <trd>borraval&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>baksjisj</trd>,
    <trd>fooi</trd>,
    <trd>smeergeld</trd>,
    <trd>drinkgeld</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bakszysz</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_k;&c_sh;&c_i;&c_sh;</trd>,
    <trd>&c_ch;&c_a;&c_je;&c_v;&c_y;&c_je;</trd>
