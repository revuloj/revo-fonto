    <trd>pourboire</trd>
  </trdgrp>
  <trd lng="he">&#1513;&#1493;&#1495;&#1491;</trd>
  <trdgrp lng="hu">
    <trd>baksis</trd>,
    <trd>borraval&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
