<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: korod.xml,v 1.20 2023/12/03 09:58:58 revo Exp $">
<kap>
  <rad>korod</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="korod.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="korod.0i.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emie <ref tip="super" cel="difekt.0i">difekti</ref> la surfacon,
      <ref tip="vid" cel="oksid.0igi">oksidigi</ref> metalon:
      <ekz>
        <ind><tld/>-imuna &scirc;talo</ind>
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2003/008042.html">Pinte!</url></vrk>
              <bib>Monato</bib>
          <lok>2003/12, p. 16</lok>
        </fnt>;
        <trd lng="de">Edelstahl</trd>
      </ekz>
      <ekz>
        <ind><tld/>rezista &scirc;talo</ind>
        <fnt>
          <aut>Lode van de Velde</aut>
          <vrk><url ref="&Monato;/2008/009517.php">Feli&ccirc;an datrevenon!</url></vrk>
              <bib>Monato</bib>
          <lok>2008/07, p. 13</lok>
        </fnt>;
        <trd lng="de">korrosionsbest&auml;ndiger Stahl</trd>
      </ekz>
      <ekz>
        aluminio <klr>[&#x2026;]</klr> bone rezistas <tld/>on
        <fnt>
          <aut>Lode van de Velde</aut>
          <vrk><url ref="&Monato;/2008/009517.php">Feli&ccirc;an datrevenon!</url></vrk>
              <bib>Monato</bib>
          <lok>2008/07, p. 13</lok>
        </fnt>;
      </ekz>
      <ekz>
        objektoj el metalo konservi&gcirc;is tie pli bone &ccirc;ar malsekeco ne
        <tld/>is ilin
        <fnt><bib>IntArkeol</bib>, <lok>p. 40a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="rust.0o">rusto</ref>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol; <ind>&c_k;&c_a;&c_r;&c_o;&c_z;&c_ib;&c_ju;</ind></trd>,
      <trd>&c_r;&c_a;&c_z;&c_mol;&c_ja;&c_d;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
  <trd lng="ca">corroir</trd>
  <trd lng="de">korrodieren</trd>
  <trdgrp lng="en">
    <trd>corrode</trd>,
    <trd>eat away</trd>
  </trdgrp>
  <trd lng="es">corroer</trd>
  <trd lng="fr">corroder</trd>
  <trdgrp lng="hu">
    <trd>korrod&aacute;l</trd>,
    <trd>oxid&aacute;l</trd>
  </trdgrp> 
  <trdgrp lng="nl">
    <trd>aantasten</trd>,
    <trd>corroderen</trd>,
    <trd>inbijten</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>korodowa&cacute;</trd>,
    <trd>korozj&eogonek; powodowa&cacute;</trd>
  </trdgrp>    
  <trdgrp lng="pt">
    <trd>corroer</trd>,
    <trd>gastar <klr tip="ind">(corroer)</klr></trd>,    
    <trd>desgastar</trd>
  </trdgrp>
  <trd lng="ro">roade</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_malmol;&c_je;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_r;&c_o;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_k;&c_i;&c_s;&c_l;&c_ja;&c_t;&c_mol; <klr>(&c_m;&c_je;&c_t;&c_a;&c_l;&c_l;)</klr></trd>
  </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x8150;&#x8680; <pr>f&#x1D4;sh&#xED;</pr></trd>,
    <trd>&#x8680; <pr>sh&#xED;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="korod.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Iom post iom <ref tip="super" cel="difekt.0i">difekti</ref>, 
      <ref tip="vid" cel="uz.el0i">eluzi</ref>, lacigi:
      <ekz>
        envio <tld/>as la animon
        <fnt>
          <aut>Unuigho</aut>,
          <vrk><url ref="http://groups.yahoo.com/group/esperanto-l/message/70313">
          Letica kaj Leobardo</url></vrk>,
          <lok>soc.culture.esperanto, 2007-08-21</lok>
        </fnt>;
      </ekz>
      <ekz>
        Aron da &scirc;okoj / la vivo por mi stokis, <klr>[&#x2026;]</klr> / La sinsekvaj doloroj /
        <tld/>is la koron
        <fnt>
          <aut>Lucija Bor&ccaron;i&cacute;</aut>
          <vrk><url ref="&Monato;/2004/008066.html">Retrorigardo al intensa vivo</url></vrk>
              <bib>Monato</bib>
          <lok>2004/04, p. 20</lok>
        </fnt>;
      </ekz>
      <ekz>
        dibo&ccirc;a mastrumado de la superuloj kaj le&gcirc;eranima vivado de la ordinaruloj
        <tld/>as la financojn de respubliko
        <fnt>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2010/010143.php">Ideoj, konceptoj, maksimoj de J. J. Becher (1635&ndash;1682)</url></vrk>
              <bib>Monato</bib>
          <lok>2010/11, p. 21</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="erozi.0i.FIG">erozii, erodi<sncref/></ref>,
      <ref cel="fos.sub0i.FIG">subfosi<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_z;&c_mol;&c_ja;&c_d;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_je;&c_s;&c_mol;&c_c;&c_ib;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_b;&c_u;&c_r;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="fr">&eacute;roder<klr> (fig.)</klr></trd>
    <trd lng="zh">&#x51B2;&#x5237; <pr>ch&#x14D;ngshu&#x101;</pr></trd>
</snc>
  <trdgrp lng="cs">
    <trd>sou&zcaron;it</trd>,
    <trd>s&zcaron;&iacute;rat</trd>,
    <trd>tr&aacute;pit</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8150;&#x98DF;&#x3059;&#x308B;  <pr>&#x3075;&#x3057;&#x3087;&#x304F;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x3080;&#x3057;&#x3070;&#x3080;</trd>,
    <trd>&#x6D88;&#x8017;&#x3055;&#x305B;&#x308B;  <pr>&#x3057;&#x3087;&#x3046;&#x3082;&#x3046;&#x3055;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>s&uacute;&zcaron;i&tcaron;</trd>,
    <trd>tr&aacute;pi&tcaron;</trd>,
    <trd>zo&zcaron;iera&tcaron;</trd>
  </trdgrp>
</drv>


</art>
