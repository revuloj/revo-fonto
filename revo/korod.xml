<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: korod.xml,v 1.15 2022/05/01 08:09:25 revo Exp $">
<kap>
  <rad>korod</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="korod.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="korod.0i.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emie <ref tip="super" cel="difekt.0i">difekti</ref> la surfacon,
      <ref tip="vid" cel="oksid.0igi">oksidigi</ref> metalon:
      <ekz>
        <ind><tld/>-imuna &scirc;talo</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="&Monato;/2003/008042.html"
              >Pinte!</url></vrk>
          <lok>2003</lok>
        </fnt>;
        <trd lng="de">Edelstahl</trd>
      </ekz>
      <ekz>
        <ind><tld/>rezista &scirc;talo</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="&Monato;/2008/009517.php"
              >Feli&ccirc;an datrevenon!</url></vrk>
          <lok>2008</lok>
        </fnt>;
        <trd lng="de">korrosionsbest&auml;ndiger Stahl</trd>
      </ekz>
      <ekz>
        aluminio <klr>[&#8230;]</klr> bone rezistas <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="&Monato;/2008/009517.php"
              >Feli&ccirc;an datrevenon!</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        objektoj el metalo konservi&gcirc;is tie pli bone &ccirc;ar malsekeco ne
        <tld/>is ilin
        <fnt><bib>IntArkeol</bib>, <lok>p. 40a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="rust.0o">rusto</ref>
  <trd lng="ca">corroir</trd>
  <trd lng="de">korrodieren</trd>
  <trdgrp lng="en">
    <trd>corrode</trd>,
    <trd>eat away</trd>
  </trdgrp>
  <trd lng="es">corroer</trd>
  <trd lng="fr">corroder</trd>
  <trdgrp lng="hu">
    <trd>korrod&aacute;l</trd>,
    <trd>oxid&aacute;l</trd>
  </trdgrp> 
  <trdgrp lng="nl">
    <trd>aantasten</trd>,
    <trd>corroderen</trd>,
    <trd>inbijten</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>korodowa&cacute;</trd>,
    <trd>korozj&eogonek; powodowa&cacute;</trd>
  </trdgrp>    
  <trdgrp lng="pt">
    <trd>corroer</trd>,
    <trd>gastar <klr tip="ind">(corroer)</klr></trd>,    
    <trd>desgastar</trd>
  </trdgrp>
  <trd lng="ro">roade</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_malmol;&c_je;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_r;&c_o;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_k;&c_i;&c_s;&c_l;&c_ja;&c_t;&c_mol; <klr>(&c_m;&c_je;&c_t;&c_a;&c_l;&c_l;)</klr></trd>
  </trdgrp>
  </snc>
  <snc mrk="korod.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Iom post iom <ref tip="super" cel="difekt.0i">difekti</ref>, 
      <ref tip="vid" cel="uz.el0i">eluzi</ref>, lacigi:
      <ekz>
        envio <tld/>as la animon
        <fnt>
          <aut>Unuigho</aut>,
          <vrk><url
          ref="http://groups.yahoo.com/group/esperanto-l/message/70313">
          Letica kaj Leobardo</url></vrk>,
          <lok>soc.culture.esperanto, 2007-08-21</lok>
        </fnt>;
      </ekz>
      <ekz>
        Aron da &scirc;okoj / la vivo por mi stokis, <klr>[&#8230;]</klr> / La sinsekvaj doloroj /
        <tld/>is la koron
        <fnt>
          <bib>Monato</bib>
          <aut>Lucija Bor&ccaron;i&cacute;</aut>
          <vrk><url ref="&Monato;/2004/008066.html"
              >Retrorigardo al intensa vivo</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        dibo&ccirc;a mastrumado de la superuloj kaj le&gcirc;eranima vivado de la ordinaruloj
        <tld/>as la financojn de respubliko
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2010/010143.php"
              >Ideoj, konceptoj, maksimoj de J. J. Becher (1635&ndash;1682)</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="erozi.0i.FIG">erozii, erodi<sncref/></ref>,
      <ref cel="fos.sub0i.FIG">subfosi<sncref/></ref>
    </refgrp>
    <trd lng="fr">&eacute;roder<klr> (fig.)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>sou&zcaron;it</trd>,
    <trd>s&zcaron;&iacute;rat</trd>,
    <trd>tr&aacute;pit</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>s&uacute;&zcaron;i&tcaron;</trd>,
    <trd>tr&aacute;pi&tcaron;</trd>,
    <trd>zo&zcaron;iera&tcaron;</trd>
  </trdgrp>
</drv>


</art>
