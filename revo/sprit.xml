    <trd>&#12392;&#12435;&#x307E;&#x306A;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>dom</trd>,
    <trd>onnozel</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>t&eogonek;py</trd>,
    <trd>niedo&lstroke;&eogonek;&zdot;ny</trd>,
    <trd>zakuty</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_u;&c_p;&c_o;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_t;&c_o;&c_l;&c_k;&c_o;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_o;&c_s;&c_t;&c_r;&c_o;&c_u;&c_m;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hl&uacute;py</trd>,
    <trd>obmedzen&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_u;&#x43F;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_g;&#x43B;&c_u;&c_z;&c_d;&c_i;&c_j;</trd>,
    <trd>&#x43D;&c_je;&c_d;&#x43E;&c_t;&c_je;&#x43F;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_d;&c_u;&c_r;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="sprit.mal0ulo">
  <kap>mal<tld/>ulo</kap>
  <snc mrk="sprit.mal0ulo.PSI">
    <dif>
      Homo kun malvigla spirito, malrapide komprenanta:
      <ekz>
        unu el la prezentistoj ludis mal<tld/>ulon
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 13a, la batalo
        komenci&gcirc;as</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Narr</trd>,
