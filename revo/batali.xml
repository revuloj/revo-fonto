<kap>
  <ofc>1</ofc>
  <rad>batalion</rad>/o
</kap>
<drv mrk="batali.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc mrk="batali.0o.MIL">
    <dif>
      Parto de infanteria <ref tip="malprt" cel="regime.0o">regimento</ref>,
      konsistanta el du &gcirc;is kvar <ref tip="prt" cel="rot.0o">rotoj</ref>:
      <ekz>
        tria ano de la irlanda Unua <tld lit="B"/>o, kiu oferis sian
        vivon
        por paco en suda Libano<fnt><aut>Irl</aut>: <vrk>Paca
        lingvo</vrk>, <bib>Monato</bib><lok>jaro 1999a, numero 7a, p.
        7a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tacxme.0o.MIL">ta&ccirc;mento</ref>
    </refgrp>
  <trd lng="be">&c_b;&c_a;&c_t;&c_a;&c_l;&c_mol;&c_jo;&c_n;</trd>
  <trd lng="de">Bataillon</trd>
  <trd lng="en">batallion</trd>
  <trd lng="es">batall&oacute;n</trd>
  <trd lng="fr">bataillon</trd>
  <trd lng="hu">z&aacute;szl&oacute;alj</trd>
  <trd lng="it">battaglione</trd>
  <trd lng="pl">batalion<klr> (oddzia&lstroke;)</klr></trd>
  <trd lng="ru">&c_b;&c_a;&c_t;&c_a;&c_l;&c_mol;&c_o;&c_n;</trd>
    <trdgrp lng="zh">
    <trd>&#x5927;&#x90E8;&#x961F; <pr>d&#xE0;b&#xF9;du&#xEC;</pr></trd>,
    <trd>&#x4E00;&#x5927;&#x5925; <pr>y&#x12B;d&#xE0;ihu&#x1D2;</pr></trd>,
    <trd>&#x9635;&#x5BB9;&#x6574;&#x9F50;&#x7684;&#x519B;&#x961F; <pr>zh&#xE8;nr&#xF3;ngzh&#x11B;ngq&#xED;dej&#x16B;ndu&#xEC;</pr></trd>,
    <trd>&#x71DF; <pr>y&#xED;ng</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="cs">
    <trd>batalion</trd>,
    <trd>prapor</trd>
  </trdgrp>
  <trd lng="he">&#x5D2;&#x5B0;&#x5D3;&#x5D5;&#x5BC;&#x5D3;</trd>
  <trd lng="ja">&#x5927;&#x968A;  <pr>&#x3060;&#x3044;&#x305F;&#x3044;</pr></trd>
  <trd lng="sk">pr&#xE1;por</trd>
  <trd lng="uk">&#x431;&#x430;&#x442;&#x430;&#x43B;&#x44C;&#x439;&#x43E;&#x43D;</trd>
