<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>vespr</rad>/o
</kap>
<drv mrk="vespr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KAT</uzo>
  <snc mrk="vespr.0o.KAT">
    <dif>
      Vespera <ref tip="super" cel="serv.Di0o">diservo</ref>,
      konsistanta el psalmoj kaj himnoj.
    </dif>
  </snc>
  <rim mrk="vespr.0o.RIM_1">
    Foje uzata plurnombre pri nur unu diservo.
  </rim>
  <trd lng="be">&c_v;&c_ja;&c_ch;&c_e;&c_r;&c_n;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Vesper</trd>,
    <trd>Abendandacht</trd>
  </trdgrp>
  <trd lng="es">v&iacute;spera<klr> (oficio religioso de tarde)</klr></trd>
  <trd lng="fr">v&ecirc;pres</trd>
  <trdgrp lng="hu">
    <trd>vesper&aacute;s</trd>,
    <trd>vecsernye</trd>
  </trdgrp>
  <trd lng="nl">vesper</trd>
  <trd lng="pl">nieszpora</trd>
  <trd lng="pl">nieszpory</trd>
  <trd lng="pt">v&eacute;spera</trd>
  <trd lng="ru">&c_v;&c_je;&c_ch;&c_je;&c_r;&c_n;&c_ja;</trd>
</drv>
</art>
