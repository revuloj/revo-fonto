  <trdgrp lng="de">
    <trd>Vesper</trd>,
    <trd>Abendandacht</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vesper&aacute;s</trd>,
    <trd>vecsernye</trd>
  </trdgrp>
  <trd lng="nl">vesper</trd>
  <trd lng="ru">&c_v;&c_je;&c_ch;&c_je;&c_r;&c_n;&c_ja;</trd>
</drv>
</art>
