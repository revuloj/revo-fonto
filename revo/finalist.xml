<drv mrk="finalist.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">SPO</uzo>
    <dif>
      Partoprenanto en finalo, t.e. <ref tip="dif" cel="final.0ano">finalano</ref>:
      <ekz>
        la sportisto-<tld/>o de la stafetkurado per tiu fajro ekbruligas la flamon en
        la Olimpia kaldrono, kiu brulas &ccirc;iutage dum la Ludoj da&ubreve;ras
        <fnt>
          <bib>Kon11</bib>
          <aut>Olga &Scirc;ilajeva</aut>
          <vrk><url ref="https://tekstaro.com/t?nomo=kontakto-2011-2019&amp;tipo=&amp;sekcio=kontakto-571">Olimpikoj: kolekti&gcirc;as pacaj batalantoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la foto estis <tld/>o &ccirc;e la foto-konkurso <nom>Nikon&#x2019;s Small World</nom> en 2017
        <fnt>
          <bib>Kon11</bib>
          <vrk><url ref="https://tekstaro.com/t?nomo=kontakto-2011-2019&amp;tipo=&amp;sekcio=kontakto-320"> Granda Eta Mondo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_f;&c_ib;&c_n;&c_a;&c_l;&c_ib;&c_s;&c_t;</trd>
    <trd lng="de">Finalist</trd>
    <trd lng="en">finalist</trd>
    <trd lng="fr">finaliste</trd>
    <trd lng="nl">finalist</trd>
    <trd lng="pl">finalista</trd>
    <trd lng="ru">&c_f;&c_i;&c_n;&c_a;&c_l;&c_i;&c_s;&c_t;</trd>
  </snc>
  <trdgrp lng="zh">
    <trd>&#x5165;&#x56F4;&#x8005; <pr>r&#xF9;w&#xE9;izh&#x11B;</pr></trd>,
    <trd>&#x51B3;&#x8D5B;&#x9009;&#x624B; <pr>ju&#xE9;s&#xE0;ixu&#x1CE;nsh&#x1D2;u</pr></trd>
  </trdgrp>
