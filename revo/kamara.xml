<drv mrk="kamara.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kamara.0igxi.KOMUNE">
    <dif>
      Fari&gcirc;i <tld/>o:
      <ekz>
        <tld/>i&gcirc;i <klr>[&#8230;]</klr> al alinaciano
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 10:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        homoj <klr>[&#8230;]</klr> jam <tld/>i&gcirc;is dum la trimonata
        unua kurso
        <fnt><aut>J. Francis</aut>,
        <vrk>La Granda Kaldrono</vrk>, 
        <lok>1978</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">devenir <ind>camarade</ind></trd>
  <trd lng="pl">skojarzy&cacute;</trd>
  <trd lng="ro">lega</trd>
</drv>

<drv mrk="kamara.0ino">
  <kap><tld/>ino</kap>
  <mlg>k-ino</mlg>
  <snc mrk="kamara.0ino.KOMUNE">
    <dif>
      Inseksa <tld/>o:
      <ekz>
        &scirc;i estas via <tld/>ino kaj via le&gcirc;a edzino
        <fnt><bib>MT</bib>, <lok>Mala&hcirc;i 2:14</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;iaj]</klr> okuloj <klr>[&#8230;]</klr> ser&ccirc;adis per la rigardo la rigardon de
        la <tld/>inoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        Marta sentis sin refre&scirc;igita kaj tre kortu&scirc;ita per la vido de la
        neatendite retrovita <tld/>ino de sia juneco
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Kameradin</trd>
  <trd lng="io">kamaradino</trd>
  <trd lng="pl">kole&zdot;anka</trd>
  <trd lng="ro">coleg&abreve;</trd>
  <trd lng="ru">&c_t;&c_o;&c_v;&c_a;&c_r;&c_k;&c_a;</trd>
</drv>
</art>
