<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kamara.xml,v 1.30 2023/10/18 14:48:09 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>kamarad</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kamara.0o">
  <kap><tld/>o</kap>
  <mlg>k-do</mlg>
  <snc mrk="kamara.0o.KOMUNE">
    <dif>
      Tiu, kiu lo&gcirc;as kutime en sama loko ol alia, kaj tial
      familiari&gcirc;as kun li; <ref tip="sin" cel="viv.kun0anto.kamarado">kunvivanto</ref>:
      <ekz>
        <ind>lerneja <tld/>o</ind>;
      </ekz>
      <ekz>
        miaj amikoj kaj <tld/>oj repa&scirc;is pro mia pesto
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 38:11</lok>
        </fnt>;
        <trd lng="de">Mitsch&uuml;ler</trd>
      </ekz>
      <ekz>
        mi fari&gcirc;is frato al la &scirc;akaloj kaj <tld/>o al la strutoj
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 30:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen unu rakontas al sia <tld/>o son&gcirc;on, kaj diras
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 7:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estas frato-<tld/>o kun &ccirc;iuj ministroj
        <fnt><bib>Revizoro</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kompan.0ano">kompaniano</ref>,
      <ref cel="kun.0ulo">kunulo</ref>
    </refgrp>
  </snc>
  <snc mrk="kamara.0o.LAB">
    <dif>
      Samokupulo, <ref cel="koleg.0o">kolego</ref>, ktp:
      <ekz>
        ne, diris la <ind>voj<tld/>o</ind>, tio ne estas nubo, tio estas monto
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
        <trd lng="de">Weggef&auml;hrte</trd>
      </ekz>
      <ekz>
        unu el liaj voja&gcirc;<tld/>oj en la kale&scirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        plej koran <tld/>an dankon mi esprimas <klr>[&#x2026;]</klr> al la senlaca loka
        organiza komitato
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Oka Kongreso Esperantista en Krakow en la 11a de a&ubreve;gusto 1912</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la malgrandaj testudoj estis miaj <tld/>oj de ludo
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        infanon malbonigas ne petolado, sed malbona <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kie mi povas trovi mian <ind>lud<tld/>o</ind>n
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>?
        <trd lng="de">Spielgef&auml;hrte</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ide.sam0ano">samideano</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_v;&c_a;&c_r;&c_y;&c_sh;</trd>
  <trdgrp lng="ca">
    <trd>camarada</trd>,
    <trd>company</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>druh</trd>,
    <trd>kamar&aacute;d</trd>,
    <trd>soudruh</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gef&auml;hrte</trd>,
    <trd>Kamerad</trd>,
    <trd>Kumpan</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>comrade</trd>,
    <trd>chum</trd>,
    <trd>pal</trd>,
    <trd>companion</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>camarada</trd>,
    <trd>colega</trd>
  </trdgrp>
  <trd lng="fr">camarade</trd>
  <trdgrp lng="hu">
    <trd>t&aacute;rs</trd>,
    <trd>pajt&aacute;s</trd>,
    <trd>elvt&aacute;rs</trd>
  </trdgrp>
  <trd lng="io">kamarado</trd>
  <trdgrp lng="ja">
    <trd>&#x4EF2;&#x9593;  <pr>&#x306A;&#x304B;&#x307E;</pr></trd>,
    <trd>&#x540C;&#x50DA;  <pr>&#x3069;&#x3046;&#x308A;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x540C;&#x5FD7;  <pr>&#x3069;&#x3046;&#x3057;</pr></trd>
  </trdgrp>
  <trd lng="nl">kameraad</trd>
  <trd lng="oc">camarada</trd>
  <trdgrp lng="pl">
    <trd>towarzysz</trd>,
    <trd>druh</trd>,
    <trd>kompan</trd>,
    <trd>kumpel</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>tovar&abreve;&#x219;</trd>,
    <trd>amic</trd>,
    <trd>camarad</trd>,
    <trd>prieten</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_o;&c_v;&c_a;&c_r;&c_i;&c_shch;</trd>
  <trdgrp lng="sk">
    <trd>kamar&aacute;t</trd>,
    <trd>priate&lcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_t;&#x43E;&c_v;&c_a;&c_r;&c_i;&c_sh;</trd>
  <trd lng="zh">&#x540C;&#x5FD7; <pr>t&oacute;ngzh&igrave;</pr></trd>
</drv>

