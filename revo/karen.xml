  <trd lng="fr">car&egrave;ne</trd>
  <trd lng="hu">haj&oacute;test v&iacute;z alatti r&eacute;sze</trd>
  <trd lng="nl">romp<klr tip="ind"> (v. schip)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_n;&c_o;</trd>,
    <trd>&c_d;&c_n;&c_i;&c_shch;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_v;&c_o;&c_d;&c_n;&c_a;&c_ja; &c_ch;&c_a;&c_s;&c_t;&c_mol; &c_s;&c_u;&c_d;&c_n;&c_a;</trd>
  </trdgrp>
</drv>
</art>
