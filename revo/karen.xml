<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: karen.xml,v 1.23 2023/12/03 08:00:23 revo Exp $">
<kap>
  <rad>karen</rad>/o
</kap>
<drv mrk="karen.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      La malsupra parto de &scirc;ipo, kiu trovi&gcirc;as sub la akvolinio:
      <ekz>
        la <tld/>oj estas ofte tegitaj per kupro.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_v;&c_o;&c_d;&c_n;&c_a;&c_ja; 
&c_ch;&c_a;&c_s;&c_t;&c_k;&c_a; <klr tip="amb">(&c_k;&c_a;&c_r;&c_a;&c_b;&c_l;&c_ja;, 
&c_l;&c_o;&c_d;&c_k;&c_ib;)</klr></trd>
  <trd lng="ca">carena</trd>
  <trd lng="de">Unterwasserteil des Schiffsrumpfes</trd>
  <trd lng="en">underwater sides of the <ind>hull</ind></trd>
  <trd lng="fr">car&egrave;ne</trd>
  <trd lng="hu">haj&oacute;test v&iacute;z alatti r&eacute;sze</trd>
  <trd lng="nl">romp<klr tip="ind"> (onder de waterlijn)</klr></trd>
  <trdgrp lng="pl">
    <trd>okr&eogonek;tu sp&oacute;d</trd>,
    <trd>kil</trd>,
    <trd>st&eogonek;pka</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>chil&abreve;</trd>,
    <trd>partea de dedesubt a navei</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_n;&c_o;</trd>,
    <trd>&c_d;&c_n;&c_i;&c_shch;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_v;&c_o;&c_d;&c_n;&c_a;&c_ja; 
&c_ch;&c_a;&c_s;&c_t;&c_mol; &c_s;&c_u;&c_d;&c_n;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="karen.K0o">
  <kap><tld lit="K"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
  </kap>
  <mlg>Car</mlg>
  <snc>
    <dif>
      Sud-hemisfera <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref> (<trd lng="la">Carina</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_K;&c_ib;&c_l;&c_mol; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#x2019;&c_je;)</klr></trd>
  <trd lng="ca">la Quilla</trd>
  <trd lng="cs">trup lodi nebo plov&aacute;ku hydropl&aacute;nu pod vodou</trd>
  <trd lng="de">Kiel des Schiffs</trd>
  <trd lng="en">Carina</trd>
  <trd lng="es">Quilla</trd>
  <trd lng="fa">&a_sh;&a_A;&a_h;&zwnj;&a_t;&a_kh;&a_t;&a_h;</trd>
  <trd lng="fr">Car&egrave;ne<klr tip="ind"> (constellation)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x55AB;&#x6C34;&#x90E8;  <pr>&#x304D;&#x3063;&#x3059;&#x3044;&#x3076;</pr></trd>,
    <trd>&#x8239;&#x5E95;  <pr>&#x305B;&#x3093;&#x3066;&#x3044;</pr></trd>,
    <trd>&#x7ADC;&#x9AA8;&#x7A81;&#x8D77;  <pr>&#x308A;&#x3085;&#x3046;&#x3053;&#x3064;&#x3068;&#x3063;&#x304D;</pr></trd>,
    <trd>&#x7ADC;&#x9AA8;&#x5F01;  <pr>&#x308A;&#x3085;&#x3046;&#x3053;&#x3064;&#x3079;&#x3093;</pr></trd>,
    <trd>&#x821F;&#x5F01;  <pr>&#x3075;&#x306D;&#x3079;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>Kiel<klr tip="ind"> (sterrenbeeld)</klr></trd>,
    <trd>Carina<klr tip="ind"> (sterrenbeeld)</klr></trd>
  </trdgrp>
  <trd lng="pl">Kil <klr>(konstelacja)</klr></trd>     
  <trd lng="ro">Carena <klr>(constela&#x21B;ie)</klr></trd>
  <trd lng="sk">povrch lode pod ponornou &ccaron;iarou</trd>
  <trdgrp lng="uk">
    <trd>&#x43E;&c_b;&c_t;&c_ib;&c_ch;&#x43D;&c_i;&#x43A;</trd>,
    <trd>&#x43F;&c_ib;&c_d;&c_v;&#x43E;&c_d;&#x43D;&c_a; &c_ch;&c_a;&c_s;&c_t;&c_i;&#x43D;&c_a; &c_s;&c_u;&c_d;&#x43D;&c_a;</trd>,
    <trd>&#x43A;&c_ib;&#x43B;&#x44C;</trd>,
    <trd>&c_ch;&#x43E;&c_v;&#x43D;&c_i;&#x43A;</trd>
  </trdgrp>
  <trd lng="zh">&#x8239;&#x5E95;&#x5EA7; <pr>Chu&#xE1;nd&#x1D0;zu&#xF2;</pr></trd>
</drv>

</art>
