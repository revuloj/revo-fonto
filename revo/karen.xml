  <trd lng="be">&c_p;&c_a;&c_d;&c_v;&c_o;&c_d;&c_n;&c_a;&c_ja; &c_ch;&c_a;&c_s;&c_t;&c_k;&c_a; <klr tip="amb">(&c_k;&c_a;&c_r;&c_a;&c_b;&c_l;&c_ja;, &c_l;&c_o;&c_d;&c_k;&c_ib;)</klr></trd>
