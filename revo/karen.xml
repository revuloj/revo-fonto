<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: karen.xml,v 1.25 2024/12/05 14:30:38 revo Exp $">
<kap>
  <rad>karen</rad>/o
  <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="karen.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">SHI</uzo>
  <snc mrk="karen.0o.SHI">
    <dif>
      La malsupra parto de <ref tip="malprt" cel="sxip.0o">&scirc;ipo</ref>, kiu trovi&gcirc;as sub la
      akvolinio;
      <ke>&scirc;ipfundo</ke>:
      <ekz>
        la <tld/>oj estas ofte tegitaj per kupro;
      </ekz>
      <ekz>
        la kareno <klr>[de skuno]</klr> estas longa kaj mallar&gcirc;a (4:1-proporcio) kun
        profunde elstara kilo kaj la varangoj havas, trans la tuta longo, S-formon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=596729"
              >Skuno</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bilgx.0o">bil&gcirc;o</ref>,
      <ref cel="kil.0o">kilo</ref>
    </refgrp>
    <trd lng="be">&c_p;&c_a;&c_d;&c_v;&c_o;&c_d;&c_n;&c_a;&c_ja;
  &c_ch;&c_a;&c_s;&c_t;&c_k;&c_a; <klr tip="amb">(&c_k;&c_a;&c_r;&c_a;&c_b;&c_l;&c_ja;,
  &c_l;&c_o;&c_d;&c_k;&c_ib;)</klr></trd>
    <trd lng="ca">carena</trd>
    <trd lng="de">Unterwasserteil des Schiffsrumpfes</trd>
    <trd lng="en">underwater sides of the <ind>hull</ind></trd>
    <trd lng="fr">car&egrave;ne</trd>
    <trd lng="hu">haj&oacute;test v&iacute;z alatti r&eacute;sze</trd>
    <trd lng="nl">romp<klr tip="ind"> (onder de waterlijn)</klr></trd>
    <trdgrp lng="pl">
      <trd>okr&eogonek;tu sp&oacute;d</trd>,
      <trd>kil</trd>,
      <trd>st&eogonek;pka</trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>chil&abreve;</trd>,
      <trd>partea de dedesubt a navei</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_n;&c_o;</trd>,
      <trd>&c_d;&c_n;&c_i;&c_shch;&c_je;</trd>,
      <trd>&c_p;&c_o;&c_d;&c_v;&c_o;&c_d;&c_n;&c_a;&c_ja;
  &c_ch;&c_a;&c_s;&c_t;&c_mol; &c_s;&c_u;&c_d;&c_n;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="karen.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <klr>(&ccirc;e fabacoj)</klr> La kunkreskintaj malsupraj korolaj folioj.
    </dif>
  <trd lng="be">&c_ch;&c_a;&c_w;&c_n;&c_a;&c_ch;&c_o;&c_k; <klr tip="amb">(&c_z;&c_r;&c_o;&c_s;&c_l;&c_y;&c_ja; &c_l;&c_ib;&c_s;&c_mol;&c_c;&c_ib;&c_k;&c_ib; &c_v;&c_ja;&c_n;&c_o;&c_ch;&c_k;&c_a;)</klr></trd>
  </snc>
</drv>

<drv mrk="karen.K0o">
  <kap><tld lit="K"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
  </kap>
  <mlg>Car</mlg>
  <uzo tip="fak">AST</uzo>
  <snc mrk="karen.K0o.AST">
    <dif>
      Sud-hemisfera <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref> (<trd
      lng="la">Carina</trd>):
      <ekz>
        NGC 2417 estas galaksio en konstelacio <tld lit="K"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=112302"
              >NGC 2417</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_K;&c_ib;&c_l;&c_mol; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#8217;&c_je;)</klr></trd>
  <trd lng="ca">la Quilla</trd>
  <trd lng="cs">trup lodi nebo plov&aacute;ku hydropl&aacute;nu pod vodou</trd>
  <trd lng="de">Kiel des Schiffs</trd>
  <trd lng="en">Carina</trd>
  <trd lng="es">Quilla</trd>
  <trd lng="fa">&a_sh;&a_A;&a_h;&#8204;&a_t;&a_kh;&a_t;&a_h;</trd>
  <trd lng="fr">Car&egrave;ne<klr tip="ind"> (constellation)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#21931;&#27700;&#37096;  <pr>&#12365;&#12387;&#12377;&#12356;&#12406;</pr></trd>,
    <trd>&#33337;&#24213;  <pr>&#12379;&#12435;&#12390;&#12356;</pr></trd>,
    <trd>&#31452;&#39592;&#31361;&#36215;  <pr>&#12426;&#12421;&#12358;&#12371;&#12388;&#12392;&#12387;&#12365;</pr></trd>,
    <trd>&#31452;&#39592;&#24321;  <pr>&#12426;&#12421;&#12358;&#12371;&#12388;&#12409;&#12435;</pr></trd>,
    <trd>&#33311;&#24321;  <pr>&#12405;&#12397;&#12409;&#12435;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>Kiel<klr tip="ind"> (sterrenbeeld)</klr></trd>,
    <trd>Carina<klr tip="ind"> (sterrenbeeld)</klr></trd>
  </trdgrp>
  <trd lng="pl">Kil <klr>(konstelacja)</klr></trd>
  <trd lng="ro">Carena <klr>(constela&#539;ie)</klr></trd>
  <trd lng="sk">povrch lode pod ponornou &ccaron;iarou</trd>
  <trdgrp lng="uk">
    <trd>&c_o;&c_b;&c_t;&c_ib;&c_ch;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_v;&c_o;&c_d;&c_n;&c_a; &c_ch;&c_a;&c_s;&c_t;&c_i;&c_n;&c_a; &c_s;&c_u;&c_d;&c_n;&c_a;</trd>,
    <trd>&c_k;&c_ib;&c_l;&c_mol;</trd>,
    <trd>&c_ch;&c_o;&c_v;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="zh">&#33337;&#24213;&#24231; <pr>Chu&aacute;nd&#464;zu&ograve;</pr></trd>
</drv>

</art>
