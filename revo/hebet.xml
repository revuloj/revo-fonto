<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hebet.xml,v 1.5 2022/04/26 06:37:24 revo Exp $">
<kap>
  <rad>hebet</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="hebet.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="hebet.0a.PSI">
    <dif>
      <ref tip="sin" cel="stupor.0a.PSI">Stupora</ref>,
      momente senreaga, sense a&ubreve; mense obtuzigita:
      <ekz>
        mi a&ubreve;skultis <tld/>e la verdikton de la kuracisto
        <fnt><aut>R. Radiguet, trad. M. Duc Goninaz</aut>, <vrk>Kun 
        Diablo en la Korpo</vrk>, <lok>2006</lok></fnt>;
      </ekz>
      <ekz>
        en la ardo de la alfronti&gcirc;oj juna viro <tld/>e atestas <klr>[&#8230;]</klr>
        <fnt>
          <bib>MD02</bib>
          <vrk>&Scirc;tatrenverso en Venezuelo</vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="perple.0a">perpleksa</ref>
    <refgrp tip="vid">
      <ref cel="embara.0i.FIG">embarasi<sncref/></ref>,
      <ref cel="konfuz.0i.perpleksigi">konfuzi<sncref/></ref>,
      <ref cel="konste1.0i">konsterni</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>bet&auml;ubt</trd>,
      <trd>benommen</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_g;&c_a;&c_l;&c_o;&c_m;&c_sh;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_u; <ind>&c_s;&c_t;&c_u;&c_p;&c_a;&c_r;&c_y;</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>dazed</trd>,
    <trd>stunned</trd>, <trd>stupefied</trd>
  </trdgrp>
  <trd lng="fr">h&eacute;b&eacute;t&eacute;</trd>
  <trd lng="uk">&c_t;&c_u;&#x43F;&#x43E;&c_u;&#x43C;&#x43D;&c_i;&c_j;</trd>
</drv>
</art>

