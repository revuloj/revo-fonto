<vortaro>
<art mrk="$Id$">
<kap>
  <rad>majonez</rad>/o
</kap>
<drv mrk="majone.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Speco de malvarma densa sa&ubreve;co, konsistanta el
      oleo, salo, pipro, vinagro a&ubreve; mustardo kaj ovoflava&jcirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_ja;&c_n;&c_e;&c_z;</trd>
  <trd lng="ca">maionesa</trd>
  <trdgrp lng="de">
    <trd>Mayonnaise</trd>,
    <trd>Majon&auml;se</trd>
  </trdgrp>
  <trd lng="en">mayonnaise</trd>
  <trd lng="es">mayonesa<klr> (salsa)</klr></trd>
  <trd lng="fr">mayonnaise</trd>
  <trd lng="hu">majon&eacute;z</trd>
  <trd lng="nl">mayonaise</trd>
  <trd lng="pl">majonez</trd>
  <trd lng="pt">maionese</trd>
  <trd lng="ru">&c_m;&c_a;&c_j;&c_o;&c_n;&c_je;&c_z;</trd>
  <trd lng="tr">majonez</trd>
</drv>
</art>
