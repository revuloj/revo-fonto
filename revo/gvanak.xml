<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: gvanak.xml,v 1.1 2023/07/20 16:31:31 afido Exp $">
    <kap>
      <rad>gvanak</rad>/o
    </kap>

<drv mrk="gvanak.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="gvanak.0o.ZOO">
    <dif>
      Sova&gcirc;a <ref tip="lst" lst="voko:zoologiaj_specioj" cel="speci.0o.BIO">specio</ref> de <ref tip="super" cel="lam1.0o">lamo</ref>
      (<trd lng="la">Lama guanicoe</trd>), vivanta en
      okcidenta kaj suda parto de Sudameriko:
      <ekz>
        <tld/>oj, lamoj, vikunoj kaj andaj vulpoj longavostaj
        videblas el la vagonaro
        <fnt>
          <bib>Monato</bib>
          <aut>Marta Sol&iacute;s</aut>
          <vrk><url ref="https://www.monato.be/2005/008399.php"
              >Unika fervojo en Andoj</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> sin vivtenis per &ccirc;asado de
        <tld/>oj, birdoj kaj kolektado de
        mituloj
        <fnt>
          <bib>Monato</bib>
          <aut>Marta Sol&iacute;s</aut>
          <vrk><url ref="https://www.monato.be/2006/008665.php"
              >La fino de l' mondo</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        alpako <klr>[&#8230;]</klr> parencas al aliaj parhufuloj
        kiel lamo, <tld/>o kaj vikuno
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="https://www.monato.be/2012/010528.php"
              >Bredi alpakojn</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Guanako</trd>
    <trd lng="en">Guanaco</trd>
    <trd lng="fr">Guanaco</trd>
  </snc>
</drv>

  </art>

