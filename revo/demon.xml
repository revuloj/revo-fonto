<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: demon.xml,v 1.25 2023/08/31 10:43:59 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>demon</rad>/o
    </kap>

    <drv mrk="demon.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="demon.0o.diablo">
        <uzo tip="fak">KRI</uzo>
        <dif>
          <ref tip="super" cel="dajmon.0o">Dajmono</ref>, kiu ne estas
          bona <ref tip="ant" cel="angxel.0o">an&gcirc;elo</ref>:
          <ekz>
            en la kristana mitologio la <tld/>oj estas kaj la
            ribelintaj an&gcirc;eloj, kaj la paganaj eksdioj, precipe la
            egiptaj, kun bestaj kapoj, a&ubreve; kaprokruraj, kiel la
            greka
            Pajno.
          </ekz>
        </dif>
        <trdgrp lng="fr">
          <trd>d&eacute;mon<klr tip="ind"> (esprit malin)</klr></trd>,
          <trd>esprit malin</trd>
        </trdgrp>
        <trd lng="pl">demon</trd>
      </snc>
      <snc mrk="demon.0o.homo">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Homo, havanta tre malican karakteron.
        </dif>
      </snc>
  <trdgrp lng="be">
        <trd>&c_d;&c_e;&c_m;&c_a;&c_n;</trd>,
        <trd>&c_b;&c_je;&c_s;</trd>
      </trdgrp>
  <trd lng="bg">&c_d;&c_je;&c_m;&c_o;&c_n;</trd>
  <trd lng="bo">&#xF56;&#xF51;&#xF74;&#xF51;&#xF0B;</trd>
  <trdgrp lng="ca">
        <trd>dimoni</trd>,
        <trd>diable</trd>
      </trdgrp>
  <trdgrp lng="cs">
    <trd>b&#x11B;s</trd>,
    <trd>d&#xE9;mon</trd>,
    <trd>zloduch</trd>,
    <trd>&#x10F;as</trd>
  </trdgrp>
  <trd lng="de">D&auml;mon</trd>
  <trd lng="en">demon</trd>
  <trd lng="es">demonio</trd>
  <trdgrp lng="fa">
        <trd>&a_A;&a_h;&a_r;&f_y;&a_m;&a_n;</trd>,
        <trd>&a_d;&f_y;&a_w;</trd>
      </trdgrp>
  <trdgrp lng="fr">
        <trd>d&eacute;mon<klr tip="ind"> (furie)</klr></trd>,
        <trd>furie</trd>
      </trdgrp>
  <trd lng="he">&#x5E9;&#x5C1;&#x5B5;&#x5D3;</trd>
  <trd lng="hu">d&eacute;mon</trd>
  <trdgrp lng="ja">
    <trd>&#x9B54;&#x795E;  [<ind>&#x307E;&#x3058;&#x3093;</ind>]</trd>,
    <trd>&#x60AA;&#x970A;  [<ind>&#x3042;&#x304F;&#x308A;&#x3087;&#x3046;</ind>]</trd>,
    <trd>&#x60AA;&#x9B3C;  [<ind>&#x3042;&#x3063;&#x304D;</ind>]</trd>,
    <trd/>,
    <trd>&#x60AA;&#x9B54;  [<ind>&#x3042;&#x304F;&#x307E;</ind>]</trd>,
    <trd>&#x60AA;&#x9B3C;&#x306E;&#x3088;&#x3046;&#x306A;&#x4EBA;  [<ind>&#x3042;&#x304F;&#x304A;&#x306B;&#x306E;&#x3088;&#x3046;&#x306A;&#x3058;&#x3093;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">demon</trd>
  <trd lng="no">demon</trd>
  <trd lng="pt">dem&ocirc;nio</trd>
  <trdgrp lng="ru">
        <trd>&c_d;&c_je;&c_m;&c_o;&c_n;</trd>,
        <trd>&c_b;&c_je;&c_s;</trd>
      </trdgrp>
  <trd lng="sk">d&eacute;mon</trd>
  <trdgrp lng="uk">
    <trd>&#x434;&#x435;&#x43C;&#x43E;&#x43D;</trd>,
    <trd>&#x437;&#x43B;&#x438;&#x439; &#x434;&#x443;&#x445;</trd>,
    <trd>&#x434;&#x438;&#x44F;&#x432;&#x43E;&#x43B;</trd>,
    <trd>&#x431;&#x456;&#x441;</trd>
  </trdgrp>
</drv>
