<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>demon</rad>/o
</kap>
<drv mrk="demon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">REL</uzo>
    <dif>
      Diablo.
    </dif>
    <trdgrp lng="fr">
      <trd>d&eacute;mon<klr tip="ind"> (esprit malin)</klr></trd>,
      <trd>esprit malin</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Homo, havanta tre malican karakteron.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>d&eacute;mon<klr tip="ind"> (furie)</klr></trd>,
    <trd>furie</trd>
  </trdgrp>
  <trd lng="hu">d&eacute;mon</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_m;&c_o;&c_n;</trd>,
    <trd>&c_b;&c_je;&c_s;</trd>
  </trdgrp>
</drv>
</art>
