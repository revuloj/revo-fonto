<drv mrk="konven.0a">
  <kap><tld/>a</kap>
  <snc mrk="konven.0a.KOMUNE">
    <dif>
      <tld lit="K"/>anta:
    </dif>
    <subsnc mrk="konven.0a.alsituacio">
      <dif>
        <tld lit="K"/>anta al situacio:
        <ekz>
          tiu &ccirc;i momento ne estas <tld/>a por revoj;
        </ekz>
        <ekz>
          ser&ccirc;i en la vagonaro <tld/>an kupeon;
        </ekz>
        <ekz>
          niaj soldatoj trovos <tld/>an por si okupon
          <fnt>
            <bib>Far2</bib>
            <lok>&Ccirc;apitro XVIII</lok>
          </fnt>;
        </ekz>
        <ekz>
          por funebro <klr>[tiuj ru&gcirc;aj &scirc;uoj]</klr> certe
          estis ne tre <tld/>aj
        <fnt>
          <bib>Fab2</bib>
          <lok>Ru&gcirc;aj &scirc;uoj</lok>
        </fnt>;
        </ekz>
        <ekz>
          <tld/>a tempo por efektivigi ion;
        </ekz>
        <ekz>
          tiu &ccirc;i esprimo estus pli <tld/>a por alia
          okazo
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="konfor.0a">konforma</ref>,
        <ref cel="oportu.0a">oportuna</ref>.
      </refgrp>
      <trdgrp lng="fr">
        <trd>adapt&eacute;</trd>,
        <trd>ad&eacute;quat</trd>,
        <trd>bienvenu</trd>,
        <trd>convenable</trd>,
        <trd>indiqu&eacute;</trd>,
        <trd>judicieux</trd>
        <trd>opportun</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>alkalmas</trd>,
        <trd>megfelel&odblac;</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="konven.0a.harmonia">
      <dif>
        <tld lit="K"/>anta al sentoj, al socia kondi&ccirc;aro:
        <ekz>
          ha, panjo, por vi pajlokolora <klr>[vesto]</klr> ne estas al<tld/>a
          <fnt>
            <bib>Revizoro</bib>
            <lok>Akto tria</lok>
          </fnt>;
        </ekz>
        <ekz>
          Ni rekompencos la viziton vian &#11005; Per plej <tld/>a re&gcirc;a rekompenco
          <fnt>
            <bib>Hamlet</bib>
            <lok>Hamleto</lok>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="fr">
        <trd>adapt&eacute;</trd>,
        <trd>ad&eacute;quat</trd>,
        <trd>bienvenu</trd>,
        <trd>convenable</trd>,
        <trd>indiqu&eacute;</trd>,
        <trd>judicieux</trd>,
        <trd>opportun</trd>,
      </trdgrp>
      <trd lng="hu">illik</trd>
    </subsnc>
    <subsnc mrk="konven.0a.MOR">
      <dif>
        <tld lit="K"/>anta al moroj, <ref cel="dec.0a">deca</ref>:
        <ekz>
          &ccirc;ar li estis en uniformo, li opiniis ne<tld/>a tiel la&ubreve;te krii
          <fnt>
            <bib>Fab1</bib>
            <lok>Persista stana soldato</lok>
          </fnt>;
        </ekz>
        <ekz>
          <klr>[ili]</klr> metis la piedojn sur la tablo, &ccirc;ar ili
          opiniis, ke &ccirc;io estas <tld/>a por ili
          <fnt>
            <bib>Fab2</bib>
            <lok>Monteto de elfoj</lok>
          </fnt>;
        </ekz>
        <ekz>
          severa moralisto trovos mian sinceron ne<tld/>a
          <fnt>
            <bib>PatrojFiloj</bib>
            <lok>III</lok>
          </fnt>;
        </ekz>
        <ekz>
          tiujn scenojn li ne konsideris <tld/>aj por la okuloj de
          juna virino.
        </ekz>
      </dif>
      <trdgrp lng="fr">
        <trd>adapt&eacute;</trd>
        <trd>convenable</trd>,
        <trd>d&eacute;cent</trd>
        <trd>indiqu&eacute;</trd>
        <trd>judicieux</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>illik</trd>,
        <trd>illend&odblac;</trd>
      </trdgrp>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="akord.0a.konveneco">akorda<sncref/></ref>,
      <ref cel="konfor.0a.obea">konforma<sncref/></ref>,
      <ref cel="konven1.0a">konvencia</ref>,
      <ref cel="kutim.0a">kutima</ref>
    </refgrp>
  </snc>
