<drv mrk="konven.0a">
  <kap><tld/>a</kap>
  <snc mrk="konven.0a.KOMUNE">
    <dif>
      <tld lit="K"/>anta:
    </dif>
    <subsnc mrk="konven.0a.alsituacio">
      <dif>
        <tld lit="K"/>anta al situacio:
        <ekz>
          tiu &ccirc;i momento ne estas <tld/>a por revoj;
        </ekz>
        <ekz>
          ser&ccirc;i en la vagonaro <tld/>an kupeon;
        </ekz>
        <ekz>
          niaj soldatoj trovos <tld/>an por si okupon
          <fnt>
            <bib>Far2</bib>
            <lok>&Ccirc;apitro XVIII</lok>
          </fnt>;
        </ekz>
        <ekz>
          por funebro <klr>[tiuj ru&gcirc;aj &scirc;uoj]</klr> certe
          estis ne tre <tld/>aj
        <fnt>
          <bib>Fab2</bib>
          <lok>Ru&gcirc;aj &scirc;uoj</lok>
        </fnt>;
        </ekz>
        <ekz>
          <tld/>a tempo por efektivigi ion;
        </ekz>
        <ekz>
          tiu &ccirc;i esprimo estus pli <tld/>a por alia
          okazo
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="konfor.0a">konforma</ref>,
        <ref cel="oportu.0a">oportuna</ref>.
      </refgrp>
      <trdgrp lng="fr">
        <trd>adapt&eacute;</trd>,
        <trd>ad&eacute;quat</trd>,
        <trd>bienvenu</trd>,
        <trd>convenable</trd>,
        <trd>indiqu&eacute;</trd>,
        <trd>judicieux</trd>
        <trd>opportun</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>alkalmas</trd>,
        <trd>megfelel&odblac;</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="konven.0a.harmonia">
      <dif>
        <tld lit="K"/>anta al sentoj, al socia kondi&ccirc;aro:
        <ekz>
          ha, panjo, por vi pajlokolora <klr>[vesto]</klr> ne estas al<tld/>a
          <fnt>
            <bib>Revizoro</bib>
            <lok>Akto tria</lok>
          </fnt>;
        </ekz>
        <ekz>
          Ni rekompencos la viziton vian &#11005; Per plej <tld/>a re&gcirc;a rekompenco
          <fnt>
            <bib>Hamlet</bib>
            <lok>Hamleto</lok>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="fr">
        <trd>adapt&eacute;</trd>,
        <trd>ad&eacute;quat</trd>,
        <trd>bienvenu</trd>,
        <trd>convenable</trd>,
        <trd>indiqu&eacute;</trd>,
        <trd>judicieux</trd>,
        <trd>opportun</trd>,
      </trdgrp>
      <trd lng="hu">illik</trd>
    </subsnc>
    <subsnc mrk="konven.0a.MOR">
      <dif>
        <tld lit="K"/>anta al moroj, <ref cel="dec.0a">deca</ref>:
        <ekz>
          &ccirc;ar li estis en uniformo, li opiniis ne<tld/>a tiel la&ubreve;te krii
          <fnt>
            <bib>Fab1</bib>
            <lok>Persista stana soldato</lok>
          </fnt>;
        </ekz>
        <ekz>
          <klr>[ili]</klr> metis la piedojn sur la tablo, &ccirc;ar ili
          opiniis, ke &ccirc;io estas <tld/>a por ili
          <fnt>
            <bib>Fab2</bib>
            <lok>Monteto de elfoj</lok>
          </fnt>;
        </ekz>
        <ekz>
          severa moralisto trovos mian sinceron ne<tld/>a
          <fnt>
            <bib>PatrojFiloj</bib>
            <lok>III</lok>
          </fnt>;
        </ekz>
        <ekz>
          tiujn scenojn li ne konsideris <tld/>aj por la okuloj de
          juna virino.
        </ekz>
      </dif>
      <trdgrp lng="fr">
        <trd>adapt&eacute;</trd>
        <trd>convenable</trd>,
        <trd>d&eacute;cent</trd>
        <trd>indiqu&eacute;</trd>
        <trd>judicieux</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>illik</trd>,
        <trd>illend&odblac;</trd>
      </trdgrp>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="akord.0a.konveneco">akorda<sncref/></ref>,
      <ref cel="konfor.0a.obea">konforma<sncref/></ref>,
      <ref cel="konven1.0a">konvencia</ref>,
      <ref cel="kutim.0a">kutima</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_d;&c_a;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_d;&c_a;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_a;&c_l;&c_je;&c_zh;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_o;&c_d;&c_h;&c_o;&c_d;&c_ja;&c_shch;</trd>
  <trd
  lng="bo">&t_sa;&t_tata;&t_ba;&t_sa;&t_tsheg;&t_ba;&t_da;&t_drengbu;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>
  <trd lng="de">passend</trd>
  <trd lng="es">conveniente</trd>
  <trd lng="nl">passend</trd>
  <trdgrp lng="pl">
    <trd>adekwatny</trd>,
    <trd>dobrany</trd>,
    <trd>celowy</trd>,
    <trd>godziwy</trd>,
    <trd>nale&zdot;yty</trd>,
    <trd>odpowiedni</trd>,
    <trd>przyzwoity</trd>,
    <trd>stosowny</trd>,
    <trd>w&lstroke;a&sacute;ciwy</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>potrivit</trd>,
    <trd>convenabil</trd>,
    <trd>adecvat</trd>
  </trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_h;&c_o;&c_d;&c_ja;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_g;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_o;&c_b;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_u;&c_m;&c_je;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>  
</drv>

<drv mrk="konven.0e">
  <kap><tld/>e</kap>
  <snc mrk="konven.0e.KOMUNE">
    <dif>
      <ref cel="gxust.0e.precize">&Gcirc;uste</ref>, ekzakte, 
      proporcie, trafe, <ref cel="oportu.0e">oportune</ref>:
      <ekz>
        nejuna elfknabino <klr>[&#8230;]</klr> krom la nuda dorso estis tre <tld/>e vestita
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e honoru lin, kiel amikon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        venu al la kontoro iun tagon, &dash; kiam estos <tld/>e? &dash; miaparte tuj
        <fnt>
          <bib>HsT</bib>
          <lok>Sia propra mastro</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi povos pagi kiam estos <tld/>e al vi, kaj pri rento vi ne pensu
        <fnt>
          <bib>HsT</bib>
          <lok>Kuni&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        estas en efektiveco ne <tld/>e tion rakonti
        <fnt>
          <bib>Fab3</bib>
          <lok>Estas tute certe</lok>
        </fnt>;
      </ekz>
      <ekz>
        Beatrico <tld/>e edzini&gcirc;is
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_p;&c_a;&c_v;&c_je;&c_d;&c_n;&c_a;</trd>,
    <trd>&c_d;&c_a;&c_r;&c_e;&c_ch;&c_n;&c_a;</trd>,
    <trd>&c_s;&c_l;&c_u;&c_sh;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>angemessen</trd>,
    <trd>passend</trd>,
    <trd>geb&uuml;hrend</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>convenablement</trd>,
    <trd>correctement</trd>,
    <trd>proprement</trd>,
    <trd>d&eacute;cemment</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ill&odblac; m&oacute;don</trd>,
    <trd>megfelel&odblac;en</trd>
    <trd>illend&odblac;en</trd>
  </trdgrp>
  <trd lng="es">convenientemente</trd>
  <trd lng="nl">toepasselijk</trd>
  <trdgrp lng="pl">
    <trd>odpowiednio</trd>,
    <trd>tak jak nale&zdot;y</trd>,
    <trd>w&lstroke;a&sacute;ciwie</trd>
  </trdgrp>  
  <trdgrp lng="ro">
    <trd>respectiv</trd>,
    <trd>cum se cuvine</trd>
  </trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_d;&c_l;&c_je;&c_zh;&c_a;&c_shch;&c_i;&c_m; &c_o;&c_b;&c_r;&c_a;&c_z;&c_o;&c_m;</trd>,
    <trd>&c_u;&c_m;&c_je;&c_s;&c_t;&c_n;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="konven.0eal">
  <kap><tld/>e al</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="konven.0eal.KOMUNE">
    <dif>
      Agante
      <ref tip="super" cel="laux.0.io_obeata">la&ubreve;<sncref/></ref>
      difinita cirkonstanco,
      <ref tip="vid" cel="akord.0ekun.KOMUNE">akorde kun</ref>...:
      <ekz>
        li neniel bremsis la pa&scirc;ojn <tld/>e al la temperaturo
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun viza&gcirc;o plena je gajeco kaj espero tute <tld/>e al la bela mateno
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        retrorigardu la militon por pli bona futura kunlaboro <klr>[&#8230;]</klr>, <tld/>e al
        la &gcirc;enerala temo de la kongreso, &leftquot;Kunlaboro por Paco, Stabileco kaj Disvolvo&rightquot;
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 4 (54)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u; <ind>&c_a;&c_d;&c_p;&c_a;&c_v;&c_je;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ib;</ind> &c_z;</trd>
  <trdgrp lng="de">
    <trd>entsprechend</trd>,
    <trd>dem ... <ind>angemessen</ind></trd>
  </trdgrp>
  <trd lng="fr">en accord avec</trd>
  <trd lng="pl">prawid&lstroke;owo</trd>     
  <trd lng="ro">&icirc;n mod corect</trd>
</drv>

<drv mrk="konven.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Eco de tio, kio estas <tld/>a al situacio, al &ccirc;irka&ubreve;mondo:
      <ekz>
        <tld/>eco de esprimo, vesto, lo&gcirc;ejo.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Angemessenheit</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>ad&eacute;quation</trd>,
      <trd>conformit&eacute;</trd>,
      <trd>convenance</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>megfelel&odblac;s&eacute;g</trd>,
      <trd>alkalmass&aacute;g</trd>
    </trdgrp>
    <trd lng="ru">&c_u;&c_m;&c_je;&c_s;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      <ref tip="dif" cel="dec.0o">Dececo</ref>:
      <ekz>
        la reguloj de la <tld/>eco estis plenumitaj
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi transpa&scirc;as la limojn de <tld/>eco
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        pro sento de alta <tld/>eco, la markizino ne volis, ke la gejunuloj
        <klr>[&#8230;]</klr> estu devigataj toleri la malagrabla&jcirc;ojn de sen&ccirc;esaj renkontoj
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        via onklino ne &ccirc;eestas plu por permesi miajn vizitojn, la <tld/>eco min devigas
        eliri
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        nur por <tld/>eco vi rigardas la desegna&jcirc;ojn
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XVI</lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      <em>Deci</em> rilatas al &gcirc;eneralaj reguloj, kaj tial estas &ccirc;iam
      uzata senkomplemente; <em>konveni</em> rilatas al aparta situacio,
      kaj tial estas plejofte uzata kun komplemento; sed tiu distingo ne
      estas &ccirc;iam precize observata.
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_r;&c_e;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_l;&c_u;&c_sh;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_s;&c_t;&c_o;&c_j;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="es">conveniencia</trd>
  <trdgrp lng="fr">
    <trd>biens&eacute;ance</trd>,
    <trd>d&eacute;cence</trd>,
    <trd>savoir-vivre</trd>
  </trdgrp>
  <trd lng="hu">illend&odblac;s&eacute;g</trd>
  <trdgrp lng="pl">
    <trd>przyzwoito&sacute;&cacute;</trd>,
    <trd>nale&zdot;yto&sacute;&cacute;</trd>
  </trdgrp>
  <trd lng="ro">decen&tcedil;&abreve;</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_l;&c_i;&c_ch;&c_i;&c_je;</trd>
</drv>

<drv mrk="konven.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="konven.mal0a.KOMUNE">
    <dif>
      Ne <tld/>a, ne akorda kun la cirkonstancoj, moroj, kutimoj:
      <ekz>
        ni &ndash; pro mal<tld/>aj kaj insultaj kondutoj de la &ccirc;irka&ubreve;uloj &ndash;
        kredas nin senvaloraj ekde infana&gcirc;o
        <fnt>
          <aut>Tatoeba</aut>,
          <vrk><url ref="http://tatoeba.org/ara/sentences/show/622512">
          Frazo 622512</url></vrk>,
          <lok>[vidita en 2015]</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekstra1.0a">ekstravaganca</ref>,
      <ref cel="dec.mal0a">maldeca</ref>,
      <ref cel="oportu.mal0a">maloportuna</ref>,
      <ref cel="skuril.0a">skurila</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_a;&c_d;&c_p;&c_a;&c_v;&c_je;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_r;&c_y;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_w;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trd
  lng="bo">&t_sa;&t_tata;&t_ba;&t_sa;&t_tsheg;&t_ba;&t_da;&t_drengbu;&t_tsheg;&t_pa;&t_naro;&t_tsheg;&t_ma;&t_gigu;&t_da;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trdgrp lng="de">
    <trd>unziemlich</trd>,
    <trd>unangebracht</trd>,
    <trd>daneben <klr>(benehmen)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>inappropri&eacute;</trd>,
    <trd>impropre</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>nieodpowiedni</trd>,
    <trd>niestosowny</trd>,
    <trd>niew&lstroke;a&sacute;ciwy</trd>
  </trdgrp>   
<trdgrp lng="ro">
    <trd>inaplicabil</trd>,
    <trd>nepotrivit</trd>,
    <trd>impropriu</trd>
  </trdgrp> 
</drv>

<drv mrk="konven.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="konven.mal0i.KOMUNE">
    <dif>
      Ne <ref tip="ant" cel="akord.0i.konveni">akordi</ref> kun cirkonstancoj, reguloj, deco:
      <ekz>
        la parolo de <nom>Kyuma Fumio</nom> mal<tld/>as al lia rolo
        kiel nacidefenda ministro
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url ref="&CRI;161/2007/07/06/1@65160.htm">
          Abdiko de nacidefenda ministro embarasis japanan
          kabineton</url></vrk>,
          <lok>2007-07-06</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne mal<tld/>as scii, &ccirc;u li apogas serbojn, &ccirc;u kroatojn, por ke la&ubreve;eble mi
        subtenu la antagonajn kaj tiel pu&scirc;u lin en sakstraton
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 6</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je; &c_p;&c_a;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_je; &c_a;&c_d;&c_p;&c_a;&c_v;&c_ja;&c_d;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sich nicht <ind>ziemen</ind></trd>,
    <trd><ind>unangemessen</ind> sein</trd>
  </trdgrp>
  <trd lng="fr">&ecirc;tre <ind>inappropri&eacute;</ind></trd>
  <trd lng="pl">nie pasowa&cacute;</trd>     
  <trd lng="ro">nu se potrive&#537;te</trd>
</drv>
