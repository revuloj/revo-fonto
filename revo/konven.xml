<drv mrk="konven.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="konven.mal0i.KOMUNE">
    <dif>
      Ne <ref tip="ant" cel="akord.0i.konveni">akordi</ref> kun cirkonstancoj, reguloj, deco:
      <ekz>
        la parolo de <nom>Kyuma Fumio</nom> mal<tld/>as al lia rolo
        kiel nacidefenda ministro
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url ref="&CRI;161/2007/07/06/1@65160.htm">
          Abdiko de nacidefenda ministro embarasis japanan
          kabineton</url></vrk>,
          <lok>2007-07-06</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne mal<tld/>as scii, &ccirc;u li apogas serbojn, &ccirc;u kroatojn, por ke la&ubreve;eble mi
        subtenu la antagonajn kaj tiel pu&scirc;u lin en sakstraton
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 6</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je; &c_p;&c_a;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_je; &c_a;&c_d;&c_p;&c_a;&c_v;&c_ja;&c_d;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sich nicht <ind>ziemen</ind></trd>,
    <trd><ind>unangemessen</ind> sein</trd>
  </trdgrp>
  <trd lng="fr">&ecirc;tre <ind>inappropri&eacute;</ind></trd>
  <trd lng="pl">nie pasowa&cacute;</trd>
  <trd lng="ro">nu se potrive&#537;te</trd>
  <trd lng="zh">&#x4E0D;&#x76F8;&#x79F0; <pr>b&#xF9;xi&amacron;ngch&#xE8;ng</pr></trd>
</drv>
