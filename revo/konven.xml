  <trd lng="es">conveniente</trd>
  <trd lng="nl">passend</trd>
  <trdgrp lng="pl">
    <trd>adekwatny</trd>,
    <trd>dobrany</trd>,
    <trd>celowy</trd>,
    <trd>godziwy</trd>,
    <trd>nale&zdot;yty</trd>,
    <trd>odpowiedni</trd>,
    <trd>przyzwoity</trd>,
    <trd>stosowny</trd>,
    <trd>w&lstroke;a&sacute;ciwy</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>potrivit</trd>,
    <trd>convenabil</trd>,
    <trd>adecvat</trd>
  </trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_h;&c_o;&c_d;&c_ja;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_g;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_o;&c_b;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_u;&c_m;&c_je;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>  
</drv>

<drv mrk="konven.0e">
  <kap><tld/>e</kap>
  <snc mrk="konven.0e.KOMUNE">
    <dif>
      &Gcirc;uste, ekzakte, proporcie, trafe, oportune:
      <ekz>
        <tld/>e plenumi sian taskon;
      </ekz>
      <ekz>
        <tld/>e &scirc;ati iun.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>passend</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>convenablement</trd>,
    <trd>correctement</trd>,
    <trd>proprement</trd>,
    <trd>d&eacute;cemment</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ill&odblac; m&oacute;don</trd>,
    <trd>megfelel&odblac;en</trd>
    <trd>illend&odblac;en</trd>
  </trdgrp>
  <trd lng="es">convenientemente</trd>
  <trd lng="nl">toepasselijk</trd>
  <trdgrp lng="pl">
    <trd>odpowiednio</trd>,
    <trd>tak jak nale&zdot;y</trd>,
    <trd>w&lstroke;a&sacute;ciwie</trd>
  </trdgrp>  
  <trdgrp lng="ro">
    <trd>respectiv</trd>,
    <trd>cum se cuvine</trd>
  </trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_d;&c_l;&c_je;&c_zh;&c_a;&c_shch;&c_i;&c_m; &c_o;&c_b;&c_r;&c_a;&c_z;&c_o;&c_m;</trd>,
    <trd>&c_u;&c_m;&c_je;&c_s;&c_t;&c_n;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="konven.0eal">
  <kap><tld/>e al</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="konven.0eal.KOMUNE">
    <dif>
      Agante
      <ref tip="super" cel="laux.0.io_obeata">la&ubreve;<sncref/></ref>
      difinita cirkonstanco,
      <ref tip="vid" cel="akord.0ekun.KOMUNE">akorde kun</ref>...:
      <ekz>
        li neniel bremsis la pa&scirc;ojn <tld/>e al la temperaturo
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">en accord avec</trd>
  <trd lng="pl">prawid&lstroke;owo</trd>     
  <trd lng="ro">&icirc;n mod corect</trd>
</drv>

<drv mrk="konven.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Eco de tio, kio estas <tld/>a al situacio, al &ccirc;irka&ubreve;mondo:
      <ekz>
        <tld/>eco de esprimo, vesto, lo&gcirc;ejo.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Angemessenheit</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>ad&eacute;quation</trd>,
      <trd>conformit&eacute;</trd>,
      <trd>convenance</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>megfelel&odblac;s&eacute;g</trd>,
      <trd>alkalmass&aacute;g</trd>
    </trdgrp>
    <trd lng="ru">&c_u;&c_m;&c_je;&c_s;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Dececo:
      <ekz>
        la reguloj de <tld/>eco<fnt>B</fnt>;
      </ekz>
      <ekz>
        nur por <tld/>eco li rigardis la desegna&jcirc;ojn<fnt>B</fnt>.
      </ekz>
    </dif>
    <rim>
      <em>Deci</em> rilatas al &gcirc;eneralaj reguloj, kaj tial estas &ccirc;iam
      uzata senkomplemente; <em>konveni</em> rilatas al aparta situacio,
      kaj tial estas plejofte uzata kun komplemento; sed tiu distingo ne
      estas &ccirc;iam precize observata.
    </rim>
  </snc>
  <trd lng="es">conveniencia</trd>
  <trdgrp lng="fr">
    <trd>biens&eacute;ance</trd>,
    <trd>d&eacute;cence</trd>,
    <trd>savoir-vivre</trd>
  </trdgrp>
  <trd lng="hu">illend&odblac;s&eacute;g</trd>
  <trdgrp lng="pl">
    <trd>przyzwoito&sacute;&cacute;</trd>,
    <trd>nale&zdot;yto&sacute;&cacute;</trd>
  </trdgrp>
  <trd lng="ro">decen&tcedil;&abreve;</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_l;&c_i;&c_ch;&c_i;&c_je;</trd>
</drv>

<drv mrk="konven.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="konven.mal0a.KOMUNE">
    <dif>
      Ne <tld/>a, ne akorda kun la cirkonstancoj, moroj, kutimoj:
      <ekz>
        ni &ndash; pro mal<tld/>aj kaj insultaj kondutoj de la &ccirc;irka&ubreve;uloj &ndash;
