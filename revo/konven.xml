    <trd>&#x4E0D;&#x4F3C;&#21512;&#12356;&#x306A;  <pr>&#12405;&#x306B;&#12354;&#12356;&#x306A;</pr></trd>,
    <trd>&#x4E0D;&#x4F5C;&#x6CD5;&#x306A;  <pr>&#12406;&#12373;&#x307B;&#12358;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
