  <trd lng="bo">&t_sa;&t_tata;&t_ba;&t_sa;&t_tsheg;&t_ba;&t_da;&t_drengbu;&t_tsheg;&t_pa;&t_naro;&t_tsheg;&t_ma;&t_gigu;&t_da;&t_tsheg;&t_pa;&t_tsheg;</trd>
