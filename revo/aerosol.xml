<art mrk="$Id: aerosol.xml,v 1.16 2023/10/27 13:42:09 revo Exp $">
<kap>
  <rad>aerosol</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="aerosol.0o">
  <kap><tld/>o</kap>
  <snc mrk="aerosol.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="super" cel="disper.0ajxo.KEM">Dispersa&jcirc;o</ref> el subtilaj 
      eroj &scirc;vebantaj en gaso:
      <ekz>
        mi kun amiko ebrii&gcirc;is <klr>...</klr> &scirc;prucis 
        &ccirc;irka&ubreve;sidantojn per kontra&ubreve;skabia <tld/>o kaj kriis "mi cin 
        subpremos!"  
        <fnt>
          <aut>junas</aut>,
          <vrk>[blogo]</vrk>,
          <lok><url ref="http://junas.livejournal.com/63556.html">
          2007-08-05</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        estis plenumataj mezuradoj de <tld/>aj partikloj de la nuboj
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://kosmonauxtiko.narod.ru/Baza_informo/Venusa_programo/La_venusa_esplora_programo.htm">
          La Venusa Programo de Sovet-unio</url></vrk>,
          <lok>[vidita en 2010]</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="fum.0o">fumo</ref>,
      <ref cel="nebul.0o.MET">nebulo</ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#x6C14;&#x6EB6;&#x80F6; <pr>q&#xEC;r&#xF3;ngji&#x101;o</pr></trd>,
    <trd>&#x6C23;&#x6EB6;&#x81A0; <pr>q&#xEC;r&#xF3;ngji&#x101;o</pr></trd>,
    <trd>&#x55B7;&#x96FE;&#x80F6; <pr>p&#x113;nw&#xF9;ji&#x101;o</pr></trd>,
    <trd>&#x5674;&#x9727;&#x81A0; <pr>p&#x113;nw&#xF9;ji&#x101;o</pr></trd>,
    <trd>&#x55B7;&#x96FE;&#x6C14; <pr>p&#x113;nw&#xF9;q&#xEC;</pr></trd>,
    <trd>&#x5674;&#x9727;&#x6C23; <pr>p&#x113;nw&#xF9;q&#xEC;</pr></trd>,
    <trd>&#x6C14;&#x96FE;&#x5242; <pr>q&#xEC;w&#xF9;j&#xEC;</pr></trd>,
    <trd>&#x6C23;&#x9727;&#x5291; <pr>q&#xEC;w&#xF9;j&#xEC;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_e;&c_r;&c_a;&c_z;&c_o;&c_l;&c_mol;</trd>
  <trd lng="ca">aerosol</trd>
  <trd lng="cs">aerosol</trd>
  <trd lng="de">Aerosol</trd>
  <trd lng="en">aerosol</trd>
  <trd lng="fr">a&eacute;rosol</trd>
  <trd lng="it">aerosol</trd>
  <trdgrp lng="ja">
    <trd>&#x30A8;&#x30FC;&#x30ED;&#x30BE;&#x30EB;</trd>,
    <trd>&#x7159;&#x9727;&#x8CEA;  <pr>&#x3048;&#x3093;&#x3080;&#x3057;&#x3064;</pr></trd>
  </trdgrp>
  <trd lng="nl">aerosol</trd>
  <trd lng="pl">aerozol</trd>
  <trd lng="ro">aerosol</trd>
  <trd lng="sk">aeros&oacute;l</trd>
  <trd lng="sv">aerosol</trd>
  <trd lng="uk">&c_a;&c_je;&c_r;&c_o;&c_z;&c_o;&c_l;&c_mol;</trd>
</drv>
