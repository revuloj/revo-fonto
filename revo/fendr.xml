<art mrk="$Id: fendr.xml,v 1.20 2023/10/14 18:06:40 revo Exp $">
<kap>
  <rad>fendr</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fendr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="fendr.0o.sxirmilo">
    <dif>
      <ref tip="dif" cel="sxirm.fajro0ilo.MEBL">Fajro&scirc;irmilo</ref>:
      <ekz>
        la Blanka Re&gcirc;ino <klr>[&#x2026;]</klr> komencis grimpi mallerte, 
        sed fervore, la flankon de la <tld/>o
        <fnt>
          <aut>Lewis Caroll, trad. Donald Broadribb</aut>
          <vrk><url ref="https://issuu.com/darsi/docs/carroll--lewis-trans-la-spegulo-kaj-kion-alico-tro">Trans
          la Spegulo: kaj kion Alico trovis tie</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="ekran.0o.mebleto">ekrano<sncref/></ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_a;&c_t;&c_y; <klr tip="amb">(&c_k;&c_a;&c_m;&c_ib;&c_n;&c_a;)</klr></trd>,
    <trd>&c_r;&c_a;&c_sh;&c_o;&c_t;&c_k;&c_a; <klr tip="amb">(&c_k;&c_a;&c_m;&c_ib;&c_n;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="cs">z&aacute;st&ecaron;na</trd>
  <trd lng="de">Kaminvorsetzer</trd>
  <trdgrp lng="en">
    <trd>fender</trd>,
    <trd>fireguard</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>garde-feu</trd>,
    <trd>pare-&eacute;tincelles</trd>
  </trdgrp>
  <trd lng="hu">k&aacute;lyhaellenz&odblac;</trd>
  <trdgrp lng="ja">
    <trd>&#x7089;&#x683C;&#x5B50;  <pr>&#x308D;&#x3053;&#x3046;&#x3057;</pr></trd>,
    <trd>&#x706B;&#x3088;&#x3051;&#x3064;&#x3044;&#x305F;&#x3066;  <pr>&#x3072;&#x3088;&#x3051;&#x3064;&#x3044;&#x305F;&#x3066;</pr></trd>,
    <trd>&#x30B9;&#x30C8;&#x30FC;&#x30D6;&#x56F2;&#x3044;  <pr>&#x3059;&#x3068;&#x30FC;&#x3076;&#x304B;&#x3053;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">haardscherm</trd>
  <trd lng="pl">krata kominkowa</trd>
  <trd lng="pt">guarda-fogo</trd>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_i;&c_n;&c_n;&c_a;&c_ja; &c_r;&c_je;&c_sh;&c_jo;&c_t;&c_k;&c_a;</trd>
  <trd lng="sk">plechov&aacute; predlo&zcaron;ka pred pecou</trd>
  <trd lng="uk">&c_k;&c_a;&c_m;&c_ib;&c_n;&c_n;&c_a; &c_r;&c_je;&c_sh;&c_ib;&c_t;&c_k;&c_a;</trd>
</drv>
</art>
