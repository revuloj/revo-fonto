<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>strof</rad>/o
</kap>
<drv mrk="strof.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      Grupo da sinsekvaj <ref tip="prt" cel="vers.0o">versoj</ref>, ligitaj
      per la <ref cel="rim.0arangxo">rimaran&gcirc;o</ref> kaj (ofte) tipografie
      apartigitaj.
    </dif>
    <refgrp tip="vid">
      <ref cel="stanc.0o">stanco</ref>,
      <ref cel="kant.re0ajxo">rekanta&jcirc;o</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_u;&c_p;&c_l;&c_je;&c_t;</trd>, 
    <trd>&c_s;&c_t;&c_r;&c_a;&c_f;&c_a;</trd> 
  </trdgrp>
  <trd lng="de">Strophe</trd>
  <trd lng="en">verse</trd>
  <trdgrp lng="fr">
    <trd>strophe</trd>,
    <trd>couplet</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>versszak</trd>,
    <trd>str&oacute;fa</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>strofe</trd>,
    <trd>couplet</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_r;&c_o;&c_f;&c_a;</trd>,
    <trd>&c_k;&c_u;&c_p;&c_l;&c_je;&c_t;</trd>
  </trdgrp>
</drv>
</art>
