<art mrk="$Id: strof.xml,v 1.21 2023/09/04 15:14:32 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>strof</rad>/o
</kap>
<drv mrk="strof.0o">
  <kap><tld/>o</kap>
  <snc mrk="strof.0o.POE">
    <uzo tip="fak">POE</uzo>
    <dif>
      Grupo da sinsekvaj <ref tip="prt" cel="vers.0o">versoj</ref>, ligitaj
      per la <ref cel="rim.0arangxo">rimaran&gcirc;o</ref> kaj ordinare tipografie
      apartigitaj:
      <ekz>
        <tld/>o <klr>...</klr> fari&gcirc;as klara kaj rekonebla por la orelo
        nur dank' al plurfoja ripetado: <tld/>o ne havas unuopan ekziston
        <fnt><bib>PGl</bib>, <lok>Strofoj, p. 61a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kant.re0ajxo">rekanta&jcirc;o</ref>,
      <ref cel="stanc.0o">stanco</ref>
    </refgrp>
    <ref tip="super" cel="poezi.0ajxo">poezia&jcirc;o</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_u;&c_p;&c_l;&c_je;&c_t;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_f;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&#xF5A;&#xF72;&#xF42;&#xF66;&#xF0B;&#xF56;&#xF45;&#xF51;&#xF0B;</trd>
  <trd lng="ca">estrofa</trd>
  <trdgrp lng="cs">
    <trd>sloka</trd>,
    <trd>strofa</trd>
  </trdgrp>
  <trd lng="de">Strophe</trd>
  <trd lng="en">verse</trd>
  <trd lng="es">estrofa</trd>
  <trdgrp lng="fr">
    <trd>strophe</trd>,
    <trd>couplet</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>versszak</trd>,
    <trd>str&oacute;fa</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7BC0;  [<ind>&#x305B;&#x3061;</ind>]</trd>,
    <trd>&#x9023;  [<ind>&#x3080;&#x3089;&#x3058;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>couplet</trd>,
    <trd>strofe</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_r;&c_o;&c_f;&c_a;</trd>,
    <trd>&c_k;&c_u;&c_p;&c_l;&c_je;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>sloha</trd>,
    <trd>strofa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x441;&#x442;&#x440;&#x43E;&#x444;&#x430;</trd>,
    <trd>&#x43A;&#x443;&#x43F;&#x43B;&#x435;&#x442;</trd>
  </trdgrp>
</drv>
</art>
