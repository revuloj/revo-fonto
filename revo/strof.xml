<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>strof</rad>/o
</kap>
<drv mrk="strof.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      Grupo da sinsekvaj <ref tip="prt" cel="vers.0o">versoj</ref>, ligitaj 
      per la <ref cel="rim.0arangxo">rimaran&gcirc;o</ref> kaj (ofte) tipografie
      apartigitaj.
    </dif>
    <refgrp tip="vid">
      <ref cel="stanc.0o">stanco</ref>,
      <ref cel="kant.re0ajxo">rekanta&jcirc;o</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Strophe</trd>
  <trd lng="en">verse</trd>
  <trdgrp lng="fr">
    <trd>strophe</trd>,
    <trd>couplet</trd></trdgrp> 
  <trdgrp lng="nl">
    <trd>strofe</trd>,
    <trd>couplet</trd>
  </trdgrp>
</drv>
</art>
