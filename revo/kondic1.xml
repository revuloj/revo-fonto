    <trd>&#x53D6;&#x308A;&#x6C7A;&#12417;&#12425;&#x308C;&#x305F; 
    [<ind>&#12392;&#x308A;&#x304D;&#12417;&#12425;&#x308C;&#x305F;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">voorwaardelijk</trd>
  <trd lng="pl">warunkowy</trd>
  <trd lng="ro">condi&#x21B;ionat</trd>
