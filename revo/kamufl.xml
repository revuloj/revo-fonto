<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kamufl.xml,v 1.21 2022/07/17 07:28:19 revo Exp $">
<kap>
  <rad>kamufl</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="kamufl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="mask.0i">Maski</ref> militobjekton, donante al &gcirc;i la aspekton, koloron
      a&ubreve;
      formon de la &ccirc;irka&ubreve;a&jcirc;oj:
      <ekz>
        <tld/>i sin inter la foliaro
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.be/2012/010786.php"
              >Nokti&gcirc;as ... kaj la papilio noktui&gcirc;as</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        vestante sin en &ccirc;ifonoj <klr>[&#8230;]</klr> oni <tld/>is sin &ccirc;iel
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>is figurojn de Sankta Maria
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="https://www.monato.be/2015/011398.php"
              >Sekretaj kristanoj, &ccirc;u veraj kristanoj?</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">camouflage</trd>
    <trdgrp lng="it">
      <trd>mimetizzare</trd>,
      <trd>camuffare</trd>
    </trdgrp>
    <trd lng="pt">camuflar</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="kasx.0i.de_scio">Ka&scirc;<sncref/></ref>maski (p.p. sento ks):
      <ekz>
        Majstrokulto kaj nebula ideismo servis por <tld/>i seriozajn disputojn
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>is miajn argumentojn kaj analizon kiel eseon
        <fnt>
          <bib>Monato</bib>
          <aut>Eskil Svane</aut>
          <vrk><url ref="https://www.monato.be/2004/008324.html"
              >Gravas la vero</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        pri sarso oni komence provis silenti, mensogi, <tld/>i, trompi
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan MAUL</aut>
          <vrk><url ref="https://www.monato.be/2003/008129.html"
              >Propagando dan&gcirc;era</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">verkleiden</trd>
    <trdgrp lng="it">
      <trd>travestire</trd>,
      <trd>camuffare</trd>
    </trdgrp>
    <trd lng="pt">camuflar</trd>
  </snc>
    <trd lng="be">&c_k;&c_a;&c_m;&c_u;&c_f;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>fingovat</trd>,
    <trd>kamuflovat</trd>,
    <trd>maskovat</trd>,
    <trd>p&rcaron;edst&iacute;rat</trd>
  </trdgrp>
  <trd lng="de">tarnen</trd>
  <trd lng="es">camuflar</trd>
  <trd lng="fr">camoufler</trd>
  <trd lng="hu">&aacute;lc&aacute;z</trd>
  <trd lng="nl">camoufleren</trd>
  <trdgrp lng="pl">
    <trd>maskowa&cacute;</trd>,
    <trd>kamuflowa&cacute;</trd>
  </trdgrp>   
  <trd lng="pt">camuflar</trd>
  <trdgrp lng="ro">
    <trd>masca</trd>,
    <trd>camuflaj</trd>
  </trdgrp>   
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_m;&c_u;&c_f;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_s;&c_k;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">maskova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_m;&c_u;&c_f;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_m;&c_a;&c_s;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="kamufl.0o">
  <kap>
    <tld/>o, <var><kap><tld/>ado</kap></var>
  </kap>
  <snc>
    <dif>
      Ago <tld/>i; maniero, rimedo per kio oni <tld/>as:
      <ekz>
        la &ccirc;ifonoj estu nur supra kaj surfaca <tld/>o
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>;
      </ekz>
      <ekz>
        nenio povis protekti la nomadon kaj liajn familianojn kaj bestaron
        <klr>[&#8230;]</klr> krom la glavo kaj la sekreteco de <tld/>ado
        <fnt>
          <bib>Monato</bib>
          <aut>Husejn Al-Amily</aut>
          <vrk><url ref="https://www.monato.be/2007/009296.php"
              >La vualo, romantika kaj erotika</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_u;&c_f;&c_l;&c_ja;&c_zh; <klr>(&c_d;&c_z;&c_je;&c_ja;&c_n;&c_mol;&c_n;&c_je;)</klr></trd>
  <trdgrp lng="cs">
    <trd>kamufl&aacute;&zcaron;</trd>,
    <trd>zast&iacute;r&aacute;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Tarnung</trd>,
    <trd>Maskerade</trd>
  </trdgrp>
  <trd lng="en">camouflage</trd>
  <trd lng="fr">camouflage</trd>
  <trdgrp lng="it">
    <trd>mimetizzazione</trd>,
    <trd>camuffamento</trd>,
    <trd>mascheramento</trd>,
    <trd>travestimento</trd>
   </trdgrp>
  <trd lng="nl">het <ind>camoufleren</ind></trd>
  <trd lng="pl">maskowanie</trd>
  <trd lng="pt">camuflagem</trd>
  <trd lng="ro">camuflaj</trd>
  <trdgrp lng="sk">
    <trd>kamufl&aacute;&zcaron;</trd>,
    <trd>maskovanie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_m;&c_u;&c_f;&c_l;&c_ja;&c_zh;</trd>,
    <trd>&c_m;&c_a;&c_s;&c_k;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>








<drv mrk="kamufl.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Io <tld/>ita, &scirc;ajnigita, maskita:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sub <tld/>a&jcirc;o de &leftquot;demokratiaj&rightquot; institucioj ka&scirc;as sin esence polica &scirc;tato
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="https://www.monato.be/2004/008392.html"
              >Reveno de heroo</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        pli fruaj eksplikoj, ekzemple de Metoro, montri&gcirc;is <tld/>a&jcirc;oj, faritaj pro la
        turmentita konscienco de la novkristani&gcirc;into
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007315.html"
              >De&ccirc;ifrado de la paskinsula skribo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_u;&c_f;&c_l;&c_ja;&c_v;&c_a;&c_n;&c_y; &c_a;&c_b;&#8217;&c_je;&c_k;&c_t;</trd>
  <trd lng="de">Tarnung</trd>
  <trd lng="en">camouflage</trd>
  <trdgrp lng="it">
    <trd>mimetizzazione</trd>,
    <trd>camuffamento</trd>,
    <trd>mascheramento</trd>,
    <trd>travestimento</trd>
  </trdgrp>
  <trd lng="nl">camouflage</trd>
  <trd lng="pl">maska</trd> 
  <trd lng="pt">camuflagem</trd>
  <trd lng="ro">masca</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_m;&c_u;&c_f;&c_l;&c_ja;&c_zh;</trd>,
    <trd>&c_m;&c_a;&c_s;&c_k;&c_u;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j; &c_z;&c_a;&c_s;&c_ib;&c_b;</trd>
  </trdgrp>
</drv>

<drv mrk="kamufl.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Tio, kio servas por <tld/>i (retoj, bran&ccirc;oj ktp):
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[&ccirc;u]</klr> la tuta diskutado pri la filino Katarina estus nur <tld/>ilo por
        la mikrofono, a&ubreve; kodo, per kiu ili babilis pri siaj spion-aferoj
        <fnt>
          <bib>CKv</bib>
          <lok>13</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_u;&c_f;&c_l;&c_ja;&c_zh; <klr>(&c_s;&c_r;&c_o;&c_d;&c_a;&c_k;)</klr></trd>
  <trd lng="cs">kamufl&aacute;&zcaron;</trd>
  <trd lng="de">Tarnmaterial</trd>
  <trd lng="en">camouflage material</trd>
  <trdgrp lng="fr">
    <trd><klr tip="ind">(moyen de) </klr>camouflage</trd>
   </trdgrp>
  <trd lng="hu">&aacute;lc&aacute;z&oacute;eszk&ouml;z</trd>
  <trdgrp lng="it">
    <trd>materiale <ind>mimetico</ind></trd>,
    <trd>materiale <ind>scenografico</ind><klr> (mil.)</klr></trd>
  </trdgrp>
  <trd lng="nl">camouflagemateriaal</trd>
  <trd lng="pl">kamufla&zdot;</trd>
  <trd lng="pt">camuflagem</trd>
  <trd lng="ro">camuflaj</trd>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_u;&c_f;&c_l;&c_ja;&c_zh;</trd>
  <trdgrp lng="sk">
    <trd>kamufl&aacute;&zcaron;</trd>,
    <trd>maskovanie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_m;&c_u;&c_f;&c_l;&c_ja;&c_zh;</trd>,
    <trd>&c_m;&c_a;&c_s;&c_k;&c_u;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j; &c_z;&c_a;&c_s;&c_ib;&c_b;</trd>
  </trdgrp>
</drv>
</art>
