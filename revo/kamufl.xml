<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kamufl</rad>/i
</kap>
<drv mrk="kamufl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Maski militobjekton, donante al &gcirc;i la aspekton, koloron a&ubreve; formon
      de la &ccirc;irka&ubreve;a&jcirc;oj:
      <ekz>la aviadisto ne povis distingi la <tld/>itajn kanonojn.</ekz>
    </dif>
    <trd lng="en">camouflage</trd>
    <trd lng="de">tarnen</trd>
  </snc>
  <snc num="2">
    <dif>
      (f) Ka&scirc;maski (p.p. sento ks.)
    </dif>
    <trd lng="de">verkleiden</trd>
  </snc>
  <trd lng="nl">camoufleren</trd>
 <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_m;&c_u;&c_f;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_s;&c_k;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="kamufl.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
    <trd lng="de">Tarnen</trd>
<trd lng="nl">het <ind>camoufleren</ind></trd></drv>
<drv mrk="kamufl.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Io <tld/>ita.
    </dif>
  </snc>
    <trd lng="de">Tarnung</trd>
<trd lng="nl">camouflage</trd>
</drv>
<drv mrk="kamufl.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Tio, kio servas por <tld/>i (retoj, bran&ccirc;oj ktp).
    </dif>
  </snc>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_u;&c_f;&c_l;&c_ja;&c_zh;</trd>
</drv>
</art>

