<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>felt</rad>/o
</kap>
<drv mrk="felt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      &Scirc;tofo ne teksita, farita el kungluitaj kaj kunpremitaj haroj a&ubreve; laneroj.
    </dif>
  </snc>
  <trd lng="de">Filz</trd>
  <trd lng="fr">feutre<klr tip="ind"> (mati&egrave;re)</klr></trd>
  <trdgrp lng="hu">
    <trd>nemez</trd>,
    <trd>filc</trd>
  </trdgrp>
  <trd lng="nl">vilt</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_j;&c_l;&c_o;&c_k;</trd>,
    <trd>&c_f;&c_je;&c_t;&c_r;</trd>
  </trdgrp>
</drv>
</art>
