<vortaro>
  <art mrk="$Id: gliptotek.xml,v 1.2 2025/01/17 08:32:25 afido Exp $">
    <kap>
      <rad>gliptotek</rad>/o
      <fnt><bib>JED</bib></fnt>
    </kap>

<drv mrk="gliptotek.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">BELA</uzo>
    <dif>
      <ref tip="super" cel="kolekt.0o.art">Kolekto</ref> de gravuritaj kaj skulptitaj &scirc;tonoj:
      <ekz>
        sinagogo, en kies renovigita konstrua&jcirc;o trovi&gcirc;as nun <tld/>o precipe por
        memora&jcirc;oj de la klasika, grekromana skulptarto.
        <fnt>
          <aut>J&oacute;zsef Kaptay</aut>
          <vrk><url ref="http://www.eszperanto.hu/turismo/tata/tata.htm"
              >La urbo &leftquot;Tata&rightquot;</url></vrk>
          <lok>2. La kastelo, la fortika&jcirc;o kaj ilia &ccirc;irka&ubreve;a&jcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen <tld lit="G"/>o, tio estas klasikismo
        <fnt>
          <aut>Vja&ccirc;eslavo Rybakov, trad. Jurij Finkel</aut>
          <vrk><url ref="https://www.esperanto.mv.ru/Gravitavio/08.html"
              >Gravitavio &leftquot;Carido&rightquot;</url></vrk>
          <lok>&ccirc;ap. 1, Alvico</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
      <ref cel="gem.0o.gliptiko">gemo<sncref/></ref>,
      <ref cel="skulpt.0ajxo">skulpta&jcirc;o</ref>
    </refgrp>
    <ref tip="vid" cel="glipti.0o">gliptiko</ref>
    <trd lng="de">Glyptothek</trd>
    <trd lng="en">glyptotheque</trd>
    <trd lng="fr">glyptoth&egrave;que</trd>
    <trd lng="ru">&c_g;&c_l;&c_i;&c_p;&c_t;&c_o;&c_t;&c_je;&c_k;&c_a;</trd>
  </snc>
<trd lng="be">&c_g;&c_l;&c_ib;&c_p;&c_t;&c_a;&c_t;&c_e;&c_k;&c_a;</trd>
</drv>

  </art>

