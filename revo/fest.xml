<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fest</rad>/o
</kap>
<drv mrk="fest.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Publikaj a&ubreve; privataj ceremonioj k plezuroj por soleni iun
okaza&jcirc;on:
      <ekz>Paska <tld/>o;</ekz>
      <ekz><tld/>o de la 1-a de Majo;</ekz>
      <ekz>nom<tld/>o de iu persono;</ekz>
      <ekz>foira <tld/>o;</ekz>
      <ekz><tld/>otago.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="solen.0o">soleno</ref>,
      <ref cel="feri.0o">ferio</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_v;&c_ja;&c_t;&c_a;</trd>,
    <trd>&c_f;&c_e;&c_s;&c_t;</trd>
  </trdgrp>
  <trd lng="cs">sv&aacute;tek</trd>
  <trdgrp lng="de">
     <trd>Fest</trd>,
     <trd>Feier</trd>,
     <trd>Fete</trd></trdgrp>
  <trd lng="fr">f&ecirc;te</trd>
  <trd lng="es">fiesta</trd>
  <trd lng="hu">&uuml;nnep</trd>
  <trd lng="nl">feest</trd>
  <trd lng="ru">&c_p;&c_r;&c_a;&c_z;&c_d;&c_n;&c_i;&c_k;</trd>
  <trd lng="sv">fest</trd>
</drv>
<drv mrk="fest.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on pro:
      <ekz>
        <tld/>i <tld/>on<fnt>Z</fnt>, geedzi&gcirc;on, jubileon, datrevenon;
      </ekz>
      <ekz><tld/>i iun per festeno;</ekz>
      <ekz><tld/>u al mi tri fojojn &ccirc;iujare<fnt>Z</fnt></ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="celebr.0i">celebri</ref>,
      <ref cel="solen.0i">soleni</ref>,
      <ref cel="glor.0i">glori</ref>.
    </refgrp>
  </snc>
  <trd
lng="be">&c_s;&c_mol;&c_v;&c_ja;&c_t;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
     <trd>feiern</trd>,
     <trd>feierlich begehen</trd></trdgrp>
  <trd lng="en">party</trd>
  <trd lng="es">festejar</trd>
  <trd lng="fr">f&ecirc;ter</trd>
  <trd lng="hu">&uuml;nnepel</trd>
  <trd lng="nl">feesten</trd>
  <trd
lng="ru">&c_p;&c_r;&c_a;&c_z;&c_d;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sv">
    <trd>fira</trd>,
    <trd>festa</trd></trdgrp>
</drv>
</art>
