<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>fest</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="fest.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fest.0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Publikaj a&ubreve; privataj ceremonioj kaj plezuroj por soleni iun
      okaza&jcirc;on:
      <ekz>
        Paska <tld/>o;
      </ekz>
      <ekz>
        <tld/>o de la 1a de Majo;
      </ekz>
      <ekz>
        foira <tld/>o;
      </ekz>
      <ekz>
        <tld/>otago.
      </ekz>
    </dif>
    <lstref lst="voko:festoj">festoj</lstref>
    <refgrp tip="vid">
      <ref cel="feri.0o">ferio</ref>,
      <ref cel="solen.0o">soleno</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_v;&c_ja;&c_t;&c_a;</trd>,
    <trd>&c_f;&c_e;&c_s;&c_t;</trd>
  </trdgrp>
  <trd lng="cs">sv&aacute;tek</trd>
  <trdgrp lng="de">
    <trd>Fest</trd>,
    <trd>Feier</trd>,
    <trd>Fete</trd>
  </trdgrp>
  <trd lng="fr">f&ecirc;te</trd>
  <trd lng="es">fiesta</trd>
  <trd lng="hu">&uuml;nnep</trd>
  <trd lng="it">festa</trd>
  <trd lng="nl">feest</trd>
  <trd lng="pl">&sacute;wi&eogonek;to</trd>
  <trdgrp lng="pt">
    <trd>festa</trd>,
    <trd>festejo</trd>,
    <trd>festividade</trd>
  </trdgrp>
  <trd lng="ro">s&abreve;rb&abreve;toare</trd>
  <trd lng="ru">&c_p;&c_r;&c_a;&c_z;&c_d;&c_n;&c_i;&c_k;</trd>
  <trd lng="sv">fest</trd>
</drv>

<drv mrk="fest.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="fest.0i.KAL">
    <dif>
      Fari <tld/>on pro:
      <ekz>
        <tld/>i <tld/>on<fnt>Z</fnt>, geedzi&gcirc;on, jubileon,
        datrevenon;
      </ekz>
      <ekz>
        <tld/>i iun per festeno;
      </ekz>
      <ekz>
        <tld/>u al mi tri fojojn &ccirc;iujare<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="celebr.0i">celebri</ref>,
      <ref cel="glor.0i">glori</ref>,
      <ref cel="solen.0i">soleni</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_v;&c_ja;&c_t;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>feiern</trd>,
    <trd>feierlich begehen</trd>
  </trdgrp>
  <trd lng="en">party</trd>
  <trd lng="es">festejar</trd>
  <trd lng="fr">f&ecirc;ter</trd>
  <trd lng="hu">&uuml;nnepel</trd>
  <trd lng="nl">feesten</trd>
  <trd lng="pl">&sacute;wi&eogonek;towa&cacute;</trd>
  <trd lng="ru">&c_p;&c_r;&c_a;&c_z;&c_d;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sv">
    <trd>fira</trd>,
    <trd>festa</trd>
  </trdgrp>
</drv>

<drv mrk="fest.pri0i">
  <kap>prifesti</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fest.pri0i.KOMUNE">
    <dif>
      <tld lit="F"/>i pro homo, okaza&jcirc;o, kiun oni honoras
      a&ubreve; memorigas:
      <ekz>
        pri<tld/>i la jar&scirc;an&gcirc;on sole en la naturo, man&gcirc;ante en nia tendo
        
        pasta&jcirc;ojn kun tomata sa&ubreve;co neniel romantikas
        <fnt>
          <aut>C. Pinardon. R. Sanz</aut>,
          <vrk><url
          ref="http://horizonto.free.fr/rakontoj/18-indonezio.html">
          Du Bicikloj je la Horizonto</url></vrk>,
          <lok>[2010]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">   
    <trd>c&eacute;l&eacute;brer</trd>,
    <trd>f&ecirc;ter<klr> (tr. qqch, qqun)</klr></trd>
  </trdgrp>
  <trd lng="pl">obchodzi&cacute; <klr>(&sacute;wi&eogonek;towa&cacute;)</klr></trd>
</drv>

<drv mrk="fest.dekduopa0o">
  <kap>dekduopa <tld/>o</kap>
  <snc mrk="fest.dekduopa0o.KRI">
    <uzo tip="fak">KAL</uzo>
    <uzo tip="fak">KRI</uzo>
    <dif>
      En <ref tip="malprt" cel="krist.orienta0anismo">orienta
      kristanisimo</ref>, &ccirc;iu el la dek du speciale gravaj <tld/>oj:
      <ekz>
        Kristnasko kaj Epifanio estas dekduopaj <tld/>oj;
      </ekz>
      <ekz>
        Pasko estas ekster la listo da dekduopaj <tld/>oj, &ccirc;ar
        &gcirc;i estas nekompareble pli grava ol &ccirc;iuj aliaj
        <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <!-- tez tip="listo" mrk="fest.dekduopaj0oj"
  cel="krist.0anismo">La dekduopaj festoj</tez --> 
  <trd lng="ru">&c_d;&c_v;&c_u;&c_n;&c_a;&c_d;&c_je;&c_s;&c_ja;&c_t;&c_y;&c_j; &c_p;&c_r;&c_a;&c_z;&c_d;&c_n;&c_i;&c_k;</trd>
</drv>

<drv mrk="fest.dipatrina0o">
  <kap>dipatrina <tld/>o</kap>
  <snc mrk="fest.dipatrina0o.KRI">
    <uzo tip="fak">KAL</uzo>
    <uzo tip="fak">KRI</uzo>
    <dif>
      Kristana <ref tip="lst" cel="fest.0o"
      lst="voko:festoj"><tld/>o</ref>
      dedi&ccirc;ita al la Dipatrino:
      <ekz>
        Endormi&gcirc;o estas unu el la dipatrinaj <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <!-- tez tip="listo" mrk="fest.dipatrinaj0oj"
  cel="krist.0anismo">La dipatrinaj festoj</tez -->
  <trd lng="fr">f&ecirc;te mariale</trd>
  <trd lng="ru">&c_b;&c_o;&c_g;&c_o;&c_r;&c_o;&c_d;&c_i;&c_ch;&c_n;&c_y;&c_j; &c_p;&c_r;&c_a;&c_z;&c_d;&c_n;&c_i;&c_k;</trd>
</drv>

<drv mrk="fest.0odelauxboj">
  <kap><tld lit="F"/>o de La&ubreve;boj</kap>
  <snc mrk="fest.0odelauxboj.JUD">
    <uzo tip="fak">REL</uzo>
    <dif>
      Septaga judisma vintra <ref tip="lst" cel="fest.0o"
      lst="voko:festoj">festo</ref>, 
      dum kiu oni tradicie restas sub la&ubreve;bo a&ubreve; en kabano
      memorige pri la bibliaj Eliro kaj migrado tra la dezerto;
      anka&ubreve; nomata <ref tip="sin" cel="sukot.0o.JUD">Sukoto</ref>:
      <ekz>
        diru al la Izraelidoj jene: Komencante de la dek-kvina tago de 
        tiu sepa monato estu <tld/>o de la&ubreve;boj dum sep tagoj
        <fnt><bib>MT</bib>, <lok>&Lev; 23:34</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>F&ecirc;te des cabanes</trd>,
    <trd>F&ecirc;te des tentes</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&Sacute;wi&eogonek;to <ind>Namiot&oacute;w</ind></trd>,
    <trd>Sukkot</trd>,
    <trd>Kuczki</trd>
  </trdgrp>
</drv>

<drv mrk="fest.masko0o">
  <kap>masko<tld/>o</kap>
  <snc mrk="fest.masko0o.KOMUNE">
    <dif>
      <ref tip="lst" cel="fest.0o" lst="voko:festoj"><tld
      lit="F"/>o</ref> 
      en kiu homoj surhavas maskojn, alivestojn:
      <ekz>
        ha, kion? Masko<tld/>o?
        <fnt>
        <aut>&Scirc;ekspiro, trad. A. E. Wackrill</aut>,
        <vrk><url ref="http://www.luin.se/inko/199-2.pdf">La venecia
        komercisto</url></vrk>,
        <lok>1914</lok>
        </fnt>;
      </ekz>
      <ekz>
        tri knaboj <klr>...</klr> vizitas fantazian masko<tld/>on en kiu
        okazas strangaj aferoj: la lo&gcirc;antoj de la domo estas spirito,
        diablo kaj aliaj fantomoj
        <fnt>
        <aut>UEA</aut>,
        <vrk><url ref="http://uea.org/kongresoj/2002/kl-arto.html">87a
        Universala Kongreso...</url></vrk>,
        <lok>2003-03-15</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Kost&uuml;mfest</trd>,
    <trd>Maskenball</trd>
  </trdgrp>
  <trd lng="fr">mascarade</trd>
  <trdgrp lng="pl">
    <trd>bal maskowy</trd>,
    <trd>maskarada</trd>
  </trdgrp>
</drv>

<drv mrk="fest.nom0o">
  <kap>nom<tld/>o</kap>
  <snc mrk="fest.nom0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      En iuj landoj <ref tip="lst" cel="fest.0o"
      lst="voko:festoj"><tld/>o</ref> 
      al homoj kun sama persona nomo, okazanta en jartago 
      dedi&ccirc;ita al tiu nomo: 
      <ekz>
        lia nom<tld/>o estas en la tago de la sankta Antono
        <fnt><bib>Revizoro</bib>, <lok>akto 4a, sceno 10a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">f&ecirc;te<klr> (d'un pr&eacute;nom)</klr></trd>
  <trd lng="pl">imieniny</trd>
  <trd lng="ro">zi onomastic&abreve;</trd>
  <trd lng="ru">&c_i;&c_m;&c_je;&c_n;&c_i;&c_n;&c_y;</trd>
</drv>

<drv mrk="fest.saturno0o">
  <kap>Saturno<tld/>o, <var><kap>Saturna <tld
  lit="F"/>o</kap></var></kap>
  <snc mrk="fest.saturno0o.KAL">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">KAL</uzo>
    <dif>
      Romia karnavala <ref tip="lst" cel="fest.0o"
      lst="voko:festoj">festo</ref> 
      dedi&ccirc;ita al la dio Saturno, kun konfuzo de sociaj roloj kaj 
      dibo&ccirc;o:
      <ekz>
        longe anta&ubreve; la komenco de la Saturna <tld/>o, mi jam sciis, ke 
        alproksimi&gcirc;as ia &gcirc;oja okazo
        <fnt>
          <aut>A. L&ouml;wenstein</aut>,
          <vrk>La &scirc;tona urbo</vrk>,
          <lok>FEL, 1999</lok>
        </fnt>;
      </ekz>
      <ekz>
        la moderna lotumo <klr>...</klr> enradiki&gcirc;as en la romiaj 
        saturno<tld/>oj
        <fnt>
          <aut>M. Pancorbo Castro</aut>,
          <vrk><url ref="http://bitakoro.blogspot.com/">
          malnova bitakoro</url></vrk>,
          <lok>2003-12-21</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="fest.saturno0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <ref tip="dif" cel="dibocx.0o.MOR">dibo&ccirc;o</ref>
  </snc>
  <trd lng="en">Saturnalia</trd>
  <trd lng="fr">saturnales</trd>
  <trd lng="pl">saturnalia</trd>
</drv>
</art>
