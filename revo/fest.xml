</drv>


<drv mrk="fest.0o_de_Duobla_Kvino">
  <kap>
    <tld lit="F"/>o de Duobla Kvino,
    <var><kap>Drakboata <tld lit="F"/>o</kap></var>
  </kap>
  <snc mrk="fest.0o_de_Duobla_Kvino.KAL_cxina">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Somerkomenca <ref tip="lst" lst="voko:festoj" cel="fest.0o.KAL">festo</ref>
      en la kvina tago de la kvina monato,
      solenata en &Ccirc;inujo kaj najbaraj landoj:
      <ekz>
        zongzi-o estas tradicia man&gcirc;a&jcirc;o de la <tld lit="F"/>o de Duobla Kvino
        <fnt>
          <aut>&Gcirc;oja</aut>
          <vrk><url ref="https://esperanto.cri.cn/recomended1/3841/20220531/760571.html"
              >Zongzi-o</url></vrk>
          <lok>&Ccirc;ina Radio Internacia, 2022-05-31</lok>
        </fnt>;
      </ekz><ekz>
        la kvina tago de la kvina monato la&ubreve; la &ccirc;ina kalendaro estas la Drakboata <tld
        lit="F"/>o
        <fnt>
          <aut>-</aut>
          <vrk><url ref="http://esperanto.china.org.cn/2022-06/15/content_78272313.htm"
              >Dol&ccirc;a? Sala? Kian zongzi-on vi preferas?</url></vrk>
          <lok>Esperanto.china.org.cn, 2022-06-15</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Drachenbootfest</trd>
  <trd lng="fr">F&ecirc;te des bateaux-dragons</trd>
  <trd lng="it">Festa delle barche drago</trd>
<trdgrp lng="zh">
  <trd>&#31471;&#21320;&#31680;</trd>,
  <trd>&#31471;&#21320;&#33410;</trd>
</trdgrp>
</drv>

<drv mrk="fest.0odelauxboj">
  <kap><tld lit="F"/>o de La&ubreve;boj</kap>
  <snc mrk="fest.0odelauxboj.JUD">
    <uzo tip="fak">REL</uzo>
    <dif>
      Septaga judisma vintra <ref tip="lst" cel="fest.0o" lst="voko:festoj">festo</ref>, 
      dum kiu oni tradicie restas sub la&ubreve;bo a&ubreve; en kabano
      memorige pri la bibliaj Eliro kaj migrado tra la dezerto;
      anka&ubreve; nomata <ref tip="sin" cel="sukot.0o.JUD">Sukoto</ref>:
      <ekz>
        diru al la Izraelidoj jene: Komencante de la dek-kvina tago de 
        tiu sepa monato estu <tld/>o de la&ubreve;boj dum sep tagoj
        <fnt><bib>MT</bib> <lok>&Lev; 23:34</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_S;&c_mol;&c_v;&c_ja;&c_t;&c_a; &c_k;&c_u;&c_sh;&c_ch;&c_a;&c_w;</trd>,
    <trd>&c_S;&c_u;&c_k;&c_o;&c_t;</trd>
  </trdgrp>
  <trd lng="de">Laubh&uuml;ttenfest</trd>
  <trdgrp lng="es">
    <trd>Fiestas de las tiendas</trd>,
    <trd>Sucot</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>F&ecirc;te des cabanes</trd>,
    <trd>F&ecirc;te des tentes</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&Sacute;wi&eogonek;to <ind>Namiot&oacute;w</ind></trd>,
    <trd>Sukkot</trd>,
    <trd>Kuczki</trd>
  </trdgrp>
</drv>

<drv mrk="fest.0o_de_Semajnoj">
  <kap>
    <tld lit="F"/>o de Semajnoj
  </kap>
  <snc>
    <dif>
      La juda <ref cel="fest.Danko0o">dankofesto</ref> 50 tagoj post Pasko:
      <ekz>
        tri fojojn en la jaro &ccirc;iu via virseksulo devas aperi anta&ubreve; la Eternulo, via Dio
        <klr>[&#8230;]</klr>: en la festo de macoj kaj en la festo de semajnoj kaj en la
        festo de la&ubreve;boj
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 16:16</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pentek.0o">Pentekosto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Wochenfest</trd>,
    <trd>Schawuot</trd>
  </trdgrp>
  <trd lng="en">Shavuot</trd>
  <trd lng="he">&shin;&#1464;&#1473;&bet;&vav;&#1468;&ayin;&vav;&#1465;&tav;</trd>
  <trd lng="fr">Chavouot</trd>
</drv>




<drv mrk="fest.Lanterno0o">
  <kap>
    Lanterno<tld/>o,
    <var><kap>Lanterna <tld lit="F"/>o</kap></var>
  </kap>
  <snc mrk="fest.Lanterno0o.KAL">
    <dif>
      Lasta tago de la
      <ref tip="malprt" cel="fest.Printempa0o.KAL">Printempa Festo</ref>,
      en kiu oni nokte promenas kun ru&gcirc;aj lanternoj:
      <ekz>
        la koreaj gastoj povas spekti la lanterno<tld/>on, sed &scirc;ajne estos tro malfrue
        resti &gcirc;is 21:30 por vidi la fajroteknika&jcirc;ojn en la sama loko
        <fnt>
          <aut>Y&ucirc;iti Sawaya</aut>
          <vrk><url
          ref="https://ru-ru.facebook.com/groups/102299479924434/permalink/737393523081690/"
              >Bunta vespero...</url></vrk>
          <lok>2017-01-31</lok>
        </fnt>;
      </ekz><ekz>
        elirmalpermeso estis &gcirc;enerale efektivigita en antikva &Ccirc;inio, sed la
        elirmalpermeso estis nuligita dum la Lanterna <tld lit="F"/>o kaj viroj kaj virinoj
        povis eliri kaj ludi tutan nokton
        <fnt>
          <aut>Shanny</aut>
          <vrk><url ref="https://www.youtube.com/watch?v=RMLb0HzDduw"
              >Lanterna Festo estas &ccirc;ina &rightquot;Valentena Tago&#8221;!</url></vrk>
          <lok>YouTube, 2022-02-15</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Laternenfest</trd>
  <trd lng="fr">F&ecirc;te des lanternes</trd>
  <trd lng="it">Festa delle lanterne</trd>
<trdgrp lng="zh">
  <trd>&#20803;&#23477;&#31680; &#29128;&#31680;</trd>,
  <trd>&#20803;&#23477;&#33410; &#28783;&#33410;</trd>
</trdgrp>
</drv>

<drv mrk="fest.Printempa0o">
  <kap>
    Printempa <tld lit="F"/>o
  </kap>
  <snc mrk="fest.Printempa0o.KAL">
    <dif>
      Jarkomenca <ref tip="lst" lst="voko:festoj" cel="fest.0o.KAL">festo</ref> la&ubreve; la
      &ccirc;ina kaj orientazia kalendaro, anka&ubreve; nomata
      <ref tip="sin" cel="tet.0o.KAL">Teto</ref> en Vjetnamujo, a&ubreve; simple
      <ref tip="super" cel="jar.nov0o.KAL">Novjaro</ref>:
      <ekz>
        la 31-an de januaro en la Konfucea Instituto en la Universitato Kartumo en
        Sudano okazis vespero por bonvenigi la Printempan <tld lit="F"/>on de la Jaro
        de Simio
        <fnt>
          <aut>Chen Ji</aut>
          <vrk><url ref="http://espero.chinareports.org.cn/2016-02/02/content_37720031.htm"
              >Printempa Festo en aliaj landoj</url></vrk>
          <lok>El Popola &Ccirc;inio, 2016-02-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Chinesisches <ind>Neujahrsfest</ind></trd>
  <trdgrp lng="fr">
    <trd>F&ecirc;te du printemps</trd>,
    <trd>Nouvel an chinois</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>Festa di primavera</trd>,
    <trd>capodanno cinese</trd>
  </trdgrp>
<trdgrp lng="zh">
  <trd>&#36786;&#26310;&#26032;&#24180; &#26149;&#31680;</trd>,
  <trd>&#20892;&#21382;&#26032;&#24180; &#26149;&#33410;</trd>
</trdgrp>
</drv>


<drv mrk="fest.saturno0o">
  <kap>Saturno<tld/>o, <var><kap>Saturna <tld lit="F"/>o</kap></var></kap>
  <snc mrk="fest.saturno0o.KAL">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">KAL</uzo>
    <dif>
      Romia karnavala <ref tip="lst" cel="fest.0o" lst="voko:festoj">festo</ref> 
      dedi&ccirc;ita al la dio Saturno, kun konfuzo de sociaj roloj kaj 
      dibo&ccirc;o:
      <ekz>
        longe anta&ubreve; la komenco de la Saturna <tld/>o, mi jam
        sciis, ke alproksimi&gcirc;as ia &gcirc;oja okazo
        <fnt>
          <aut>A. L&ouml;wenstein</aut>
          <vrk>La &scirc;tona urbo</vrk>
          <lok>1999</lok>
        </fnt>;
      </ekz><ekz>
        la moderna lotumo <klr>[&#8230;]</klr> enradiki&gcirc;as en la romiaj 
        saturno<tld/>oj
        <fnt>
          <aut>M. Pancorbo Castro</aut>,
          <vrk><url ref="http://bitakoro.blogspot.com/">
          malnova bitakoro</url></vrk>,
          <lok>2003-12-21</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="fest.saturno0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <ref tip="dif" cel="dibocx.0o.MOR">dibo&ccirc;o</ref>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_t;&c_u;&c_r;&c_n;&c_a;&c_l;&c_ib;&c_ib;</trd>
  <trd lng="de">Saturnalien</trd>
  <trd lng="en">Saturnalia</trd>
  <trd lng="es">Saturnalia</trd>
  <trd lng="fr">Saturnales</trd>
  <trd lng="pl">saturnalia</trd>
  <trd lng="ru">&c_s;&c_a;&c_t;&c_u;&c_r;&c_n;&c_a;&c_l;&c_i;&c_i;</trd>
</drv>
