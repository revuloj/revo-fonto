<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fest</rad>/o
</kap>
<drv mrk="fest.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Publikaj a&ubreve; privataj ceremonioj k plezuroj por soleni iun okaza&jcirc;on:
      <ekz>Paska <tld/>o;</ekz>
      <ekz><tld/>o de la 1-a de Majo;</ekz>
      <ekz>nom<tld/>o de iu persono;</ekz>
      <ekz>foira <tld/>o;</ekz>
      <ekz><tld/>otago.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="solen.0o">soleno</ref>,
      <ref cel="feri.0o">ferio</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">sv&aacute;tek</trd>
  <trd lng="fr">f&ecirc;te</trd>
  <trd lng="ru">&c_p;&c_r;&c_a;&c_z;&c_d;&c_n;&c_i;&c_k;</trd>
</drv>
<drv mrk="fest.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra> 
  <snc>
    <dif>
      Fari <tld/>on pro: 
      <ekz>
        <tld/>i <tld/>on<fnt>Z</fnt>, geedzi&gcirc;on, jubileon, datrevenon;
      </ekz>
      <ekz><tld/>i iun per festeno;</ekz>
      <ekz><tld/>u al mi tri fojojn &ccirc;iujare<fnt>Z</fnt></ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="celebr.0i">celebri</ref>, 
      <ref cel="solen.0i">soleni</ref>, 
      <ref cel="glor.0i">glori</ref>.
    </refgrp>
  </snc>
  <trd lng="en">party</trd>
  <trd lng="fr">f&ecirc;ter</trd>
  <trd lng="ru">&c_p;&c_r;&c_a;&c_z;&c_d;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
