<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>fest</rad>/o
    </kap>

    <drv mrk="fest.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc>
        <dif>
          Publikaj a&ubreve; privataj ceremonioj kaj plezuroj por soleni iun
          okaza&jcirc;on:
          <ekz>
            Paska <tld/>o;</ekz><ekz>
            <tld/>o de la 1-a de Majo;
          </ekz><ekz>
            <ind>nom<tld/>o</ind> de persono;
            <trd lng="ru">&c_i;&c_m;&c_je;&c_n;&c_i;&c_n;&c_y;</trd>
          </ekz><ekz>
            foira <tld/>o;
          </ekz><ekz>
            <tld/>otago.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="solen.0o">soleno</ref>,
          <ref cel="feri.0o">ferio</ref>.
        </refgrp>
      </snc>
      <trdgrp lng="be">
        <trd>&c_s;&c_mol;&c_v;&c_ja;&c_t;&c_a;</trd>,
        <trd>&c_f;&c_e;&c_s;&c_t;</trd>
      </trdgrp>
      <trd lng="cs">sv&aacute;tek</trd>
      <trdgrp lng="de">
        <trd>Fest</trd>,
        <trd>Feier</trd>,
        <trd>Fete</trd></trdgrp>
      <trd lng="fr">f&ecirc;te</trd>
      <trd lng="es">fiesta</trd>
      <trd lng="hu">&uuml;nnep</trd>
      <trd lng="nl">feest</trd>
      <trd lng="ru">&c_p;&c_r;&c_a;&c_z;&c_d;&c_n;&c_i;&c_k;</trd>
      <trd lng="sv">fest</trd>
    </drv>

    <drv mrk="fest.0i">
      <kap><tld/>i</kap>
      <gra><vspec>x</vspec></gra>
      <snc>
        <dif>
          Fari <tld/>on pro:
          <ekz>
            <tld/>i <tld/>on<fnt>Z</fnt>, geedzi&gcirc;on, jubileon, datrevenon;
          </ekz><ekz>
            <tld/>i iun per festeno;</ekz><ekz>
            <tld/>u al mi tri fojojn &ccirc;iujare<fnt>Z</fnt></ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="celebr.0i">celebri</ref>,
          <ref cel="solen.0i">soleni</ref>,
          <ref cel="glor.0i">glori</ref>.
        </refgrp>
      </snc>
      <trd
        lng="be">&c_s;&c_mol;&c_v;&c_ja;&c_t;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
      <trdgrp lng="de">
        <trd>feiern</trd>,
        <trd>feierlich begehen</trd></trdgrp>
      <trd lng="en">party</trd>
      <trd lng="es">festejar</trd>
      <trd lng="fr">f&ecirc;ter</trd>
      <trd lng="hu">&uuml;nnepel</trd>
      <trd lng="nl">feesten</trd>
      <trd lng="ru">&c_p;&c_r;&c_a;&c_z;&c_d;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
      <trdgrp lng="sv">
        <trd>fira</trd>,
        <trd>festa</trd></trdgrp>
    </drv>

    <drv mrk="fest.dekduopa0o">
      <kap>dekduopa <tld/>o</kap>
      <uzo tip="fak">KRI</uzo>
      <snc>
        <dif>
          En <ref tip="malprt" cel="krist.orienta0anismo">orienta
            kristanisimo</ref>,
          &ccirc;iu el la dek du speciale gravaj <tld/>oj:
          <ekz>
            Kristnasko kaj Epifanio estas dekduopaj <tld/>oj;
          </ekz><ekz>
            Pasko estas ekster la listo da dekduopaj <tld/>oj, &ccirc;ar
            &gcirc;i estas nekompareble pli grava ol &ccirc;iuj aliaj festoj.
          </ekz>
        </dif>
      </snc>
      <tez tip="listo" mrk="fest.dekduopaj0oj"
        cel="krist.0anismo">La dekduopaj festoj</tez>
      <trd lng="ru">&c_d;&c_v;&c_u;&c_n;&c_a;&c_d;&c_je;&c_s;&c_ja;&c_t;&c_y;&c_j;
        &c_p;&c_r;&c_a;&c_z;&c_d;&c_n;&c_i;&c_k;</trd>
    </drv>

    <drv mrk="fest.dipatrina0o">
      <kap>dipatrina <tld/>o</kap>
      <uzo tip="fak">KRI</uzo>
      <snc>
        <dif>
          Kristana <tld/>o dedi&ccirc;ita al la Dipatrino:
          <ekz>
            Endormi&gcirc;o estas unu el la dipatrinaj <tld/>oj.
          </ekz>
        </dif>
      </snc>
      <tez tip="listo" mrk="fest.dipatrinaj0oj"
        cel="krist.0anismo">La dipatrinaj festoj</tez>
      <trd lng="ru">&c_b;&c_o;&c_g;&c_o;&c_r;&c_o;&c_d;&c_i;&c_ch;&c_n;&c_y;&c_j;
        &c_p;&c_r;&c_a;&c_z;&c_d;&c_n;&c_i;&c_k;</trd>
    </drv>

  </art>
