<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: ommetr.xml,v 1.4 2023/09/03 07:12:34 afido Exp $">
    <kap>
      <rad>ommetr</rad>/o <fnt><bib>PIV1</bib></fnt>,
      <var><kap><rad var="o">omometr</rad>/o</kap></var>
    </kap>

<drv mrk="ommetr.0o">
  <kap><tld/>o, <var><kap><tld var="o"/>o</kap></var></kap>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      Instrumento por mezuri la <ref cel="rezist1.0o">rezistanco</ref>n:
      <ekz>
        plurmezurilo <klr>[&#x2026;]</klr> povas fari&gcirc;i voltmetro, ampermetro a&ubreve; 
        <tld var="o"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=735748">Plurmezurilo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      Pro homonimeco kun la mezurunuo <ref tip="hom" cel="om.o0metro.unuo">omometro</ref>
      prefere uzu la terminon &leftquot;rezistancmezurilo&rightquot;.
    </rim>
    <trd lng="de">Widerstandsmessger&auml;t</trd>
    <trd lng="en">ohmmeter</trd>
    <trd lng="fr">ohmm&egrave;tre</trd>
    <trd lng="nl">ohmmeter</trd>
  </snc>
  <trd lng="ja">&#x30AA;&#x30FC;&#x30E0;&#x30FB;&#x30E1;&#x30FC;&#x30C8;&#x30EB;</trd>
</drv>

  </art>

