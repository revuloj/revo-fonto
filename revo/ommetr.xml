<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ommetr</rad>/o
</kap>

<drv mrk="ommetr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ELE</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="aparat.0o.TEK">Aparato</ref> por mezuri 
      <ref cel="rezist1.0o">rezistancon</ref>
      de elektra cirkvito.
    </dif>
    <rim>
      Ne konfuzu kun la mezurunuo
      <ref tip="hom" cel="om.o0metro">om(o)metro</ref>.
    </rim>
  </snc>
  <trdgrp lng="de">
    <trd>Widerstandsmessger&auml;t</trd>,
    <trd>Ohmmeter</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ohmm&egrave;tre</trd>,
    <trd>ohm-m&egrave;tre<klr tip="ind"> (appareil de mesure)</klr>
    </trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ellen&aacute;ll&aacute;sm&eacute;r&odblac;</trd>,
    <trd>ohm-m&eacute;r&odblac;</trd>
  </trdgrp>
  <trd lng="nl">ohmmeter</trd>
  <trdgrp lng="pl">
    <trd>omomierz</trd>,
    <trd>miernik rezystancji</trd>
  </trdgrp>
  <trd lng="pt">ohm&iacute;metro</trd>
</drv>
</art>

