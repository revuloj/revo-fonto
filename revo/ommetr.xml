<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ommetr</rad>/o
</kap>

<drv mrk="ommetr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ELE</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="aparat.0o.TEK">Aparato</ref> por mezuri 
      <ref cel="rezist1.0o">rezistancon</ref>
      de elektra cirkvito.
    </dif>
    <rim>
      Ne konfuzu kun la mezurunuo
      <ref tip="hom" cel="om.o0metro">om(o)metro</ref>.
    </rim>
  </snc>
  <trdgrp lng="ca">
    <trd>ohm&iacute;metre</trd>,
    <trd>&ograve;hmmetre</trd>
  </trdgrp>
  <trd lng="cs">ohmmetr</trd>
  <trdgrp lng="de">
    <trd>Widerstandsmessger&auml;t</trd>,
    <trd>Ohmmeter</trd>
  </trdgrp>
  <trd lng="en">ohmmeter</trd>
  <trd lng="fa">&a_A;&a_h;&a_m;&zwnj;&a_m;&a_t;&a_r;</trd>
  <trdgrp lng="fr">
    <trd>ohmm&egrave;tre</trd>,
    <trd>ohm-m&egrave;tre<klr tip="ind"> (appareil de mesure)</klr>
    </trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ellen&aacute;ll&aacute;sm&eacute;r&odblac;</trd>,
    <trd>ohm-m&eacute;r&odblac;</trd>
  </trdgrp>
  <trd lng="id">ohmmeter</trd>
  <trd lng="nl">ohmmeter</trd>
  <trdgrp lng="pl">
    <trd>omomierz</trd>,
    <trd>miernik rezystancji</trd>
  </trdgrp>
  <trd lng="pt">ohm&iacute;metro</trd>
  <trd lng="sk">ohmmeter</trd>
</drv>
</art>

