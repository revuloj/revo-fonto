<art mrk="$Id: fuksi.xml,v 1.26 2023/11/10 06:08:01 revo Exp $">
<kap>
  <rad>fuksi</rad>/o
</kap>
<drv mrk="fuksi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="enoter1.0oj">enoteracoj</ref>
      (<trd lng="la">Fuchsia</trd>), kun belaj pendantaj floroj:
      <ekz>
        en la okcidenta marbordo <klr>[de Skotlando]</klr> abundas <tld/>oj, rododendroj
        kaj azaleoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3779">Skotlando</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/b/bc/Fuchsia_regia_-_blossom_(aka).jpg/640px-Fuchsia_regia_-_blossom_(aka).jpg" prm="CC BY-SA 2.5">floranta bran&ccirc;o de <tld/>o (Fuchsia regia)
        <fnt>
          <aut>Andr&eacute; Karwath aka Aka, propra verko</aut>
          <lok><url ref="&WCW;/File:Fuchsia_regia_-_blossom_(aka).jpg">&WCW;/File:Fuchsia_regia_-_blossom_(aka).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_f;&c_u;&c_k;&c_s;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_f;&c_u;&c_k;&c_s;&c_i;&c_ja;</trd>
  <trd lng="ca">f&uacute;csia</trd>
  <trd lng="cs">fuchsie</trd>
  <trd lng="de">Fuchsie</trd>
  <trd lng="en">fuchsia</trd>
  <trd lng="fa">&f_g;&a_l; &a_A_madda;&a_w;&f_y;&a_z;</trd>
  <trd lng="fr">fuchsia</trd>
  <trdgrp lng="hu">
    <trd>fukszia</trd>,
    <trd>cs&uuml;ng&odblac;ke</trd>
  </trdgrp>
  <trd lng="ja">&#x30D5;&#x30AF;&#x30B7;&#x30A2;</trd>
  <trd lng="oc">f&uacute;csia</trd>
  <trdgrp lng="pl">
    <trd>fuksja</trd>,
    <trd>u&lstroke;anka <klr>(pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>f&uacute;csia</trd>,
    <trd>brinco-de-princesa</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_u;&c_k;&c_s;&c_i;&c_ja;</trd>
  <trd lng="sk">fuksia</trd>
  <trd lng="uk">&#x444;&#x443;&#x43A;&#x441;&#x456;&#x44F;</trd>
  <trd lng="zh">&#x665A;&#x6A31;&#x79D1;&#x690D;&#x7269; <pr>w&#x1CE;ny&#x12B;ngk&#x113;zh&#xED;w&#xF9;</pr></trd>
</drv>
