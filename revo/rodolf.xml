<kap>
  <rad>Rodolf</rad>/o
</kap>
<drv mrk="rodolf.0o">
  <kap><tld/>o</kap>
  <snc mrk="rodolf.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> varia&jcirc;o de
      <ref tip="vid" cel="rudolf.0o">Rudolfo</ref>:
      <ekz>
        <tld/>o sidi&gcirc;as sur la &scirc;tona benko apud la pordo
        <fnt>
          <aut>V. Hugo, trad. D. Luez</aut>,
          <vrk>Angelo, Tirano de Padovo, 1:3</vrk>,
          <lok>Franca Esperantisto, 1986-04, numero 370</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o kaj Ana ne sidas en la &ccirc;ambro, ili parolas en la 
        &gcirc;ardeno
        <fnt>
          <aut>J. Hess</aut>,
          <vrk><url
          ref="http://esperantofre.com/eeo/EoDktH/Sabe/L02h.htm">
          &#191;Sabe Usted Esperanto?</url></vrk>,
          <lok>2000</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_R;&c_u;&c_d;&c_o;&c_l;&c_mol;&c_f;</trd>
    <trd lng="fr">Rodolphe</trd>
    <trd lng="pl">Rudolf</trd>
    <trd lng="ro">Rudolf</trd>
  </snc>
</drv>
</art>

