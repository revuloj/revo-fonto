<drv mrk="anonc.0anto">
  <kap>
    <tld/>anto
  </kap>
  <snc mrk="anonc.0anto.KOMUNE">
    <dif>
      Tiu, kiu <tld/>as:
      <ekz>
        <tld/>antinoj
        <fnt>
          <bib>MT</bib>
          <lok>&Psa; 68:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>antoj de paco
        <fnt>
          <bib>MT</bib>
          <lok>&Jes; 33:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>antoj de la evangelio de bono
        <fnt>
          <bib>NT</bib>
          <lok>&Rom; 10:15</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ja;&c_sh;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_v;&c_ja;&c_s;&c_t;&c_u;&c_n;</trd>
  </trdgrp>
  <trd lng="de">Verk&uuml;nder</trd>
  <trdgrp lng="fr">
    <trd>annonciateur<klr> (subst.)</klr></trd>,
    <trd>messager</trd>
  </trdgrp>
  <trd lng="he">&#x5DE;&#x5E4;&#x5E8;&#x5E1;&#x5DD;</trd>
</drv>

<drv mrk="anonc.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="anonc.0isto.KOMUNE">
    <dif>
      Homo, kiu profesie vo&ccirc;e <tld/>as.
    </dif>
    <subsnc mrk="anonc.0isto.strata">
      <dif>
        Homo kiu promenas tra urbo kaj <tld/>as
        oficialajn avizojn al la lo&gcirc;antoj:
        <ekz>
          mi konati&gcirc;is kun iu ru&gcirc;vestita viro,
          la urb-<tld/>isto
          <fnt>
            <aut>[J. Audenaert]</aut>,
            <vrk><url ref="http://users.skynet.be/audenaert.johan/kongreso.htm">
            Kongreso</url></vrk>,
            <lok>[vidita en 2011]</lok>
          </fnt>;
        </ekz>
        <ekz>
          se vi <klr>[la monologon]</klr> krios patose kiel multaj el niaj aktoroj, tiam
          &gcirc;i pla&ccirc;os al mi tiom same, kiel se miaj versoj estus elkriataj de publika
          <tld/>isto
          <fnt>
            <bib>Hamlet</bib>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="bg">&c_g;&c_l;&c_a;&c_sh;&c_a;&c_t;&c_a;&c_j;</trd>
    </subsnc>
    <subsnc mrk="anonc.0isto.TEL">
      <uzo tip="fak">TEL</uzo>
      <dif>
        <ref tip="dif" cel="parol.0isto.KOMUNE">Parolisto</ref>:
        <ekz>
          anta&ubreve; nelonge, iu fama &ccirc;ina televida <tld/>isto
          menciis sian
          filinon, kiu nun lernas en alilanda mezlernejo
          <fnt>
            <aut>Xiang Hong</aut>,
            <vrk><url ref="http://www.espero.com.cn/old/epch/11s/11s-1e/kejiao1-2.htm">
            Kiam lerneja studo fari&gcirc;is suferiga</url></vrk>,
            <lok>[vidita en 2011]</lok>
          </fnt>;
        </ekz>
        <ekz>
          du radio-<tld/>istoj, alproprigantaj al si britan ak&ccirc;enton, telefonis al la
          malsanulejo, &scirc;ajnigante, ke ili estas re&gcirc;ino <nom>Elizabeth</nom> kaj princo
          <nom>Charles</nom>
          <fnt>
            <bib>Monato</bib>
            <aut>Paul Gubbins</aut>
            <vrk><url ref="&Monato;/2013/010931.php">Mortas flegistino: kiu kulpas?</url></vrk>
            <lok>2013</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="bg">&c_g;&c_o;&c_v;&c_o;&c_r;&c_i;&c_t;&c_je;&c_l;</trd>
    </subsnc>
