<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>anonc</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="anonc.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="anonc.0i.antausciigi">
    <dif>
      Anta&ubreve;sciigi:
      <ekz>
        mi <tld/>os al vi mian alvenon;
      </ekz>
      <ekz>
        la kongresanoj devos sin <tld/>i plej balda&ubreve;.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dir.antaux0i">anta&ubreve;diri</ref>,
      <ref cel="aviz.0i">avizi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>anzeigen</trd>,
      <trd>ank&uuml;ndigen</trd>
    </trdgrp>
    <trd lng="nl">aankondigen</trd>
  </snc>
  <snc mrk="anonc.0i.publike">
    <dif>
      Publike sciigi:
      <ekz>
        <tld/>i per gazetoj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fam">disfamigi</ref>,
      <ref cel="trumpe.dis0i.FIG">distrumpeti</ref>,
      <ref cel="herold.0i">heroldi</ref>,
      <ref cel="publik.0igi">publikigi</ref>
    </refgrp>
    <trd lng="en">announcement</trd>
    <trdgrp lng="de">
      <trd>anzeigen</trd>,
      <trd>ver&ouml;ffentlichen</trd>,
      <trd>annoncierenen</trd>
    </trdgrp>
    <trd lng="nl">adverteren</trd>
  </snc>
  <snc mrk="anonc.0i.vizitanton">
    <dif>
      Komuniki al la dommastro la nomon de vizitanto.
    </dif>
    <trdgrp lng="de">
      <trd>ank&uuml;ndigen</trd>,
      <trd>anmelden</trd>
    </trdgrp>
    <trd lng="nl">aanmelden</trd>
    <trd lng="sk">ohl&aacute;si&tcaron;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;'&c_ja;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_je;&c_d;&c_a;&c_m;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">ozn&aacute;mit</trd>
  <trd lng="en">announce</trd>
  <trd lng="es">anunciar</trd>
  <trd lng="fr">annoncer</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_malmol;&c_ja;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_malmol;&c_ja;&c_v;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">ozn&aacute;mi&tcaron;</trd>
</drv>

<drv mrk="anonc.0o">
  <kap><tld/>o</kap>
  <snc mrk="anonc.0o.KOMUNE">
    <dif>
      Paroloj a&ubreve; skriboj por <tld/>i:
      <ekz>
        edzi&gcirc;<tld/>o;
      </ekz>
      <ekz>
        komerca <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;'&c_ja;&c_v;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_je;&c_d;&c_a;&c_m;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ozn&aacute;men&iacute;</trd>,
    <trd>inzer&aacute;t</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Anzeige</trd>,
    <trd>Zeitungsanzeige</trd>,
    <trd>Annonce</trd>
  </trdgrp>
  <trd lng="es">anuncio</trd>
  <trd lng="fr">annonce</trd>
  <trd lng="ru">&c_o;&c_b;&c_malmol;&c_ja;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="nl">
    <trd>aankondiging</trd>,
    <trd>advertentie</trd>
  </trdgrp>
  <trd lng="sk">oznam</trd>
</drv>

<drv mrk="anonc.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="anonc.0isto.KOMUNE">
    <dif>
      Homo, kiu profesie vo&ccirc;e <tld/>as.
    </dif>
    <subsnc mrk="anonc.0isto.strata">
      <dif>
        Homo kiu promenas tra urbo kaj <tld/>as
        oficialajn avizojn al la lo&gcirc;antoj:
        <ekz>
          mi konati&gcirc;is kun iu ru&gcirc;vestita viro,
          la urb-<tld/>isto
          <fnt>
            <aut>[J. Audenaert]</aut>,
            <vrk><url
            ref="http://users.skynet.be/audenaert.johan/kongreso.htm">
            Kongreso</url></vrk>,
            <lok>[vidita en 2011]</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="anonc.0isto.TEL">
      <uzo tip="fak">TEL</uzo>
      <ref tip="dif" cel="parol.0isto.KOMUNE">parolisto</ref>
      <ekz>
        anta&ubreve; nelonge, iu fama &ccirc;ina televida <tld/>isto menciis sian 
        filinon, kiu nun lernas en alilanda mezlernejo
        <fnt>
          <aut>Xiang Hong</aut>,
          <vrk><url
          ref="http://www.espero.com.cn/old/epch/11s/11s-1e/kejiao1-2.htm">
          Kiam lerneja studo fari&gcirc;is suferiga</url></vrk>,
          <lok>[vidita en 2011]</lok>
        </fnt>.
      </ekz>
    </subsnc>
  </snc>
  <trdgrp lng="fr">
    <trd>crieur public</trd>,
    <trd>pr&eacute;sentateur</trd>
  </trdgrp>
</drv>

<drv mrk="anonc.antaux0i">
  <kap>anta&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="anonc.antaux0i.KOMUNE">
    <dif>
      Anticipe <tld/>i:
      <ekz>
        tiel Dio plenumis tion, kion Li anta&ubreve;<tld/>is
        per la bu&scirc;o de &ccirc;iuj profetoj
        <fnt><bib>NT</bib><lok>&Ago; 3:18</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">annoncer &agrave; l'avance</trd>
</drv>
</art>
