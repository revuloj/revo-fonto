<drv mrk="anonc.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="anonc.0i.antausciigi">
    <dif>
      Anta&ubreve;sciigi:
      <ekz>
        mi <tld/>os al vi mian alvenon;
      </ekz>
      <ekz>
        la kongresanoj devos sin <tld/>i plej balda&ubreve;;
      </ekz>
      <ekz>
        la tuta komunumo sendis, por paroli kun la Benjamenidoj <klr>[&#x2026;]</klr> kaj
        <tld/>i al ili pacon
        <fnt>
          <bib>MT</bib>
          <lok>&Jug; 21:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>u tion, kio estos kaj kio venos
        <fnt>
          <bib>MT</bib>
          <lok>&Jes; 44:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[en letero]</klr> mi <tld/>is al li mian decidon veturi en Rusujon
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
          <lok>Impresoj de svedoj, voja&gcirc;antaj en Rusujo</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam mi efektive eklo&gcirc;os tie, mi <tld/>os mian novan adreson
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i liberecon al kaptitoj
        <fnt>
          <bib>MT</bib>
          <lok>&Jes; 61:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve;te kaj bone a&ubreve;deble li <tld/>is, ke la doma instruisto ludos solon sur la
        fluto
        <fnt>
          <bib>Fab3</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dir.antaux0i">anta&ubreve;diri</ref>,
      <ref cel="profet.0i">profeti</ref>,
      <ref cel="aviz.0i">avizi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>anzeigen</trd>,
      <trd>ank&uuml;ndigen</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd><baz>bawa</baz>membawa berita</trd>,
      <trd><baz>bawa</baz>membawa kabar</trd>,
      <trd><baz>beri</baz>memberi tahu</trd>,
      <trd><baz>beri</baz>memberitahukan</trd>,
      <trd><baz>berita</baz>memberitakan</trd>,
      <trd><baz>kabar</baz>mengabarkan</trd>,
      <trd><baz>maklum</baz>mempermaklumkan</trd>,
      <trd><baz>sampai</baz>menyampaikan berita</trd>,
      <trd><baz>sampai</baz>menyampaikan kabar</trd>
    </trdgrp>
    <trd lng="nl">aankondigen</trd>
    <trd lng="zh">&#x524D;&#x594F; <pr>qi&aacute;nz&ograve;u</pr></trd>
  </snc>
  <snc mrk="anonc.0i.publike">
    <dif>
      Publike <ref tip="super" cel="sci.0igi">sciigi</ref>:
      <ekz>
        <tld/>i per gazetoj;
      </ekz>
      <ekz>
        tio estis grava okazanta&jcirc;o, la urbo sciis pri tio, kvankam oni ne <tld/>is &gcirc;in
        per publika tamburado
        <fnt>
          <bib>Fab4</bib>
          <lok>&leftquot;Bela&rightquot;</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ordonis, ke per &ccirc;iuj pre&gcirc;ejaj sonoriloj oni <tld/>u la edzi&gcirc;ofeston
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        tempas, al mond&#x2019; <tld/>i pri meritoj liaj
        <fnt>
          <aut>A. Mickiewicz, trad. A. Grabowski</aut>
          <vrk>Sinjoro Tadeo</vrk>
          <lok>1918</lok>
        </fnt>;
      </ekz>
      <ekz>
        la re&gcirc;o <tld/>is, ke tiu, kiu venkos lian filinon en vetkurado, edzi&gcirc;os kun &scirc;i
        <fnt>
          <bib>ElektFab</bib>
          <lok>Sesope oni trairas la tutan mondon</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ju&gcirc;isto <tld/>is al &ccirc;iuj &ccirc;eestantoj: <klr>[&#x2026;]</klr>, ke ili povas eliri en la
        korton
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam estis <tld/>itaj la rezultoj de la referendumoj, montri&gcirc;is, ke partoprenis
        en la referendumoj 60,3&#x202F;% de la vo&ccirc;donrajtigitoj
        <fnt>
          <bib>Monato</bib>
          <aut>Zlatko TI&Scaron;LJAR</aut>
          <vrk><url ref="&Monato;/2003/008112.html">Jes al E&ubreve;ropo sed &ccirc;u &scirc;anco perdita?</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fam.dis0igi">disfamigi</ref>,
      <ref cel="trumpe.dis0i.FIG">distrumpeti</ref>,
      <ref cel="herold.0i">heroldi</ref>,
      <ref cel="publik.0igi">publikigi</ref>,
      <ref cel="deklar.0i">deklari</ref>
    </refgrp>
    <trd lng="en">announcement</trd>
    <trd lng="ms">pengumuman</trd>
    <trdgrp lng="de">
      <trd>anzeigen</trd>,
      <trd>bekanntmachen</trd>,
      <trd>ver&ouml;ffentlichen</trd>,
      <trd>annoncieren</trd>
