<drv mrk="anonc.gazet0o">
  <kap>gazet<tld/>o</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="anonc.gazet0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      <tld lit="A"/>o publikigita per
      <ref cel="gazet.0o">gazeto</ref>:
      <ekz>
        Piro&scirc;ima trovis solvon / Bon&scirc;ance en gazet<tld/>' / Kaj tuj li sendas sian filon
        / A&ccirc;eti la harkreskigilon
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
          <lok>Japana Balado</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ar li mem havas nek tempon nek emon fal&ccirc;i la
        park-gazonon, li ser&ccirc;as per gazet<tld/>o &gcirc;ardeniston
        <fnt>
          <aut>Ipfelkofer Jomo</aut>:
          <vrk>Matematiko: Komparo de instrumetodoj</vrk>
