  <trd lng="be">&c_g;&c_a;&c_z;&c_e;&c_t;&c_n;&c_a;&c_ja; &c_a;&c_b;&#x2019;&c_ja;&c_v;&c_a;</trd>
  <trd lng="bg">&c_o;&c_b;&c_ja;&c_v;&c_a; &c_v;&c_malmol;&c_v; &c_v;&c_je;&c_s;&c_t;&c_n;&c_i;&c_k;</trd>
