    <trd>&#x43F;&#x43E;&c_v;&c_ib;&c_d;&#x43E;&#x43C;&#x43B;&#x44F;&c_t;&c_i;</trd>,
    <trd>&c_d;&#x43E;&c_v;&#x43E;&c_d;&c_i;&c_t;&c_i; &c_d;&#x43E; &c_v;&c_ib;&c_d;&#x43E;&#x43C;&c_a;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_je;&c_d;&c_v;&c_ib;&c_shch;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="anonc.0o">
  <kap><tld/>o</kap>
  <snc mrk="anonc.0o.KOMUNE">
    <dif>
      <ref tip="lst" lst="voko:komunikoj" cel="parol.0o.ago">Parolo</ref>j
      a&ubreve; skriboj por <tld/>i:
      <ekz>
        edzi&gcirc;<tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Ib kaj malgranda kristino</lok>
        </fnt>;
      </ekz>
      <ekz>
        komerca <tld/>o;
      </ekz>
      <ekz>
        mi abonas la Gazeton de <tld lit="A"/>oj
        <fnt>
          <bib>Fab3</bib>
          <lok>Bona humoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkfoje tage vi ricevos <tld/>ojn de nova&jcirc;-tekstoj
        <fnt>
          <bib>Monato</bib>
