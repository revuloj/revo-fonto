<drv mrk="anonc.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="anonc.0isto.KOMUNE">
    <dif>
      Homo, kiu profesie vo&ccirc;e <tld/>as.
    </dif>
    <subsnc mrk="anonc.0isto.strata">
      <dif>
        Homo kiu promenas tra urbo kaj <tld/>as
        oficialajn avizojn al la lo&gcirc;antoj:
        <ekz>
          mi konati&gcirc;is kun iu ru&gcirc;vestita viro,
          la urb-<tld/>isto
          <fnt>
            <aut>[J. Audenaert]</aut>,
            <vrk><url ref="http://users.skynet.be/audenaert.johan/kongreso.htm">
            Kongreso</url></vrk>,
            <lok>[vidita en 2011]</lok>
          </fnt>;
        </ekz>
        <ekz>
          se vi <klr>[la monologon]</klr> krios patose kiel multaj el niaj aktoroj, tiam
          &gcirc;i pla&ccirc;os al mi tiom same, kiel se miaj versoj estus elkriataj de publika
          <tld/>isto
          <fnt>
            <bib>Hamlet</bib>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="bg">&c_g;&c_l;&c_a;&c_sh;&c_a;&c_t;&c_a;&c_j;</trd>
    </subsnc>
    <subsnc mrk="anonc.0isto.TEL">
      <uzo tip="fak">TEL</uzo>
      <dif>
        <ref tip="dif" cel="parol.0isto.KOMUNE">Parolisto</ref>:
        <ekz>
          anta&ubreve; nelonge, iu fama &ccirc;ina televida <tld/>isto
          menciis sian
          filinon, kiu nun lernas en alilanda mezlernejo
          <fnt>
            <aut>Xiang Hong</aut>,
            <vrk><url ref="http://www.espero.com.cn/old/epch/11s/11s-1e/kejiao1-2.htm">
            Kiam lerneja studo fari&gcirc;is suferiga</url></vrk>,
            <lok>[vidita en 2011]</lok>
          </fnt>;
        </ekz>
        <ekz>
          du radio-<tld/>istoj, alproprigantaj al si britan ak&ccirc;enton, telefonis al la
          malsanulejo, &scirc;ajnigante, ke ili estas re&gcirc;ino <nom>Elizabeth</nom> kaj princo
          <nom>Charles</nom>
          <fnt>
            <bib>Monato</bib>
            <aut>Paul Gubbins</aut>
            <vrk><url ref="&Monato;/2013/010931.php">Mortas flegistino: kiu kulpas?</url></vrk>
            <lok>2013</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="bg">&c_g;&c_o;&c_v;&c_o;&c_r;&c_i;&c_t;&c_je;&c_l;</trd>
    </subsnc>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ja;&c_sh;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_v;&c_ja;&c_s;&c_t;&c_u;&c_n;</trd>,
    <trd>&c_d;&c_y;&c_k;&c_t;&c_a;&c_r; <klr>(&c_n;&c_a; &c_T;&c_V;, &c_r;&c_a;&c_d;&c_y;&c_jo;)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ohla&scaron;ovatel</trd>,
    <trd>oznamovatel</trd>
  </trdgrp>
  <trd lng="de">Verk&uuml;nder</trd>
  <trdgrp lng="fr">
    <trd>crieur public</trd>,
    <trd>pr&eacute;sentateur</trd>
  </trdgrp>
  <trd lng="he">&mem;&gimel;&yod;&shin;</trd>
  <trdgrp lng="id">
    <trd><baz>bawa</baz>pembawa berita</trd>,
    <trd><baz>berita</baz>pemberita</trd>,
    <trd>juru kabar</trd>,
    <trd><baz>siar</baz>penyiar</trd>,
    <trd><baz>warta</baz>pewarta</trd>
  </trdgrp>
  <trd lng="ja">&#x30A2;&#x30CA;&#x30A6;&#x30F3;&#x30B5;&#x30FC;
  <pr>&#x3042;&#x306A;&#x3046;&#x3093;&#x3055;&#x30FC;</pr></trd>
  <trdgrp lng="pl">
    <trd>prezenter</trd>,
    <trd>spiker</trd>,
    <trd>og&lstroke;aszaj&aogonek;cy wiadomo&sacute;ci</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ohlasovate&lcaron;</trd>,
    <trd>oznamovate&lcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="anonc.antaux0i">
  <kap>anta&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="anonc.antaux0i.KOMUNE">
    <dif>
      Anticipe <tld/>i:
      <ekz>
        tiel Dio plenumis tion, kion Li anta&ubreve;<tld/>is
        per la bu&scirc;o de &ccirc;iuj profetoj
        <fnt><bib>NT</bib> <lok>&Ago; 3:18</lok></fnt>;
      </ekz>
      <ekz>
        la glora instruisto de surdamutaj, <nom>Sicard</nom>, kun granda pompo anta&ubreve;<tld/>is
        oportunan pazigrafion
        <fnt>
          <aut>L. Einstein</aut>,
          <bib>FK</bib>, <lok>Al la
          historio de la provoj de lingvoj tutmondaj
          de Leibnitz &gcirc;is la nuna tempo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&#x2019;&c_ja;&c_w;&c_l;&c_ja;&c_c;&c_mol; &c_z;&c_a;&c_g;&c_a;&c_d;&c_z;&c_ja;</trd>,
    <trd>&c_a;&c_n;&c_a;&c_n;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_o;&c_b;&c_ja;&c_v;&c_ja;&c_v;&c_a;&c_m; &c_p;&c_r;&c_je;&c_d;&c_v;&c_a;&c_r;&c_i;&c_t;&c_je;&c_l;&c_n;&c_o;</trd>
  <trd lng="de">vorank&uuml;ndigen</trd>
  <trd lng="fr">annoncer &agrave; l'avance</trd>
  <trd lng="id"><baz>beri</baz>memberi tahu sebelumnya</trd>
  <trdgrp lng="pl">
    <trd>zapowiedzie&cacute; naprz&oacute;d</trd>,
    <trd>zapowiedzie&cacute; z g&oacute;ry</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_o;&c_g;&c_o;&c_l;&c_o;&c_sh;&c_u;&c_v;&c_a;&c_t;&c_i; &c_z;&c_a;&c_z;&c_d;&c_a;&c_l;&c_je;&c_g;&c_ib;&c_d;&c_mol;/&c_z;&c_a;&c_v;&c_ch;&c_a;&c_s;&c_n;&c_o;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_v;&c_ib;&c_shch;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

