      <trd><baz>sebar</baz>menyebar</trd>,
      <trd><baz>sebar</baz>menyebarkan</trd>,
      <trd><baz>sebar</baz>menyebarluaskan</trd>,
      <trd><baz>siar</baz>menyiarkan</trd>,
      <trd><baz>umum</baz>mengumumkan</trd>,
      <trd><baz>warta</baz>mewartakan</trd>
    </trdgrp>
    <trd lng="nl">adverteren</trd>
    <trdgrp lng="zh">
