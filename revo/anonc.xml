<drv mrk="anonc.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="anonc.0i.antausciigi">
    <dif>
      Anta&ubreve;sciigi:
      <ekz>
        mi <tld/>os al vi mian alvenon;
      </ekz>
      <ekz>
        la kongresanoj devos sin <tld/>i plej balda&ubreve;;
      </ekz>
      <ekz>
        la tuta komunumo sendis, por paroli kun la Benjamenidoj <klr>[&#x2026;]</klr> kaj
        <tld/>i al ili pacon
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 21:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>u tion, kio estos kaj kio venos
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 44:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i liberecon al kaptitoj
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 61:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve;te kaj bone a&ubreve;deble li <tld/>is, ke la doma instruisto ludos solon sur la
        fluto
        <fnt>
          <bib>Fab3</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dir.antaux0i">anta&ubreve;diri</ref>,
      <ref cel="profet.0i">profeti</ref>,
      <ref cel="aviz.0i">avizi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>anzeigen</trd>,
      <trd>ank&uuml;ndigen</trd>
    </trdgrp>
    <trd lng="nl">aankondigen</trd>
  </snc>
  <snc mrk="anonc.0i.publike">
    <dif>
      Publike <ref tip="super" cel="sci.0igi">sciigi</ref>:
      <ekz>
        <tld/>i per gazetoj;
      </ekz>
      <ekz>
        tio estis grava okazanta&jcirc;o, la urbo sciis pri tio, kvankam oni ne <tld/>is &gcirc;in
        per publika tamburado
        <fnt>
          <bib>Fab4</bib>
          <lok>&leftquot;Bela&rightquot;</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ordonis, ke per &ccirc;iuj pre&gcirc;ejaj sonoriloj oni <tld/>u la edzi&gcirc;ofeston
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        Alfredo fian&ccirc;i&gcirc;is kun Kala <klr>[&#x2026;]</klr> kaj pri la fian&ccirc;i&gcirc;o estis <tld/>ite en
        la urbaj gazetoj
        <fnt>
          <bib>Fab4</bib>
          <lok>&leftquot;Bela&rightquot;</lok>
        </fnt>;
      </ekz>
      <ekz>
        la re&gcirc;o <tld/>is, ke tiu, kiu venkos lian filinon en vetkurado, edzi&gcirc;os kun &scirc;i
        <fnt>
          <bib>ElektFab</bib>
          <lok>Sesope oni trairas la tutan mondon</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ju&gcirc;isto <tld/>is al &ccirc;iuj &ccirc;eestantoj: <klr>[&#x2026;]</klr>, ke ili povas eliri en la
        korton
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam estis <tld/>itaj la rezultoj de la referendumoj, montri&gcirc;is, ke partoprenis
        en la referendumoj 60,3&#x202F;% de la vo&ccirc;donrajtigitoj
        <fnt>
          <bib>Monato</bib>
          <aut>Zlatko TI&Scaron;LJAR</aut>
          <vrk><url ref="https://www.monato.be/2003/008112.html">Jes al E&ubreve;ropo sed &ccirc;u &scirc;anco perdita?</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fam">disfamigi</ref>,
      <ref cel="trumpe.dis0i.FIG">distrumpeti</ref>,
      <ref cel="herold.0i">heroldi</ref>,
      <ref cel="publik.0igi">publikigi</ref>,
      <ref cel="deklar.0i">deklari</ref>
    </refgrp>
    <trd lng="en">announcement</trd>
    <trd lng="ms">pengumuman</trd>
    <trdgrp lng="de">
      <trd>anzeigen</trd>,
      <trd>ver&ouml;ffentlichen</trd>,
      <trd>annoncieren</trd>
    </trdgrp>
    <trd lng="nl">adverteren</trd>
  </snc>
  <snc mrk="anonc.0i.vizitanton">
    <dif>
      Komuniki al la dommastro la nomon de vizitanto.
    </dif>
    <trdgrp lng="de">
      <trd>ank&uuml;ndigen</trd>,
      <trd>anmelden</trd>
    </trdgrp>
    <trd lng="nl">aanmelden</trd>
    <trd lng="sk">ohl&aacute;si&tcaron;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;'&c_ja;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_je;&c_d;&c_a;&c_m;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_o;&c_b;&c_ja;&c_v;&c_ja;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_a;&c_n;&c_o;&c_n;&c_s;&c_i;&c_r;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hl&aacute;sit</trd>,
    <trd>nahl&aacute;sit</trd>,
    <trd>ohl&aacute;sit n&aacute;v&scaron;t&ecaron;vu</trd>,
    <trd>ozn&aacute;mit</trd>,
    <trd>oznamovat</trd>
  </trdgrp>
  <trd lng="en">announce</trd>
  <trd lng="es">anunciar</trd>
  <trd lng="fr">annoncer</trd>
  <trd lng="he">&#x5DC;&#x5D4;&#x5DB;&#x5E8;&#x5D9;&#x5D6;</trd>
  <trdgrp lng="pt">
    <trd>anunciar</trd>,
    <trd>noticiar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_malmol;&c_ja;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_malmol;&c_ja;&c_v;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nahl&aacute;si&tcaron;</trd>,
    <trd>ozn&aacute;mi&tcaron;</trd>,
    <trd>oznamova&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="anonc.0o">
  <kap><tld/>o</kap>
  <snc mrk="anonc.0o.KOMUNE">
    <dif>
      <ref tip="lst" lst="voko:komunikoj" cel="parol.0o.ago">Parolo</ref>j
      a&ubreve; skriboj por <tld/>i:
      <ekz>
        edzi&gcirc;<tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Ib kaj malgranda kristino</lok>
        </fnt>;
      </ekz>
      <ekz>
        komerca <tld/>o;
      </ekz>
      <ekz>
        mi abonas la Gazeton de <tld lit="A"/>oj
        <fnt>
          <bib>Fab3</bib>
          <lok>Bona humoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkfoje tage vi ricevos <tld/>ojn de nova&jcirc;-tekstoj
        <fnt>
          <bib>Monato</bib>
          <aut>Grigorij Arosev, Andrej GRIGORJEVSKIJ</aut>
          <vrk><url ref="https://www.monato.be/2004/008210.html">Abonu!</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>egoj en &jcirc;urnaloj, belaj multekostaj afi&scirc;oj &ccirc;ie en la urboj
        <fnt>
          <bib>Monato</bib>
          <aut>Bo Sandelin</aut>
          <vrk><url ref="https://www.monato.be/2004/008235.html">Svedoj balotis kontra&ubreve; e&ubreve;ro</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="af">kennisgewing</trd>
