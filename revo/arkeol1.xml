<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>arkeologi</rad>/o
</kap>
<drv mrk="arkeol1.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Scienco pri la monumentoj kaj artoj de la antikva epoko.
    </dif>
  </snc>
  <trd lng="es">arqueolog&iacute;a</trd>
  <trd lng="fr">arch&eacute;ologie</trd>
  <trd lng="ru">&c_a;&c_r;&c_h;&c_je;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trdgrp lng="sv">
    <trd>arkeologi</trd>,
    <trd>fornforskning</trd>
  </trdgrp>
</drv>

<drv mrk="arkeol1.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Fakulo pri <tld/>o.
    </dif>
    <ref tip="sin" cel="arkeol.0o">arkeologo.</ref>
  </snc>
  <trd lng="es">arque&oacute;logo</trd>
  <trd lng="fr">arch&eacute;ologue</trd>
  <trd lng="sv">arkeolog</trd>
</drv>
</art>
