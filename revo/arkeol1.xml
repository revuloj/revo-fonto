<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: arkeol1.xml,v 1.32 2021/02/28 18:35:45 revo Exp $">
<kap>
  <ofc>3</ofc><rad>arkeologi</rad>/o <fnt><bib>PV</bib></fnt>,
  <var><kap><ofc>3</ofc><rad var="&hcirc;">ar&hcirc;eologi</rad>/o</kap></var>
</kap>

<drv mrk="arkeol1.0o">
  <kap><tld/>o, <var><kap><tld var="&hcirc;"/>o</kap></var></kap>
  <snc mrk="arkeol1.0o.ARKE">
    <uzo tip="fak">ARKE</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Scienco pri la monumentoj kaj artoj de antikvaj epokoj
      <ekz>
        ni distingas prahistorian, klasikepokan kaj mezepokan <tld/>on,
        aldonante ke tiu disdivido validas unuavice por
        E&ubreve;ropo
        <fnt><bib>IntArkeol</bib>, <lok>p. 7a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_h;&c_je;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_a;&c_r;&c_h;&c_je;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="cs">archeologie</trd>
  <trd lng="de">Arch&auml;ologie</trd>
  <trdgrp lng="en">
    <trd>archeology</trd>,
    <trd>archaeology</trd>,
  </trdgrp>
  <trd lng="es">arqueolog&iacute;a</trd>
  <trd lng="fa">&a_b;&a_A;&a_s;&a_t;&a_A;&a_n;&zwnj;&a_sh;&a_n;&a_A;&a_s;&f_y;</trd>
  <trd lng="fr">arch&eacute;ologie</trd>
  <trd lng="he">&#x5D0;&#x5B7;&#x5E8;&#x5DB;&#x5B5;&#x5D9;&#x5D0;&#x5D5;&#x5B9;&#x5DC;&#x5D5;&#x5B9;&#x5D2;&#x5B4;&#x5D9;&#x5B8;&#x5D4;</trd>
  <trdgrp lng="hu">
    <trd>r&eacute;g&eacute;szet</trd>,
    <trd>archeol&oacute;gia</trd>
  </trdgrp>
  <trd lng="it">archeologia</trd>
  <trdgrp lng="nl">
    <trd>oudheidkunde</trd>,
    <trd>archeologie</trd>
  </trdgrp>
  <trd lng="pl">archeologia</trd>
  <trd lng="ru">&c_a;&c_r;&c_h;&c_je;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>archeol&oacute;gia</trd>,
    <trd>archeol&oacute;gia</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>arkeologi</trd>,
    <trd>fornforskning</trd>
  </trdgrp>
</drv>

<drv mrk="arkeol1.0a">
  <kap><tld/>a, <var><kap><tld var="&hcirc;"/>a</kap></var></kap>
  <snc mrk="arkeol1.0a.scienca">
    <dif>
      Rilata al <tld/>o, al ties metodoj, esploradoj, konkludoj:
      <ekz>
        direktoro de la <tld/>a esploro-projekto
        <fnt>
          <aut>A. Giridhar Rao</aut>
          <vrk>La plej frua skribo</vrk>
          <bib>Monato</bib>
          <lok>1999:8, p. 12a</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi estas ano de la loka <tld/>a societo kaj iam a&ubreve;dis prelegon
        pri la plej
        antikva ponto en Irlando
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006878.html">Dommastrumo: &Gcirc;ardenumi</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        trovita dum <tld/>aj foslaboroj
        <fnt>
          <bib>Monato</bib>
          <aut>Lida Elbakjan</aut>
          <vrk><url ref="https://www.monato.be/2003/007590.html">Armenio festis 1700 jarojn da kristanismo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        multaj <tld/>a&jcirc;oj trovi&gcirc;as anka&ubreve; &ccirc;e la lagobordo
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="https://www.monato.be/2007/008994.php">Printempa etoso &ccirc;e la lago de Ohrid</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la urbestro de Jerusalemo anoncis gravan <tld var="&hcirc;"/>an malkovron
        <fnt>
          <aut>P. Cezar Pires</aut>,
          <vrk><url ref="https://esperanto.blog/2018/01/03/sigelo-2700-jaraga-de-la-tiama-jerusalema-substatestro-estis-trovita/">
          Sigelo 2700-jara&gcirc;a de la tiama jerusalema
          sub&scirc;tatestro estis trovita</url></vrk>,
          <lok>esperanto.blog, 2018-01-03</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">archeologisch</trd>
    <trd lng="fa">&a_b;&a_A;&a_s;&a_t;&a_A;&a_n;&zwnj;&a_sh;&a_n;&a_A;&a_kh;&a_t;&f_y;</trd>
  </snc>
  <snc mrk="arkeol1.0a.praepoka">
    <dif>
      Praepoka:
      <ekz>
        <tld/>aj kulturoj en &Ccirc;inio
        <fnt>
          <aut>B. Golden</aut>
          <vrk>&Ccirc;ina ceramika historio</vrk>
          <bib>Monato</bib>,
          <lok>1995:8, p. 22</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>archaisch</trd>,
      <trd>vorzeitlich</trd>
    </trdgrp>
    <trd lng="fa">&a_b;&a_A;&a_s;&a_t;&a_A;&a_n;&f_y;</trd>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_h;&c_je;&c_a;&c_l;&c_ja;&c_g;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="bg">&c_a;&c_r;&c_h;&c_je;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;</trd>
  
  <trdgrp lng="en">
    <trd>archeological</trd>,
    <trd>archaeological</trd>,
  </trdgrp>
  <trd lng="fr">arch&eacute;ologique</trd>
  <trd lng="he">&#x5D0;&#x5B7;&#x5E8;&#x5DB;&#x5B5;&#x5D9;&#x5D0;&#x5D5;&#x5B9;&#x5DC;&#x5D5;&#x5B9;&#x5D2;&#x5B4;&#x5D9;&#x5B8;&#x5D4;</trd>
  <trdgrp lng="hu">
    <trd>r&eacute;g&eacute;szeti</trd>,
    <trd>archeol&oacute;giai</trd>
  </trdgrp>
  <trd lng="it">archeologico</trd>
  <trdgrp lng="nl">
    <trd>oudheidkundig</trd>,
    <trd>archeologisch</trd>
  </trdgrp>
  <trd lng="pl">archeologiczny</trd>
  <trd lng="ru">&c_a;&c_r;&c_h;&c_je;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="arkeol1.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="arkeol1.0isto.ARKE">
    <uzo tip="fak">ARKE</uzo>
    <dif>
      Fakulo pri <tld/>o.
    </dif>
    <ref tip="sin" cel="arkeol.0o">arkeologo</ref>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_h;&c_je;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="bg">&c_a;&c_r;&c_h;&c_je;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="de">Arch&auml;ologe</trd>
  <trdgrp lng="en">
    <trd>archeologist</trd>,
    <trd>archaeologist</trd>,
  </trdgrp>
  <trd lng="es">arque&oacute;logo</trd>
  <trd lng="fa">&a_b;&a_A;&a_s;&a_t;&a_A;&a_n;&zwnj;&a_sh;&a_n;&a_A;&a_s;</trd>
  <trd lng="fr">arch&eacute;ologue</trd>
  <trdgrp lng="hu">
    <trd>r&eacute;g&eacute;sz</trd>,
    <trd>archeol&oacute;gus</trd>
  </trdgrp>
  <trd lng="it">archeologo</trd>
  <trdgrp lng="nl">
    <trd>oudheidkundige</trd>,
    <trd>archeoloog</trd>
  </trdgrp>
  <trd lng="pl">archeolog</trd>
  <trd lng="ro">arheolog</trd>
  <trd lng="ru">&c_a;&c_r;&c_h;&c_je;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="sk">archeol&oacute;g</trd>
  <trd lng="sv">arkeolog</trd>
</drv>

<drv mrk="arkeol1.subakva0o">
  <kap>subakva <tld/>o,
  <var><kap>submara <tld/>o</kap></var></kap>
  <snc mrk="arkeol1.subakva0o.ARKE">
    <uzo tip="fak">ARKE</uzo>
    <dif>
      Bran&ccirc;o de <ref tip="malprt" cel="arkeol1.0o"><tld/>o</ref>, kiu
      esploras per ta&ubreve;gaj rimedoj subakvi&gcirc;intajn antikva&jcirc;ojn:
      <ekz>
        la submara, respektive subakva, <tld/>o, estas la plej nova
        <tld/>a bran&ccirc;o
        <fnt><bib>IntArkeol</bib>, <lok>p. 51a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Unterwasser<ind>arch&auml;ologie</ind></trd>
  <trdgrp lng="en">
    <trd>underwater archeology</trd>,
    <trd>underwater archaeology</trd>,
  </trdgrp>
  <trd lng="bg">&c_p;&c_o;&c_d;&c_v;&c_o;&c_d;&c_n;&c_a; &c_a;&c_r;&c_h;&c_je;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="fa">&a_b;&a_A;&a_s;&a_t;&a_A;&a_n;&zwnj;&a_sh;&a_n;&a_A;&a_s;&f_y; &a_z;&f_y;&a_r;&a_d;&a_r;&f_y;&a_A;</trd>
  <trd lng="fr">arch&eacute;ologie sous-marine</trd>
  <trd lng="hu">tenger alatti <ind>r&eacute;g&eacute;szet</ind></trd>
  <trd lng="it"><ind>archeologia</ind> subacquea</trd>
  <trd lng="nl">onderwaterarcheologie</trd>
  <trd lng="pl">archeologia podwodna</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_v;&c_o;&c_d;&c_n;&c_a;&c_ja; <ind>&c_a;&c_r;&c_h;&c_je;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</ind></trd>
</drv>
</art>
