<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: leksik1.xml,v 1.24 2025/04/02 14:41:09 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>leksikon</rad>/o
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="leksik1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">LIN</uzo>
  <snc mrk="leksik1.0o.LIN">
    <dif>
      Mallonga <ref tip="super" cel="vort.0aro">vortaro</ref> sen ekzemploj:
      <ekz>
        unu el ili sciis parkere la tutan latinan <tld/>on
        <fnt>
          <bib>Fab3</bib>
          <lok>Johan&ccirc;jo-malsa&gcirc;ulo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu lingvo postulis apartan <tld/>on, kiu devis enhavi &ccirc;iun vorton kun &gcirc;iaj
        formoj deklinaciaj kaj konjugacia
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la greka teksto &ccirc;i tie uzas la vorton <nac>&#181;&epsilon;&lambda;&alfa;&gamma;&chi;&omikron;&lambda;&jota;&kappa;&omikron;&sigma_fina;</nac>, kiun <tld/>o kutime
        difinas
        kiel &leftquot;deprimo&rightquot;
        <fnt>
          <bib>Rsp</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i tiu verko ne celas esti erudicia <tld/>o
        <fnt><aut>Gaston Waringhien</aut>, <bib>PIV1</bib>, <lok>&gcirc;enerala
        anta&ubreve;parolo</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="leksik2.0o">leksiko</ref>
    <trdgrp lng="de">
      <trd>Glossar</trd>,
      <trd>W&ouml;rterbuch</trd>,
      <trd>W&ouml;rterverzeichnis</trd>,
      <trd>W&ouml;rterliste</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>sz&oacute;k&eacute;szlet</trd>,
      <trd>sz&oacute;t&aacute;r <klr tip="amb">(pl. Pet&odblac;fi-)</klr></trd>
    </trdgrp>      
  </snc>
  <snc mrk="leksik1.0o.faka">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Speciala faka vortaro:
      <ekz>
        Na&ubreve;lingva Etimologia <tld lit="L"/>o, Fervojteknika <tld lit="L"/>o
        <fnt>
          <bib>EeP</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="encikl1.0o">enciklopedio</ref>
    <refgrp tip="vid">
       <ref cel="terminologi.0o.terminaro">terminologio<sncref/></ref>,
       <ref cel="viki.0o">vikio</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Lexikon</trd>,
      <trd>Nachschlagewerk</trd>,
      <trd>Fachw&ouml;rterbuch</trd>,
      <trd>Sachw&ouml;rterbuch</trd>
    </trdgrp>
    <trd lng="hu">lexikon</trd>    
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Anka&#365; eblas diri <ke>vortaro</ke>, <ke>te&#293;nika/faka terminaro</ke>, <ke>terminaro</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_l;&c_je;&c_k;&c_s;&c_ib;&c_k;&c_o;&c_n;</trd>,
    <trd>&c_s;&c_l;&c_o;&c_w;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="br">geriaoueg</trd>
  <trdgrp lng="cs">
    <trd>encyklopedie</trd>,
    <trd>lexikon</trd>,
    <trd>nau&ccaron;. slovn&iacute;k</trd>
  </trdgrp>
  <trd lng="el">&lambda;&epsilon;&xi;&jota;&kappa;&omikron;&lambda;&omikron_ton;&gamma;&jota;&omikron;</trd>
  <trd lng="en">lexicon</trd>
  <trd lng="fr">lexique</trd>
  <trdgrp lng="ja">
    <trd>&#29992;&#35486;&#38598;  <pr>&#12424;&#12358;&#12372;&#12375;&#12421;&#12358;</pr></trd>,
    <trd>&#23567;&#36766;&#20856;  <pr>&#12375;&#12423;&#12358;&#12376;&#12390;&#12435;</pr></trd>,
    <trd>&#35486;&#24409;  <pr>&#12372;&#12356;</pr></trd>,
    <trd>&#35486;&#24409;&#30446;&#37682;  <pr>&#12372;&#12356;&#12418;&#12367;&#12429;&#12367;</pr></trd>
  </trdgrp>
  <trd lng="nl">lexicon</trd>
  <trdgrp lng="pl">
    <trd>leksykon</trd>,
    <trd>wokabularz</trd>
  </trdgrp>
  <trd lng="pt">l&eacute;xico</trd>
  <trd lng="ro">lexicon</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_je;&c_k;&c_s;&c_i;&c_k;&c_o;&c_n;</trd>,
    <trd>&c_s;&c_l;&c_o;&c_v;&c_a;&c_r;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>encyklop&eacute;dia</trd>,
    <trd>lexik&oacute;n</trd>,
    <trd>slovn&iacute;k <klr>(n&aacute;u&ccaron;n&yacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#1083;&c_je;&#1082;&c_s;&c_i;&#1082;&#1086;&#1085;</trd>,
    <trd>(&c_je;&#1085;&c_c;&c_i;&#1082;&#1083;&#1086;&#1087;&c_je;&c_d;&c_i;&c_ch;&#1085;&c_i;&c_j;) &c_s;&#1083;&#1086;&c_v;&#1085;&c_i;&#1082;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#35422;&#24235; <pr>c&#237;k&#249;</pr></trd>,
    <trd>&#35789;&#24211; <pr>c&#237;k&#249;</pr></trd>,
    <trd>&#35422;&#20856; <pr>c&#237;di&#462;n</pr></trd>,
    <trd>&#35789;&#20856; <pr>c&#237;di&#462;n</pr></trd>
  </trdgrp>
</drv>
</art>
