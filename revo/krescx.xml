<kap>
  <rad>kres&ccirc;end</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="krescx.0o">
  <kap><tld/>o</kap>
  <snc mrk="krescx.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Iom-post-ioma plila&ubreve;tigo.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_e;&c_sh;&c_ch;&c_e;&c_n;&c_d;&c_a;</trd>
  <trd lng="de">Crescendo</trd>
  <trd lng="en">crescendo</trd>
  <trd lng="fr">crescendo</trd>
  <trd lng="ja">&#x30AF;&#x30EC;&#x30C3;&#x30B7;&#x30A7;&#x30F3;&#x30C9;</trd>
  <trd lng="nl">crescendo</trd>
  <trd lng="pl">crescendo <klr>(coraz g&lstroke;o&sacute;niej)</klr></trd>
  <trd lng="pt">crescendo</trd>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_shch;&c_je;&c_n;&c_d;&c_o;</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_r;&c_je;&c_s;&c_ch;&c_je;&#x43D;&c_d;&#x43E;</trd>,
    <trd>&#x43A;&c_r;&c_je;&c_shch;&c_je;&#x43D;&c_d;&#x43E;</trd>
  </trdgrp>
</drv>

<drv mrk="krescx.0i">
  <kap><tld/>i</kap>
  <snc mrk="krescx.0i.MUZ">
    <dif>
      Iom post iom la&ubreve;ti&gcirc;i:
      <ekz>
        &scirc;ia vo&ccirc;o <tld/>as
        <fnt>
          <aut>C. Rossetti</aut>,
          <vrk>Kredu Min, Sinjorino!</vrk>,
          <lok>1950</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_s;&c_t;&c_u;&c_p;&c_o;&c_v;&c_a; &c_w;&c_z;&c_m;&c_a;&c_c;&c_n;&c_ja;&c_c;&c_c;&c_a; <klr>(&c_a;&c_b; &c_g;&c_o;&c_l;&c_a;&c_s;&c_je;, &c_m;&c_e;&c_l;&c_jo;&c_d;&c_y;&c_ib;)</klr></trd>,
    <trd>&c_g;&c_u;&c_ch;&c_a;&c_c;&c_mol; <ind>&c_k;&c_r;&c_e;&c_sh;&c_ch;&c_e;&c_n;&c_d;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="nl">crescendo gaan</trd>
</drv>
</art>
