<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>6</ofc>
  <rad>susur</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="susur.0i">
  <kap><tld/>i</kap>
  <snc mrk="susur.0i.SON">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      <ref tip="super" cel="bru.0i.soni">Brui</ref> malforte kaj iom seke
      kiel ĉifata papero, silkaĵo, teksaĵo, arbofolioj kaj similaj:
      <ekz>
        facila venteto <tld/>as inter la olivarboj;
      </ekz>
      <ekz>
        la fontano <tld/>adis kun plaĉa murmureto;
      </ekz>
      <ekz>
        la longa trenaĵo de ŝia vesto <tld/>is sur la pargeto
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>шамацець</trd>,
    <trd>шалясьцець</trd>,
    <trd>шастаць</trd>,
    <trd>шапацець</trd>
  </trdgrp>
  <trd lng="cs">šustit</trd>
  <trdgrp lng="de">
    <trd>knistern</trd>,
    <trd>rauschen</trd>,
    <trd>rascheln</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>rustle</trd>,
    <trd>swish</trd>
  </trdgrp>
  <trd lng="es">susurrar</trd>
  <trd lng="fr">susurrer</trd>
  <trdgrp lng="hu">
    <trd>susog</trd>,
    <trd>suhog</trd>
    <trd>zizeg</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ritselen</trd>,
    <trd>ruisen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>шелестеть</trd>,
    <trd>шуршать</trd>
  </trdgrp>
</drv>

<drv mrk="susur.0o">
  <kap><tld/>o</kap>
  <snc mrk="susur.0o.SON">
    <dif>
      Mallaŭta frota bruo:
      <ekz>
        la <tld/>ado de la radoj, helicoj;
      </ekz>
      <ekz>
        kun facila <tld/>o la boato sursabliĝis
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>rustle</trd>,
    <trd>swish</trd>
  </trdgrp>
  <trd lng="es">susurro</trd>
  <trd lng="fr">susurrement</trd>
  <trdgrp lng="hu">
    <trd>susogás</trd>,
    <trd>suhogás</trd>
    <trd>zizegés</trd>
  </trdgrp>
</drv>
</art>
