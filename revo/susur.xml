<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>6</ofc>
  <rad>susur</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="susur.0i">
  <kap><tld/>i</kap>
  <snc mrk="susur.0i.SON">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      <ref tip="super" cel="bru.0i.soni">Brui</ref> malforte kaj iom seke
      kiel &ccirc;ifata papero, silka&jcirc;o, teksa&jcirc;o, arbofolioj kaj similaj:
      <ekz>
        facila venteto <tld/>as inter la olivarboj;
      </ekz>
      <ekz>
        la fontano <tld/>adis kun pla&ccirc;a murmureto;
      </ekz>
      <ekz>
        la longa trena&jcirc;o de &scirc;ia vesto <tld/>is sur la pargeto
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_a;&c_m;&c_a;&c_c;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_a;&c_l;&c_ja;&c_s;&c_mol;&c_c;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_a;&c_s;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_a;&c_p;&c_a;&c_c;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">&scaron;ustit</trd>
  <trdgrp lng="de">
    <trd>knistern</trd>,
    <trd>rauschen</trd>,
    <trd>rascheln</trd>
  </trdgrp>
  <trd lng="fr">susurrer</trd>
  <trdgrp lng="nl">
    <trd>ritselen</trd>,
    <trd>ruisen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_je;&c_l;&c_je;&c_s;&c_t;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_sh;&c_u;&c_r;&c_sh;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="susur.0o">
  <kap><tld/>o</kap>
  <snc mrk="susur.0o.SON">
    <dif>
      Malla&ubreve;ta frota bruo:
      <ekz>
        la <tld/>ado de la radoj, helicoj;
      </ekz>
      <ekz>
        kun facila <tld/>o la boato sursabli&gcirc;is
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">susurrement</trd>
</drv>
</art>
