<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: susur.xml,v 1.22 2018/08/07 07:10:20 revo Exp $">
<kap>
  <ofc>6</ofc>
  <rad>susur</rad>/i
  <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="susur.0i">
  <kap><tld/>i</kap>
  <snc mrk="susur.0i.SON">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      <ref tip="super" cel="bru.0i.soni">Brui</ref> malforte kaj iom
      seke
      kiel &ccirc;ifata papero, silka&jcirc;o, teksa&jcirc;o, arbofolioj kaj similaj:
      <ekz>
        facila venteto <tld/>as inter la olivarboj;
      </ekz>
      <ekz>
        la fontano <tld/>adis kun pla&ccirc;a murmureto;
      </ekz>
      <ekz>
        la longa trena&jcirc;o de &scirc;ia vesto <tld/>is sur la pargeto
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_a;&c_m;&c_a;&c_c;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_a;&c_l;&c_ja;&c_s;&c_mol;&c_c;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_a;&c_s;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_a;&c_p;&c_a;&c_c;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>&scaron;ustit</trd>,
    <trd>hara&scaron;it</trd>,
    <trd>&scaron;elestit</trd>,
    <trd>&scaron;um&ecaron;t</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>knistern</trd>,
    <trd>rauschen</trd>,
    <trd>rascheln</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>rustle</trd>,
    <trd>swish</trd>
  </trdgrp>
  <trd lng="es">susurrar (un objeto)</trd>
  <trd lng="fr">susurrer</trd>
  <trdgrp lng="hu">
    <trd>susog</trd>,
    <trd>suhog</trd>
    <trd>zizeg</trd>
  </trdgrp>
  <trd lng="it">mormorare</trd>
  <trdgrp lng="nl">
    <trd>ritselen</trd>,
    <trd>ruisen<klr tip="ind"> (werkw.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_je;&c_l;&c_je;&c_s;&c_t;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_sh;&c_u;&c_r;&c_sh;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>&scaron;uchota&tcaron;</trd>,
    <trd>&scaron;umie&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="susur.0o">
  <kap><tld/>o</kap>
  <snc mrk="susur.0o.SON">
    <dif>
      Malla&ubreve;ta frota bruo:
      <ekz>
        la <ind><tld/>ado</ind> de la radoj, helicoj;
        <trd lng="it">mormorio</trd>
      </ekz>
      <ekz>
        kun facila <tld/>o la boato sursabli&gcirc;is
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>&scaron;elest</trd>,
    <trd>&scaron;ustot</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>rustle</trd>,
    <trd>swish</trd>
  </trdgrp>
  <trd lng="es">susurro (ruidito)</trd>
  <trd lng="fr">susurrement</trd>
  <trdgrp lng="hu">
    <trd>susog&aacute;s</trd>,
    <trd>suhog&aacute;s</trd>
    <trd>zizeg&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>mormorazione</trd>, 
    <trd>brusio</trd>, <trd>sussurro<klr> (fig.)</klr></trd>
  </trdgrp>
  <trd lng="nl">ruisen<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="sk">
    <trd>&scaron;elest</trd>,
    <trd>&scaron;um</trd>
  </trdgrp>
</drv>
</art>
