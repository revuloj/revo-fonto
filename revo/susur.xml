<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: susur.xml,v 1.29 2024/05/24 21:25:15 revo Exp $">
<kap>
  <ofc>6</ofc>
  <rad>susur</rad>/i
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="susur.0i">
  <kap><tld/>i</kap>
  <snc mrk="susur.0i.SON">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      <ref tip="super" cel="bru.0i.soni">Brui</ref> malforte kaj iom
      seke kiel &ccirc;ifata papero, silka&jcirc;o, teksa&jcirc;o, arbofolioj kaj similaj:
      <ekz>
        kiel <tld/>as &scirc;ia multekosta silka vesto
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>!
      </ekz>
      <ekz>
        la libro sub la kapo <klr>[&#8230;]</klr> ek<tld/>is
        <fnt>
          <bib>Fab2</bib>
          <lok>Malgranda Tuk</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i premis en la mano la delikatan koverton, en kiu <tld/>is kelke da monaj
        paperetoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la longa trena&jcirc;o de &scirc;ia vesto <tld/>is sur la pargeto
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="krak.0eti">kraketi</ref>,
      <ref cel="murmur.0eti">murmureti</ref>,
      <ref cel="sibl.0i">sibli</ref>,
      <ref cel="sxusx.0i">&scirc;u&scirc;i</ref>,
      <ref cel="zum.0i">zumi</ref>
    </refgrp>
    <rim>
      Oni povas priskribi <tld/>adon kiel ia kombina&jcirc;o de delikataj mallongaj siblo-, &scirc;u&scirc;o- kaj
      kraket-sonoj.
      <aut>W. Diestel</aut>
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_a;&c_m;&c_a;&c_c;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_a;&c_l;&c_ja;&c_s;&c_mol;&c_c;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_a;&c_s;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_a;&c_p;&c_a;&c_c;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>&scaron;ustit</trd>,
    <trd>hara&scaron;it</trd>,
    <trd>&scaron;elestit</trd>,
    <trd>&scaron;um&ecaron;t</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>knistern</trd>,
    <trd>rauschen</trd>,
    <trd>rascheln</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>rustle</trd>,
    <trd>swish</trd>
  </trdgrp>
  <trd lng="es">susurrar (un objeto)</trd>
  <trd lng="fr">susurrer</trd>
  <trdgrp lng="hu">
    <trd>susog</trd>,
    <trd>suhog</trd>
    <trd>zizeg</trd>
  </trdgrp>
  <trd lng="it">mormorare</trd>
  <trdgrp lng="ja">
    <trd>&#12373;&#12425;&#12373;&#12425;&#x9CF4;&#x308B;  <pr>&#12373;&#12425;&#12373;&#12425;&#x306A;&#x308B;</pr></trd>,
    <trd>&#x304B;&#12373;&#x304B;&#12373;&#x97F3;&#12434;&#x7ACB;&#12390;&#x308B;
    <pr>&#x304B;&#12373;&#x304B;&#12373;&#x304A;&#12392;&#12434;&#x305F;&#12390;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ritselen</trd>,
    <trd>ruisen<klr tip="ind"> (werkw.)</klr></trd>
  </trdgrp>
  <trd lng="pl">szele&sacute;ci&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_je;&c_l;&c_je;&c_s;&c_t;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_sh;&c_u;&c_r;&c_sh;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>&scaron;uchota&tcaron;</trd>,
    <trd>&scaron;umie&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_sh;&c_je;&#x43B;&c_je;&c_s;&c_t;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_sh;&c_a;&c_r;&c_u;&c_d;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_d;&c_z;&c_i;&c_zh;&c_ch;&c_a;&c_t;&c_i; (&#x43F;&c_r;&#x43E; &#x43A;&#x43E;&#x43C;&c_a;&c_h;&c_u;)</trd>,
    <trd>&c_d;&c_z;&#x44E;&c_r;&c_ch;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4F4E;&#x8A9E; <pr>d&#x12B;y&#x1D4;</pr></trd>,
    <trd>&#x4F4E;&#x8BED; <pr>d&#x12B;y&#x1D4;</pr></trd>,
    <trd>&#x6C99;&#x6C99; <pr>sh&amacron;sh&amacron;</pr></trd>,
    <trd>&#22070;&#22070;&#x58F0; <pr>s&#x12B;s&#x12B;sh&emacron;ng</pr></trd>,
    <trd>&#22070;&#22070;&#32882; <pr>s&#x12B;s&#x12B;sh&emacron;ng</pr></trd>,
    <trd>&#x6DD9; <pr>c&#xF3;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="susur.0o">
  <kap><tld/>o, <var><kap><tld/>ado</kap></var></kap>
  <snc mrk="susur.0o.SON">
    <dif>
      Malla&ubreve;ta frota <ref tip="super" cel="bru.0o">bruo</ref>:
      <ekz>
        &scirc;i a&ubreve;dis post si <tld/>adon de atlaso
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[oni]</klr> a&ubreve;dis kantadon kaj muzikon, la <tld/>adon kaj zumadon de multe da homoj,
        precipe
        <klr>[&#8230;]</klr> de la &gcirc;ardeno
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la instruistino eka&ubreve;dis la gazet<tld/>on, ekinteresi&gcirc;is pri la afero
        <fnt>
          <bib>PGM</bib>
        </fnt>;
      </ekz>
      <ekz>
        molsona kiel venta <tld/>o inter herboj
        <fnt>
          <bib>HsT</bib>
        </fnt>;
      </ekz>
      <ekz>
        kun sono de kvieta arbara <tld/>o glitis la fal&ccirc;iloj tra rospeza herbo, kiu
        balda&ubreve; ku&scirc;is por seki&gcirc;o en lar&gcirc;aj strioj
        <fnt>
          <bib>HsT</bib>
        </fnt>;
      </ekz>
      <ekz>
        neniu sono estis a&ubreve;debla krom la <tld/>o de la sango en liaj oreloj
        <fnt>
          <bib>ViV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kun facila <tld/>o la boato sursabli&gcirc;is
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_o;&c_r;&c_g;&c_a;&c_t;</trd>,
    <trd>&c_sh;&c_o;&c_l;&c_a;&c_h;</trd>,
    <trd>&c_sh;&c_a;&c_m;&c_a;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_sh;&c_a;&c_s;&c_t;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>&scaron;elest</trd>,
    <trd>&scaron;ustot</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Rascheln</trd>,
    <trd>Knistern</trd>,
    <trd>Rauschen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>rustle</trd>,
    <trd>swish</trd>
  </trdgrp>
  <trd lng="es">susurro (ruidito)</trd>
  <trd lng="fr">susurrement</trd>
  <trdgrp lng="hu">
    <trd>susog&aacute;s</trd>,
    <trd>suhog&aacute;s</trd>
    <trd>zizeg&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>mormorazione</trd>,
    <trd>brusio</trd>, <trd>sussurro<klr> (fig.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12373;&#12425;&#12373;&#12425;&#x9CF4;&#x308B;&#x97F3;  <pr>&#12373;&#12425;&#12373;&#12425;&#x306A;&#x308B;&#x304A;&#12392;</pr></trd>,
    <trd>&#x304B;&#12373;&#12371;&#x305D;&#12392;&#12356;&#12358;&#x97F3;  <pr>&#x304B;&#12373;&#12371;&#x305D;&#12392;&#12356;&#12358;&#x304A;&#12392;</pr></trd>
  </trdgrp>
  <trd lng="nl">ruisen<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="pl">
    <trd>szelest</trd>,
    <trd>szmer</trd>,
    <trd>szum</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>&scaron;elest</trd>,
    <trd>&scaron;um</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_sh;&c_je;&#x43B;&c_je;&c_s;&c_t;</trd>,
    <trd>&c_sh;&c_je;&c_r;&c_je;&c_h;</trd>,
    <trd>&c_sh;&c_u;&c_r;&c_h;&c_ib;&c_t;</trd>,
    <trd>&c_sh;&c_a;&c_r;&c_u;&c_d;&c_ib;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_d;&c_z;&c_i;&c_zh;&c_ch;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_d;&c_z;&#x44E;&c_r;&#x43A;&c_ib;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6C99;&#x6C99; <pr>sh&amacron;sh&amacron;</pr></trd>,
    <trd>&#x4F4E;&#x8A9E; <pr>d&#x12B;y&#x1D4;</pr></trd>,
    <trd>&#x4F4E;&#x8BED; <pr>d&#x12B;y&#x1D4;</pr></trd>,
    <trd>&#x677E;&#x6D9B; <pr>s&#x14D;ngt&#xE1;o</pr></trd>,
    <trd>&#x677E;&#x6FE4; <pr>s&#x14D;ngt&#xE1;o</pr></trd>,
    <trd>&#x55D6; <pr>s&#x14D;u</pr></trd>,
    <trd>&#x55D6;&#x55D6; <pr>s&#x14D;us&#x14D;u</pr></trd>
  </trdgrp>
</drv>
</art>
