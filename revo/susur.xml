<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>6</ofc>
  <rad>susur</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="susur.0i">
  <kap><tld/>i</kap>
  <snc>
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Murmuri 1, flustri 2:
      <ekz>
        facila venteto <tld/>as inter la olivarboj;
      </ekz>
      <ekz>
        la fontano <tld/>adis kun pla&ccirc;a murmureto;
      </ekz>
      <ekz>
        la <tld/>ado de la radoj, helicoj.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>rauschen</trd>,
      <trd>rascheln</trd>
    </trdgrp>
  </snc>
  <snc>
    <gra><vspec>ntr</vspec></gra>
    <dif>
      A&ubreve;digi sonon malfortan kaj iom sekan de &ccirc;ifita papero, silka&jcirc;o,
      teksa&jcirc;o, arbofolio ks:
      <ekz>
        la silkaj vestoj <tld/>is<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">knistern</trd>
  </snc>
  <trd lng="cs">&scaron;ustit</trd>
  <trdgrp lng="nl">
    <trd>ritselen</trd>,
    <trd>ruisen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_je;&c_l;&c_je;&c_s;&c_t;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_sh;&c_u;&c_r;&c_sh;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
