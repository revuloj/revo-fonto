<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>magnoli</rad>/o
</kap>
<drv mrk="magnoli.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="magnoli1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Magnolia</trd>), arbo kun blankaj &gcirc;is ru&gcirc;aj
      tulipsimilaj floroj.
    </dif>
  </snc>
  <trd lng="en">magnolia</trd>
  <trd lng="nl">magnolia</trd>
  <trd lng="de">Magnolie</trd>
</drv>

<drv mrk="magnoli.0oplantoj">
  <kap><tld/>oplantoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Divizio de la <ref tip="super" cel="plant.0oj">plantoj</ref>,
      havantaj angiojn kaj reprodukti&gcirc;antaj per semgrajnoj,
      kiuj estas enfermitaj en ovario.
      Oni klasigas la <tld/>oplantojn en
      <ref tip="sub" cel="kotile.unu0aj">unukotiledonajn</ref> kaj
      <ref tip="sub" cel="kotile.du0aj">dukotiledonajn</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Bedecktsamige Pflanzen</trd>,
    <trd>Decksamer</trd>
  </trdgrp>
  <trdgrp lng="la">
    <trd>Magnoliophyta</trd>,
    <trd>Angiospermae <klr>(tradicie)</klr></trd>
  </trdgrp>
</drv>
</art>
