<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: magnoli.xml,v 1.29 2022/04/29 17:34:27 revo Exp $">
<kap>
  <rad>magnoli</rad>/o
</kap>
<drv mrk="magnoli.0o">
  <kap><tld/>o</kap>
  <snc mrk="magnoli.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="magnoli1.0oj"><tld/>acoj</ref>, arbo kun blankaj &gcirc;is ru&gcirc;aj
      tulipsimilaj floroj (<trd lng="la">Magnolia</trd>):
      <ekz>
        tie estis granda &gcirc;ardeno kun kelkaj <tld/>oj
        <fnt><bib>LaFamilio</bib>, <lok>&ccirc;apitro 14a, p.
        165-166a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_g;&c_n;&c_o;&c_l;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_m;&c_a;&c_g;&c_n;&c_o;&c_l;&c_i;&c_ja;</trd>
  <trd lng="ca">magn&ograve;lia</trd>
  <trd lng="cs">magnolie</trd>
  <trd lng="de">Magnolie</trd>
  <trd lng="en">magnolia</trd>
  <trd lng="fa">&a_m;&a_A;&f_g;&a_n;&a_w;&a_l;&f_y;&a_A;</trd>
  <trd lng="fr">magnolia</trd>
  <trdgrp lng="hu">
    <trd>liliomfa</trd>,
    <trd>many&oacute;lia</trd>,
    <trd>magn&oacute;lia</trd>
  </trdgrp>
  <trd lng="nl">magnolia</trd>
  <trd lng="pl">magnolia</trd>
  <trd lng="pt">magn&oacute;lia</trd>
  <trd lng="ro">magnolia</trd>
  <trd lng="ru">&c_m;&c_a;&c_g;&c_n;&c_o;&c_l;&c_i;&c_ja;</trd>
  <trd lng="sk">magn&oacute;lia</trd>
  <trd lng="uk">&#x43C;&c_a;&c_gu;&#x43D;&#x43E;&#x43B;&c_ib;&#x44F;</trd>
</drv>

<drv mrk="magnoli.0oj">
  <kap><tld/>oj</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="klas.0o.BIO" lst="voko:botanikaj_klasoj">Klaso</ref> de la <ref
      tip="malprt" cel="flor.0oplant">floroplantoj</ref>,
      havantaj angiojn kaj reprodukti&gcirc;antaj per semgrajnoj,
      kiuj estas enfermitaj en ovario;
      oni klasigas la <tld/>ojn en
      <ref tip="sub" cel="kotile.unu0aj">unukotiledonajn</ref> kaj
      <ref tip="sub" cel="kotile.du0aj">dukotiledonajn</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_v;&c_je;&c_t;&c_k;&c_a;&c_v;&c_y;&c_ja; &c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_y;</trd>,
    <trd>&c_ja;&c_w;&c_n;&c_a;&c_s;&c_je;&c_m;&c_n;&c_y;&c_ja; &c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>magnoliofitins</trd>,
    <trd>angiospermes</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Decksamer</trd>,
    <trd>Bedecktsamer</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Magnoliidae</trd>,
    <trd>Magnoliid&eacute;es</trd>
  </trdgrp>
  <trd lng="hu">liliomf&aacute;k</trd>
  <trdgrp lng="la">
    <trd>Magnoliophyta</trd>,
    <trd>Angiospermae <klr>(tradicie)</klr></trd>
  </trdgrp>
  <trd lng="pl">magnoliowce</trd>
  <trd lng="ro">Magnoliales</trd>
  <trdgrp lng="ru">
    <trd>&c_c;&c_v;&c_je;&c_t;&c_k;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_o;&c_t;&c_d;&c_je;&c_l;
    &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_j;)</klr></trd>,
    <trd>&c_p;&c_o;&c_k;&c_r;&c_y;&c_t;&c_o;&c_s;&c_je;&c_m;&c_je;&c_n;&c_n;&c_y;&c_je; <klr>(tradicie)</klr></trd>
  </trdgrp>
</drv>
</art>
