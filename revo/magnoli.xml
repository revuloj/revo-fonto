<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>magnoli</rad>/o
</kap>
<drv mrk="magnoli.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="magnoli.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Magnolia</trd>), arbo kun blankaj &gcirc;is ru&gcirc;aj
      tulipsimilaj floroj.
    </dif>
  </snc>
  <trd lng="en">magnolia</trd>
  <trd lng="de">Magnolie</trd>
</drv>
<drv mrk="magnoli.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Magnoliaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Magnoliengew&auml;chse</trd>
</drv>
<drv mrk="magnoli.0oplantoj">
  <kap><tld/>oplantoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Grupo de <ref tip="super" cel="flor.0oplantoj">floroplantoj</ref>
      (<trd lng="la">Magnoliophyta</trd>), kies semgrajnojn
      &ccirc;irka&ubreve;as ujo el fruktofolioj, en kiu la
      semgrajnoj maturi&gcirc;as. Oni dividas la <tld/>oplantojn en
      <ref tip="sub" cel="kotile.unu0aj">unukotiledonajn</ref> kaj
      <ref tip="sub" cel="kotile.du0aj">dukotiledonajn</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Bedecktsamige Pflanzen</trd>,
    <trd>Decksamer</trd>
  </trdgrp>     
</drv>
</art>
