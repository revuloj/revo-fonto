<vortaro>
<art mrk="$Id: rojali.xml,v 1.16 2019/07/18 20:10:17 revo Exp $">
<kap>
  <rad>rojalism</rad>/o
  <fnt>Z</fnt>
</kap>

<drv mrk="rojali.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POL</uzo>
  <uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="monarh.0ismo">Monar&hcirc;ismo</ref>
      <klr>(en formo rilata al re&gcirc;a re&gcirc;imo)</klr>:
      <ekz>
        <nom>Han Fei</nom> firme oponis kontra&ubreve; <tld/>o kaj resta&ubreve;rismo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=57367"
              >Han Fei</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_ja;&c_l;&c_ib;&c_z;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>monarchizmus</trd>,
    <trd>roajalizmus</trd>,
    <trd>royalizmus</trd>
  </trdgrp>
  <trd lng="de">Royalismus</trd>
  <trd lng="en">royalism</trd>
  <trd lng="fr">royalisme</trd>
  <trdgrp lng="hu">
    <trd>kir&aacute;lyp&aacute;rtis&aacute;g</trd>,
    <trd>royalizmus</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>royalisme</trd>,
    <trd>koningsgezindheid</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>realismo</trd>,
    <trd>monarquismo</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_o;&c_ja;&c_l;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">royalizmus</trd>
</drv>
