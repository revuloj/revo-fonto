<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>rojalism</rad>/o
      <fnt>Z</fnt>
    </kap>

    <drv mrk="rojali.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">POL</uzo>
      <uzo tip="fak">HIS</uzo>
      <snc>
        <dif>
          <ref tip="super" cel="monarhxi.0ismo">Monar&hcirc;ismo</ref>
          <klr>(en formo rilata al re&gcirc;a re&gcirc;imo)</klr>.
        </dif>
      </snc>
      <trd lng="de">Royalismus</trd>
      <trd lng="en">royalism</trd>
      <trdgrp lng="nl">
        <trd>royalisme</trd>,
        <trd>koningsgezindheid</trd>
      </trdgrp>
      <trd lng="ru">&c_r;&c_o;&c_ja;&c_l;&c_i;&c_z;&c_m;</trd>
    </drv>
  </art>
