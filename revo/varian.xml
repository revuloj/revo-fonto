<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: varian.xml,v 1.16 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>variant</rad>/o
</kap>
<drv mrk="varian.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TIP</uzo>
  <snc>
    <dif>
      Iomete diferenca
      <ref tip="super" cel="redakc.0o.versio">redakcio</ref>
      de teksto, liverita de alia manuskripto a&ubreve; eldono:
      <ekz>
        kolekti la <tld/>ojn de poemo en kritikan aparaton.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_r;&c_y;&c_ja;&c_n;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>biol. odchylka</trd>,
    <trd>mat.</trd>,
    <trd>nevh. proměnná</trd>,
    <trd>obměna</trd>,
    <trd>varianta</trd>
  </trdgrp>
  <trd lng="de">Variante</trd>
  <trd lng="el">&pi;&alfa;&rho;&alfa;&lambda;&lambda;&alfa;&gamma;&eta_ton;</trd>
  <trdgrp lng="en">
    <trd>variant</trd>,
    <trd>different form</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>lectura<klr> (de un manuscrito)</klr></trd>,
    <trd>variante<klr> (textual)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>le&ccedil;on<klr> (d'un manuscrit)</klr></trd>,
    <trd>variante<klr> (textuelle)</klr></trd>
  </trdgrp>
  <trd lng="hu">sz&ouml;vegv&aacute;ltozat</trd>
  <trd lng="nl">variant</trd>
  <trdgrp lng="pl">
    <trd>wariant</trd>,
    <trd>odmiana</trd>
  </trdgrp>
  <trd lng="pt">variante</trd>
  <trd lng="ru">&c_v;&c_a;&c_r;&c_i;&c_a;&c_n;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>obmena</trd>,
    <trd>odchýlka</trd>,
    <trd>variant</trd>
  </trdgrp>
</drv>
</art>
