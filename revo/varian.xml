<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>variant</rad>/o
</kap>
<drv mrk="varian.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TIP</uzo>
  <snc>
    <dif>
      Iomete diferenca
      <ref tip="super" cel="redakc.0o.versio">redakcio</ref>
      de teksto, liverita de alia manuskripto a&ubreve; eldono:
      <ekz>
        kolekti la <tld/>ojn de poemo en kritikan aparaton.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Variante</trd>
  <trdgrp lng="es">
    <trd>lectura<klr> (de un manuscrito)</klr></trd>,
    <trd>variante<klr> (textual)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>le&ccedil;on<klr> (d'un manuscrit)</klr></trd>,
    <trd>variante<klr> (textuelle)</klr></trd>
  </trdgrp>
  <trd lng="nl">variant</trd>
  <trd lng="ru">&c_v;&c_a;&c_r;&c_i;&c_a;&c_n;&c_t;</trd>
</drv>
</art>
