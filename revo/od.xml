<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>od</rad>/o
</kap>
<drv mrk="od.0o">
  <kap><tld/>o</kap>
  <snc mrk="od.0o.POE">
    <uzo tip="fak">POE</uzo>
    <dif>
      Lirika poemo, dividita en strofoj, ordinare similaj inter si.
    </dif>
  </snc>
  <trd lng="be">&c_o;&c_d;&c_a;</trd>
  <trd lng="bg">&c_o;&c_d;&c_a;</trd>
  <trd lng="de">Ode</trd>
  <trd lng="el">&omega;&delta;&eta_ton;</trd>
  <trd lng="en">ode</trd>
  <trdgrp lng="fa">
    <trd>&a_q;&a_S;&f_y;&a_d;&a_h;</trd>,
    <trd>&f_ch;&f_k;&a_A;&a_m;&a_h;</trd>,
    <trd>&a_sh;&a_ayn;&a_r; &a_t;&a_gh;&a_z;&a_l;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">ode</trd>
  <trd lng="hu">&oacute;da</trd>
  <trd lng="nl">ode</trd>
  <trd lng="pl">oda</trd>
  <trd lng="pt">ode</trd>
  <trdgrp lng="sv">
    <trd>ode</trd>,
    <trd>kv&auml;de</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_d;&c_a;</trd>
</drv>
</art>
