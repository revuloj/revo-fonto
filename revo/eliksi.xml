<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: eliksi.xml,v 1.22 2023/08/31 11:49:28 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>eliksir</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="eliksi.0o">
  <kap><tld/>o</kap>
  <snc mrk="eliksi.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Kuracilo en formo de alkohola solva&jcirc;o.
    </dif>
    <trd lng="hu">elix&iacute;r</trd>
  </snc>
  <snc mrk="eliksi.0o.KOMUNE">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ia <ref tip="lst" lst="voko:trinka&jcirc;oj" cel="trink.0ajxo">trinka&jcirc;o</ref> posedanta miraklajn ecojn:
      <ekz>
        <tld/>o de la juneco;
      </ekz>
      <ekz>
        la aero de la mar&ccirc;o, la libereco de propra hejmo,
        la matenru&gcirc;o de feli&ccirc;o de la sendependeco:
        &ccirc;io tio efikis je Diana kaj Maria kiel viviganta
        <tld/>o
        <fnt>
          <aut>C. Bront&euml;, trad. H. J. Bulthuis</aut>,
          <vrk>Jane Eyre</vrk>,
          <lok>1931</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fa">&a_A;&f_k;&a_s;&f_y;&a_r;</trd>
    <trd lng="pl">eliksir</trd>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_ib;&c_k;&c_s;&c_ib;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>roztok v alkoholu</trd>,
    <trd>tekut&yacute; l&eacute;k</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Elixier</trd>,
     <trd>Heiltrank</trd>,
     <trd>alchemistischer <ind>Zaubertrank</ind></trd>
  </trdgrp>
  <trd lng="en">elixir</trd>
  <trd lng="es">el&iacute;xir</trd>
  <trd lng="fr">&eacute;lixir</trd>
  <trdgrp lng="hu">
     <trd>csodaszer</trd>,
     <trd>elix&iacute;r</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30A8;&#x30EA;&#x30AD;&#x30B7;&#x30EB;&#x5264;  [<ind>&#x30A8;&#x30EA;&#x30AD;&#x30B7;&#x30EB;&#x3056;&#x3044;</ind>]</trd>,
    <trd>&#x30A8;&#x30EA;&#x30AF;&#x30B7;&#x30EB;</trd>,
    <trd>&#x970A;&#x85AC;  [<ind>&#x308C;&#x3044;&#x3084;&#x304F;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">elixir</trd>
  <trd lng="pt">elixir</trd>
  <trd lng="ru">&c_e;&c_l;&c_i;&c_k;&c_s;&c_i;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>elix&iacute;r</trd>,
    <trd>z&aacute;zra&ccaron;n&yacute; n&aacute;poj</trd>
  </trdgrp>
  <trd lng="uk">&#x435;&#x43B;&#x456;&#x43A;&#x441;&#x438;&#x440;</trd>
</drv>
</art>
