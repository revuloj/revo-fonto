<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <ofc>1</ofc>
    <rad>eliksir</rad>/o
  </kap>
  <drv mrk="eliksi.0o">
    <kap><tld/>o</kap>
    <snc num="1">
      <uzo tip="fak">MED</uzo>
      <dif>
        Kuracilo en formo de alkohola solva&jcirc;o.
      </dif>
      <trd lng="hu">elix&iacute;r</trd>
    </snc>
    <snc num="2">
      <uzo tip="stl">FIG</uzo><dif>
        Solva&jcirc;o posedanta miraklajn ecojn: <ekz><tld/>o de la
        juneco.</ekz>
      </dif>
    </snc>
    <trd lng="be">&c_e;&c_l;&c_ib;&c_k;&c_s;&c_ib;&c_r;</trd>
    <trdgrp lng="de">
       <trd>Elixier</trd>,
       <trd>Heiltrank</trd>,
       <trd>alchemistischer <ind>Zaubertrank</ind></trd>
    </trdgrp>
    <trd lng="en">elixir</trd>
    <trd lng="es">el&iacute;xir</trd>
    <trd lng="fr">&eacute;lixir</trd>
    <trdgrp lng="hu">
       <trd>csodaszer</trd>,
       <trd>elix&iacute;r</trd>
    </trdgrp>
    <trd lng="nl">elixir</trd>
    <trd lng="pt">elixir</trd>
    <trd lng="ru">&c_e;&c_l;&c_i;&c_k;&c_s;&c_i;&c_r;</trd>
  </drv>
</art>
