<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>stakat</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="stakat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Vo&ccirc;a a&ubreve; muzikila sonigo,
      kiu disigas la sinsekvajn tonojn per abruptaj ekoj
      a&ubreve; e&ccirc; pa&ubreve;zetoj.
    </dif>
  </snc>
  <refgrp tip="ant">
    <ref cel="legat1.0o">legato</ref>
  </refgrp>
  <trdgrp lng="fr">
    <trd>staccato</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>staccato</trd>
  </trdgrp>
</drv>

<drv mrk="stakat.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Mallige, disigante la sinsekvajn tonojn.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>staccato</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>staccato</trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
-->
</vortaro>