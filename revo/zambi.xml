<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: zambi.xml,v 1.14 2022/03/24 15:59:19 revo Exp $">
<kap>
  <rad>Zambi</rad>/o
  <fnt><bib>RLN</bib></fnt>
</kap>

<drv mrk="zambi.Z0o">
  <kap>
    <tld lit="Z"/>o
  </kap>
  <mlg kod="ISO-3166">ZM</mlg>
  <snc mrk="zambi.Z0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o.POL">&Scirc;tato</ref>
      en la suda <ref tip="malprt" cel="afrik.A0o">Afriko</ref> 
      kun &ccirc;efurbo Lusako; teritorio de Respubliko <tld/>o,
      limanta kun
      <ref tip="vid" cel="angol.A0o.GEOG">Angolo</ref>,
      <ref tip="vid" cel="bocvan.B0o.GEOG">Bocvano</ref>,
      <ref tip="vid" cel="kong.K0oKinsxasa.GEOG">Kongo
      Kin&scirc;asa</ref>,
      <ref tip="vid" cel="malavi.M0o.GEOG">Malavio</ref>,
      <ref tip="vid" cel="mozambik.M0o.GEOG">Mozambiko</ref>,
      <ref tip="vid" cel="namibi.N0o.GEOG">Namibio</ref>,
      <ref tip="vid" cel="tanzani.T0o.GEOG">Tanzanio</ref>,
      kaj <ref tip="vid" cel="zimbabv.Z0o.GEOG">Zimbabvo</ref>;
      iama Norda <ref cel="rodezi.0o">Rodezio</ref>:
      <ekz>
        Zimbabvo havis sangan historion, same kiel aliaj sud-afrikaj landoj, kiel
        <tld/>o, Mozambiko, Angolo kaj Sud-Afriko
        <fnt>
          <bib>Monato</bib>
          <aut>Princo Henriko Oguinye</aut>
          <vrk><url ref="&Monato;/2005/008764.php">Dronanta diktatoro</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        fari&gcirc;is <nom>Mugabe</nom> la &ccirc;efa batalanto por vere sendependa Suda Rodezio, kaj, kiam en
        1964 eksa Norda Rodezio sendependi&gcirc;is kaj fari&gcirc;is <tld/>o sub gvidado de
        <nom>Kaunda</nom>, kreskis la internacia premo kontra&ubreve; <nom>Smith</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>Princo Henriko Oguinye</aut>
          <vrk><url ref="&Monato;/2005/008764.php">Dronanta diktatoro</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    </snc>
    <trd lng="be">&c_Z;&c_a;&c_m;&c_b;&c_ib;&c_ja;</trd>
    <trd lng="bg">&c_Z;&c_a;&c_m;&c_b;&c_i;&c_ja;</trd>
    <trdgrp lng="ca">
      <trd>Z&agrave;mbia</trd>,
      <trd>Rep&uacute;blica de Z&agrave;mbia</trd>
    </trdgrp>
    <trd lng="de">Sambia</trd>
    <trd lng="en">Zambia</trd>
    <trdgrp lng="fr">
  <trd>Zambie</trd>,
  <trd>R&eacute;publique de Zambie</trd>
</trdgrp>
    <trd lng="hu">Zambia</trd>
    <trd lng="id">Zambia</trd>
    <trd lng="it">Zambia</trd>
    <trdgrp lng="pl">
      <trd>Zambia</trd>,
      <trd>Republika <ind>Zambii</ind></trd>
    </trdgrp>
    <trd lng="pt">Z&acirc;mbia</trd>
  <trd lng="uk">&#x417;&#x430;&#x43C;&#x431;&#x456;&#x44F;</trd>
</drv>

</art>

