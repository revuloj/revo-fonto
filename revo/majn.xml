<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: majn.xml,v 1.24 2024/09/24 08:48:13 revo Exp $">
<kap><ofc>10</ofc>
  <rad>Majn</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="majn.M0o">
  <kap><tld/>o</kap>
  <snc mrk="majn.M0o.GEOG_rivero_fr">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Rivero en
      <refgrp tip="vid">
        <ref cel="franc.F0ujo">Francujo</ref>.
      </refgrp>
    </dif>
    <trd lng="be">&c_M;&c_e;&c_n; <klr tip="amb">(&c_r;&c_a;&c_k;&c_a; &c_w; &c_F;&c_r;&c_a;&c_n;&c_c;&c_y;&c_ib;)</klr></trd>
    <trd lng="ca">Maine</trd>
    <trd lng="de">Maine</trd>
    <trd lng="fr">Maine</trd>
    <trd lng="hu">Maine</trd>
    <trd lng="nl">Maine</trd>
    <trd lng="pl">Maine</trd>
    <trd lng="pt">Maine</trd>
    <trd lng="ro">Maine</trd>
    <trdgrp lng="ru">
      <trd>&c_M;&c_e;&c_n; <klr tip="amb">(&c_r;&c_je;&c_k;&c_a;)</klr></trd>,
      <trd>&c_M;&c_je;&c_n;</trd>,
      <trd>&c_M;&c_je;&c_j;&c_n;</trd>
    </trdgrp>
  </snc>
  <snc mrk="majn.M0o.GEOG_rivero_de">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Rivero en
      <refgrp tip="vid">
        <ref cel="german1.G0ujo">Germanujo</ref>
      </refgrp>
      enfluanta &ccirc;e <ref tip="vid" cel="majenc.M0o">Majenco</ref> en <ref tip="vid" cel="rejn.R0o">Rejnon</ref>.
    </dif>
    <trd lng="be">&c_M;&c_a;&c_j;&c_n; <klr tip="amb">(&c_r;&c_a;&c_k;&c_a; &c_w; &c_N;&c_ja;&c_m;&c_je;&c_ch;&c_ch;&c_y;&c_n;&c_je;)</klr></trd>
    <trd lng="ca">Main</trd>
    <trd lng="de">Main</trd>
    <trd lng="hu">Majna</trd>
    <trd lng="lat">Moenus</trd>
    <trd lng="nl">Main</trd>
    <trd lng="pl">Men</trd>
    <trd lng="pt">Main</trd>
    <trd lng="ro">Main</trd>
    <trd lng="ru">&c_M;&c_a;&c_j;&c_n;&c_a; <klr tip="amb">(&c_r;&c_je;&c_k;&c_a;)</klr></trd>
    <trd lng="zh">&#x7F8E;&#x56E0;&#x6CB3; <pr>m&#x11B;iy&#x12B;nh&#xE9;</pr></trd>
</snc>
  <snc mrk="majn.M0o.GEOG_sxtato">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Sub&scirc;tato en la nordorienta parto de 
      <ref tip="malprt" cel="uson.U0o.GEOG">Usono</ref>:
      <ekz>
        nun restas nur 4 <ctl>Shakers</ctl> en la tuta 
        mondo, lo&gcirc;antaj en suda <tld/>o
        <fnt>
          <aut>K. Miner</aut>,
          <vrk><url ref="http://www.sunflower.com/~miner/TAGLIBRO_package/Auxgusto_2006.html">
          taglibro</url></vrk>,
          <lok>2006-08-01</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_M;&c_e;&c_n; <klr tip="amb">(&c_sh;&c_t;&c_a;&c_t; &c_Z;&c_Sh;&c_A;)</klr></trd>
    <trd lng="ca">Maine</trd>
    <trd lng="de">Maine</trd>
    <trd lng="en">Maine</trd>
    <trd lng="fr">Maine</trd>
    <trd lng="hu">Maine</trd>
    <trd lng="nl">Maine</trd>
    <trd lng="pl">Maine</trd>
    <trd lng="pt">Maine</trd>
    <trd lng="ro">Maine</trd>
    <trd lng="ru">&c_M;&c_e;&c_n; <klr tip="amb">(&c_sh;&c_t;&c_a;&c_t;)</klr></trd>
  </snc>
  <trd lng="cs">Maine</trd>
  <trd lng="ja">&#x30E1;&#x30A4;&#x30F3;&#x5DDE;  <pr>&#x30E1;&#x30A4;&#x30F3;&#x3057;&#x3085;&#x3046;</pr></trd>
  <trd lng="sk">Maine</trd>
  <trdgrp lng="uk">
    <trd>&#x41C;&#x430;&#x439;&#x43D;</trd>,
    <trd>&#x41C;&#x435;&#x43D;</trd>
  </trdgrp>
</drv>
</art>
