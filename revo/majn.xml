<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>Majn</rad>/o
</kap>
<drv mrk="majn.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc num="1">
    <dif>
     Nomo de rivero en <refgrp tip="vid"><ref
cel="franc.F0ujo">Francujo</ref>.</refgrp>
    </dif>
    <trd lng="fr">Maine</trd>
    <trd lng="nl">Maine</trd>
  </snc>
  <snc num="2">
    <dif>
     Nomo de rivero en <refgrp tip="vid"><ref
cel="german1.G0ujo">Germanujo</ref>.</refgrp>
    </dif>
       <trd lng="de">Main</trd>
       <trd lng="nl">Main</trd>
   </snc>
  <snc num="3">
    <dif>
     Unu el la &scirc;tatoj de <refgrp
tip="vid"><ref cel="uson.0o">Usono</ref> en la norda oriento.</refgrp>
Regurbo estas A&ubreve;gusto.
    </dif>
       <trd lng="de">Maine</trd>
       <trd lng="en">Maine</trd>
       <trd lng="fr">Maine</trd>
       <trd lng="nl">Maine</trd>
</snc>
</drv>
</art>

