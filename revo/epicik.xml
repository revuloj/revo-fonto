<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>epicikloid</rad>/o
</kap>
<drv mrk="epicik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 35</lok></fnt>
  <snc>
    <dif>
      Ebena <ref tip="super" cel="kurb.0o.MAT">kurbo</ref>, naskita de punkto 
      apartenanta al cirklo, kiu ruli&gcirc;as sur la ekstera periferio de 
      alia cirklo.
    </dif>
    <refgrp tip="vid">
      <ref cel="cikloi.0o">cikloido</ref>,
      <ref cel="hipoci.0o">hipocikloido</ref>.
    </refgrp>
  </snc>
  <bld tip="svg" lok="../bld/k_epicikloido.svg" alt="300" lrg="600">
    <tld/>oj kun diversaj valoroj de la rilato inter la du radiusoj
  </bld>
  <trd lng="de">Epizykloide</trd>
  <trd lng="en">epicycloid</trd>
  <trd lng="fr">&eacute;picyclo&iuml;de</trd>
  <trd lng="hu">epicikloid</trd>
  <trd lng="pl">epicykloida</trd>
  <trd lng="ru">&c_e;&c_p;&c_i;&c_c;&c_i;&c_k;&c_l;&c_o;&c_i;&c_d;&c_a;</trd>
</drv>
</art>
