<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>epicikloid</rad>/o
</kap>
<drv mrk="epicik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 35</lok></fnt>
  <snc>
    <dif>
      Ebena <ref tip="super" cel="kurb.0o.MAT">kurbo</ref>, naskita de punkto 
      apartenanta al cirklo, kiu ruli&gcirc;as sur la ekstera periferio de 
      alia cirklo.
    </dif>
    <refgrp tip="vid">
      <ref cel="epicik.longigita0o">longigita <tld/>o</ref>,
      <ref cel="epicik.mallongigita0o">mallongigita <tld/>o</ref>;
      <ref cel="cikloi.0o">cikloido</ref>,
      <ref cel="hipoci.0o">hipocikloido</ref>.
    </refgrp>
  </snc>
  <bld tip="svg" lok="../bld/k_epicikloido.svg" alt="300" lrg="600">
    <tld/>oj kun diversaj valoroj de la rilato inter la du radiusoj
  </bld>
  <trd lng="be">&c_e;&c_p;&c_ib;&c_c;&c_y;&c_k;&c_l;&c_jo;&c_ib;&c_d;&c_a;</trd>
  <trd lng="de">Epizykloide</trd>
  <trd lng="en">epicycloid</trd>
  <trd lng="fr">&eacute;picyclo&iuml;de</trd>
  <trd lng="hu">epicikloid</trd>
  <trd lng="pl">epicykloida</trd>
  <trd lng="pt">epicicl&#243;ide</trd>
  <trd lng="ru">&c_e;&c_p;&c_i;&c_c;&c_i;&c_k;&c_l;&c_o;&c_i;&c_d;&c_a;</trd>
</drv>

<drv mrk="epicik.longigita0o">
  <kap>longigita <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      Ebena <ref tip="super" cel="kurb.0o.MAT">kurbo</ref>, naskita de
      punkto ligita al <ref cel="cirkl.0o.linio">cirklo</ref> kaj ekstera
      al &gcirc;i, kiu cirklo ruli&gcirc;as sur la ekstera periferio de 
      alia cirklo.
    </dif>
    <ref tip="super" cel="epitro.0o">epitro&hcirc;oido.</ref>
  </snc>
  <bld tip="svg" lok="../bld/k_epitrohxoido.svg" alt="300" lrg="600">
    longigita kaj mallongigita <tld/>oj
  </bld>
  <trd lng="de">verl&auml;ngerte <ind>Epizykloide</ind></trd>
  <trd lng="en">prolate <ind>epicycloid</ind></trd>
  <trd lng="fr">&eacute;picyclo&iuml;de allong&eacute;e</trd>
  <trd lng="pl">epicykloida wyd&lstroke;u&zdot;ona</trd>
  <trd lng="ru">&c_u;&c_d;&c_l;&c_i;&c_n;&c_jo;&c_n;&c_n;&c_a;&c_ja; <ind>&c_e;&c_p;&c_i;&c_c;&c_i;&c_k;&c_l;&c_o;&c_i;&c_d;&c_a;</ind></trd>
</drv>

<drv mrk="epicik.mallongigita0o">
  <kap>mallongigita <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      Ebena <ref tip="super" cel="kurb.0o.MAT">kurbo</ref>, naskita de
      punkto ligita al <ref cel="cirkl.0o.linio">cirklo</ref> kaj interna
      al &gcirc;i, kiu cirklo ruli&gcirc;as sur la ekstera periferio de 
      alia cirklo.
    </dif>
    <ref tip="super" cel="epitro.0o">epitro&hcirc;oido.</ref>
    <rim>
      Ekzistas anka&ubreve; la formo <ctl>kurtigita <tld/>o</ctl>, ekz-e
      en <fnt><bib>MatVort</bib></fnt>.
    </rim>
  </snc>
  <trd lng="de">verk&uuml;rzte <ind>Epizykloide</ind></trd>
  <trd lng="en">curtate <ind>epicycloid</ind></trd>
  <trd lng="fr">&eacute;picyclo&iuml;de raccourcie</trd>
  <trd lng="pl">epicykloida skr&oacute;cona</trd>
  <trd lng="ru">&c_u;&c_k;&c_o;&c_r;&c_o;&c_ch;&c_je;&c_n;&c_n;&c_a;&c_ja; <ind>&c_e;&c_p;&c_i;&c_c;&c_i;&c_k;&c_l;&c_o;&c_i;&c_d;&c_a;</ind></trd>
</drv>
</art>
