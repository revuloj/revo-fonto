<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>talent</rad>/o
</kap>
<drv mrk="talent.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="talent.0o.kapablo">
    <dif>
      Eminenta aparta spirita kapablo, &ccirc;u donacita de naturo,
      &ccirc;u akirita per laboro:
      <ekz>
        ni &ccirc;iuj havas malsamajn <tld/>ojn kaj ta&ubreve;gas por
        malsamaj laboroj
        <fnt>
        <aut>Bo Sandelin</aut>,
        <vrk>Anta&ubreve;merkantilisma ekonomia pensado</vrk>,
        <bib>Monato</bib>, <lok>jaro 2001a, numero 2a, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="geni.0o.intelekto">genio <sncref/></ref>,
      <ref cel="kapabl.0eco">kapableco</ref>,
      <ref cel="kompet.0eco">kompetenteco</ref>
    </refgrp>
    <trd lng="be">&c_t;&c_a;&c_l;&c_je;&c_n;&c_t;</trd>
    <trd lng="fa">&a_A;&a_s;&a_t;&a_ayn;&a_d;&a_A;&a_d;</trd>
    <trd lng="nl">talent</trd>
    <trdgrp lng="hu">
      <trd>tehets&eacute;g</trd>,
      <trd>talentum</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>talent</trd>,
      <trd>uzdolnienie</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>beg&aring;vning</trd>,
      <trd>talang</trd>
    </trdgrp>
  </snc>
  <snc mrk="talent.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="stl">EVI</uzo>
    <ref tip="dif" cel="talant.0o">talanto</ref>
    <trd lng="be">&c_t;&c_a;&c_l;&c_a;&c_n;&c_t;</trd>
    <trd lng="hu">talentum</trd>
  </snc>
  <trd lng="ca">talent</trd>
  <trd lng="de">Talent</trd>
  <trd lng="fr">talent</trd>
  <trdgrp lng="pt">
   <trd>talento</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_l;&c_a;&c_n;&c_t;</trd>
</drv>

<drv mrk="talent.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Montranta <tld/>on por iu tasko, pri iu fako:
      <ekz>
        pli bone konata sub la pse&ubreve;donimo Lewis G. Gibbon, li
        restis en la konscio de nia jarcento kiel la plej <tld/>a skota
        prozverkisto
        <fnt>
        <aut>Nicolae Macovei</aut>,
        <vrk>Arkitekto de la platona respubliko (J. L.
        Mitchell/Spartako)</vrk>,
        <bib>Monato</bib>, <lok>jaro 1995a, numero 8a, p. 23a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">talent&oacute;s</trd>
  <trd lng="de">talentiert</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_A;&a_A;&a_s;&a_t;&a_ayn;&a_d;&a_A;&a_d;</trd>,
    <trd>&a_m;&a_s;&a_t;&a_ayn;&a_d;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>dou&eacute;</trd>,
    <trd>talentueux</trd>
  </trdgrp>
  <trd lng="hu">tehets&eacute;ges</trd>
  <trdgrp lng="pl">
    <trd>utalentowany</trd>,
    <trd>uzdolniony</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_l;&c_a;&c_n;&c_t;&c_l;&c_i;&c_v;&c_y;&c_j;
  </trd>
</drv>

<drv mrk="talent.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Persono kun granda kapablo pri iu arto:
      <ekz>
        rimarkinde interesa romano de rimarkinda
        verk<tld/>ulo
        <fnt>
        <aut>Donald Broadribb</aut>,
        <vrk>Inter la dek plej bonaj romanoj (Ser&gcirc;o Elgo,
        Surklifa)</vrk>,
        <bib>Monato</bib>, <lok>jaro 2000a, numero 11a, p. 26a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kompet.0ulo">kompetentulo</ref>,
      <ref cel="virtuo.0o">virtuozo</ref>
    </refgrp>
  </snc>
  <trd lng="ca">geni</trd>
  <trd lng="de">talentierte Person</trd>
  <trd lng="fa">&a_sh;&a_kh;&a_S; <ind>&a_b;&a_A;&a_A;&a_s;&a_t;&a_ayn;&a_d;&a_A;&a_d;</ind></trd>
  <trdgrp lng="fr">
    <trd>g&eacute;nie</trd>,
    <trd>virtuose</trd>
  </trdgrp>
  <trd lng="hu">tehets&eacute;ges ember</trd>
  <trdgrp lng="pl">
    <trd>talent<klr> (o cz&lstroke;owieku)</klr></trd>,
    <trd>kto&sacute; <ind>utalentowany</ind></trd>,
    <trd>zdolniacha<klr> (pot.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_l;&c_a;&c_n;&c_t;<klr>
  (&c_o; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;&c_je;)</klr></trd>
</drv>
</art>
