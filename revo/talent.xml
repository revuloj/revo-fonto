<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>talent</rad>/o
</kap>
<drv mrk="talent.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Antikva pezmezuro.
    </dif>
    <trd lng="be">&c_t;&c_a;&c_l;&c_a;&c_n;&c_t;</trd>
  </snc>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Antikva grandvalora monunuo.
    </dif>
    <trd lng="be">&c_t;&c_a;&c_l;&c_a;&c_n;&c_t;</trd>
  </snc>
  <snc>
    <dif>
      Eminenta aparta spirita kapablo, &ccirc;u donacita de naturo, &ccirc;u akirita
      per laboro.
    </dif>
    <refgrp tip="sin">
      <ref cel="geni.0o">genio</ref>,
      <ref cel="virtuo.0o">virtuozo</ref>.
    </refgrp>
    <trd lng="nl">talent</trd>
    <trdgrp lng="sv">
      <trd>beg&aring;vning</trd>,
      <trd>talang</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_l;&c_je;&c_n;&c_t;</trd>
  <trd lng="de">Talent</trd>
  <trd lng="ru">&c_t;&c_a;&c_l;&c_a;&c_n;&c_t;</trd>
</drv>
</art>
