<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Tiberi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="tiberi.0o">
  <kap><tld/>o</kap>
  <snc mrk="tiberi.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira anta&ubreve;nomo latindevena:
      <ekz>
        en la dek-kvina jaro de la regado de <tld/>o Cezaro
        <fnt><bib>NT</bib>, <lok>&Luk; 3:1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_T;&c_i;&c_b;&c_je;&c_r;&c_i;&c_j;</trd>
  <trd lng="de">Tiberius</trd>
  <trd lng="en">Tiberius</trd>
  <trd lng="fr">Tib&egrave;re</trd>
  <trd lng="pl">Tyberiusz</trd>
  <trd lng="pt">Tib&eacute;rio</trd>
</drv>
</art>

