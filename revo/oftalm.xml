<art mrk="$Id: oftalm.xml,v 1.23 2025/04/04 16:57:06 revo Exp $">
<kap><rad>oftalmit</rad>/o<fnt><bib>PVEF</bib></fnt></kap>
<drv mrk="oftalm.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="inflam.0o" lst="voko:malsanoj">Inflamo</ref>
      de l' okulo:
      <ekz>
        printempe revigli&gcirc;as <klr>[&#8230;]</klr> mikroboj kaj virusoj
        <klr>[&#8230;]</klr> oni estas facile atakata de epidemia encefalito,
        morbilo, parotidito, akuta oftalmito kaj aliaj infektaj malsanoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=54924">Sanprotekto</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="inflam.okul0o">okulinflamo</ref>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>okulinflamo</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_a;&c_f;&c_t;&c_a;&c_l;&c_mol;&c_m;&c_ib;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je; &c_v;&c_o;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Augenentz&uuml;ndung</trd>
  <trd lng="el">&omikron;&phi;&theta;&alfa;&lambda;&my;&jota_ton;&alfa;</trd>
  <trdgrp lng="fr">
    <trd>ophtalmie</trd>
  </trdgrp>
  <trd lng="hu">szemgyullad&aacute;s</trd>
  <trd lng="id">oftalmia</trd>
  <trd lng="ja">&#30524;&#28814;  <pr>&#12364;&#12435;&#12360;&#12435;</pr></trd>
  <trd lng="nl">oogontsteking</trd>
  <trd lng="pl">zapalenie  <ind>oka</ind></trd>
  <trd lng="pt">oftalmia</trd>
  <trdgrp lng="ro">
    <trd>oftalmie</trd>,
    <trd>inflama&#539;ie ocular&abreve;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_f;&c_t;&c_a;&c_l;&c_mol;&c_m;&c_i;&c_t;</trd>,
    <trd>&c_o;&c_f;&c_t;&c_a;&c_l;&c_mol;&c_m;&c_i;&c_ja;</trd>,
    <trd>&c_v;&c_o;&c_s;&c_p;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je; &c_g;&c_l;&c_a;&c_z;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">o&ccaron;n&yacute; z&aacute;pal</trd>
  <trd lng="cs">o&ccaron;n&iacute; z&aacute;n&ecaron;t</trd>
  <trdgrp lng="uk">
    <trd>&#1086;&c_f;&c_t;&c_a;&#1083;&#1100;&#1084;&c_ib;&c_t;</trd>,
    <trd>&#1086;&c_f;&c_t;&c_a;&#1083;&#1100;&#1084;&c_ib;&#1103;</trd>,
    <trd>&c_z;&c_a;&#1087;&c_a;&#1083;&c_je;&#1085;&#1085;&#1103; &#1086;&#1082;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#30524;&#30142; <pr>y&#462;nj&#237;</pr></trd>
</drv>
</art>
