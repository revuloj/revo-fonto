<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>oftalmit</rad>/o</kap>
<drv mrk="oftalm.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Inflamo de l' okulo.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="inflam.okul0o">okulinflamo</ref>
  </refgrp>
  <trd lng="de">Augenentz&uuml;ndung</trd>
  <trd lng="el">&omikron;&phi;&theta;&alfa;&lambda;&my;&jota_ton;&alfa;</trd>
  <trdgrp lng="fr">
    <trd>ophtalmie</trd>
  </trdgrp>
  <trd lng="hu">szemgyullad&aacute;s</trd>
  <trd lng="nl">oogontsteking</trd>
  <trd lng="pt">oftalmia</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_f;&c_t;&c_a;&c_l;&c_mol;&c_m;&c_i;&c_t;</trd>,
    <trd>&c_o;&c_f;&c_t;&c_a;&c_l;&c_mol;&c_m;&c_i;&c_ja;</trd>,
    <trd>&c_v;&c_o;&c_s;&c_p;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je; &c_g;&c_l;&c_a;&c_z;&c_a;</trd>
  </trdgrp>
</drv>
</art>
