<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>trib</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="trib.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Grupo da fratrioj a&ubreve; kurioj kunigitaj per religia ligilo, havanta
      propran protektantan dion kaj propran komunan sanktejon.
    </dif>
    <refgrp tip="sin">
      <ref cel="gent.0o">gento</ref>,
      <ref cel="klan.0o">klano</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Idaro de &ccirc;iu el la 12 hebreaj patriarkoj.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_je;&c_m;&c_ja;</trd>
  <trd lng="de">Stamm</trd>
  <trd lng="en">tribe</trd>
  <trd lng="hu">t&ouml;rzs</trd>
  <trdgrp lng="nl">
    <trd>stam</trd>,
    <trd>volksstam</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_je;&c_m;&c_ja;</trd>,
    <trd>&c_r;&c_o;&c_d;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_je;&c_n;&c_o;<klr tip="ind"> (&c_r;&c_o;&c_d;)</klr></trd>
  </trdgrp>
</drv>
</art>
