<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>9</ofc><rad>trib</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="trib.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="trib.0o.ETN">
    <uzo tip="fak">POL</uzo>
    <dif>
      Parto de
      <ref tip="malprt" cel="naci.0o.popolo">nacio</ref>
      kun ne-&scirc;tata organizo, &gcirc;enerale ariganta plurajn
      <ref tip="prt" cel="gent.0o.familio">gentojn</ref>.
    </dif>
    <subsnc mrk="trib.0o.HIS">
      <uzo tip="fak">HIS</uzo>
      <dif>
        En la antikva grek-romia kulturo grupo de
        <ref tip="prt" cel="kuri.0o.HIS">kurioj</ref>
        a&ubreve; <ref tip="prt" cel="fratri.0o.HIS">fratrioj</ref>
        starigita por administraj celoj.
      </dif>
      <trdgrp lng="pl">
        <trd>plemi&eogonek;</trd>,
        <trd>szczep</trd>,
        <trd>klan</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="trib.0o.BIB">
      <uzo tip="fak">BIB</uzo>
      <dif>
        Idaro de &ccirc;iu el la dekdu hebreaj prapatroj.
      </dif>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="klan.0o">klano</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_je;&c_m;&c_ja;</trd>
  <trd lng="de">Stamm</trd>
  <trd lng="en">tribe</trd>
  <trd lng="fr">tribu</trd>
  <trd lng="hu">t&ouml;rzs</trd>
  <trd lng="it">trib&ugrave;</trd>
  <trdgrp lng="nl">
    <trd>stam</trd>,
    <trd>volksstam</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>plemi&eogonek;</trd>,
    <trd>r&oacute;d</trd>,
    <trd>pokolenie</trd>
  </trdgrp>
  <trd lng="pt">tribo</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_je;&c_m;&c_ja;</trd>,
    <trd>&c_r;&c_o;&c_d;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_je;&c_n;&c_o;
    <klr tip="ind"> (&c_r;&c_o;&c_d;)</klr></trd>
  </trdgrp>
</drv>
</art>
