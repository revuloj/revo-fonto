<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap><rad>argan</rad>/o
  <fnt><bib>PV</bib></fnt>
  </kap>
<drv mrk="argan.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc mrk="argan.0o.TEK">
    <dif>
      <ref cel="cxarpe.0ajxo">&Ccirc;arpenta&jcirc;o</ref> por 
      manovri pezan objekton super truo, ekz. por &ccirc;erpi el puto
      a&ubreve; por levi la <ref cel="dril.0o">drilegon</ref> de nafto&scirc;akto:
      <ekz>
        <nom>Leonardo da Vinci</nom> <klr>[&#8230;]</klr> plibonigis horlo&gcirc;ojn, teksilojn, <tld/>ojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2459">Leonardo da Vinci</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[aer]</klr>teleskopoj bezonis skafaldojn a&ubreve; longajn mastojn kaj <tld/>ojn por apogi ilin
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=410501">Aerteleskopo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la fandota&jcirc;o estas transportata en la fornon per specialaj <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=297082">Forno de Siemens-Martin</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lrg="800" lok=
    "https://upload.wikimedia.org/wikipedia/commons/8/86/Argano_sollevatore_pesi_Leonardo_Museo_scienza_e_tecnologia_Milano.jpg">
     Muzeo de Scienco kaj Teknologio &quot;Leonardo da Vinci&quot; [CC BY-SA 4.0 tra Wikimedia Commons]</bld>
    <trd lng="be">&c_v;&c_y;&c_sh;&c_k;&c_a; <klr tip="amb">(&c_n;&c_a;&c_f;&c_t;&c_a;&c_v;&c_a;&c_ja;)</klr></trd>
    <trd lng="ca">torre de perforaci&oacute;</trd>
    <trd lng="cs">vrtn&aacute; v&ecaron;&zcaron;</trd>
    <trdgrp lng="de">
      <trd>Hebezeug</trd>,
      <trd>Bohrger&uuml;st</trd>
    </trdgrp>
    <trd lng="en">hoist <klr>(device)</klr></trd>
    <trd lng="fr">treuil</trd>
    <trd lng="hu">f&uacute;r&oacute;torony</trd>
    <trd lng="it">argano</trd>
    <trd lng="nl">boortoren</trd>
    <trdgrp lng="pl">
      <trd>wie&zdot;a wiertnicza</trd>,
      <trd>maszt wiertniczy</trd>
    </trdgrp>
    <trd lng="ru">&c_n;&c_je;&c_f;&c_t;&c_ja;&c_n;&c_a;&c_ja; <ind>&c_v;&c_y;&c_sh;&c_k;&c_a;</ind></trd>
    <trd lng="sk">vrtn&aacute; ve&zcaron;a</trd>
  </snc>
  <snc>
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="gru.0o.TEK">gruo</ref>.
    </dif>
  </snc>
</drv>
</art>
