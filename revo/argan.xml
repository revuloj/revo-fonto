<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: argan.xml,v 1.24 2023/10/13 16:46:48 revo Exp $">
  <kap><rad>argan</rad>/o
  <fnt><bib>PV</bib></fnt>
  </kap>
<drv mrk="argan.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc mrk="argan.0o.TEK">
    <dif>
      <ref cel="cxarpe.0ajxo">&Ccirc;arpenta&jcirc;o</ref> por 
      manovri pezan objekton super truo, ekz. por &ccirc;erpi el <ref cel="put.0o">puto</ref>
      a&ubreve; por levi la <ref cel="dril.0o">drilegon</ref> de nafto<ref cel="sxakt.0o">&scirc;akto</ref>:
      <ekz>
        <nom>Leonardo da Vinci</nom> <klr>[&#x2026;]</klr> plibonigis horlo&gcirc;ojn, teksilojn, <tld/>ojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2459">Leonardo da Vinci</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[aer]</klr>teleskopoj bezonis skafaldojn a&ubreve; longajn mastojn kaj <tld/>ojn por apogi
        ilin
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=410501">Aerteleskopo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la fandota&jcirc;o estas transportata en la fornon per specialaj <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=297082">Forno de Siemens-Martin</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/8/86/Argano_sollevatore_pesi_Leonardo_Museo_scienza_e_tecnologia_Milano.jpg/640px-Argano_sollevatore_pesi_Leonardo_Museo_scienza_e_tecnologia_Milano.jpg" prm="CC BY-SA 4.0">argano de Leonardo da Vinci (Codex Atlanticus, folio 30v., &ccirc; 1480),
    rekonstruo en la Muzeo de Scienco kaj Teknologio &leftquot;Leonardo da Vinci&rightquot;, Milano
         <fnt>
           <aut>Alessandro Nassiri por la Nacia Muzeo de la Sciencoj kaj Teknologio Leonardo da
           Vinci, Milano</aut>
           <lok><url ref="&WCW;/File:Argano_sollevatore_pesi_Leonardo_Museo_scienza_e_tecnologia_Milano.jpg">&WCW;/File:Argano_sollevatore_pesi_Leonardo_Museo_scienza_e_tecnologia_Milano.jpg</url></lok>
         </fnt>
    </bld>

    <trd lng="be">&c_v;&c_y;&c_sh;&c_k;&c_a; <klr tip="amb">(&c_n;&c_a;&c_f;&c_t;&c_a;&c_v;&c_a;&c_ja;)</klr></trd>
    <trd lng="ca">torre de perforaci&oacute;</trd>
    <trd lng="cs">vrtn&aacute; v&ecaron;&zcaron;</trd>
    <trdgrp lng="de">
      <trd>Hebezeug</trd>,
      <trd>Bohrger&uuml;st</trd>,
      <trd>Hebewinde</trd>
    </trdgrp>
    <trd lng="en">hoist <klr>(device)</klr></trd>
    <trd lng="fr">treuil</trd>
    <trd lng="hu">f&uacute;r&oacute;torony</trd>
    <trd lng="it">argano</trd>
    <trd lng="nl">boortoren</trd>
    <trdgrp lng="pl">
      <trd>wie&zdot;a wiertnicza</trd>,
      <trd>maszt wiertniczy</trd>
    </trdgrp>
    <trd lng="ru">&c_n;&c_je;&c_f;&c_t;&c_ja;&c_n;&c_a;&c_ja; <ind>&c_v;&c_y;&c_sh;&c_k;&c_a;</ind></trd>
    <trd lng="sk">vrtn&aacute; ve&zcaron;a</trd>
  </snc>
  <snc>
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="gru.0o.TEK">gruo</ref>.
    </dif>
  </snc>
  <trd lng="he">&#x5DB;&#x5BC;&#x5B7;&#x5E0;&#x5B6;&#x5E0;&#x5B6;&#x5EA;</trd>
  <trdgrp lng="ja">
    <trd>&#x6CB9;&#x4E95;&#x3084;&#x3050;&#x3089;  <pr>&#x3086;&#x305B;&#x3044;&#x3084;&#x3050;&#x3089;</pr></trd>,
    <trd>&#x8D77;&#x91CD;&#x6A5F;  <pr>&#x304D;&#x3058;&#x3085;&#x3046;&#x304D;</pr></trd>,
    <trd>&#x30AF;&#x30EC;&#x30FC;&#x30F3;</trd>
  </trdgrp>
  <trd lng="uk">&#x431;&#x443;&#x440;&#x43E;&#x432;&#x430;&#xA0;&#x432;&#x438;&#x448;&#x43A;&#x430;</trd>
</drv>
</art>
