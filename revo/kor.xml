    <trd>&c_s;&c_je;&c_r;&c_d;&c_je;&c_ch;&c_n;&c_o;&c_je; &c_z;&c_a;&c_b;&c_o;&c_l;&c_je;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
