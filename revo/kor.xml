<drv mrk="kor.0a">
  <kap><tld/>a</kap>
  <snc mrk="kor.0a.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Rilata al la funkciado de koro:
      <ekz>
        <tld/>a malsano;
      </ekz>
      <ekz>
        <tld/>a kirurgio
        <fnt>
          <bib>Monato</bib>
          <aut>Steven K. Smith</aut>
          <vrk><url ref="&Monato;/2009/009659.php">La
          Helpanto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">cardiaque</trd>
    <trd lng="it">cardiaco</trd>
    <trdgrp lng="nl">
      <trd>cordiaal</trd>,
      <trd>hart&#x2010;</trd>
    </trdgrp>
    <trd lng="pl">sercowy</trd>
  </snc>
  <snc mrk="kor.0a.FIG">
    <dif>
      Venanta el <tld/>o; amika, sincera, favora,
      <ref tip="sin" cel="kor.el0a.KOMUNE">el<tld/>a</ref>:
      <ekz>
        <tld/>a akcepto, saluto, danko
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a &gcirc;ojo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a doloro
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e sindona amiko;
      </ekz>
      <ekz>
        mi salutas vin tut<tld/>e;
      </ekz>
      <ekz>
        pre&gcirc;u <tld/>e kaj laboru fervore<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="fr">cordial</trd>
    <trd lng="it">cordiale</trd>
    <trdgrp lng="nl">
      <trd>hartelijk</trd>,
      <trd>innig</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_r;&c_d;&c_e;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>ch&oacute;rick&yacute;</trd>,
    <trd>ch&oacute;rov&yacute;</trd>,
    <trd>sborov&yacute;</trd>
  </trdgrp>
  <trd lng="de">herzlich</trd>
  <trd lng="es">cordial</trd>
  <trd lng="fa">&a_q;&a_l;&a_b;&f_y;</trd>
  <trd lng="he">&lamed;&bet;&bet;&yod;</trd>
  <trdgrp lng="hu">
    <trd>sz&iacute;v-</trd>,
    <trd>sz&iacute;v&eacute;lyes</trd>,
    <trd>sz&iacute;vb&odblac;l j&ouml;v&odblac;</trd>
  </trdgrp>
  <trd lng="pl">serdeczny</trd>
  <trdgrp lng="ro">
    <trd>cordial</trd>,
    <trd>cardiac</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_je;&c_r;&c_d;&c_je;&c_ch;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>ch&oacute;rov&yacute;</trd>,
    <trd>zborov&yacute;</trd>
  </trdgrp>
  <trd lng="sv">hj&auml;rtlig</trd>
  <trd lng="vo">ladlik</trd>
</drv>

<drv mrk="kor.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="kor.0eco.KOMUNE">
    <dif>
      Varme amika teni&gcirc;o:
      <ekz>
        neniam mi premis al iu la manon kun pli sincera estimo kaj
        <tld/>eco
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        en &scirc;ia okulo kaj en la tono de &scirc;ia vo&ccirc;o oni povis rimarki tiun
        saman 
        seriozan <tld/>econ, kiun &ccirc;e &scirc;ia pli maljuna fratino 
        la senpatrine travivita juneco jam longe maturigis
        <fnt>
          <bib>BdV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">srde&ccaron;nost</trd>
  <trd lng="es">cordialidad</trd>
  <trd lng="fr">cordialit&eacute;</trd>
  <trdgrp lng="nl">
    <trd>hartelijkheid</trd>,
    <trd>innigheid</trd>
  </trdgrp>
  <trd lng="pl">serdeczno&sacute;&cacute;</trd>     
  <trd lng="ro">cordialitate</trd>
  <trd lng="sk">srde&ccaron;nos&tcaron;</trd>
</drv>

<drv mrk="kor.bon0a">
  <kap>bon<tld/>a</kap>
  <snc mrk="kor.bon0a.KOMUNE">
    <dif>
      Helpema, kompatema, el sincera sento altruisma:
      <ekz>
        ili estas homoj gastamaj kaj bon<tld/>aj
        <fnt><bib>Revizoro</bib>, <lok>akto 5a, sceno 8a</lok></fnt>;
      </ekz>
      <ekz>
        per bon<tld/>a <klr>(milda)</klr> admonado
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_r;&c_d;&c_e;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_d;&c_o;&c_b;&c_r;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bodr&yacute;</trd>,
    <trd>dobrosrde&ccaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="de">gutherzig</trd>
  <trdgrp lng="fa">
    <trd>&a_kh;&a_w;&a_sh;&zwnj;&a_q;&a_l;&a_b;</trd>,
    <trd>&a_m;&a_h;&a_r;&a_b;&a_A;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bienveillant</trd>,
    <trd>gentil</trd>
  </trdgrp>
  <trd lng="he">&tet;&vav;&bet;-&lamed;&bet;</trd>
