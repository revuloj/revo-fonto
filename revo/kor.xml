<drv mrk="kor.0a">
  <kap><tld/>a</kap>
  <snc mrk="kor.0a.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Rilata al la funkciado de koro:
      <ekz>
        <tld/>a malsano;
      </ekz>
      <ekz>
        <tld/>a dolorego
        <fnt>
          <bib>NT</bib>
          <lok>II. Korintanoj 2:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a kirurgio
        <fnt>
          <bib>Monato</bib>
          <aut>Steven K. Smith</aut>
          <vrk><url ref="&Monato;/2009/009659.php">La
          Helpanto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Herz-</trd>
    <trd lng="fr">cardiaque</trd>
    <trd lng="it">cardiaco</trd>
    <trdgrp lng="nl">
      <trd>cordiaal</trd>,
      <trd>hart&#8208;</trd>
    </trdgrp>
    <trd lng="pl">sercowy</trd>
  </snc>
  <snc mrk="kor.0a.FIG">
    <dif>
      Venanta el <tld/>o; ama, amika, sincera, favora,
      <ref tip="sin" cel="kor.el0a.KOMUNE">el<tld/>a</ref>:
      <ekz>
        <tld/>a akcepto, saluto
        <fnt><bib>DL</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>an dankon
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>!
      </ekz>
      <ekz>
        neniu &scirc;in konsolis per ia <tld/>a vorto
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a &gcirc;ojo
        <fnt><bib>FK</bib></fnt>;
      </ekz>
      <ekz>
        li faris <tld/>an adia&ubreve;on
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e sindona pastro
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Pri la homaranismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi amas vian filinon tut<tld/>e
        <fnt>
          <bib>TAA</bib>
          <lok>&Ccirc;apitro XII.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> kantos pro <tld/>a gajeco
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 65:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        amo pli <tld/>a, disi&gcirc;o pli dolora
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        pre&gcirc;u <tld/>e kaj laboru fervore
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="de">herzlich</trd>
    <trd lng="fr">cordial</trd>
    <trd lng="it">cordiale</trd>
