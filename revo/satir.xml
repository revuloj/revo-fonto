<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>satir</rad>/o
</kap>
<drv mrk="satir.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELE</uzo>
  <snc>
    <dif>
      Proza a&ubreve; versa teksto
      moke ridindiganta kaj kritikanta iun a&ubreve; ion:
      <ekz>
        <tld/>a viza&gcirc;esprimo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="apolog1.0o">apologio</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="lauxd.mal0i">malla&ubreve;di</ref>,
      <ref cel="mok.0i">moki</ref>,
      <ref cel="parodi.0o">parodio</ref>,
      <ref cel="paskvi.0o">paskvilo</ref>,
      <ref cel="vip.0i">vipi</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_t;&c_y;&c_r;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>přen. jízlivý posměšek</trd>,
    <trd>satira</trd>
  </trdgrp>
  <trd lng="de">Satire</trd>
  <trd lng="en">satire</trd>
  <trdgrp lng="fr">
    <trd>satire</trd>
  </trdgrp>
  <trd lng="hu">szat&iacute;ra</trd>
  <trd lng="it">satira</trd>
  <trd lng="nl">satire</trd>
  <trd lng="pl">satyra</trd>
  <trd lng="ru">&c_s;&c_a;&c_t;&c_i;&c_r;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>posmešok</trd>,
    <trd>satira</trd>
  </trdgrp>
</drv>
</art>
