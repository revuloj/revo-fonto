<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>9</ofc><rad>pund</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="pund.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="pund.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="unu.mon0o.EKON">Monunuo</ref>, interalie de
Britujo:
      <ekz>
        la firmao &jcirc;us anoncis jaran malprofiton
        de 61 milionoj da <tld/>oj anstata&ubreve; 27 milionoj, kiel
        planite<fnt><aut>E. Grimley Evans</aut>, <vrk>Interreto
        senpage</vrk>, <bib>Monato</bib><lok>jaro 2000a, numero 11a, p.
        23a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="funt.sterlinga0o">sterlinga funto</ref>
    <ref tip="prt" cel="penc.0o">penco</ref>
  </snc>
  <trd lng="be">&c_f;&c_u;&c_n;&c_t;
&c_s;&c_t;&c_e;&c_r;&c_l;&c_ib;&c_n;&c_g;&c_a;&c_w;</trd>
  <trd lng="ca">lliura</trd>
  <trd lng="de">Pfund</trd>
  <trd lng="en">pound</trd>
  <trd lng="es">libra</trd>
  <trdgrp lng="fa">
    <trd>&fa_lam;&fa_ye;&fa_re;&fa_heh;</trd>
    <trd>&fa_pe;&fa_vav;&fa_nun;&fa_dal;</trd>
  </trdgrp>
  <trd lng="fr">livre<klr tip="ind"> (monnaie)</klr></trd>
  <trd lng="hu">font<klr tip="ind"> (p&eacute;nz)</klr></trd>
  <trd lng="nl">pond<klr tip="ind"> (muntsoort)</klr></trd>
  <trd lng="pl">funt</trd>
  <trd lng="pt">libra <klr tip="amb">(esterlina)</klr></trd>
  <trd lng="ru">&c_f;&c_u;&c_n;&c_t;
&c_s;&c_t;&c_je;&c_r;&c_l;&c_i;&c_n;&c_g;&c_o;&c_v;</trd>
</drv>
