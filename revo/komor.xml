<vortaro>
<art mrk="$Id$">
<kap>
  <rad>komor</rad>/oj
</kap>

<drv mrk="komor.0oj">
  <kap>
    <tld lit="K"/>oj
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="komor.0oj.GEOG">
