<vortaro>
  <art mrk="$Id: jude.xml,v 1.5 2022/03/21 17:23:46 revo Exp $">
    <kap>
      <rad>Jude</rad>/o
    </kap>

<drv mrk="jude.0o">
  <kap><tld/>o</kap>
  <snc mrk="jude.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="dif" cel="jud.J0ujo">Judujo</ref>:
      <ekz>
        post dekkelko da jaroj <tld/>o kaj Fenicio falus en la manojn de
        Asirianoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sekto mirakle naski&gcirc;is en <nom>Bet-Le&hcirc;em</nom>, urbeto en
        <tld/>o
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="https://www.monato.net/2003/008041.html">La aristarka raporto de Albisturo Kvinke</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_Ju;&c_d;&c_e;&c_ja;</trd>
    <trd lng="de">Jud&auml;a</trd>
    <trd lng="fr">Jud&eacute;e</trd>
  </snc>
  <trdgrp lng="uk">
    <trd>&#x42E;&#x434;&#x435;&#x44F;</trd>,
    <trd>&#x406;&#x443;&#x434;&#x435;&#x44F;</trd>
  </trdgrp>
</drv>

  </art>

