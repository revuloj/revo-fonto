<vortaro>
  <art mrk="$Id: jude.xml,v 1.8 2025/12/18 20:33:49 revo Exp $">
    <kap>
      <rad>Jude</rad>/o
    </kap>

<drv mrk="jude.0o">
  <kap><tld/>o</kap>
  <snc mrk="jude.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="dif" cel="jud.J0ujo">Judujo</ref>:
      <ekz>
        post dekkelko da jaroj <tld/>o kaj Fenicio falus en la manojn de
        Asirianoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sekto mirakle naski&gcirc;is en <nom>Bet-Le&hcirc;em</nom>, urbeto en
        <tld/>o
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Monato;/2003/008041.html">La aristarka raporto de Albisturo
          Kvinke</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_Ju;&c_d;&c_e;&c_ja;</trd>
    <trd lng="de">Jud&auml;a</trd>
    <trd lng="fr">Jud&eacute;e</trd>
    <trd lng="pl">Judea</trd>
    <trd lng="zh">&#x72B9;&#22320;&#x4E9A; <pr>Y&#xF3;ud&#xEC;y&#xE0;</pr></trd>
</snc>
  <trdgrp lng="uk">
    <trd>&#x42E;&c_d;&c_je;&#x44F;</trd>,
    <trd>&c_Ib;&c_u;&c_d;&c_je;&#x44F;</trd>
  </trdgrp>
</drv>
</art>

