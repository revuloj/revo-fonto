<art mrk="$Id: tomas.xml,v 1.16 2023/10/07 06:29:09 revo Exp $">
<kap>
    <rad>Tomas</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tomas.0o">
    <kap><tld/>o</kap>
    <snc mrk="tomas.0o.NOM">
      <uzo tip="fak">NOM</uzo>
      <dif>
        Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref>
        aramedevena:
        <ekz>
          tiam li diris al <tld/>o : Etendu &ccirc;i tien vian fingron
          <fnt><bib>NT</bib>, <lok>&Mat; 20:27</lok></fnt>
        </ekz>
      </dif>
    </snc>
    <trd lng="be">&c_T;&c_a;&c_m;&c_a;&c_sh;</trd>
    <trd lng="br">Tomaz</trd>
    <trd lng="ca">Tom&agrave;s</trd>
    <trd lng="de">Thomas</trd>
    <trd lng="fr">Thomas</trd>
    <trd lng="hu">Tam&aacute;s</trd>
    <trd lng="it">Tommaso</trd>
    <trdgrp lng="ja">
    <trd>&#x30C8;&#x30FC;&#x30DE;&#x30B9;</trd>,
    <trd>&#x30C8;&#x30DE;&#x30B9;</trd>
  </trdgrp>
  <trdgrp lng="pl">
      <trd>Tomasz</trd>,
      <trd>Tomek<klr> (zdrob.)</klr></trd>
    </trdgrp>
    <trd lng="pt">Tom&aacute;s</trd>
    <trdgrp lng="ru">
      <trd>&c_F;&c_o;&c_m;&c_a;</trd>,
      <trd>&c_T;&c_o;&c_m;&c_a;&c_s;<klr>
      (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>
    </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_T;&#x43E;&#x43C;&c_a;</trd>,
    <trd>&c_F;&#x43E;&#x43C;&c_a;</trd>,
    <trd>&c_H;&#x43E;&#x43C;&c_a;</trd>
  </trdgrp>
</drv>
</art>

