    </snc>                     
    <trd lng="be">&c_T;&c_a;&c_m;&c_a;&c_sh;</trd>
    <trd lng="br">Tomaz</trd>
    <trd lng="fr">Thomas</trd>
    <trd lng="hu">Tam&aacute;s</trd>
    <trdgrp lng="pl">
      <trd>Tomasz</trd>,
      <trd>Tomek<klr> (zdrob.)</klr></trd>
    </trdgrp>
    <trd lng="pt">Tom&aacute;s</trd>
    <trdgrp lng="ru">
      <trd>&c_F;&c_o;&c_m;&c_a;</trd>,
      <trd>&c_T;&c_o;&c_m;&c_a;&c_s;<klr>
      (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>
    </trdgrp>
</drv>
</art>

