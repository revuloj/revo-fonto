<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: grak.xml,v 1.24 2024/09/30 18:35:44 revo Exp $">
<kap>
  <rad>grak</rad>/i <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="grak.0">
  <kap><tld/></kap>
  <snc mrk="grak.0.SON">
    <dif>
      Akra <ref tip="lst" cel="blek.0o" lst="voko:blekoj">bleko</ref>
      de korvo a&ubreve; simila sono:
      <ekz>
        korniko flugis super lia kapo, kaj
        <tld/>is: <ctl><tld/>-<tld/>-<tld/>-stulÂ­tuleto</ctl>
        <fnt>
          <aut>trad. A. M'Callum</aut>,
          <vrk><url ref="http://www.literaturo.dk/muraho.htm">
          Mura&hcirc;o kaj Mina&hcirc;o</url></vrk>,
          <lok>The Esperanto Monthly, 1919-09 (8)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_r;&c_r;!</trd>
  <trd lng="de">krah!</trd>
  <trd lng="fr">croa</trd>
</drv>

<drv mrk="grak.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="grak.0i.korve">
    <dif>
      <ref tip="super" cel="blek.0i.beste">Bleki</ref> kiel <ref tip="vid"
      cel="korv.0o.genro">korvo</ref>.
    </dif>
    <trd lng="be">&c_k;&c_a;&c_r;&c_k;&c_a;&c_c;&c_mol;</trd>
    <trd lng="ca">grallar</trd>
    <trd lng="de">kr&auml;chzen</trd>
    <trd lng="en">caw</trd>
    <trd lng="es">graznar</trd>
    <trd lng="fr">croasser</trd>
    <trd lng="hu">k&aacute;rog</trd>
    <trd lng="nl">krassen<klr tip="ind"> (v.kraai of raaf)</klr></trd>
    <trdgrp lng="pt">
     <trd>grasnar<klr tip="amb"> (o corvo)</klr></trd>,
     <trd>crocitar</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_a;&c_r;&c_k;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="grak.0i.home">
    <dif>
      Estigi ra&ubreve;kan bruon en la gor&gcirc;o, eligante mukon,
      <ref tip="vid" cel="vocx.klarigila0on.KOMUNE">klarigi
      la vo&ccirc;on</ref>:
      <ekz>
        L' almozulo kun kolegoj &parsl; Dancas farandolon &parsl; Kaj ebriaj ili <tld/>as Ven&gcirc;an
        karmanjolon
        <fnt>
          <bib>LSB</bib>
          <lok>L' Almozulo</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiuj ne devus sidi hejme kaj <tld/>i ra&ubreve;kvo&ccirc;e kaj imagi, ke ilia pepado kaj
        kriado havas ion komunan kun la vera kantado
        <fnt>
          <bib>VRA</bib>
          <lok>Eyvind Johnson</lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;vana!&rightquot; li <tld/>is, &leftquot;la tuta verko de mia vivo, miaj esploroj, miaj jarlongaj
        observoj &dash; &ccirc;io vana!&rightquot;
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Vo&ccirc;o de la Kvieto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kartav.0i">kartavi</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_h;&c_a;&c_r;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_ch;&c_y;&c_sh;&c_ch;&c_a;&c_c;&c_mol; <ind>&c_g;&c_o;&c_r;&c_l;&c_a;</ind></trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>gargamellejar</trd>,
      <trd>escurar-se el coll</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>sich <ind>r&auml;uspern</ind></trd>,
      <trd>heiser sprechen</trd>
    </trdgrp>
    <trd lng="es">tener <ind>carraspera</ind></trd>
    <trdgrp lng="en">
      <trd>croak</trd>,
      <trd>rasp</trd>
    </trdgrp>
    <trd lng="fr">se <ind>racler</ind> la gorge</trd>
    <trd lng="hu">kr&aacute;kog</trd>
    <trd lng="nl">rochelen</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_ch;&c_i;&c_s;&c_t;&c_i;&c_t;&c_mol; &c_g;&c_o;&c_r;&c_l;&c_o;</trd>,
      <trd>&c_o;&c_t;&c_k;&c_a;&c_sh;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>chrchlat</trd>,
    <trd>odka&scaron;l&aacute;vat</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12459;&#12450;&#12459;&#12450;&#40180;&#12367;
    <pr>&#12459;&#12450;&#12459;&#12450;&#12394;&#12367;</pr></trd>,
    <trd>&#12398;&#12393;&#12434;&#40180;&#12425;&#12377;  <pr>&#12398;&#12393;&#12434;&#12394;&#12425;&#12377;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chrchla&tcaron;</trd>,
    <trd>odka&scaron;l&aacute;va&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_r;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_h;&c_a;&c_r;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_ch;&c_i;&c_shch;&c_a;&c_t;&c_i; &c_g;&c_o;&c_r;&c_l;&c_o; &c_h;&c_a;&c_r;&c_k;&c_a;&c_n;&c_n;&c_ja;&c_m;</trd>
  </trdgrp>
</drv>
</art>
