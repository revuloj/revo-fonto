<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>grak</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="grak.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc num="1">
    <dif>
      Bleki kiel korvo. 
    </dif>
    <trd lng="de">kr&auml;chzen</trd>
    <trd lng="en">caw</trd>
    <trd lng="fr">croasser</trd>
    <trd lng="ru">&c_k;&c_a;&c_r;&c_k;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc num="2">
    <dif>
      Estigi ra&ubreve;kan bruon en la gor&gcirc;o, eligante mukon.  
    </dif>
    <trdgrp lng="de">
      <trd>sich <ind>r&auml;uspern</ind></trd>,
      <trd>heiser sprechen</trd></trdgrp>
    <trdgrp lng="en">
      <trd>croak</trd>,
      <trd>rasp</trd>
    </trdgrp>
    <trd lng="fr">se <ind>r&acirc;cler</ind> la gorge</trd>
  </snc>
</drv>
</art>
