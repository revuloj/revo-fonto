<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>piramid</rad>/o
</kap>
<drv mrk="pirami.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Monumento kun kvar triangulaj facoj kaj kvarangula bazo, servanta
      kiel tombo por antikvaj re&gcirc;oj de Egiptujo.
    </dif>
    <trd lng="hu">piramis</trd>
  </snc>
  <snc mrk="pirami.0o.figuro">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 30</lok></fnt>
    <dif>
      <ref tip="super" cel="edr.plur0o">Pluredro</ref>, kies unu faco
      <klr>(&gcirc;ia <ref tip="prt" cel="baz.0o.figuro">bazo</ref>)</klr> estas
      ajna <ref cel="later.plur0o">plurlatero</ref> kaj &ccirc;iuj aliaj facoj
      estas <ref cel="angul.tri0o">trianguloj</ref>.
    </dif>
    <trd lng="en">pyramid</trd>
    <trd lng="hu">g&uacute;la</trd>
  </snc>
  <trd lng="de">Pyramide</trd>
  <trd lng="fr">pyramide</trd>
  <trd lng="nl">piramide</trd>
  <trd lng="ru">&c_p;&c_i;&c_r;&c_a;&c_m;&c_i;&c_d;&c_a;</trd>
</drv>
</art>
