<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: alabas.xml,v 1.24 2023/10/12 18:05:09 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>alabastr</rad>/o
</kap>
<drv mrk="alabas.0o">
  <kap><tld/>o</kap>
  <snc mrk="alabas.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Varia&jcirc;o de <ref tip="super" cel="gips.0o">gipso</ref>, 
      kalcisulfato, diafaneta, similanta <ref cel="marmor.0o">marmoron</ref>:
      <ekz>
        unu muro estis el <tld/>o, anta&ubreve; &gcirc;i staris la instruisto kaj skribis signojn per
        diverskoloraj krajonoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        viza&gcirc;koloro kiel <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_ja;&c_b;&c_a;&c_s;&c_t;&c_a;&c_r;</trd>
  <trd lng="ca">alabastre</trd>
  <trdgrp lng="cs">
    <trd>alabastr</trd>,
    <trd>stavebn&iacute; s&aacute;dra</trd>
  </trdgrp>
  <trd lng="de">Alabaster</trd>
  <trd lng="en">alabaster</trd>
  <trd lng="es">alabastro</trd>
  <trd lng="fr">alb&acirc;tre</trd>
  <trd lng="he">&#x5D1;&#x5BC;&#x5B7;&#x5D4;&#x5B7;&#x5D8;</trd>
  <trd lng="hu">alab&aacute;strom</trd>
  <trdgrp lng="ja">
    <trd>&#x30A2;&#x30E9;&#x30D0;&#x30B9;&#x30BF;&#x30FC;</trd>,
    <trd>&#x96EA;&#x82B1;&#x77F3;&#x818F;  <pr>&#x305B;&#x3063;&#x304B;&#x305B;&#x3063;&#x3053;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">albast</trd>
  <trd lng="pl">alabaster</trd>
  <trd lng="pt">alabastro</trd>
  <trd lng="ru">&c_a;&c_l;&c_je;&c_b;&c_a;&c_s;&c_t;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>alabaster</trd>,
    <trd>&uacute;be&lcaron;</trd>
  </trdgrp>
  <trd lng="uk">&#x430;&#x43B;&#x435;&#x431;&#x430;&#x441;&#x442;&#x440;</trd>
</drv>
</art>
