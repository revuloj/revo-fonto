<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>regiment</rad>/o
    </kap>
    <drv mrk="regime.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc>
        <dif>
          Parto de armeo, konsistanta  el kelkaj
          <ref tip="prt" cel="batali.0o">batalionoj</ref>
          a&ubreve; <ref tip="prt" cel="skadr.0o">skadroj</ref>:
          <ekz>
            la oficira rango responda al <tld/>estro estas
            <ref cel="kolone.0o">kolonelo</ref>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            li ne estas el la <tld/>o de timuloj<fnt>Z</fnt>.
          </ekz>
        </dif>
        <refgrp tip="malprt">
          <ref cel="divizi.0o">divizio</ref>,
          <ref cel="legi.0o">legio</ref>
        </refgrp>
      </snc>
      <trd lng="de">Regiment</trd>
      <trd lng="nl">regiment</trd>
      <trd lng="ru">&c_p;&c_o;&c_l;&c_k;</trd>
    </drv>
  </art>
