<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kriminal</rad>/a
</kap>
<drv mrk="krimin.0a">
  <kap>
    <ofc>*</ofc><tld/>a
  </kap>
  <snc>
    <dif>
      Koncernanta la krimojn:
      <ekz>
        <tld/>a juro, ju&gcirc;isto, tribunalo, proceso<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_m;&c_ib;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trdgrp lng="de">
     <trd>Kriminal-</trd>,
     <trd>Straf-</trd>
  </trdgrp>
  <trd lng="en">criminal</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_g;&c_o;&c_l;&c_o;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_k;&c_r;&c_i;&c_m;&c_i;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="krimin.0isto">
  <kap>
    <tld/>isto
  </kap>
  <snc>
    <dif>
      Juristo, fakulo pri <tld/>a juro.
    </dif>
    <trdgrp lng="de">
      <trd>Kriminalist</trd>,
      <trd>Strafrechtler</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      <tld lit="K"/>a sekreta policano.
    </dif>
    <trd lng="de">Geheimpolizist</trd>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_m;&c_ib;&c_n;&c_a;&c_l;&c_ib;&c_s;&c_t;</trd>
  <trd lng="ru">&c_k;&c_r;&c_i;&c_m;&c_i;&c_n;&c_a;&c_l;&c_i;&c_s;&c_t;</trd>
</drv>
</art>
