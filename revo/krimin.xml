<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: krimin.xml,v 1.25 2023/12/03 09:59:01 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kriminal</rad>/a
</kap>
<drv mrk="krimin.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="krimin.0a.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Koncernanta la <ref cel="jur.0o">juro</ref>n pri <ref cel="krim.0o">krimo</ref>j:
      <ekz>
        <tld/>a ju&gcirc;isto, tribunalo;
      </ekz>
      <ekz>
        <tld/>a policisto
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        nenio interesa en la <tld/>a rubriko
        <fnt>
          <bib>LGA</bib>
          <lok>I.</lok>
        </fnt>;
      </ekz>
      <ekz>
        la helpo de fakuloj-juristoj estas deviga en &ccirc;iuj pli gravaj civilaj a&ubreve; <tld/>aj
        procesoj
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj aferoj estis anta&ubreve;viditaj en la konvencio
        <fnt>
          <bib>CKv</bib>
          <lok>3</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Haughey</nom> devis aperi anta&ubreve; <tld/>a kortumo
        <fnt>
          <aut>Garbhan MacAoidh</aut>
            <vrk><url ref="&Monato;/2000/007096.php">Ekskapo eskapas</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 8</lok>
          </fnt>;
      </ekz>
      <ekz>
        la hungara <tld/>a kodo en sia paragrafo 198 difinas kiel punendajn nur drastajn
        seks-agojn
        <fnt>
          <bib>Monato</bib>
          <aut>Istv&aacute;n Ertl</aut>
          <vrk><url ref="&Monato;/2004/008238.html">A&gcirc;o de konsento</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        aperis anka&ubreve; <tld/>romaneto: Tiel Okazis, a&ubreve; Mistero Minora
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ordu la &ccirc;ambrojn, &ccirc;ar sinjoroj de la <tld lit="K"/>a
        Ministerio balda&ubreve; venos
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 103a, volumo 3a, p.
        338a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_m;&c_ib;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>krimin&aacute;ln&iacute;</trd>,
    <trd>trestn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kriminal-</trd>,
    <trd>Straf-</trd>
  </trdgrp>
  <trd lng="en">criminal</trd>
  <trd lng="es">criminal</trd>
  <trd lng="fr">criminel</trd>
  <trdgrp lng="hu">
    <trd>b&udblac;n&uuml;gyi</trd>,
    <trd>b&udblac;ntet&odblac;-</trd>,
    <trd>krimin&aacute;l-</trd>
  </trdgrp>
  <trd lng="ja">&#x5211;&#x4E8B;&#x4E0A;&#x306E;  <pr>&#x3051;&#x3044;&#x3058;&#x3058;&#x3087;&#x3046;&#x306E;</pr></trd>
  <trd lng="nl">strafrechtelijk</trd>
  <trd lng="pl">kryminalny</trd>
  <trd lng="pt">criminal</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_g;&c_o;&c_l;&c_o;&c_v;&c_n;&c_y;&c_j;</trd>, 
    <trd>&c_k;&c_r;&c_i;&c_m;&c_i;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>krimin&aacute;lny</trd>,
    <trd>trestn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_r;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_k;&c_r;&c_i;&c_m;&c_ib;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="zh">&#x5211;&#x4E8B; <pr>x&#xED;ngsh&#xEC;</pr></trd>
</drv>

<drv mrk="krimin.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="krimin.0isto.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      <ref tip="super" cel="jur.0isto">Juristo</ref>, fakulo pri
      krimjuro:
      <ekz>
        la <tld/>isto <nom>Thomas M&uuml;ller</nom> diris, ke lia psikologia profilo estas nekutima
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2006/009093.php">Postulataj: pli severaj punoj</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
<ref tip="vid" cel="krim.0osciencisto">krimofakulo</ref>
    <trdgrp lng="de">
      <trd>Kriminalist</trd>,
      <trd>Strafrechtler</trd>
    </trdgrp>
    <trd lng="es">criminalista</trd>
    <trd lng="fr">criminaliste</trd>
    <trdgrp lng="hu">
      <trd>b&uuml;ntet&odblac;jog&aacute;sz</trd>,
      <trd>kriminalista</trd>
    </trdgrp>
    <trd lng="nl">criminalist</trd>
    <trd lng="pl">kryminolog</trd>
  </snc>
  <snc mrk="krimin.0isto.POL">
    <dif>
      <tld lit="K"/>a sekreta policano:
      <ekz>
        envenis juna, rozviza&gcirc;a, kvieta kaj mildmaniera viro, neniom similanta
        <tld/>iston, kaj tamen unu el la plej bonaj Moskvaj <tld/>istoj
        <fnt>
          <bib>MkM</bib>
          <lok>27</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Kriminalpolizist</trd>
    <trd lng="hu">titkosrend&odblac;r</trd>
    <trd lng="nl">rechercheur</trd>
    <trd lng="pl">agent kryminalny</trd>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_m;&c_ib;&c_n;&c_a;&c_l;&c_ib;&c_s;&c_t;</trd>
  <trd lng="ja">&#x5211;&#x6CD5;&#x5B66;&#x8005;  <pr>&#x3051;&#x3044;&#x307B;&#x3046;&#x304C;&#x304F;&#x3057;&#x3083;</pr></trd>
  <trd lng="ru">&c_k;&c_r;&c_i;&c_m;&c_i;&c_n;&c_a;&c_l;&c_i;&c_s;&c_t;</trd>
  <trd lng="uk">&c_k;&c_r;&c_i;&c_m;&c_ib;&c_n;&c_a;&c_l;&c_ib;&c_s;&c_t;</trd>
</drv>
</art>
