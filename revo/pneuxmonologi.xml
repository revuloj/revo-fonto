<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pneuxmonologi.xml,v 1.6 2022/03/26 12:56:09 revo Exp $">
<kap>
    <rad>pne&ubreve;monologi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="pneuxmonologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="pneuxmonologi.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="malprt" cel="medici.0o">Medicina</ref>
      <ref tip="lst" lst="voko:sciencoj" cel="fak.0o.brancxo">fako</ref>,
      kiu studas la <ref tip="vid" cel="fiziol1.0o">fiziologion</ref>
      kaj la
      <ref tip="vid" cel="patolo.0o">patologion</ref> de la spira
      sistemo:
      <ekz>
        la <tld/>o ampleksas la preventon, diagnozadon kaj sen-operacian
        terapion de la
        malsanoj de la pulmo, la bronkoj, la mediastino kaj de la ple&ubreve;ro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=269140">Pne&ubreve;monologio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="pneuxmonolog.0o">pne&ubreve;monologo</ref>
    <trdgrp lng="de">
      <trd>Lungenheilkunde</trd>,
      <trd>Pulmologie</trd>,
      <trd>Pneumologie</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_l;&c_mol;&c_m;&c_a;&c_n;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trdgrp lng="en">
    <trd>pulmonology</trd>,
    <trd>pneumology</trd>
  </trdgrp>
  <trd lng="es">neumolog&iacute;a</trd>
  <trd lng="fr">pneumologie</trd>
  <trd lng="pt">pneumologia</trd>
  <trd lng="uk">&#x43F;&#x443;&#x43B;&#x44C;&#x43C;&#x43E;&#x43D;&#x43E;&#x43B;&#x43E;&#x433;&#x456;&#x44F;</trd>
</drv>
</art>

