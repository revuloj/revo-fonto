<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>pne&ubreve;monologi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="pneuxmonologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="pneuxmonologi.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Medicina fako, kiu studas la <ref tip="vid"
      cel="fiziol1.0o">fiziologion</ref> kaj la <ref tip="vid"
      cel="patolo.0o">patologion</ref> de la spira sistemo.
    </dif>
    <ref tip="vid" cel="pneuxmonolog.0o">pne&ubreve;monologo</ref>
  </snc>
  <trdgrp lng="en">
    <trd>pulmonology</trd>,
    <trd>pneumology</trd>
  </trdgrp>
  <trd lng="es">neumolog&iacute;a</trd>
  <trd lng="fr">pneumologie</trd>
</drv>
</art>

