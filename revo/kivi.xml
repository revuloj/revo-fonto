<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kivi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kivi.0o">
  <kap><tld/>o</kap>
  <snc mrk="kivi.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Novzelanda neflugipova nokta birdo, proksimume granda kiel koko,
      simbolo de Novzelando, sola genro de familio apterigoj
      (<trd lng="la">Apteryx</trd>).
    </dif>
  </snc>
  <trd lng="de">Kiwi<klr> (Vogel)</klr></trd>
  <trd lng="fa">&f_k;&f_y;&a_w;&f_y;<klr tip="amb"> (&f_p;&a_r;&a_n;&a_d;&a_h;)</klr></trd>
  <trd lng="fr">kiwi<klr> (oiseau)</klr></trd>
  <trd lng="nl">kiwi<klr tip="ind"> (vogel)</klr></trd>
  <trd lng="pl">kiwi<klr> (ptak)</klr></trd>
</drv>
</art>

