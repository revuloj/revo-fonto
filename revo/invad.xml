<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>invad</rad>/i
</kap>
<drv mrk="invad.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Perforte eniri kaj ekokupi fremdan landon:
      <ekz>
        la malamikaj armeoj <tld/>is la landon;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la pesto <tld/>is la landon.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_u;&c_r;&c_y;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_u;&c_v;&c_a;&c_r;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_a;&c_p;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>&uuml;berfallen</trd>,
      <trd>angreifen</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>megsz&aacute;ll</trd>,
      <trd>elfoglal</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_t;&c_o;&c_r;&c_g;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_z;&c_a;&c_p;&c_o;&c_l;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Grandnombre eniri iun lokon:
      <ekz>
        la popolamaso <tld/>is la domon;
      </ekz>
      <ekz>
        la gaja junularo <tld/>is la dancejon;
      </ekz>
      <ekz>
        domo, &scirc;ipo <tld/>ita de ratoj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        nokto <tld/>is la teron
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 16</lok></fnt>;
      </ekz>
      <ekz>
        la akvo <tld/>is la kampojn;
      </ekz>
      <ekz>
        &gcirc;ardeno <tld/>ita de herba&ccirc;oj;
      </ekz>
      <ekz>
        la politiko &ccirc;ion <tld/>as.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_p;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_p;&c_o;&c_w;&c_n;&c_ib;&c_c;&c_mol; <klr tip="amb">(&c_s;&c_a;&c_b;&c_o;&c_j;)</klr></trd>
    </trdgrp>
    <trd lng="hu">el&aacute;raszt <klr tip="ind">(&aacute;tv.)</klr></trd>
    <trd lng="ru">&c_z;&c_a;&c_p;&c_o;&c_l;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="de">
    <trd>einfallen</trd>,
    <trd>eindringen</trd>
  </trdgrp>
  <trd lng="en">invade</trd>
  <trd lng="es">invadir</trd>
  <trdgrp lng="fr">
    <trd>envahir</trd>,
    <trd>d&eacute;ferler (sur)</trd>,
    <trd>inonder<klr tip="ind"> (envahir)</klr></trd>
  </trdgrp>
  <trd lng="nl">binnenvallen</trd>
  <trd lng="pt">invadir</trd>
</drv>

<drv mrk="invad.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        la <tld/>oj de la Barbaroj detruis la Roman imperion;
      </ekz>
      <ekz>
        <tld/>o de akridoj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_v;&c_a;&c_r;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_n;&c_a;&c_sh;&c_e;&c_s;&c_mol;&c_c;&c_je;</trd>
  </trdgrp>
  <trd lng="de">Invasion</trd>
  <trd lng="es">invasi&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>invasion</trd>,
    <trd>d&eacute;ferlement</trd>,
    <trd>envahissement</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megsz&aacute;ll&aacute;s</trd>,
    <trd>inv&aacute;zi&oacute;</trd>
  </trdgrp>
  <trd lng="nl">invasie</trd>
  <trd lng="pt">invas&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_t;&c_o;&c_r;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_n;&c_a;&c_sh;&c_je;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
