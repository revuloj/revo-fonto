<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kas</rad>/o
</kap>
<drv mrk="kas.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Monkesto:
      <ekz>
        regna <tld/>o;
      </ekz>
      <ekz>
        vivi de &scirc;parita <tld/>o<fnt>Z</fnt>
        <klr>(mono)</klr>;
      </ekz>
      <ekz>
        kiu rompis la glason, tiu ordigu la <tld/>on<fnt>Z</fnt>
        <klr>(sk pagante la perdon, &ccirc;ar &ccirc;iu devas prirespondi pri siaj
        faroj)</klr>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="posx.0o">po&scirc;o</ref>,
      <ref cel="sak.0o">sako</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="en">
    <trd>cash box</trd>,
    <trd>money box</trd>,
    <trd>till</trd>
  </trdgrp>
  <trd lng="nl">kas</trd>
  <trd lng="ru">&c_k;&c_a;&c_s;&c_s;&c_a;</trd>
  <trd lng="sv">kassa</trd>
</drv>

<drv mrk="kas.0ejo">
  <kap>
    <tld/>ejo
  </kap>
  <snc>
    <dif>
      &Ccirc;ambro, destinita por enspezado kaj elspezado de mono, kaj en kiu
      oni konservas la <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>cash box</trd>,
    <trd>money box</trd>,
    <trd>till</trd>
  </trdgrp>
  <trd lng="nl">kas</trd>
  <trd lng="ru">&c_k;&c_a;&c_s;&c_s;&c_a;</trd>
  <trd lng="sv">kassa</trd>
</drv>

<drv mrk="kas.0isto">
  <kap>
    <tld/>isto
  </kap>
  <snc>
    <dif>
      Oficisto, kiu en- kaj elspezas monon en <tld/>ejo; societano, kies
      tasko estas kolekti la kotizojn kaj prizorgi la monon.
    </dif>
    <trdgrp lng="en">
      <trd>treasurer</trd>,
      <trd>paymaster</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="nl">
    <trd>kassier</trd>,
    <trd>penningmeester</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_s;&c_s;&c_i;&c_r;</trd>
  <trd lng="sv">kass&ouml;r</trd>
</drv>

<drv mrk="kas.en0igi">
  <kap>
    en<tld/>igi
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Enspezi monon.
    </dif>
    <trd lng="en">pay in</trd>
    <trd lng="nl">incasseren</trd>
  </snc>
</drv>
</art>
