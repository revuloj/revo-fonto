<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>kas</rad>/o
</kap>
<drv mrk="kas.0o">
   <kap>
     <ofc>*</ofc><tld/>o
   </kap>
   <uzo tip="fak">EKON</uzo>
   <snc>
     <dif>
       Monkesto:
       <ekz>
         regna <tld/>o;
       </ekz>
       <ekz>
         vivi de <ind>&scirc;parita <tld/>o</ind>
         <klr>(mono)</klr><fnt><bib>PrV</bib></fnt>;
         <trd lng="he">&het;&samekh;&kaf;&vav;&nun;&vav;&tav;</trd>
         <trd lng="hu">megtakar&iacute;&aacute;s</trd>
       </ekz>
       <ekz>
         kiu rompis la glason, tiu ordigu la <tld/>on<fnt>Z</fnt>
         <klr>(sk pagante la perdon, &ccirc;ar &ccirc;iu devas 
prirespondi pri siaj faroj)</klr>.
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="posx.0o">po&scirc;o</ref>,
       <ref cel="sak.0o">sako</ref>.
     </refgrp>
   </snc>
   <trd lng="be">&c_k;&c_a;&c_s;&c_a;</trd>
   <trdgrp lng="de">
      <trd>Kasse</trd>,
      <trd>Geldkassette</trd>,
      <trd>Geldkasten</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>cash box</trd>,
     <trd>money box</trd>,
     <trd>till</trd>
   </trdgrp>
   <trdgrp lng="he">
     <trd>&qof;&vav;&pe;&he;<klr tip="ind"> 
(&kaf;&samekh;&fpe;&tav;)</klr></trd>,
     <trd>&alef;&vav;&tsadi;&resh;<klr tip="ind"> 
(&qof;&vav;&pe;&he;)</klr></trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>p&eacute;nzesl&aacute;da</trd>,
     <trd>kassza</trd>,
     <trd>k&eacute;zip&eacute;nzt&aacute;r</trd>,
     <trd>p&eacute;nzkazetta</trd>
   </trdgrp>
   <trd lng="nl">kas</trd>
   <trd lng="fr">caisse</trd>
   <trd lng="ru">&c_k;&c_a;&c_s;&c_s;&c_a;</trd>
   <trd lng="sv">kassa</trd>
</drv>

<drv mrk="kas.0ejo">
   <kap>
     <tld/>ejo
   </kap>
   <uzo tip="fak">EKON</uzo>
   <snc>
     <dif>
       &Ccirc;ambro, destinita por enspezado kaj elspezado de mono, kaj 
en kiu
       oni konservas la <tld/>on..
     </dif>
   </snc>
   <trd lng="be">&c_k;&c_a;&c_s;&c_a;</trd>
   <trdgrp lng="de">
      <trd>Kasse</trd>,
      <trd>Kassenraum</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>cashier's office</trd>,
     <trd>cashpoint</trd>,
     <trd>cashdesk</trd>
   </trdgrp>
   <trd lng="he">&qof;&vav;&pe;&he;<klr tip="ind"> 
(&nun;&qof;&vav;&dalet;&tav; &mem;&kaf;&yod;&resh;&he;)</klr></trd>
   <trdgrp lng="hu">
      <trd>p&eacute;nzt&aacute;r</trd>,
      <trd>p&eacute;nzt&aacute;rhelyis&eacute;g</trd>,
      <trd>p&eacute;nzt&aacute;rterem</trd>
   </trdgrp>
   <trd lng="nl">kas</trd>
   <trd lng="ru">&c_k;&c_a;&c_s;&c_s;&c_a;</trd>
   <trd lng="sv">kassa</trd>
</drv>

<drv mrk="kas.0isto">
   <kap>
     <tld/>isto
   </kap>
   <uzo tip="fak">EKON</uzo>
   <snc>
     <dif>
       Oficisto, kiu en- kaj elspezas monon en <tld/>ejo; societano, kies
       tasko estas kolekti la kotizojn kaj prizorgi la monon.
     </dif>
     <trd lng="be">&c_k;&c_a;&c_s;&c_ib;&c_r;</trd>
     <trdgrp lng="de">
      <trd>Kassierer</trd>,
      <trd>Kassenf&uuml;hrer</trd>,
      <trd>Kassenverwalter</trd>
     </trdgrp>
     <trdgrp lng="en">
       <trd>treasurer</trd>,
       <trd>paymaster</trd>
     </trdgrp>
   </snc>
   <trd lng="he">&gimel;&zayin;&bet;&resh;</trd>
   <trd lng="hu">p&eacute;nzt&aacute;ros</trd>
   <trdgrp lng="nl">
     <trd>kassier</trd>,
     <trd>penningmeester</trd>
   </trdgrp>
   <trd lng="ru">&c_k;&c_a;&c_s;&c_s;&c_i;&c_r;</trd>
   <trd lng="sv">kass&ouml;r</trd>
   <trdgrp lng="fr"><trd>caissier</trd><trd>tr&eacute;sorier;</trd></trdgrp>
</drv>

<drv mrk="kas.en0igi">
   <kap>
     en<tld/>igi
   </kap>
   <gra><vspec>tr</vspec></gra>
   <uzo tip="fak">EKON</uzo>
   <snc>
     <dif>
       Enspezi monon.
     </dif>
     <trd 
lng="be">&c_ib;&c_n;&c_k;&c_a;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
     <trdgrp lng="de">
       <trd>einnehmen</trd>,
       <trd>einziehen</trd>,
       <trd>einkassieren</trd>
     </trdgrp>
     <trd lng="en">pay in</trd>
     <trd lng="fr">encaisser</trd>
     <trd lng="he">&lamed;&he;&kaf;&nun;&yod;&samekh;<klr tip="ind"> 
(&kaf;&samekh;&fpe;)</klr></trd>
     <trdgrp lng="hu">
       <trd>bev&eacute;telez</trd>,
       <trd>behajt</trd>,
       <trd>p&eacute;nzt <ind>beszed</ind></trd>,
       <trd>inkassz&aacute;l</trd>
     </trdgrp>
     <trd lng="nl">incasseren</trd>
   </snc>
</drv>
</art>
