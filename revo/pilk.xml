  <trd lng="be">&c_b;&c_a;&c_s;&c_k;&c_je;&c_t;&c_b;&c_o;&c_l;&c_mol;&c_n;&c_y;
  <ind>&c_m;&c_ja;&c_ch;</ind></trd>
  <trd lng="bo">&t_la;&t_ga;&t_tsheg;&t_ra;&t_tsata;&t_drengbu;&t_da;&t_tsheg;&t_sa;&t_pata;&t_naro;&t_tsheg;&t_la;&t_naro;&t_tsheg;</trd>
