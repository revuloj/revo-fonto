    <uzo tip="fak">SPO</uzo>
    <dif>
      Sporto en kiu du teamoj gajnas poentojn
      man&jcirc;etante <tld/>on tiel ke &gcirc;i eniru la
      <ref tip="vid" cel="gol.0ejo">golejon</ref> (en la formo de
      ortangulo) de kontra&ubreve;uloj;
      <ref tip="sin" cel="handbal.0o.SPO">handbalo</ref>:
      <ekz>
        olimpika &ccirc;ampionado de man<tld/>ado
        <fnt>
          <aut>T. Ujlaky-Nagy</aut>,
          <vrk>La sporta lingvo en Esperanto</vrk>.
          <lok>1972</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_n;&c_d;&c_b;&c_o;&c_l;</trd>
  <trd lng="bg">&c_h;&c_a;&c_n;&c_d;&c_b;&c_a;&c_l;</trd>
  <trd lng="ca">handbol</trd>
  <trd lng="cs">h&aacute;zen&aacute;</trd>
