<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
 <ofc>*</ofc>
 <rad>pilk</rad>/o
</kap>
<drv mrk="pilk.0o">
 <kap><ofc>*</ofc><tld/>o</kap>
 <snc num="1">
  <uzo tip="fak">SPO</uzo>
  <dif>
   Luda &jcirc;etbulo, ordinare el elasta materio. 
  </dif>
 <trdgrp lng="fr">
   <trd>balle</trd>,
   <trd>ballon<klr tip="ind"> (balle)</klr></trd></trdgrp> 
 <trd lng="sv">boll</trd>
 <trd lng="tr">top</trd>
 </snc>
 <snc num="2">
  <uzo tip="fak">TEK</uzo>
  <uzo tip="fak">MED</uzo>
  <dif>
   Pirforma ka&ubreve;&ccirc;uka ujo, kiu fini&gcirc;as per tubeto
   k kiun oni premas por eligi la entenatan aeron a&ubreve; flua&jcirc;on.
  </dif>
  <trd lng="fr">poire<klr tip="ind"> (en caoutchouc)</klr></trd>
 </snc>
 <trd lng="en">ball</trd>
 <trd lng="es">bal&oacute;n</trd>
 <trd lng="de">Ball</trd>
 <trd lng="nl">bal</trd>
 <trd lng="pl">pi&lstroke;ka</trd>
</drv>
</art>
