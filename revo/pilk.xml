<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pilk</rad>/o
</kap>
<drv mrk="pilk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">SPO</uzo>
    <dif>
      Luda &jcirc;etbulo, ordinare el elasta materio.
    </dif>
    <trd lng="be">&c_m;&c_ja;&c_ch;</trd>
    <trdgrp lng="fr">
      <trd>balle</trd>,
      <trd>ballon<klr tip="ind"> (balle)</klr></trd>
    </trdgrp>
    <trd lng="hu">labda</trd>
    <trd lng="pt">bola</trd>
    <trd lng="ru">&c_m;&c_ja;&c_ch;</trd>
    <trd lng="sv">boll</trd>
    <trd lng="tr">top</trd>
  </snc>
  <snc>
    <uzo tip="fak">TEK</uzo>
    <uzo tip="fak">MED</uzo>
    <dif>
      Pirforma ka&ubreve;&ccirc;uka ujo, kiu fini&gcirc;as per tubeto
      kaj kiun oni premas por eligi la entenatan aeron a&ubreve; flua&jcirc;on.
    </dif>
    <trd lng="fr">poire<klr tip="ind"> (en caoutchouc)</klr></trd>
    <trd lng="ru">&c_g;&c_r;&c_u;&c_sh;&c_a; <klr tip="amb">(&c_r;&c_je;&c_z;&c_i;&c_n;&c_o;&c_v;&c_a;&c_ja;)</klr></trd>
  </snc>
  <trd lng="en">ball</trd>
  <trd lng="es">bal&oacute;n</trd>
  <trd lng="de">Ball</trd>
  <trd lng="nl">bal</trd>
  <trd lng="pl">pi&lstroke;ka</trd>
</drv>
</art>
