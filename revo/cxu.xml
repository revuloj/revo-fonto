  <trd lng="kk">&#x43C;&c_a;</trd>
  <trd lng="km">&#6032;&#x17B6;&#x178F;&#x17BE;</trd>
  <trd lng="kn">&#xC8E;&#xC82;&#xCAC;&#xCC1;&#xCA6;&#xCA8;&#xCCD;&#xCA8;&#xCC1;</trd>
  <trd lng="ko">&#xC5EC;&#xBD80;</trd>
  <trd lng="ku">gelo</trd>
  <trd lng="ky">&c_zh;&c_je;</trd>
  <trd lng="lo">&#3738;&#3789;&#3784;&#3751;&#3784;&#3762;&#3720;&#3760;&#3776;&#3739;&#3761;&#3737;</trd>
  <trd lng="mi">ahakoa</trd>
