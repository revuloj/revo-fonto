  <trd lng="th">&#3652;&#3617;&#3656;&#3623;&#3656;&#3634;&#3592;&#3632;&#3648;&#3611;&#3655;&#3609;</trd>
  <trd lng="tl">kung</trd>
  <trdgrp lng="tr">
     <trd>m&inodot;</trd>,
     <trd>mi</trd>,
