     <trd>m&uuml;</trd>
   </trdgrp>
   <trd lng="ur">&f_ch;&a_A;&#1729;&#1746;</trd>
  <trd lng="vi">li&#7879;u</trd>
  <trd lng="vo">-li</trd>
   <trd lng="yi">&tsadi;&yod;</trd>
  <trdgrp lng="zh">
