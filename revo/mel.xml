<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mel</rad>/o
</kap>
<drv mrk="mel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="mel.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="mustel1.0oj">musteledoj</ref>
      (<trd lng="la">Meles</trd>), mezgranda malbonodora mambesto
      kun nigre blanke striata kapo kaj plataj piedoj por faciligi la
      fosadon.
    </dif>
  </snc>                     
  <trd lng="be">&c_b;&c_a;&c_r;&c_s;&c_u;&c_k;</trd>
  <trd lng="br">broc'h<klr tip="ind"> (loen)</klr></trd>
  <trd lng="cs">jezevec</trd>
  <trd lng="de">Dachs</trd>
  <trd lng="en">badger</trd>
  <trd lng="fr">blaireau</trd>
  <trd lng="it">tasso</trd>
  <trd lng="hu">borz</trd>
  <trd lng="nl">das<klr tip="ind"> (dier)</klr></trd>
  <trd lng="ru">&c_b;&c_a;&c_r;&c_s;&c_u;&c_k;</trd>
</drv>
</art>
