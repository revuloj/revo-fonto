<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mel</rad>/o
</kap>
<drv mrk="mel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="mel.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Mezgranda malbonodora <ref tip="lst" cel="best.rabo0o"
      lst="voko:rabobestoj">rabobesto</ref> kun griza a&ubreve; nigra felo kaj
      nigre blanke striata kapo, plataj piedoj por faciligi la
      fosadon, sin nutranta de malgrandaj vertebruloj, insektoj sed
      anka&ubreve; plantoj;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt"
      cel="mustel1.0oj">musteledoj</ref>
      (<trd lng="la">Meles</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_r;&c_s;&c_u;&c_k;</trd>
  <trd lng="br">broc'h<klr tip="ind"> (loen)</klr></trd>
  <trd lng="cs">jezevec</trd>
  <trd lng="de">Dachs</trd>
  <trd lng="en">badger</trd>
  <trd lng="es">tej&oacute;n</trd>
  <trd lng="fr">blaireau</trd>
  <trd lng="hu">borz</trd>
  <trd lng="id">luak</trd>
  <trd lng="it">tasso</trd>
  <trd lng="nl">das<klr tip="ind"> (dier)</klr></trd>
  <trdgrp lng="pl">
    <trd>borsuk</trd>,
    <trd>ja&zacute;wiec</trd>
  </trdgrp>
  <trd lng="pt">texugo</trd>
  <trd lng="ru">&c_b;&c_a;&c_r;&c_s;&c_u;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>borsuk</trd>,
    <trd>jazvec</trd>
  </trdgrp>
</drv>
</art>
