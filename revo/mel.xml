<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mel</rad>/o
</kap>
<drv mrk="mel.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="mustel1.0oj">musteledoj</ref>
      (<trd lng="la">Meles</trd>), malgranda malbonodora mambesto
      kun nigre blanke striata kapo kaj plataj piedoj por faciligi la
      fosadon.
    </dif>
  </snc>
  <trd lng="de">Dachs</trd>
  <trd lng="en">badger</trd>
  <trd lng="nl">das<klr tip="ind"> (dier)</klr></trd>
  <trd lng="ru">&c_b;&c_a;&c_r;&c_s;&c_u;&c_k;</trd>
</drv>

<drv mrk="mel.0hundo">
  <kap>
    <tld/>hundo
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Raso de <ref tip="super" cel="hund.0o.dombesto">hundo</ref>
      kun mallongaj kruroj aparte ta&ubreve;gantaj por
      &ccirc;asi bestojn lo&gcirc;antajn en tertruoj.
    </dif>
  </snc>
  <trd lng="de">Dackel</trd>
  <trd lng="en">dachshund</trd>
  <trd lng="nl">dashond</trd>
  <trd lng="ru">&c_t;&c_a;&c_k;&c_s;&c_a; <klr tip="amb">(&c_p;&c_o;&c_r;&c_o;&c_d;&c_a; &c_s;&c_o;&c_b;&c_a;&c_k;)</klr></trd>
</drv>
</art>
