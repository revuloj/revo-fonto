<drv mrk="hejm.0a">
  <kap><tld/>a</kap>
  <snc mrk="hejm.0a.rilata">
    <dif>
      De <tld/>o, rilata al <tld/>o:
      <ekz>
        <tld/>a lingvo
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a lando
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili flugis super la <tld/>aj lokoj
        <fnt>
          <bib>Fab2</bib>
          <lok>An&gcirc;elo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li sentis kvaza&ubreve; odoron el la <tld/>aj arbaroj
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        nia lingvo <klr>[&#8230;]</klr> estas al ili <klr>[&#8230;]</klr> <tld/>a
        <klr>(malfremda)</klr>
        <fnt>
          <bib>LR</bib>
          <lok>4. Vortaroj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a <klr>(negli&gcirc;a)</klr> vesto;
      </ekz>
      <ekz>
        <tld/>a dometo estas kiel patrineto
        <fnt><bib>PrV</bib></fnt>
      </ekz>
      <ekz>
        <tld/>aj dia&jcirc;oj.
      </ekz>
      <ekz>
        sen<tld/>aj malri&ccirc;uloj
        <fnt><bib>MT</bib> <lok>&Jes; 58:7</lok></fnt>;
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="famili1.0a">familiara</ref>,
      <ref cel="intim.0a">intima</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_h;&c_a;&c_t;&c_n;&c_ib;</trd>,
      <trd>&c_d;&c_a;&c_m;&c_a;&c_sh;&c_n;&c_ib;</trd>,
      <trd>&c_s;&c_v;&c_o;&c_j;</trd>,
      <trd>&c_r;&c_o;&c_d;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>otthoni</trd>,
      <trd>honi</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>domowy</trd>,
      <trd>rodzinny</trd>,
      <trd>ojczysty</trd>
    </trdgrp>
  </snc>
  <snc mrk="hejm.0a.apudhejma">
    <dif>
      Vivanta &ccirc;e <tld/>o:
      <ekz>
        <ind><tld/>a besto</ind>;
        <trd lng="de">Haustier</trd>
        <trd lng="fr">animal domestique</trd>
        <trd lng="hu">h&aacute;zi&aacute;llat</trd>
        <trd lng="nl">huisdier</trd>
        <trd lng="no">husdyr</trd>
        <trd lng="pl">zwierz&eogonek; <ind>domowe</ind></trd>
        <trd lng="ru">&c_d;&c_o;&c_m;&c_a;&c_sh;&c_n;&c_je;&c_je; &c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_o;&c_je;</trd>
      </ekz>
      <ekz>
        sur la herbejo sin pa&scirc;tis sennombraj aroj de <tld/>aj bestoj: bovoj senkornaj
        kaj kornaj, &scirc;afinoj, kaprinoj, azenoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="sovagx.mal0a">malsova&gcirc;a</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_h;&c_a;&c_t;&c_n;&c_ib;</trd>,
      <trd>&c_s;&c_v;&c_o;&c_j;&c_s;&c_k;&c_ib;</trd>
    </trdgrp>
    <trd lng="hu">h&aacute;zi</trd>
    <trd lng="pl">domowy</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>dom&aacute;ck&yacute;</trd>,
    <trd>&uacute;tuln&yacute;</trd>
  </trdgrp>
  <trd lng="de">heimisch</trd>
  <trd lng="es">dom&eacute;stico</trd>
  <trdgrp lng="fr">
    <trd>domestique</trd>,
    <trd>familier</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>domestik</trd>,
    <trd>familier</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>domestico<klr tip="amb"> (agg.)</klr></trd>,
    <trd>casalingo</trd>,
    <trd>famigliare</trd>, <trd>familiare</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5BB6;&#x5EAD;&#x306E;  <pr>&#x304B;&#12390;&#12356;&#x306E;</pr></trd>,
    <trd>&#x81EA;&#x5B85;&#x306E;  <pr>&#12376;&#x305F;&#x304F;&#x306E;</pr></trd>,
    <trd>&#x98FC;&#12356;&#x306A;&#12425;&#12373;&#x308C;&#x305F;  <pr>&#x304B;&#12356;&#x306A;&#12425;&#12373;&#x308C;&#x305F;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>thuis</trd>,
    <trd>huis-</trd>
  </trdgrp>
  <trd lng="no">hus-</trd>
  <trd lng="pt">dom&eacute;stico</trd>
  <trd lng="ru">&c_d;&c_o;&c_m;&c_a;&c_sh;&c_n;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>dom&aacute;cky</trd>,
    <trd>&uacute;tuln&yacute;</trd>
  </trdgrp>
  <trd lng="tok">tomo</trd>
  <trd lng="uk">&c_d;&#x43E;&#x43C;&c_a;&c_sh;&#x43D;&c_ib;&c_j;</trd>
</drv>

