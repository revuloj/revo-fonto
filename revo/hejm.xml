<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>hejm</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="hejm.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="hejm.0o.KOMUNE">
    <dif>
      Loko, lo&gcirc;ejo a&ubreve; konstrua&jcirc;o en kiu koncernulo
      vivas konstante, precipe la familia restadejo:
      <ekz>
        vagulo ne havanta <tld/>on<fnt>B</fnt>;
      </ekz>
      <ekz>
        Jehuda estis <ind>el<tld/>igi</ind>ta el sia lando
        <fnt><bib>MT</bib>, <lok>II. Re&gcirc;oj 25:21</lok></fnt>.
        <trd lng="fr">expulser<klr tip="ind"> (de son domicile)</klr>
        </trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dom.0o">domo</ref>,
      <ref cel="famili.0o">familio</ref>,
      <ref cel="mastr.0umi">mastrumi</ref>
    </refgrp>
    <trd lng="es">hogar</trd> 
    <trd lng="hu">otthon</trd>
    <trd lng="it">casa<klr tip="amb"> (propria dimora)</klr></trd>
    <trdgrp lng="pl">
      <trd>dom<klr> (rodzinny)</klr></trd>,
      <trd>pielesze</trd>,
      <trd>ognisko domowe</trd>
    </trdgrp>
    <trd lng="pt">lar</trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_o;&c_m;
      <klr>(&c_s;&c_v;&c_o;&c_j;, &c_r;&c_o;&c_d;&c_n;&c_o;&c_j;)</klr>
      </trd>,
      <trd>
      &c_d;&c_o;&c_m;&c_a;&c_sh;&c_n;&c_i;&c_j; &c_o;&c_ch;&c_a;&c_g;
      </trd>
    </trdgrp>
    <trd lng="tp">tomo</trd>
  </snc>
  <snc mrk="hejm.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
       Komenca, deira a&ubreve; normala pozicio:
       <ekz>
         <uzo tip="fak">KOMP</uzo>
         en TTT, <ind><tld/>pa&gcirc;o</ind> estas tiu hiperteksta
         pa&gcirc;o per kiu oni komencas esplori ies pa&gcirc;aron;
         <trd lng="en">home page</trd>
         <trdgrp lng="fr">
            <trd>page d'accueil</trd>,
            <trd>page principale</trd>,
            <trd>vitrine</trd>
         </trdgrp>
         <trd lng="hu">honlap</trd>
         <trd lng="it">pagina iniziale</trd>
         <trd lng="pt">p&aacute;gina inicial</trd>
         <trd lng="ru">
         &c_d;&c_o;&c_m;&c_a;&c_sh;&c_n;&c_ja;&c_ja;
         &c_s;&c_t;&c_r;&c_a;&c_n;&c_i;&c_ch;&c_k;&c_a;
         </trd>
       </ekz><ekz>
          <uzo tip="fak">KOMP</uzo>
          la &leftquot;<ind><tld/>en-klavo</ind>&rightquot; movas la
          kursoron en ties &leftquot;<tld/>an
          pozicion&rightquot;, al la komenco de linio a&ubreve; ekrano.
          <trd lng="en">home key</trd>
          <trd lng="it">inizio<klr tip="amb"> (tasto
          computer)</klr></trd>
          <trd lng="hu">home-billenty&udblac;</trd>
       </ekz>
     </dif>
     <trdgrp lng="de">
       <trd>Anfangsposition</trd>, <trd>Anfangsstellung</trd>,
       <trd>Nulllage</trd>
     </trdgrp>
     <trdgrp lng="hu">
       <trd>home</trd>,
       <trd>kiindul&aacute;si pont</trd>
     </trdgrp>
     <trd lng="it">home<klr tip="amb"> (inf.)</klr></trd>
     <trdgrp lng="ru">
       <trd>&c_n;&c_a;&c_ch;&c_a;&c_l;&c_o;</trd>,
       <trd>
       &c_i;&c_s;&c_h;&c_o;&c_d;&c_n;&c_a;&c_ja;
       &c_p;&c_o;&c_z;&c_i;&c_c;&c_i;&c_ja;
       </trd>,
       <trd>
       &c_n;&c_o;&c_r;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_je;
       &c_p;&c_o;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;
       </trd>
     </trdgrp>
  </snc>
  <trd lng="cs">domov</trd>
  <trdgrp lng="de">
    <trd>Zuhause</trd>,
    <trd>Heim</trd>
  </trdgrp>
  <trd lng="en">home</trd>
  <trdgrp lng="fr">
    <trd>chez-soi</trd>,
    <trd>appartement<klr tip="ind"> (chez-soi)</klr></trd>,
    <trd>domicile<klr tip="ind"> (chez-soi)</klr></trd>,
    <trd>foyer<klr tip="ind"> (chez-soi)</klr></trd>,
    <trd>maison<klr tip="ind"> (chez-soi)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>thuis</trd>,
    <trd>tehuis</trd>
  </trdgrp>
  <trd lng="no">hjem</trd>
  <trd lng="sv">hem</trd>
</drv>

<drv mrk="hejm.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      De <tld/>o, rilata al <tld/>o:
      <ekz>
        <tld/>a lando, lingvo;
      </ekz>
      <ekz>
        nia lingvo estas al ili <tld/>a<fnt>Z</fnt>
        <klr>(malfremda)</klr>;
      </ekz>
      <ekz>
        <tld/>a <klr>(negli&gcirc;a)</klr> vesto;
      </ekz>
      <ekz><tld/>a dometo estas kiel patrineto
        <fnt><bib>PrV</bib></fnt></ekz>
      <ekz>
        <tld/>aj dia&jcirc;oj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="famili1.0a">familiara</ref>,
      <ref cel="intim.0a">intima</ref>.
    </refgrp>
    <trdgrp lng="hu">
      <trd>otthoni</trd>,
      <trd>honi</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>domowy</trd>,
      <trd>rodzinny</trd>,
      <trd>ojczysty</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Vivanta &ccirc;e <tld/>o:
      <ekz>
        <ind><tld/>a besto</ind>;
        <trd lng="de">Haustier</trd>
        <trd lng="fr">animal domestique</trd>
        <trd lng="hu">h&aacute;zi&aacute;llat</trd>
        <trd lng="nl">huisdier</trd>
        <trd lng="no">husdyr</trd>
        <trd lng="ru">
        &c_d;&c_o;&c_m;&c_a;&c_sh;&c_n;&c_je;&c_je;
        &c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_o;&c_je;
        </trd>
      </ekz>
      <ekz>
        <tld/>aj katoj, &scirc;afinoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="sovagx.mal0a">malsova&gcirc;a</ref>.
    </refgrp>
    <trd lng="hu">h&aacute;zi</trd>
    <trd lng="pl">domowy</trd>
  </snc>
  <trd lng="es">dom&eacute;stico</trd> 
  <trdgrp lng="fr">
    <trd>domestique</trd>,
    <trd>familier</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>domestico<klr tip="amb"> (agg.)</klr></trd>, 
    <trd>casalingo</trd>,
    <trd>famigliare</trd>, <trd>familiare</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>thuis</trd>,
    <trd>huis-</trd>
  </trdgrp>
  <trd lng="no">hus-</trd>
  <trd lng="pt">dom&eacute;stico</trd>
  <trd lng="ru">&c_d;&c_o;&c_m;&c_a;&c_sh;&c_n;&c_i;&c_j;</trd>
  <trd lng="tp">tomo</trd>
</drv>

<drv mrk="hejm.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      En sia <tld/>o:
      <ekz>
        &ccirc;u s-ro N. estas <tld/>e?;
      </ekz>
      <ekz>
        reveni <tld/>en;
      </ekz>
      <ekz>
        senti sin kiel <tld/>e
        <klr>(ne &gcirc;eni sin, sidi komforte)</klr>;
      </ekz>
      <ekz>
        al &scirc;i fari&gcirc;is en la koro ankora&ubreve; pli agrable
        kaj <tld/>e<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiu &ccirc;i ideo en la publiko ne estas tiel <tld/>e <klr>
        (kutime akceptata)</klr>, kiel &gcirc;i devos esti<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">doma</trd>
  <trd lng="de">zuhause</trd>
  <trdgrp lng="fr">
    <trd>&agrave; la <ind>maison</ind></trd>,
    <trd>chez soi</trd>
  </trdgrp>
  <trd lng="hu">otthon</trd>
  <trd lng="nl">thuis</trd>
  <trd lng="no">hjemme</trd>
  <trd lng="pl">w <ind>domu</ind></trd>
  <trd lng="pt">em <ind>casa</ind><klr tip="ind"> (adv.)</klr></trd>
  <trd lng="ru">&c_d;&c_o;&c_m;&c_a;</trd>
  <trd lng="sv">hemma</trd>
</drv>

<drv mrk="hejm.0i">
  <kap><tld/>i</kap>
  <snc mrk="hejm.0i.KOMUNE">
    <dif>
      Resti en <tld/>o:
      <ekz>
        domo, en kiu mia familio <tld/>is
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://es.chinabroadcast.cn">
          Libro Flosa, Son&gcirc;o Flua</url></vrk>,
          <lok>&Ccirc;ina Radio Internacia, 2007-11-23</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="hejm.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Esti en sia normala, konvena loko a&ubreve; medio:
      <ekz>
        <tld/>as anka&ubreve; en la franca tiu dirmaniero
        <fnt>
          <aut>Filu</aut>,
          <vrk>-</vrk>,
          <lok>lernu.net, 2007-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">estar es casa</trd>
  <trdgrp lng="fr">
    <trd>&ecirc;tre &agrave; la maison</trd>,
    <trd>&ecirc;tre chez soi</trd>
  </trdgrp>
  <trd lng="it">stare a <ind>casa</ind></trd>
</drv>

<drv mrk="hejm.0eca">
  <kap><tld/>eca<fnt>Z</fnt></kap>
  <snc mrk="hejm.0eca.KOMUNE">
    <dif>
      Kvaza&ubreve; <tld/>a.
    </dif>
    <ref tip="vid" cel="komfor.0a">komforta</ref>
  </snc>
  <trdgrp lng="es">
    <trd>hogare&ntilde;o</trd>,
    <trd>casero</trd>
  </trdgrp>
  <trd lng="fr">familier</trd>
  <trd lng="hu">otthonos</trd>
  <trdgrp lng="it">
    <trd>domestico<klr tip="amb"> (agg.)</klr></trd>, 
    <trd>casalingo</trd>,
    <trd>famigliare</trd>, <trd>familiare</trd>
  </trdgrp>
  <trd lng="nl">huiselijk</trd>
  <trdgrp lng="pl">
    <trd>przytulny</trd>,
    <trd>familiarny</trd>,
    <trd>znajomy</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_m;&c_a;&c_sh;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_je;&c_m;&c_je;&c_j;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_i;&c_n;&c_t;&c_i;&c_m;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="hejm.0igxi">
  <kap><tld/>i&gcirc;i, <var><kap>en<tld/>i&gcirc;i</kap></var></kap>
  <snc mrk="hejm.0igxi.KOMUNE">
    <dif>
      Fari iun lokon sia <tld/>o.
    </dif>
    <refgrp tip="vid">
      <ref cel="logx.en0igxi">enlo&gcirc;i&gcirc;i</ref>,
      <ref cel="instal.0igxi">instali&gcirc;i</ref>
    </refgrp>
  </snc>
  <snc mrk="hejm.0igxi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Adapti&gcirc;i al iu loko, medio:
      <ekz>
        post kelkaj tagoj li plene <tld/>i&gcirc;is inter la fulgonigraj
        viroj de la <klr>...</klr> uzinoj
        <fnt>
          <aut>S. Engholm</aut>,
          <vrk>Al Torento</vrk>,
          <lok>1930</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili ne povis, a&ubreve; ne volis adapti&gcirc;i,
        en<tld/>i&gcirc;i
        <fnt>
          <aut>J. Drahotov&aacute;</aut>,
          <vrk>Miskompreniga</vrk>,
          <bib>Monato</bib>,
          <lok>[2001]</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akomod.0igxi">akomodi&gcirc;i</ref>,
      <ref cel="komfor.0igxi">komforti&gcirc;i</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>s'<ind>acclimater</ind><klr> (fig.)</klr></trd>,
    <trd>s'<ind>installer</ind></trd>
  </trdgrp>
  <trd lng="pl">zadomawia&cacute; si&eogonek;</trd>
</drv>

<drv mrk="hejm.el0igxi">
  <kap>el<tld/>i&gcirc;i</kap>
  <snc mrk="hejm.el0igxi.KOMUNE">
    <dif>
      Eliri el sia <tld/>o:
      <ekz>
        Fradeko el<tld/>i&gcirc;is matene
        <fnt>
          <aut>H. Vallienne</aut>,
          <vrk>&Ccirc;u li?</vrk>,
          <lok>1908</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">sortir de chez soi</trd>
  <trd lng="it">uscire di <ind>casa</ind></trd>
  <trdgrp lng="pl">
    <trd>wychodzi&cacute; z domu</trd>,
    <trd>opuszcza&cacute; dom</trd>
  </trdgrp>
</drv>

<drv mrk="hejm.re0igxi">
  <kap>re<tld/>i&gcirc;i</kap>
  <snc mrk="hejm.re0igxi.KOMUNE">
    <dif>
      Reiri en sian <tld/>on:
      <ekz>
        San&hcirc;erib, la re&gcirc;o de Asirio, elmovi&gcirc;is kaj
        iris kaj re<tld/>i&gcirc;is kaj restis en Nineve
        <fnt><bib>MT</bib>, <lok>&Re2; 19:36</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>rentrer &agrave; la maison</trd>,
    <trd>revenir chez soi</trd>
  </trdgrp>
  <trd lng="it">rincasare</trd>
  <trdgrp lng="pl">
    <trd>wchodzi&cacute; do domu</trd>,
    <trd>wraca&cacute; do domu</trd>
  </trdgrp>
</drv>

<drv mrk="hejm.sen0ulo">
  <kap>sen<tld/>ulo</kap>
  <snc mrk="hejm.sen0ulo.KOMUNE">
    <dif>
      Vagulo.
    </dif>
    <refgrp tip="vid">
      <ref cel="migr.0i">migri</ref>,
      <ref cel="nomad.0o">nomado</ref>
    </refgrp>
  </snc>
  <trd lng="de">Obdachloser</trd>
  <trdgrp lng="fr">
    <trd>sans-abri<klr> (subst.)</klr></trd>,
    <trd>sans-logis<klr> (subst.)</klr></trd>,
    <trd>SDF</trd>,
    <trd>vagabond<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="hu">hajl&eacute;ktalan</trd>
  <trd lng="it">senza fissa <ind>dimora</ind></trd>
  <trd lng="nl">dakloze</trd>
  <trd lng="no">husl&oslash;se</trd>
  <trdgrp lng="pl">
    <trd>bezdomny</trd>,
    <trd>w&lstroke;&oacute;cz&eogonek;ga</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_d;&c_o;&c_m;&c_n;&c_y;&c_j;
<klr>(&c_s;&c_u;&c_shch;.)</klr></trd>
  <trd lng="sv">heml&ouml;s</trd>
  <trd lng="pt">sem-teto</trd>
</drv>

<drv mrk="hejm.fiks0a">
  <kap>fiks<tld/>a</kap>
  <snc>
    <dif>
      Nemigra, <ctl>loksida</ctl>, kies <tld/>o estas fiksita
      en difinita loko, kontraste al la nomadoj, kiuj kunportas sian
      <tld/>on dum siaj migradoj:
      <ekz>
        konstanta fiks<tld/>a lo&gcirc;anto
        <fnt><bib>Homaranismo</bib></fnt>;
      </ekz>
      <ekz>
        la beduenoj kontraste al la fiks<tld/>aj araboj.
      </ekz>
    </dif>
    <ref tip="ant" cel="nomad.0a">nomada</ref>
  </snc>
  <adm>
    La vorto "loksida" aperanta en la dif ne estas en
    Revo, kiu tamen konas "loksidi&gcirc;i" (la&ubreve; mi tre
    malofte uzata). <aut>MB</aut>
  </adm>
  <trdgrp lng="de">
     <trd>sesshaft</trd>,
     <trd>bodenst&auml;ndig</trd>
  </trdgrp>
  <trd lng="es">sedentario<klr tip="amb"> (opuesto a
  n&oacute;mada)</klr></trd> 
  <trdgrp lng="fr">
    <trd>s&eacute;dentaire<klr tip="ind">
    (par opp. &agrave; nomade)</klr></trd>
  </trdgrp>
  <trd lng="hu">letelepedett</trd>
  <trd lng="it">stanziale</trd>
  <trd lng="nl">vasthuizig</trd>
  <trd lng="pl">osiad&lstroke;y</trd>
  <trd lng="pt">sedent&aacute;rio<klr tip="amb">
  (de resid&ecirc;ncia fixa)</klr></trd>
  <trd lng="ru">&c_o;&c_s;&c_je;&c_d;&c_l;&c_y;&c_j;</trd>
</drv>

<drv mrk="hejm.0ama">
  <kap><tld/>ama</kap>
  <snc>
    <dif>
      &Scirc;atanta restadi <tld/>e.
    </dif>
    <refgrp tip="sin">
      <ref cel="sid.forno0a.hejmama">fornosida</ref>,
      <ref cel="hejm.0osida">hejmosida</ref>
    </refgrp>
  </snc>
  <trd lng="de">h&auml;uslich</trd>
 <trdgrp lng="es">
   <trd>hogare&ntilde;o<klr tip="amb"> (que sale poco de casa)</klr></trd>,
   <trd>casero<klr tip="amb"> (que sale poco de casa)</klr></trd>
 </trdgrp>  
  <trdgrp lng="fr">
    <trd>casanier</trd>,
    <trd>pantouflard</trd>,
    <trd>s&eacute;dentaire<klr tip="ind"> (casanier)</klr></trd>
  </trdgrp>
  <trd lng="hu">otthonszeret&odblac;</trd>
  <trdgrp lng="it">
    <trd>casalingo</trd>,
    <trd>pantofolaio<klr> (fig.)</klr></trd>
  </trdgrp>
  <trd lng="nl">huiselijk</trd>
  <trd lng="pl">domatorski</trd>
  <trd lng="pt">caseiro<klr tip="ind"> (adj.)</klr></trd>
</drv>

<drv mrk="hejm.0osida">
  <kap><tld/>osida</kap>
  <snc>
    <ref tip="dif" cel="hejm.0ama"><tld/>ama</ref>
  </snc>
  <trd lng="de">h&auml;uslich</trd>
  <trdgrp lng="fr">
    <trd>casanier</trd>,
    <trd>s&eacute;dentaire<klr tip="ind"> (casanier)</klr></trd>
  </trdgrp>
  <trd lng="hu">otthon&uuml;l&odblac;</trd>
  <trdgrp lng="it">
    <trd>casalingo</trd>,
    <trd>pantofolaio<klr> (fig.)</klr></trd>, 
    <trd>sedentario<klr> (pantofolaio)</klr></trd>
  </trdgrp>
  <trd lng="nl">huiselijk</trd>
  <trd lng="pt">caseiro<klr tip="ind"> (adj.)</klr></trd>
</drv>
</art>
