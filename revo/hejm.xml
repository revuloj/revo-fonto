  </trdgrp>
  <trd lng="pt">sem-teto</trd>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_d;&c_o;&c_m;&c_n;&c_y;&c_j;
  <klr>(&c_s;&c_u;&c_shch;.)</klr></trd>
  <trd lng="sk">bezdomovec</trd>
  <trd lng="sv">heml&ouml;s</trd>
<trd lng="th">&#xE04;&#xE19;&#xE44;&#xE23;&#xE49;&#xE1A;&#xE49;&#xE32;&#xE19;</trd>
  <trdgrp lng="uk">
