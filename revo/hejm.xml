<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>hejm</rad>/o
</kap>
<drv mrk="hejm.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Propra aparta domo a&ubreve; lo&gcirc;ejo, kie oni vivas
      konstante kaj mastre; familia restadejo:
      <ekz>
        vagulo ne havanta <tld/>on<fnt>B</fnt>;
      </ekz>
      <ekz>
        Jehuda estis el<tld/>igita el sia lando
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>II. Re&gcirc;oj 25:21</lok>)</fnt>;
      </ekz>
      <ekz>
        konstanta fiks<tld/>a lo&gcirc;anto
        <fnt>(<aut>Zamenhof</aut>: <vrk>Homaranismo</vrk>)</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dom.0o">domo</ref>,
      <ref cel="famili.0o">familio</ref>,
      <ref cel="mastr">mastr(um)a&jcirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="cs">domov</trd>
  <trdgrp lng="de">
    <trd>Zuhause</trd>,
    <trd>Heim</trd>
  </trdgrp>
  <trd lng="en">home</trd>
</drv>
<drv mrk="hejm.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      De <tld/>o, rilata al <tld/>o:
      <ekz>
        <tld/>a lando, lingvo;
      </ekz>
      <ekz>
        nia lingvo estas al ili <tld/>a<fnt>Z</fnt>
        <klr>(malfremda)</klr>;
      </ekz>
      <ekz>
        <tld/>a <klr>(negli&gcirc;a)</klr> vesto;
      </ekz>
      <ekz>
        <tld/>a dometo estas kiel patrineto
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
      </ekz>
      <ekz>
        <tld/>aj dia&jcirc;oj.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="famili1.0a">familiara</ref>,
      <ref cel="intim.0a">intima</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Vivanta. &ccirc;e <tld/>o:
      <ekz>
        <ind><tld/>a besto</ind>;
        <trd lng="de">Haustier</trd>
      </ekz>
      <ekz>
        <tld/>aj katoj, &scirc;afinoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="sovagx.mal0a">malsova&gcirc;a</ref>.
    </refgrp>
  </snc>
</drv>
<drv mrk="hejm.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      En sia <tld/>o:
      <ekz>
        &ccirc;u s-ro N. estas <tld/>e?
      </ekz>
      <ekz>
        reveni <tld/>en;
      </ekz>
      <ekz>
        senti sin kiel <tld/>e
        <klr>(ne &gcirc;eni sin, sidi komforte)</klr>;
      </ekz>
      <ekz>
        al &scirc;i fari&gcirc;is en la koro ankora&ubreve; pli agrable kaj
        <tld/>e<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiu &ccirc;i ideo en la publiko ne estas tiel <tld/>e (kutime
        akceptata), kiel &gcirc;i devos esti<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">doma</trd>
  <trd lng="de">zuhause</trd>
</drv>
<drv mrk="hejm.0eca">
  <kap><tld/>eca<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Kvaza&ubreve; <tld/>a
    </dif>
    <ref tip="vid" cel="komfor.0a">komforta</ref>
  </snc>
</drv>
<drv mrk="hejm.0ama0osida">
  <kap><tld/>ama, <tld/>osida</kap>
  <snc>
    <dif>
      &Scirc;atanta restadi <tld/>e.
    </dif>
  </snc>
  <trd lng="de">h&auml;slich</trd>
</drv>
<drv mrk="hejm.sen0ulo">
  <kap>sen<tld/>ulo</kap>
  <snc>
    <dif>
      Vagulo.
    </dif>
    <refgrp tip="sin">
      <ref cel="nomad.0o">nomado</ref>,
      <ref cel="migr.0i">migri</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Obdachloser</trd>
</drv>
<drv mrk="hejm.0veo">
  <kap><tld/>veo</kap>
  <snc>
    <dif>
      Dolora sopiro al <tld/>o.
    </dif>
    <ref tip="sin" cel="nostal.0o">nostalgio</ref>
  </snc>
  <trd lng="de">Heimweh</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
