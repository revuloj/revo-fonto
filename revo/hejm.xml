<drv mrk="hejm.sen0ulo">
  <kap>sen<tld/>ulo</kap>
  <snc mrk="hejm.sen0ulo.KOMUNE">
    <dif>
      Vagulo:
      <ekz>
        triono de la sen<tld/>uloj sur la stratoj de Britio estas eksarmeanoj
        <fnt>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2003/008119.html">Taglibro de l' milito ... el la lando de
          &Scirc;ekspiro</url></vrk>
          <bib>Monato</bib>
          <lok>2003/05, p. 9</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estas a&ubreve; sen<tld/>ulo a&ubreve; drogulo
        <fnt>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2005/008463.php">Strangulo en la naturo</url></vrk>
          <bib>Monato</bib>
          <lok>2005/04, p. 22</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam ili ne havas lo&gcirc;ejon, ili ne povas ser&ccirc;i alian laboron, &ccirc;ar kompanioj ne
        volas dungi sen<tld/>ulojn
        <fnt>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="&Monato;/2009/009770.php">Vila&gcirc;o por maldungitoj</url></vrk>
          <bib>Monato</bib>
          <lok>2009/05, p. 10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ja;&c_z;&c_d;&c_o;&c_m;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_b;&c_ja;&c_z;&c_d;&c_o;&c_m;&c_a;&c_k;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_h;&c_a;&c_c;&c_ib;&c_n;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="cs">bezdomovec</trd>
