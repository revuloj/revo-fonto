    <trd>przytulny</trd>,
    <trd>familiarny</trd>,
    <trd>znajomy</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_m;&c_a;&c_sh;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_je;&c_m;&c_je;&c_j;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_i;&c_n;&c_t;&c_i;&c_m;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dom&aacute;cky</trd>,
    <trd>&uacute;tuln&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x434;&#x43E;&#x43C;&#x430;&#x448;&#x43D;&#x456;&#x439;</trd>,
    <trd>&#x441;&#x456;&#x43C;&#x435;&#x439;&#x43D;&#x438;&#x439;</trd>,
