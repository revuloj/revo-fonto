    <dif>
      Eliri el sia <tld/>o:
      <ekz>
        en el<tld/>i&gcirc;on, en kaptitecon ili iros
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 12:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        Fradeko el<tld/>i&gcirc;is matene
        <fnt>
          <aut>H. Vallienne</aut>,
          <vrk>&Ccirc;u li?</vrk>,
          <lok>1908</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de"><ind>ausgesto&szlig;en</ind> werden</trd>
  <trd lng="fr">sortir de chez soi</trd>
  <trd lng="it">uscire di <ind>casa</ind></trd>
  <trdgrp lng="pl">
