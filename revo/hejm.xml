<drv mrk="hejm.0igxi">
  <kap>
    <tld/>i&gcirc;i,
    <var>
      <kap>en<tld/>i&gcirc;i</kap>
    </var>
  </kap>
  <snc mrk="hejm.0igxi.KOMUNE">
    <dif>
      Fari iun lokon sia <tld/>o:
      <ekz>
        &ccirc;irka&ubreve; 2000 judoj alvenis de orienta E&ubreve;ropo kaj <tld/>i&gcirc;is en Belfasto
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="&Monato;/2003/008105.html">Renkonti&gcirc;o kun &scirc;rumpanta komunumo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        dumtempe ili en<tld/>i&gcirc;is, kreante siajn proprajn kulturon kaj lingvojn
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="&Monato;/2004/008265.html">Parakosmo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="logx.en0igxi">enlo&gcirc;i&gcirc;i</ref>,
      <ref cel="instal.0igxi">instali&gcirc;i</ref>
    </refgrp>
  </snc>
  <snc mrk="hejm.0igxi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Adapti&gcirc;i al iu loko, medio:
      <ekz>
        post kelkaj tagoj li plene <tld/>i&gcirc;is inter la fulgonigraj
        viroj de la <klr>[&#x2026;]</klr> uzinoj
        <fnt>
          <aut>S. Engholm</aut>,
          <vrk>Al Torento</vrk>,
          <lok>1930</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili ne povis, a&ubreve; ne volis adapti&gcirc;i,
        en<tld/>i&gcirc;i
        <fnt>
          <aut>J. Drahotov&aacute;</aut>,
          <vrk>Miskompreniga</vrk>,
          <bib>Monato</bib>,
          <lok>[2001]</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akomod.0igxi">akomodi&gcirc;i</ref>,
      <ref cel="komfor.0igxi">komforti&gcirc;i</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>sich <ind>einleben</ind></trd>,
    <trd>sich <ind>ansiedeln</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>acclimater</ind><klr> (fig.)</klr></trd>,
    <trd>s'<ind>installer</ind></trd>
  </trdgrp>
  <trd lng="pl">zadomawia&cacute; si&eogonek;</trd>
</drv>

<drv mrk="hejm.el0igxi">
