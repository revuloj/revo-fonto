<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>hejm</rad>/o
</kap>
<drv mrk="hejm.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Loko, lo&gcirc;ejo a&ubreve; konstrua&jcirc;o en kiu koncernulo vivas
      konstante, precipe la familia restadejo:
      <ekz>
        vagulo ne havanta <tld/>on<fnt>B</fnt>;
      </ekz>
      <ekz>
        Jehuda estis <ind>el<tld/>igi</ind>ta el sia lando
        <fnt><bib>MT</bib>, <lok>II. Re&gcirc;oj 25:21</lok></fnt>.
        <trd lng="fr">expulser<klr tip="ind"> (de son domicile)</klr></trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dom.0o">domo</ref>,
      <ref cel="famili.0o">familio</ref>,
      <ref cel="mastr.0umi">mastrumi</ref>
    </refgrp>
  </snc>
  <trd lng="cs">domov</trd>
  <trdgrp lng="de">
    <trd>Zuhause</trd>,
    <trd>Heim</trd>
  </trdgrp>
  <trd lng="en">home</trd>
  <trdgrp lng="fr">
    <trd>chez-soi</trd>,
    <trd>appartement<klr tip="ind"> (chez-soi)</klr></trd>,
    <trd>domicile<klr tip="ind"> (chez-soi)</klr></trd>,
    <trd>foyer<klr tip="ind"> (chez-soi)</klr></trd>,
    <trd>maison<klr tip="ind"> (chez-soi)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>thuis</trd>,
    <trd>tehuis</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_m; <klr>(&c_s;&c_v;&c_o;&c_j;, &c_r;&c_o;&c_d;&c_n;&c_o;&c_j;)</klr></trd>,
    <trd>&c_d;&c_o;&c_m;&c_a;&c_sh;&c_n;&c_i;&c_j; &c_o;&c_ch;&c_a;&c_g;</trd>
  </trdgrp>
  <trd lng="sv">hem</trd>
</drv>

<drv mrk="hejm.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      De <tld/>o, rilata al <tld/>o:
      <ekz>
        <tld/>a lando, lingvo;
      </ekz>
      <ekz>
        nia lingvo estas al ili <tld/>a<fnt>Z</fnt>
        <klr>(malfremda)</klr>;
      </ekz>
      <ekz>
        <tld/>a <klr>(negli&gcirc;a)</klr> vesto;
      </ekz>
      <ekz>
        <tld/>a dometo estas kiel patrineto
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>aj dia&jcirc;oj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="famili1.0a">familiara</ref>,
      <ref cel="intim.0a">intima</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Vivanta &ccirc;e <tld/>o:
      <ekz>
        <ind><tld/>a besto</ind>;
        <trd lng="de">Haustier</trd>
        <trd lng="fr">animal domestique</trd>
        <trd lng="nl">huisdier</trd>
        <trd lng="ru">&c_d;&c_o;&c_m;&c_a;&c_sh;&c_n;&c_je;&c_je; &c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_o;&c_je;</trd>
      </ekz>
      <ekz>
        <tld/>aj katoj, &scirc;afinoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="sovagx.mal0a">malsova&gcirc;a</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>domestique</trd>,
    <trd>familier</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>thuis</trd>,
    <trd>huis-</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_o;&c_m;&c_a;&c_sh;&c_n;&c_i;&c_j;</trd>
</drv>

<drv mrk="hejm.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      En sia <tld/>o:
      <ekz>
        &ccirc;u s-ro N. estas <tld/>e?;
      </ekz>
      <ekz>
        reveni <tld/>en;
      </ekz>
      <ekz>
        senti sin kiel <tld/>e
        <klr>(ne &gcirc;eni sin, sidi komforte)</klr>;
      </ekz>
      <ekz>
        al &scirc;i fari&gcirc;is en la koro ankora&ubreve; pli agrable kaj
        <tld/>e<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiu &ccirc;i ideo en la publiko ne estas tiel <tld/>e <klr>(kutime
        akceptata)</klr>, kiel &gcirc;i devos esti<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">doma</trd>
  <trd lng="de">zuhause</trd>
  <trdgrp lng="fr">
    <trd>&agrave; la <ind>maison</ind></trd>,
    <trd>chez soi</trd>
  </trdgrp>
  <trd lng="nl">thuis</trd>
  <trd lng="ru">&c_d;&c_o;&c_m;&c_a;</trd>
  <trd lng="sv">hemma</trd>
</drv>

<drv mrk="hejm.0eca">
  <kap><tld/>eca<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Kvaza&ubreve; <tld/>a.
    </dif>
    <ref tip="vid" cel="komfor.0a">komforta</ref>
  </snc>
  <trd lng="fr">familier</trd>
  <trd lng="nl">huiselijk</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_m;&c_a;&c_sh;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_je;&c_m;&c_je;&c_j;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_i;&c_n;&c_t;&c_i;&c_m;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="hejm.0ama">
  <kap><tld/>ama</kap>
  <snc>
    <dif>
      &Scirc;atanta restadi <tld/>e.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="sid.forno0a.hejmama">fornosida</ref>,
    <ref cel="hejm.0osida">hejmosida</ref>
  </refgrp>
  <trd lng="de">h&auml;slich</trd>
  <trdgrp lng="fr">
    <trd>casanier</trd>,
    <trd>pantouflard</trd>,
    <trd>s&eacute;dentaire<klr tip="ind"> (casanier)</klr></trd>
  </trdgrp>
  <trd lng="nl">huiselijk</trd>
</drv>

<drv mrk="hejm.0osida">
  <kap><tld/>osida</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="hejm.0ama"><tld lit="H"/>ama</ref>.
    </dif>
  </snc>
  <trd lng="de">h&auml;slich</trd>
  <trdgrp lng="fr">
    <trd>casanier</trd>,
    <trd>s&eacute;dentaire<klr tip="ind"> (casanier)</klr></trd>
  </trdgrp>
  <trd lng="nl">huiselijk</trd>
</drv>

<drv mrk="hejm.sen0ulo">
  <kap>sen<tld/>ulo</kap>
  <snc>
    <dif>
      Vagulo.
    </dif>
    <refgrp tip="vid">
      <ref cel="nomad.0o">nomado</ref>,
      <ref cel="migr.0i">migri</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Obdachloser</trd>
  <trdgrp lng="fr">
    <trd>sans-abri<klr> (subst.)</klr></trd>,
    <trd>sans-logis<klr> (subst.)</klr></trd>,
    <trd>SDF</trd>,
    <trd>vagabond<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="nl">dakloze</trd>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_d;&c_o;&c_m;&c_n;&c_y;&c_j; <klr>(&c_s;&c_u;&c_shch;.)</klr></trd>
  <trd lng="sv">heml&ouml;s</trd>
</drv>

<drv mrk="hejm.fiks0a">
  <kap>fiks<tld/>a</kap>
  <snc>
    <dif>
      Nemigra, <ctl>loksida</ctl>, kies <tld/>o estas fiksita
      en difinita loko, kontraste al la nomadoj, kiuj kunportas sian
      <tld/>on dum siaj migradoj:
      <ekz>
        konstanta fiks<tld/>a lo&gcirc;anto
        <fnt>(<aut>Zamenhof</aut>: <vrk>Homaranismo</vrk>)</fnt>;
      </ekz>
      <ekz>
        la beduenoj kontraste al la fiks<tld/>aj araboj.
      </ekz>
    </dif>
    <ref tip="ant" cel="nomad.0a">nomada</ref>
  </snc>
  <adm>
    La vorto "loksida" aperanta en la dif ne estas en
    Revo, kiu tamen konas "loksidigxi" (laux mi tre
    malofte uzata). <aut>MB</aut>
  </adm>
  <trdgrp lng="fr">
    <trd>s&eacute;dentaire<klr tip="ind"> (par opp. &agrave; nomade)</klr></trd>
  </trdgrp>
  <trd lng="nl">vasthuizig</trd>
  <trd lng="ru">&c_o;&c_s;&c_je;&c_d;&c_l;&c_y;&c_j;</trd>
</drv>
</art>
