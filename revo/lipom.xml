          <vrk><url ref="http://www.esperanto.mv.ru/Lem/index.html">
          Surloka Esploro</url></vrk>,
          <lok>[2003]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">lipoma</trd>
  <trd lng="fr">lipome</trd>
  <trd lng="sk">lipom</trd>
</drv>
</art>

