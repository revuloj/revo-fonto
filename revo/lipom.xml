<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lipom.xml,v 1.6 2022/03/22 18:26:51 revo Exp $">
<kap>
  <rad>lipom</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="lipom.0o">
  <kap><tld/>o</kap>
  <snc mrk="lipom.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="tumor.0o.MED">Tumoro</ref>
      aperanta en grasa &ccirc;elaro, ordinare nedan&gcirc;era:
      <ekz>
        sur lia verto estis <tld/>o
        <fnt>
          <aut>S. Lem, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Lem/index.html">
          Surloka Esploro</url></vrk>,
          <lok>[2003]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_p;&c_o;&c_m;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>lipom</trd>,
    <trd>lipoma</trd>
  </trdgrp>
  <trd lng="en">lipoma</trd>
  <trd lng="fr">lipome</trd>
  <trdgrp lng="pl">
    <trd>t&lstroke;uszczak</trd>,
    <trd>lipoma</trd>
  </trdgrp>
  <trd lng="sk">lipom</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x456;&#x43F;&#x43E;&#x43C;&#x430;</trd>,
    <trd>&#x436;&#x438;&#x440;&#x43E;&#x432;&#x438;&#x43A;</trd>
  </trdgrp>
</drv>
</art>

