<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>prajm</rad>/o
</kap>
<drv mrk="prajm.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Ujeto, enhavanta eksplodeman substancon kaj per kiu oni eksplodigas
      la pulvon de karto&ccirc;o.
    </dif>
  </snc>
  <trd lng="de">Z&uuml;ndkapsel</trd>
  <trd lng="nl">slaghoedje</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_i;&c_s;&c_t;&c_o;&c_n;</trd>,
    <trd>&c_k;&c_a;&c_p;&c_s;&c_ju;&c_l;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;</trd>
  </trdgrp>
</drv>

<drv mrk="prajm.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Meti <tld/>on.
    </dif>
  </snc>
  <trd lng="de">Z&uuml;ndkapsel aufsetzen</trd>
  <trd lng="nl">v.e. <ind>slaghoedje</ind> voorzien</trd>
</drv>
</art>
