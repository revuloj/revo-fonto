<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tornad</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tornad.0o">
  <kap><tld/>o</kap>
  <snc mrk="tornad.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Tre vigla kirlovento,
      <ref tip="vid" cel="vortic.0o.MET">vortico</ref>
      suprenire turni&gcirc;anta inter la tero kaj
      &scirc;tormonubo kun loka forta detruemo:
      <ekz>
        <tld/>oj <klr>...</klr> okazas en &scirc;tormoj tutmonde,
        tamen ili formi&gcirc;as en Usono pli ofte
        <fnt>
          <bib>Viki</bib>,
          <lok><url
          ref="http://eo.wikipedia.org/wiki/Tornado_%28%C5%9Dtormo%29">
          Tornado (&scirc;tormo)</url>, 2012-01-16</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>met. smršť</trd>,
    <trd>tornádo</trd>
  </trdgrp>
  <trd lng="fr">tornade</trd>
  <trdgrp lng="sk">
    <trd>smršť</trd>,
    <trd>tornádo</trd>
  </trdgrp>
</drv>
</art>

