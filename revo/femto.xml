<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: femto.xml,v 1.7 2023/02/27 14:30:35 revo Exp $">
<kap>
    <rad>femto</rad>- <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="femto.0">
  <kap><tld/>-</kap>
  <mlg>f</mlg>
  <gra><vspec>prefikso</vspec></gra>
  <snc mrk="femto.0.MEZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      &leftquot;Biliardono da&rightquot;, 10<sup>-15</sup>
      en la Internacia Sistemo de unuoj (SI):
      <ekz>
        tinkturaj laseroj povas produkti lumon en lar&gcirc;a amplekso de ondolongoj, tiu eco
        ta&ubreve;gigas ilin por produktado de ege mallongaj pulsoj de lumo, &ccirc;irka&ubreve;
        <tld/>sekundaj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18858">Lasero</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        interago inter nukleonoj, kiu konsistigas la fortan nuklean forton
        <klr>[&#8230;]</klr> efikas &gcirc;is distanco de &ccirc;irka&ubreve; 2,5 <tld/>metroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=27377">Forta nuklea forto</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_je;&c_m;&c_t;&c_a;-</trd>
  <trd lng="bg">&c_f;&c_je;&c_m;&c_t;&c_o;-</trd>
  <trd lng="ca">femto-</trd>
  <trd lng="de">Femto-</trd>
  <trd lng="en">femto-</trd>
  <trd lng="es">femto-</trd>
  <trd lng="fa">&a_f;&a_m;&a_t;&a_w;</trd>
  <trd lng="fr">femto-</trd>
  <trd lng="hu">femto-</trd>
  <trd lng="it">femto-</trd>
  <trd lng="nl">femto-</trd>
  <trd lng="no">femto-</trd>
  <trd lng="pl">femto-</trd>
  <trd lng="pt">femto-</trd>
  <trd lng="ru">&c_f;&c_je;&c_m;&c_t;&c_o;-</trd>
  <trd lng="sv">femto-</trd>
  <trd lng="uk">&c_f;&c_je;&#x43C;&c_t;&#x43E;&#8230;</trd>
</drv>
</art>

