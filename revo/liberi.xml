<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>liberi</rad>/o
</kap>

<drv mrk="liberi.0o">
  <kap>
    <tld lit="L"/>o
    <ofc>8</ofc>
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="liberi.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la okcidenta <ref tip="malprt" 
      cel="afrik.A0o">Afriko</ref> kun &ccirc;efurbo Monrovio.
      Teritorio de Respubliko Liberio.
      ISO-3166 kodo: LR.
      &Gcirc;i limas kun
      <ref tip="vid" cel="ebur.0bordo.GEOG">Ebur-Bordo</ref>,
      <ref tip="vid" cel="gvine.0o.GEOG">Gvineo</ref>,
      <ref tip="vid" cel="sieraleo.0o.GEOG">Sieraleono</ref>
      kaj <ref tip="vid" cel="atlant.0o.GEOG">Atlantiko</ref>.
    </dif>
    <trd lng="bg">&c_L;&c_i;&c_b;&c_je;&c_r;&c_i;&c_ja;</trd>
    <trd lng="de">Liberia</trd>
    <trd lng="en">Liberia</trd>
    <trdgrp lng="nl">
      <trd>Liberi&euml;</trd>,
      <trd>Liberia</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>Liberia</trd>,
      <trd>Republika <ind>Liberii</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

