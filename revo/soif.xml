<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: soif.xml,v 1.34 2025/09/09 06:40:58 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>soif</rad>/i <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="soif.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="soif.0i.KUI">
    <dif>
      Senti bezonon <ref cel="trink.0i">trinki</ref>:
      <ekz>
        &ccirc;u la infano <tld/>as a&ubreve; malsatas
        <fnt>
          <bib>IdO</bib>
          <lok>Dua Parto</lok>
        </fnt>?
      </ekz>
      <ekz>
        multaj trinkas pli ol ili <tld/>as
        <fnt>
          <bib>Fab3</bib>,
          <vrk>&Scirc;i estis tute senta&ubreve;ga</vrk>
        </fnt>;
      </ekz>
      <ekz>
        elportu akvon renkonte al la <tld/>anto
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 21:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam mi <tld/>os, mi melkos mian bovinon kaj trinkos lakton
        <fnt>
          <bib>ElektFab</bib>
          <lok>Feli&ccirc;a Jo&ccirc;jo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Durst haben</trd>
    <trd lng="es">tener sed</trd>
    <trd lng="hu">szomjazik</trd>
    <trdgrp lng="pl">
      <trd>mie&cacute; <ind>pragnienie</ind></trd>,
      <trd>by&cacute; <ind>spragnionym</ind></trd>,
      <trd>odczuwa&cacute; <ind>pragnienie</ind></trd>,
      <trd>chcie&cacute; <ind>pi&cacute;</ind></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_h;&c_o;&c_t;&c_je;&c_t;&c_mol; &c_p;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_i;&c_s;&c_p;&c_y;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol; <ind>&c_zh;&c_a;&c_zh;&c_d;&c_u;</ind></trd>,
      <trd>&c_zh;&c_a;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="soif.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Forte <ref tip="super" cel="dezir.0i.porSiMem">deziri</ref>;
      <ref cel="sopir.0i.dezirajxon">sopiri<sncref/></ref>:
      <ekz>
        <tld/>i ven&gcirc;on, al ven&gcirc;o, al libero;
      </ekz>
      <ekz>
        Mia animo <tld/>as Dion
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 42:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> <tld/>as justecon
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 5:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        feli&ccirc;econ <klr>[&#x2026;]</klr> li sentas, trovinte fine sinceran amemon, kiun <tld/>as
        lia koro
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sepa</lok>
        </fnt>;
      </ekz>
      <ekz>
        se la duko <tld/>is la morton, lin samtempe teruris la ideo, ke eble oni povus
        lin entombigi <klr>[&#x2026;]</klr> vivanta
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekoka</lok>
        </fnt>;
      </ekz>
      <ekz>
        la popolo <tld/>as ven&gcirc;on
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XLIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;efpastroj <tld/>as <klr>[&#x2026;]</klr> mian sangon
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        glavo sangon <ind><tld/>anta</ind>
        <fnt>
          <aut>L. L. Zamenhof</aut>
          <vrk>La Espero</vrk>
        </fnt>.
        <trd lng="ru">&c_zh;&c_a;&c_zh;&c_d;&c_u;&c_shch;&c_i;&c_j;</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aspir.0i">aspiri</ref>,
      <ref cel="dezir.0egi">deziregi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd><ind>d&uuml;rsten</ind> nach</trd>,
      <trd>ersehnen</trd>,
      <trd>begehren</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&aacute;h&iacute;tozik</trd>,
      <trd>s&oacute;v&aacute;rog</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>pragn&aogonek;&cacute;</trd>,
      <trd>&lstroke;akn&aogonek;&cacute;</trd>,
      <trd>by&cacute; <ind>spragnionym</ind> <klr>(czego&sacute;)</klr></trd>,
      <trd>odczuwa&cacute; <ind>pragnienie</ind> <klr>(czego&sacute;)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_zh;&c_a;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_m;&c_a;&c_g;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_g;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_h;&c_a;&c_c;&c_je;&c_c;&c_mol; &c_p;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>&zcaron;&iacute;znit</trd>,
    <trd>prahnout</trd>
  </trdgrp>
  <trd lng="en">thirst</trd>
  <trd lng="fr">avoir <ind>soif</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x306E;&#x3069;&#x304C;&#x6E07;&#x304F;
    <pr>&#x306E;&#x3069;&#x304C;&#x304B;&#x308F;&#x304F;</pr></trd>,
    <trd>&#x6E07;&#x671B;&#x3059;&#x308B;  <pr>&#x304B;&#x3064;&#x307C;&#x3046;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">dorst hebben</trd>
  <trdgrp lng="sk">
    <trd>by&tcaron; sm&auml;dn&yacute;</trd>,
    <trd>prahn&uacute;&tcaron;</trd>,
    <trd>sm&auml;di&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_h;&c_o;&c_t;&c_ib;&c_t;&c_i; &c_p;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i; &c_s;&c_p;&c_r;&c_a;&c_g;&c_u;</trd>,
    <trd>&c_zh;&c_a;&c_d;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="soif.0o">
  <kap><tld/>o</kap>
  <snc mrk="soif.0o.KUI">
    <dif>
      Sento de tiu, kiu bezonas trinki:
      <ekz>
        suferi pro, de <tld/>o;
      </ekz>
      <ekz>
        ilia lango seki&gcirc;as de <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 41:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>kvietigi la <tld/>on</ind>
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
        <trd lng="hu">szomj&aacute;t csillap&iacute;tja</trd>
      </ekz>
      <ekz>
        &scirc;i drinkis iom super la <tld/>o <klr>(ebrii&gcirc;is)</klr>
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        mortigi <klr>[&#x2026;]</klr> brutojn per <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 17:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        en mia <tld/>o ili trinkigis al mi vinagron
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 69:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>on li estingas per eksterlandaj vinoj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        Donu nur <tld/>on al mi &parsl;
        Sed ne donu akvon
        <fnt>
          <aut>Man&gcirc;u Kan&ccirc;ul</aut>,
          <vrk><url ref="http://www.geocities.ws/jeneeya/esperanto/montejo/montejo8/poemo.htm">
          Bonvolu Ne Doni Akvon</url></vrk>,
          <lok>Montejo, 2000:1</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="de">Durst</trd>
  </snc>
  <snc mrk="soif.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Forta <ref tip="super" cel="dezir.0o">deziro</ref>; <ref tip="sin" cel="sopir.0o.sento">sopiro<sncref/></ref>:
      <ekz>
        <tld/>o al la scienco
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i pensis pri sia <tld/>o al viroj, <klr>[&#x2026;]</klr> neniam &scirc;i trovos unu, kiu
        sukcesos kontentigi &scirc;in
        <fnt>
          <bib>ChM</bib>
          <lok>15</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> eliros el la malliberejo kun <tld/>o al ven&gcirc;ado
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Kvara</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Sehnsucht</trd>,
      <trd>Begehren</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_m;&c_a;&c_g;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_g;&c_a;</trd>
  </trdgrp>
  <trd lng="fr">soif</trd>
  <trdgrp lng="ja">
    <trd>&#x306E;&#x3069;&#x306E;&#x6E07;&#x304D;
    <pr>&#x306E;&#x3069;&#x306E;&#x304B;&#x308F;&#x304D;</pr></trd>,
    <trd>&#x6E07;&#x671B;  <pr>&#x304B;&#x3064;&#x307C;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="pl">pragnienie</trd>
  <trd lng="uk">&c_s;&c_p;&c_r;&c_a;&c_g;&c_a;</trd>
  <trd lng="zh">&#x53E3;&#x6E34; <pr>k&#x1D2;uk&ecaron;</pr></trd>
</drv>

<drv mrk="soif.0a">
  <kap><tld/>a</kap>
  <snc mrk="soif.0a.KUI">
    <dif>
      Sentanta <tld/>on:
      <ekz>
        li estis malsata kaj <tld/>a
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kiam &scirc;i unu fojon gustumis inkon, &scirc;i fari&gcirc;is <tld/>a je &gcirc;i, <klr>[&#x2026;]</klr>
        kaj &scirc;i volis subskribi &ccirc;ion eblan kaj meti sian nomon en &ccirc;iuj
        anguloj
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi estis <tld/>a je okazoj, nu, mi do ricevis ilin
        <fnt>
          <bib>VaK</bib>
          <lok>Na&ubreve;a &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_s;&c_m;&c_a;&c_g;&c_l;&c_y;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_g;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bo">&t_kha;&t_tsheg;&t_sa;&t_kata;&t_naro;&t_ma;&t_tsheg;</trd>
  <trd lng="cs">&zcaron;&iacute;zniv&yacute;</trd>
  <trd lng="de">durstig</trd>
  <trd lng="en">thirsty</trd>
  <trd lng="es">sediento</trd>
  <trd lng="fr">assoiff&eacute;</trd>
  <trdgrp lng="hu">
    <trd>szomjas</trd>,
    <trd>s&oacute;v&aacute;r</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x306E;&#x3069;&#x306E;&#x6E07;&#x3044;&#x305F;
    <pr>&#x306E;&#x3069;&#x306E;&#x304B;&#x308F;&#x3044;&#x305F;</pr></trd>,
    <trd>&#x6E07;&#x671B;&#x3057;&#x3066;&#x3044;&#x308B;  <pr>&#x304B;&#x3064;&#x307C;&#x3046;&#x3057;&#x3066;&#x3044;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">dorstig</trd>
  <trdgrp lng="pl">
    <trd>pragn&aogonek;cy</trd>,
    <trd>spragniony</trd>,
    <trd>chc&aogonek;cy <ind>pi&cacute;</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_zh;&c_a;&c_zh;&c_d;&c_u;&c_shch;&c_i;&c_j;</trd>
  <trd lng="sk">sm&auml;dn&yacute;</trd>
  <trd lng="uk">&c_s;&c_p;&c_r;&c_a;&c_g;&c_l;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x6E34; <pr>k&ecaron;</pr></trd>,
    <trd>&#x6E34;&#x671B; <pr>k&ecaron;w&agrave;ng</pr></trd>,
    <trd>&#x4E7E;&#x6E34; <pr>g&amacron;nk&ecaron;</pr></trd>,
    <trd>&#x5E72;&#x6E34; <pr>g&amacron;nk&ecaron;</pr></trd>,
    <trd>&#x6E34;&#x7740; <pr>k&ecaron;zhu&oacute;</pr></trd>,
    <trd>&#x6E34;&#x8457; <pr>k&ecaron;zhu&oacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="soif.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="soif.0igi.KUI">
    <dif>
      Al iu ka&ubreve;zi <tld/>on:
      <ekz>
        li punas la teron, kaj, soifiginte &gcirc;in, li &gcirc;in ri&ccirc;igas
        <fnt>
          <bib>Fab3</bib>,
          <vrk>Juda knabino</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol; <ind>&c_s;&c_m;&c_a;&c_g;&c_u;</ind></trd>
  <trd lng="de"><ind>d&uuml;rsten</ind> lassen</trd>
  <trd lng="fr">assoiffer</trd>
  <trdgrp lng="ja">
    <trd>&#x306E;&#x3069;&#x3092;&#x6E07;&#x304B;&#x305B;&#x308B;
    <pr>&#x306E;&#x3069;&#x3092;&#x304B;&#x308F;&#x304B;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x6E07;&#x671B;&#x3055;&#x305B;&#x308B;  <pr>&#x304B;&#x3064;&#x307C;&#x3046;&#x3055;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="pl">czyni&cacute;  <ind>spragnionym</ind></trd>
  <trd lng="uk">&c_v;&c_i;&c_k;&c_l;&c_i;&c_k;&c_a;&c_t;&c_i; &c_s;&c_p;&c_r;&c_a;&c_g;&c_u;</trd>
</drv>

<drv mrk="soif.sen0igi">
  <kap>sen<tld/>igi, <var><kap>mal<tld/>igi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="soif.sen0igi.KUI">
    <dif>
      &Ccirc;esigi ies soifon:
      <ekz>
        <klr>[ili]</klr> altabli&gcirc;is en kafejoj por sin sen<tld/>igi
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Deka</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvar kelistoj penis <klr>[&#x2026;]</klr> sen<tld/>igi la
        <tld/>antajn dancantojn
        <fnt>
          <aut>H. Vallienne</aut>,
          <vrk>Kastelo de Prelongo</vrk>,
          <lok>&Ccirc;apitro dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Mi sen<tld/>igos animon lacan
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 31:25</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_t;&c_a;&c_l;&c_ib;&c_c;&c_mol; <klr>(&c_s;&c_m;&c_a;&c_g;&c_u;)</klr></trd>,
    <trd>&c_s;&c_p;&c_a;&c_t;&c_o;&c_l;&c_ib;&c_c;&c_mol; <klr>(&c_s;&c_m;&c_a;&c_g;&c_u;)</klr></trd>,
    <trd>&c_z;&c_a;&c_d;&c_a;&c_v;&c_o;&c_l;&c_ib;&c_c;&c_mol; <klr>(&c_s;&c_m;&c_a;&c_g;&c_u;)</klr></trd>
  </trdgrp>
  <trd lng="de"><klr>(jemandes)</klr> Durst <ind>stillen</ind></trd>
  <trd lng="fr">d&eacute;salt&eacute;rer</trd>
  <trd lng="ja">&#x6E07;&#x304D;&#x3092;&#x3044;&#x3084;&#x3059;  <pr>&#x304B;&#x308F;&#x304D;&#x3092;&#x3044;&#x3084;&#x3059;</pr></trd>
  <trd lng="pl">gasi&cacute;  <ind>pragnienie</ind></trd>
  <trdgrp lng="uk">
    <trd>&c_u;&c_g;&c_a;&c_m;&c_u;&c_v;&c_a;&c_t;&c_i; &c_s;&c_p;&c_r;&c_a;&c_g;&c_u; (&c_ch;&c_i;&c_ju;&c_s;&c_mol;)</trd>,
    <trd>&c_n;&c_a;&c_p;&c_o;&c_ji;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="soif.sen0igxi">
  <kap>sen<tld/>i&gcirc;i, <var><kap>mal<tld/>i&gcirc;i</kap></var></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="soif.sen0igxi.KUI">
    <dif>
      &Ccirc;esigi sian <tld/>on:
      <ekz>
        Alma-Ato ne prezentas multajn eblecojn sen<tld/>i&gcirc;i aliloke ol hejme
        <fnt>
          <bib>CKv</bib>
          <lok>10</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili trinkas akvon somere kaj sen<tld/>i&gcirc;as per ne&gcirc;o kaj glacio
        vintre
        <fnt>
          <aut>Vikipedio</aut>,
          <vrk><url ref="&Viki;/Gruntbovo">Gruntbovo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_a;&c_t;&c_o;&c_l;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trd lng="de"><klr>(den eigenen)</klr> Durst <ind>stillen</ind></trd>
  <trd lng="fr">se d&eacute;salt&eacute;rer</trd>
  <trd lng="pl">napi&cacute; si&eogonek;</trd>
</drv>
</art>
