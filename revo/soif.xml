<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>soif</rad>/i
</kap>
<drv mrk="soif.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Senti bezonon trinki:
      <ekz>
        suferi pro, de <tld/>o;
      </ekz>
      <ekz>
        <ind>kvietigi la <tld/>on</ind>.
        <trd lng="hu">szomj&aacute;t csillap&iacute;tja</trd>
      </ekz>
    </dif>
    <trd lng="de">Durst haben</trd>
    <trd lng="es">tener sed</trd>
    <trd lng="hu">szomjazik</trd>    
    <trdgrp lng="ru">
      <trd>&c_h;&c_o;&c_t;&c_je;&c_t;&c_mol; &c_p;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_i;&c_s;&c_p;&c_y;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol; <ind>&c_zh;&c_a;&c_zh;&c_d;&c_u;</ind></trd>,
      <trd>&c_zh;&c_a;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Forte deziri:
      <ekz>
        <tld/>i ven&gcirc;on, al ven&gcirc;o, al libero;
      </ekz>
      <ekz>
        <tld/>o al la scienco<fnt>B</fnt>;
      </ekz>
      <ekz>
        glavo sangon <ind><tld/>anta</ind><fnt>Z</fnt>;
        <trd lng="ru">&c_zh;&c_a;&c_zh;&c_d;&c_u;&c_shch;&c_i;&c_j;</trd>
      </ekz>
      <ekz>
        la pluvo mal<tld/>igas
        (<ref tip="sin" cel="satur.0i">saturi</ref>)
        la teron<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">d&uuml;rsten</trd>
    <trdgrp lng="hu">
      <trd>&aacute;h&iacute;tozik</trd>,
      <trd>s&oacute;v&aacute;rog</trd>
    </trdgrp>
    <trd lng="ru">&c_zh;&c_a;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_m;&c_a;&c_g;&c_n;&c_u;&c_c;&c_mol;</trd>, 
    <trd>&c_h;&c_a;&c_c;&c_je;&c_c;&c_mol; &c_p;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">&zcaron;&iacute;znit</trd>
  <trd lng="en">thirst</trd>
  <trd lng="nl">dorst hebben</trd>
</drv>

<drv mrk="soif.0a">
  <kap><tld/>a<fnt>Z</fnt></kap>
  <snc>
    <dif>
      <tld lit="S"/>anta.
    </dif>
  </snc>
  <trd lng="de">durstig</trd>
  <trd lng="en">thirsty</trd>
  <trd lng="es">sediento</trd>
  <trdgrp lng="hu">
    <trd>szomjas</trd>,
    <trd>s&oacute;v&aacute;r</trd>
  </trdgrp>
  <trd lng="nl">dorstig</trd>
  <trd lng="ru">&c_zh;&c_a;&c_zh;&c_d;&c_u;&c_shch;&c_i;&c_j;</trd>
</drv>
</art>
