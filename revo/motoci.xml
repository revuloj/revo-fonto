<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: motoci.xml,v 1.15 2021/11/10 09:31:05 revo Exp $">
<kap>
  <rad>motocikl</rad>/o <fnt><bib>PV</bib></fnt>,
  <var><kap><rad var="r">motorcikl</rad>/o</kap></var>
</kap>

<drv mrk="motoci.0o">
  <kap><tld/>o, <var><kap><tld var="r"/>o</kap></var></kap>
  <snc mrk="motoci.0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      Durada <ref tip="lst" lst="voko:veturiloj" cel="vetur.0ilo.TRA">veturilo</ref>
      movata per motoro:
      <ekz>
        policanoj <klr>[&#8230;]</klr> sur &ccirc;evaloj kaj <tld/>oj maltrankvile a&ubreve;skultis la
        bruegon
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        jam li ekrajdis sian <tld var="r"/>on, kaj veturis for
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld var="r"/>o <klr>[&#8230;]</klr> estas tro kosta, kaj pro tio ekaperis taksi-bicikloj, kiuj
        intertempe fari&gcirc;is tre popularaj
        <fnt>
          <bib>Monato</bib>
          <aut>J&eacute;r&eacute;mie Sabiyumva</aut>
          <vrk><url ref="https://www.monato.be/2009/009690.php"
              >Pendole pedale</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvado estas kvarrada <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=29281"
              >Kvado</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bicikl.0o">biciklo</ref>,
      <ref cel="moped.0o">mopedo</ref>,
      <ref cel="veloci.0o">velocipedo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_t;&c_a;&c_c;&c_y;&c_k;&c_l;</trd>
  <trd lng="cs">motocykl</trd>
  <trd lng="de">Motorrad</trd>
  <trd lng="fr">motocyclette</trd>
  <trd lng="pl">motocykl</trd>
  <trdgrp lng="pt">
    <trd>motocicleta</trd>,
    <trd>moto <klr tip="amb">(BR, motocicleta)</klr></trd>,
    <trd>motoca <klr tip="amb">(BR, motocicleta)</klr></trd>    
  </trdgrp>
  <trd lng="ro">motociclet&abreve;</trd>
  <trd lng="sk">motocykel</trd>
</drv>
</art>
