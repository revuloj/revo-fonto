<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>bajonet</rad>/o
</kap>
<drv mrk="bajone.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Pika kaj kelkfoje tran&ccirc;a mallonga batalilo, kiu adapti&gcirc;as al la
      tubo de pafilo.
    </dif>
  </snc>
  <trd lng="de">Bajonett</trd>
  <trd lng="en">bayonet</trd>
  <trd lng="es">bayoneta</trd>
  <trd lng="fr">ba&iuml;onnette</trd>
  <trdgrp lng="hu">
    <trd>szurony</trd>,
    <trd>bajonett</trd>
  </trdgrp>
  <trd lng="nl">bajonet</trd>
  <trd lng="ru">&c_sh;&c_t;&c_y;&c_k;</trd>
  <trd lng="sk">bajonet</trd>
  <trd lng="sv">bajonett</trd>
</drv>
</art>
