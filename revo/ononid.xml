<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: ononid.xml,v 1.3 2023/10/19 09:24:05 afido Exp $">
    <kap>
      <rad>ononid</rad>/o <fnt><bib>PIV2</bib></fnt>,
      <var><kap><rad var="s">ononis</rad>/o</kap></var>
    </kap>

<drv mrk="ononid.0o">
  <kap><tld/>o, <var><kap><tld var="s"/>o</kap></var></kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="ononid.0o.BOT">
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio <ref tip="super" cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Ononis</trd>) de herboj a&ubreve; arbustetoj kun glandaj,
      foje gluemaj haroj a&ubreve; dornoj, tripartaj folioj kaj flavaj, roz- a&ubreve; purpurkoloraj floroj:
      <ekz>
        kampa <tld/>o, <tld var="s"/>o estas alia rara&jcirc;o trovebla en la insulo <nom>Harakka</nom>
        apud Helsinko
        <fnt>
          <vrk><url
          ref="https://nordanaturo.wordpress.com/2011/03/12/planto-de-la-semajno-11-ononido-kampa/"
              >Planto de la semajno 11: ononido kampa</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Vincento <klr>[&#8230;]</klr> kirlis super fajro magiajn dekoktojn el <klr>[&#8230;]</klr>
        bistorto, centa&ubreve;reo, <tld var="s"/>o
        <fnt>
          <aut>Karel &Ccaron;apek, trad. Josef Vondrou&scaron;ek</aut>
          <vrk><url
          ref="https://github.com/LaPingvino/esperantolibroj/blob/master/Nau%27%20fabeloj%20-%20Karel%20C%27apek.md"
              >Na&ubreve; Fabeloj, Granda doktora fabelo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/2/24/Ononis_spinosa_2005.07.03_12.14.09.jpg/360px-Ononis_spinosa_2005.07.03_12.14.09.jpg"
      prm="CC BY-SA 3.0"
      >dorna <tld/>o (Ononis spinosa)
        <fnt>
          <aut>Michael Becker, taken by Michael Becker</aut>
          <lok><url ref="&WCW;/File:Ononis_spinosa_2005.07.03_12.14.09.jpg"
              >https://commons.wikimedia.org/wiki/File:Ononis_spinosa_2005.07.03_12.14.09.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="be">
      <trd>&c_s;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>,
      <trd>&c_v;&c_a;&c_w;&c_ch;&c_u;&c_k;</trd>
    </trdgrp>
    <trd lng="de">Hauhechel</trd>
    <trd lng="en">Ononis</trd>
    <trd lng="fr">Ononis</trd>
  </snc>
</drv>

  </art>

