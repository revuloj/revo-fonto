<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>diluv</rad>/o
</kap>
<drv mrk="diluv.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Superakvego, inundego: <ekz>la Biblia <tld/>o;</ekz> 
      <ekz>
        <ind><tld/>a</ind> pluvo;
        <trdgrp lng="de">
          <trd>sintflutlich</trd>
         </trdgrp>
        <trd lng="fr">diluvien</trd>
        <trd lng="nl">zondvloeds-</trd>
      </ekz>
      <ekz>
        <ind>anta&ubreve;<tld/>a</ind> monstro.
        <trd lng="be">&c_d;&c_a;&c_p;&c_a;&c_t;&c_o;&c_p;&c_n;&c_y;</trd>
        <trdgrp lng="de">
          <trd>vorsintflutlich</trd>
        </trdgrp>
        <trd lng="en">antediluvian</trd>
        <trd lng="fr">ant&eacute;diluvien</trd>
        <trd lng="nl">van voor de <ind>zondvloed</ind></trd>
        <trd lng="pl">przedpotopowy</trd>
        <trd
        lng="ru">&c_d;&c_o;&c_p;&c_o;&c_t;&c_o;&c_p;&c_n;&c_y;&c_j;</trd>
      </ekz>
    </dif>
    <ref tip="vid" cel="inund.0i">inundi</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_t;&c_o;&c_p;</trd>
  <trdgrp lng="de">
     <trd>Sintflut</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>deluge</trd>,
    <trd>great flood</trd>
  </trdgrp>
  <trd lng="es">diluvio</trd>
  <trd lng="fr">d&eacute;luge</trd>
  <trd lng="nl">zondvloed</trd>
  <trd lng="pl">potop</trd>
  <trd lng="pt">dil&uacute;vio</trd>
  <trd lng="ru">&c_p;&c_o;&c_t;&c_o;&c_p;</trd>
</drv>
</art>
