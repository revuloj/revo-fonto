<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>*</ofc><rad>v</rad> <fnt><bib>PIV1</bib></fnt>
</kap>
<subart>
<drv mrk="v1.0.konsonanto">
  <kap><tld/></kap>
    <uzo tip="fak">TIP</uzo>
    <uzo tip="fak">GRA</uzo>
    <uzo tip="fak">FON</uzo>
  <snc>    
    <dif>
      <ref tip="dif" cel="konson1.0o">Konsonanto</ref>, vo&ccirc;a
      <ref tip="dif" cel="frikat.0o">frikativa</ref> <ref tip="dif"
      cel="labial.0o">labialo</ref> [v].
    </dif>
  <trd lng="fr">v<klr> (consonne)</klr></trd>
  </snc>
  <snc>
     <dif>
      La dudeksepa <ref tip="dif" cel="grafem.0o">grafemo</ref> de la
      Esperanta alfabeto, uzata i.a. kiel:
     </dif>
  <trd lng="fr">v<klr> (lettre)</klr></trd>
  <subsnc>
  <uzo tip="fak">MAT</uzo>
  <dif>
  (maj.) romana cifero: <ekz><tld lit="V"/> (5)</ekz>;
  </dif>
  <trd lng="fr">5<klr> (chiffre romain)</klr></trd>
  </subsnc>
  <subsnc>
  <uzo tip="fak">KEM</uzo>
  <dif>
  (maj.) simbolo de vanado: <ekz><tld lit="V"/></ekz>;
  </dif>
  <trd lng="fr">vanadium</trd>
  </subsnc>
  <subsnc>
  <dif>
  (maj.) signo de formo: <ekz><tld lit="V"/>-fero</ekz>;
  </dif>
  <trd lng="fr">V<klr> (forme de)</klr></trd>
  </subsnc>
  <subsnc>
  <uzo tip="fak">FIZ</uzo>
  <dif>
  (maj.) simbolo de volto: <ekz><tld lit="V"/></ekz>;
  </dif>
  <trd lng="fr">Volt</trd>
  </subsnc>
  <subsnc>
  <uzo tip="fak">TIP</uzo>
  <dif>
  (min.) mallongigo de vidu: <ekz><tld/>.</ekz>;
  </dif>
  <trd lng="fr">voir<klr> (abr&eacute;viation)</klr></trd>
  </subsnc>
  
  
  </snc>
</drv>
</subart>

<subart>
<drv mrk="v1.0.nomo">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Nomo de tiuj <ref tip="dif" cel="fonem.0o">fonemo</ref> kaj
      grafemo.    
    </dif>
  <trd lng="fr">v</trd>
  </snc>
  </drv>
</subart>
</art>

