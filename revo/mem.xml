<subart>
<drv mrk="mem.0_pref">
 <kap><tld/>-</kap>
  <dif>
    Prefikso uzata samsence
  </dif>
  <snc mrk="mem.0_pref.sola">
    <dif>
      kiel <tld/><sncref ref="mem.0.sola"/>:
      <ekz>
        <ind><tld/>mova</ind>;
        <trdgrp lng="es">
          <trd>automotriz</trd>,
          <trd>semoviente</trd>
        </trdgrp>
        <trd lng="hu">&ouml;nmozg&oacute;</trd>
        <trdgrp lng="pl">
          <trd>samobie&zdot;ny</trd>
