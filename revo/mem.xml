        </trdgrp>
        <trdgrp lng="ro">
          <trd>autopropulsat</trd>
          <trd>automat&abreve;</trd>
        </trdgrp>
        <trd lng="ru">&c_s;&c_a;&c_m;&c_o;&c_d;&c_v;&c_i;&c_zh;&c_u;&c_shch;&c_i;&c_j;&c_s;&c_ja;</trd>
