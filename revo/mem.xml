        <ind><tld/>lerninto</ind>;
        <trd lng="be">&c_s;&c_a;&c_m;&c_a;&c_v;&c_u;&c_k;</trd>
        <trd lng="es">autodidacta</trd>
        <trd lng="fi">itseoppinut</trd>
        <trd lng="nl">autodidact</trd>
        <trdgrp lng="pl">
          <trd>samouk</trd>
          <trd>autodydakta</trd>
        </trdgrp>
        <trd lng="ro">autodidact</trd>
        <trd lng="ru">&c_s;&c_a;&c_m;&c_o;&c_u;&c_ch;&c_k;&c_a;</trd>
        <trd lng="sv">sj&auml;lvl&auml;rd</trd>
      </ekz><ekz>
        <ind><tld/>farita homo</ind>
        <fnt>
          <bib>FK</bib>
        </fnt>;
        <trd lng="en">self-made man</trd>
        <trd lng="es">persona hecha a s&iacute; misma</trd>
        <trdgrp lng="hu">
          <trd>self-made man</trd>,
          <trd>&ouml;ner&odblac;b&odblac;l felemelkedett ember</trd>
        </trdgrp>
        <trd lng="pl">domoros&lstroke;y cz&lstroke;owiek</trd>
        <trd lng="ro">om f&abreve;cut de sine</trd>
