  </trdgrp>
  <trd lng="pl">egoizm</trd>
  <trd lng="ro">egoism</trd>
</drv>

<drv mrk="mem.en0igxi">
  <kap>en<tld/>i&gcirc;i</kap>
  <snc mrk="mem.en0igxi.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      Sin turni al interna penso, forgesante la &ccirc;irka&ubreve;an
      mondon kaj cirkonstancojn:
      <ekz>
        anka&ubreve; mi morga&ubreve; posttagmeze en<tld/>i&gcirc;os
        en pre&gcirc;ado
        <fnt>
          <aut>Benedikto la 16a</aut>,
          <vrk><url ref="http://www.ikue.org/papoj/benediktoxvi/prediko051101.html">;
            Gustumi la &gcirc;ojon...</url>
          </vrk>,
          <lok>2005-11-01</lok>
        </fnt>;
      </ekz>
      <ekz>
        foliumado de la pa&gcirc;oj ebligas pli facile al homoj en<tld/>i&gcirc;i en roloj,
        encerbigi informojn, kompreni detalojn de la teksto
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2015/011371.php">Cifereca eldono komprenigas nur
onon</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="absorb.0igxi.PSI">absorbi&gcirc;i</ref>,
      <ref cel="pens.en0igxi">enpensi&gcirc;i</ref>,
      <ref cel="medit.0i.KOMUNE">mediti</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_m;&c_y;&c_k;&c_a;&c_c;&c_c;&c_a; &c_w; &c_s;&c_a;&c_b;&c_je;</trd>,
      <trd>&c_p;&c_a;&c_g;&c_r;&c_u;&c_zh;&c_a;&c_c;&c_c;&c_a; &c_w; &c_d;&c_u;&c_m;&c_k;&c_ib;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>in sich hinein<ind>horchen</ind></trd>,
      <trd>sich in <ind>Gedanken</ind> verlieren</trd>,
      <trd>meditieren</trd>,
    </trdgrp>
    <trd lng="zh">&#x5750;&#x795E; <pr>zu&#xF2;sh&#xE9;n</pr></trd>
</snc>
  <trd lng="es">abstraerse</trd>
  <trdgrp lng="fi">
    <trd>uppoutua</trd>,
    <trd>syventy&auml;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>perdre dans ses pens&eacute;es</ind></trd>,
    <trd>se <ind>recueillir</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>s&lstroke;ucha&cacute; siebie</trd>,
    <trd>zatraca&cacute; si&eogonek; w my&sacute;lach</trd>,
    <trd>medytowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>ascult&abreve;-te</trd>,
    <trd>pierde-te &icirc;n g&acirc;nduri</trd>,
    <trd>medita</trd>
  </trdgrp>
  <trd lng="sv">g&aring; in i sig <ind>sj&auml;lv</ind></trd>
</drv>
</subart>

