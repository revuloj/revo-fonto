<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aneks.xml,v 1.25 2022/04/25 15:10:59 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>aneks</rad>/i
</kap>
<drv mrk="aneks.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="aneks.0i.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="don.al0i">Aldoni</ref> al regno fremdan teritorion:
      <ekz>
        en 1919 Prezidanto <nom>Wilson</nom> ne konsentis, ke la venkintoj simple <tld/>u la
        koloniojn de Germanujo
        <fnt>
          <bib>FSp</bib>
          <lok>Tria Parto: Ligo De Nacioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam <nom>Hitler</nom> aneksis A&ubreve;strion, <nom>IEMW</nom> estis fermita kaj &gcirc;ia direktoro enkarcerigita
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la minimuma plano de <nom>Putin</nom> rilate al Ukrainio estas <tld/>i Krimeon kaj el la
        Donecka kaj Luhanska provincoj krei novan &Ccirc;ednestrion
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &Ccirc;amerio estis iam lo&gcirc;ata de albanetnuloj apartenantaj kaj al la kristana kaj al
        la islama religioj, kiam Grekio <tld/>is la regionon en 1913, okazis
        etnopurigado kaj genocido
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2011/010443.php">Malpermeso pro deveno</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="milit.0akiri">militakiri</ref>,
      <ref cel="okupaci.0i">okupacii</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_a;&c_n;&c_e;&c_k;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_h;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>angliedern</trd>,
      <trd>annektieren</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_t;&a_S;&a_r;&a_f; &f_k;&a_r;&a_d;&a_n;</trd>,
      <trd>&a_A;&a_sh;&a_gh;&a_A;&a_l; &f_k;&a_r;&a_d;&a_n;</trd>,
      <trd>&a_t;&a_s;&a_kh;&f_y;&a_r; &f_k;&a_r;&a_d;&a_n;</trd>
    </trdgrp>
    <trd lng="fr">annexer</trd>
    <trd lng="nl">annexeren</trd>
    <trd lng="it">annettere</trd>
    <trdgrp lng="ru">
      <trd>&c_a;&c_n;&c_n;&c_je;&c_k;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_h;&c_v;&c_a;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>inf&ouml;rliva</trd>,
      <trd>annektera</trd>
    </trdgrp>
  </snc>
  <snc mrk="aneks.0i.KOMUNE">
    <dif>
      Aligi ion malpli gravan kaj ne&ccirc;efan al io &ccirc;efa kaj pli grava.
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_a;&c_l;&c_u;&c_ch;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_d;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>beilegen</trd>,
      <trd>anh&auml;ngen</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_D;&a_m;&f_y;&a_m;&a_h; &f_k;&a_r;&a_d;&a_n;</trd>,
      <trd>&a_m;&a_n;&a_D;&a_m; &f_k;&a_r;&a_d;&a_n;</trd>,
      <trd>&a_A;&a_l;&a_H;&a_A;&a_q; &f_k;&a_r;&a_d;&a_n;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>anektovat</trd>,
    <trd>n&aacute;siln&ecaron; p&rcaron;ipojit</trd>,
    <trd>zabrat</trd>
  </trdgrp>
  <trd lng="en">annex</trd>
  <trd lng="es">anexar</trd>
  <trdgrp lng="fr">
    <trd>joindre</trd>,
    <trd>annexer</trd>
  </trdgrp>
  <trd lng="he">&lamed;&#x5B0;&samekh;&#x5B7;&pe;&#x5BC;&#x5B5;&het;&#x5B7;</trd>
  <trd lng="it">aggiungere</trd>
  <trd lng="nl">toevoegen</trd>
  <trd lng="pt">anexar</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_s;&c_o;&c_je;&c_d;&c_i;&c_n;&c_ja;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>anektova&tcaron;</trd>,
    <trd>n&aacute;silne zabra&tcaron; <klr>(&uacute;zemie)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x430;&#x43D;&#x435;&#x43A;&#x441;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x437;&#x430;&#x445;&#x43E;&#x43F;&#x43B;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x437;&#x430;&#x433;&#x430;&#x440;&#x431;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x440;&#x438;&#x454;&#x434;&#x43D;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="aneks.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="aneks.0ado.POL">
    <dif>
      Ago de tiu, kiu <tld/>as:
      <ekz>
        re&gcirc;o <nom>Kamehameha</nom> <klr>[&#x2026;]</klr> regis la Havajajn Insulojn anta&ubreve; ties <tld/>ado
        fare de Usono
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2005/008508.php">Ua mau ke ea o ka aina ka pono</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel vi travivis la <tld/>adon de
        Krimeo?
        <fnt><bib>Monato</bib>, <lok>2012-2018</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="fr">annexation</trd>
    <trd lng="pt">anexa&ccedil;&atilde;o</trd>
  </snc>
  <trdgrp lng="de">
    <trd>Annexion</trd>,
    <trd>Angliederung</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x430;&#x43D;&#x435;&#x43A;&#x441;&#x456;&#x44F;</trd>,
    <trd>&#x437;&#x430;&#x445;&#x43E;&#x43F;&#x43B;&#x435;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x437;&#x430;&#x433;&#x430;&#x440;&#x431;&#x430;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43F;&#x440;&#x438;&#x454;&#x434;&#x43D;&#x430;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>


<drv mrk="aneks.0ajxo">
  <kap><tld/>a&jcirc;o
    <fnt><bib>PV</bib></fnt>
  </kap>
  <snc mrk="aneks.0ajxo.POL">
    <dif>
      Aneksita lando.
    </dif>
    <trd lng="be">&c_a;&c_n;&c_e;&c_k;&c_s;&c_a;&c_v;&c_a;&c_n;&c_a;&c_ja; &c_t;&c_e;&c_r;&c_y;&c_t;&c_o;&c_r;&c_y;&c_ja;</trd>
    <trdgrp lng="de">
      <trd>Erwerbung</trd>,
      <trd>annektiertes Gebiet</trd>
    </trdgrp>
    <trd lng="fa">&a_m;&a_n;&a_T;&a_q;&a_h;&a_hamza_sure; <ind>&a_A;&a_sh;&a_gh;&a_A;&a_l;&f_y;</ind></trd>
    <trd lng="fr">d&eacute;pendance</trd>
    <trd lng="it">annessione</trd>
  </snc>
  <snc mrk="aneks.0ajxo.KOMUNE">
    <dif>
      Ne&ccirc;efa&jcirc;o, dependa&jcirc;o:    
      <ekz>
        kiel <tld/>a&jcirc;ojn la libro enhavas kronologian tabelon 
        <fnt><bib>Monato</bib>, <lok>1997-2003</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akceso.0a">akcesora</ref>,
      <ref cel="komple1.0o">komplemento</ref>,
      <ref cel="send.kun0ajxo">kunsenda&jcirc;o</ref>,
      <ref cel="suplem.0o">suplemento</ref>
    </refgrp>
    <trd lng="be">&c_d;&c_a;&c_d;&c_a;&c_t;&c_a;&c_k;</trd>
    <trdgrp lng="de">
      <trd>Annex</trd>,
      <trd>Anlage</trd>,
      <trd>Anhang</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&f_p;&f_y;&a_w;&a_s;&a_t;</trd>,
      <trd>&f_p;&f_y;&zwnj;&a_A;&a_f;&a_z;&a_w;&a_d;</trd>,
      <trd>&f_p;&f_y;&zwnj;&a_n;&a_w;&f_y;&a_s;</trd>,
      <trd>&a_D;&a_m;&f_y;&a_m;&a_h;</trd>,
      <trd>&a_m;&a_l;&a_H;&a_q;&a_h;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>pi&egrave;ce jointe</trd>,
      <trd>annexe</trd>
    </trdgrp>
  </snc>
  <trd lng="en">annex</trd>
  <trd lng="es">anexo</trd>
  <trd lng="he">&shin;&tet;&het; &mem;&samekh;&vav;&pe;&het;</trd>
  <trd lng="nl">bijvoegsel</trd>
  <trd lng="pt">anexo</trd>
  <trdgrp lng="uk">
    <trd>&#x437;&#x430;&#x433;&#x430;&#x440;&#x431;&#x430;&#x43D;&#x430; &#x442;&#x435;&#x440;&#x438;&#x442;&#x43E;&#x440;&#x456;&#x44F;</trd>,
    <trd>&#x43F;&#x440;&#x438;&#x434;&#x430;&#x442;&#x43E;&#x43A;</trd>,
    <trd>&#x434;&#x43E;&#x434;&#x430;&#x442;&#x43E;&#x43A;</trd>
  </trdgrp>
</drv>
</art>
