<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mortez</rad>/o
</kap>
<drv mrk="mortez.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      En&ccirc;iza&jcirc;o, &gcirc;enerale prismoforma, farita en ligna a&ubreve; metala peco
      por samforma <ref tip="vid" cel="tenon.0o">tenono</ref>
      de la kunigota peco.
    </dif>
  </snc>
  <trd lng="nl">tapgat</trd>
  <trd lng="ru">&c_p;&c_a;&c_z;</trd>
</drv>

<drv mrk="mortez.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on en io.
    </dif>
  </snc>
</drv>

<drv mrk="mortez.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Instrumento por fari <tld/>ojn.
    </dif>
  </snc>
</drv>

<drv mrk="mortez.0omasxino">
  <kap><tld/>oma&scirc;ino</kap>
  <snc>
    <dif>
      Ma&scirc;ino por fari <tld/>ojn.
    </dif>
  </snc>
</drv>
</art>
