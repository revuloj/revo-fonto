<kap>
  <rad>mortez</rad>/o
</kap>
<drv mrk="mortez.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      En&ccirc;iza&jcirc;o, &gcirc;enerale prismoforma, farita en ligna
      a&ubreve; metala peco por samforma <ref tip="vid" cel="tenon.0o">tenono</ref>
      de la kunigota peco:
      <ekz>
        en la mezepoko oni konstruis malmultekoste domojn el ligno, sen najloj la&ubreve; la tekniko de la tenono kaj <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Elisabeth Le Dru</aut>
          <vrk><url ref="&Monato;/2010/009948.php">Vizito al Vannes</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="fold.0o">foldo</ref> kaj
       <ref cel="lang.0o.FIG">lango</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_p;&c_a;&c_r;</trd>
  <trd lng="bg">&c_g;&c_n;&c_je;&c_z;&c_d;&c_o;</trd>
  <trdgrp lng="cs">
    <trd>dlaba&ccaron;ka</trd>,
    <trd>dl&aacute;to</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Zapfenloch</trd>,
    <trd>Schlitz</trd>
  </trdgrp>
  <trd lng="en">mortise</trd>
  <trd lng="fr">mortaise</trd>
  <trd lng="nl">tapgat</trd>
  <trdgrp lng="pl">
    <trd>wpust</trd>,
    <trd>otw&oacute;r <ind>czopowy</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_z;</trd>
  <trdgrp lng="sk">
    <trd>dlaba&ccaron;ka</trd>,
    <trd>dl&aacute;to</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x430;&#x437;</trd>,
    <trd>&#x433;&#x43D;&#x456;&#x437;&#x434;&#x43E;</trd>
  </trdgrp>
</drv>

