<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mortez</rad>/o
</kap>
<drv mrk="mortez.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      En&ccirc;iza&jcirc;o, &gcirc;enerale prismoforma, farita en ligna
      a&ubreve; metala peco por samforma
      <ref tip="vid" cel="tenon.0o">tenono</ref>
      de la kunigota peco.
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_p;&c_a;&c_r;</trd>
  <trd lng="bg">&c_g;&c_n;&c_je;&c_z;&c_d;&c_o;</trd>
  <trdgrp lng="cs">
    <trd>dlabačka</trd>,
    <trd>dláto</trd>
  </trdgrp>
  <trd lng="de">Zapfenloch</trd>
  <trd lng="en">mortise</trd>
  <trd lng="fr">mortaise</trd>
  <trd lng="nl">tapgat</trd>
  <trdgrp lng="pl">
    <trd>wpust</trd>,
    <trd>otw&oacute;r <ind>czopowy</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_z;</trd>
  <trdgrp lng="sk">
    <trd>dlabačka</trd>,
    <trd>dláto</trd>
  </trdgrp>
</drv>

<drv mrk="mortez.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on en io.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>dlabat</trd>,
    <trd>čepovat <klr>(tech.)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>spájať čapmi</trd>,
    <trd>čapovať</trd>
  </trdgrp>
</drv>

<drv mrk="mortez.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Instrumento por fari <tld/>ojn.
    </dif>
  </snc>
  <trd lng="cs">dláto</trd>
  <trd lng="sk">čapovací nástroj</trd>
</drv>

<drv mrk="mortez.0omasxino">
  <kap><tld/>oma&scirc;ino</kap>
  <snc>
    <dif>
      Ma&scirc;ino por fari <tld/>ojn.
    </dif>
  </snc>
</drv>
</art>
