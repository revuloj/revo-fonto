<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>taburet</rad>/o
</kap>
<drv mrk="tabure.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Eta <ref tip="super" cel="segx.0o.meblo">seĝo</ref> sen dorsapogilo:
      <ekz>
        avino ordonis, ke oni alportu <tld/>ojn por la oldulinoj
        <fnt><bib>RugxDom</bib>, <lok>ĉapitro 43a, volumo 2a, p.
        28a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_b;&c_u;&c_r;&c_e;&c_t;</trd>
  <trd lng="de">Hocker</trd>
  <trd lng="fr">tabouret</trd>
  <trdgrp lng="hu">
    <trd>zs&aacute;moly</trd>,
    <trd>&uuml;l&odblac;ke</trd>,
    <trd>s&aacute;mli</trd>
  </trdgrp>
  <trd lng="nl">krukje</trd>
  <trdgrp lng="pt">
   <trd>tamborete</trd>,
   <trd>mocho</trd>,
   <trd>banco</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_a;&c_b;&c_u;&c_r;&c_je;&c_t;</trd>,
    <trd>&c_t;&c_a;&c_b;&c_u;&c_r;&c_je;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sv">pall<klr> (att sitta p&aring;)</klr></trd>
</drv>
</art>
