<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tabure.xml,v 1.25 2020/07/19 10:10:40 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>taburet</rad>/o
</kap>
<drv mrk="tabure.0o">
  <kap><tld/>o</kap>
  <snc mrk="tabure.0o.segxo">
    <dif>
      Eta <ref tip="lst" lst="voko:mebloj" cel="segx.0o.meblo">se&gcirc;o</ref> sen
      dorsapogilo:
      <ekz>
        la infanoj sidas ankora&ubreve; tre bonkondute <klr>[&#8230;]</klr> sur sia <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        povra kamparano sidanta sur malalta <tld/>o en angulo
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
      <ekz>
        servistoj <klr>[&#8230;]</klr> starigis anta&ubreve; la trono multekostajn <tld/>ojn por la
        ministro kaj generaloj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e &scirc;iaj piedoj sur <tld/>o ku&scirc;is malgranda hundo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        avino ordonis, ke oni alportu <tld/>ojn por la oldulinoj
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 43a, volumo 2a, p.
        28a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_b;&c_u;&c_r;&c_e;&c_t;</trd>
  <trdgrp lng="ca">
    <trd>escambell</trd>,
    <trd>tamboret</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>stoli&ccaron;ka bez op&ecaron;radla</trd>,
    <trd>taburet</trd>
  </trdgrp>
  <trd lng="de">Hocker</trd>
  <trd lng="en">stool</trd>
  <trd lng="fa">&f_ch;&a_h;&a_A;&a_r;&f_p;&a_A;&f_y;&a_h;</trd>
  <trd lng="fr">tabouret</trd>
  <trdgrp lng="hu">
    <trd>zs&aacute;moly</trd>,
    <trd>&uuml;l&odblac;ke</trd>,
    <trd>s&aacute;mli</trd>
  </trdgrp>
  <trd lng="ja"><klr tip="amb">(&#32972;&#12418;&#12383;&#12428;&#12398;&#12394;&#12356;)
  </klr>&#33136;&#25499;
  [<ind>&#12371;&#12375;&#12363;&#12369;</ind>]</trd>
  <trdgrp lng="nl">
    <trd>kruk<klr tip="ind"> (meubel)</klr></trd>
    <trd>krukje</trd>
  </trdgrp>
  <trd lng="pl">taboret</trd>
  <trdgrp lng="pt">
   <trd>tamborete</trd>,
   <trd>mocho</trd>,
   <trd>banco</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_a;&c_b;&c_u;&c_r;&c_je;&c_t;</trd>,
    <trd>&c_t;&c_a;&c_b;&c_u;&c_r;&c_je;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">taburetka</trd>
  <trd lng="sv">pall<klr> (att sitta p&aring;)</klr></trd>
</drv>
</art>
