<drv mrk="rezist.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="rezist.0i.KOMUNE">
    <dif>
      <ref tip="vid" cel="ced.mal0i">Malcedi</ref>,
      <ref tip="vid" cel="star.kontraux0i">kontra&ubreve;stari</ref>:
      <ekz>
        vi ne povos <tld/>i anta&ubreve; viaj malamikoj
        <fnt><bib>MT</bib>, <lok>&Jos; 7:13</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>u la diablon, kaj li forkuros de vi
        <fnt><bib>NT</bib>, <lok>&Jak; 4:7</lok></fnt>;
      </ekz>
      <ekz>
        al <nom>Ursus</nom> neniu kapablos <tld/>i
        <fnt><aut>H. Sienkewicz, trad. L. Zamenhof</aut>, <vrk>Quo
        vadis?</vrk>, <lok>1933</lok></fnt>;
      </ekz>
      <ekz>
        tiu homa inklino <tld/>i kontra&ubreve; la propra bono
        <fnt>
          <bib>BonaLingvo</bib>
          <lok>6. Ies Malfido, Fido Mia</lok>
        </fnt>;
      </ekz>
      <ekz>
        se estas virto <tld/>i fizike kontra&ubreve; fizikaj atakoj, des pli estas aprobinda la
        mensa sindefendo
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        toleremo devas <tld/>i kontra&ubreve; maltoleremo
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2004/008301.html">La monstro denove inter ni</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi lin <tld/>is viza&gcirc;on kontra&ubreve; viza&gcirc;o
        <fnt>
          <bib>NT</bib>
          <lok>Galatoj 2:11</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ten.el0i">elteni</ref>,
      <ref cel="persis.0i">persisti</ref>
    </refgrp>
    <trd lng="de">abwehren</trd>
    <trdgrp lng="pl">
      <trd>opiera&cacute; si&eogonek;</trd>,
      <trd>stawia&cacute; <ind>op&oacute;r</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="rezist.0i.FIZ_ELE">
    <dif>
      <klr>(p.p. materialo)</klr>
      Havi la econ kontra&ubreve;i la efikon de iuj
      eksteraj influoj:
      <ekz>
        la &scirc;nureto ne longe <tld/>is
        <fnt><bib>Mortul&Scirc;ip</bib></fnt>;
      </ekz>
      <ekz>
        oni transiris al la tiam relative nova metalo aluminio, &ccirc;ar &gcirc;i pli bone <tld/>as
        korodon
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="&Monato;/2008/009517.php">Feli&ccirc;an datrevenon!</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        japanoj <klr>[&#8230;]</klr> scias konstrui e&ccirc; nubskrapulojn tiel, ke ili <tld/>as al
        tertremo
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2005/008775.php">Humiligita Usono</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      kaj pli speciale:
    </dif>
    <subsnc mrk="rezist.0i.FIZ">
      <uzo tip="fak">FIZ</uzo>
      <dif>
        ne &scirc;an&gcirc;i sian formon sub iuj fortoj;
      </dif>
    </subsnc>
    <subsnc mrk="rezist.0i.ELE">
      <uzo tip="fak">ELE</uzo>
      <dif>
        prezenti nenulan <ref cel="rezist1.0o">rezistancon</ref>
        al kurento.
      </dif>
    </subsnc>
    <trd lng="pl">stawia&cacute; <ind>op&oacute;r</ind></trd>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_p;&c_r;&c_a;&c_c;&c_ib;&c_w;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
  <trd lng="ca">resistir</trd>
  <trdgrp lng="cs">
    <trd>b&yacute;t odoln&yacute;</trd>,
    <trd>odporovat</trd>,
    <trd>vzdorovat</trd>,
    <trd>vzp&iacute;rat se</trd>,
    <trd>&ccaron;elit</trd>
  </trdgrp>
  <trd lng="de">widerstehen</trd>
  <trd lng="en">resist</trd>
  <trd lng="es">resistir</trd>
  <trd lng="fr">r&eacute;sister</trd>
  <trd lng="hu">ellen&aacute;ll</trd>
  <trdgrp lng="ja">
    <trd>&#x62B5;&#25239;&#12377;&#x308B;  <pr>&#12390;&#12356;&#12371;&#12358;&#12377;&#x308B;</pr></trd>,
    <trd>&#x53CD;&#25239;&#12377;&#x308B;  <pr>&#x306F;&#12435;&#12371;&#12358;&#12377;&#x308B;</pr></trd>,
    <trd>&#32784;&#12360;&#x308B;  <pr>&#x305F;&#12360;&#x308B;</pr></trd>,
    <trd>&#x62B5;&#25239;&#12434;&#x793A;&#12377;  <pr>&#12390;&#12356;&#12371;&#12358;&#12434;&#12375;&#12417;&#12377;</pr></trd>,
    <trd>&#32784;&#24615;&#12434;&#x793A;&#12377;  <pr>&#x305F;&#12356;&#x305B;&#12356;&#12434;&#12375;&#12417;&#12377;</pr></trd>
  </trdgrp>
  <trd lng="nl">weerstaan</trd>
  <trd lng="pt">resistir</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_o;&c_k;&c_a;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;
    <ind>&c_s;&c_o;&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kl&aacute;s&tcaron; odpor</trd>,
    <trd>odol&aacute;va&tcaron;</trd>,
    <trd>odporova&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">motst&aring;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_u;&c_ch;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_o;&c_p;&c_i;&c_r;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_ch;&c_i;&c_n;&c_i;&c_t;&c_i; &c_o;&c_p;&c_ib;&c_r;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_t;&c_i;&c_s;&c_t;&c_o;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_n;&c_je; &c_p;&c_ib;&c_d;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#29300; <pr>d&#x1D0;</pr></trd>,
    <trd>&#x89DD; <pr>d&#x1D0;</pr></trd>,
    <trd>&#x62B5;&#25239; <pr>d&#x1D0;k&#xE0;ng</pr></trd>,
    <trd>&#x62B5;&#x5FA1; <pr>d&#x1D0;y&#xF9;</pr></trd>,
    <trd>&#x62B5;&#x79A6; <pr>d&#x1D0;y&#xF9;</pr></trd>,
    <trd>&#25239; <pr>k&#xE0;ng</pr></trd>,
    <trd>&#x53CD;&#25239; <pr>f&#x1CE;nk&#xE0;ng</pr></trd>,
    <trd>&#x62D2; <pr>j&#xF9;</pr></trd>,
    <trd>&#x5F37;&#x5FCD; <pr>qi&#xE1;ngr&#x11B;n</pr></trd>,
    <trd>&#x5F3A;&#x5FCD; <pr>qi&#xE1;ngr&#x11B;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rezist.0o">
  <kap><tld/>o</kap>
  <snc ref="rezist.0i.KOMUNE" mrk="rezist.0o.KOMUNE">
    <dif>
      Ago <tld/>i <sncref ref="rezist.0i.KOMUNE"/>:
      <ekz>
        en tiuj kontra&ubreve;a&jcirc;oj, en tiu <tld/>o <klr>[&#8230;]</klr> de <nom>Ligia</nom> estis, krome, io
        nekomprenebla por li
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        kontra&ubreve;aj opinioj, esprimataj per aproboj, <tld/>oj, interrompoj, rimarkoj, krioj
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la spirito estas nevenkebla krom se &gcirc;i mem rezignas pri defendo kaj <tld/>o
        <fnt>
          <bib>Ret</bib>
          <lok>Aldono</lok>
        </fnt>!
      </ekz>
      <ekz>
        oni komprenis la obstinan <tld/>on de la malnovaj lo&gcirc;antoj al la projekto krei
        <klr>[&#8230;]</klr> lar&gcirc;an &scirc;oseon
        <fnt>
          <bib>ChB</bib>
          <lok>1</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_u;&c_p;&c_r;&c_a;&c_c;&c_ib;&c_w;</trd>,
      <trd>&c_a;&c_d;&c_p;&c_o;&c_r;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_c;&c_ib;&c_s;&c_t;&c_a;&c_ja;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trd lng="de">Abwehr</trd>
  </snc>
  <snc ref="rezist.0i.FIZ_ELE" mrk="rezist.0o.FIZ_ELE">
    <dif>
      <klr>(de materialo)</klr>
      Kapablo <tld/>i<sncref ref="rezist.0i.FIZ_ELE"/>,
      grando &gcirc;in karakterizanta.
    </dif>
    <subsnc mrk="rezist.0o.FIZ">
      <uzo tip="fak">FIZ</uzo>
      <fnt><bib>PIV2</bib></fnt>
      <dif>
        <klr>(de materialo)</klr>
        Konstanto, karakterizanta &gcirc;ian kapablon
        <tld/>i<sncref ref="rezist.0i.FIZ"/>.
      </dif>
      <trd lng="de">Festigkeit</trd>
      <trd lng="en">structural <ind>robustness</ind></trd>
      <trd lng="ru">&c_p;&c_r;&c_o;&c_ch;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
      <adm>
        Tio estas pli-malpli la difino el PIV2, sed kiun
        fizikan grandon &gcirc;i precize celas?? En la sama
        verko rimarkindas la parentezo "pp pri la rezisto
        de la materialoj", kiu uzas "rezisto" kun alia signifo
        (pli-malpli: kapablo rezisti). Cetere mi ne certas,
        ke "rezisto de la materialoj" estas ta&ubreve;ga nomo por
        la koncerna fako.
        <aut>MB</aut>
      </adm>
    </subsnc>
    <subsnc mrk="rezist.0o.rezistanco">
      <uzo tip="fak">ELE</uzo>
      <fnt><bib>ScTerm</bib><lok>p. 35</lok></fnt>
      <uzo tip="stl">ARK</uzo>
      <ref tip="dif" cel="rezist1.0o">rezistanco</ref>
    </subsnc>
    <subsnc mrk="rezist.0o.rezistivo">
      <uzo tip="fak">ELE</uzo>
      <fnt><bib>PIV2</bib></fnt>
      <uzo tip="stl">EVI</uzo>
      <ref tip="dif" cel="rezist2.0o">rezistivo</ref>
    </subsnc>
    <trd lng="be">&c_s;&c_u;&c_p;&c_r;&c_a;&c_c;&c_ib;&c_w;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>odolnost</trd>,
    <trd>odpor</trd>,
    <trd>rezistence</trd>,
    <trd>vzdor</trd>
  </trdgrp>
  <trd lng="de">Widerstand</trd>
  <trd lng="en">resistance</trd>
  <trd lng="es">resistencia</trd>
  <trd lng="fr">r&eacute;sistance</trd>
  <trdgrp lng="ja">
    <trd>&#x62B5;&#25239;  <pr>&#12390;&#12356;&#12371;&#12358;</pr></trd>,
    <trd>&#x53CD;&#25239;  <pr>&#x306F;&#12435;&#12371;&#12358;</pr></trd>,
    <trd>&#25239;&#x529B;  <pr>&#12371;&#12358;&#x308A;&#12423;&#x304F;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>weerstand</trd>,
    <trd>tegenstand</trd>,
    <trd>verzet</trd>
  </trdgrp>
  <trd lng="pl">op&oacute;r</trd>
  <trd lng="ru">&c_s;&c_o;&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>nepoddajnos&tcaron;</trd>,
    <trd>odolnos&tcaron;</trd>,
    <trd>odpor</trd>,
    <trd>rezistencia</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_o;&c_p;&c_ib;&c_r;</trd>,
    <trd>&c_o;&c_p;&c_i;&c_r;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_s;&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_t;&c_i;&c_d;&c_ib;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_t;&c_i;&c_s;&c_t;&c_o;&c_ja;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#30005;&#x963B; <pr>di&#xE0;nz&#x1D4;</pr></trd>,
    <trd>&#x96FB;&#x963B; <pr>di&#xE0;nz&#x1D4;</pr></trd>
  </trdgrp>
</drv>

