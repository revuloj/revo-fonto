<art mrk="$Id: lukum.xml,v 1.8 2024/01/15 19:32:08 revo Exp $">
<kap>
  <rad>lukum</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="lukum.0o">
  <kap><tld/>o</kap>
  <snc mrk="lukum.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Turkdevena dol&ccirc;a <ref tip="super" cel="frand.0ajxo.KUI">franda&jcirc;o</ref>,
      ordinare en formo de mola, iom elasta kubo, kun fruktaj a&ubreve; spicaj
      enmetoj:
      <ekz>
        en <klr>...</klr> butiko nomita &leftquot;Bagdad&#x201D; &scirc;i estis a&ccirc;etinta
        
        <tld/>ojn
        <fnt>
          <aut>H. Rapley</aut>,
          <vrk>Blanka dramo en Bureso</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2004a, numero 9a, p. 30a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_u;&c_k;&c_u;&c_m;</trd>
  <trd lng="bg">&c_l;&c_o;&c_k;&c_u;&c_m;</trd>
  <trd lng="cs">tureck&yacute; med</trd>
  <trd lng="de">Lokum</trd>
  <trd lng="en">turkish delight</trd>
  <trdgrp lng="fr">
    <trd>loukoum</trd>,
    <trd>rahat-loukoum</trd>
  </trdgrp>
  <trd lng="nl">Turks fruit</trd>
  <trd lng="pl">rachat&lstroke;ukum</trd>     
  <trd lng="ro">rahat</trd>
  <trd lng="ru">&c_l;&c_u;&c_k;&c_u;&c_m;</trd>
  <trd lng="sk">tureck&yacute; med</trd>
  <trd lng="tr">lokum</trd>
  <trd lng="uk">&#x43B;&#x443;&#x43A;&#x443;&#x43C;</trd>
  <trdgrp lng="zh">
    <trd>&#x571F;&#x8033;&#x5176;&#x8EDF;&#x7CD6; <pr>T&#x1D4;&#x11B;rq&#xED;ru&#x1CE;nt&#xE1;ng</pr></trd>,
    <trd>&#x571F;&#x8033;&#x5176;&#x8F6F;&#x7CD6; <pr>T&#x1D4;&#x11B;rq&#xED;ru&#x1CE;nt&#xE1;ng</pr></trd>
  </trdgrp>
</drv>
