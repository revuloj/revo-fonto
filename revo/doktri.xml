<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>doktrin</rad>/o
</kap>
<drv mrk="doktri.0o">
  <kap><tld/>o</kap>
  <snc mrk="doktri.0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <uzo tip="fak">REL</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Kohera tuto de principoj a&ubreve; teorioj, kiujn iu religio,
      filozofio, scienco a&ubreve; aparta skolo de ili instruas:
      <ekz>
        la <tld/>o de Epikuro;
      </ekz>
      <ekz>
        vi estos bona servanto de Kristo Jesuo, nutrita per la vortoj de
        la fido kaj de la bona <tld/>o <klr>...</klr>; sed forrifuzu
        profanajn kaj avinajn fabelojn
        <fnt><bib>NT</bib>, <lok>&Ti1; 4:6-7</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dogm.0aro">dogmaro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_k;&c_t;&c_r;&c_y;&c_n;&c_a;</trd>
  <trd lng="de">Doktrin</trd>
  <trd lng="en">doctrine</trd>
  <trd lng="es">doctrina</trd>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_m;&a_w;&a_z;&a_h;</trd>,
    <trd>&a_d;&a_damma;&f_k;&a_t;&a_r;&f_y;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">doctrine</trd>
  <trd lng="hu">doktr&iacute;na</trd>
  <trdgrp lng="nl">
    <trd>doctrine</trd>,
    <trd>leerstelling</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>doktryna</trd>,
    <trd>szko&lstroke;a</trd>
  </trdgrp>
  <trd lng="pt">doutrina</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_k;&c_t;&c_r;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_u;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
