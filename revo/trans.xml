<drv mrk="trans.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="trans.0igi.porti">
    <dif>
      Porti <tld/>en, meti <tld/>en:
      <ekz>
        li <klr>[&#8230;]</klr> <tld/> la riveron <klr>[&#8230;]</klr> <tld/>igis tion, kion li havis
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 32:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> sinjoro deziras, ke ni &scirc;ip<tld/>igu lin al <nom>Holm</nom>
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vento transigadis la foliojn <klr>[de]</klr> la pre&gcirc;olibro
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        pro mortminacoj de kunarestitoj oni <tld/>igas lin al alia karcero
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2006/008942.php">Satiro kaj realo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        virus<tld/>igo inter homoj kaj bestoj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2005/008721.php">De kie venis kaj kial &ccirc;esis netipa
          pne&ubreve;monio?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        aerguta <tld/>igo de la viruso de &leftquot;hispana gripo&#x201D;
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2009/009882.php">Kelkaj meditoj rilate al la porka
          gripo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la elrusigo <klr>[&#8230;]</klr> krome estas <tld/>igo al alia alfabeto
        <fnt>
          <bib>Monato</bib>
          <aut>W. M. A. De Smet</aut>
          <vrk><url ref="&Monato;/2004/008418.html">Edward Croegaert, la forgesita
          lingvokreanto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="es">
      <trd>transferir</trd>,
      <trd>transmitir</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>passer par-dessus</trd>,
      <trd>transmettre</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&aacute;tvisz</trd>,
      <trd>&aacute;ttesz</trd>,
      <trd>&aacute;tjuttat</trd>,
      <trd>k&ouml;z&ouml;l <klr tip="amb">(pl. mozg&aacute;st)</klr></trd>,
      <trd>tov&aacute;bb&iacute;t</trd>
    </trdgrp>
    <trd lng="nl">overzetten</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_je;&c_r;&c_je;&c_n;&c_je;&c_s;&c_t;&c_i;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_je;&c_z;&c_t;&c_i;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_je;&c_s;&c_t;&c_i;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_je;&c_d;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_je;&c_s;&c_l;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="trans.0igi.doni">
    <dif>
      <tld lit="T"/>doni:
      <ekz>
        la fabelo en la da&ubreve;ro de mil jaroj estis <tld/>igata de unu <klr>[&#8230;]</klr>
        patrino al alia
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>igu mian nomon al la plej malproksimaj generacioj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igi <klr>[&#8230;]</klr> ian raporton, opinion, <klr>[&#8230;]</klr> artikolon,
        rakonton
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>El la mondo homarana (hilelista).</vrk>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>igu plej multe da publika havo en privatajn manojn
        <fnt>
          <bib>Monato</bib>
          <aut>Josef Mendl</aut>
          <vrk><url ref="&Monato;/2005/008560.php">De la tria E&ubreve;ropa al la kvina Monda
          Sociala Forumo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <ind>mon<tld/>igo</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="&Monato;/2010/010100.php">Tri regionoj: unu krizo - kelkaj
          precizigoj (2)</url></vrk>
        </fnt>;
        <trd lng="de">Geld&uuml;berweisung</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        emocie li ne povas <tld/>igi siajn sentojn al siaj kunvoja&gcirc;antoj
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Peeraerts</aut>
          <vrk><url ref="&Monato;/2007/009201.php">Babel</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>&uuml;bergeben</trd>,
      <trd>weitergeben</trd>
    </trdgrp>
    <trd lng="en">commit</trd>
  </snc>
  <snc mrk="trans.0igi.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <tld lit="T"/>meti datumojn de unu aplika&jcirc;o al alia, resp. alia versio de la aplika&jcirc;o,
      la&ubreve;bezone konvertante ilin por la celaplika&jcirc;o:
      <ekz>
        mi intencis efektive <tld/>igi miajn po&scirc;to-dosierojn al la nova versio
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2006/008860.php">Novaj versioj i&gcirc;is pli bonaj, sed
          restas mankoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">migrieren</trd>
  </snc>
<!-- mi ne trovis ekz-oj de tia uzo, dif iom nepreciza, &ccirc;u komuti a&ubreve; veturigi sur alian relon?
