<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: farnient.xml,v 1.8 2023/11/09 19:13:16 revo Exp $">
<kap>
  <rad>farnient</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="farnient.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="farnient.0o.KOMUNE">
    <dif>
      <ref tip="dif" cel="far.neni0o.KOMUNE">Nenifaro</ref>:
      <ekz>
        nin volvas dol&ccirc;o de l&#x2019; <tld/>o
        <fnt>
          <aut>William Auld</aut>
          <vrk><url ref="http://www.autodidactproject.org/other/auld_orkestro.html">Movimento unua:
          Andante kun multa esprimo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        dum <tld/>' de l' vespero
        <fnt>
          <aut>E. Mi&hcirc;alski</aut>,
          <vrk>Prologo</vrk>,
          <lok>1929</lok>
        </fnt>;
      </ekz>
      <ekz>
        dek tu<klr>t</klr>ajn tagojn en dol&ccirc;a <tld/>o,
        ho, benaj ferioj
        <fnt>
          <aut>M. Fern&aacute;ndez</aut>,
          <vrk>Du Gardenioj</vrk>,
          <lok>Ekstremoj, 1997</lok>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ja;&c_z;&c_mol;&c_d;&c_z;&c_je;&c_j;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_j;&c_s;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">nicned&ecaron;l&aacute;n&iacute;</trd>
  <trdgrp lng="de">
    <trd>M&uuml;&szlig;iggang</trd>,
    <trd>Dolcefarniente</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>farniente</trd>,
    <trd>oisivet&eacute;</trd>
  </trdgrp>
  <trd lng="sk">ni&ccaron;nerobenie</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_a;&c_r;&#x43C;&c_u;&c_v;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_b;&c_a;&c_j;&c_d;&c_i;&#x43A;&c_u;&c_v;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43B;&c_je;&c_d;&c_a;&c_r;&#x44E;&c_v;&c_a;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
  <trd lng="zh">&#x61D2;&#x6563; <pr>l&#x1CE;ns&#x1CE;n</pr></trd>
</drv>
</art>

