<art mrk="$Id: farnient.xml,v 1.11 2025/07/31 06:50:12 revo Exp $">
<kap>
  <rad>farnient</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="farnient.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="farnient.0o.KOMUNE">
    <dif>
      <ref tip="dif" cel="far.neni0o.KOMUNE">Nenifaro</ref>:
      <ekz>
        nin volvas dol&ccirc;o de l&#8217; <tld/>o
        <fnt>
          <aut>William Auld</aut>
          <vrk><url ref="http://www.autodidactproject.org/other/auld_orkestro.html">Movimento unua:
          Andante kun multa esprimo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        dum <tld/>' de l' vespero
        <fnt>
          <aut>E. Mi&hcirc;alski</aut>,
          <vrk>Prologo</vrk>,
          <lok>1929</lok>
        </fnt>;
      </ekz>
      <ekz>
        dek tu<klr>t</klr>ajn tagojn en dol&ccirc;a <tld/>o,
        ho, benaj ferioj
        <fnt>
          <aut>M. Fern&aacute;ndez</aut>,
          <vrk>Du Gardenioj</vrk>,
          <lok>Ekstremoj, 1997</lok>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>nenifarado</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_b;&c_ja;&c_z;&c_mol;&c_d;&c_z;&c_je;&c_j;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_j;&c_s;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>ociositat</trd>,
    <trd>inactivitat</trd>
  </trdgrp>
  <trd lng="cs">nicned&ecaron;l&aacute;n&iacute;</trd>
  <trdgrp lng="de">
    <trd>M&uuml;&szlig;iggang</trd>,
    <trd>Dolcefarniente</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>farniente</trd>,
    <trd>oisivet&eacute;</trd>
  </trdgrp>
  <trd lng="pl">obijanie si&eogonek;</trd>
  <trd lng="sk">ni&ccaron;nerobenie</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_a;&c_r;&c_m;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_b;&c_a;&c_j;&c_d;&c_i;&c_k;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_l;&c_je;&c_d;&c_a;&c_r;&c_ju;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
  <trd lng="zh">&#25042;&#25955; <pr>l&#462;ns&#462;n</pr></trd>
</drv>
</art>

