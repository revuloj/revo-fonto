<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: farnient.xml,v 1.6 2022/03/18 13:01:59 revo Exp $">
<kap>
  <rad>farnient</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="farnient.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="farnient.0o.KOMUNE">
    <dif>
      <ref tip="dif" cel="far.neni0o.KOMUNE">Nenifaro</ref>:
      <ekz>
        nin volvas dol&ccirc;o de l&#x2019; <tld/>o
        <fnt>
          <aut>William Auld</aut>
          <vrk><url ref="http://www.autodidactproject.org/other/auld_orkestro.html">Movimento unua: Andante kun multa esprimo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        dum <tld/>' de l' vespero 
        <fnt>
          <aut>E. Mi&hcirc;alski</aut>,
          <vrk>Prologo</vrk>,
          <lok>1929</lok>
        </fnt>;
      </ekz>
      <ekz>
        dek tu<klr>t</klr>ajn tagojn en dol&ccirc;a <tld/>o,
        ho, benaj ferioj
        <fnt>
          <aut>M. Fern&aacute;ndez</aut>,
          <vrk>Du Gardenioj</vrk>,
          <lok>Ekstremoj, 1997</lok>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">nicned&ecaron;l&aacute;n&iacute;</trd>
  <trdgrp lng="de">
    <trd>M&uuml;&szlig;iggang</trd>,
    <trd>Dolcefarniente</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>farniente</trd>,
    <trd>oisivet&eacute;</trd>
  </trdgrp>
  <trd lng="sk">ni&ccaron;nerobenie</trd>
  <trdgrp lng="uk">
    <trd>&#x434;&#x430;&#x440;&#x43C;&#x443;&#x432;&#x430;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x431;&#x430;&#x439;&#x434;&#x438;&#x43A;&#x443;&#x432;&#x430;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43B;&#x435;&#x434;&#x430;&#x440;&#x44E;&#x432;&#x430;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>
</art>

