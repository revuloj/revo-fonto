<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: megabitok.xml,v 1.8 2024/01/25 20:10:35 revo Exp $">
<kap>
  <rad>megabitok</rad>/o
  <fnt><bib>GDFE</bib></fnt>
</kap>
<drv mrk="megabitok.0o">
  <kap><tld/>o</kap>
  <mlg>MB</mlg>
  <snc mrk="megabitok.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <ref tip="lst" cel="unu.mezur0o" lst="voko:mezurunuoj">Mezurunuo</ref>
      de ciferecaj datumoj,
      <ref tip="prt" cel="bit.0oko.KOMP">bitok</ref>miliono
      (fakte a&ubreve; 1000000 a&ubreve; 1048576 bitokoj):
      <ekz>
        por montri pli da koloroj oni bezonas ekrankarton kun 2
        &gcirc;is 4
        <tld/>oj <klr>...</klr> da memoro
        <fnt>
          <aut>P. Coppens</aut>,
          <vrk><url ref="http://users.telenet.be/pecop/ekc/aparataro.html">
          Esperanta Komputilfaka Centrejo, aparataro</url></vrk>,
          <lok>1996-2000</lok>
        </fnt>;
      </ekz>
      <ekz>
        la e-libro ampleksas tutan <tld/>on &ccirc;ar ni uzis softvaron,
        kiu
        per tiparoj enhavantaj signojn je iomete malsamaj lar&gcirc;oj,
        plej
        bone dividas kaj &gcirc;ustigas la liniojn
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://lit.crudfactory.com">The Crud
          Factory</url></vrk>,
          <lok>[vidita en 2007-01]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_g;&c_a;&c_b;&c_a;&c_j;&c_t;</trd>
  <trdgrp lng="ca">
    <trd>megabyte</trd>,
    <trd>MB</trd>
  </trdgrp>
  <trd lng="de">Megabyte</trd>
  <trd lng="fr">m&eacute;gaoctet</trd>
  <trd lng="id">megabita</trd>
  <trd lng="it">megabyte</trd>
  <trd lng="pl">megabajt</trd>
  <trdgrp lng="zh">
    <trd>&#x5146;&#x5B57;&#x7BC0; <pr>zh&#xE0;oz&#xEC;ji&#xE9;</pr></trd>,
    <trd>&#x5146;&#x5B57;&#x8282; <pr>zh&#xE0;oz&#xEC;ji&#xE9;</pr></trd>
  </trdgrp>
</drv>
</art>

