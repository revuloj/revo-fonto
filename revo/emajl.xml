<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>emajl</rad>/o
</kap>
<drv mrk="emajl.0o">
<kap><ofc>*</ofc><tld/>o</kap>
<snc num="1">
<dif>
Vitrosimila diverskolora glazuro, per kiu oni kovras potojn,
fajencon, metalon: <ekz>nedifekteblaj estas la <tld/>aj koloroj.</ekz>
</dif>
<refgrp tip="sin">
<ref cel="lak.0o">lako</ref>.
</refgrp>
</snc>
<snc num="2">
<dif>
Speco de brila natura glazuro, kiu &scirc;irmas la dentojn. 
</dif>
</snc>
<trdgrp lng="ru">
  <trd>&c_e;&c_m;&c_a;&c_l;&c_mol;</trd>,
  <trd>&c_g;&c_l;&c_a;&c_z;&c_u;&c_r;&c_mol;</trd>
</trdgrp>
</drv>
<drv mrk="emajl.0i">
<kap><tld/>i</kap>
<gra><vspec>tr</vspec></gra>
<snc>
<dif>
Kovri per <tld/>o.  
</dif>
</snc>
<trd lng="ru">&c_e;&c_m;&c_a;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
<!--
$Log$
-->
</vortaro>
