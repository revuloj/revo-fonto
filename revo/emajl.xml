<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: emajl.xml,v 1.21 2015/07/04 08:37:33 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>emajl</rad>/o
    </kap>

    <drv mrk="emajl.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="emajl.0o.TEK">
        <uzo tip="fak">BELA</uzo>
        <uzo tip="fak">TEK</uzo>
        <dif>
          Malmola vitrosimila kovra&jcirc;o, fiksita sur fabrika&jcirc;o
          (precipe metala) per elektra-&hcirc;emia procedo a&ubreve;
          bakado, por
            protekto a&ubreve; ornamo:
          <ekz>
            nedifekteblaj estas la <tld/>aj koloroj;
          </ekz>
          <ekz>
            <tld/>o sur ar&gcirc;enta medaliono, kaserolo, elektra
            fornelo.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="glazur.0o.TEK">glazuro</ref>,
          <ref cel="lak.0o">lako</ref>
        </refgrp>
        <trd lng="be">&c_e;&c_m;&c_a;&c_l;&c_mol;</trd>
        <trd lng="bg">&c_je;&c_m;&c_a;&c_j;&c_l;</trd>
        <trdgrp lng="de">
          <trd>Email</trd>,
          <trd>Emaille</trd>,
          <trd>Schmelz</trd>
        </trdgrp>
        <trd lng="en">enamel</trd>
        <trd lng="hu">zom&aacute;nc</trd>
        <trdgrp lng="pl">
          <trd>emalia</trd>,
          <trd>szkliwo</trd>
        </trdgrp>
          <trd lng="pt">esmalte</trd>
        <trdgrp lng="ru">
          <trd>&c_e;&c_m;&c_a;&c_l;&c_mol;</trd>,
          <trd>&c_f;&c_i;&c_n;&c_i;&c_f;&c_t;&c_mol;</trd>
        </trdgrp>
      </snc>
      <snc mrk="emajl.0o.ANA">
        <uzo tip="fak">ANA</uzo>
        <dif>
          Tre malmola <ref tip="super" cel="hist.0o">histo</ref>,
          brila natura tavolo, teganta la kronon de la
          <ref tip="malprt" cel="dent.0o.busxo">dentoj</ref>.
        </dif>
        <trd lng="be">&c_e;&c_m;&c_a;&c_l;&c_mol;</trd>
        <trdgrp lng="de">
          <trd>Schmelz</trd>,
          <trd>Zahnschmelz</trd>
        </trdgrp>
        <trd lng="en">enamel</trd>
        <trd lng="hu">fogzom&aacute;nc</trd>  
        <trd lng="pl">szkliwo</trd>
        <trd lng="pt">esmalte</trd>
        <trd lng="ru">&c_e;&c_m;&c_a;&c_l;&c_mol;<klr tip="ind">
        (&c_z;&c_u;&c_b;&c_n;&c_a;&c_ja;)</klr></trd>
      </snc>
  <trdgrp lng="cs">
    <trd>email</trd>,
    <trd>poleva</trd>,
    <trd>smalt</trd>
  </trdgrp>
  <trd lng="es">esmalte</trd>
  <trd lng="fr">&eacute;mail</trd>
  <trd lng="it">smalto</trd>
  <trdgrp lng="sk">
    <trd>poleva</trd>,
    <trd>sklovina</trd>,
    <trd>smalt</trd>
  </trdgrp>
  <trd lng="sv">emalj</trd>
</drv>

    <drv mrk="emajl.0i">
      <kap><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <uzo tip="fak">BELA</uzo>
      <uzo tip="fak">TEK</uzo>
      <snc>
        <dif>
          Kovri per <tld/>o.
        </dif>
      </snc>
  <trd
      lng="be">&c_e;&c_m;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>emailovat</trd>,
    <trd>smaltovat</trd>
  </trdgrp>
  <trdgrp lng="de">
        <trd>emaillieren</trd>
      </trdgrp>
  <trd lng="es">esmaltar</trd>
  <trd lng="fr">&eacute;mailler<klr tip="ind"> (sens
      propre)</klr></trd>
  <trdgrp lng="hu">
        <trd>zom&aacute;ncoz</trd>,
        <trd>zom&aacute;nccal bevon</trd>
      </trdgrp>
  <trd lng="it">smaltare</trd>
  <trdgrp lng="pl">
        <trd>emaliowa&cacute;</trd>,
        <trd>szkliwi&cacute;</trd>
      </trdgrp>
  <trd lng="ru">&c_e;&c_m;&c_a;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">emailova≈•</trd>
</drv>

    <drv mrk="emajl.cxel0o">
      <kap>&ccirc;el<tld/>o</kap> <fnt><bib>PIV2</bib></fnt>
      <uzo tip="fak">BELA</uzo>
      <snc>
        <dif>
          Juvela artverko el diverskoloraj <tld/>aj pecoj disigitaj per
          metalaj vandoj velditaj sur surfaco, iom simila al mozaiko.
        </dif>
        <ref tip="sub" cel="emajl.drat0o">drat<tld/>o</ref>
        <ref tip="sin" cel="klozone.0o">klozoneo</ref>
      </snc>
      <trdgrp lng="fr">
        <trd>cloisonn&eacute;<klr> (joaillerie)</klr></trd>
      </trdgrp>
      <trdgrp lng="it">
        <trd>cloisonn&eacute;</trd>,
        <trd><ind>lustro</ind> di Bisanzio</trd>
      </trdgrp>
      <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_g;&c_o;&c_r;&c_o;&c_d;&c_ch;&c_a;&c_t;&c_a;&c_ja;
      &c_e;&c_m;&c_a;&c_l;&c_mol;</trd>
    </drv>

    <drv mrk="emajl.drat0o">
      <kap>drat<tld/>o</kap>
      <uzo tip="fak">BELA</uzo>
      <snc>
        <dif>
          Speco de
          <ref tip="super" cel="emajl.cxel0o">&ccirc;el<tld/>o</ref>
          en kiu la vandoj estas el platigita drato a&ubreve; filigrano:
          <ekz>
            oni konservas la bustrelikvujon de re&gcirc;o Sankta
            Ladislao la 1a,
            mirindan memora&jcirc;on de la mezepoka hungara cizelarto en
            drat<tld/>a
            tekniko
            <fnt>
              <aut>Jozefo Horvath</aut>:
                 <vrk>Gy&odblac;r a&ubreve; kie ekkokerikis
                 ventokoko</vrk>,
                <bib>Monato</bib>
                <lok>jaro 2002a, numero 8a, p. 22a</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="de">Drahtemail</trd>
      <trdgrp lng="fr">
        <trd>cloisonn&eacute;<klr> (joaillerie)</klr></trd>
      </trdgrp>
      <trdgrp lng="it">
        <trd>cloisonn&eacute;<klr> (in filigrana)</klr></trd>,
      </trdgrp>
      <trd lng="ru">&c_f;&c_i;&c_n;&c_i;&c_f;&c_t;&c_mol; &c_n;&c_a;
      &c_s;&c_k;&c_a;&c_n;&c_i;</trd>
    </drv>
  </art>
