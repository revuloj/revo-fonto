<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tik1.xml,v 1.9 2023/09/04 19:01:52 revo Exp $">
<kap>
  <rad>tik</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tik1.0o">
  <kap><tld/>o</kap>
  <snc mrk="tik1.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Speco de <ref tip="super" cel="tol.0o.TEKS">tolo</ref> el
      duobligitaj fadeno; ofte striita lina a&ubreve; kotona,
      uzata por fari sakon de matraco:
      <ekz>
        la kuiristo komencis pridemandi, kien iras
        la servisto, vestinte sin, kiel por festo,
        anstata&ubreve; la tola jaketo kaj <tld/>a anta&ubreve;tuko
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/vojo.html">
          La vojo nenien</url></vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="tik1.0o.RIM_HOM">
    <ref tip="hom" cel="tik.0o.MED">Tiko</ref>
    signifas anka&ubreve; nevolan tiri&gcirc;on de muskolo.
  </rim>
  <trdgrp lng="be">
    <trd>&c_c;&c_y;&c_t;</trd>,
    <trd>&c_c;&c_ib;&c_k; <klr tip="amb">(&c_t;&c_k;&c_a;&c_n;&c_ib;&c_n;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="de">Zwillich</trd>
  <trd lng="en"><klr>(mattress)</klr> ticking</trd>
  <trd lng="fr">coutil</trd>
  <trdgrp lng="hu">
    <trd>csinvat</trd>,
    <trd>zs&aacute;voly</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30C1;&#x30C3;&#x30AF;</trd>,
    <trd>&#x3051;&#x3044;&#x308C;&#x3093;</trd>
  </trdgrp>
  <trd lng="nl">tijk</trd>
  <trd lng="ru">&c_t;&c_i;&c_k; <klr tip="amb">(&c_t;&c_k;&c_a;&c_n;&c_mol;)</klr></trd>
  <trd lng="sk">nerv&#xF3;zne mykanie svalu</trd>
  <trdgrp lng="cs">
    <trd>tic</trd>,
    <trd>tik</trd>,
    <trd>&scaron;kub&#xE1;n&#xED;</trd>
  </trdgrp>
  <trd lng="uk">&c_t;&c_i;&#x43A;</trd>
</drv>
</art>
