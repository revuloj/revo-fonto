<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: plasm.xml,v 1.25 2023/10/20 16:38:17 revo Exp $">
<kap>
  <rad>plasm</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="plasm.0o">
  <kap><tld/>o</kap>
  <snc mrk="plasm.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Duonflua diafana substanco en viva&jcirc;o.
    </dif>
    <subsnc mrk="plasm.0o.FIZL_sanga">
      <dif>
        La likva parto de la sango, en kiu na&gcirc;as blankaj kaj
        ru&gcirc;aj sango&ccirc;eloj:
        <ekz>
          por disponigi grandan kvanton de <tld/>o, multaj homoj en Germanio rapidis
          fari&gcirc;i sangdonantoj
          <fnt>
            <bib>Monato</bib>
            <aut>Vladimir Lemelev</aut>
            <vrk><url ref="&Monato;/2011/010451.php">E&ubreve;ropo en la fajro de la epidemio</url></vrk>
            <lok>2011</lok>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="pl">                                        
        <trd>osocze</trd>,                       
        <trd>plazma</trd>,                                   
      </trdgrp>
    </subsnc>
    <subsnc mrk="plasm.0o.FIZL_cxela">
      <ref tip="dif" cel="plasm.cxelo0o.FIZL">&ccirc;elo<tld/>o</ref>
    </subsnc>
    <trd lng="hu">v&eacute;rplazma</trd>
    <trd lng="pl">cytoplazma</trd>
  </snc>
  <snc mrk="plasm.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      <ref tip="super" cel="stat.0o.FIZ">Stato</ref> de materio,
      konsistanta el jonoj, liberaj elektronoj kaj ordinare anka&ubreve; ne&ubreve;tralaj 
      atomoj kaj molekuloj; stato en kiu ekzemple trovi&gcirc;as la materio de la suno,
      tera jonosfero kaj fulmoj.
    </dif>
    <trd lng="hu">plazma</trd>
    <trd lng="pl">plazma</trd>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ja;&c_z;&c_m;&c_a;</trd>
  <trd lng="bg">&c_p;&c_l;&c_a;&c_z;&c_m;&c_a; </trd>
  <trd lng="ca">plasma</trd>
  <trd lng="cs">plazma</trd>
  <trd lng="de">Plasma</trd>
  <trd lng="en">plasma</trd>
  <trd lng="es">plasma</trd>
  <trd lng="fa">&f_p;&a_l;&a_A;&a_s;&a_m;&a_A;</trd>
  <trd lng="fr">plasma</trd>
  <trd lng="it">plasma</trd>
  <trdgrp lng="ja">
    <trd>&#x8840;&#x6F3F;  <pr>&#x3051;&#x3063;&#x3057;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x539F;&#x5F62;&#x8CEA;  <pr>&#x3052;&#x3093;&#x3051;&#x3044;&#x3057;&#x3064;</pr></trd>,
    <trd>&#x30D7;&#x30E9;&#x30BA;&#x30DE;</trd>
  </trdgrp>
  <trd lng="nl">plasma</trd>
  <trd lng="pt">plasma</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_z;&c_m;&c_a;</trd>
  <trd lng="sk">plazma</trd>
  <trd lng="uk">&c_p;&c_l;&c_a;&c_z;&c_m;&c_a;</trd>
</drv>

<drv mrk="plasm.cxelo0o">
  <kap>
    &ccirc;elo<tld/>o,
    <var>
      <kap>
        &ccirc;el<tld/>o
      </kap>
    </var>
  </kap>
  <snc mrk="plasm.cxelo0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      La enhavo de <ref tip="malprt" cel="cxel.0o.BIO">&ccirc;elo</ref>
      &ccirc;irka&ubreve;anta &gcirc;ian kernon:
      <ekz>
        sporoj <klr>[&#x2026;]</klr> kun &ccirc;el<tld/>o enhavanta 4
        oleogutetojn
        <fnt>
          <aut>C. Roux</aut>,
          <vrk><url ref="http://lichenologue.org/fichiers/docs/2009RouxNaetrocymbe.pdf">
          Naetrocymbe saxicola, likeno kun Trentepohlia</url></vrk>,
          <lok>Bull. Soc. linn. Provence, vol. 60, 2009</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="citoplasm.0o.FIZL">citoplasmo</ref>
    </refgrp>
    <trd lng="bg">&c_c;&c_i;&c_t;&c_o;&c_p;&c_l;&c_a;&c_z;&c_m;&c_a;</trd>
    <trd lng="ca">citoplasma</trd>
    <trd lng="fa">&a_s;&f_y;&a_t;&a_w;&f_p;&a_l;&a_A;&a_s;&a_m;</trd>
    <trd lng="fr">cytoplasme</trd>
    <trd lng="nl">cytoplasma</trd>
    <trd lng="pl">cytoplazma</trd>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_t;&c_a;&c_p;&c_l;&c_ja;&c_z;&c_m;&c_a;</trd>
  <trd lng="ja">&#x7D30;&#x80DE;&#x8CEA;  <pr>&#x3055;&#x3044;&#x307C;&#x3046;&#x3057;&#x3064;</pr></trd>
  <trd lng="uk">&c_c;&c_i;&c_t;&c_o;&c_p;&c_l;&c_a;&c_z;&c_m;&c_a;</trd>
</drv>
</art>
