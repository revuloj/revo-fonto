<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>plasm</rad>/o
</kap>
<drv mrk="plasm.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">ANA</uzo>
    <dif>
      La likva parto de la sango, en kiu na&gcirc;as blankaj kaj ru&gcirc;aj globetoj.
    </dif>
  </snc>
  <snc>
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Forte jonigita gaso, en kiu kvantoj de negativaj kaj pozitivaj
      &scirc;argoj estas praktike egalaj.
    </dif>
  </snc>
  <trd lng="de">Plasma</trd>
  <trd lng="en">plasma</trd>
  <trd lng="es">plasma</trd>
  <trd lng="fr">plasma</trd>
  <trd lng="nl">plasma</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_z;&c_m;&c_a;</trd>
</drv>
</art>
