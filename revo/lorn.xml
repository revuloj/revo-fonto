<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lorn</rad>/o
</kap>
<drv mrk="lorn.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lorn.0o.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      Optika tuba instrumento, kiu ebligas vidi malproksimajn objektojn,
      malpliigante ilian &scirc;ajnan distancon:
      <ekz>
        anta&ubreve; ili hodia&ubreve; montri&gcirc;is kvar &scirc;ipoj, kaj &scirc;ajnis al li, ke
        inter
        ili li rimarkas per la <tld/>o la &leftquot;Karen Bronne&rightquot;
        <fnt><bib>Fab4</bib>, <lok>historio el la dunoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">lunedenn</trd>
  <trd lng="de">Fernrohr</trd>
  <trd lng="el">&my;&omikron;&ny;&ypsilon_ton;&epsilon;&lambda;&omikron;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>spyglass</trd>,
    <trd>spy glass</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>lunette <klr>(d'approche)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>t&aacute;vcs&odblac;</trd>,
    <trd>messzel&aacute;t&oacute;</trd>
  </trdgrp>
  <trd lng="nl">enkele <ind>verrekijker</ind></trd>
  <trd lng="pt">luneta</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_z;&c_o;&c_r;&c_n;&c_a;&c_ja; &c_t;&c_r;&c_u;&c_b;&c_a;</trd>
</drv>

<drv mrk="lorn.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="lorn.0eto.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      Malgranda, facile kunportebla <ref tip="super"
      cel="lorn.0o.FOT">lorno</ref>:
      <ekz>
        mi direktis al &scirc;i mian <tld/>eton kaj rimarkis, ke &scirc;i ridetis
        al
        lia rigardo
        <fnt><bib>Mary</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">lorgnette</trd>
</drv>

<drv mrk="lorn.du0eto">
  <kap>du<tld/>eto</kap>
  <snc mrk="lorn.du0eto.FOT">
    <uzo tip="fak">FOT</uzo>
    <ref tip="dif" cel="binokl.0o">binoklo</ref>
  </snc>
  <trd lng="be">&c_b;&c_ib;&c_n;&c_o;&c_k;&c_l;&c_mol;</trd>
  <trd lng="de">Fernglas</trd>
  <trdgrp lng="es">
    <trd>gemelos</trd>,
    <trd>prism&aacute;ticos</trd>,
    <trd>binoculares</trd>
  </trdgrp>
  <trd lng="el">&kappa;&ypsilon;&alfa_ton;&lambda;&jota;&alfa;</trd>
  <trd lng="en">binoculars</trd>
  <trd lng="fr">jumelles <klr>(optique)</klr></trd>
  <trdgrp lng="hu">
    <trd>l&aacute;tcs&odblac;</trd>,
    <trd>binokul&aacute;r</trd>
  </trdgrp>
  <trd lng="nl">dubbele <ind>verrekijker</ind></trd>
  <trd lng="pt">bin&oacute;culo</trd>
  <trd lng="ru">&c_b;&c_i;&c_n;&c_o;&c_k;&c_l;&c_mol;</trd>
</drv>
</art>
