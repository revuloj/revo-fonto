<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: dolcxa.xml,v 1.13 2019/02/19 20:10:21 revo Exp $">
<kap>
<rad>dol&ccirc;amar</rad>/o
</kap>
<drv mrk="dolcxa.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:botanikaj_specioj">Specio</ref>
       de <ref tip="super" cel="solan.0o">solano</ref>
      (<trd lng="la">Solanum dulcamara</trd>), 
      grimpa <ref tip="lst" cel="veget.0ajxo" lst="voko:solanoparencoj">vegeta&jcirc;o</ref>
      kun violkoloraj floroj kaj ru&gcirc;aj beroj:
      <ekz>
        burdo polenigas <tld/>on per vibradpolenigado
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=603429"
              >Vibradpolenigado</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/f/f5/Solanum_dulcamara_(7735859646).jpg/320px-Solanum_dulcamara_(7735859646).jpg"
      prm="CC BY 2.0"
      >floroj de <tld/>o (Solanum dulcamara)
        <fnt>
          <aut>Radio Tonreg el Vieno</aut>
          <lok><url ref="&WCW;/File:Solanum_dulcamara_(7735859646).jpg"
              >https://commons.wikimedia.org/wiki/File:Solanum_dulcamara_(7735859646).jpg</url></lok>
        </fnt>
    </bld>
    <bld
      lok="&WCU;/thumb/3/32/Solanum_dulcamara_sl3.jpg/360px-Solanum_dulcamara_sl3.jpg"
      prm="CC BY-SA 4.0"
      >beroj de <tld/>o (Solanum dulcamara), Nov&eacute; Hrady, &Ccirc;e&hcirc;ujo
        <fnt>
          <aut>Stefan.lefnaer, propra verko</aut>
          <lok><url ref="&WCW;/File:Solanum_dulcamara_sl3.jpg"
              >https://commons.wikimedia.org/wiki/File:Solanum_dulcamara_sl3.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="de">Bitters&uuml;&szlig;er Nachtschatten</trd>
  <trdgrp lng="fa">
    <trd>&a_t;&a_A;&a_j;&a_r;&f_y;&a_z;&f_y; &f_p;&f_y;&f_ch;</trd>,
    <trd>&a_t;&a_A;&a_j;&a_r;&f_y;&a_z;&f_y; &a_A;&f_y;&a_r;&a_A;&a_n;&f_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>morelle</trd>,
    <trd>douce-am&egrave;re</trd></trdgrp>
  <trdgrp lng="hu">
    <trd>keser&udblac; <ind>csucsor</ind></trd>,
    <trd>cserj&eacute;s <ind>csucsor</ind></trd>,
    <trd>piros <ind>ebsz&odblac;l&odblac;</ind></trd>
  </trdgrp> 
  <trdgrp lng="nl">
    <trd>nachtschade</trd>
    <trd>bitterzoet</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>doce-amarga</trd>,
    <trd>dulcamara</trd>,
    <trd>dul&ccedil;amara</trd>,
    <trd>uva-de-c&atilde;o</trd>,
    <trd>maria-preta</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_l;&c_jo;&c_n;
&c_s;&c_l;&c_a;&c_d;&c_k;&c_o;-&c_g;&c_o;&c_r;&c_mol;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
