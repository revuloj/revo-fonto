<kap>
  <rad>meteorism</rad>/o
</kap>
<drv mrk="meteor1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Abdomena &scirc;velo, ka&ubreve;zata de intestaj gasoj:
      <ekz>
        la lakso estas la plej konstanta intesta simptomo, kiu da&ubreve;ras ofte &gcirc;is la
        senfebri&gcirc;o, <tld/>o, pligrandi&gcirc;o de la hepato kaj lieno, bronkito kaj
        relative malrapida pulso estas aliaj simptomoj de <klr>[tifoida febro]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=255400">Tifoida febro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_t;&c_e;&c_a;&c_r;&c_y;&c_z;&c_m;</trd>
  <trd lng="ca">meteorisme</trd>
  <trdgrp lng="cs">
    <trd>meteorizmus</trd>,
    <trd>nad&yacute;m&aacute;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Meteorismus</trd>,
    <trd>Bl&auml;hsucht</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>meteorism</trd>,
    <trd>tympanites</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>m&eacute;t&eacute;orisme</trd>,
    <trd>flatulence</trd>,
    <trd>ballonnement</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>haspuffad&aacute;s</trd>,
    <trd>felf&uacute;v&oacute;d&aacute;s</trd>,
    <trd>meteorizmus</trd>
  </trdgrp>
  <trd lng="ja">&#x9F13;&#x8178;  <pr>&#x3053;&#x3061;&#x3087;&#x3046;</pr></trd>
  <trdgrp lng="nl">
    <trd>tympanie</trd>,
    <trd>windzucht</trd>,
    <trd>trommelzucht</trd>
  </trdgrp>
  <trd lng="pt">meteorismo</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_t;&c_je;&c_o;&c_r;&c_i;&c_z;&c_m;</trd>,
    <trd>&c_v;&c_z;&c_d;&c_u;&c_t;&c_i;&c_je; &c_zh;&c_i;&c_v;&c_o;&c_t;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nad&uacute;vanie</trd>,
    <trd>plynatos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x435;&#x442;&#x435;&#x43E;&#x440;&#x438;&#x437;&#x43C;</trd>,
    <trd>&#x437;&#x434;&#x443;&#x442;&#x442;&#x44F; &#x436;&#x438;&#x432;&#x43E;&#x442;&#x430; (&#x433;&#x430;&#x437;&#x430;&#x43C;&#x438;)</trd>
  </trdgrp>
</drv>
