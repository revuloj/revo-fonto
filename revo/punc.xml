<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: punc.xml,v 1.17 2015/07/04 08:37:42 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>punc</rad>/a
</kap>
<drv mrk="punc.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Hele <ref tip="super" cel="rugx.0a">ru&gcirc;a</ref> kiel kampa papavo:
      <ekz>
        jen kia li, libere ku&scirc;anta en <tld/>a lintola
        &ccirc;emizo<fnt><bib>RugxDom</bib>
        <lok>&ccirc;apitro 36a, p. 509a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rugx.0a">ru&gcirc;a</ref>,
      <ref cel="skarla">skarlata</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_n;&c_s;&c_o;&c_v;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>ohnivě červený</trd>,
    <trd>rudý</trd>
  </trdgrp>
  <trd lng="de">ponceau</trd>
  <trdgrp lng="en">
    <trd>crimson red</trd>,
    <trd>poppy red</trd>,
    <trd>flaming red</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ponceau<klr> (adj.)</klr></trd>,
    <trd>rouge vif</trd>
  </trdgrp>
  <trd lng="hu">pipacsv&ouml;r&ouml;s</trd>
  <trd lng="nl">klaproosrood<klr tip="ind"> (bn.)</klr></trd>
  <trdgrp lng="pt">
    <trd>rubro</trd>,  
    <trd>rubicundo</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_u;&c_n;&c_c;&c_o;&c_v;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>jasne červený</trd>,
    <trd>ohnivo červený</trd>
  </trdgrp>
</drv>

<drv mrk="punc.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="P"/>a <ref tip="lst" cel="kolor.0o.propre"
      lst="voko:koloroj">koloro</ref>.
    </dif>
  </snc>
  <trd lng="en">crimson red</trd>
  <trdgrp lng="fr">
    <trd>ponceau<klr> (couleur)</klr></trd>
  </trdgrp>
  <trd lng="hu">pipacsv&ouml;r&ouml;s sz&iacute;n</trd>
  <trd lng="nl">klaproosrood<klr tip="ind"> (zn.)</klr></trd>
</drv>
</art>
