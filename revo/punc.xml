<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>punc</rad>/a
</kap>
<drv mrk="punc.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Hele ru&gcirc;a kiel kampa papavo:
      <ekz>
        jen kia li, libere ku&scirc;anta en <tld/>a lintola
        &ccirc;emizo<fnt><bib>RugxDom</bib>
        <lok>&ccirc;apitro 36a, p. 509a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rugx.0a">ru&gcirc;a</ref>,
      <ref cel="skarla">skarlata</ref>.
    </refgrp>
  </snc>
  <trd lng="de">ponceau</trd>
  <trdgrp lng="fr">
    <trd>ponceau<klr> (adj.)</klr></trd>,
    <trd>rouge vif</trd>
  </trdgrp>
  <trd lng="nl">hoog<ind>rood</ind></trd>
  <trd lng="ru">&c_p;&c_u;&c_n;&c_c;&c_o;&c_v;&c_y;&c_j;</trd>
</drv>

<drv mrk="punc.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="P"/>a <ref tip="super" cel="kolor.0o.propre">koloro</ref>.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>ponceau<klr> (couleur)</klr></trd>
  </trdgrp>
</drv>
</art>
