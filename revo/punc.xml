<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: punc.xml,v 1.22 2023/09/03 12:45:14 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>punc</rad>/a
</kap>
<drv mrk="punc.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Hele <ref tip="super" cel="rugx.0a">ru&gcirc;a</ref> 
      kiel kampa <ref cel="papav.0o.BOT">papavo</ref>:
      <ekz>
        brakse&gcirc;oj tegitaj per <tld/>a lana damasko
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i sur&scirc;ovis sur la kapon la <tld/>an kapu&ccirc;on kaj, malfermante la pordon,
        ekkantetis
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        jen kia li, libere ku&scirc;anta en <tld/>a lintola
        &ccirc;emizo
        <fnt><bib>RugxDom</bib>
        <lok>&ccirc;apitro 36a, p. 509a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rugx.0a">ru&gcirc;a</ref>,
      <ref cel="skarla">skarlata</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_n;&c_s;&c_o;&c_v;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>ohniv&ecaron; &ccaron;erven&yacute;</trd>,
    <trd>rud&yacute;</trd>
  </trdgrp>
    <trdgrp lng="de">
      <trd>blutrot</trd>,
      <trd>hochrot</trd>,
      <trd>karmesinrot</trd>
    </trdgrp>
  <trdgrp lng="en">
    <trd>crimson red</trd>,
    <trd>poppy red</trd>,
    <trd>flaming red</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ponceau<klr> (adj.)</klr></trd>,
    <trd>rouge vif</trd>
  </trdgrp>
  <trd lng="hu">pipacsv&ouml;r&ouml;s</trd>
  <trdgrp lng="ja">
    <trd>&#x3072;&#x306A;&#x3052;&#x3057;&#x8272;&#x306E;  [<ind>&#x3072;&#x306A;&#x3052;&#x3057;&#x3044;&#x308D;&#x306E;</ind>]</trd>,
    <trd>&#x9BAE;&#x7D05;&#x8272;&#x306E;  [<ind>&#x9BAE;&#x7D05;&#x3044;&#x308D;&#x306E;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">klaproosrood<klr tip="ind"> (bn.)</klr></trd>
  <trd lng="pl">p&aogonek;sowy</trd>
  <trdgrp lng="pt">
    <trd>rubro</trd>,  
    <trd>rubicundo</trd>
  </trdgrp>
  <trd lng="ro">purpurie</trd>
  <trd lng="ru">&c_p;&c_u;&c_n;&c_c;&c_o;&c_v;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>jasne &ccaron;erven&yacute;</trd>,
    <trd>ohnivo &ccaron;erven&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ja;&c_s;&c_k;&c_r;&c_a;&c_v;&c_o;-&c_ch;&c_je;&c_r;&c_v;&c_o;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_ch;&c_je;&c_r;&c_v;&c_o;&c_n;&c_o;&c_g;&c_a;&c_r;&c_ja;&c_ch;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="punc.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="P"/>a <ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">koloro</ref>:
      <ekz>
        <tld/>ofloraj faboj
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
<!-- mi ne certas &ccirc;u en tiu frazo temas pri koloro de pentra&jcirc;o, de honta konsterna viza&gcirc;o 
     a&ubreve; iu alia senco, eventuale e&ccirc; francismo...:
      <ekz>
        anta&ubreve; lia pentrostablo, <klr>[&#8230;]</klr> anta&ubreve; &gcirc;enerala <tld/>o oni malka&scirc;as al la
        novulo la ekziston de &gcirc;emeloj
        <fnt>
          <bib>ChL</bib>
          <lok>Noto De L&#8217; A&ubreve;toro</lok>
        </fnt>.
      </ekz>
-->
    </dif>
  </snc>
    <trdgrp lng="de">
      <trd>Blutrot</trd>,
      <trd>Hochrot</trd>,
      <trd>Karmesinrot</trd>
    </trdgrp>
  <trd lng="en">crimson red</trd>
  <trdgrp lng="fr">
    <trd>ponceau<klr> (couleur)</klr></trd>
  </trdgrp>
  <trd lng="hu">pipacsv&ouml;r&ouml;s sz&iacute;n</trd>
  <trd lng="nl">klaproosrood<klr tip="ind"> (zn.)</klr></trd>
  <trd lng="pl">p&aogonek;s</trd>
  <trd lng="ro">purpuriu</trd>
</drv>
</art>
