<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>aloj</rad>/o
</kap>
<drv mrk="aloj.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Miksa&jcirc;o de kunfanditaj metaloj.
    </dif>
    <refgrp tip="vid">
      <ref cel="amalga.0o">amalgamo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_l;&c_a;&c_w;</trd>
  <trd lng="de">Legierung</trd>
  <trd lng="en">alloy</trd>
  <trd lng="es">aleaci&oacute;n</trd>
  <trd lng="fr">alliage</trd>
  <trd lng="hu">&ouml;tv&ouml;zet</trd>
  <trd lng="nl">legering</trd>
  <trd lng="pt">liga</trd>
  <trd lng="ru">&c_s;&c_p;&c_l;&c_a;&c_v;</trd>
  <trd lng="sk">zliatina</trd>
</drv>

<drv mrk="aloj.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Miksi per <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_l;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol; <klr tip="amb">(&c_m;&c_e;&c_t;&c_a;&c_l;&c_y;)</klr></trd>
  <trd lng="de">legieren</trd>
  <trd lng="en">alloy</trd>
  <trd lng="es">alear</trd>
  <trd lng="hu">&ouml;tv&ouml;z</trd>
  <trdgrp lng="pt">
    <trd>ligar <klr>metais</klr></trd>,
    <trd>combinar <klr>metais</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_p;&c_l;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol; <klr tip="amb">(&c_m;&c_je;&c_t;&c_a;&c_l;&c_l;&c_y;)</klr></trd>
</drv>
</art>
