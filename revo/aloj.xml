<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aloj.xml,v 1.29 2023/10/12 18:05:09 revo Exp $">
<kap>
  <rad>aloj</rad>/o
</kap>
<drv mrk="aloj.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="miks.0ajxo">Miksa&jcirc;o</ref>
     de kunfanditaj <ref tip="prt" cel="metal.0o">metaloj</ref>:
      <ekz>
        <tld/>o de fero kaj nikelo
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2011/010474.php">Ri&ccirc;a tero, malri&ccirc;a popolo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ar&gcirc;entaj moneroj estis anstata&ubreve;igitaj per aliaj, &ccirc;i-foje faritaj el blanka,
        kupronikela <tld/>o senar&gcirc;enta
        <fnt>
          <bib>Monato</bib>
          <aut>DJ Cane</aut>
          <vrk><url ref="&Monato;/2012/010564.php">Monfalsada ekonomio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en etekspluato de oro oni miksas orercon kun hidrargo por gajni <tld/>on kaj
        varmigas &gcirc;in por gajni puran oron post vapori&gcirc;o de la hidrargo
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2013/011021.php">Minamata, alia tragedio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="amalga.0o">amalgamo</ref>,
      <ref cel="bronz.0o">bronzo</ref>,
      <ref cel="alfeni.0o">alfenido</ref>,
      <ref cel="latun.0o">latuno</ref>,  
      <ref cel="stanio.0o">staniolo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_l;&c_a;&c_w;</trd>
  <trd lng="bg">&c_s;&c_p;&c_l;&c_a;&c_v;</trd>
  <trd lng="de">Legierung</trd>
  <trd lng="en">alloy</trd>
  <trd lng="es">aleaci&oacute;n</trd>
  <trdgrp lng="fa">
    <trd>&a_h;&a_m;&a_b;&a_s;&a_t;&a_h;</trd>,
    <trd>&a_A_madda;&a_l;&f_y;&a_A;&f_zh;</trd>
  </trdgrp>
  <trd lng="fr">alliage</trd>
  <trd lng="he">&#x5E1;&#x5B7;&#x5D2;&#x5E1;&#x5D5;&#x5B9;&#x5D2;&#x5B6;&#x5EA;</trd>
  <trd lng="hu">&ouml;tv&ouml;zet</trd>
  <trd lng="ja">&#x5408;&#x91D1;  <pr>&#x3054;&#x3046;&#x304D;&#x3093;</pr></trd>
  <trd lng="nl">legering</trd>
  <trd lng="pl">stop<klr tip="ind"> (metali)</klr></trd>
  <trd lng="pt">liga</trd>
  <trd lng="ru">&c_s;&c_p;&c_l;&c_a;&c_v;</trd>
  <trd lng="sk">zliatina</trd>
  <trd lng="sk">ligat&uacute;ra</trd>
  <trd lng="cs">ligatura</trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x43F;&#x43B;&#x430;&#x432;</trd>,
    <trd>&#x43B;&#x456;&#x491;&#x430;&#x442;&#x443;&#x440;&#x430;</trd>
  </trdgrp>
</drv>

<drv mrk="aloj.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="miks.0i.kunmiksi">Miksi</ref>
      je <tld/>o:
      <ekz>
        malofte oni <tld/>is kupron, stanon kaj plumbon (aparta bronzo, uzita ekzemple
        por moneroj), a&ubreve; kupron kun oro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17346">Kupro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
     <ref cel="galvan1.0i">galvanizi</ref>,
     <ref cel="fand.0i">fandi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_l;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol; <klr tip="amb">(&c_m;&c_e;&c_t;&c_a;&c_l;&c_y;)</klr></trd>
  <trdgrp lng="bg">
    <trd>&c_p;&c_r;&c_a;&c_v;&c_ja; <ind>&c_s;&c_p;&c_l;&c_a;&c_v;</ind></trd>,
    <trd>&c_s;&c_m;&c_je;&c_s;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>legovat</trd>,
    <trd>m&iacute;sit kovy</trd>,
    <trd>sl&eacute;vat</trd>
  </trdgrp>
  <trd lng="de">legieren</trd>
  <trd lng="en">alloy</trd>
  <trd lng="es">alear</trd>
  <trd lng="fr">allier</trd>
  <trd lng="he">&#x5DC;&#x5D4;&#x5EA;&#x5D9;&#x5DA; &#x5DC;&#x5E1;&#x5D2;&#x5E1;&#x5D5;&#x5D2;&#x5EA;</trd>
  <trd lng="hu">&ouml;tv&ouml;z</trd>
  <trdgrp lng="pt">
    <trd>ligar <klr>metais</klr></trd>,
    <trd>combinar <klr>metais</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_p;&c_l;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol; <klr tip="amb">(&c_m;&c_je;&c_t;&c_a;&c_l;&c_l;&c_y;)</klr></trd>
  <trdgrp lng="sk">
    <trd>legova&tcaron;</trd>,
    <trd>mie&scaron;a&tcaron; kovy</trd>,
    <trd>zlieva&tcaron;</trd>
  </trdgrp>
</drv>
</art>
