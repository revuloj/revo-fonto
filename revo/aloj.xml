<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>aloj</rad>/o
</kap>
<drv mrk="aloj.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">TEK</uzo>
    <dif>
      Miksa&jcirc;o de kunfanditaj metaloj.
    </dif>
  </snc>
    <refgrp tip="vid">
      <ref cel="amalga.0o">amalgamo</ref>.
    </refgrp>
  <trd lng="be">&c_s;&c_p;&c_l;&c_a;&c_w;</trd>
  <trd lng="bg">&c_s;&c_p;&c_l;&c_a;&c_v;</trd>
  <trd lng="de">Legierung</trd>
  <trd lng="en">alloy</trd>
  <trd lng="es">aleaci&oacute;n</trd>
  <trdgrp lng="fa">
    <trd>&a_h;&a_m;&a_b;&a_s;&a_t;&a_h;</trd>,
    <trd>&a_A_madda;&a_l;&f_y;&a_A;&f_zh;</trd>
  </trdgrp>
  <trd lng="fr">alliage</trd>
  <trd lng="hu">&ouml;tv&ouml;zet</trd>
  <trd lng="nl">legering</trd>
  <trd lng="pl">stop<klr tip="ind"> (metali)</klr></trd>
  <trd lng="pt">liga</trd>
  <trd lng="ru">&c_s;&c_p;&c_l;&c_a;&c_v;</trd>
  <trd lng="sk">zliatina</trd>
  <trd lng="sk">ligatúra</trd>
  <trd lng="cs">ligatura</trd>
</drv>

<drv mrk="aloj.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">TEK</uzo>
    <dif>
      Miksi per <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_l;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol; <klr
  tip="amb">(&c_m;&c_e;&c_t;&c_a;&c_l;&c_y;)</klr></trd>
  <trdgrp lng="bg">
    <trd>&c_p;&c_r;&c_a;&c_v;&c_ja; <ind>&c_s;&c_p;&c_l;&c_a;&c_v;</ind></trd>,
    <trd>&c_s;&c_m;&c_je;&c_s;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>legovat</trd>,
    <trd>mísit kovy</trd>,
    <trd>slévat</trd>
  </trdgrp>
  <trd lng="de">legieren</trd>
  <trd lng="en">alloy</trd>
  <trd lng="es">alear</trd>
  <trd lng="fr">allier</trd>
  <trd lng="hu">&ouml;tv&ouml;z</trd>
  <trdgrp lng="pt">
    <trd>ligar <klr>metais</klr></trd>,
    <trd>combinar <klr>metais</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_p;&c_l;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol; <klr
  tip="amb">(&c_m;&c_je;&c_t;&c_a;&c_l;&c_l;&c_y;)</klr></trd>
  <trdgrp lng="sk">
    <trd>legovať</trd>,
    <trd>miešať kovy</trd>,
    <trd>zlievať</trd>
  </trdgrp>
</drv>
</art>
