<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kluk.xml,v 1.30 2023/10/18 16:50:28 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>kluk</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kluk.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kluk.0i.ZOO">
    <dif>
      <ref tip="super" cel="blek.0i.beste">Bleki</ref>
      kiel kokinoj:
      <ekz>
        la kokinoj kaj kokidoj <tld/>is kaj pepis
        <fnt>
          <bib>Fab2</bib>,
          <lok>Korta koko kaj ventkoko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kato komencis &scirc;pini kaj la kokino <tld/>i
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kokinoj <tld/>is, kaj la anasoj kriis gik-gak, gik-gak
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_v;&c_a;&c_h;&c_t;&c_a;&c_c;&c_mol;</trd>
    <trd lng="ca">escatainar</trd>
    <trd lng="de">glucken</trd>
    <trd lng="en">cluck</trd>
    <trdgrp lng="es">
      <trd>cacarear</trd>,
      <trd>cloquear</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>caqueter</trd>,
      <trd>glousser</trd>
    </trdgrp>
    <trd lng="nl">kakelen</trd>
    <trd lng="pl">gdaka&cacute;</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_u;&c_d;&c_a;&c_h;&c_t;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_k;&c_v;&c_o;&c_h;&c_t;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kluk.0i.FIG">
    <dif>
      <ref tip="dif" cel="kluk.rid0i.SON">Rid<tld/>i</ref>:
      <ekz>
        Klara entuziasme <tld/>is kaj ridegis
        <fnt>
          <aut>K. &Ccaron;apek, trad. M. Luk&aacute;&scaron;</aut>,
          <vrk><url ref="http://www.literatura.bucek.name/capekk/capek-karel_blua%20krizantemo.doc">
          Blua Krizantemo</url></vrk>,
          <lok>[sen dato]</lok>
        </fnt>.
      </ekz>
    </dif>
      <trdgrp lng="be">
    <trd>&c_g;&c_ib;&c_g;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_h;&c_ib;&c_h;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_ib;&c_h;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
    <trd lng="en">cackle</trd>
    <trd lng="es">hacer risitas<klr tip="ind"> (risue&ntilde;o)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>kd&aacute;kat</trd>,
    <trd>kvokat</trd>
  </trdgrp>
  <trd lng="ja">&#x30AF;&#x30C3;&#x30AF;&#x30C3;&#x3068;&#x9CF4;&#x304F;  <pr>&#x30AF;&#x30C3;&#x30AF;&#x30C3;&#x3068;&#x306A;&#x304F;</pr></trd>
  <trdgrp lng="sk">
    <trd>kotkod&aacute;ka&tcaron;</trd>,
    <trd>kvoka&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_k;&c_u;&c_d;&c_k;&c_u;&c_d;&c_a;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_k;&c_v;&c_o;&c_k;&c_t;&c_a;&c_t;&c_i; (&c_p;&c_r;&c_o; &c_k;&c_u;&c_r;&c_k;&c_u;)</trd>
  </trdgrp>
</drv>

<drv mrk="kluk.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="kluk.0.SON">
    <dif>
      Ordinara
      <ref tip="lst" cel="blek.0o" lst="voko:blekoj">bleko</ref>
      de koko:
      <ekz>
        la kokino palpebrumadis per unu okulo kaj
        aspektis sufi&ccirc;e bone; <ctl><tld/>, <tld/>!</ctl>
        &gcirc;i kriadis
        <fnt>
          <bib>Fab4</bib>,
          <vrk>Kion la patro faras, estas &ccirc;iam &gcirc;usta</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;!</trd>
  <trd lng="en">cluck!</trd>
  <trd lng="es">clo!</trd>
  <trd lng="fr">cot !</trd>
  <trd lng="pl">ko!</trd>
</drv>

<drv mrk="kluk.kluk0i">
  <kap>kluk<tld/>i<fnt>Z</fnt></kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <ref tip="dif" cel="glugl.0i">glugli</ref>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_l;&c_mol;&c_k;&c_a;&c_t;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>bublat</trd>,
    <trd>klokotat</trd>,
    <trd>&zcaron;blu&ncaron;kat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>glucksen</trd>,
    <trd>gluckern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>gurgle</trd>,
    <trd>glug</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>gorgotear</trd>,
    <trd>cloquear</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gargouiller</trd>,
    <trd>glouglouter</trd>
  </trdgrp>
  <trd lng="ja">&#x30C8;&#x30AF;&#x30C8;&#x30AF;&#x3068;&#x6D41;&#x308C;&#x308B;  <pr>&#x30C8;&#x30AF;&#x30C8;&#x30AF;&#x3068;&#x306A;&#x304C;&#x308C;&#x308B;</pr></trd>
  <trd lng="nl">klokken<klr tip="ind"> (v.vloeistof)</klr></trd>
  <trd lng="ru">&c_b;&c_u;&c_l;&c_mol;&c_k;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>klokota&tcaron;</trd>,
    <trd>&zcaron;blnkota&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="kluk.rid0i">
  <kap>rid<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kluk.rid0i.SON">
    <dif>
      Akute, singulte ridi;
      <ref tip="sin" cel="kluk.0i.FIG"><tld/>i<sncref/></ref>:
      <ekz>
        &ccirc;iuj feinoj rid<tld/>is forirante
        <fnt>
          <aut>Pu Songling, trad. Guozhu</aut>,
          <vrk>Muro kun Pentra&jcirc;o</vrk>,
          <lok><url ref="http://www.elerno.cn/penseo/pen26.htm">
          Penseo, 1992-08 &gcirc;is 09 (26-27)</url></lok>
        </fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hihi.0i">hihii</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_ib;&c_g;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_h;&c_ib;&c_h;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_ib;&c_h;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">chichotat se</trd>
  <trd lng="en">cackle</trd>
  <trd lng="es">hacer risitas<klr tip="ind"> (risue&ntilde;o)</klr></trd>
  <trd lng="fr">glousser<klr tip="ind"> (en riant)</klr></trd>
  <trd lng="ja">&#x30AF;&#x30C3;&#x30AF;&#x30C3;&#x3068;&#x7B11;&#x3046;  <pr>&#x30AF;&#x30C3;&#x30AF;&#x30C3;&#x3068;&#x308F;&#x3089;&#x3046;</pr></trd>
  <trd lng="sk">chichota&tcaron; sa</trd>
</drv>
</art>
