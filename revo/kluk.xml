<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>kluk</rad>/i
</kap>
<drv mrk="kluk.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Krii kiel la kokinoj:
      (f) <ekz>rid<tld/>i.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">cluck</trd>
  <trd lng="nl">kakelen</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_d;&c_a;&c_h;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_v;&c_o;&c_h;&c_t;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="kluk.kluk0i">
  <kap>kluk<tld/>i<fnt>Z</fnt></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Fari bruon de flua&jcirc;o eliranta el botelo.  
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>gurgle</trd>,
    <trd>glug</trd>
  </trdgrp>
  <trd lng="nl">klokken<klr tip="ind"> (v.vloeistof)</klr></trd>
  <trd lng="ru">&c_b;&c_u;&c_l;&c_mol;&c_k;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
