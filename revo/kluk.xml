<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>kluk</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kluk.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="kluk.0.SON">
    <dif>
      Ordinara
      <ref tip="lst" cel="blek.0o" lst="voko:blekoj">bleko</ref>
      de koko:
      <ekz>
        la kokino palpebrumadis per unu okulo kaj
        aspektis sufi&ccirc;e bone; <ctl><tld/>, <tld/>!</ctl>
        &gcirc;i kriadis
        <fnt>
          <bib>Fab4</bib>,
          <vrk>Kion la patro faras, estas &ccirc;iam &gcirc;usta</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">clo!</trd>
  <trd lng="fr">cot !</trd>
  <trd lng="pl">ko!</trd>
</drv>

<drv mrk="kluk.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kluk.0i.ZOO">
    <dif>
      <ref tip="super" cel="blek.0i.beste">Bleki</ref>
      kiel kokinoj:
      <ekz>
        la kokinoj kaj kokidoj <tld/>is kaj pepis
        <fnt>
          <bib>Fab2</bib>,
          <lok>Korta koko kaj ventkoko</lok>
        </fnt>. 
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_v;&c_a;&c_h;&c_t;&c_a;&c_c;&c_mol;</trd>
    <trd lng="ca">escatainar</trd>
    <trd lng="de">glucken</trd>
    <trd lng="en">cluck</trd>
    <trdgrp lng="es">
      <trd>cacarear</trd>,
      <trd>cloquear</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>caqueter</trd>,
      <trd>glousser</trd>
    </trdgrp>
    <trd lng="nl">kakelen</trd>
    <trd lng="pl">gdaka&cacute;</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_u;&c_d;&c_a;&c_h;&c_t;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_k;&c_v;&c_o;&c_h;&c_t;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kluk.0i.FIG">
    <ref tip="dif" cel="kluk.rid0i.SON">rid<tld/>i</ref>
    <ekz>
      Klara entuziasme <tld/>is kaj ridegis
      <fnt>
        <aut>K. &Ccaron;apek, trad. M. Luk&aacute;&scaron;</aut>,
        <vrk><url
        ref="http://www.literatura.bucek.name/capekk/capek-karel_blua%20krizantemo.doc">
        Blua Krizantemo</url></vrk>,
        <lok>[sen dato]</lok>
      </fnt>.
    </ekz>
  </snc>
</drv>

<drv mrk="kluk.kluk0i">
  <kap>kluk<tld/>i<fnt>Z</fnt></kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <ref tip="dif" cel="glugl.0i">glugli</ref>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_l;&c_mol;&c_k;&c_a;&c_t;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>glucksen</trd>,
    <trd>gluckern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>gurgle</trd>,
    <trd>glug</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>gorgotear</trd>,
    <trd>cloquear</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gargouiller</trd>,
    <trd>glouglouter</trd>
  </trdgrp>
  <trd lng="nl">klokken<klr tip="ind"> (v.vloeistof)</klr></trd>
  <trd lng="ru">&c_b;&c_u;&c_l;&c_mol;&c_k;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="kluk.rid0i">
  <kap>rid<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kluk.rid0i.SON">
    <dif>
      Akute, singulte ridi;
      <ref tip="sin" cel="kluk.0i.FIG"><tld/>i<sncref/></ref>:
      <ekz>
        &ccirc;iuj feinoj rid<tld/>is forirante
        <fnt>
          <aut>Pu Songling, trad. Guozhu</aut>,
          <vrk>Muro kun Pentra&jcirc;o</vrk>,
          <lok><url ref="http://www.elerno.cn/penseo/pen26.htm">
          Penseo, 1992-08 &gcirc;is 09 (26-27)</url></lok>
        </fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hihi.0i">hihii</ref>
    </refgrp>
  </snc>
  <trd lng="es">risita<klr tip="ind"> (risue&ntilde;o)</klr></trd>
  <trd lng="fr">glousser<klr tip="ind"> (en riant)</klr></trd>
</drv>
</art>
