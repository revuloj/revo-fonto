<art mrk="$Id: nif.xml,v 1.10 2024/01/29 15:59:10 revo Exp $">
<kap>
  <rad>nif</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>

<drv mrk="nif.0o">
  <kap><tld/>o</kap>
  <snc mrk="nif.0o.MET">
    <dif>
      Nerekonata objekto vidata en la &ccirc;ielo,
      iafoje rigardata de iuj homoj kiel
      <ref tip="super" cel="masxin.flug0o.AVI">flugma&scirc;ino</ref>
      de <ref tip="vid" cel="ter.ekster0ano.BELE">eksterteranoj</ref>:
      <ekz>
        s-ro Martin <klr>...</klr> havas hobion ser&ccirc;i &ccirc;iutage
        <tld/>o-jn sur la firmamento
        <fnt>
          <aut>H. Currie</aut>,
          <vrk>Kiulingve paroli kun NIFO-na&ubreve;toj?</vrk>,
          <lok><url ref="http://www.eventoj.hu/arkivo/eve-096.htm">Eventoj,
          1996-02, numero 96a</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="nif.0o.RIM_ETIM">
    Mallongigo de &leftquot;neidentigita fluganta objekto&rightquot;.
  </rim>
  <trd lng="bg">&c_N;&c_L;&c_O;</trd>
  <trd lng="de">Ufo</trd>
  <trd lng="en">ufo</trd>
  <trd lng="es">ovni</trd>
  <trd lng="fr">ovni</trd>
  <trd lng="it">ovni</trd>
  <trd lng="nl">ufo</trd>
  <trd lng="pl">UFO  <klr>(niezidentyfikowany obiekt lataj&aogonek;cy)</klr></trd>
  <trdgrp lng="pt">
    <trd>&oacute;vni</trd>,
    <trd>ufo</trd>,
    <trd>disco voador</trd>    
  </trdgrp>
  <trd lng="ro">OZN  <klr>(obiect zbur&abreve;tor neidentificat)</klr></trd>
  <trd lng="sk">UFO</trd>
  <trd lng="cs">UFO</trd>
  <trdgrp lng="uk">
    <trd>&#x43D;&#x435;&#x43E;&#x43F;&#x456;&#x437;&#x43D;&#x430;&#x43D;&#x438;&#x439; &#x43B;&#x456;&#x442;&#x430;&#x44E;&#x447;&#x438;&#x439; &#x43E;&#x431;&#x2019;&#x454;&#x43A;&#x442;</trd>,
    <trd>&#x41D;&#x41B;&#x41E;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x98DB;&#x789F; <pr>f&#x113;idi&#xE9;</pr></trd>,
    <trd>&#x98DE;&#x789F; <pr>f&#x113;idi&#xE9;</pr></trd>
  </trdgrp>
</drv>
</art>

