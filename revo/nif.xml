<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>nif</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>

<drv mrk="nif.0o">
  <kap><tld/>o</kap>
  <snc mrk="nif.0o.MET">
    <dif>
      Nerekonata objekto vidata en la &ccirc;ielo,
      iafoje rigardata de iuj homoj kiel
      <ref tip="super" cel="masxin.flug0o.AVI">flugma&scirc;ino</ref>
      de <ref tip="vid" cel="ter.ekster0ano.BELE">eksterteranoj</ref>:
      <ekz>
        s-ro Martin <klr>...</klr> havas hobion ser&ccirc;i &ccirc;iutage <tld/>o-jn sur la firmamento
        <fnt>
          <aut>H. Currie</aut>,
          <vrk>Kiulingve paroli kun NIFO-na&ubreve;toj?</vrk>,
          <lok><url ref="http://www.eventoj.hu/arkivo/eve-096.htm">Eventoj, 1996-02, numero 96a</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="nif.0o.RIM_ETIM">
    Mallongigo de &leftquot;neidentigita fluganta objekto&rightquot;.
  </rim>
  <trd lng="de">Ufo</trd>
  <trd lng="en">ufo</trd>
  <trd lng="es">ovni</trd>
  <trd lng="fr">ovni</trd>
  <trd lng="it">ovni</trd>
  <trdgrp lng="pt">
    <trd>&oacute;vni</trd>,
    <trd>ufo</trd>,
    <trd>disco voador</trd>    
  </trdgrp>
</drv>
</art>

