<art mrk="$Id: bankok.xml,v 1.11 2022/04/26 15:00:32 revo Exp $">
<kap>
  <rad>Bankok</rad>/o <fnt><bib>PIV3</bib></fnt>,
  <var><kap><rad var="g">Bangkok</rad>/o</kap></var>
</kap>
<drv mrk="bankok.0o">
  <kap><tld/>o, <var><kap><tld var="g"/>o</kap></var>
  </kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&Ccirc;efurbo</ref>
      de <ref tip="malprt" cel="taj.T0lando">Tajlando</ref>:
      <ekz>
        ASEM estis la unuan fojon aran&gcirc;ita en 1996 en la tajlanda &ccirc;efurbo <tld var="g"/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2004/008465.html">&Ccirc;u e&ubreve;rop-azia bloko kontra&ubreve;
          Usono?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &Gcirc;akarto kreskis pli rapide ol <nom>Kuala Lumpur</nom>, Pekino kaj <tld var="g"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=9423">&Gcirc;akarto</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> en 2010 <klr>[&#x2026;]</klr> havis 8&nbsp;280&nbsp;925 lo&gcirc;antojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=27296">Bankoko</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_B;&c_a;&c_n;&c_k;&c_o;&c_k;</trd>
  <trd lng="de">Bangkok</trd>
  <trd lng="fr">Bangkok</trd>
  <trd lng="he">&#x5D1;&#x5E0;&#x5E7;&#x5D5;&#x5E7;</trd>
  <trd lng="nl">Bangkok</trd>
  <trd lng="no">Bangkok</trd>
  <trdgrp lng="th">
    <trd>&#xE01;&#xE23;&#xE38;&#xE07;&#xE40;&#xE17;&#xE1E;&#xE21;&#xE2B;&#xE32;&#xE19;&#xE04;&#xE23;</trd>,
    <trd>&#xE1A;&#xE32;&#xE07;&#xE01;&#xE2D;&#xE01;</trd>
  </trdgrp>
  <trd lng="uk">&#x411;&#x430;&#x43D;&#x491;&#x43A;&#x43E;&#x43A;</trd>
</drv>
</art>

