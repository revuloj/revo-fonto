  <trdgrp lng="cs">
    <trd>prasklina</trd>,
    <trd>skulina</trd>,
    <trd>&scaron;t&ecaron;rbina</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Leck</trd>,
    <trd><ind>undicht</ind>e Stelle</trd>
  </trdgrp>
  <trd lng="en">leak</trd>
  <trd lng="es">fuga</trd>
  <trd lng="fr">fuite</trd>
  <trdgrp lng="hu">
    <trd>l&eacute;k</trd>,
    <trd>sziv&aacute;rg&oacute; hely</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6F0F;&#x308C;&#x53E3;  [<ind>&#x3082;&#x308C;&#x3053;&#x3046;</ind>]</trd>,
    <trd>&#x6D78;&#x6C34;&#x53E3;  [<ind>&#x3057;&#x3093;&#x3059;&#x3044;&#x3053;&#x3046;</ind>]</trd>,
    <trd>&#x6F0F;&#x96FB;&#x500B;&#x6240;  [<ind>&#x308D;&#x3046;&#x3067;&#x3093;&#x304B;&#x3057;&#x3087;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">lek</trd>
  <trdgrp lng="pl">
    <trd>przeciek</trd>,
    <trd>wyciek</trd>,
    <trd>przebicie <klr>(elektr.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_ch;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>1.netesnos&tcaron;</trd>,
    <trd>2.izola&ccaron;n&yacute; defekt <klr>(el. tech.)</klr></trd>,
