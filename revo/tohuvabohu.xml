<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tohuvabohu</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tohuvabohu.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="konfuz.0o">Konfuza</ref>, malorda kunmikso de
      &ccirc;iaj aferoj a&ubreve; agadoj:
      <ekz>
        balda&ubreve; la ejo fari&gcirc;is <tld/>o
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 61a, p. 320a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">tohubohu</trd>
  <trd lng="fr">tohu-bohu</trd>
  <trd lng="he">&tav;&vav;&he;&vav; &vav;&bet;&vav;&he;&vav;</trd>
  <trdgrp lng="hu">
    <trd>z&udblac;rzavar</trd>,
    <trd>tohuvabohu</trd>
  </trdgrp>
</drv>
</art>

