<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tohuvabohu.xml,v 1.12 2023/06/12 09:26:59 revo Exp $">
<kap>
  <rad>tohuvabohu</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tohuvabohu.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="tohuvabohu.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="konfuz.0o">Konfuzo</ref>, malorda kunmikso
      de &ccirc;iaj aferoj a&ubreve; agadoj:  
      <ekz>
        kompleta <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url ref="https://www.monato.be/2005/008691.php"
              >Familia nomo</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        balda&ubreve; la ejo fari&gcirc;is <tld/>o
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 61a, p.
        320a</lok></fnt>.
      </ekz>
      <ekz>
        <tld lit="T"/>o, <klr>[kanto de]</klr> Kajto
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470;2</lok>
        </fnt>;
      </ekz>
      <ekz>
        pelmele <tld/>a
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi komprenas tiun financ-<tld/>on? mi ne, ne vere: svarmas fakterminoj
        kaj strangaj vortoj kiel transakcio, financ-&jcirc;onglistoj (kiuj vetas kontra&ubreve;
        &scirc;tatoj &ndash; kiel funkcias tio?), kreditoj, &scirc;uld-amortizo, financ-akrobatoj,
        investobankoj, &scirc;uld-atestoj, obligacioj, kreditoroj, debitoroj, &scirc;uldonuligo,
        deficitlandoj kaj &ndash; fi! &ndash; spekulistoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2010/010078.php"
              >&Scirc;uldoj kaj kulpoj</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="hxaos.0o">&hcirc;aoso</ref>
    <refgrp>
      <ref cel="pot.miks0ajxo.FIG">mikspoto<sncref/></ref>,
      <ref cel="misxmasx.0o">mi&scirc;ma&scirc;o</ref>,
      <ref cel="pelmel.0o.KOMUNE">pelmelo</ref>,
      <ref cel="salat.0o.FIG">salato<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_r;&c_m;&c_ib;&c_d;&c_a;&c_r;</trd>,
    <trd>&c_b;&c_ja;&c_z;&c_l;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_a;</trd>,
    <trd>&c_b;&c_ja;&c_z;&c_l;&c_a;&c_d;&c_z;&c_mol;&c_d;&c_z;&c_je;</trd>,
    <trd>&c_b;&c_l;&c_y;&c_t;&c_a;&c_n;&c_ib;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>agitaci&oacute;</trd>,
    <trd>bull&iacute;cia</trd>,
    <trd>confusi&oacute;</trd>,
    <trd>desgavell</trd>,
    <trd>gran <ind>desordre</ind></trd>,
    <trd>enrenou</trd>,
    <trd>merder <klr>(fig)</klr></trd>,
    <trd>renou</trd>
  </trdgrp>
  <trd lng="en">tohubohu</trd>
  <trd lng="es">agitaci&oacute;n</trd>
  <trdgrp lng="de">
    <trd>Tohuwabohu</trd>,
    <trd>Chaos</trd>,
    <trd>heilloses <ind>Durcheinander</ind></trd>,
    <trd>Irrsal und Wirrsal</trd>
  </trdgrp>
  <trd lng="fr">tohu-bohu</trd>
  <trd lng="he">&tav;&vav;&he;&vav; &vav;&bet;&vav;&he;&vav;</trd>
  <trdgrp lng="hu">
    <trd>z&udblac;rzavar</trd>,
    <trd>tohuvabohu</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>galimatias</trd>,
    <trd>ba&lstroke;agan</trd>,
    <trd>bajzel<klr> (pot.)</klr></trd>,
    <trd>pl&aogonek;tanina</trd>,
    <trd>nieporz&aogonek;dek</trd>
  </trdgrp>
  <trd lng="pt">muvuca<klr>(g&iacute;ria)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_s;&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_k;</trd>,
    <trd>&c_s;&c_u;&c_m;&c_a;&c_t;&c_o;&c_h;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_t;&c_o;&c_l;&c_o;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&#x43E;&c_g;&c_u;-&c_v;&c_a;-&c_b;&#x43E;&c_g;&c_u;</trd>,
    <trd>&#x43F;&#x43E;&c_v;&#x43D;&c_je; &c_b;&c_je;&c_z;&#x43B;&c_a;&c_d;&c_d;&#x44F;</trd>,
    <trd>&c_h;&c_a;&#x43E;&c_s;</trd>
  </trdgrp>
</drv>
</art>

