<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vrak.xml,v 1.17 2023/02/01 08:33:59 revo Exp $">
<kap>
  <rad>vrak</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="vrak.0o">
  <kap><tld/>o</kap>
  <snc mrk="vrak.0o.ARKE">
    <dif>
      <ref tip="super" cel="romp.0itajxo">Rompita&jcirc;o</ref> de a&ubreve;to, &scirc;ipo, aviadilo a&ubreve; 
      alia <ref tip="super" cel="vetur.0ilo">veturilo</ref>:
      <ekz>
        <tld/>oj de kra&scirc;intaj aviadiloj
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="&Monato;/2011/010328.php">Fragmentoj de la dua mondmilito</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        en 1694 <tld/>i&gcirc;is dum &scirc;tormo en Mediteraneo apud &Gcirc;ibraltaro la brita milit&scirc;ipo
        <nom>Sussex</nom> <klr>[&#8230;]</klr>, la <tld/>o situas &ccirc;. 700 metrojn sub la surfaco de la
        maro
        <fnt>
          <bib>Monato</bib>
          <aut>Elson B. Snow</aut>
          <vrk><url ref="&Monato;/2004/008155.html">Trez-oro submara</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        la nigran a&ubreve;ton &ccirc;irka&ubreve;is policistoj, alveninta skipo de la fajrobrigado devis
        elsegi la vunditan &scirc;oforon el la <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="&Monato;/2009/009822.php">Katastrofa re&gcirc;inotago</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        falis nekonata helikoptero en la norda montaro de Albanio, <klr>[&#8230;]</klr>
        malgra&ubreve; malfacilaj kondi&ccirc;oj, inkluzive de ne&gcirc;o kaj nebulo, ser&ccirc;is la <tld/>on
        helikopteroj de la albana armeo
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2008/009463.php">Montara mistero</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frakas.0i.ruinigi">frakasi<sncref/></ref>,
      <ref cel="krasx.0i">kra&scirc;i</ref>,
      <ref cel="ruin.0o.ARKE">ruino</ref>
    </refgrp>
  </snc>
  <snc mrk="vrak.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="kaduk.0ulo">Kadukulo</ref>:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        policanino malkovris min kiel jam duonmortan <tld/>on en la
        stacidoma necesejo
        <fnt><aut>H. Vogt</aut>, <vrk>Paperfolioj</vrk>,
        <lok>La Gazeto, 2005:117, p. 35a</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        flegejoj estas plenaj je tiaj homaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2003/008117.html">Grizi&gcirc;anta mondo</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>    
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_sh;&c_t;&c_k;&c_ib; <klr>(&c_p;&c_a;&c_s;&c_mol;&c_l;&c_ja; &c_a;&c_v;&c_a;&c_r;&c_y;&c_ib;)</klr></trd>,
    <trd>&c_l;&c_a;&c_m;&c_a;&c_ch;&c_ch;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">vrak <klr>(lodi| auta)</klr></trd>
  <trd lng="de">Wrack</trd>
  <trd lng="en">wreck</trd>
  <trd lng="fr">&eacute;pave</trd>
  <trd lng="hu">roncs</trd>
  <trd lng="nl">wrak</trd>
  <trd lng="pt">destro&ccedil;o<klr tip="amb"> (de carro ou outro
  ve&iacute;culo)</klr></trd>
  <trdgrp lng="sk">
    <trd>vrak <klr>(auta, lode)</klr></trd>,
    <trd>vrak <klr>(auta| lode)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_ib;&c_s;&c_t;&#x44F;&#x43A; &c_r;&#x43E;&c_z;&c_b;&c_i;&c_t;&#x43E;&c_g;&#x43E; &c_s;&c_u;&c_d;&#x43D;&c_a;</trd>,
    <trd>&#x43B;&c_ib;&c_t;&c_a;&#x43A;&c_a;</trd>,
    <trd>&c_r;&c_je;&c_sh;&c_t;&#x43A;&c_i; &c_a;&c_v;&c_a;&c_r;&c_ib;&c_ji; &#x43A;&#x43E;&c_r;&c_a;&c_b;&#x43B;&#x44F;</trd>,
    <trd>&#x43B;&c_ib;&c_t;&c_a;&#x43A;&c_a;</trd>,
    <trd>&c_a;&c_v;&c_t;&#x43E;&#x43C;&#x43E;&c_b;&c_ib;&#x43B;&#x44F; &c_ib; &c_t;.&#x43F;.</trd>,
    <trd>&c_r;&#x43E;&c_z;&c_b;&c_i;&c_t;&c_je; &c_s;&c_u;&c_d;&#x43D;&#x43E;</trd>,
    <trd>&c_r;&#x43E;&c_z;&c_b;&c_i;&c_t;&c_i;&c_j; &#x43B;&c_ib;&c_t;&c_a;&#x43A;</trd>,
    <trd>&c_a;&c_v;&c_t;&#x43E;&#x43C;&#x43E;&c_b;&c_ib;&#x43B;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="vrak.auxto0ejo">
  <kap>a&ubreve;to<tld/>ejo</kap>
  <snc mrk="vrak.auxto0ejo.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      Loko por a&ubreve;taj <tld/>oj, eventuale por ilia recikligo:
      <ekz>
        la malgranda <nom>Peugeot 207</nom> <klr>[&#8230;]</klr>
        fini&gcirc;is en a&ubreve;to<tld/>ejo
        <fnt>
          <aut>S. Kl&uuml;ber, trad. D. kaj H.-G. Kaiser</aut>
          <vrk><url ref="http://www.ipernity.com/blog/cezar/4702566">Estas ja nur
          lado...</url></vrk>
          <lok>2018-06-02</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rub.fer0o">ferrubo</ref>,
      <ref cel="cikl.re0igi">recikligi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_w;&c_t;&c_a;&c_z;&c_v;&c_a;&c_l;&c_k;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Autoschrottplatz</trd>,
    <trd>Autorecyclinghof</trd>
  </trdgrp>
  <trd lng="fr">casse automobile</trd>
</drv>
</art>
