<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sekant.xml,v 1.26 2024/03/24 20:00:00 revo Exp $">
<kap>
<rad>sekant</rad>/o
</kap>
<drv mrk="sekant.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 20</lok></fnt>
  <snc mrk="sekant.0o.entriangulo">
    <dif>
      <klr>(de <ref cel="angul.0o.MAT">angulo</ref> <frm am="alpha">&alfa;</frm>)</klr>
      Inverso de &gcirc;ia <ref cel="kosinu.0o">kosinuso</ref>; 
      simb. <frm>sec &alfa;</frm> a&ubreve; <frm>sek &alfa;</frm>.
    </dif>
    <bld lok="../bld/trigo.jpg">
      <!-- la <tld/>o de angulo <frm am="alpha">&alfa;</frm> egalas al 
      <frm am="OT//OM">OT/OM</frm>
      -->
      la <tld/>o de angulo &alfa; egalas al OT/OM
    </bld>
    <trd lng="be">&c_s;&c_e;&c_k;&c_a;&c_n;&c_s;</trd>
    <trd lng="de">Sekans</trd>
    <trd lng="en">secant</trd>
    <trd lng="fr">s&eacute;cante<klr tip="ind"> (trigo.)</klr></trd>
    <trd lng="it">secante<klr> (trigon.)</klr>
    </trd><trd lng="nl">secans</trd>
    <trd lng="pl">secans</trd>
    <trd lng="ru">&c_s;&c_je;&c_k;&c_a;&c_n;&c_s;</trd>
    <trdgrp lng="zh">
    <trd>&#x6B63;&#x5272;&#x51FD;&#x6570; <pr>zh&#xE8;ngg&#x113;h&#xE1;nsh&#xF9;</pr></trd>,
    <trd>&#x6B63;&#x5272;&#x51FD;&#x6578; <pr>zh&#xE8;ngg&#x113;h&#xE1;nsh&#xF9;</pr></trd>,
    <trd>&#x6B63;&#x5272; <pr>zh&#xE8;ngg&#x113;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="sekant.0o.funkcio">
    <dif>
      Reela <ref tip="super" cel="funkci.0o.MAT">funkcio</ref>, kiu
      &jcirc;etas
      reelon <frm am="x"><k>x</k></frm> al la 
      <tld/>o <sncref ref="sekant.0o.entriangulo"/> de angulo, kies
      mezuro en 
      <ref cel="radian.0o">radianoj</ref> diferencas
      de <frm am="x"><k>x</k></frm> per oblo de <frm am="2pi">2&pi;</frm>;
      simb.
      <frm>sec</frm>
      a&ubreve; <frm>sek</frm>:
      <ekz>
        <tld/>o estas perioda funkcio, kun 
        <ref cel="period.0o.funkcio">periodo <sncref/></ref>
        <frm am="2pi">2&pi;</frm>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_e;&c_k;&c_a;&c_n;&c_s;</trd>
    <trd lng="de">Sekans</trd>
    <trd lng="en">secant</trd>
    <trd lng="fr">s&eacute;cante<klr tip="ind"> (trigo.)</klr></trd>
    <trd lng="it">secante<klr> (trigon.)</klr></trd>
    <trd lng="nl">secans</trd>
    <trd lng="pl">secans</trd>
    <trd lng="ru">&c_s;&c_je;&c_k;&c_a;&c_n;&c_s;</trd>
  </snc>
  <snc mrk="sekant.0o.sekcanto">
     <uzo tip="stl">ARK</uzo>
     <fnt><bib>PV</bib></fnt>
     <ref tip="dif" cel="sekc.0anto">sekcanto.</ref>
  </snc>
  <trdgrp lng="cs">
    <trd>sekanta</trd>,
    <trd>se&ccaron;na</trd>
  </trdgrp>
  <trd lng="hu">szek&aacute;ns</trd>
  <trd lng="ja">&#x30BB;&#x30AB;&#x30F3;&#x30C8;</trd>
  <trd lng="sk">sekans <klr>(uhla)</klr></trd>
  <trd lng="uk">&#x441;&#x435;&#x43A;&#x430;&#x43D;&#x441;</trd>
</drv>

<drv mrk="sekant.ark0o">
  <kap>ark<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      <ref cel="bild.inversa0igo">Inversa bildigo</ref> de
      la <ref cel="vast.mal0igajxo.bildigo">malvastiga&jcirc;o</ref> de
      <ref tip="super" cel="funkci.0o.MAT">funkcio</ref> 
      <ref cel="sekant.0o.funkcio"><tld/>o</ref> al la
      intervalo 
      <frm am="[0,pi//2) uu (pi//2,pi]">[0,&pi;/2[&#x222A;]&pi;/2,&pi;]</frm>; 
      simb. <frm>arcsec</frm> a&ubreve; <frm>arksek</frm>:
      <ekz>
        ark<tld/>o de <frm>1</frm> egalas al <frm>0</frm>.
      </ekz>
    </dif>
    <rim>
      Vd rimarkon sub <ref cel="ark.0o.angulo">arko</ref>.
    </rim>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_k;&c_s;&c_e;&c_k;&c_a;&c_n;&c_s;</trd>
  <trd lng="de">Arkussekans</trd>
  <trd lng="en">arc secant</trd>
  <trd lng="fr">arc s&eacute;cante</trd>
  <trd lng="hu">arkusz szek&aacute;ns</trd>  
  <trd lng="nl">arcsecans</trd>
  <trd lng="pl">arcus secans</trd>
  <trd lng="ru">&c_a;&c_r;&c_k;&c_s;&c_je;&c_k;&c_a;&c_n;&c_s;</trd>
</drv>
</art>

