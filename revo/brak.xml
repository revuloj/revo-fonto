  <trd lng="bo">&t_tha;&t_ma;&t_tsheg;&t_-a;&t_kha;&t_yata;&t_shabchu;&t_da;&t_tsheg;&t_ba;&t_yata;&t_drengbu;&t_da;&t_tsheg;</trd>
