    <trd>&#25602; <pr>l&#x1D2;u</pr></trd>,
    <trd>&#x62B1; <pr>b&#xE0;o</pr></trd>,
    <trd>&#x62E5;&#x62B1; <pr>y&#x14D;ngb&#xE0;o</pr></trd>,
    <trd>&#x62B1;&#x62E5; <pr>b&#xE0;oy&#x14D;ng</pr></trd>,
    <trd>&#x63FD;&#20837;&#24576;&#x4E2D; <pr>l&#x1CE;nr&#xF9;hu&#xE1;izh&#x14D;ng</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_d;&c_y;&c_m;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_n;&c_ja;&c_c;&c_mol;</trd>
