<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>brak</rad>/o
</kap>
<drv mrk="brak.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="brak.0o.homa_membro">
    <uzo tip="fak">ANA</uzo>
    <dif>
      La supera membro &ccirc;e la homo: 
      <ekz>
        <tld/>o en <tld/>o ili iris
        <fnt><bib>Fab1</bib>, <lok>La virineto de maro</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i pa&scirc;is &ccirc;e
        <klr>(sub)</klr><fnt>Z</fnt> la <tld/>o
        de sia edzo.
      </ekz>
    </dif>
    <trd lng="de">Arm</trd>
    <trd lng="ru">&c_r;&c_u;&c_k;&c_a; <klr>(&c_v;&c_s;&c_ja;)</klr></trd>
    <trd lng="sk">pa&zcaron;a</trd>
  </snc>
  <snc mrk="brak.0o.sxultro_gxis_kubuto">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Parto de tiu membro ekde &scirc;ultro &gcirc;is kubuto. 
    </dif>
    <trd lng="de">Oberarm</trd>
    <trd lng="ru">&c_r;&c_u;&c_k;&c_a; <klr>(&c_o;&c_t; &c_p;&c_l;&c_je;&c_ch;&c_a; &c_d;&c_o; &c_k;&c_i;&c_s;&c_t;&c_i;)</klr></trd>
    <trd lng="sk">rameno</trd>
  </snc>
  <snc mrk="brak.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Parto de objekto, kiu etendi&gcirc;as, flanke de &gcirc;i kiel <tld/>o:
      <ekz>
        <tld/>o de cirkelo, de argano, de kandelabro<fnt>Z</fnt>,
      </ekz>
      <ekz>
        de lustro, de helico, de levilo, de apogse&gcirc;o, de rivero
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="al1.0o">alo</ref>,
      <ref cel="brancx.0o">bran&ccirc;o</ref>.
    </refgrp>
    <trd lng="de">Arm</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_u;&c_ch;&c_k;&c_a;</trd>,
      <trd>&c_r;&c_u;&c_k;&c_o;&c_ja;&c_t;&c_k;&c_a;</trd>,
      <trd>&c_p;&c_l;&c_je;&c_ch;&c_o; <klr>(&c_r;&c_y;&c_ch;&c_a;&c_g;&c_a;)</klr></trd>,
      <trd>&c_r;&c_u;&c_k;&c_a;&c_v; <klr>(&c_r;&c_je;&c_k;&c_i;)</klr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>pa&zcaron;e</trd>,
    <trd>rameno</trd>
  </trdgrp>
  <trd lng="en">arm</trd>
  <trd lng="es">brazo</trd>
  <trd lng="fr">bras</trd>
  <trd lng="tr">kol</trd>
</drv>
<drv mrk="brak.sub0o">
  <kap>sub<tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="aksel.0o">Akselo</ref>.
    </dif>
  </snc>
  <trd lng="de">Achsel</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_m;&c_y;&c_sh;&c_k;&c_a;</trd>
</drv>
<drv mrk="brak.antaux0o">
  <kap>anta&ubreve;<tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Parto de la supera membro, ekde kubuto &gcirc;is mano. 
    </dif>
  </snc>
  <trd lng="de">Unterarm</trd>
  <trd lng="fr">avant-bras</trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_p;&c_l;&c_je;&c_ch;&c_mol;&c_je;</trd>
  <trd lng="sk">predlaktie</trd>
</drv>
<drv mrk="brak.0ringo">
  <kap><tld/>ringo</kap>
  <snc>
    <dif>
      braceleto. 
    </dif>
  </snc>
  <trd lng="de">Armreif</trd>
  <trd lng="es">pulsera</trd>
  <trd lng="ru">&c_b;&c_r;&c_a;&c_s;&c_l;&c_je;&c_t;</trd>
  <trd lng="sk">n&aacute;ramok</trd>
</drv>
<drv mrk="brak.0segxo">
  <kap><tld/>se&gcirc;o</kap>
  <snc>
    <dif>
      Komforta se&gcirc;o kun dors- kaj <tld/>apogilo 
    </dif>
    <refgrp tip="sin">
      <ref cel="fotel.0o">fotelo</ref>,
      <ref cel="kanap.0o">kanapo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Sessel</trd>
  <trd lng="fr">fauteuil</trd>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_s;&c_l;&c_o;</trd>
  <trd lng="sk">kreslo</trd>
  <trd lng="tr">koltuk</trd>
</drv>
<drv mrk="brak.cxirkaux0ien0igibrakumi">
  <kap>&ccirc;irka&ubreve;<tld/>i, en<tld/>igi, <tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Ccirc;irka&ubreve;igi per amba&ubreve; <tld/>oj.  
    </dif>
  </snc>
  <trd lng="de">umarmen</trd>
  <trd lng="es">abrazar</trd>
  <trdgrp lng="fr"><trd>embrasser</trd>, <trd>enlacer</trd></trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_t;&c_mol; &c_v; &c_o;&c_b;&c_malmol;&c_ja;&c_t;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">obja&tcaron;</trd>
</drv>
</art>
