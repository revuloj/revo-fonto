      <trd>&c_r;&c_u;&c_k;&c_a;&c_w;
      <klr>(&c_r;&c_a;&c_k;&c_ib;)</klr></trd>
    </trdgrp>
    <trd lng="de">Arm</trd>
    <trd lng="hu">kar</trd>
    <trd lng="nl">arm</trd>
    <trd lng="pl">rami&eogonek;</trd>
    <trdgrp lng="ru">
