<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>fonetik</rad>/o
</kap>
<drv mrk="foneti.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">LIN</uzo>
  <snc num="1">
    <dif>
      Bran&ccirc;o de <ref tip="malprt" cel="lingvi.0o">lingvistiko</ref>,
      kiu studas la lingvajn sonojn. 
    </dif>
  </snc>
  <snc num="2">
    <dif>
      <ref tip="dif" cel="foneti1.0o">Fonetismo</ref>:
      <ekz>
        E-o praktike kombinas en siaj vortoj la grafikon kaj la <tld/>on.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Phonetik</trd>,
     <trd>Lautlehre</trd></trdgrp>
  <trd lng="es">fon&eacute;tica</trd>
  <trd lng="fr">phon&eacute;tique<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="ru">&c_f;&c_o;&c_n;&c_je;&c_t;&c_i;&c_k;&c_a;</trd>
</drv>
<drv mrk="foneti.0a">
  <kap><tld/>a</kap>
  <snc num="1">
    <dif>
      Rilata al <tld/>o:
      <ekz><tld/>aj studoj.</ekz>
    </dif>
    <trd lng="de">phonetisch</trd>
  </snc>
  <snc num="2">
    <dif>
      Konforma al <tld/>o:
      <ekz><tld/>a ortografio.</ekz>
    </dif>
    <trdgrp lng="de">
     <trd>phonetisch</trd>,
     <trd>lautgerecht</trd></trdgrp>
  </snc>
  <trd lng="es">fon&eacute;tico</trd>
  <trd lng="fr">phon&eacute;tique<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="ru">&c_f;&c_o;&c_n;&c_je;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
