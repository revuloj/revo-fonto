<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>fonetik</rad>/o
</kap>
<drv mrk="foneti.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Parto de la lingvistiko, scienco pri la lingvaj sonoj. 
    </dif>
  </snc>
  <snc num="2">
    <dif>
      = <ref tip="sin" cel="foneti1.0o">fonetismo</ref>:
      <ekz>
        esp-o praktike kombinas en siaj vortoj la grafikon k la <tld/>on.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Phonetik</trd>,
     <trd>Lautlehre</trd></trdgrp>
  <trd lng="ru">&c_f;&c_o;&c_n;&c_je;&c_t;&c_i;&c_k;&c_a;</trd>
</drv>
<drv mrk="foneti.0a">
  <kap><tld/>a</kap>
  <snc num="1">
    <dif>
      Rilata al <tld/>o:
      <ekz><tld/>aj studoj.</ekz>
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Konforma al <tld/>o:
      <ekz><tld/>a ortografio.</ekz>
    </dif>
  </snc>
  <trd lng="de">phonetisch</trd>
  <trdgrp lng="de">
     <trd>phonetisch</trd>,
     <trd>lautgerecht</trd></trdgrp>
  <trd lng="ru">&c_f;&c_o;&c_n;&c_je;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
