<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mikrofilm.xml,v 1.18 2025/04/06 19:37:31 revo Exp $">
<kap>
  <rad>mikrofilm</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="mikrofilm.0o">
  <kap><tld/>o</kap>
  <snc mrk="mikrofilm.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref tip="super" cel="film.0o.FOT">Filmo</ref> portanta seriajn
      bildetojn de dokumento poste rigardeblajn per projekciilo:
      <ekz>
        la genealogia servo de mormonoj provizas siajn <tld/>ojn anka&ubreve;
        por nemembroj
        <fnt>
          <aut>J. Pietil&auml;inen</aut>,
          <vrk>Iom da aldono al la demandoj en Eventoj nr-o. 3</vrk>,
          <lok><url ref="http://www.eventoj.hu/arkivo/eve-008.htm">Eventoj,
          1992-06, numero 8a</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        ne eblas konsulti la originalojn, sed oni povas spekti <tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2006/008970.php">Genealogio</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_k;&c_r;&c_a;&c_f;&c_ib;&c_l;&c_mol;&c_m;</trd>
  <trd lng="ca">microfilm</trd>
  <trd lng="cs">mikrofilm</trd>
  <trd lng="de">Mikrofilm</trd>
  <trd lng="en">microfilm</trd>
  <trd lng="fr">microfilm</trd>
  <trd lng="hu">mikrofilm</trd>
  <trd lng="ja">&#12510;&#12452;&#12463;&#12525;&#12501;&#12451;&#12523;&#12512;</trd>
  <trd lng="nl">microfilm</trd>
  <trdgrp lng="pt">
    <trd>microfilme</trd>
  </trdgrp>
  <trd lng="sk">mikrofilm</trd>
  <trd lng="uk">&c_m;&c_ib;&c_k;&c_r;&c_o;&c_f;&c_ib;&c_l;&c_mol;&c_m;</trd>
  <trdgrp lng="zh">
    <trd>&#24494;&#32302;&#33184;&#25458; <pr>w&emacron;isu&omacron;ji&amacron;oju&#462;n</pr></trd>,
    <trd>&#24494;&#32553;&#33014;&#21367; <pr>w&emacron;isu&omacron;ji&amacron;oju&#462;n</pr></trd>,
    <trd>&#32302;&#24494;&#24179;&#29255; <pr>su&omacron;w&emacron;ip&iacute;ngpi&agrave;n</pr></trd>,
    <trd>&#32553;&#24494;&#24179;&#29255; <pr>su&omacron;w&emacron;ip&iacute;ngpi&agrave;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mikrofilm.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="mikrofilm.0i.TIP">
    <dif>
      <ref cel="regist.0i.TEK">Registri</ref> sur <tld/>on:
      <ekz>
        por ne difekti la originalojn, la plej valoraj kolektoj jam delonge estis
        <tld/>itaj kaj <klr>[&#8230;]</klr> la originaloj restas sekure stokitaj
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2009/009721.php">Europeana: kultura hereda&jcirc;o i&gcirc;os alirebla por &ccirc;iuj</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_k;&c_r;&c_a;&c_f;&c_ib;&c_l;&c_mol;&c_m;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">microfilmar</trd>
  <trd lng="de">verfilmen auf <ind>Mikrofilm</ind></trd>
  <trd lng="en">microfilm</trd>
  <trd lng="fr">microfilmer</trd>
  <trd lng="hu">mikrofilmez</trd>
  <trd lng="ja">&#12510;&#12452;&#12463;&#12525;&#12501;&#12451;&#12523;&#12512;&#12395;&#25774;&#24433;&#12377;&#12427;  <pr>&#12510;&#12452;&#12463;&#12525;&#12501;&#12451;&#12523;&#12512;&#12395;&#12373;&#12388;&#12360;&#12356;&#12377;&#12427;</pr></trd>
  <trd lng="nl">microfilmen</trd>
  <trdgrp lng="pt">
    <trd>microfilmar</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_m;&c_ib;&c_k;&c_r;&c_o;&c_f;&c_ib;&c_l;&c_mol;&c_m;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_n;&c_ib;&c_m;&c_a;&c_t;&c_i; &c_m;&c_ib;&c_k;&c_r;&c_o;&c_f;&c_ib;&c_l;&c_mol;&c_m;</trd>
  </trdgrp>
</drv>
</art>

