<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hiened.xml,v 1.13 2023/11/15 17:08:00 revo Exp $">
<kap>
  <rad>hiened</rad>/oj
</kap>
<drv mrk="hiened.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="best.rabo0oj">rabobestoj</ref>
      (<trd lng="la">Hyaenidae</trd>):
      <ekz>
        la <tld/>oj estas familio de mamuloj karnoman&gcirc;aj devenantaj el Afriko kaj Azio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=322197">Hienedoj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la Makulhieno estas la plej granda konata membro de <tld lit="H"/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=533446">Makulhieno</url></lok>
        </fnt>.
      </ekz>      
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_je;&c_n;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">hi&egrave;nids</trd>
  <trd lng="de">Hy&auml;nen<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="fr">hy&eacute;nid&eacute;s</trd>
  <trd lng="hu">hi&eacute;naf&eacute;l&eacute;k</trd>
  <trd lng="it">ienidi</trd>
  <trd lng="nl">hyena's<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pt">hien&iacute;deos</trd>
  <trd lng="ru">&c_g;&c_i;&c_je;&c_n;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="zh">&#x9B23;&#x72D7; <pr>li&#xE8;g&#x1D2;u</pr></trd>
</drv>
</art>
