<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>dezir</rad>/i
</kap>

<drv mrk="dezir.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="dezir.0i.porSiMem">
    <dif>
      Konscie emi al io, aspiri posedon a&ubreve; &gcirc;uon de io:
      <ekz>
        <tld/>i dormon;
      </ekz>
      <ekz>
        <tld/>i a&ubreve;di muzikon;
      </ekz>
      <ekz>
        kiom la koro <tld/>as
        <fnt><bib>PrV</bib></fnt>
        <klr>(abundege)</klr>
      </ekz>
      <ekz>
        havi <ind><tld/>o</ind>n por io.
        <trdgrp lng="fr">
          <trd>d&eacute;sir<klr tip="ind"> (de qc)</klr></trd>,
          <trd>souhait<klr tip="ind"> (de qc)</klr></trd>
        </trdgrp>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="avid.0i">avidi</ref>,
      <ref cel="bezon.0i">bezoni</ref>,
      <ref cel="sopir.0i">sopiri</ref>,
      <ref cel="envi.0i">envii</ref>,
      <ref cel="ambici">ambicii</ref>,
      <ref cel="brul.0i">bruli</ref>,
      <ref cel="soif.0i">soifi</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_zh;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_g;&c_n;&c_u;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;sirer<klr tip="ind"> (qc)</klr></trd>,
      <trd>souhaiter<klr tip="ind"> (qc)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>pragn&aogonek;&cacute;</trd>,
      <trd>&zdot;yczy&cacute;<klr> sobie</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="dezir.0i.alIu">
    <dif>
      Esprimi al iu bonvolan esperon, ke li &gcirc;uu ian
      feli&ccirc;a&jcirc;on:
      <ekz>
        <tld/>i al iu bonan farton, feli&ccirc;an
        voja&gcirc;on, feli&ccirc;on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind><tld/>esprimoj</ind> por novjaro.
        <trd lng="fr">v&oelig;ux<klr tip="ind"> (de nouvel an)</klr>
        </trd>
        <trd lng="hu">j&oacute;k&iacute;v&aacute;ns&aacute;gok</trd>
      </ekz>
    </dif>
    <rim> Apud izolita akuzativigita vorto, kiel <ctl>saluton!</ctl>,
    <ctl>gratulon</ctl>,
    ktp..., la frazparto <ctl>(mi deziras al vi)</ctl> estas
    subkomprenita kaj forlasita.
    </rim>
    <trdgrp lng="be">
      <trd>&c_zh;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_y;&c_ch;&c_y;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;sirer<klr tip="ind"> (qc pour qn)</klr></trd>,
      <trd>souhaiter<klr tip="ind"> (qc &agrave; qn)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>&zdot;yczy&cacute;</trd>
    </trdgrp>
  </snc>
  <trd lng="bg">&c_zh;&c_je;&c_l;&c_a;&c_ja;</trd>
  <trd lng="cs">p&rcaron;&aacute;t (si)</trd>
  <trd lng="de">w&uuml;nschen</trd>
  <trd lng="en">desire</trd>
  <trd lng="es">desear</trd>
  <trdgrp lng="hu">
    <trd>k&iacute;v&aacute;n</trd>,
    <trd>&oacute;hajt</trd>,
    <trd>szeretne</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>desejar</trd>,
    <trd>pretender</trd>
  </trdgrp>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_a;&c_t;&c_mol;</trd>
  <trd lng="tp">wile</trd>
</drv>

<drv mrk="dezir.0egi">
  <kap><tld/>egi</kap>
  <snc>
    <dif>
      Pasie, senbride <tld/>i:
      <ekz>
        &Scirc;e&hcirc;em, mia filo, <tld/>egas per sia animo vian 
        filinon
        <fnt><bib>MT</bib>, <lok>&Gen; 34:8</lok></fnt>;
      </ekz>
      <ekz>
        mi <tld/>egas Viajn ordonojn
        <fnt><bib>MT</bib>, <lok>&Psa; 119:40</lok></fnt>;
      </ekz>
      <ekz>
        tial, fratoj, <tld/>egu profeti, kaj ne malpermesu paroli per
        lingvoj
        <fnt><bib>NT</bib>, <lok>&Ko1; 14:39</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>egon de la karno vi ne plenumos
        <fnt><bib>NT</bib>, <lok>&Gal; 5:16</lok></fnt>.
      </ekz>
    </dif>
    <adm>
      PIV1 limigas la sencon al "pekaj" ~oj, sed la bibliaj ekz-oj
      estas pli vastasencaj. <aut>MB</aut>
    </adm>
  </snc>
  <trd lng="es">anhelar</trd>
  <trdgrp lng="fr">
    <trd>aspirer (&agrave;)</trd>,
    <trd>&ecirc;tre <ind>&eacute;pris</ind> (de)</trd>
  </trdgrp>
  <trd lng="hu">&aacute;h&iacute;t</trd>
  <trdgrp lng="pl">
    <trd>po&zdot;&aogonek;da&cacute;</trd>
  </trdgrp>
</drv>

<drv mrk="dezir.bon0i">
  <kap>bon<tld/>i</kap>
  <snc>
    <dif>
      <tld lit="D"/>i 2:
      <ekz>
        akceptu miajn sincerajn <ind>bon<tld/>o</ind>jn!
        <trd lng="bg">&c_p;&c_o;&c_zh;&c_je;&c_l;&c_a;&c_n;&c_i;&c_ja;</trd>
        <trd lng="de">Gl&uuml;ckwunsch</trd>
        <trd lng="fr">souhait<klr tip="ind"> (de bonheur)</klr></trd>
        <trd lng="hu">j&oacute;k&iacute;v&aacute;ns&aacute;gok</trd>
        <trd lng="ru">&c_p;&c_o;&c_zh;&c_je;&c_l;&c_a;&c_n;&c_i;&c_je;
&c_b;&c_l;&c_a;&c_g;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">gl&uuml;ckw&uuml;nschen</trd>
  <trd lng="be">&c_z;&c_y;&c_ch;&c_y;&c_c;&c_mol;
<klr>(&c_d;&c_a;&c_b;&c_r;&c_a;)</klr></trd>
  <trd lng="fr">souhaiter<klr tip="ind"> (qc &agrave; qn)</klr></trd>
  <trd lng="hu">j&oacute;t <ind>k&iacute;v&aacute;n</ind></trd>
  <trdgrp lng="pl">
    <trd><klr>dobrze </klr>&zdot;yczy&cacute;</trd>
  </trdgrp>
</drv>

<drv mrk="dezir.0o">
  <kap><tld/>o</kap>
  <snc mrk="dezir.0o.ago">
    <dif>
      Ago de <tld/>anto:
      <ekz>
        <tld/>o de iu pri io;
      </ekz>
      <ekz>
        volo kaj le&gcirc;o <tld/>ojn ne konas
        <fnt>Z</fnt>;
      </ekz>
      <ekz>
        plenumita <tld/>o
        <fnt>Z</fnt>;
      </ekz>
      <ekz>
        tio estas ne mia pia <tld/>o (iluzio), sed tion plene certigas 
        al ni simple logiko
        <fnt>Z</fnt>;
      </ekz>
      <ekz>
        bruta volupta <tld/>o
        <fnt>Z</fnt>;
      </ekz>
      <ekz>
        sub la &scirc;velantaj roboj rampas nia <tld/>o;
      </ekz>
      <ekz>
        havi <tld/>on veni
        <fnt>Z</fnt>;
      </ekz>
    </dif>
    <trd lng="de">W&uuml;nschen</trd>
    <trdgrp lng="pl">
      <trd>&zdot;yczenie</trd>,
      <trd>ch&eogonek;&cacute;</trd>,
      <trd>oskoma<klr> (przest.)</klr></trd>,
      <trd>apetyt</trd>,
      <trd>ch&eogonek;tka<klr> (rzadkie)</klr></trd>,
      <trd>chrapka<klr> (rzadkie)</klr></trd>,
      <trd>pragnienie</trd>,
      <trd>&zdot;&aogonek;dza</trd>,
      <trd>chu&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="dezir.0o.io">
    <dif>
      Io <tld/>ata:
      <ekz>
        la <tld/>o de virtuloj estas nur bono
        <fnt><bib>MT</bib></fnt>;
      </ekz>
      <ekz>
        &ccirc;ion kion mi skribas al vi, volu rigardi nur kiel mian
        opinion, sed ne kiel mian <tld/>on
        <fnt>Z</fnt>;
      </ekz>
    </dif>
    <trd lng="bg">&c_zh;&c_je;&c_l;&c_a;&c_n;&c_i;&c_je;</trd>
    <trd lng="de">Wunsch</trd>
    <trd lng="en">wish</trd>
  </snc>
</drv>
</art>
