<drv mrk="dezir.bon0i">
  <kap>bon<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="dezir.bon0i.KOMUNE">
    <dif>
      <ref tip="vid" cel="dezir.0i.alIu"><tld lit="D"/>i<sncref/></ref>
      bonon, feli&ccirc;on, prosperon:
      <ekz>
        <klr>[ili]</klr> sendis al mi amikajn <ind>bon<tld/>o</ind>jn
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Tria Kongreso Esperantista en Cambridge en la 12a de a&ubreve;gusto
          1907</vrk>
        </fnt>;
        <trd lng="bg">&c_p;&c_o;&c_zh;&c_je;&c_l;&c_a;&c_n;&c_i;&c_ja;</trd>
        <trd lng="de">Gl&uuml;ckwunsch</trd>
        <trd lng="fr">souhait<klr tip="ind"> (de bonheur)</klr></trd>
        <trd lng="hu">j&oacute;k&iacute;v&aacute;ns&aacute;gok</trd>
        <trd lng="ru">&c_p;&c_o;&c_zh;&c_je;&c_l;&c_a;&c_n;&c_i;&c_je;
        &c_b;&c_l;&c_a;&c_g;</trd>
      </ekz>
      <ekz>
        plenumi <klr>[&#x2026;]</klr> konsilon bon<tld/>an
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj okuloj malgajaj, enpensaj, <klr>[&#x2026;]</klr> ser&ccirc;is sur la viza&gcirc;o de la nejuna
        virino <klr>[&#x2026;]</klr> brileton de kortu&scirc;iteco kaj bon<tld/>eco
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        iu mia bon<tld/>anto sciigis min per anonima letero, ke tiu &ccirc;i afero &gcirc;is nun
        ankora&ubreve; trovi&gcirc;as en la Moskva Ju&gcirc;ejo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        miaj malbon<tld/>antoj parolas pri mia pereo
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 38:12</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="gratul.0i">gratuli</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_y;&c_ch;&c_y;&c_c;&c_mol;
  <klr>(&c_d;&c_a;&c_b;&c_r;&c_a;)</klr></trd>
  <trd lng="cs">blahop&rcaron;&aacute;t</trd>
  <trdgrp lng="de">
    <trd>gl&uuml;ckw&uuml;nschen</trd>,
    <trd>wohl <ind>meinen</ind></trd>
  </trdgrp>
  <trd lng="en">wish well</trd>
  <trd lng="fr">pr&eacute;senter ses v&oelig;ux</trd>
  <trdgrp lng="he">
    <trd>&lamed;&alef;&het;&lamed;</trd>,
    <trd>&lamed;&shin;&lamed;&vav;&het; &dalet;&resh;&yod;&shin;&tav; &shin;&lamed;&vav;&fmem;</trd>
  </trdgrp>
  <trd lng="hu">j&oacute;t <ind>k&iacute;v&aacute;n</ind></trd>
  <trdgrp lng="pl">
    <trd><klr>dobrze </klr>&zdot;yczy&cacute;</trd>
  </trdgrp>
  <trd lng="sk">blahopria&tcaron;</trd>
</drv>

<drv mrk="dezir.viv0o">
  <kap>viv<tld/>o</kap>
  <snc mrk="dezir.viv0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Impulso por vivado, por agado:
      <ekz>
        kiam vere ne plu eblas foriri por dormi, tiam viv<tld/>o mem
        malaperas
        <fnt>
          <aut>L.-F. Celine, trad. A. LeQuint kaj &Jcirc;. Le Puil</aut>,
          <vrk>Voja&gcirc;o &gcirc;is noktofino</vrk>,
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        se &ccirc;iu el ni &gcirc;isvivus tian a&gcirc;on kaj konservus tian viv<tld/>on!
        <fnt>
          <bib>LOdE</bib>
          <vrk><url ref="https://tekstaro.com/t?nomo=ondo-de-esperanto">La tempo ankora&ubreve; ne pasis...</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en la punlaborejo <klr>[&#x2026;]</klr> li vere spertis tiun viv<tld/>on, &ccirc;e la limo de
        trudita laboro, morto kaj ekzilo
        <fnt>
          <bib>MD11</bib>
          <vrk> Nelson Mandela, la neatenditaj vojoj</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="viv.0ema">vivema</ref>
    </refgrp>
  </snc>
  <trd lng="de">Lebenswille</trd>
  <trd lng="en">desire to live</trd>
  <trdgrp lng="fr">
    <trd>&eacute;lan vital</trd>,
    <trd>soif de vivre</trd>
  </trdgrp>
</drv>
