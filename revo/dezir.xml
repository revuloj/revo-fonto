<drv mrk="dezir.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="dezir.0i.porSiMem">
    <dif>
      Konscie <ref tip="super" cel="em.0i">emi</ref> al io,
      <ref tip="super" cel="vol.0i.deziri">voli</ref> posedon a&ubreve; &gcirc;uon de io:
      <ekz>
        vi <tld/>is glacia&jcirc;on, diris la kelnero
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &jcirc;urnalisto <klr>[&#8230;]</klr> <tld/>is interparoli kun ili
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i lin kisis kaj rekisis kaj forte <tld/>is, ke li restu viva
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi tiel forte <tld/>is ekscii mian historion
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> tuj sendis al li la <tld/>itan sumon da mono
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        ni bezonas nur ek<tld/>i kaj elekti
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iam pli &scirc;i <tld/>is <klr>[&#8230;]</klr> vivi inter ili
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la opero iris tre bone, nur la
        &hcirc;oroj lasis ion por <tld/>i
        <klr>(ne kantis perfekte)</klr>
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>as a&ubreve;di belan muzikon
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiom la koro <tld/>as
        <fnt><bib>PrV</bib></fnt>
        <klr>(abundege)</klr>
      </ekz>
      <ekz>
        vi forte <tld/>is esti en la domo de via patro
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 31:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        Faraono a&ubreve;dis pri tiu afero kaj <tld/>is mortigi Moseon
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 2:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        tapi&scirc;on ne <tld/>is, maton akiris
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aspir.0i">aspiri</ref>,
      <ref cel="avid.0i">avidi</ref>,
      <ref cel="envi.0i">envii</ref>,
      <ref cel="sopir.0i">sopiri</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_zh;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_g;&c_n;&c_u;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;sirer<klr tip="ind"> (qc)</klr></trd>,
      <trd>souhaiter<klr tip="ind"> (qc)</klr></trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>(meng)<ind>ingin</ind>(kan)</trd>,
      <trd>meng<ind>idam</ind>kan</trd>,
      <trd>men<ind>damba</ind>kan</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>pragn&aogonek;&cacute;</trd>,
      <trd>&zdot;yczy&cacute;<klr> sobie</klr></trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x4F01;&#x6C42; <pr>q&#x1D0;qi&#xFA;</pr></trd>,
    <trd>&#24680;&#x4E0D;&#x5F97; <pr>h&#xE8;nb&#xF9;d&#xE9;</pr></trd>,
    <trd>&#24850; <pr>k&#xE0;i</pr></trd>,
    <trd>&#x795D;&#x613F; <pr>zh&#xF9;yu&#xE0;n</pr></trd>,
    <trd>&#x60F3;&#35201; <pr>xi&#x1CE;ngy&#xE0;o</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="dezir.0i.alIu">
    <dif>
      <ref cel="esprim.0i">Esprimi</ref> al iu bonvolan esperon, ke li &gcirc;uu ian
      feli&ccirc;a&jcirc;on:
      <ekz>
        <tld/>i al iu &ccirc;ion bonan
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>as al vi bonan tagon, sinjoro! &#8208; bonan matenon! &#8208; &gcirc;ojan feston! (mi
        <tld/>as al vi)
        <fnt>
          <bib>F</bib>
          <lok>&FE; 10</lok>
        </fnt>;
      </ekz>
      <ekz>
        nun, samideanoj, mi <tld/>as por vi &ccirc;iuj feli&ccirc;an voja&gcirc;on al viaj hejmoj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la ferma ceremonio de la Sesa Kongreso Esperantista en Washington en
          la 20a de a&ubreve;gusto 1910 (b)</vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi havas la honoron gratuli vin, via mo&scirc;to, kaj <tld/>i al vi feli&ccirc;on por multaj
        jaroj
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> kiel ordinare <tld/>is al li bonan nokton kaj &scirc;ajnis sin prepari
        por iri dormi
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la junkoj klini&gcirc;adis tien kaj reen, <tld/>ante al li bonan vesperon kaj bonan
        matenon
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>as al li multe da bono kaj anta&ubreve; &ccirc;io tre feli&ccirc;an voja&gcirc;on
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="adiaux.0i.KOMUNE">adia&ubreve;i</ref>,
      <ref cel="gratul.0i">gratuli</ref>,
      <ref cel="salut.0i.MOR">saluti</ref>
    </refgrp>
    <rim>
      La frazparto <ctl>(mi <tld/>as al vi)</ctl> estas
      subkomprenita kaj forlasita, apud izolita akuzativigita vorto, kiel
      <ctl>saluton!</ctl>, <ctl>gratulon!</ctl>, ktp.
    </rim>
    <trdgrp lng="be">
      <trd>&c_zh;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_y;&c_ch;&c_y;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="en">wish</trd>
    <trdgrp lng="fr">
      <trd>d&eacute;sirer<klr tip="ind"> (qc pour qn)</klr></trd>,
      <trd>souhaiter<klr tip="ind"> (qc &agrave; qn)</klr></trd>
    </trdgrp>
    <trd lng="id">meng<ind>harap</ind>kan</trd>
    <trdgrp lng="pl">
      <trd>&zdot;yczy&cacute;</trd>
    </trdgrp>
    <trd lng="zh">&#x795D; <pr>zh&#xF9;</pr></trd>
</snc>
  <trd lng="bg">&c_zh;&c_je;&c_l;&c_a;&c_ja;</trd>
  <trd lng="bo">&t_sa;&t_nyata;&t_gigu;&t_nga;&t_tsheg;&amp;t_-a;&t_da;&t_naro;&t_da;&t_tsheg;</trd>
