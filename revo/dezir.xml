  <trd lng="bn">&#x987;&#x99A;&#x9CD;&#x99B;&#x9C1;&#x995;</trd>
  <trd lng="bo">&#xF51;&#xF7C;&#xF51;&#xF0B;&#xF54;&#xF0B;</trd>
  <trd lng="bs">&#x17E;ele</trd>
  <trd lng="co">vulete</trd>
  <trdgrp lng="cs">
    <trd>desiderium</trd>,
    <trd>p&rcaron;&aacute;n&iacute;</trd>,
    <trd>tu&zcaron;ba</trd>,
    <trd>&zcaron;&aacute;dost</trd>
  </trdgrp>
  <trd lng="da">&#xF8;nske</trd>
  <trd lng="es">deseo</trd>
  <trd lng="et">soovida</trd>
