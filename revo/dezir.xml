<drv mrk="dezir.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="dezir.0i.porSiMem">
    <dif>
      Konscie <ref tip="super" cel="em.0i">emi</ref> al io, 
      <ref tip="super" cel="vol.0i.deziri">voli</ref> posedon a&ubreve; &gcirc;uon de io:
      <ekz>
        vi <tld/>is glacia&jcirc;on, diris la kelnero
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &jcirc;urnalisto <klr>[&#x2026;]</klr> <tld/>is interparoli kun ili
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i lin kisis kaj rekisis kaj forte <tld/>is, ke li restu viva
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi tiel forte <tld/>is ekscii mian historion
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> tuj sendis al li la <tld/>itan sumon da mono
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        ni bezonas nur ek<tld/>i kaj elekti
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iam pli &scirc;i <tld/>is <klr>[&#x2026;]</klr> vivi inter ili
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la opero iris tre bone, nur la 
        &hcirc;oroj lasis ion por <tld/>i
        <klr>(ne kantis perfekte)</klr>
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>as a&ubreve;di belan muzikon
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiom la koro <tld/>as
        <fnt><bib>PrV</bib></fnt>
        <klr>(abundege)</klr>
      </ekz>
      <ekz>
        vi forte <tld/>is esti en la domo de via patro
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 31:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        Faraono a&ubreve;dis pri tiu afero kaj <tld/>is mortigi Moseon
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 2:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        tapi&scirc;on ne <tld/>is, maton akiris
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aspir.0i">aspiri</ref>,
      <ref cel="avid.0i">avidi</ref>,
      <ref cel="envi.0i">envii</ref>,
      <ref cel="sopir.0i">sopiri</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_zh;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_g;&c_n;&c_u;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;sirer<klr tip="ind"> (qc)</klr></trd>,
      <trd>souhaiter<klr tip="ind"> (qc)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>pragn&aogonek;&cacute;</trd>,
      <trd>&zdot;yczy&cacute;<klr> sobie</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="dezir.0i.alIu">
    <dif>
      <ref cel="esprim.0i">Esprimi</ref> al iu bonvolan esperon, ke li &gcirc;uu ian
      feli&ccirc;a&jcirc;on:
      <ekz>
        <tld/>i al iu &ccirc;ion bonan
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>as al vi bonan tagon, sinjoro! &#x2010; bonan matenon! &#x2010; &gcirc;ojan feston! (mi
        <tld/>as al vi)
        <fnt>
          <bib>F</bib>
          <lok>&FE; 10</lok>
        </fnt>;
      </ekz>
      <ekz>
        nun, samideanoj, mi <tld/>as por vi &ccirc;iuj feli&ccirc;an voja&gcirc;on al viaj hejmoj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la ferma ceremonio de la Sesa Kongreso Esperantista en Washington en
          la 20a de a&ubreve;gusto 1910 (b)</vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi havas la honoron gratuli vin, via mo&scirc;to, kaj <tld/>i al vi feli&ccirc;on por multaj
        jaroj
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> kiel ordinare <tld/>is al li bonan nokton kaj &scirc;ajnis sin prepari
        por iri dormi
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la junkoj klini&gcirc;adis tien kaj reen, <tld/>ante al li bonan vesperon kaj bonan
        matenon
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>as al li multe da bono kaj anta&ubreve; &ccirc;io tre feli&ccirc;an voja&gcirc;on
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="adiaux.0i.KOMUNE">adia&ubreve;i</ref>,
      <ref cel="gratul.0i">gratuli</ref>,
      <ref cel="salut.0i.MOR">saluti</ref>
    </refgrp>
    <rim>
      La frazparto <ctl>(mi <tld/>as al vi)</ctl> estas
      subkomprenita kaj forlasita, apud izolita akuzativigita vorto, kiel
      <ctl>saluton!</ctl>, <ctl>gratulon!</ctl>, ktp.
    </rim>
    <trdgrp lng="be">
      <trd>&c_zh;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_y;&c_ch;&c_y;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="en">wish</trd>
    <trdgrp lng="fr">
      <trd>d&eacute;sirer<klr tip="ind"> (qc pour qn)</klr></trd>,
      <trd>souhaiter<klr tip="ind"> (qc &agrave; qn)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>&zdot;yczy&cacute;</trd>
    </trdgrp>
  </snc>
  <trd lng="bg">&c_zh;&c_je;&c_l;&c_a;&c_ja;</trd>
  <trd lng="bo">&t_sa;&t_nyata;&t_gigu;&t_nga;&t_tsheg;&amp;t_-a;&t_da;&t_naro;&t_da;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;&aacute;t (si)</trd>,
    <trd>p&rcaron;&aacute;t <klr>(si)</klr></trd>
  </trdgrp>
  <trd lng="de">w&uuml;nschen</trd>
  <trd lng="en">desire</trd>
  <trd lng="es">desear</trd>
  <trd lng="he">&resh;&tsadi;&vav;&fnun; &ayin;&zayin;</trd>
  <trdgrp lng="hu">
    <trd>k&iacute;v&aacute;n</trd>,
    <trd>&oacute;hajt</trd>,
    <trd>szeretne</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>desejar</trd>,
    <trd>pretender</trd>
  </trdgrp>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>pria&tcaron;</trd>,
    <trd>&zcaron;ela&tcaron;</trd>
  </trdgrp>
  <trd lng="tok">wile</trd>
  <trdgrp lng="uk">
    <trd>&#x431;&#x430;&#x436;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x437;&#x430;&#x431;&#x430;&#x433;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x440;&#x430;&#x433;&#x43D;&#x443;&#x442;&#x438;</trd>,
    <trd>&#x437;&#x438;&#x447;&#x438;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="dezir.0o">
  <kap><tld/>o</kap>
  <snc mrk="dezir.0o.ago">
    <dif>
      Ago, sento de <tld/>anto:
      <ekz>
        naski&gcirc;is en &scirc;i fortega <tld/>o foriri, foriri tuj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de mia koro
        <fnt>
          <bib>NT</bib>
          <lok>Romanoj 10:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        man&gcirc;u <klr>[&#x2026;]</klr> la&ubreve; la tuta <tld/>o de via animo
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 12:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        bruta volupta <tld/>o
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiuj &ccirc;i rondetoj <klr>[&#x2026;]</klr> servas por liberaj amikaj interparoladoj pri
        &ccirc;iuj eblaj temoj, la&ubreve; la <tld/>o de la partoprenantoj
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Aldono al al "Dogmoj de Hilelismo"</vrk>
        </fnt>;
      </ekz>
      <ekz>
        volo kaj le&gcirc;o <tld/>ojn ne konas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne valoras la akiro e&ccirc; la penon de l' <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>W&uuml;nschen</trd>,
      <trd>Begehren</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>&zdot;yczenie</trd>,
      <trd>ch&eogonek;&cacute;</trd>,
      <trd>oskoma<klr> (przest.)</klr></trd>,
      <trd>apetyt</trd>,
      <trd>ch&eogonek;tka<klr> (rzadkie)</klr></trd>,
      <trd>chrapka<klr> (rzadkie)</klr></trd>,
      <trd>pragnienie</trd>,
      <trd>&zdot;&aogonek;dza</trd>,
      <trd>chu&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="dezir.0o.io">
    <dif>
      Io <tld/>ata, <ref cel="dezir.0atajxo.PSI"><tld/>ata&jcirc;o</ref>:
      <ekz>
        la <tld/>o de virtuloj estas nur bono
        <fnt><bib>MT</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>o plenumita estas agrabla por la animo
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 13:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        plenumita <tld/>o estas arbo de vivo
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 13:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio estas ne nia pia <klr>(nerealisma)</klr> <tld/>o, 
        sed tion plene certigas al ni simpla logiko kaj prudento
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista en Washington en la 15a de a&ubreve;gusto
          1910</vrk>
        </fnt>;
      </ekz>
      <ekz>
        li esprimis la <tld/>on, ke lia verko <klr>[&#x2026;]</klr> enhavu anka&ubreve; parton
        Esperantan
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_zh;&c_je;&c_l;&c_a;&c_n;&c_i;&c_je;</trd>
    <trd lng="de">Wunsch</trd>
    <trd lng="en">wish</trd>
  </snc>
  <trd lng="af">wens</trd>
