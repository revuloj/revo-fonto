<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: agnosk.xml,v 1.33 2023/08/30 18:23:31 revo Exp $">
<kap>
  <rad>agnosk</rad>/i <fnt><bib>PV</bib></fnt> 
</kap>
<drv mrk="agnosk.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="agnosk.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="akcept.0i.ion">Akcepti</ref> 
      ion kiel validan, ekzistantan, la&ubreve;le&gcirc;an, valoran:
      <ekz>
        li sportiste, ridete <tld/>os sian fiaskon
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> rifuzas humile <tld/>i la limojn de la homa scio
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        internacia teritorio, jure <tld/>ita kiel tia de la lokaj 
        kaj landaj a&ubreve;toritatoj
        <fnt>
          <bib>CKv</bib>
        </fnt>;
      </ekz>
      <ekz>
        la edukaj instancoj oficiale <tld/>u diplomojn, por tiuj, kiuj deziras instrui
        Esperanton
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        la subskribintoj <tld/>as, ke la enkonduko de <klr>[&#x2026;]</klr> komuna lingvo por
        internacia publikigo de sciencaj laboroj estas tre efika rimedo
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        la itala &Scirc;tatprezidento <tld/>is <nom>IEF</nom> kiel &leftquot;juran personon&rightquot;
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        Esperanto post la Dua Vatikana Koncilio estis oficiale <tld/>ita kiel liturgia
        lingvo de la Vatikano
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i al la nele&gcirc;aj filoj la samajn rajtojn kiel al la le&gcirc;aj
        <fnt>
          <aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>
          <vrk>Cent Jaroj da Soleco</vrk>
          <lok>1992</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kon.re0i.agnoski">rekoni<sncref/></ref>,
      <ref cel="konfes.0i.deklari_sia">konfesi<sncref/></ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="deklar.0i">deklari</ref>,
      <ref cel="oficia.0igi">oficialigi</ref>,
      <ref cel="sankci.0i.JUR">sankcii</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_z;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_z;&c_a;&c_k;&c_o;&c_n;&c_mol;&c_v;&c_a;&c_c;&c_mol;
    <klr>(&c_d;&c_z;&c_ib;&c_c;&c_ja;)</klr>
    </trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>agnoskovat</trd>,
    <trd>identifikovat</trd>,
    <trd>ofici&aacute;ln&ecaron; uznat</trd>
  </trdgrp>
  <trd lng="de">anerkennen</trd>
  <trdgrp lng="en">
    <trd>acknowledge<klr> (tr, that something exists, is true, valid, legal, valuable)</klr></trd>,
    <trd>recognize<klr> (tr, that something exists, is true, valid, legal, valuable)</klr></trd>,
    <trd>grant<klr> (tr, that something exists, is true, valid, legal, valuable)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>reconocer</trd>,
    <trd>admitir</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>admettre</trd>,
    <trd>reconnaitre</trd>
  </trdgrp>
  <trd lng="he">&lamed;&he;&kaf;&yod;&resh; &resh;&shin;&mem;&yod;&tav;</trd>
  <trdgrp lng="hu">
    <trd>elismer</trd>,
    <trd>felismer</trd>
  </trdgrp>
  <trd lng="it">riconoscere</trd>
  <trdgrp lng="ja">
    <trd>&#x8A8D;&#x3081;&#x308B;  [<ind>&#x307F;&#x3068;&#x3081;&#x308B;</ind>]</trd>,
    <trd>&#x8A8D;&#x53EF;&#x3059;&#x308B;  [<ind>&#x306B;&#x3093;&#x304B;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x8A8D;&#x77E5;&#x3059;&#x308B;  [<ind>&#x306B;&#x3093;&#x3061;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">erkennen</trd>
  <trd lng="pl">uznawa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>reconhecer</trd>,
    <trd>admitir</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_z;&c_n;&c_a;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>ofici&aacute;lne uzna&tcaron;</trd>,
    <trd>uzna&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">erk&auml;nna</trd>
  <trd lng="tr">kabullenmek</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_z;&c_n;&c_a;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="agnosk.0o">
  <kap><tld/>o</kap>
  <snc mrk="agnosk.0o.KOMUNE">
    <dif>
      Esprimo, ke oni akceptas ion
      kiel realan, validan, gravan:
      <ekz>
        peti de la ministro la <tld/>on de <klr>[&#x2026;]</klr> la diplomo disdonota post
        celkonforma ekzamenado
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiu konsentita <tld/>o povos esti konsiderata kiel &gcirc;enerala oficialigo
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiu <tld/>o ne estis bonvenigita de &ccirc;iuj romiaj katolikoj
        <fnt>
          <bib>Kon11</bib>
        </fnt>;
      </ekz>
      <ekz>
        lia premio konsistigas <tld/>on de lia batalo favore al la 
        infanaj rajtoj
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://neniammilitointerni.over-blog.com/2014/10/barato-kiu-estas-kailash-satyarthi-barata-aktivisto-por-edukado-kaj-kun-nobelpremiulo-por-paco-2014.html">
          Kiu estas Kailash Satyarthi, barata aktivisto por 
          edukado...?</url></vrk>,
          <lok>Neniam milito inter ni, 2014-10-15</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kon.re0o.agnosko">rekono</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_z;&c_n;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>agnoskace</trd>,
    <trd>identifikace</trd>,
    <trd>uzn&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Anerkennung</trd>
  <trdgrp lng="en">
    <trd>acknowledgement<klr> (that something exists, is true, valid, legal, valuable)</klr></trd>,
    <trd>recognition<klr> (that something exists, is true, valid, legal, valuable)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>reconocimiento</trd>,
    <trd>consagraci&oacute;n</trd>,
    <trd>instituci&oacute;n</trd>
  </trdgrp>
  <trd lng="fr">reconnaissance</trd>
  <trd lng="he">&he;&kaf;&resh;&he;</trd>
  <trdgrp lng="ja">
    <trd>&#x8A8D;&#x53EF;  [<ind>&#x306B;&#x3093;&#x304B;</ind>]</trd>,
    <trd>&#x516C;&#x8A8D;  [<ind>&#x3053;&#x3046;&#x306B;&#x3093;</ind>]</trd>
  </trdgrp>
  <trd lng="pl">uznanie</trd>
  <trdgrp lng="sk">
    <trd>potvrdenie</trd>,
    <trd>uznanie</trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_i;&c_z;&c_n;&c_a;&c_n;&c_n;&c_ja; <klr>(&c_f;&c_a;&c_k;&c_t;&c_u; &c_ib; &c_t;.&c_p;.)</klr></trd>
</drv>

<drv mrk="agnosk.0inda">
  <kap><tld/>inda</kap>
  <snc mrk="agnosk.0inda.KOMUNE">
    <dif>
      Kiu meritas <tld/>on:
      <ekz>
        ili ne plu kontrolis la ekonomiajn politikojn, kaj tio ne estis <tld/>inda
        <fnt>
          <bib>MD14</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u tio estas <tld/>inda kiel sukceso?
        <fnt>
          <aut>A. S. Markon</aut>,
          <vrk><url ref="https://groups.yahoo.com/neo/groups/esper-rus/conversations/topics/467">
          Ni kresku kvante (pri REU) </url></vrk>,
          <lok>Esperanto en Rusio, 1999-12-22</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_r;&c_t;&c_y; <ind>&c_p;&c_r;&c_y;&c_z;&c_n;&c_a;&c_n;&c_mol;&c_n;&c_ja;</ind></trd>
  <trd lng="de">anerkennenswert</trd>
  <trdgrp lng="en">
    <trd>worthy of acknowledgement</trd>,
    <trd>worthy of recognition</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>estimable</trd>,
    <trd>que merece <ind>reconocimiento</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>estimable</trd>,
    <trd>qui m&eacute;rite <ind>reconnaissance</ind></trd>
  </trdgrp>
</drv>

<drv mrk="agnosk.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="agnosk.mal0i.KOMUNE">
    <dif>
      Ne a&ubreve; ne plu <tld/>i, publike konsideri kiel nevalidan, fremdan,
      <ref tip="vid" cel="konfes.mal0i.agnoski">malkonfesi<sncref/></ref>:
      <ekz>
        li neniam revidu la landon, kiun li mal<tld/>is
        <fnt>
        <aut>E. Everett Hale, trad. E. L. Robinson, trad. Don
        Harlow</aut>,
        <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Noveloj/La_Viro_Senpatria.html">La
        viro senpatria</url></vrk>,
        <lok>1907 kaj 1994</lok>
        </fnt>;
      </ekz>
      <ekz>
        la reganta politika klaso retrovi&gcirc;is <klr>[&#x2026;]</klr> mal<tld/>ita de la publika
        opinio
        <fnt>
          <bib>MD02</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je; <ind>&c_p;&c_r;&c_y;&c_z;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</ind></trd>,
    <trd>&c_a;&c_d;&c_r;&c_y;&c_n;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ablehnen</trd>,
    <trd>delegitimieren</trd>
  </trdgrp><trdgrp lng="es">
    <trd>recusar</trd>,
    <trd>renegar</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>reject<klr> (tr)</klr></trd>,
    <trd>not acknowledge<klr> (tr, that something exists, is true, valid, legal, valuable)</klr></trd>,
    <trd>not recognize<klr> (tr, that something exists, is true, valid, legal, valuable)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>rejeter</trd>,
    <trd>r&eacute;pudier</trd>
  </trdgrp>
  <trd lng="pl">odwo&lstroke;a&cacute;</trd>
  <trdgrp lng="sk">
    <trd>neuzna&tcaron;</trd>,
    <trd>upiera&tcaron;</trd>
  </trdgrp>
  <trd lng="tr">kabullenmemek</trd>
  <trd lng="ms">mengakui</trd>
</drv>
</art>
