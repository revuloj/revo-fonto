<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap><rad>agnosk</rad>/i</kap>
<drv mrk="agnosk.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1">
    <dif>
      Rekoni 2:
      <ekz>
        ne &ccirc;iuj filozofoj <tld/>as la  senlimecon de la universo.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="konfes.0i">konfesi</ref>.
    </refgrp>
  </snc>
  <snc num="2">
    <dif>
      Rekoni 3:
      <ekz>
        nur kiam esp-o estos oficiale <tld/>ita de plej multaj 
        registaroj, ni povos pensi pri &scirc;an&gcirc;oj en la lingvo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_z;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>, 
    <trd>&c_u;&c_z;&c_a;&c_k;&c_o;&c_n;&c_mol;&c_v;&c_a;&c_c;&c_mol; 
      <klr>(&c_d;&c_z;&c_ib;&c_c;&c_ja;)</klr>
    </trd>
  </trdgrp>
  <trd lng="de">anerkennen</trd>
  <trdgrp lng="en">
     <trd>acknowledge</trd>,
     <trd>recognize</trd>
  </trdgrp>
  <trdgrp lng="es">
     <trd>reconocer</trd>,
     <trd>admitir</trd>
  </trdgrp>
  <trdgrp lng="fr">
     <trd>admettre</trd>,
     <trd>reconna&icirc;tre</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>elismer</trd>,
     <trd>felismer</trd>
  </trdgrp>
  <trd lng="nl">erkennen</trd>
  <trd lng="it">riconoscere</trd>
  <trdgrp lng="pt">
     <trd>reconhecer</trd>,
     <trd>admitir</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_z;&c_n;&c_a;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">erk&auml;nna</trd>
</drv>
</art>
