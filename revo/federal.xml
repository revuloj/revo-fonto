<vortaro>
<art mrk="$Id: federal.xml,v 1.12 2019/11/17 13:10:13 revo Exp $">
<kap>
  <rad>federal</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="federal.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="federal.0a.POL">
  <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="federa.0a">Federacia</ref>:
      <ekz>
        mi publikigis <klr>[&#8230;]</klr> verkon pri la unui&gcirc;oj <tld/>aj
        inter &scirc;tatoj Usono, Svislando <klr>[&#8230;]</klr>
        <fnt>
          <bib>Aventuroj</bib>, <lok>&ccirc;apitro 54a, p. 121a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a parlamento de A&ubreve;stralio
        <fnt>
          <bib>Monato</bib>
          <aut>Kep Enderby</aut>
          <vrk><url ref="https://www.monato.be/2005/008600.php"
              >Morto, morfino kaj moralo</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        la usona <tld/>a &scirc;uldo jam atingas 100&#8239;%, sed &gcirc;i estas 120&#8239;%, se oni aldonas la
        &scirc;uldojn de la &scirc;tatoj
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="https://www.monato.be/2011/010473.php"
              >Magraj jaroj</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igo la&ubreve; etna principo
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006818.html"
              >Balkanio: Nova &scirc;anco por &ccirc;iuj etnoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
<!-- ne traduku evitindajn 
  <trdgrp lng="be">
    <trd>&c_f;&c_e;&c_d;&c_e;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_f;&c_e;&c_d;&c_e;&c_r;&c_a;&c_c;&c_y;&c_j;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">Bundes-</trd>
  <trd lng="fr">f&eacute;d&eacute;ral</trd>
  <trdgrp lng="hu">
    <trd>sz&ouml;vets&eacute;gi</trd>,
    <trd>f&ouml;der&aacute;lis</trd>
  </trdgrp>
  <trd lng="it">federale</trd>
  <trd lng="nl">federaal<klr tip="ind"> (ontr.)</klr></trd>
  <trd lng="pl">federalny</trd>
  <trd lng="pt">federal</trd>
-->
</drv>

<drv mrk="federal.0ismo">
  <kap><tld/>ismo</kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="federal.0ismo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="federa.0ismo">Federaciismo</ref>:
      <ekz>
        <nom>Proudhon</nom> <klr>[&#8230;]</klr> defendis la ideon de <tld/>ismo
        <fnt>
          <aut>J. &Scirc;ram</aut>,
          <vrk><url ref="http://www.satesperanto.org/Pri-klasbatalo.html">
          Pri klasbatalo</url></vrk>,
          <lok>SAT Kulturo, [vidita en 2008-09]</lok>
        </fnt>;
      </ekz>
      <ekz>
        mond<tld/>ismo
        <fnt>
          <bib>Monato</bib>
          <aut>Lawrence Mee</aut>
          <vrk><url ref="https://www.monato.be/2005/008283.php"
              >La stelo</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
<!-- ne traduku evitindajn 
  <trd lng="fr">f&eacute;d&eacute;ralisme</trd>
  <trd lng="it">federalismo</trd>
  <trd lng="nl">federalisme<klr tip="ind"> (ontr.)</klr></trd>
-->
</drv>
