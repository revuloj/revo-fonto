<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kazin.xml,v 1.24 2025/10/27 16:00:21 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>kazin</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="kazin.0o">
  <kap><tld/>o</kap>
  <snc mrk="kazin.0o.TUR">
    <uzo tip="fak">SPO</uzo>
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="konstr.0ajxo">Konstrua&jcirc;o</ref> por publikaj kunvenoj
      kaj amuza&jcirc;oj (dancoj, monludoj kaj ceteraj) precipe en kurac- a&ubreve; 
      <ref tip="malprt" cel="ban.0urbo0loko">banurbo</ref>j:
      <ekz>
        urba banejo kaj <tld/>o de <nom>Boulogne-sur-mer</nom>
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="">
          [reklamo]</url></vrk>,
          <lok>Kongresa Libro, 1905</lok>
        </fnt>.
      </ekz> 
    </dif>  
    <trdgrp lng="de">
      <trd>Kurhaus</trd>,
      <trd>Kursaal</trd>
    </trdgrp>
  </snc>
  <snc mrk="kazin.0o.vetludejo">
    <dif>
      <ref cel="lud.vet0o">Vetlud</ref>ejo:
      <ekz>
        Monako <klr>[&#8230;]</klr> tradicie vivas el bankoj, <tld/>o kaj turismo
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="&Monato;/2006/008835.php">500 &ccirc;efverkoj en Monako</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        la usona <tld/>o-urbo <nom>Las Vegas</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2014/011375.php">Kanibalismo - kaj kazinoj</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ludotabloj de la lokaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Lenio Marobin</aut>
          <vrk><url ref="&Monato;/2013/011053.php">Ekonomia fato</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        li perdis sian monon en <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2010/010015.php">Eleganta krimo</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        la enkonduko de <tld/>oj en Japanion sendube naskos novajn vetludmaniulojn
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2014/011375.php">Kanibalismo - kaj kazinoj</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Spielkasino</trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_z;&c_ib;&c_n;&c_o;</trd>
  <trd lng="br">kazino</trd>
  <trd lng="cs">kasino</trd>
  <trd lng="en">casino</trd>
  <trd lng="fr">casino</trd>
  <trd lng="hu">kaszin&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#23087;&#27005;&#22580;  <pr>&#12372;&#12425;&#12367;&#12376;&#12423;&#12358;</pr></trd>,
    <trd>&#31038;&#20132;&#22580;  <pr>&#12375;&#12419;&#12371;&#12358;&#12376;&#12423;&#12358;</pr></trd>,
    <trd>&#12459;&#12472;&#12494;</trd>,
    <trd>&#36077;&#21338;&#22580;  <pr>&#12392;&#12400;&#12367;&#12376;&#12423;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">casino</trd>
  <trd lng="pl">kasyno</trd>     
  <trd lng="pt">cassino</trd>
  <trd lng="ro">cazinou</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_z;&c_i;&c_n;&c_o;</trd>,
    <trd>&c_i;&c_g;&c_o;&c_r;&c_n;&c_o;&c_je; &c_z;&c_a;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">kas&iacute;no</trd>
  <trd lng="uk">&c_k;&c_a;&c_z;&c_i;&c_n;&c_o;</trd>
  <trdgrp lng="zh">
    <trd>&#36077;&#22580; <pr>d&#468;ch&#462;ng</pr></trd>,
    <trd>&#23067;&#27138;&#22580; <pr>y&uacute;l&egrave;ch&#462;ng</pr></trd>
  </trdgrp>
</drv>
