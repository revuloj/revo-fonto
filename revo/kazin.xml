<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kazin.xml,v 1.16 2019/01/25 23:10:26 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>kazin</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kazin.0o">
  <kap><tld/>o</kap>
  <snc mrk="kazin.0o.TUR">
    <uzo tip="fak">SPO</uzo>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Konstrua&jcirc;o por publikaj kunvenoj kaj amuza&jcirc;oj (dancoj, monludoj
      kaj
      ceteraj) precipe en kurac- a&ubreve; banurboj:
      <ekz>
        urba banejo kaj <tld/>o de Boulogne-sur-mer
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="">
          [reklamo]</url></vrk>,
          <lok>Kongresa Libro, 1905</lok>
        </fnt>.
      </ekz> 
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_z;&c_ib;&c_n;&c_o;</trd>
  <trd lng="br">kazino</trd>
  <trd lng="cs">kasino</trd>
  <trdgrp lng="de">
    <trd>Festsaal</trd>,
    <trd>Veranstaltungssaal</trd>
  </trdgrp>
  <trd lng="en">casino</trd>
  <trd lng="fr">casino</trd>
  <trd lng="hu">kaszin&oacute;</trd>
  <trd lng="nl">casino</trd>
  <trd lng="pl">kasyno</trd>     
  <trd lng="ro">cazinou</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_z;&c_i;&c_n;&c_o;</trd>,
    <trd>&c_i;&c_g;&c_o;&c_r;&c_n;&c_o;&c_je; &c_z;&c_a;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">kas&iacute;no</trd>
</drv>
</art>
