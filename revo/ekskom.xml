<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ekskom.xml,v 1.29 2024/09/25 03:37:35 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>ekskomunik</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ekskom.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ekskom.0i.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Solene <ref tip="super" cel="pel.el0i.KOMUNE">elpeli</ref>
      el religia komunumo.
    </dif>
    <refgrp tip="vid">
      <ref cel="ben.mal0i.REL">malbeni</ref>
    </refgrp>
    <trd lng="be">&c_a;&c_d;&c_l;&c_u;&c_ch;&c_a;&c_c;&c_mol; <klr>(&c_a;&c_d; &c_c;&c_a;&c_r;&c_k;&c_v;&c_y;)</klr></trd>
    <trd lng="bg">&c_o;&c_t;&c_l;&c_malmol;&c_ch;&c_v;&c_a;&c_m; &c_o;&c_t; &c_c;&c_malmol;&c_r;&c_k;&c_v;&c_a;&c_t;&c_a;</trd>
    <trd lng="ca">excomunicar</trd>
    <trdgrp lng="de">
       <trd>exkommunizieren</trd>,
       <trd>mit <ind>Bann belegen</ind></trd>
    </trdgrp>
    <trd lng="io">exkomunikar</trd>
    <trdgrp lng="nl">
      <trd>excommuniceren</trd>,
      <trd>in de kerkban doen</trd>,
      <trd>in de kerkban slaan</trd>,
      <trd>de kerkban uitspreken<klr> (over iem.)</klr></trd>
    </trdgrp>
    <trd lng="pl">ekskomunikowa&cacute;</trd>
    <trd lng="pt">excomungar</trd>
    <trd lng="ru">&c_o;&c_t;&c_l;&c_u;&c_ch;&c_i;&c_t;&c_mol; <klr>(&c_o;&c_t; &c_c;&c_je;&c_r;&c_k;&c_v;&c_i;)</klr></trd>
  </snc>
  <snc mrk="ekskom.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Solene forpeli el iu grupo a&ubreve; partio.
    </dif>
    <adm>
      Mi demandas al mi, &ccirc;u vere temas pri normala
      figura senco a&ubreve; pri ironia esprimo (kiel en la
      franca). <aut>MB</aut>
    </adm>
    <trd lng="be">&c_v;&c_y;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_c;&c_mol; <klr>(&c_z; &c_p;&c_a;&c_r;&c_t;&c_y;&c_ib;)</klr></trd>
    <trd lng="ca">expulsar</trd>
    <trdgrp lng="de">
       <trd>ausschlie&szlig;en</trd>,
       <trd>aussto&szlig;en</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>wyklucza&cacute;</trd>,
      <trd>wyrzuca&cacute;</trd>
    </trdgrp>
    <trd lng="pt">expulsar</trd>
    <trd lng="ru">&c_i;&c_s;&c_k;&c_l;&c_ju;&c_ch;&c_i;&c_t;&c_mol; <klr>(&c_i;&c_z; &c_p;&c_a;&c_r;&c_t;&c_i;&c_i;)</klr></trd>
    <trdgrp lng="zh">
    <trd>&#x657A; <pr>q&#x16B;</pr></trd>,
    <trd>&#25380; <pr>j&#x1D0;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="cs">
    <trd>exkomunikovat</trd>,
    <trd>vyobcovat</trd>
  </trdgrp>
  <trd lng="en">excommunicate</trd>
  <trdgrp lng="fa">
    <trd>&a_t;&f_k;&a_f;&f_y;&a_r; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_T;&a_r;&a_d; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">excommunier</trd>
  <trdgrp lng="hu">
    <trd>kik&ouml;z&ouml;s&iacute;t</trd>,
    <trd>ki&aacute;tkoz</trd>
  </trdgrp>
  <trd lng="it">scomunicare</trd>
  <trdgrp lng="ja">
    <trd>&#30772;&#38272;&#12377;&#x308B;  <pr>&#x306F;&#12418;&#12435;&#12377;&#x308B;</pr></trd>,
    <trd>&#x8FFD;&#x653E;&#12377;&#x308B;  <pr>&#12388;&#12356;&#x307B;&#12358;&#12377;&#x308B;</pr></trd>,
    <trd>&#38500;&#x540D;&#12377;&#x308B;  <pr>&#12376;&#12423;&#12417;&#12356;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="sk">vyl&uacute;&ccaron;i&tcaron; z cirkvi</trd>
  <trd lng="uk">&c_v;&c_ib;&c_d;&#x43B;&c_u;&c_ch;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="ekskom.0o">
  <kap><tld/>o</kap>
  <snc mrk="ekskom.0o.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Rito per kiu oni elpelas anon de
      religia komunumo:
      <ekz>
        la &scirc;okita kaj mal&gcirc;oja kolegio de <ctl>parnassim</ctl> elparolis
        kontra&ubreve; Spinoza'n la malgrandan <tld/>on
        <fnt>
          <aut>T. De Vries, trad. H. Beijne</aut>,
          <vrk><url ref="http://www.autodidactproject.org/other/spinoza_ekskomuniko.html">
          La Ekskomuniko de Baru&hcirc; de Spinoza</url></vrk>,
          <lok>La Gazeto, 2000-01-31 (86), p. 6a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_l;&c_u;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je; <klr>(&c_a;&c_d; &c_c;&c_a;&c_r;&c_k;&c_v;&c_y;)</klr></trd>
  <trd lng="de">Exkommunikation</trd>
  <trd lng="en">excommunication</trd>
  <trd lng="fr">excommunication</trd>
  <trd lng="io">exkomuniko</trd>
  <trd lng="ja">&#30772;&#38272;  <pr>&#x306F;&#12418;&#12435;</pr></trd>
  <trdgrp lng="nl">
    <trd>excommunicatie</trd>,
    <trd>kerkban</trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_i;&#x43A;&#x43B;&#x44E;&c_ch;&c_je;&#x43D;&#x43D;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x7D55;&#x7F70; <pr>ju&#xE9;f&#xE1;</pr></trd>,
    <trd>&#36880;&#x51FA;&#25945;&#x4F1A; <pr>zh&#xFA;ch&#x16B;ji&#xE0;ohu&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>
