<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>ekskomunik</rad>/i
</kap>
<drv mrk="ekskom.0i">
	<kap><tld/>i</kap>
	<gra><vspec>tr</vspec></gra>
	<snc num="1">
		<uzo tip="fak">KAT</uzo>
		<dif>
		Forpeli el la eklezio. 
		</dif>
		<trd
lng="be">&c_a;&c_d;&c_l;&c_u;&c_ch;&c_a;&c_c;&c_mol;
        <klr>(&c_a;&c_d; &c_c;&c_a;&c_r;&c_k;&c_v;&c_y;)</klr></trd>
		  <trdgrp lng="de">
		     <trd>exkommunizieren</trd>,
		     <trd>mit <ind>Bann belegen</ind></trd>
		  </trdgrp>
		<trd lng="fr">excommunier<klr tip="ind">
(rel.)</klr></trd>
		<trd lng="pl">ekskomunikowa&cacute;</trd>
		<trd lng="pt">excomungar</trd>
		<trd
lng="ru">&c_o;&c_t;&c_l;&c_u;&c_ch;&c_i;&c_t;&c_mol;
        <klr>(&c_o;&c_t; &c_c;&c_je;&c_r;&c_k;&c_v;&c_i;)</klr></trd>
	</snc>
	<snc num="2">
		<uzo tip="stl">FIG</uzo><dif>
		Solene forpeli el iu partio.  
		</dif>
		<adm>
		Mi demandas al mi, &ccirc;u vere temas pri normala
		figura senco a&ubreve; pri ironia esprimo (kiel en la
		franca). <aut>MB</aut>
		</adm>
		<trd lng="be">
        &c_v;&c_y;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_c;&c_mol; <klr>(&c_z; 
		&c_p;&c_a;&c_r;&c_t;&c_y;&c_ib;)</klr></trd>
	  <trdgrp lng="de">
	     <trd>ausschlie&szlig;en</trd>,
	     <trd>aussto&szlig;en</trd>
	  </trdgrp>
		<trdgrp lng="fr">
	 <trd>excommunier<klr tip="ind"> (d'un parti,
ironique)</klr></trd>,
	  <trd>exclure<klr tip="ind"> (d'un parti)</klr></trd></trdgrp> 
	  <trdgrp lng="pl">
	    <trd>wyklucza&cacute;</trd>,
	    <trd>wyrzuca&cacute;</trd>
	  </trdgrp>
	  <trd lng="pt">expulsar</trd>
		<trd
lng="ru">&c_i;&c_s;&c_k;&c_l;&c_ju;&c_ch;&c_i;&c_t;&c_mol; 
		<klr>(&c_i;&c_z;
&c_p;&c_a;&c_r;&c_t;&c_i;&c_i;)</klr></trd>
	</snc>
	<trdgrp lng="hu">
	   <trd>kik&ouml;z&ouml;s&iacute;t</trd>,
	   <trd>ki&aacute;tkoz</trd>
	</trdgrp>
</drv>
</art>
