<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>dros</rad>/o
</kap>
<drv mrk="dros.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      &Scirc;nuro a&ubreve; &ccirc;eno, kiu &ccirc;irka&ubreve;volvas
      la direktilan radon de la &scirc;ipo kaj kiu movas la
      <ref cel="direkt.0ilo">direktilon</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Trosse</trd>,
    <trd>Ruderreep</trd>,
    <trd>Steuerreep</trd>
  </trdgrp>
  <trd lng="fr">drosse</trd>
  <trdgrp lng="hu">
    <trd>korm&aacute;nyl&aacute;nc</trd>,
    <trd>korm&aacute;nyk&ouml;t&eacute;l</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_t;&c_u;&c_r;&c_t;&c_r;&c_o;&c_s;</trd>
</drv>
</art>
