<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dros.xml,v 1.17 2023/11/04 18:50:52 revo Exp $">
<kap>
  <rad>dros</rad>/o
</kap>
<drv mrk="dros.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="sxnur.0o">&Scirc;nuro</ref>
      a&ubreve; <ref tip="super" cel="cxen.0o.TEK">&ccirc;eno</ref>, kiu &ccirc;irka&ubreve;volvas
      la direktilan radon de la &scirc;ipo kaj kiu movas la
      <ref cel="direkt.0ilo">direktilon</ref>:
      <ekz>
        la unua pafo trafis la <tld/>on, tiel ke la stirrado turni&gcirc;is senutile en la
        manoj de la direktilisto, la rudro ne plu stre&ccirc;ita de la &scirc;nuroj centri&gcirc;is.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_t;&c_u;&c_r;&c_t;&c_r;&c_o;&c_s;</trd>
  <trdgrp lng="cs">
    <trd>kormideln&iacute; lano</trd>,
    <trd>kormideln&iacute; &rcaron;et&ecaron;z</trd>,
    <trd>t&aacute;hlo</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Trosse</trd>,
    <trd>Ruderreep</trd>,
    <trd>Steuerreep</trd>
  </trdgrp>
  <trd lng="fr">drosse</trd>
  <trdgrp lng="hu">
    <trd>korm&aacute;nyl&aacute;nc</trd>,
    <trd>korm&aacute;nyk&ouml;t&eacute;l</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8235;&#x7DB1;  <pr>&#x304B;&#x3058;&#x3064;&#x306A;</pr></trd>,
    <trd>&#x8235;&#x9396;  <pr>&#x304B;&#x3058;&#x304F;&#x3055;&#x308A;</pr></trd>
  </trdgrp>
  <trd lng="pt">cabo do <ind>leme</ind></trd>
  <trd lng="ru">&c_sh;&c_t;&c_u;&c_r;&c_t;&c_r;&c_o;&c_s;</trd>
  <trd lng="sk">kormidlov&eacute; lano <klr>(re&tcaron;az)</klr></trd>
  <trd lng="uk">&#x431;&#x435;&#x439;&#x444;&#x443;&#x442;</trd>
  <trdgrp lng="zh">
    <trd>&#x7E9C;&#x7D22; <pr>l&#x1CE;nsu&#x1D2;</pr></trd>,
    <trd>&#x7E9C; <pr>l&#x1CE;n</pr></trd>,
    <trd>&#x7E9C;&#x7E69; <pr>l&#x1CE;nsh&#xE9;ng</pr></trd>
  </trdgrp>
</drv>
</art>
