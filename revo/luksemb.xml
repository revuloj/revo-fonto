<art mrk="$Id: luksemb.xml,v 1.19 2024/11/25 10:33:50 revo Exp $">
<kap><ofc>10</ofc>
  <rad>Luksemburg</rad>/o
</kap>
<drv mrk="luksemb.L0o">
  <kap>
    <ofc>10</ofc><tld/>o
  </kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="luksemb.L0o.urb">
    <dif>
      <ref tip="lst" cel="urb.0o" lst="voko:urboj">Urbo</ref> en la
      centro de la &scirc;tato
      <ref tip="malprt" cel="luksemb.L0o.sxtat">Luksemburgo</ref>
      (6&#730;8'E,
      49&#730;36'N):
    </dif>
    <trd lng="cs">Lucemburk</trd>
  </snc>
  <snc mrk="luksemb.L0o.prov">
    <dif>
      <ref tip="super" cel="provin.0o">Provinco</ref> en
      <ref tip="malprt" cel="valon.V0ujo">Valonujo</ref> (suda
      Belgujo):
      <ekz>
        la E&ubreve;<klr>[rop]</klr>regiono Mozo-Rejno konsistas el la belgaj
        provincoj
        Limburgo
        kaj <tld/>o,
        el la suda parto de nederlanda Limburgo, kaj el la subregiono
        Akeno
        <fnt><aut>Jean-Marie JACQUES</aut>:
        <vrk>Senlime</vrk>,
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="luksemb.L0o.sxtat">
    <dif>
      <ref tip="lst" cel="sxtat.0o" lst="voko:&scirc;tatoj">&Scirc;tato</ref>
      situanta en
      okcidenta <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref>
      kaj limanta okcidente al <ref tip="vid" cel="belg.B0ujo">Belgujo</ref>, oriente
      al <ref tip="vid" cel="german1.G0ujo">Germanujo</ref>, sude al
      <ref tip="vid" cel="franc.F0ujo">Francujo</ref>:
      <ekz>
        Fogra kolektis
        grason en deponejoj en suda Belgio, norda Francio kaj <tld/>o
        <fnt><aut>Roland ROTSAERT</aut>:
        <vrk><url ref="&Monato;/1999/006610.html">Dioksino-krizo</url></vrk>,
        <bib>Monato</bib>,
        <lok>1999/07</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="cs">Lucembursko</trd>
  </snc>
  <trd lng="bg">&c_L;&c_ju;&c_k;&c_s;&c_je;&c_m;&c_b;&c_u;&c_r;&c_g;</trd>
  <trd lng="be">&c_L;&c_ju;&c_k;&c_s;&c_e;&c_m;&c_b;&c_u;&c_r;&c_g;</trd>
  <trd lng="br">Luksembourg</trd>
  <trd lng="ca">Luxemburg</trd>
  <trd lng="da">Luxembourg</trd>
  <trd lng="de">Luxemburg</trd>
  <trd lng="en">Luxemburg</trd>
  <trd lng="el">&Lambda;&omikron;&ypsilon;&xi;&epsilon;&my;&beta;&omikron;&ypsilon_acute;&rho;&gamma;&omikron;</trd>
  <trd lng="es">Luxemburgo</trd>
  <trd lng="fa">&a_l;&a_w;&f_k;&a_z;&a_A;&a_m;&a_b;&a_w;&a_r;&f_g;</trd>
  <trd lng="fi">Luxemburg</trd>
  <trd lng="fr">Luxembourg</trd>
  <trd lng="gd">Lucsamburg</trd>
  <trd lng="hu">Luxemburg</trd>
  <trd lng="id">Luksemburg</trd>
  <trd lng="is">L&uacute;xemborg</trd>
  <trd lng="it">Lussemburgo</trd>
  <trd lng="ja">&#12523;&#12463;&#12475;&#12531;&#12502;&#12523;&#12463;</trd>
  <trd lng="lt">Liuksemburgas</trd>
  <trd lng="lv">Luksemburga</trd>
  <trd lng="nl">Luxemburg</trd>
  <trd lng="pl">Luksemburg</trd>
  <trd lng="pt">Luxemburgo</trd>
  <trd lng="ru">&c_L;&c_ju;&c_k;&c_s;&c_je;&c_m;&c_b;&c_u;&c_r;&c_g;</trd>
  <trd lng="sk">Luxemburg</trd>
  <trd lng="sl">Luksemburg</trd>
  <trd lng="sv">Luxemburg</trd>
  <trd lng="tr">L&uuml;ksenburg</trd>
  <trd lng="uk">&c_L;&c_ju;&c_k;&c_s;&c_je;&c_m;&c_b;&c_u;&c_r;&c_gu;</trd>
  <trdgrp lng="zh">
    <trd>&#21346;&#26862;&#22561; <pr>L&uacute;s&emacron;nb&#462;o</pr></trd>,
    <trd>&#30439;&#26862;&#22561; <pr>L&uacute;s&emacron;nb&#462;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="luksemb.0a">
  <kap>
    <ofc>10</ofc><tld lit="l"/>a
  </kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        la <tld/>a ju&gcirc;istaro oficiale konfesis la ekzistadon de la grafologio, turnante
        sin al la redaktoro de la dirita gazeto en unu ju&gcirc;a afero, en kiu ordinaraj
        kaligrafoj ne povis helpi
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Claude Nourmont</nom>, vic-prezidantino de Esperanto-France, plusendis gazetaran
        komunikon pri la E&ubreve;ropa Tago al <tld lit="l"/>aj gazetoj
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#8470; 7 (105)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ju;&c_k;&c_s;&c_e;&c_m;&c_b;&c_u;&c_r;&c_s;&c_k;&c_ib;</trd>
  <trd lng="de">luxemburgisch</trd>
</drv>

<drv mrk="luksemb.0ano">
  <kap>
    <ofc>10</ofc><tld/>ano
  </kap>
  <snc>
    <dif>
      Ano de <tld/>o:
      <ekz>
        la centraziano profitis de la momenta malekvilibro de l&#8217; <tld lit="l"/>ano por
        pu&scirc;i &ccirc;i-lastan &ccirc;e la dorso
        <fnt>
          <bib>CKv</bib>
          <lok>19</lok>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve; &gcirc;i, la <klr>[&#8230;]</klr> <tld lit="l"/>anoj volos denove aparteni al la
        Granddukujo <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="&Monato;/2007/009338.php"
              >Separismo ne, egalrajteco jes</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ju;&c_k;&c_s;&c_e;&c_m;&c_b;&c_u;&c_r;&c_zh;&c_a;&c_c;</trd>
  <trd lng="de">Luxemburger</trd>
</drv>

</art>
