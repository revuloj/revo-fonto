<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vertigx.xml,v 1.14 2024/09/05 15:30:02 revo Exp $">
<kap>
  <rad>verti&gcirc;</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="vertigx.0o">
  <kap><tld/>o</kap>
  <snc mrk="vertigx.0o.MED">
  <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="dif" cel="turn.kap0o.MED">Kapturno</ref>:
      <ekz>
        tra la <tld/>o mi ankora&ubreve; vidas
        <fnt>
          <aut>S. Br&oacute;dy, trad. B. Szemenyei</aut>,
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/np/np5606/remb
            randtu.html">
          Rembrandt</url></vrk>,
          <lok>Norda Prismo 1956:6, p. 229-238a</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi tuj sekvis kaj falis kiel sako, pro la malforto kaj <tld/>o,
        kiuj estis rezultoj de tiu longa sunbruli&gcirc;o
        <fnt><aut>R. L. Stevenson, trad. A. Goodheir</aut>,
        <vrk>Forkaptita</vrk>, <lok>Kardo, 1976</lok></fnt>;
      </ekz>
      <ekz>
        junaj viroj ne rajtis edzi&gcirc;i anta&ubreve; ol montri siajn kura&gcirc;on kaj reziston al
        <tld/>o, starante sur alta roko super la maro, &ccirc;e la rando de la klifoj
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2007/009252.php">Kantado en la gaela lingvo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_l;&c_a;&c_v;&c_a;&c_k;&c_r;&c_u;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="ca">
    <trd>vertigen</trd>,
    <trd>rodament de cap</trd>
  </trdgrp>
  <trd lng="de">Schwindel<klr>(anfall)</klr></trd>
  <trdgrp lng="en">
    <trd>dizziness</trd>,
    <trd>vertigo</trd>
  </trdgrp>
  <trd lng="fa">&a_s;&a_r;&f_g;&f_y;&a_j;&a_h;</trd>
  <trd lng="fr">vertige</trd>
  <trd lng="ja">&#12417;&#x307E;&#12356;</trd>
  <trdgrp lng="pl">
    <trd>zawr&oacute;t <ind>g&lstroke;owy</ind></trd>,
    <trd>zawroty <ind>g&lstroke;owy</ind></trd>
  </trdgrp>
  <trd lng="pt">vertigem</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_je;&c_r;&c_t;&c_i;&c_g;&c_o;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_m;&c_o;&c_r;&c_o;&c_ch;&c_je;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#30505;&#26197; <pr>xu&#xE0;ny&#xF9;n</pr></trd>,
    <trd>&#30505;&#26248; <pr>xu&#xE0;ny&#xF9;n</pr></trd>,
    <trd>&#22836;&#x660F; <pr>t&#xF3;uh&#x16B;n</pr></trd>,
    <trd>&#x982D;&#x660F; <pr>t&#xF3;uh&#x16B;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="vertigx.0i">
  <kap>
    <tld/>i
  </kap>
  <snc mrk="vertigx.0i.ekhavi">
    <dif>
      Ekhavi kapturnon:
      <ekz>
        lia kapo <tld/>is kaj li eksentis na&ubreve;zi&gcirc;on
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="&Monato;/2010/009883.php">Trancporto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i diris, rigardu en miajn okulojn! Bastjano faris tion, kvankam batis lia koro
        kaj li &ccirc;e tio iom <tld/>is
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Perelin', la Noktarbaro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_c;&c_mol; <ind>&c_g;&c_a;&c_l;&c_a;&c_v;&c_a;&c_k;&c_r;&c_u;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</ind></trd>,
    <trd>&c_k;&c_r;&c_u;&c_zh;&c_y;&c_c;&c_c;&c_a; <klr tip="amb">(&c_a;&c_b; &c_g;&c_a;&c_l;&c_a;&c_v;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="de"><ind>schwindlig</ind> werden</trd>
  <trdgrp lng="en">
    <trd>get dizzy</trd>,
    <trd>feel vertigo</trd>
  </trdgrp>
  <trd lng="ja">&#12417;&#x307E;&#12356;&#x304C;&#12377;&#x308B;</trd>
  <trd lng="pl">mie&cacute; zawr&oacute;t  <ind>g&lstroke;owy</ind></trd>
  <trdgrp lng="zh">
    <trd>&#30505;&#26197; <pr>xu&#xE0;ny&#xF9;n</pr></trd>,
    <trd>&#30505;&#26248; <pr>xu&#xE0;ny&#xF9;n</pr></trd>,
    <trd>&#x4EE4;&#x4EBA;&#x773C;&#x82B1;&#x7E5A;&#x4E82;
    <pr>l&#xEC;ngr&#xE9;ny&#x1CE;nhu&amacron;li&#xE1;olu&#xE0;n</pr></trd>,
    <trd>&#x4EE4;&#x4EBA;&#x773C;&#x82B1;&#x7F2D;&#x4E71;
    <pr>l&#xEC;ngr&#xE9;ny&#x1CE;nhu&amacron;li&#xE1;olu&#xE0;n</pr></trd>,
    <trd>&#26197;&#x9AD8;&#x513F; <pr>y&#xF9;ng&amacron;or</pr></trd>,
    <trd>&#26248;&#x9AD8;&#20818; <pr>y&#xF9;ng&amacron;or</pr></trd>,
    <trd>&#22836;&#x660F;&#x773C;&#26263; <pr>t&#xF3;uh&#x16B;ny&#x1CE;n&#xE0;n</pr></trd>,
    <trd>&#x982D;&#x660F;&#x773C;&#26263; <pr>t&#xF3;uh&#x16B;ny&#x1CE;n&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="vertigx.0igi">
  <kap>
    <tld/>igi
  </kap>
  <snc mrk="vertigx.0igi.kauxzi">
    <dif>
      Ka&ubreve;zi kapturnon:
      <ekz>
        <tld/>iga alteco
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Goabo, la dezerto de la Koloroj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u tio tiel na&ubreve;zas kaj <tld/>igas vin?
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="&Monato;/2010/009883.php">Trancporto</url></vrk>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol; <ind>&c_g;&c_a;&c_l;&c_a;&c_v;&c_a;&c_k;&c_r;&c_u;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</ind></trd>
  <trd lng="de"><ind>Schwindel</ind> erregen</trd>
  <trdgrp lng="en">
    <trd>make dizzy</trd>,
    <trd>cause vertigo</trd>
  </trdgrp>
  <trd lng="pl">wywo&lstroke;a&cacute; zawr&oacute;t  <ind>g&lstroke;owy</ind></trd>
</drv>
</art>

