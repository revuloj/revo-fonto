<kap>
  <ofc>2</ofc><rad>pagod</rad>/o
</kap>
<drv mrk="pagod.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ARKI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="templ.0o.rezidejo">Templo</ref> de aziaj dia&jcirc;oj, precipe de Budao, en &Ccirc;inujo
      kaj Hindujo:
      <ekz>
        kvinoble malgrandigita modelo de la plej malnova en la mondo
        ligna <tld/>o de <nom>Horin&gcirc;i</nom>
        <fnt><aut>Laimius
        Stra&zcaron;nickas</aut>:
        <vrk>Entuziasmulo pri Japanio</vrk>, <bib>Monato</bib><lok>jaro
        2002a, numero 5a, p. 26a</lok></fnt>;
      </ekz>
      <ekz>
        konstrua&jcirc;o en formo de <tld/>o videblis en nia &gcirc;ardeno
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/006878.html">Dommastrumo: &Gcirc;ardenumi</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_g;&c_a;&c_d;&c_a;</trd>
  <trd lng="bg">&c_p;&c_a;&c_g;&c_o;&c_d;&c_a;</trd>
  <trd lng="cs">pagoda</trd>
  <trd lng="de">Pagode</trd>
  <trd lng="en">pagoda</trd>
  <trdgrp lng="fr">
    <trd>pagode</trd>
  </trdgrp>
  <trd lng="hu">pagoda</trd>
  <trd lng="nl">pagode</trd>
  <trd lng="pl">pagoda</trd>
  <trdgrp lng="pt">
   <trd>pagode</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_g;&c_o;&c_d;&c_a;</trd>
  <trd lng="sk">pagoda</trd>
  <trd lng="th">&#xE40;&#xE08;&#xE14;&#xE35;&#xE22;&#xE4C;</trd>
  <trd lng="uk">&#x43F;&#x430;&#x491;&#x43E;&#x434;&#x430;</trd>
</drv>
