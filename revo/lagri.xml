<art mrk="$Id: lagri.xml,v 1.22 2023/09/01 19:10:57 revo Exp $">
<kap>
  <rad>lagri</rad>/o
</kap>
<drv mrk="lagri.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="lagri.0o.ZOO">
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> de
      malgrandaj <ref tip="malprt" cel="koleop.0oj">koleopteroj</ref>
      kun flavbrunaj lanecaj flugilkovriloj:
      <ekz>
        la plenkreska <tld/>o estas 7&#x202F;&dash;&#x202F;10&#x202F;mm longa kaj havas
        piedformulon 5,5,4.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/8/89/Lagria_hirta_02.JPG/640px-Lagria_hirta_02.JPG" prm="CC BY-SA     4.0">Hirta <tld/>o (Lagria hirta)
        <fnt>
          <aut>AfroBrazilian, propra verko</aut>
          <lok><url ref="&WCW;/File:Lagria_hirta_02.JPG">&WCW;/File:Lagria_hirta_02.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_h;&c_n;&c_a;&c_t;&c_k;&c_a; <klr>(&c_n;&c_a;&c_s;&c_ja;&c_k;&c_o;&c_m;&c_a;&c_je;)</klr></trd>
  <trd lng="de">Wollk&auml;fer</trd>
  <trd lng="fr">lagria</trd>
  <trd lng="hu">gyapjasbog&aacute;r</trd>
  <trd lng="ja">&#x30CF;&#x30E0;&#x30B7;&#x30C0;&#x30DE;&#x30B7;</trd>
  <trd lng="nl">ruigkever</trd>
  <trd lng="pl">omi&eogonek;k  <klr>(chrz&aogonek;szcz)</klr></trd>
  <trdgrp lng="pt">
    <trd>idiamim </trd>,
    <trd>bicho-capixaba</trd>,
    <trd>capixabinha</trd>
  </trdgrp>
  <trd lng="ro">lagria hirta</trd>
  <trd lng="ru">&c_m;&c_o;&c_h;&c_n;&c_a;&c_t;&c_k;&c_a;</trd>
  <trd lng="uk">&#x43C;&#x43E;&c_h;&#x43D;&c_a;&c_t;&#x43A;&c_a;</trd>
</drv>
