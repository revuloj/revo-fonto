<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>lagri</rad>/o
</kap>
<drv mrk="lagri.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="lagri.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Lagria</trd>),
      malgranda koleoptero kun flavbrunaj lanecaj flugilkovriloj.
    </dif>
  </snc>
  <trd lng="de">Wollk&auml;fer</trd>
</drv>
<drv mrk="lagri.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="koleop.0oj">koleopteroj</ref>
      (<trd lng="la">Lagriidae</trd>)
    </dif>
  </snc>
  <trd lng="de">Wollk&auml;fer <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>
