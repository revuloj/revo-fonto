<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lagri.xml,v 1.18 2020/05/24 20:36:58 revo Exp $">
<kap>
  <rad>lagri</rad>/o
</kap>
<drv mrk="lagri.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="lagri.0o.ZOO">
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> de
      malgrandaj <ref tip="malprt" cel="koleop.0oj">koleopteroj</ref>
      kun flavbrunaj lanecaj flugilkovriloj:
      <ekz>
        la plenkreska <tld/>o estas 7&#8239;&dash;&#8239;10&#8239;mm longa kaj havas
        piedformulon 5,5,4.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/8/89/Lagria_hirta_02.JPG/640px-Lagria_hirta_02.JPG"
      prm="CC BY-SA 4.0"
      >Hirta <tld/>o (Lagria hirta)
        <fnt>
          <aut>AfroBrazilian, propra verko</aut>
          <lok><url ref="&WCW;/File:Lagria_hirta_02.JPG"
              >https://commons.wikimedia.org/wiki/File:Lagria_hirta_02.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="de">Wollk&auml;fer</trd>
  <trd lng="fr">lagria</trd>
  <trd lng="hu">gyapjasbog&aacute;r</trd>
  <trd lng="nl">ruigkever</trd>
  <trd lng="pl">omi&eogonek;k  <klr>(chrz&aogonek;szcz)</klr></trd>
  <trdgrp lng="pt">
    <trd>idiamim </trd>,
    <trd>bicho-capixaba</trd>,
    <trd>capixabinha</trd>
  </trdgrp>
  <trd lng="ro">lagria hirta</trd>
  <trd lng="ru">&c_m;&c_o;&c_h;&c_n;&c_a;&c_t;&c_k;&c_a;</trd>
</drv>
</art>
