<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>akre</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="akre.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Eksa e&ubreve;ropa <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de
      areo valoranta inter 0,19 kaj 0,86 hektarojn la&ubreve; lokoj kaj
      epokoj:
      <ekz>
        vi posedas nur <klr>...</klr> du <tld/>ojn da plugotero kaj
        apena&ubreve;
        paron da &ccirc;ifonoj
        <fnt><bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 2a, p.
        407a</lok></fnt>
      </ekz>
    </dif>
    <subsnc mrk="akre.0o.brita">
      <dif>
        Brita <tld/>o: proksimume 0,4 hektarojn.
      </dif>
    </subsnc>
    <subsnc mrk="akre.0o.franca">
      <dif>
        Franca <tld/>o: proksimume 0,5 hektarojn.
      </dif>
    </subsnc>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_r;</trd>
  <trd lng="de">Acre<klr> (Einheit)</klr></trd>
  <trd lng="fa">&a_A_madda;&f_k;&a_r;<klr> (&a_w;&a_A;&a_H;&a_d; &a_s;&a_T;&a_H;)</klr></trd>
  <trd lng="fr">acre<klr> (mesure de surface)</klr></trd>
  <trd lng="pt">acre</trd>
  <trd lng="ru">&c_a;&c_k;&c_r;</trd>
</drv>
</art>

