<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ekspert</rad>/o
</kap>

<drv mrk="eksper1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="vid" cel="eksper2.0isto">Ekspertizisto</ref> dungita por
      priju&gcirc;i aferon anta&ubreve; kortumo, por asekuro ktp.:
      <ekz>
        tiaj kolektoj stimulis la pristudadon kaj ofte la posedanto
        petis
        la konsilon de <tld/>oj<fnt><aut>Johan Van Eenoo</aut>: <vrk>La
        bru&gcirc;a Centuriae plantarum rariorum retrovita en
        Krakovo</vrk>,
        <bib>Monato</bib><lok>jaro 1998a, numero 1a, p. 16a</lok></fnt>;
      </ekz>
      <ekz>
        mi estas <tld/>o pri nigra magio<fnt><bib>MkM</bib>,
        <lok>&ccirc;ap. 1a,
        p. 14a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_k;&c_s;&c_p;&c_e;&c_r;&c_t;</trd>,
    <trd>&c_a;&c_d;&c_m;&c_y;&c_s;&c_l;&c_o;&c_v;&c_je;&c_c;</trd>,
    <trd>&c_s;&c_p;&c_e;&c_c;&c_y;&c_ja;&c_l;&c_ib;&c_s;&c_t;</trd>,
    <trd>&c_z;&c_n;&c_a;&c_w;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_je;&c_k;&c_s;&c_p;&c_je;&c_r;&c_t;</trd>,
    <trd>&c_s;&c_p;&c_je;&c_c;&c_i;&c_a;&c_l;&c_i;&c_s;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>expert</trd>,
    <trd>t&egrave;cnic</trd>,
    <trd>especialista</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>odborník</trd>,
    <trd>specialista</trd>,
    <trd>znalec</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Fachmann</trd>,
    <trd>Experte</trd>,
    <trd>Kenner der Materie</trd>,
    <trd>Spezialist</trd>
  </trdgrp>
  <trd lng="es">experto</trd>
  <trdgrp lng="fr">
    <trd>connaisseur</trd>,
    <trd>expert</trd>,
    <trd>sp&eacute;cialiste</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ekspert</trd>,
    <trd>specjalista</trd>,
    <trd>bieg&lstroke;y</trd>,
    <trd>znawca</trd>,
    <trd>rzeczoznawca</trd>,
    <trd>fachowiec</trd>
  </trdgrp>
  <trd lng="pt">expert</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_k;&c_s;&c_p;&c_je;&c_r;&c_t;</trd>,
    <trd>&c_z;&c_n;&c_a;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_s;&c_p;&c_je;&c_c;&c_i;&c_a;&c_l;&c_i;&c_s;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odborník</trd>,
    <trd>znalec</trd>
  </trdgrp>
</drv>
</art>
