<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: flat.xml,v 1.21 2015/07/04 08:37:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>flat</rad>/i
</kap>
<drv mrk="flat.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Provi pla&ccirc;i al iu, provi logi a&ubreve; igi favora iun per troaj
      la&ubreve;doj:
      <ekz>
        <tld/>i iun, al iu<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i ies ambicion;
      </ekz>
      <ekz>
        <tld/>aj vortoj;
      </ekz>
      <ekz>
        <tld/>a portreto;
      </ekz>
      <ekz>
        rolo tre <tld/>a sed tre &scirc;ar&gcirc;a<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &ccirc;irka&ubreve;<tld/>adi kaj &ccirc;irka&ubreve;flirtadi iun<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kares.0i">karesi</ref>.
    </refgrp>
    <trdgrp lng="ca">
      <trd>adular</trd>,
      <trd>amoixar</trd>
    </trdgrp>
    <trd lng="de">schmeicheln</trd>
  </snc>
  <snc>
    <dif>
      Esti tre agrabla al:
      <ekz>
        tiaj interrilatoj <tld/>is lian ambicion<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi estis tre <tld/>ita de la favora
        akcepto, kiun vi faris al mi<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi ne <tld/>as min <klr>(ne havas la eble iluzian
        esperon)</klr>,
        ke mia
        koro estas sama, kiel tiu<fnt>B</fnt>;
      </ekz>
      <ekz>
        ili <tld/>is al si, ke ilia ruzo sukcesis;
      </ekz>
      <ekz>
        vi <tld/>as al vi, ke vi falos kiel herooj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>lloar</trd>,
      <trd>delectar</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_s;&c_mol;&c_l;&c_ib;&c_v;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="ca">afalagar</trd>
  <trd lng="cs">lichotit</trd>
  <trd lng="en">flatter</trd>
  <trd lng="es">halagar</trd>
  <trd lng="fr">flatter</trd>
  <trd lng="hu">h&iacute;zeleg</trd>
  <trd lng="nl">flatteren</trd>
  <trdgrp lng="pt">
    <trd>lisonjear</trd>,
    <trd>adular</trd>
    <trd>puxar o <ind>saco</ind> de</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_mol;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">lichotiť</trd>
</drv>

<drv mrk="flat.0anto0ulo">
  <kap><tld/>anto, <var><kap><tld/>ulo</kap></var></kap>
  <snc>
    <dif>
      Homo, kiu emas <tld/>i.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_s;&c_mol;&c_l;&c_ib;&c_v;&c_je;&c_c;</trd>
  <trdgrp lng="ca">
    <trd>adulador</trd>,
    <trd>llagoter</trd>
  </trdgrp>
  <trd lng="cs">šplhoun</trd>
  <trd lng="de">Schmeichler</trd>
  <trd lng="en">flatterer</trd>
  <trd lng="es">halagador</trd>
  <trd lng="fr">flatteur</trd>
  <trd lng="hu">h&iacute;zelg&odblac;</trd>
  <trd lng="nl">vleier</trd>
  <trd lng="ru">&c_l;&c_mol;&c_s;&c_t;&c_je;&c_c;</trd>
  <trd lng="sk">šplhúň</trd>
</drv>

<drv mrk="flat.cxirkaux0adi">
  <kap>&ccirc;irka&ubreve;<tld/>adi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ade &ccirc;irka&ubreve;igi per <tld/>oj:
      <ekz>
        &ccirc;irka&ubreve;<tld/>adi kaj &ccirc;irka&ubreve;flirtadi iun
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>encensar</trd>,
    <trd>ensabonar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>entourer de <ind>flatteries</ind></trd>,
    <trd>l&eacute;cher les bottes (de)</trd>
  </trdgrp>
</drv>
</art>
