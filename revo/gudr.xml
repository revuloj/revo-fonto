<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gudr.xml,v 1.25 2023/11/11 11:36:37 revo Exp $">
<kap><ofc>*</ofc>
<rad>gudr</rad>/o</kap>
<drv mrk="gudr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Nigra glua substanco, resta&jcirc;o el distilado de
      <refgrp tip="vid">
        <ref cel="ken.0o">keno</ref>
        a&ubreve; <ref cel="karb.0o">karbo</ref>:
      </refgrp>
      <ekz>
        li &scirc;miris la fundon de la mezurilo per <tld/>o, por ke el tio, kio estos
        mezurata, io restu algluita
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni povas &gcirc;in prezenti kiel kun ciro, tiel anka&ubreve; kun <tld/>o, mi miksas amba&ubreve; en
        egalaj kvantoj
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
      <ekz>
        la radojn protektas <tld/>o-tolo, envolvita en la aertuboj
        <fnt>
       <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2000/006877.php">Nekutimaj transportiloj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 21</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> stratoj pavimitaj per betono <klr>[&#x2026;]</klr> enhavas kelke da
        interspacoj plenigitaj per <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011332.php">Dilati&gcirc;o, kuntiri&gcirc;o ... kaj eksperimento por
          infana amuzi&gcirc;o</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="asfalt.0o">asfalto</ref>,
      <ref cel="bitum.0o">bitumo</ref>,
      <ref cel="makada.0o">makadamo</ref>,
      <ref cel="pecx.ter0o">terpe&ccirc;o</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_m;&c_a;&c_l;&c_a;</trd>,
    <trd>&c_d;&c_z;&c_jo;&c_g;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_a;&c_r;&c_v;&c_o;&c_l;&c_mol;</trd>,
    <trd>&c_g;&c_u;&c_d;&c_r;&c_o;&c_n;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_a;&c_m;&c_a;&c_z;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_k;&c_a;&c_t;&c_r;&c_a;&c_n;</trd>
  <trd lng="ca">quitr&agrave;</trd>
  <trd lng="cs">dehet</trd>
  <trd lng="de">Teer</trd>
  <trd lng="en">tar</trd>
  <trd lng="es">brea</trd>
  <trd lng="fr">goudron</trd>
  <trd lng="hu">k&aacute;tr&aacute;ny</trd>
  <trd lng="ja">&#x30BF;&#x30FC;&#x30EB;</trd>
  <trd lng="nl">teer</trd>
  <trd lng="pl">smo&lstroke;a</trd>
  <trd lng="pt">alcatr&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_m;&c_o;&c_l;&c_a;</trd>,
    <trd>&c_d;&c_jo;&c_g;&c_o;&c_t;&c_mol;</trd>,
    <trd>&c_g;&c_u;&c_d;&c_r;&c_o;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>decht</trd>,
    <trd>t&eacute;r</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_u;&c_d;&c_r;&#x43E;&#x43D;</trd>,
    <trd>&c_d;&#x44C;&#x43E;&c_g;&#x43E;&c_t;&#x44C;</trd>,
    <trd>&c_s;&#x43C;&#x43E;&#x43B;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#x7126;&#x6CB9; <pr>ji&#x101;oy&#xF3;u</pr></trd>
</drv>

<drv mrk="gudr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;miri per <tld/>o:
      <ekz>
        la domo estis <tld/>ita, la flankaj konstrua&jcirc;oj anstata&ubreve; tegmento havis sur si
        renversitan boaton
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ricevis <klr>[&#x2026;]</klr> rezine odoretantan vinon, kiun oni enver&scirc;is el
        <tld/>ita felsako
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> pendigis kiel insignon super sia tendo sonorilon, kiu, &scirc;irme
        kontra&ubreve; la pluvo, estis <tld/>ita kaj ne havis frapilon
        <fnt>
          <bib>Fab2</bib>
          <lok>Sonorilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        tradiciaj boatoj <klr>[&#x2026;]</klr> faritaj el ligna framo kun <tld/>ita kanvasa
        tega&jcirc;o <klr>(vd <ref cel="korakl.0o">koraklo</ref>)</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2007/009255.php">Insulo de verkistoj</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_m;&c_a;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_p;&c_r;&c_a;&c_c;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol; <ind>&c_d;&c_z;&c_jo;&c_g;&c_c;&c_je;&c_m;</ind></trd>
  </trdgrp>
  <trd lng="ca">enquitranar</trd>
  <trdgrp lng="cs">
    <trd>dehtovat</trd>,
    <trd>nat&iacute;rat dehtem</trd>
  </trdgrp>
  <trd lng="de">teeren</trd>
  <trd lng="en">tar</trd>
  <trd lng="fr">goudronner</trd>
  <trd lng="hu">bek&aacute;tr&aacute;nyoz</trd>
  <trd lng="ja">&#x30BF;&#x30FC;&#x30EB;&#x3092;&#x5857;&#x308B;  <pr>&#x30BF;&#x30FC;&#x30EB;&#x3092;&#x306C;&#x308B;</pr></trd>
  <trd lng="nl">teren</trd>
  <trd lng="pl">smo&lstroke;owa&cacute;</trd>
  <trd lng="pt">alcatroar</trd>
  <trd lng="ru">&c_s;&c_m;&c_o;&c_l;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>dechtova&tcaron;</trd>,
    <trd>natiera&tcaron; dechtom</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43C;&#x43E;&#x43B;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_z;&#x43C;&c_a;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i; &c_d;&#x44C;&#x43E;&c_g;&c_t;&c_je;&#x43C;</trd>,
    <trd>&#x43D;&c_a;&#x43C;&c_a;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i; &c_d;&#x44C;&#x43E;&c_g;&c_t;&c_je;&#x43C;</trd>
  </trdgrp>
  <trd lng="zh">&#x5857;&#x4E0A;&#x7126;&#x6CB9; <pr>t&#xFA;sh&#xE0;ngji&#x101;oy&#xF3;u</pr></trd>
</drv>
</art>
