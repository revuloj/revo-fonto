<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>gudr</rad>/o
</kap>
<drv mrk="gudr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Nigra glua substanco, resta&jcirc;o el distilado de keno a&ubreve; karbo. 
    </dif>
  </snc>
  <trd lng="de">Teer</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_m;&c_o;&c_l;&c_a;</trd>,
    <trd>&c_d;&c_jo;&c_g;&c_o;&c_t;&c_mol;</trd>,
    <trd>&c_g;&c_u;&c_d;&c_r;&c_o;&c_n;</trd>
  </trdgrp>
</drv>
<drv mrk="gudr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;miri per <tld/>o:
      <ekz><tld/>i &scirc;ipon<fnt>Z</fnt>,</ekz>
      <ekz>&scirc;nuron;</ekz>
      <ekz><tld/>ita pavimo de strato.</ekz>
    </dif>
  </snc>
  <trd lng="de">teeren</trd>
  <trd lng="ru">&c_s;&c_m;&c_o;&c_l;&c_i;&c_t;&c_mol;</trd>
</drv>
</art>
