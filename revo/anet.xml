<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: anet.xml,v 1.13 2022/03/12 10:16:47 revo Exp $">
<kap>
  <rad>anet</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="anet.0o">
  <kap><tld/>o</kap>
  <snc mrk="anet.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> 
      el familio <ref tip="malprt" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">Anethum</trd>), jara herbo el mezvarmaj kaj varmaj landoj.
      La folipintoj, floroj kaj semoj de la specio <trd lng="la">Anethum graveolens</trd>
      estas uzataj kiel <ref tip="lst" cel="spic.0o.KUI" lst="voko:spicoj">spico</ref>:
      <ekz>
        <tld/>o fajnigas la guston de salatoj, sa&ubreve;coj, fi&scirc;a&jcirc;o, kazeo,
        kukumo, kukurbo, legomaj supoj, pano, terpoma&jcirc;o kaj ova&jcirc;o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=293">Aneto</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[al]</klr> acida brasiko <klr>[&#x2026;]</klr> oni anka&ubreve; aldonas spica&jcirc;ojn kiel pipro, <tld/>o, kreno ks
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=31881">Acida brasiko</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="aniz.0o">anizo</ref>
  </snc>
  <trd lng="bg">&c_k;&c_o;&c_p;&c_malmol;&c_r;</trd>
  <trd lng="ca">anet</trd>
  <trd lng="cs">kopr</trd>
  <trd lng="de">Dill</trd>
  <trd lng="en">dill</trd>
  <trd lng="fr">aneth</trd>
  <trd lng="hu">kapor</trd>
  <trd lng="nl">dille</trd>
  <trdgrp lng="pl">
    <trd>koper</trd>,
    <trd>koperek</trd>
  </trdgrp>
  <trd lng="pt">aneto</trd>
  <trd lng="ru">&c_u;&c_k;&c_r;&c_o;&c_p;</trd>
  <trd lng="sk">k&ocirc;por</trd>
  <trd lng="uk">&#x43A;&#x440;&#x456;&#x43F;</trd>
</drv>
</art>

