<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>anet</rad>/o
</kap>
<drv mrk="anet.0o">
  <kap><tld/>o <fnt><bib>HejmVort</bib></fnt>
    <fnt>
      <bib>Viki</bib>
      <lok><url ref="http://eo.wikipedia.org/wiki/Aneto">Aneto</url></lok>
    </fnt>
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">anethum graveolens</trd>), kies
      folipintoj, floroj kaj la semoj estas uzataj
      kiel spicherbo en kuirarto.
    </dif>
  </snc>
  <trd lng="bg">&c_k;&c_o;&c_p;&c_malmol;&c_r;</trd>
  <trd lng="de">Dill</trd>
  <trd lng="en">dill</trd>
  <trd lng="pt">aneto</trd>
  <trd lng="ru">&c_u;&c_k;&c_r;&c_o;&c_p;</trd>
</drv>
</art>

