      <rad>Zagreb</rad>/o
    </kap>

<drv mrk="zagreb.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc mrk="zagreb.0o.cxefurbo">
    <dif>
      <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&Ccirc;efurbo</ref>
      de <ref tip="malprt" cel="kroat.K0ujo">Kroatujo</ref>:
      situo:  (<frm>45&#x2DA;48&minute;N  15&#x2DA;58&minute;E</frm>):
      <ekz>
        multaj serboj lo&gcirc;is kaj ankora&ubreve; lo&gcirc;as en kroataj grandurboj &dash; &ccirc;efe en Zagrebo
        <fnt>
	<aut>Zlatko Ti&scaron;ljar</aut>
	<vrk><url ref="&Monato;/2000/006811.php">Nekredeblaj malveroj</url></vrk>
	<bib>Monato</bib>
	<lok>2000/03, p. 12-13</lok>
        </fnt>;
      </ekz>
      <ekz>
        la libro pri germana aktorino fari&gcirc;as certagrade tre viveca historia romaneto
        pri <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="&Monato;/2004/007930.html">Interna pejza&gcirc;o de alia virino</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="z"/>a deklaracio anta&ubreve;vidas por la balkanaj landoj kunlaboron kun
        la Haga Tribunalo pri militkrimoj kune kun evoluigo de homaj rajtoj kaj
        demokratio
        <fnt>
          <aut>Zlatko Ti&scaron;ljar</aut>
            <vrk><url ref="&Monato;/2001/007325.php">Pa&scirc;o al paco</url></vrk>
            <bib>Monato</bib>
            <lok>2001/22, p. 7</lok>
          </fnt>;
      </ekz>
      <ekz>
        la <tld lit="z"/>a Universala Kongreso de Esperanto en 2001
        <fnt>
          <bib>Monato</bib>
          <aut>Zlatko Ti&scaron;ljar</aut>
          <vrk><url ref="&Monato;/2004/008453.html">Prezidanto postulas, prezidanto
          rifuzas</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld lit="z"/>anino
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="&Monato;/2004/007930.html">Interna pejza&gcirc;o de alia virino</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_Z;&c_a;&c_g;&c_r;&c_a;&c_b;</trd>
    <trd lng="de">Zagreb</trd>
    <trd lng="en">Zagreb</trd>
    <trd lng="fr">Zagreb</trd>
    <trd lng="nl">Zagreb</trd>
    <trd lng="pl">Zagrzeb</trd>
    <trdgrp lng="zh">
    <trd>&#x672D;&#x683C;&#x96F7;&#x5E03; <pr>Zh&#xE1;g&#xE9;l&#xE9;ib&#xF9;</pr></trd>,
    <trd>&#x672D;&#x683C;&#x62C9;&#x5E03; <pr>Zh&#xE1;g&#xE9;l&amacron;b&#xF9;</pr></trd>,
    <trd>&#x672D;&#x683C;&#x745E;&#x5E03; <pr>Zh&#xE1;g&#xE9;ru&#xEC;b&#xF9;</pr></trd>,
    <trd>&#x8428;&#x683C;&#x52D2;&#x5E03; <pr>S&#xE0;g&#xE9;l&#xE8;b&#xF9;</pr></trd>,
    <trd>&#x85A9;&#x683C;&#x52D2;&#x5E03; <pr>S&#xE0;g&#xE9;l&#xE8;b&#xF9;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="ja">&#x30B6;&#x30B0;&#x30EC;&#x30D6;</trd>
