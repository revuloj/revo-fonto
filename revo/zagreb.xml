<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: zagreb.xml,v 1.4 2022/05/06 12:17:22 revo Exp $">
    <kap>
      <rad>Zagreb</rad>/o
    </kap>

<drv mrk="zagreb.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc mrk="zagreb.0o.cxefurbo">
    <dif>
      <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&Ccirc;efurbo</ref>
      de <ref tip="malprt" cel="kroat.K0ujo">Kroatujo</ref>:
      <ekz>
        multaj serboj lo&gcirc;is kaj ankora&ubreve; lo&gcirc;as en kroataj grandurboj &dash; &ccirc;efe en Zagrebo
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Steloj;monato2/006811.html">Reago: Nekredeblaj malveroj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la libro pri germana aktorino fari&gcirc;as certagrade tre viveca historia romaneto
        pri <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="&Monato;/2004/007930.html">Interna pejza&gcirc;o de alia virino</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="z"/>a deklaracio anta&ubreve;vidas por la balkanaj landoj kunlaboron kun
        la Haga Tribunalo pri militkrimoj kune kun evoluigo de homaj rajtoj kaj
        demokratio
        <fnt>
          <bib>Monato</bib>
          <aut>Zlatko Ti&scaron;ljar</aut>
          <vrk><url ref="&Steloj;monato2/007325.html">Pa&scirc;o al paco</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="z"/>a Universala Kongreso de Esperanto en 2001
        <fnt>
          <bib>Monato</bib>
          <aut>Zlatko Ti&scaron;ljar</aut>
          <vrk><url ref="&Monato;/2004/008453.html">Prezidanto postulas, prezidanto rifuzas</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld lit="z"/>anino
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="&Monato;/2004/007930.html">Interna pejza&gcirc;o de alia virino</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_Z;&c_a;&c_g;&c_r;&c_a;&c_b;</trd>
    <trd lng="de">Zagreb</trd>
    <trd lng="en">Zagreb</trd>
    <trd lng="fr">Zagreb</trd>
  </snc>
  <trd lng="uk">&#x417;&#x430;&#x491;&#x440;&#x435;&#x431; (&#x441;&#x442;&#x43E;&#x43B;&#x438;&#x446;&#x44F;)</trd>
</drv>

  </art>

