<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kapucx.xml,v 1.32 2025/03/05 06:18:50 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kapu&ccirc;</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="kapucx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kapucx.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="vest.kap0o.TEKS">Kapvesto</ref>,
      <ref tip="super" cel="kuf.0o">kufo</ref>, alkudrita al mantelo:
      <ekz>
        apud &scirc;i staris homo, kovrita de granda funebra mantelo, kies <tld/>o estis
        tirita anta&ubreve;en super lia kapo
        <fnt>
          <bib>Fab4</bib>
          <lok>Infano en la tombo</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili forpu&scirc;is de l' viza&gcirc;o la <tld/>ojn
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro 11a</lok>
        </fnt>;
      </ekz>
      <ekz>
        originala mantelo kun punca <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>o mona&hcirc;on ne faras</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>
        <klr>(ne ju&gcirc;u la&ubreve; ekstero)</klr>.
        <trd lng="ca">l'h&agrave;bit no fa el monjo</trd>
        <trd lng="pl">kaptur nie czyni mnicha</trd>
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>capell</trd>
      <trd>caputxa</trd>,
      <trd>caputx&oacute;</trd>
    </trdgrp>
    <trd lng="de">Kapuze</trd>
    <trdgrp lng="fr">
      <trd>capuche</trd>
      <trd>capuchon<klr tip="ind"> (v&ecirc;t.)</klr></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>hood</trd>,
      <trd>cowl</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>csuklya</trd>,
      <trd>kapucni</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>capuchon</trd>,
      <trd>kap</trd>
    </trdgrp>
  </snc>
  <snc mrk="kapucx.0o.ajxo">
    <dif>
      &Scirc;irma <ref tip="super" cel="kovr.0ilo.KOMUNE">kovrilo</ref> etendebla kaj refaldebla.
    </dif>
    <subsnc mrk="kapucx.0o.AUT">
      <uzo tip="fak">AUT</uzo>
      <dif>
        Surmetebla kaj detirebla kovrilo de
        a&ubreve;to, kiu fiksitan tegmenton ne havas:
        <ekz>
          iuj parkis sian riki&scirc;on en ombra loko, starigis
          <tld/>on kaj dormetis sur la sidloko
          <fnt>
            <aut>Lao She, trad. Wang Chongfang</aut>,
            <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0026/">
            Kamelo &Scirc;jangzi</url></vrk>,
            <lok>1988</lok>
          </fnt>.
        </ekz>
        </dif>
      <trd lng="de">Verdeck <klr>(Cabrio)</klr></trd>
      <trdgrp lng="fr">
        <trd>capuche</trd>
        <trd>capuchon<klr tip="ind"> (v&ecirc;t.)</klr></trd>
      </trdgrp>
      <trd lng="nl">kap</trd>
    </subsnc>
    <subsnc mrk="kapucx.0o.ZOO">
      <uzo tip="fak">ZOO</uzo>
      <dif>
        Amba&ubreve;flanka ha&ubreve;ta lobo, kiun iuj serpentoj povas etendi malanta&ubreve; sia kapo:
        <ekz>
          el la herbaro levi&gcirc;is la kapo kaj disvastigita <tld/>o de <nom>Nag</nom>, la granda nigra
          kobro
        <fnt>
          <aut>R. Kipling, trad. Don Harlow</aut>
          <vrk><url ref="http://literaturo.org/HARLOW-Don/Esperanto/Literaturo/Noveloj/Rikki_Tikki_Tavi.html">
          Rikki-Tikki-Tavi</url></vrk>
          <lok>[vidita en 2008]</lok>
        </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="kapucx.0o.kondomo">
      <uzo tip="stl">FIG</uzo>
      <fnt>
      <aut>W. F. Pilger</aut>
      <vrk><url ref="http://pilger.home.xs4all.nl/wfp/slangou8.htm">
      Slango kaj &Jcirc;argono</url></vrk>
      <lok>1996</lok>
      </fnt>
      <ref tip="dif" cel="kondom.0o">kondomo</ref>
      <trd lng="ca">condom</trd>
      <trd lng="nl">condoom<klr tip="ind"> (fam.)</klr></trd>
      <trdgrp lng="fr">
        <trd>capote<klr tip="ind"> (pr&eacute;servatif)</klr></trd>,
        <trd>capote anglaise</trd>
      </trdgrp>
    </subsnc>
  </snc>
  <trd lng="bo">&t_ma;&t_ga;&t_naro;&t_tsheg;&t_sha;&t_shabchu;&t_ba;&t_sa;&t_tsheg;</trd>
  <trd lng="ca">caputxa</trd>
  <trd lng="es">capucha</trd>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_p;&c_ju;&c_sh;&c_o;&c_n;</trd>,
    <trd>&c_k;&c_a;&c_p;&c_t;&c_u;&c_r;</trd>,
    <trd>&c_k;&c_a;&c_w;&c_p;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x305A;&#x304D;&#x3093;</trd>,
    <trd>&#x30D5;&#x30FC;&#x30C9;</trd>,
    <trd>&#x5E4C;  <pr>&#x3068;&#x3070;&#x308A;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kaptur</trd>,
    <trd>basz&lstroke;yk</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>capot&abreve;</trd>,
    <trd>glug&abreve;</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_p;&c_ju;&c_sh;&c_o;&c_n;</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_a;&#x43F;&c_t;&c_u;&c_r;</trd>,
    <trd>&#x43A;&c_a;&#x43F;&#x43E;&c_r;</trd>,
    <trd>&#x43A;&#x43E;&c_v;&#x43F;&c_a;&c_ch;&#x43E;&#x43A;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_h; (&c_je;&#x43A;&c_ib;&#x43F;&c_a;&c_zh;&c_a;</trd>,
    <trd>&c_a;&c_v;&c_t;&#x43E;&#x43C;&c_a;&c_sh;&c_i;&#x43D;&c_i;)</trd>,
    <trd>&#x43A;&c_a;&#x43F;&#x43E;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="kapucx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kapucx.0i.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Kovri per <tld/>o:
      <ekz>
        &leftquot;&ccirc;u ni kuru?&#x201F; &scirc;i proponis, sin
        <tld/>ante
        <fnt><aut>C. Piron</aut>, <vrk>&Ccirc;u li venis
        trakosme?</vrk>, <lok>1980</lok></fnt>;
      </ekz>
      <ekz>
        li alvenis en Egiptio, ili kapu&ccirc;is kaj mankatenis lin kaj kondukis lin al la
        kelo de konstrua&jcirc;o
        <fnt>
          <bib>MD05</bib>
          <vrk>Usono inventas la eksterigon de torturo</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_k;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol; <ind>&c_k;&c_a;&c_p;&c_t;&c_u;&c_r;&c_o;&c_m;</ind></trd>
  <trd lng="de">eine Kapuze <ind>aufsetzen</ind></trd>
  <trd lng="es">encapuchar</trd>
  <trd lng="fr">s'<ind>encapuchonner</ind></trd>
  <trd lng="ja">&#x982D;&#x5DFE;&#x3092;&#x304B;&#x3076;&#x305B;&#x308B;
  <pr>&#x305A;&#x304D;&#x3093;&#x3092;&#x304B;&#x3076;&#x305B;&#x308B;</pr></trd>
  <trd lng="pl">kaptur zak&lstroke;ada&cacute;</trd>
  <trd lng="ro">&#x219;i asume capota</trd>
</drv>

<drv mrk="kapucx.kamen0o">
  <kap>kamen<tld/>o</kap>
  <snc mrk="kapucx.kamen0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Tegmenteto en formo de renversita funelo, masonita
      super la fajrujo en iuj <ref cel="kamen.0o">kamenoj</ref>
      kaj servanta kiel enirejo al la fumtubo:
      <ekz>
        en la antikvaj kaj invitaj hejmoj de la alt-aragonaj Pireneoj lante &gcirc;ojige
        flamis ankora&ubreve;, sub la protekto de la volba kamen<tld/>o, la ega trunko, kiun
        la&ubreve; praaj ritoj oni ekbruligas okaze de la tenere intima festo de la
        Kristnasko
        <fnt>
          <lok><url ref="https://archive.org/stream/Boletin_1973_n197_maj/Boletin_%281973%29_n197_maj_djvu.txt">Bolet&iacute;n
          n197 (maj 1973)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/8/8a/Juelich-Kamin.jpg/338px-Juelich-Kamin.jpg" prm="CC-BY-SA-3.0">kameno
    (supre la tegmentoforma, blanke kalkita la kamen<tld/>o)
        <fnt>
          <aut>Christopher Meyer-Kretschmer, propra verko</aut>
          <lok><url ref="&WCW;/File:Juelich-Kamin.jpg">&WCW;/File:Juelich-Kamin.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_m;&c_a;&c_a;&c_t;&c_v;&c_o;&c_d;&c_n;&c_y; <ind>&c_k;&c_a;&c_w;&c_p;&c_a;&c_k;</ind></trd>
  <trd lng="ca">campana</trd>
  <trd lng="de">Rauchsammler</trd>
  <trd lng="fr">hotte<klr> (de chemin&eacute;e)</klr></trd>
<!-- &scirc;ajnas mal&gcirc;usta traduko, temas pri masona&jcirc;o super fajro, sed *sub* la kamentubo!
  <trdgrp lng="nl">
    <trd>gek<klr tip="ind"> (schoorsteenkap)</klr></trd>,
    <trd>schoorsteenkap</trd>
  </trdgrp>
-->
  <trdgrp lng="ja">
    <trd>&#x30D5;&#x30FC;&#x30C9;</trd>,
    <trd>&#x7159;&#x7A81;&#x53E3;  <pr>&#x3048;&#x3093;&#x3068;&#x3064;&#x3053;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="pl">okap</trd>
  <trd lng="ro">strea&#x219;in&abreve;</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&c_v;&#x43F;&c_a;&#x43A; &#x43D;&c_a;&c_d; &#x43A;&c_a;&#x43C;&c_ib;&#x43D;&#x43E;&#x43C;</trd>,
    <trd>&c_v;&c_i;&c_t;&#x44F;&c_zh;&#x43D;&c_i;&c_j; &#x43A;&#x43E;&c_v;&#x43F;&c_a;&#x43A;</trd>
  </trdgrp>
</drv>
</art>
