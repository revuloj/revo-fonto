<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>kapu&ccirc;</rad>/o</kap>
<drv mrk="kapucx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="vest.kap0o">Kapvesto</ref> alkudrita al
      mantelo:
      <ekz>
        <ind><tld/>o monakon ne faras</ind><fnt>Z</fnt>
        <klr>(ne ju&gcirc;u la&ubreve; ekstero)</klr>.
        <trd lng="ca">l'h&agrave;bit no fa el monjo</trd>
      </ekz>
    </dif>
    <ref tip="vid" cel="vest.0o">vesto</ref>
    <trdgrp lng="ca">
      <trd>caputxa</trd>,
      <trd>caputx&oacute;</trd>
    </trdgrp>
    <trd lng="de">Kapuze</trd>
    <trdgrp lng="en">
      <trd>hood</trd>,
      <trd>cowl</trd>
    </trdgrp>
  <trdgrp lng="fr">
    <trd>capuche</trd>
    <trd>capuchon<klr tip="ind"> (v&ecirc;t.)</klr></trd>
  </trdgrp>
    <trdgrp lng="hu">
      <trd>csuklya</trd>,
      <trd>kapucni</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>capuchon</trd>,
      <trd>kap</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Io simila al <tld/>o.
    </dif>
    <trd lng="ca">capell</trd>
    <trd lng="fr">capuchon<klr tip="ind"> (couvercle)</klr></trd>
    <trd lng="nl">kap</trd>
  </snc>
  <snc mrk="kapucx.0o.kondomo">
    <uzo tip="stl">FIG</uzo>
    <fnt>
    <aut>W.F. Pilger</aut>
    <vrk>Slango kaj &jcirc;argono</vrk>
    <lok><url
    ref="http://www.geocities.com/Paris/8678/slangou8.htm">http://www.geocities.com/Paris/8678/slangou8.htm</url></lok>
    </fnt>
    <ref tip="dif" cel="kondom.0o">kondomo</ref>
    <trd lng="ca">condom</trd>
    <trd lng="nl">condoom<klr tip="ind"> (fam.)</klr></trd>
    <trdgrp lng="fr">
      <trd>capote<klr tip="ind"> (pr&eacute;servatif)</klr></trd>,
      <trd>capote anglaise</trd>,
      <trd>condom</trd>,
      <trd>pr&eacute;servatif</trd>
    </trdgrp>
  </snc>
  <trd lng="ca">caputxa</trd>
  <trd lng="es">capucha</trd>
  <trd lng="be">&c_k;&c_a;&c_p;&c_ju;&c_sh;&c_o;&c_n;</trd>
  <trd lng="ru">&c_k;&c_a;&c_p;&c_ju;&c_sh;&c_o;&c_n;</trd>
</drv>

<drv mrk="kapucx.kamen0o">
  <kap>kamen<tld/>o</kap>
  <snc>
    <dif>
      Tegmenteto en formo de renversita funelo, masonita
      super la fajrujo en iuj <ref cel="kamen.0o">kamenoj</ref>
      kaj servanta kiel enirejo al la fumtubo.
    </dif>
  </snc>
  <trd lng="ca">campana</trd>
  <trd lng="fr">hotte<klr tip="ind"> (de chemin&eacute;e)</klr></trd>
  <trdgrp lng="nl">
    <trd>gek<klr tip="ind"> (schoorsteenkap)</klr></trd>,
    <trd>schoorsteenkap</trd>
  </trdgrp>
</drv>
</art>
