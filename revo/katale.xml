<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: katale.xml,v 1.23 2023/12/03 08:00:24 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>katalepsi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="katale.0o">
  <kap><tld/>o</kap>
  <snc mrk="katale.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Nerva patologia stato, karakterizata per inerteco de la volaj
      muskoloj kaj perdo de sentpovo:
      <ekz>
        la kelnero, starinte kelkajn sekundojn rigide
        kiel viro atakata de <tld/>o, sin turnis
        <fnt>
          <aut>G. K. Chesterton, trad. C. Bean</aut>,
          <vrk>La Naiveco de Pastro Brown</vrk>,
          <lok>1937</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_t;&c_a;&c_l;&c_je;&c_p;&c_s;&c_ib;&c_ja;</trd>,
    <trd>&c_s;&c_l;&c_u;&c_p;&c_n;&c_ja;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_k;&c_a;&c_t;&c_a;&c_l;&c_je;&c_p;&c_s;&c_i;&c_ja;</trd>,
    <trd>&c_n;&c_je;&c_r;&c_v;&c_je;&c_n; &c_p;&c_r;&c_i;&c_p;&c_a;&c_d;&c_malmol;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">catal&egrave;psia</trd>
  <trdgrp lng="cs">
    <trd>katalepsie</trd>,
    <trd>strnulost</trd>,
    <trd>zmrtv&ecaron;lost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Katalepsie</trd>,
    <trd>Starrsucht</trd>
  </trdgrp>
  <trd lng="en">catalepsy</trd>
  <trd lng="es">catalepsia</trd>
  <trd lng="fr">catalepsie</trd>
  <trdgrp lng="hu">
    <trd>katalepszia</trd>,
    <trd>k&oacute;ros <ind>merevs&eacute;g</ind></trd>
  </trdgrp>
  <trd lng="id">katalepsi</trd>
  <trdgrp lng="ja">
    <trd>&#x30AB;&#x30BF;&#x30EC;&#x30D7;&#x30B7;&#x30FC;</trd>,
    <trd>&#x5F37;&#x786C;&#x75C7;  <pr>&#x304D;&#x3087;&#x3046;&#x3053;&#x3046;&#x3057;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>catalepsie</trd>,
    <trd>verstijving</trd>
  </trdgrp>
  <trd lng="pl">katalepsja</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_t;&c_a;&c_l;&c_je;&c_p;&c_s;&c_i;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_l;&c_b;&c_n;&c_ja;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>katalepsia</trd>,
    <trd>stuhnutos&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&#x430;&#x442;&#x430;&#x43B;&#x435;&#x43F;&#x441;&#x456;&#x44F;</trd>
  <trd lng="zh">&#x50F5;&#x4F4F;&#x75C7; <pr>ji&#x101;ngzh&#xF9;zh&#xE8;ng</pr></trd>
</drv>
</art>
