<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>katalepsi</rad>/o
</kap>
<drv mrk="katale.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Nerva patologia stato, karakterizata per inerteco de la volaj
      muskoloj kaj perdo de sentpovo.  
    </dif>
  </snc>
  <trdgrp lng="de">
      <trd>Katalepsie</trd>,
      <trd>Starrsucht</trd>
  </trdgrp>
  <trd lng="en">catalepsy</trd>
  <trdgrp lng="nl">
    <trd>catalepsie</trd>,
    <trd>verstijving</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_t;&c_a;&c_l;&c_je;&c_p;&c_s;&c_i;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_l;&c_b;&c_n;&c_ja;&c_k;</trd>
  </trdgrp>
</drv>
</art>
