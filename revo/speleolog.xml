<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: speleolog.xml,v 1.10 2024/03/16 17:25:39 revo Exp $">
<kap>
  <rad>speleolog</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="speleolog.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SCI</uzo>
  <snc mrk="speleolog.0o.speleologiisto">
    <dif>
      <ke>Kavernesploristo</ke>, <ref tip="dif" cel="speleologi.0isto.SCI">speleologiisto</ref>:
      <ekz>
        dum lastaj jaroj litovaj <tld/>oj komencis &scirc;an&gcirc;adon de
        soveta du&scirc;nura tekniko al unu&scirc;nura tekniko
        <fnt>
          <aut>E. E. Laiconas</aut>,
          <vrk><url ref="http://www.laiconas.lt/E.Laiconas.KELIONIU_ATSPINDZIAI.2005.pdf">
          10 jaroj de Litova speleologio</url></vrk>,
          <lok>Vesperto, 1988:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        alveninte al Palermo, oni havigu al si <klr>[&#8230;]</klr> gumajn botojn, kaskon
        <tld/>an, tor&ccirc;on, gantojn
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011265.php"
              >En Palermo subtere</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_e;&c_l;&c_je;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="de">Spel&auml;ologe</trd>
  <trd lng="en">speleologist</trd>
  <trd lng="fr">sp&eacute;l&eacute;ologue</trd>
  <trdgrp lng="nl">
    <trd>speleoloog</trd>,
    <trd>holenkundige</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>speleolog</trd>,
    <trd>groto&lstroke;az</trd>
  </trdgrp>
  <trd lng="sk">speleol&oacute;g</trd>
  <trdgrp lng="cs">
    <trd>jesky&ncaron;&aacute;&rcaron;</trd>,
    <trd>speleolog</trd>
  </trdgrp>
<trdgrp lng="de">
  <trd>Spel&auml;ologe</trd>,
  <trd>H&ouml;hlenforscher</trd>
</trdgrp>
</drv>
</art>

