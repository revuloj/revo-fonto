    <trd>t&eacute;cnica</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_h;&c_n;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">technika</trd>
  <trd lng="sv">teknik</trd>
  <trd lng="uk">&c_t;&c_je;&c_h;&#x43D;&c_ib;&#x43A;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#25216;&#x6CD5; <pr>j&#xEC;f&#x1CE;</pr></trd>,
    <trd>&#25216;&#x672F; <pr>j&#xEC;sh&#xF9;</pr></trd>,
    <trd>&#25216;&#34899; <pr>j&#xEC;sh&#xF9;</pr></trd>,
    <trd>&#x672F; <pr>sh&#xF9;</pr></trd>,
    <trd>&#34899; <pr>sh&#xF9;</pr></trd>,
    <trd>&#25216;&#x672F;&#x89C4;&#x683C; <pr>j&#xEC;sh&#xF9;gu&#x12B;g&#xE9;</pr></trd>,
