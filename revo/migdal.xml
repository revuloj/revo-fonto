<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>migdal</rad>/o
</kap>
<drv mrk="migdal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Man&gcirc;ebla nukso de 
      <ref tip="malprt" cel="migdal.0arbo"><tld/>arbo</ref>.
    </dif>
  </snc>
  <trd lng="de">Mandel</trd>
  <trd lng="fr">amande</trd>
</drv>
<drv mrk="migdal.0arbo">
  <kap><tld/>arbo, <tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="prunus.0o">prunuso</ref>
      (<trd lng="la">Prunus dulcis</trd>).
    </dif>
  </snc>
  <trd lng="de">Mandelbaum</trd>
  <trd lng="fr">amandier</trd>
</drv>
</art>
