<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pojn.xml,v 1.22 2024/02/17 07:52:34 revo Exp $">
<kap>
<rad>pojn</rad>/o
</kap>
<drv mrk="pojn.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc mrk="pojn.0o.manartiko">
    <dif>
      <ref tip="dif" cel="man.0artiko">Manartiko</ref>:
      <ekz>
         sur la maldekstra <tld/>o videblis multekosta brakhorlo&gcirc;o
         <fnt><aut>Julian Modest</aut><vrk>La glita kaj dan&gcirc;era vojo</vrk>
         <bib>LOdE</bib><lok>jaro 2002a, numero 3a</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>rimeno, fiksita je bastono, estu elasta
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2005/008790.php">Praktiki bastonmar&scirc;adon</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne devas rigardi al mia <tld/>o por konstati, ke mi ne portas mian
        brakhorlo&gcirc;on
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2012/010654.php">Horobato, korobato</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>oj, almena&ubreve; tiuj al mi videblaj, plejparte nudas, sentiktakilas
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2012/010621.php">Montriloj de la tempoj</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
   </dif>
   <trd lng="be">&c_z;&c_a;&c_p;&c_ja;&c_s;&c_mol;&c_c;&c_je;</trd>
   <trd lng="bg">&c_k;&c_i;&c_t;&c_k;&c_a;<klr tip="amb"> (&c_n;&c_a; &c_r;&c_malmol;&c_k;&c_a;)</klr></trd>
   <trd lng="ca">canell</trd>
   <trd lng="en">wrist</trd>
   <trd lng="es">mu&ntilde;eca</trd>
   <trd lng="fr">poignet</trd>
   <trd lng="id">per<ind>gelang</ind>an tangan</trd>
   <trd lng="it">polso</trd>
   <trd lng="nl">pols</trd>
   <trd lng="pl">przegub r&eogonek;ki</trd>
   <trd lng="pt">pulso<klr tip="amb"> (anatomia humana)</klr></trd>
  </snc>
  <trd lng="cs">kloub z&aacute;p&ecaron;st&iacute;</trd>
  <trd lng="de">Handgelenk</trd>
  <trd lng="ja">&#x624B;&#x9996;  <pr>&#x3066;&#x304F;&#x3073;</pr></trd>
  <trd lng="sk">k&lacute;b z&aacute;p&auml;stia</trd>
  <trd lng="uk">&c_z;&c_a;&#x43F;&#x2019;&#x44F;&c_s;&c_t;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x624B;&#x8155;&#x5B50; <pr>sh&#x1D2;uw&#xE0;nzi</pr></trd>,
    <trd>&#x8155; <pr>w&#xE0;n</pr></trd>,
    <trd>&#x8155;&#x5173;&#x8282; <pr>w&#xE0;ngu&#x101;nji&#xE9;</pr></trd>,
    <trd>&#x8155;&#x95DC;&#x7BC0; <pr>w&#xE0;ngu&#x101;nji&#xE9;</pr></trd>
  </trdgrp>
</drv>
</art>

