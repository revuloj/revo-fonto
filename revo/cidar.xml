<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cidar.xml,v 1.16 2025/10/27 20:30:18 revo Exp $">
<kap>
  <rad>cidar</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="cidar.0o">
  <kap><tld/>o</kap>
  <snc mrk="cidar.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Alta okcidentazia <ref tip="super" cel="vest.kap0o">kapvesto</ref>.
    </dif>
    <subsnc mrk="cidar.0o.JUD">
      <dif>
        <ref tip="super" cel="mitr.0o">Mitro</ref> de la juda &ccirc;efpastro:
        <ekz>
          metu la <tld/>on sur lian kapon,
          kaj metu la sanktan kronon sur la <tld/>on
          <fnt><bib>MT</bib>, <lok>&Eli; 29:6</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="nl">joodse <ind>mijter</ind></trd>
      <trd lng="pl">mitra</trd>
    </subsnc>
    <subsnc mrk="cidar.0o.HIS">
      <dif>
        <ref tip="super" cel="turban.0o">Turbano</ref> de la persaj re&gcirc;oj:
        <ekz>
          hetajroj ekvidis sian gvidanton sidi sur
          trono el oro, en longa persa vesto, kun
          alta <tld/>o sur la kapo
          <fnt>
            <aut>I. Efremov, trad. J. Finkel</aut>,
            <vrk><url ref="http://esperanto-mv.pp.ru/Efremov/tais_12.html">
            Tais el Ateno</url></vrk>,
            <lok>2008</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="nl">kidaris</trd>
      <trd lng="pl">turban</trd>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="tiar.0o">tiaro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_y;&c_ja;&c_r;&c_a;</trd>
  <trd lng="de">Kidaris</trd>
  <trd lng="fr">kidaris</trd>
  <trd lng="he">&mem;&tsadi;&nun;&pe;&tav; &kaf;&vav;&nun;&fnun;</trd>
  <trd lng="hu">tiara<klr tip="ind"> (perzsa, zsid&oacute;)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x30AD;&#x30C0;&#x30EA;&#x30B9;</trd>,
    <trd>&#x5927;&#x796D;&#x53F8;&#x5E3D;  <pr>&#x3060;&#x3044;&#x3055;&#x3044;&#x3057;&#x307C;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_i;&c_a;&c_r;&c_a;</trd>
  <trd lng="uk">&c_k;&c_i;&c_d;&c_a;&c_r;</trd>
</drv>
</art>
