<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: kuritib.xml,v 1.4 2023/12/03 09:59:04 afido Exp $">
    <kap>
      <rad>Kuritib</rad>/o
    </kap>

<drv mrk="kuritib.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Granda <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      en la sudo de <ref tip="malprt" cel="brazil.B0o">Brazilo</ref>,
      &ccirc;. 400&#x202F;km sudokcidente de <ref cel="sanpauxl.0o">San-Pa&ubreve;lo</ref>:
      <ekz>
        <tld/>o estis fondita en 1693
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Kuritibo">Kuritibo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        2018 okazis en <tld/>o la 38-a Brazila Esperantista Junulara Kongreso
        <fnt>
          <vrk><url ref="https://bejo.esperanto.org.br/bejk-2018-kuritibo-neforgeseblos/">BEJK 2018: Kuritibo neforgeseblos! - Esperanto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        nia hodia&ubreve;a felietono referencas al la 95-jari&gcirc;o de la monumento de Pola
        Grensemanto en <tld/>o
        <fnt>
          <vrk><url ref="http://esperanto.brazilo.org/archives/12316">E-elsendo el la 21.02.2020 - Esperanto en Brazilo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_K;&c_u;&c_r;&c_y;&c_t;&c_y;&c_b;&c_a;</trd>
    <trd lng="de">Curitiba</trd>
    <trd lng="en">Curitiba</trd>
    <trd lng="fr">Curitiba</trd>
  </snc>
  <trdgrp lng="zh">
    <trd>&#x5E93;&#x91CC;&#x63D0;&#x5DF4; <pr>K&#xF9;l&#x1D0;t&#xED;b&#x101;</pr></trd>,
    <trd>&#x5EAB;&#x91CC;&#x63D0;&#x5DF4; <pr>K&#xF9;l&#x1D0;t&#xED;b&#x101;</pr></trd>
  </trdgrp>
</drv>

  </art>

