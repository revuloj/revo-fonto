<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>beril</rad>/o
</kap>
<drv mrk="beril.0o">
  <kap><tld/>o</kap>
  <snc mrk="beril.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Mineralo. Kiel juvel&scirc;tono, speco de smeraldo kun koloro de
marakvo, konsistanta
      el natura <ref tip="super" cel="silici.0ato">siliciato</ref> de 
      <ref tip="super" cel="alumin1.0o">aluminio</ref> kaj 
      <ref tip="super" cel="berili.0o">berilio</ref> 
      <frm>(Be<sub>3</sub>Al<sub>2</sub>Si<sub>6</sub>O<sub>18</sub>)</frm>.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_e;&c_r;&c_y;&c_l;</trd>
  <trd lng="ca">beril</trd>
  <trd lng="en">beryl</trd>
  <trd lng="es">berilo</trd>
  <trd lng="fr">b&eacute;ryl</trd>
  <trd lng="hu">berill</trd>
  <trd lng="pl">beryl</trd>
  <trd lng="ru">&c_b;&c_je;&c_r;&c_i;&c_l;&c_l;</trd>
</drv>
</art>
