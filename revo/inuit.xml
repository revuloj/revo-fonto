<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>inuit</rad>/o
  <fnt><bib>SPIV</bib></fnt>
</kap>

<drv mrk="inuit.0o">
  <kap><tld/>o</kap>
  <snc>
    <ref tip="dif" cel="eskim.0o">eskimo</ref>
  </snc>
  <trd lng="be">&c_e;&c_s;&c_k;&c_ib;&c_m;&c_o;&c_s;</trd>
  <trd lng="cs">Eskym&aacute;k</trd>
  <trdgrp lng="de">
    <trd>Inuit</trd>,
    <trd>Eskimo</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Inuit</trd>,
    <trd>Eskimo</trd>
  </trdgrp>
  <trd lng="es">esquimal<klr> (sust.)</klr></trd>
  <trdgrp lng="fr">
    <trd>Esquimau<klr tip="ind"> (subst.)</klr></trd>,
    <trd>Eskimo<klr tip="ind"> (subst.)</klr></trd>,
    <trd>Inuit<klr tip="ind"> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="hu">inuit</trd>
  <trd lng="nl">Inuit</trd>
  <trd lng="pl">Inuita</trd>
  <trd lng="pt">inu&iacute;te</trd>
  <trd lng="ru">&c_e;&c_s;&c_k;&c_i;&c_m;&c_o;&c_s;</trd>
  <trd lng="sk">Eskim&aacute;k</trd>
</drv>

<drv mrk="inuit.0a">
  <kap><tld/>a</kap>
  <snc>
    <ref tip="dif" cel="eskim.0a">eskima</ref>
    <ekz>
      la <tld/>a kaj la kria estas lingvoj de nordaj popoloj
      <fnt>
      <aut>Garbhan MacAoidh</aut>,
      <vrk>Korekto de verkinto</vrk>,
      <bib>Monato</bib>,
      <lok>jaro 2002a, numero 7a, p. 7a</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_e;&c_s;&c_k;&c_ib;&c_m;&c_o;&c_s;&c_k;&c_ib;</trd>
  <trd lng="de">eskimoisch</trd>
  <trdgrp lng="en">
    <trd>Inuit</trd>,
    <trd>Eskimo</trd>
  </trdgrp>
  <trd lng="es">esquimal<klr> (adj.)</klr></trd>
  <trdgrp lng="fr">
    <trd>esquimau<klr tip="ind"> (adj.)</klr></trd>,
    <trd>eskimo<klr tip="ind"> (adj.)</klr></trd>,
    <trd>inuit<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="hu">inuit</trd>
  <trd lng="nl">inuit</trd>
  <trd lng="pl">inuicki</trd>
  <trd
  lng="ru">&c_e;&c_s;&c_k;&c_i;&c_m;&c_o;&c_s;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="inuit.0aj.lingvoj">
  <kap><tld/>aj lingvoj</kap>
  <snc mrk="inuit.0aj.lingvoj.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      Grupo da parencaj eskimaj lingvoj el la eskim-aleuta lingvaro: la
      inuktituta, la gronlanda, la inupiaka kaj aliaj.
    </dif>
  </snc>
  <trd lng="en">Inuit languages</trd>
  <trd lng="fr">langues inuites</trd>
  <trd lng="hu">eszkim&oacute; nyelvek</trd>
  <trd lng="id"><ind>bahasa</ind> Inuit</trd>
  <trd lng="nl">Inuittalen</trd>
</drv>
</art>
