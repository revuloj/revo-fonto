<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>inuit</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="inuit.0o">
  <kap><tld/>o</kap>
  <snc>
    <ref tip="dif" cel="eskim.0o">eskimo</ref>
  </snc>
  <trdgrp lng="fr">
    <trd>Esquimau<klr> (subst.)</klr></trd>,
    <trd>Eskimo<klr> (subst.)</klr></trd>,
    <trd>Inuit<klr> (subst.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="inuit.0a">
  <kap><tld/>a</kap>
  <snc>
    <ref tip="dif" cel="eskim.0a">eskima</ref>
    <ekz>
      la <tld/>a kaj la kria estas lingvoj de nordaj popoloj
      <fnt><aut>Garbhan MacAoidh</aut>: <vrk>Korekto de verkinto</vrk>,
      <bib>Monato</bib><lok>jaro 2002a, numero 7a, p. 7a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="fr">
    <trd>esquimau<klr> (adj.)</klr></trd>,
    <trd>eskimo<klr> (adj.)</klr></trd>,
    <trd>inuit<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
