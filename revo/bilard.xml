<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>bilard</rad>/o
</kap>
<drv mrk="bilard.0o">
<kap><tld/>o</kap>
<snc num="1">
<dif>
<ref tip="super" cel="lud.0o">Ludo</ref>, ludata per eburaj globoj 
sur tablo tegita per drapo. 
</dif>
    <trd lng="sv">biljard</trd>
    <trd lng="de">Billard</trd>
    <trdgrp lng="en">
        <trd>pool</trd>,
        <trd>billiards</trd>
    </trdgrp>
    <trd lng="es">billar</trd>
    <trd lng="hu">bili&aacute;rd</trd>
    <trd lng="sk">biliard</trd>
</snc>
<snc num="2">
<dif>
Tiu tablo mem.  
</dif>
    <trd lng="de">Billardtisch</trd>
    <trdgrp lng="en">
        <trd>pool table</trd>,
        <trd>billiards table</trd>
    </trdgrp>
    <trd lng="es">mesa de <ind>billar</ind></trd>
    <trd lng="hu">bili&aacute;rdasztal</trd>
    <trd lng="sv">biljardbord</trd>
    <trd lng="sk">biliardov&yacute; st&ocirc;l</trd>
</snc>
<trd lng="fr">billard</trd>
<trd lng="ru">&c_b;&c_i;&c_l;&c_mol;&c_ja;&c_r;&c_d;</trd>
</drv>
    
    <drv mrk="bilard.0ejo">
      <kap><tld/>ejo</kap>
      <snc>
        <dif>
          &Ccirc;ambro en domo a&ubreve; kafejo, destinita por tiu ludo.
        </dif>
      </snc>
      <trd lng="de">Billardsaal</trd>
      <trdgrp lng="en">
        <trd>pool hall</trd>,
        <trd>billiards hall</trd>
      </trdgrp>
      <trd lng="es">billar</trd>
      <trd lng="fr">salle de <ind>billard</ind></trd>
      <trd lng="hu">bili&aacute;rdterem</trd>
    </drv>
</art>
    
