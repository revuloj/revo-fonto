<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bilard.xml,v 1.25 2023/11/25 09:24:12 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>bilard</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="bilard.0o">
  <kap><tld/>o</kap>
  <snc mrk="bilard.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="super" cel="lud.0o">Ludo</ref>, ludata per eburaj globoj
      sur tablo tegita per drapo:
      <ekz>
        li estis sportulo, lerte ludis piedpilkon, kriketon, <tld/>on kaj golfon
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 3</lok>
        </fnt>;
      </ekz>
      <ekz>
        sub lar&gcirc;a &scirc;edo tondras lignaj globoj de <tld/>oj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi pli amas ludi <tld/>on ol viziti muzeojn
        <fnt>
          <bib>TAA</bib>
          <lok>&Ccirc;apitro VIII.</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_ib;&c_l;&c_mol;&c_ja;&c_r;&c_d;</trd>
    <trd lng="bg">&c_b;&c_i;&c_l;&c_ja;&c_r;&c_d;</trd>
    <trd lng="sv">biljard</trd>
    <trd lng="de">Billard</trd>
    <trdgrp lng="en">
      <trd>pool</trd>,
      <trd>billiards</trd>
    </trdgrp>
    <trd lng="es">billar</trd>
    <trd lng="hu">bili&aacute;rd</trd>
    <trd lng="pl">bilard</trd>
    <trd lng="sk">biliard</trd>
    <trdgrp lng="zh">
    <trd>&#x53F0;&#29699; <pr>t&#xE1;iqi&#xFA;</pr></trd>,
    <trd>&#x53F0;&#29699;&#x6E38;&#x620F; <pr>t&#xE1;iqi&#xFA;y&#xF3;ux&#xEC;</pr></trd>,
    <trd>&#x649E;&#29699; <pr>zhu&#xE0;ngqi&#xFA;</pr></trd>,
    <trd>&#x5F39;&#29699; <pr>d&#xE0;nqi&#xFA;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="bilard.0o.bilardotablo">
    <dif>
      Tiu tablo mem.
    </dif>
    <trd lng="be">&c_b;&c_ib;&c_l;&c_mol;&c_ja;&c_r;&c_d;&c_a;&c_v;&c_y;
    &c_s;&c_t;&c_o;&c_l;</trd>
    <trd lng="de">Billardtisch</trd>
    <trdgrp lng="en">
      <trd>pool table</trd>,
      <trd>billiards table</trd>
    </trdgrp>
    <trd lng="es">mesa de <ind>billar</ind></trd>
    <trd lng="hu">bili&aacute;rdasztal</trd>
    <trd lng="pl">st&oacute;&lstroke; <ind>bilardowy</ind></trd>
    <trd lng="ru">&c_b;&c_i;&c_l;&c_mol;&c_ja;&c_r;&c_d;&c_n;&c_y;&c_j;
    &c_s;&c_t;&c_o;&c_l;</trd>
    <trd lng="sv">biljardbord</trd>
    <trd lng="sk">biliardov&yacute; st&ocirc;l</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>bili&aacute;r</trd>,
    <trd>kule&ccaron;n&iacute;k</trd>
  </trdgrp>
  <trd lng="fr">billard</trd>
  <trd lng="he">&bet;&#x5BC;&#x5B4;&yod;&lamed;&#x5B0;&yod;&#x5B7;&alef;&resh;&dalet;</trd>
  <trdgrp lng="ja">
    <trd>&#x30D3;&#x30EA;&#x30E4;&#x30FC;&#x30C9;</trd>,
    <trd>&#29577;&#x7A81;&#x304D;  <pr>&#x305F;&#x307E;&#12388;&#x304D;</pr></trd>,
    <trd>&#x30D3;&#x30EA;&#x30E4;&#x30FC;&#x30C9;&#x53F0; 
    <pr>&#x30D3;&#x30EA;&#x30E4;&#x30FC;&#x30C9;&#12384;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_i;&c_l;&c_mol;&c_ja;&c_r;&c_d;</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_ib;&c_l;&c_mol;&c_ja;&c_r;&c_d;</trd>,
    <trd>&c_b;&c_ib;&c_l;&c_mol;&c_ja;&c_r;&c_d;&c_n;&c_i;&c_j; &c_s;&c_t;&c_ib;&c_l;</trd>
  </trdgrp>
</drv>

<drv mrk="bilard.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="bilard.0ejo.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      &Ccirc;ambro en domo a&ubreve; kafejo, destinita por tiu ludo:
      <ekz>
        li posedis la <nom>Yellowstone</nom>-<tld/>ejojn kaj
        enspezis multe da mono je la boksbataloj
        <fnt>
          <aut>J. London, trad. D. Kaiser, F Vohla, H.G. Kaiser</aut>,
          <vrk>La meksikano</vrk>,
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="be">&c_b;&c_ib;&c_l;&c_mol;&c_ja;&c_r;&c_d;&c_a;&c_v;&c_y; &c_p;&c_a;&c_k;&c_o;&c_j;</trd>
  <trd lng="de">Billardsaal</trd>
  <trdgrp lng="en">
    <trd>pool hall</trd>,
    <trd>billiards hall</trd>
  </trdgrp>
  <trd lng="es">billar</trd>
  <trd lng="fr">salle de <ind>billard</ind></trd>
  <trd lng="hu">bili&aacute;rdterem</trd>
  <trd lng="pl">sala <ind>bilardowa</ind></trd>
  <trd lng="ru">&c_b;&c_i;&c_l;&c_mol;&c_ja;&c_r;&c_d;&c_n;&c_a;&c_ja;<klr>
  (&c_k;&c_o;&c_m;&c_n;&c_a;&c_t;&c_a;)</klr></trd>
  </snc>
  <trd lng="cs">kule&ccaron;n&iacute;kov&yacute; s&aacute;l</trd>
  <trd lng="he">&het;&dalet;&resh; &bet;&yod;&lamed;&yod;&alef;&resh;&dalet;</trd>
  <trd lng="ja">&#x30D3;&#x30EA;&#x30E4;&#x30FC;&#x30C9;&#22580; 
  <pr>&#x30D3;&#x30EA;&#x30E4;&#x30FC;&#x30C9;&#12376;&#12423;&#12358;</pr></trd>
  <trd lng="sk">biliardov&aacute; s&aacute;la</trd>
  <trd lng="uk">&c_b;&c_ib;&c_l;&c_mol;&c_ja;&c_r;&c_d;&c_n;&c_a;&nbsp;(&c_ib;&c_m;.)</trd>
</drv>
</art>
