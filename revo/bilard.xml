<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bilard.xml,v 1.19 2021/03/04 20:39:52 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>bilard</rad>/o <fnt><bib>PV</bib></fnt> 
</kap>
<drv mrk="bilard.0o">
  <kap><tld/>o</kap>
  <snc mrk="bilard.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="super" cel="lud.0o">Ludo</ref>, ludata per eburaj globoj
      sur tablo tegita per drapo.
    </dif>
    <trd lng="be">&c_b;&c_ib;&c_l;&c_mol;&c_ja;&c_r;&c_d;</trd>
    <trd lng="bg">&c_b;&c_i;&c_l;&c_ja;&c_r;&c_d;</trd>
    <trd lng="sv">biljard</trd>
    <trd lng="de">Billard</trd>
    <trdgrp lng="en">
      <trd>pool</trd>,
      <trd>billiards</trd>
    </trdgrp>
    <trd lng="es">billar</trd>
    <trd lng="hu">bili&aacute;rd</trd>
    <trd lng="pl">bilard</trd>
    <trd lng="sk">biliard</trd>
  </snc>
  <snc mrk="bilard.0o.bilardotablo">
    <dif>
      Tiu tablo mem.
    </dif>
    <trd lng="be">&c_b;&c_ib;&c_l;&c_mol;&c_ja;&c_r;&c_d;&c_a;&c_v;&c_y;
    &c_s;&c_t;&c_o;&c_l;</trd>
    <trd lng="de">Billardtisch</trd>
    <trdgrp lng="en">
      <trd>pool table</trd>,
      <trd>billiards table</trd>
    </trdgrp>
    <trd lng="es">mesa de <ind>billar</ind></trd>
    <trd lng="hu">bili&aacute;rdasztal</trd>
    <trd lng="pl">st&oacute;&lstroke; <ind>bilardowy</ind></trd>
    <trd lng="ru">&c_b;&c_i;&c_l;&c_mol;&c_ja;&c_r;&c_d;&c_n;&c_y;&c_j;
    &c_s;&c_t;&c_o;&c_l;</trd>
    <trd lng="sv">biljardbord</trd>
    <trd lng="sk">biliardov&yacute; st&ocirc;l</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>bili&#xE1;r</trd>,
    <trd>kule&#x10D;n&#xED;k</trd>
  </trdgrp>
  <trd lng="fr">billard</trd>
  <trd lng="he">&#x5D1;&#x5BC;&#x5B4;&#x5D9;&#x5DC;&#x5B0;&#x5D9;&#x5B7;&#x5D0;&#x5E8;&#x5D3;</trd>
  <trd lng="ru">&c_b;&c_i;&c_l;&c_mol;&c_ja;&c_r;&c_d;</trd>
</drv>

<drv mrk="bilard.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="bilard.0ejo.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      &Ccirc;ambro en domo a&ubreve; kafejo, destinita por tiu ludo:
      <ekz>
        li posedis la Yellowstone-<tld/>ejojn kaj
        enspezis multe da mono je la boksbataloj
        <fnt>
          <aut>J. London, trad. D. Kaiser, F Vohla, H.G. Kaiser</aut>,
          <vrk>La meksikano</vrk>,
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="be">&c_b;&c_ib;&c_l;&c_mol;&c_ja;&c_r;&c_d;&c_a;&c_v;&c_y; &c_p;&c_a;&c_k;&c_o;&c_j;</trd>
  <trd lng="de">Billardsaal</trd>
  <trdgrp lng="en">
    <trd>pool hall</trd>,
    <trd>billiards hall</trd>
  </trdgrp>
  <trd lng="es">billar</trd>
  <trd lng="fr">salle de <ind>billard</ind></trd>
  <trd lng="hu">bili&aacute;rdterem</trd>
  <trd lng="pl">sala <ind>bilardowa</ind></trd>
  <trd lng="ru">&c_b;&c_i;&c_l;&c_mol;&c_ja;&c_r;&c_d;&c_n;&c_a;&c_ja;<klr>
  (&c_k;&c_o;&c_m;&c_n;&c_a;&c_t;&c_a;)</klr></trd>
  </snc>
  <trd lng="cs">kule&#x10D;n&#xED;kov&#xFD; s&#xE1;l</trd>
  <trd lng="he">&#x5D7;&#x5D3;&#x5E8; &#x5D1;&#x5D9;&#x5DC;&#x5D9;&#x5D0;&#x5E8;&#x5D3;</trd>
  <trd lng="sk">biliardov&#xE1; s&#xE1;la</trd>
</drv>
</art>
