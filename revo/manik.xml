<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>manik</rad>/o
</kap>
<drv mrk="manik.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Parto de <ref tip="malprt" cel="vest.0o">vesto</ref>, kiu kovras
      brakon:
        <ekz>
	ne butonumitaj, la manikoj nature falis
        <fnt><aut>Machado de Assis, trad. Paulo Viana</aut>
        <vrk>La divenistino kaj aliaj rakontoj</vrk><lok>Oportuno, jaro 2005a, p.
        32a</lok></fnt>.
	</ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="manik1.0o">Maniko</ref>
  <trd lng="be">&c_r;&c_u;&c_k;&c_a;&c_w;</trd>
  <trd lng="ca">m&agrave;niga</trd>
  <trd lng="br">milgin</trd>
  <trd lng="de">&Auml;rmel</trd>
  <trd lng="en">sleeve</trd>
  <trd lng="es">manga</trd>
  <trdgrp lng="fr">
    <trd>manche<klr tip="ind"> (de v&ecirc;tement)</klr></trd>
  </trdgrp>
  <trd lng="hu">ruhaujj</trd>
  <trd lng="nl">mouw</trd>
  <trd lng="pt">manga<klr tip="amb"> (de roupa)</klr></trd>
  <trd lng="ru">&c_r;&c_u;&c_k;&c_a;&c_v;</trd>
  <trd lng="sv">&auml;rm</trd>
</drv>
</art>
