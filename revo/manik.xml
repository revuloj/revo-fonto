<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: manik.xml,v 1.40 2024/03/15 10:27:02 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>manik</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="manik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="manik.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Parto de <ref tip="malprt" cel="vest.0o">vesto</ref>,
      kiu kovras brakon:
      <ekz>
        al lia ma&scirc;kiraso mankis unu <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        tre embarasita, li tiretis siajn mallongajn <tld/>ojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Sonorilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj <tld/>oj estis falditaj &gcirc;is la kubutoj
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i portis &ccirc;iam mallongajn <tld/>ojn kaj havis &ccirc;iam kontuzitajn kubutojn
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj manoj estis ka&scirc;itaj en la <tld/>oj de la supervesto
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne butonumitaj, la <tld/>oj nature falis
        <fnt>
          <aut>M. de Assis, trad. P. Viana</aut>,
          <vrk>La divenistino kaj aliaj rakontoj</vrk>,
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="man.0umo">manumo</ref>
    <ref tip="hom" cel="manik1.0o">Maniko</ref>
  </snc>
  <trd lng="be">&c_r;&c_u;&c_k;&c_a;&c_w;</trd>
  <trd lng="bg">&c_r;&c_malmol;&c_k;&c_a;&c_v;</trd>
  <trd lng="bo">&t_pha;&t_shabchu;&t_tsheg;&t_da;&t_shabchu;&t_nga;&t_tsheg;</trd>
  <trd lng="br">milgin</trd>
  <trd lng="ca">m&agrave;niga</trd>
  <trd lng="cs">ruk&aacute;v</trd>
  <trd lng="de">&Auml;rmel</trd>
  <trd lng="en">sleeve</trd>
  <trd lng="es">manga</trd>
  <trd lng="fr">manche<klr tip="ind"> (de v&ecirc;tement)</klr></trd>
  <trd lng="hu">ruhaujj</trd>
  <trd lng="id"><ind>lengan</ind> baju</trd>
  <trdgrp lng="ja">
    <trd>&#x305D;&#12391;</trd>,
    <trd>&#x305F;&#12418;&#12392;</trd>,
    <trd>&#x98A8;&#x898B;&#29992;&#21561;&#x6D41;&#12375;  <pr>&#x304B;&#12374;&#x307F;&#12424;&#12358;&#12405;&#x304D;&#x306A;&#x304C;&#12375;</pr></trd>
  </trdgrp>
  <trd lng="nl">mouw</trd>
  <trd lng="pl">r&eogonek;kaw</trd>
  <trd lng="pt">manga<klr tip="amb"> (de roupa)</klr></trd>
  <trd lng="ro">m&acirc;nec&abreve;</trd>
  <trd lng="ru">&c_r;&c_u;&c_k;&c_a;&c_v;</trd>
  <trd lng="sk">ruk&aacute;v</trd>
  <trd lng="sv">&auml;rm</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_u;&c_k;&c_a;&c_v; (&c_p;&c_l;&c_a;&c_t;&c_t;&c_ja;</trd>,
    <trd>&c_s;&c_o;&c_r;&c_o;&c_ch;&c_k;&c_i;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#34966;&#x5B50; <pr>xi&#xF9;zi</pr></trd>,
    <trd>&#34966; <pr>xi&#xF9;</pr></trd>,
    <trd>&#34915;&#34966; <pr>y&#x12B;xi&#xF9;</pr></trd>,
    <trd>&#34966;&#x7B52; <pr>xi&#xF9;t&#x1D2;ng</pr></trd>,
    <trd>&#x897C; <pr>y&#xEC;</pr></trd>,
    <trd>&#34946; <pr>m&#xE8;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="manik.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="manik.sen0a.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Kiu malhavas <tld/>ojn, t.e. kovras nur la torson, ne la
      brakojn:
      <ekz>
        knabinoj en bluaj sen<tld/>aj bluzetoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        skarlata sen<tld/>a tuniko
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        flordesegna, sen<tld/>a somerrobo
        <fnt>
          <aut>I. Kert&eacute;sz, trad. I. Ertl</aut>,
          <vrk><url ref="&LOdE;Tl-ukr01.htm">
          Sensorteco</url></vrk>,
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_je;&c_z;&c_r;&c_u;&c_k;&c_a;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_r;&c_u;&c_k;&c_a;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="de">&auml;rmellos</trd>
  <trd lng="fr">sans manche</trd>
  <trdgrp lng="ja">
    <trd>&#x305D;&#12391;&#x306A;&#12375;&#x306E;</trd>,
    <trd>&#x30CE;&#x30FC;&#x30B9;&#x30EA;&#x30FC;&#x30D6;&#x306E;</trd>
  </trdgrp>
  <trd lng="pl">bez <ind>r&eogonek;kaw&oacute;w</ind></trd>
  <trd lng="ro">f&abreve;r&abreve; <ind>m&acirc;neci</ind></trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_je;&c_z;&c_r;&c_u;&c_k;&c_a;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_r;&c_u;&c_k;&c_a;&c_v;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="manik.vent0o">
  <kap>vent<tld/>o,
  <var><kap>vento<tld/>o</kap></var></kap>
  <snc mrk="manik.vent0o.MET">
    <uzo tip="fak">AVI</uzo>
    <uzo tip="fak">MET</uzo>
    <dif>
      <ref tip="super" cel="montr.vent0ilo">Ventmontrilo</ref>
      en formo de teksa&jcirc;a maniko fiksita &ccirc;e turnebla ringo sur masto:
      <ekz>
        kelkcent futojn sube etendis sin pejza&gcirc;o konsistanta el
        arbaretoj, tri-kvar malgrandaj lagetoj kaj, preter tiuj, plata
        kampego kun kabanoj, hangaroj, vento<tld/>o
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_je;&c_t;&c_r;&c_a;&c_p;&c_a;&c_k;&c_a;&c_z;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trd lng="de">Windsack</trd>
  <trd lng="fr">manche &agrave; air</trd>
  <trd lng="ja">&#x98A8;&#x898B;&#29992;&#21561;&#x6D41;&#12375; 
  <pr>&#x304B;&#12374;&#x307F;&#12424;&#12358;&#12405;&#x304D;&#x306A;&#x304C;&#12375;</pr></trd>
  <trd lng="pl">wiatr&oacute;wka</trd>
  <trd lng="ro">arm&abreve; cu aer comprimat</trd>
</drv>

<drv mrk="manik.ella0o">
  <kap>el la <tld/>o</kap>
  <snc mrk="manik.ella0o.FRAZ">
    <uzo tip="stl">FRAZ</uzo>
    <dif>
      <ref tip="super" cel="facil.0e.fareble">Facile</ref> farante, verkante:
      <ekz>
        verkisto forskuas el la <tld/>o siajn ravajn mensoga&jcirc;ojn
        <fnt>
          <aut>J. Baghy</aut>,
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr65/otelo.html">
          &Ccirc;u 'otelo, &ccirc;u ne 'otelo</url></vrk>,
          <lok>Nica Literatura Revuo, 1961-05 &gcirc;is 06, numero 6/5, p.
          164a-173a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Reto Rossetti</nom>, en &leftquot;El la <tld/>o&rightquot;, verkas kortu&scirc;e, skizante realisme pecetojn el
        la vivo
        <fnt>
          <bib>Plumamikoj</bib>
          <lok>Prozo fikcia</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z; <ind>&c_r;&c_u;&c_k;&c_a;&c_v;&c_a;</ind></trd>
  <trdgrp lng="ca">
    <trd>de bon</trd>,
    <trd>jugant, jugant jugant</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>aus dem <ind>&Auml;rmel</ind> heraus</trd>,
    <trd>mit <ind>Links</ind></trd>
  </trdgrp>
  <trd lng="fr">avec <ind>brio</ind></trd>
  <trd lng="pl">z <ind>r&eogonek;kawa</ind></trd>
  <trd lng="ro">din <ind>m&acirc;nec&abreve;</ind></trd>
</drv>
</art>
