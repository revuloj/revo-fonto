<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>leter</rad>/o
</kap>
<drv mrk="leter.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Skriba&jcirc;o, per kiu oni korespondas kun alia persono:
      <ekz>
        sendi <tld/>on per po&scirc;to;
      </ekz>
      <ekz>
        <tld/>a papero, invito;
      </ekz>
      <ekz>
        <tld/>e korespondi;
      </ekz>
      <ekz>
        rekomendita, registrita, valora <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">dopis</trd>
  <trdgrp lng="en">
    <trd>letter</trd>,
    <trd>epistle</trd>,
    <trd>missive</trd>
  </trdgrp>
  <trd lng="es">carta</trd>
  <trd lng="fr">lettre</trd>
  <trd lng="de">Brief</trd>
  <trd lng="nl">brief</trd>
  <trd lng="pl">list</trd>
  <trd lng="ru">&c_p;&c_i;&c_s;&c_mol;&c_m;&c_o;</trd>
  <trd lng="sv">brev</trd>
</drv>

<drv mrk="leter.0kesto">
  <kap>
    <tld/>kesto
  </kap>
  <snc>
    <dif>
      Kesto por enmeti sendotajn a&ubreve; ricevitajn leterojn a&ubreve; papera&jcirc;ojn.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>mail box</trd>,
    <trd>letter box</trd>
  </trdgrp>
  <trd lng="nl">brievenbus</trd>
  <trd lng="ru">&c_p;&c_o;&c_ch;&c_t;&c_o;&c_v;&c_y;&c_j; &c_ja;&c_shch;&c_i;&c_k;</trd>
  <trd lng="sv">brevl&aring;da</trd>
</drv>

<drv mrk="leter.inform0o">
  <kap>
    inform<tld/>o
  </kap>
  <snc>
    <dif>
      Mallonga, ofte unufolia, <ref tip="vid"
      cel="bulten.0o">bulteno</ref>, kiun firmao a&ubreve; institucio periode
      eldonas kaj senpage sendas al la legantoj por informi ilin pri siaj
      produktoj kaj faroj.
    </dif>
    <ref tip="super" cel="period.0ajho">perioda&jcirc;o</ref>
  </snc>
  <trd lng="fr">lettre d'information</trd>
  <trd lng="nl">nieuwsbrief</trd>
  <trd lng="ru">&c_i;&c_n;&c_f;&c_o;&c_r;&c_m;&c_a;&c_c;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j; &c_l;&c_i;&c_s;&c_t;&c_o;&c_k;</trd>
</drv>

<drv mrk="leter.novajh0o">
  <kap>
    nova&jcirc;<tld/>o
  </kap>
  <snc>
    <dif>
      <ref tip="sin" cel="leter.inform0o">Inform<tld/>o</ref>.
    </dif>
  </snc>
  <trd lng="en">news letter</trd>
  <trd lng="nl">nieuwsbrief</trd>
</drv>
</art>
