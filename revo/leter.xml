      <trd>&epsilon;&pi;&jota;&sigma;&tau;&omikron;&lambda;&eta_ton;</trd>
    </trdgrp>
  <trdgrp lng="en">
      <trd>letter</trd>,
      <trd>epistle</trd>,
      <trd>missive</trd>
    </trdgrp>
  <trd lng="es">carta</trd>
  <trd lng="et">kiri</trd>
  <trd lng="eu">gutuna</trd>
  <trd lng="fa">&a_n;&a_A;&a_m;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>courrier</trd>,
    <trd>lettre<klr> (courrier)</klr></trd>
  </trdgrp>
  <trd lng="fy">letter</trd>
  <trd lng="gl">carta</trd>
  <trd lng="gu">&#xA85;&#xA95;&#xACD;&#xAB7;&#xAB0;</trd>
  <trd lng="ha">wasika</trd>
  <trd lng="hi">&#x92A;&#x924;&#x94D;&#x930;</trd>
  <trd lng="hr">pismo</trd>
  <trd lng="hu">lev&eacute;l <klr tip="ind">(postai)</klr></trd>
  <trd lng="hy">&#x576;&#x561;&#x574;&#x561;&#x56F;</trd>
  <trd lng="id">surat</trd>
  <trd lng="is">br&#xE9;f</trd>
  <trdgrp lng="it">
      <trd>lettera<klr> (epistola)</klr></trd>,
      <trd>epistola</trd>,
      <trd>missiva</trd>
    </trdgrp>
  <trd lng="ja">&#x624B;&#x7D19;</trd>
  <trd lng="jw">huruf</trd>
  <trd lng="ka">&#x10EC;&#x10D4;&#x10E0;&#x10D8;&#x10DA;&#x10D8;</trd>
  <trd lng="km">&#x179B;&#x17B7;&#x1781;&#x17B7;&#x178F;</trd>
  <trd lng="ko">&#xD3B8;&#xC9C0;</trd>
  <trd lng="lo">&#xE88;&#xEBB;&#xE94;&#xEAB;&#xEA1;&#xEB2;&#xE8D;&#xEAA;&#xEB0;&#xE9A;&#xEB1;&#xE9A;</trd>
  <trd lng="ml">&#xD05;&#xD15;&#xD4D;&#xD37;&#xD30;&#xD02;</trd>
  <trd lng="mr">&#x92A;&#x924;&#x94D;&#x930;</trd>
  <trd lng="my">&#x1005;&#x102C;</trd>
  <trd lng="ne">&#x92A;&#x924;&#x94D;&#x930;</trd>
  <trd lng="nl">brief</trd>
