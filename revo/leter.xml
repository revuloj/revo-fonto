<drv mrk="leter.inform0o">
  <kap>inform<tld/>o</kap>
  <snc mrk="leter.inform0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Mallonga, ofte unufolia, <ref tip="vid" cel="bulten.0o">bulteno</ref>, per kiu firmao a&ubreve;
      institucio
      periode informas pri siaj produktoj kaj faroj:
      <ekz>
        la dua inform<tld/>o, kiu estos sendita al la ali&gcirc;intoj, <klr>[&#x2026;]</klr> enhavos
        precizajn informojn pri la kongresejo, programo, lo&gcirc;- kaj man&gcirc;-eblecoj
        <fnt>
          <bib>LOdE</bib>
          <aut>Holger Boos</aut>
          <vrk>Venu al la Ruslanda Esperanto-Kongreso</vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="period.0ajxo">perioda&jcirc;o</ref>
    <refgrp tip="sin">
      <ref cel="leter.novajx0o">nova&jcirc;letero</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_f;&c_a;&c_r;&c_m;&c_a;&c_c;&c_y;&c_j;&c_n;&c_y; &c_b;&c_ju;&c_l;&c_je;&c_t;&c_e;&c_n;&c_mol;</trd>
  <trd lng="de">Rundbrief</trd>
  <trd lng="en">news letter</trd>
  <trd lng="fa">&a_kh;&a_b;&a_r;&a_n;&a_A;&a_m;&a_h;</trd>
  <trd lng="fr">lettre d'information</trd>
  <trd lng="hu">h&iacute;rlev&eacute;l</trd>
  <trd lng="id">nawala</trd>
