<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rimes.xml,v 1.16 2024/04/14 08:18:38 revo Exp $">
<kap>
  <rad>rimes</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="rimes.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc mrk="rimes.0o.kambio">
    <dif>
      <ref tip="super" cel="kambi.0o">Kambio</ref>,
      kiun &scirc;uldanto sendas al sia kreditoro por tiel pagi
      la pli malfrue pagotan &scirc;uldon:
      <ekz>
        ni proponas al &ccirc;iuj esperantistaj klientoj, kiuj sendas al ni mendojn kun <tld/>o
        (per &ccirc;eko, se eble) por nia fame konata viskio Esperanto specialan rabaton da
        5 &scirc;.
        <fnt>
          <lok><url ref="https://books.google.de/books?id=o0QOAAAAYAAJ&amp;lpg=RA5-PA180-IA3&amp;ots=Isggw7DyLY&amp;pg=RA5-PA180-IA3#v=onepage"
              >The British Esperantisto, vol. 1-3, 1905, p. vii</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        po&scirc;tejo estas oficejo <klr>[kie oni]</klr> povas <klr>[&#8230;]</klr>
        sendi kaj ricevi <tld/>ojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=46027">Po&scirc;tejo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxek.0o">&ccirc;eko</ref>,
      <ref cel="trat.0o">trato</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_y;&c_m;&c_e;&c_s;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Rimesse</trd>,
    <trd>Besitzwechsel <klr>(Wertpapier)</klr></trd>,
    <trd>Wechsel <klr>(Wertpapier)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&uuml;ldv&eacute;ny</trd>,
    <trd>rimessza</trd>
  </trdgrp>
  <trd lng="ja">&#36865;&#37329;  <pr>&#12381;&#12358;&#12365;&#12435;</pr></trd>
  <trdgrp lng="nl">
    <trd>overmaking</trd>,
    <trd>remise</trd>
  </trdgrp>
  <trd lng="pl">rymesa</trd>
  <trd lng="pt">remessa</trd>
  <trd lng="ro">presta&tcomma;ie</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_i;&c_m;&c_je;&c_s;&c_s;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_o;&c_d;&c_n;&c_o;&c_j; &c_v;&c_je;&c_k;&c_s;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_o;&c_d; &c_d;&c_je;&c_n;&c_je;&c_g;</trd>
  </trdgrp>
  <trd lng="uk">&c_r;&c_i;&c_m;&c_je;&c_s;&c_a; (&c_v;&c_i;&c_d; &c_v;&c_je;&c_k;&c_s;&c_je;&c_l;&c_ja;)</trd>
</drv>
</art>
