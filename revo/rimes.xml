<art mrk="$Id: rimes.xml,v 1.15 2023/10/21 05:38:06 revo Exp $">
<kap>
  <rad>rimes</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="rimes.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Speco de <ref tip="super" cel="kambi.0o">kambio</ref>,
      kiun &scirc;uldanto sendas al sia kreditoro por tiel pagi
      la pli malfrue pagotan &scirc;uldon:
      <ekz>
        po&scirc;tejo estas oficejo <klr>[kie]</klr> <klr>[oni]</klr> povas <klr>[&#x2026;]</klr>
        sendi kaj ricevi <tld/>ojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=46027">Po&scirc;tejo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxek.0o">&ccirc;eko</ref>,
      <ref cel="trat.0o">trato</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_y;&c_m;&c_e;&c_s;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Rimesse</trd>,
    <trd>Besitzwechsel</trd>,
    <trd>Wechsel</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&uuml;ldv&eacute;ny</trd>,
    <trd>rimessza</trd>
  </trdgrp>
  <trd lng="ja">&#x9001;&#x91D1;  <pr>&#x305D;&#x3046;&#x304D;&#x3093;</pr></trd>
  <trdgrp lng="nl">
    <trd>overmaking</trd>,
    <trd>remise</trd>
  </trdgrp>
  <trd lng="pl">rymesa</trd>
  <trd lng="pt">remessa</trd>
  <trd lng="ro">presta&#x21B;ie</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_i;&c_m;&c_je;&c_s;&c_s;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_o;&c_d;&c_n;&c_o;&c_j; &c_v;&c_je;&c_k;&c_s;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_o;&c_d; &c_d;&c_je;&c_n;&c_je;&c_g;</trd>
  </trdgrp>
  <trd lng="uk">&c_r;&c_i;&#x43C;&c_je;&c_s;&c_a; (&c_v;&c_i;&c_d; &c_v;&c_je;&#x43A;&c_s;&c_je;&#x43B;&#x44F;)</trd>
</drv>
</art>
