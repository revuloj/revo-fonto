<kap><rad>fotometr</rad>/o<fnt><bib>PVEF</bib></fnt></kap>
<drv mrk="fotome.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="instru2.0o">Instrumento</ref>
      por mezuri la intensecon de la lumo:
      <ekz>
        scinitilometro estas <tld/>o kiu a&ubreve;tomate registras scintiladon de astroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=7761">Scintilometro</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Kepler</nom> <klr>[&#8230;]</klr> utiligante kosman <tld/>on disvolvita de NASA,
        <klr>[&#8230;]</klr> observas la brilecon de pli ol 100.000 steloj dum 3,5 jaroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=278481">Misio Kepler</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>lummezurilo</ke>.
  </rim>

  <trd lng="be">&c_f;&c_a;&c_t;&c_o;&c_m;&c_e;&c_t;&c_a;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>expozimetr</trd>,
    <trd>fotometr</trd>,
    <trd>osvitom&ecaron;r</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Fotometer</trd>,
    <trd>Photometer</trd>
  </trdgrp>
  <trd lng="es">fot&oacute;metro</trd>
  <trd lng="fr">photom&egrave;tre</trd>
  <trdgrp lng="hu">
    <trd>f&eacute;nym&eacute;r&odblac;</trd>,
    <trd>megvil&aacute;g&iacute;t&aacute;sm&eacute;r&odblac;</trd>,
    <trd>fotom&eacute;ter</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#28204;&#20809;&#22120;  <pr>&#12381;&#12387;&#12371;&#12358;&#12365;</pr></trd>,
    <trd>&#20809;&#24230;&#35336;  <pr>&#12371;&#12358;&#12393;&#12369;&#12356;</pr></trd>,
    <trd>&#38706;&#20809;&#35336;  <pr>&#12429;&#12371;&#12358;&#12369;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">lichtmeter</trd>
  <trd lng="pt">fot&ocirc;metro</trd>
  <trd lng="ru">&c_f;&c_o;&c_t;&c_o;&c_m;&c_je;&c_t;&c_r;</trd>
  <trd lng="sk">expozimeter</trd>
  <trd lng="uk">&#1092;&#1086;&#1090;&#1086;&#1084;&#1077;&#1090;&#1088;</trd>
  <trd lng="zh">&#20809;&#24230;&#35336; <pr>gu&#257;ngd&#249;j&#236;</pr></trd>
</drv>
