<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pasament</rad>/o
</kap>
<drv mrk="pasame.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Ornama <ref tip="super" cel="ruband.0o.ornamo">rubando</ref> el plekta&jcirc;o
      de oraj, ar&gcirc;entaj, silkaj, lanaj kaj similaj fadenoj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Posamenten</trd>,
    <trd>Besatzwaren</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>passement</trd>,
    <trd>passementerie</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>passement</trd>,
    <trd>belegsel</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_z;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_g;&c_a;&c_l;&c_u;&c_n;</trd>
  </trdgrp>
</drv>
</art>
