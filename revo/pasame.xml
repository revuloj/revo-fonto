<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pasame.xml,v 1.25 2023/10/20 14:25:20 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pasament</rad>/o
</kap>
<drv mrk="pasame.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="plekt.0ajxo">Plekta&jcirc;o</ref> el valormetalaj 
      kaj similaj fadenoj por ornami teksa&jcirc;on, verston...:
      <ekz>
        flava livreo kun blua <tld/>o
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 15a, p.
        79a</lok></fnt>;
      </ekz>
      <ekz>
        brokantu per malnovaj <tld/>oj kaj bukoj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi en&scirc;teli&gcirc;is en la pre&gcirc;ejon de la sankta Stefano kaj mi de&scirc;iris la <tld/>ojn de
        la altara tuko
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="epolet.0o">epoleto</ref>,
      <ref cel="galon.0o">galono</ref>,
      <ref cel="kordon.0o.TEKS">kordono</ref>,
      <ref cel="kvast.0o">kvasto</ref>,
      <ref cel="punt.0o">punto</ref>,
      <ref cel="ruband.0o.ornamo">rubando<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="bo">&t_la;&t_hata;&t_sa;&t_tsheg;&t_ma;&t_tsheg;</trd>
  <trd lng="br">pasamant</trd>
  <trd lng="ca">passam&agrave;</trd>
  <trdgrp lng="cs">
    <trd>lemovka</trd>,
    <trd>pr&yacute;mek</trd>,
    <trd>pr&yacute;mka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Posament</trd>,
    <trd>Besatz <klr>(Kleidung)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>passement</trd>,
    <trd>braid</trd>,
    <trd>trim</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>passement</trd>,
    <trd>passementerie</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>paszom&aacute;ny</trd>,
    <trd>sujt&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30E2;&#x30FC;&#x30EB;  <pr>&#x3082;&#x30FC;&#x308B;</pr></trd>,
    <trd>&#x98FE;&#x308A;&#x3072;&#x3082;  <pr>&#x304B;&#x3056;&#x308A;&#x3072;&#x3082;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>passement</trd>,
    <trd>belegsel</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>lam&oacute;wka</trd>,
    <trd>pasmanteria</trd>,
    <trd>tasiemka</trd>
  </trdgrp>
  <trd lng="pt">passamane(s)</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_z;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_g;&c_a;&c_l;&c_u;&c_n;</trd>,
    <trd>&c_b;&c_a;&c_s;&c_o;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>margine</trd>,
    <trd>mercerie</trd>,
    <trd>fund&abreve;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd><klr>(ozdobn&aacute;)</klr> pasom&aacute;n</trd>,
    <trd>zlat&aacute; &scaron;n&uacute;ra</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&c_z;&c_u;&#x43C;&c_je;&#x43D;&c_t;</trd>,
    <trd>&c_g;&c_a;&#x43B;&c_u;&#x43D;</trd>,
    <trd>&c_t;&c_a;&c_s;&#x44C;&#x43C;&c_a;</trd>,
    <trd>&#x43B;&#x44F;&#x43C;&c_ib;&c_v;&#x43A;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="pasame.0i">
  <kap><tld/>i
    <fnt><bib>PIV2</bib></fnt>
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="ornam.0i">Ornami</ref> per <tld/>o:
      <ekz>
        por rimarkigi sin, la aristokrato devis a&ccirc;eti pli <tld/>itajn vesta&jcirc;ojn, pli
        luksajn kol&ccirc;enojn
        <fnt>
          <bib>MD05</bib>
          <vrk>&Ccirc;iuj sur la sama galero!</vrk>
        </fnt>;
      </ekz>
      <ekz>
        li skur&gcirc;as <klr>[&#x2026;]</klr> tiujn &leftquot;<tld/>itajn marionetojn, arogantajn soldata&ccirc;ojn&rightquot;
        <fnt>
          <bib>MD11</bib>
          <vrk>Majstroverko de ambigueco</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bo">&t_la;&t_hata;&t_sa;&t_tsheg;&t_ma;&t_tsheg;&t_ba;&t_sa;&t_lata;&t_sa;&t_tsheg;</trd>
  <trd lng="br">pasamanta&ntilde;</trd>
  <trd lng="ca">passamanar</trd>
  <trd lng="de">mit <ind>Besatz</ind> versehen</trd>
  <trdgrp lng="en">
    <trd>passement</trd>,
    <trd>braid</trd>,
    <trd>trim</trd>
  </trdgrp>
  <trd lng="fr">passementer</trd>
  <trd lng="ja">&#x98FE;&#x308A;&#x3072;&#x3082;&#x3092;&#x3064;&#x3051;&#x308B;  <pr>&#x304B;&#x3056;&#x308A;&#x3072;&#x3082;&#x3092;&#x3064;&#x3051;&#x308B;</pr></trd>
  <trd lng="nl">een <ind>passement</ind> maken</trd>
  <trdgrp lng="pl">
    <trd>bramowa&cacute;</trd>,
    <trd>oplata&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>&icirc;mpleti</trd>,
    <trd>&icirc;nf&abreve;&#x219;ura</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_sh;&c_i;&c_t;&c_mol; <ind>&c_p;&c_o;&c_z;&c_u;&c_m;&c_je;&c_n;&c_t;&c_o;&c_m;</ind></trd>,
    <trd>&c_o;&c_b;&c_sh;&c_i;&c_t;&c_mol; <ind>&c_g;&c_a;&c_l;&c_u;&c_n;&c_o;&c_m;</ind></trd>,
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43E;&c_b;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_i; &#x43F;&#x43E;&c_z;&c_u;&#x43C;&c_je;&#x43D;&c_t;&#x43E;&#x43C;</trd>,
    <trd>&c_g;&c_a;&#x43B;&c_u;&#x43D;&#x43E;&#x43C;</trd>,
    <trd>&#x43E;&c_b;&#x43B;&#x44F;&#x43C;&#x43E;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="pasame.0isto">
  <kap><ofc>*</ofc><tld/>isto</kap>
  <snc>
    <dif>
      Komercisto a&ubreve; <ref tip="lst" lst="voko:metioj" cel="meti.0isto">metiisto</ref> de <tld/>oj.
    </dif>
  </snc>
  <trd lng="br">pasamanter</trd>
  <trd lng="ca">passamaner</trd>
  <trdgrp lng="de">
    <trd>Posamentierer</trd>,
    <trd>Besatzmacher</trd>
  </trdgrp>
  <trd lng="fr">passementier</trd>
  <trdgrp lng="nl">
    <trd>passementmaker</trd>,
    <trd>boordselwever</trd>,
    <trd>passementwerker</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>szmuklerz</trd>,
    <trd>pasamonik</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_a;&c_s;&c_o;&c_n;&c_shch;&c_i;&c_k;</trd>
  <trd lng="uk">&#x43F;&#x43E;&c_z;&c_u;&#x43C;&c_je;&#x43D;&c_t;&#x43D;&c_i;&#x43A;</trd>
</drv>
</art>
