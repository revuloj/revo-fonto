<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pasame.xml,v 1.20 2021/05/28 12:15:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pasament</rad>/o
</kap>
<drv mrk="pasame.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Ornama <ref tip="super" cel="ruband.0o.ornamo">rubando</ref> el
      plekta&jcirc;o de oraj, ar&gcirc;entaj, silkaj, lanaj kaj similaj fadenoj:
      <ekz>
        flava livreo kun blua <tld/>o
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 15a, p.
        79a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bo">&#xF63;&#xFB7;&#xF66;&#xF0B;&#xF58;&#xF0B;</trd>
  <trd lng="br">pasamant</trd>
  <trd lng="ca">passam&agrave;</trd>
  <trdgrp lng="cs">
    <trd>lemovka</trd>,
    <trd>pr&#xFD;mek</trd>,
    <trd>pr&#xFD;mka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Posamenten</trd>,
    <trd>Besatzwaren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>passement</trd>,
    <trd>braid</trd>,
    <trd>trim</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>passement</trd>,
    <trd>passementerie</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>paszom&aacute;ny</trd>,
    <trd>sujt&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>passement</trd>,
    <trd>belegsel</trd>
  </trdgrp>
  <trd lng="pt">passamane(s)</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_z;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_g;&c_a;&c_l;&c_u;&c_n;</trd>,
    <trd>&c_b;&c_a;&c_s;&c_o;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd><klr>(ozdobn&#xE1;)</klr> pasom&#xE1;n</trd>,
    <trd>zlat&#xE1; &#x161;n&#xFA;ra</trd>
  </trdgrp>
</drv>

<drv mrk="pasame.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <dif>
      Ornami per <tld/>o.
    </dif>
  </snc>
  <trd lng="bo">&#xF63;&#xFB7;&#xF66;&#xF0B;&#xF58;&#xF0B;&#xF56;&#xF66;&#xFB3;&#xF66;&#xF0B;</trd>
  <trd lng="br">pasamanta&ntilde;</trd>
  <trd lng="ca">passamanar</trd>
  <trdgrp lng="en">
    <trd>passement</trd>,
    <trd>braid</trd>,
    <trd>trim</trd>
  </trdgrp>
  <trd lng="fr">passementer</trd>
  <trd lng="nl">een <ind>passement</ind> maken</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_sh;&c_i;&c_t;&c_mol; <ind>&c_p;&c_o;&c_z;&c_u;&c_m;&c_je;&c_n;&c_t;&c_o;&c_m;</ind></trd>,
    <trd>&c_o;&c_b;&c_sh;&c_i;&c_t;&c_mol; <ind>&c_g;&c_a;&c_l;&c_u;&c_n;&c_o;&c_m;</ind></trd>,
  </trdgrp>
</drv>

<drv mrk="pasame.0isto">
  <kap><ofc>*</ofc><tld/>isto</kap>
  <snc>
    <dif>
      Komercisto a&ubreve; metiisto per <tld/>o.
    </dif>
  </snc>
  <trd lng="br">pasamanter</trd>
  <trd lng="ca">passamaner</trd>
  <trd lng="fr">passementier</trd>
  <trdgrp lng="nl">
    <trd>passementmaker</trd>,
    <trd>boordselwever</trd>,
    <trd>passementwerker</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_a;&c_s;&c_o;&c_n;&c_shch;&c_i;&c_k;</trd>
</drv>
</art>
