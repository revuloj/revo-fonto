<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pasament</rad>/o
</kap>
<drv mrk="pasame.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Mallar&gcirc;a plekta&jcirc;o el oraj, ar&gcirc;entaj, silkaj, lanaj ks fadenoj, kiun
      oni alkudras por borderi a&ubreve; ornami veston a&ubreve; meblon.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Posamenten</trd>,
    <trd>Besatzwaren</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>passement</trd>,
    <trd>belegsel</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_z;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_g;&c_a;&c_l;&c_u;&c_n;</trd>
  </trdgrp>
</drv>
</art>
