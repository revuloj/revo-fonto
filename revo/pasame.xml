<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pasament</rad>/o
</kap>
<drv mrk="pasame.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Ornama <ref tip="super" cel="ruband.0o.ornamo">rubando</ref> el
      plekta&jcirc;o de oraj, ar&gcirc;entaj, silkaj, lanaj kaj similaj fadenoj:
      <ekz>
        flava livreo kun blua <tld/>o
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 15a, p. 79a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Posamenten</trd>,
    <trd>Besatzwaren</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>passement</trd>,
    <trd>passementerie</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>paszom&aacute;ny</trd>,
    <trd>sujt&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>passement</trd>,
    <trd>belegsel</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_z;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_g;&c_a;&c_l;&c_u;&c_n;</trd>
  </trdgrp>
</drv>

<drv mrk="pasame.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <dif>
      Ornami per <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">passementer</trd>
</drv>

<drv mrk="pasame.0isto">
  <kap><ofc>*</ofc><tld/>isto</kap>
  <snc>
    <dif>
      Komercisto a&ubreve; metiisto per <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">passementier</trd>
</drv>
</art>
