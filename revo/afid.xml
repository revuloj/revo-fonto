<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>afid</rad>/o
</kap>
<drv mrk="afid.0o">
  <kap><tld/>o</kap>
  <snc mrk="afid.0o.familio">
    <dif>
      Iu el <ref tip="malprt" cel="afid.0edoj"><tld/>edoj</ref>.
    </dif>
    <trdgrp lng="de">
      <trd>R&ouml;hrenlaus</trd>,
      <trd>Blattlaus <klr tip="ind">(bestimmte)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="afid.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="afid.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Aphis</trd>), kiuj su&ccirc;as la sukon de plantoj.  
    </dif>
  </snc>
</drv>
<drv mrk="afid.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="homopt.0oj">homopteroj</ref>
      (<trd lng="la">Aphididae</trd>).
    </dif>
  </snc>
  <trd lng="de">R&ouml;hrenl&auml;se</trd>
</drv> 
</art>
<!--
$Log$
-->
</vortaro>