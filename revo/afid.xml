<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>afid</rad>/o
</kap>
<drv mrk="afid.0o">
  <kap><tld/>o</kap>
  <snc mrk="afid.0o.familio">
    <dif>
      Iu el <ref tip="malprt" cel="afided.0oj"><tld/>edoj</ref>.
    </dif>
    <trdgrp lng="de">
      <trd>R&ouml;hrenlaus</trd>,
      <trd>Blattlaus <klr tip="ind">(bestimmte)</klr></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>aphid</trd>,
      <trd>aphis</trd>
    </trdgrp>
    <trd lng="fr">puceron</trd>
    <trd lng="nl">luis</trd>
    <trdgrp lng="pt">
      <trd>pulg&atilde;o</trd>,
      <trd>af&iacute;dio</trd>
    </trdgrp>
  </snc>
  <snc mrk="afid.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="afided.0oj"><tld/>edoj</ref>
      (<trd lng="la">Aphis</trd>).
    </dif>
  </snc>
  <trd lng="ru">&c_t;&c_l;&c_ja;</trd>
</drv>
</art>
