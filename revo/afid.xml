<vortaro>
<art mrk="$Id: afid.xml,v 1.28 2025/09/13 20:00:22 revo Exp $">
<kap>
  <rad>afid</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="afid.0o">
  <kap><tld/>o</kap>
  <snc mrk="afid.0o.familio">
    <dif>
      Iu el <ref tip="malprt" cel="afided.0oj"><tld/>edoj</ref>.
    </dif>
    <trd lng="ca">pug&oacute;</trd>
    <trdgrp lng="de">
      <trd>R&ouml;hrenlaus</trd>,
      <trd>Blattlaus<klr tip="ind"> (bestimmte)</klr></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>aphid</trd>,
      <trd>aphis</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>afide</trd>,
      <trd><ind>pidocchio</ind> delle piante</trd>
    </trdgrp>
    <trd lng="nl">luis</trd>
    <trdgrp lng="pt">
      <trd>pulg&atilde;o</trd>,
      <trd>af&iacute;dio</trd>
    </trdgrp>
    <trd lng="hu">lev&eacute;ltet&udblac;</trd>
    <trdgrp lng="zh">
    <trd>&#33147;&#34411; <pr>n&igrave;ch&oacute;ng</pr></trd>,
    <trd>&#33193;&#34802; <pr>n&igrave;ch&oacute;ng</pr></trd>,
    <trd>&#34460; <pr>y&aacute;</pr></trd>,
    <trd>&#34460;&#34411; <pr>y&aacute;ch&oacute;ng</pr></trd>,
    <trd>&#34460;&#34802; <pr>y&aacute;ch&oacute;ng</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="afid.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="afided.0oj"><tld/>edoj</ref>
      (<trd lng="la">Aphis</trd>):
      <ekz>
        <tld/>oj <klr>...</klr> volonte fordonas sian
        dol&ccirc;an ekskrecion al formikoj
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 7a</lok></fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/22/Aphis_citricola1.jpg/640px-Aphis_citricola1.jpg"
    prm="CC-BY-SA-3.0"><tld/>oj sur tigo (Aphis citricola)
        <fnt>
          <aut>I, Kenpei, propra foto</aut>
          <lok><url
          ref="&WCW;/File:Aphis_citricola1.jpg">&WCW;/File:Aphis_citricola1.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="it">
      <trd>afidi</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_t;&c_l;&c_ja;</trd>
  <trd lng="cs">m&scaron;ice</trd>
  <trd lng="fr">puceron</trd>
  <trd lng="he">&kaf;&#1468;&#1456;&nun;&#1460;&yod;&mem;&#1464;&he;</trd>
  <trdgrp lng="ja">
    <trd>&#12450;&#12502;&#12521;&#12512;&#12471;</trd>,
    <trd>&#12450;&#12522;&#12510;&#12461;</trd>
  </trdgrp>
  <trd lng="pl">mszyca</trd>
  <trd lng="ru">&c_t;&c_l;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>m&scaron;ica</trd>,
    <trd>vo&scaron;ka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_p;&c_je;&c_l;&c_i;&c_c;&c_ja;</trd>,
    <trd>&c_t;&c_l;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
