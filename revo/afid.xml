<vortaro>
<art mrk="$Id: afid.xml,v 1.24 2022/03/11 12:15:12 revo Exp $">
<kap>
  <rad>afid</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="afid.0o">
  <kap><tld/>o</kap>
  <snc mrk="afid.0o.familio">
    <dif>
      Iu el <ref tip="malprt" cel="afided.0oj"><tld/>edoj</ref>.
    </dif>
    <trdgrp lng="de">
      <trd>R&ouml;hrenlaus</trd>,
      <trd>Blattlaus<klr tip="ind"> (bestimmte)</klr></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>aphid</trd>,
      <trd>aphis</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>afide</trd>,
      <trd><ind>pidocchio</ind> delle piante</trd>
    </trdgrp>
    <trd lng="nl">luis</trd>
    <trdgrp lng="pt">
      <trd>pulg&atilde;o</trd>,
      <trd>af&iacute;dio</trd>
    </trdgrp>
    <trd lng="hu">lev&eacute;ltet&udblac;</trd>
  </snc>
  <snc mrk="afid.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="afided.0oj"><tld/>edoj</ref>
      (<trd lng="la">Aphis</trd>):
      <ekz>
        <tld/>oj <klr>...</klr> volonte fordonas sian
        dol&ccirc;an ekskrecion al formikoj
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 7a</lok></fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/22/Aphis_citricola1.jpg/640px-Aphis_citricola1.jpg" prm="CC-BY-SA-3.0"><tld/>oj sur tigo (Aphis citricola)
        <fnt>
          <aut>I, Kenpei, propra foto</aut>
          <lok><url ref="&WCW;/File:Aphis_citricola1.jpg">&WCW;/File:Aphis_citricola1.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="it">
      <trd>afidi</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_t;&c_l;&c_ja;</trd>
  <trd lng="cs">m&scaron;ice</trd>
  <trd lng="fr">puceron</trd>
  <trd lng="he">&#x5DB;&#x5BC;&#x5B0;&#x5E0;&#x5B4;&#x5D9;&#x5DE;&#x5B8;&#x5D4;</trd>
  <trd lng="pl">mszyca</trd>
  <trd lng="ru">&c_t;&c_l;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>m&scaron;ica</trd>,
    <trd>vo&scaron;ka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43F;&#x435;&#x43B;&#x438;&#x446;&#x44F;</trd>,
    <trd>&#x442;&#x43B;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
