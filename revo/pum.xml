<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>pum</rad> <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="pum.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc>
    <dif>
      Bruo de subita bato a&ubreve; falo:
      <ekz>
        <tld/>! la granda Niko falis en la riveron
        <fnt><bib>Fab1</bib>, <lok>malgranda Niko kaj granda
        Niko</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bum1.0">bum</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>boum !</trd>,
    <trd>plaf !</trd>
  </trdgrp>
  <trd lng="pt">pum!</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_h;!</trd>,
    <trd>&c_b;&c_u;&c_m;!</trd>
  </trdgrp>
</drv>

<drv mrk="pum.pum0ado">
  <kap><tld/><tld/>ado</kap>
  <snc>
    <dif>
      Ripetata a&ubreve; ritma bata <ref tip="super" cel="bru.0o.sonaro">bruo</ref>:
      <ekz>
        Li <klr>...</klr> a&ubreve;skultis la kriojn kaj la <tld/><tld/>adon
        venantajn de la lumplena dormo&ccirc;ambro
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 20a, p. 202-203a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">martellement</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_b;&c_a;&c_h;&c_a;&c_n;&c_mol;&c_je;</trd>,
    <trd>&c_g;&c_r;&c_o;&c_h;&c_o;&c_t;</trd>,
    <trd>&c_s;&c_t;&c_u;&c_k;</trd>
  </trdgrp>
</drv>
</art>

