<art mrk="$Id: palank.xml,v 1.20 2024/02/15 09:24:49 revo Exp $">
<kap>
 <ofc>2</ofc><rad>palanken</rad>/o
 <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="palank.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Litse&gcirc;o a&ubreve; <ref tip="super" cel="hamak.0o">hamako</ref>,
      kiun servistoj portas sur &scirc;ultroj, uzata en orientaj landoj:
      <ekz>
        <nom>Sarah Bernhardt</nom> <klr>[&#8230;]</klr> estis kutime portata en
        <tld/>o la&ubreve; &scirc;ia propra koncepto, kun du longaj teniloj, kiun portis
        du viroj, &scirc;i dekoraciigis la <ref tip="super" cel="segx.porto0o">portose&gcirc;on</ref>
        la&ubreve; la stilo Ludoviko la 15-a,
        kun blankaj flankoj kaj orumitaj ornama&jcirc;oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=13316">Sarah Bernhardt</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="https://upload.wikimedia.org/wikipedia/commons/thumb/f/f1/Palankin%2C_Calcutta%2C_KITLV_36C242.tiff/lossy-page1-640px-Palankin%2C_Calcutta%2C_KITLV_36C242.tiff.jpg"
      prm="CC BY 4.0"
      ><tld/>o, Kalkuto
        <fnt>
          <aut>Clercq, J.H.W. le., http://hdl.handle.net/1887.1/item:706867</aut>
          <lok><url ref="&WCW;/File:Palankin,_Calcutta,_KITLV_36C242.tiff"
              >https://commons.wikimedia.org/wiki/File:Palankin,_Calcutta,_KITLV_36C242.tiff</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ja;&c_n;&c_k;&c_ib;&c_n;</trd>
  <trd lng="cs">orient&aacute;ln&iacute; nos&iacute;tka</trd>
  <trdgrp lng="de">
    <trd>Palankin</trd>,
    <trd>Tragstuhl</trd>,
    <trd>S&auml;nfte</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>palanquin</trd>,
    <trd>litter</trd>
  </trdgrp>
  <trd lng="fr">palanquin</trd>
  <trdgrp lng="hu">
    <trd>hordsz&eacute;k</trd>,
    <trd>gyaloghint&oacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12363;&#12372;</trd>,
    <trd>&#12371;&#12375;</trd>
  </trdgrp>
  <trd lng="nl">draagstoel</trd>
  <trdgrp lng="pl">
    <trd>lektyka</trd>,
    <trd>palankin</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>palanquim</trd>,
    <trd>liteira</trd>,
    <trd>machila</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>lectic&abreve;</trd>,
    <trd>palanchin</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_l;&c_a;&c_n;&c_k;&c_i;&c_n;</trd>
  <trd lng="sk">orient&aacute;lne nosidl&aacute; <klr>(pre &lcaron;ud&iacute;)</klr></trd>
  <trd lng="uk">&c_p;&c_a;&c_l;&c_a;&c_n;&c_k;&c_ib;&c_n;</trd>
</drv>
</art>
