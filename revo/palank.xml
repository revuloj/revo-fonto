  <trd lng="be">&c_p;&c_a;&c_l;&c_ja;&c_n;&c_k;&c_ib;&c_n;</trd>
  <trdgrp lng="de">
    <trd>S&auml;nfte</trd>,
    <trd>Tragstuhl</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hordsz&eacute;k</trd>,
    <trd>gyaloint&oacute;</trd>
  </trdgrp>
  <trd lng="nl">draagstoel</trd>
  <trdgrp lng="pt">
    <trd>palanquim</trd>,
    <trd>liteira</trd>,
    <trd>machila</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_l;&c_a;&c_n;&c_k;&c_i;&c_n;</trd>
</drv>
</art>
