<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: talped.xml,v 1.10 2018/09/06 20:10:17 revo Exp $">
<kap>
  <rad>talped</rad>/oj
</kap>
<drv mrk="talped.0oj">
  <kap><tld/>oj</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="mangx.insekto0uloj">insektoman&gcirc;uloj</ref>
      (<trd lng="la">Talpidae</trd>):
      <ekz>
        E&ubreve;ropa <ref tip="prt" cel="talp.0o">talpo</ref>
        (<nom>Talpa europaea</nom>) estas malgranda mamulo el la familio de <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=163517">E&ubreve;ropa talpo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_a;&c_t;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">t&agrave;lpids</trd>
  <trd lng="de">Maulw&uuml;rfe<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="fr">Talpid&eacute;s</trd>
  <trd lng="hu">vakondokf&eacute;l&eacute;k</trd>
  <trd lng="nl">mollen<klr tip="ind"> (fam.)</klr></trd>
  <trdgrp lng="pt">
   <trd>talp&iacute;deos</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_r;&c_o;&c_t;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
