<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kver.xml,v 1.20 2024/12/18 18:36:09 revo Exp $">
<kap>
  <rad>kver</rad>/i <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="kver.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kver.0i.SON">
    <uzo>(pri turtoj kaj kolomboj)</uzo>
    <dif>
      <ref tip="lst" cel="blek.0o" lst="voko:blekoj">Bleki</ref>
      per milda murmuro:
      <ekz>
        la arbaraj kolomboj <tld/>is
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        turtoj <tld/>is sur la &gcirc;ardenteraso
        <fnt>
          <bib>MkM</bib>
          <lok>2</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bebo <tld/>is kaj ridis, skuante l' infan&ccirc;areton
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2013/011130.php"
              >Morto de elstara irlanda poeto kaj homaramanto</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kurre.0.ZOO">kurre</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>gurren</trd>,
      <trd>girren</trd>
    </trdgrp>
    <trd lng="pl">grucha&cacute; <klr>(o go&lstroke;&eogonek;biu)</klr></trd>
  </snc>
  <snc mrk="kver.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ame interparoli:
      <ekz>
        junaj vo&ccirc;oj <tld/>adas kaj flustradas, kaj parope gejunuloj promenas sur solecaj
        vojetoj
        <fnt>
          <bib>Tor</bib>
          <lok>IV.</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>gurren</trd>,
      <trd>turteln</trd>
    </trdgrp>
    <trd lng="pl">grucha&cacute;</trd>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_r;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>cukrovat <klr>(hrdli&ccaron;ka)</klr></trd>,
    <trd>vrkat</trd>
  </trdgrp>
  <trd lng="en">coo</trd>
  <trd lng="fr">roucouler</trd>
  <trd lng="hu">turb&eacute;kol</trd>
  <trdgrp lng="ja">
    <trd>&#12463;&#12454;&#12463;&#12454;&#40180;&#12367;  <pr>&#12463;&#12454;&#12463;&#12454;&#12394;&#12367;</pr></trd>,
    <trd>&#29976;&#12356;&#35328;&#33865;&#12434;&#12373;&#12373;&#12420;&#12367;  <pr>&#12354;&#12414;&#12356;&#12371;&#12392;&#12400;&#12434;&#12373;&#12373;&#12420;&#12367;</pr></trd>
  </trdgrp>
  <trd lng="nl">kirren</trd>
  <trdgrp lng="pt">
    <trd>arrulhar</trd>,
    <trd>gorjear</trd>
  </trdgrp>
  <trd lng="ro">se giugiuli</trd>
  <trd lng="ru">&c_v;&c_o;&c_r;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">hrk&uacute;ta&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_o;&c_r;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_o;&c_r;&c_k;&c_o;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>(&c_p;&c_r;&c_o; &c_g;&c_o;&c_l;&c_u;&c_b;&c_ib;&c_v; &c_shch;&c_je;) &c_t;&c_u;&c_r;&c_k;&c_o;&c_t;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
</art>
