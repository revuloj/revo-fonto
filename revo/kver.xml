<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kver.xml,v 1.16 2018/10/28 23:10:16 revo Exp $">
<kap>
  <rad>kver</rad>/i <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kver.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kver.0i.SON">
    <uzo>(pri turtoj kaj kolomboj)</uzo>
    <dif>
      <ref tip="lst" cel="blek.0o" lst="voko:blekoj">Bleki</ref>
      per milda murmuro.
    </dif>
    <refgrp tip="vid">
      <ref cel="kurre.0.ZOO">kurre</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>gurren</trd>,
      <trd>girren</trd>
    </trdgrp>
    <trd lng="pl">grucha&cacute; <klr>(o go&lstroke;&eogonek;biu)</klr></trd>
  </snc>
  <snc mrk="kver.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ame interparoli:
      <ekz>
      ... kaj kverantaj vo&ccirc;oj de ridetantaj junulinoj...  Junaj vo&ccirc;oj
      kveradas kaj flustradas,...
    <fnt><aut>Stellan Engholm</aut>: <vrk>Al Torento</vrk>, <lok>&ccirc;apitro
    IV, 1930</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>gurren</trd>,
      <trd>turteln</trd>
    </trdgrp>
    <trd lng="pl">grucha&cacute;</trd>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_r;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>cukrovat <klr>(hrdli&ccaron;ka)</klr></trd>,
    <trd>vrkat</trd>
  </trdgrp>
  <trd lng="en">coo</trd>
  <trd lng="fr">roucouler</trd>
  <trd lng="hu">turb&eacute;kol</trd>
  <trd lng="nl">kirren</trd>
  <trdgrp lng="pt">
    <trd>arrulhar</trd>,
    <trd>gorjear</trd>
  </trdgrp>
  <trd lng="ro">se giugiuli</trd>
  <trd lng="ru">&c_v;&c_o;&c_r;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">hrk&uacute;ta&tcaron;</trd>
</drv>
</art>
