<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kver</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="kver.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Murmuri pp turtoj kaj kolomboj.
    </dif>
    <trdgrp lng="de">
      <trd>gurren</trd>,
      <trd>girren</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ame interparoli.
    </dif>
    <trdgrp lng="de">
      <trd>gurren</trd>,
      <trd>turteln</trd>
    </trdgrp>
  </snc>
  <trd lng="nl">kirren</trd>
  <trd lng="ru">&c_v;&c_o;&c_r;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
