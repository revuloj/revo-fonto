<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: glauxk1.xml,v 1.11 2025/09/22 20:32:35 revo Exp $">
<kap>
  <rad>gla&ubreve;k</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="glauxk1.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Bluverda, kiel iuj akvejoj:
      <ekz>
        servoknaboj altiris &ccirc;aron, kapu&ccirc;itan per <tld/>a silka dreliko
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 3a, p. 39a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>bluverda</ke>.
  </rim>

  <trd lng="be">&c_k;&c_o;&c_l;&c_je;&c_r;&c_u; &c_m;&c_a;&c_r;&c_s;&c_k;&c_o;&c_j; &c_h;&c_v;&c_a;&c_l;&c_ib;</trd>
  <trdgrp lng="ca">
    <trd>glauc</trd>,
    <trd>verd blanquin&oacute;s</trd>
  </trdgrp>
  <trd lng="de">blaugr&uuml;n</trd>
  <trd lng="fa">&a_A_madda;&a_b;&f_y; &a_z;&a_n;&f_g;&a_A;&a_r;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>glauque<klr> (adj., bleu-vert)</klr></trd>,
    <trd>bleu-vert<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="pl">niebieskozielony</trd>
  <trdgrp lng="pt">
   <trd>verde-mar<klr tip="ind"> (adj.)</klr></trd>,
   <trd>glauco<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_i;&c_n;&c_je;-&c_z;&c_je;&c_l;&c_jo;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_c;&c_v;&c_je;&c_t;&c_a; &c_m;&c_o;&c_r;&c_s;&c_k;&c_o;&c_j; &c_v;&c_o;&c_l;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&c_n;&c_mol;&c_o;-&c_z;&c_je;&c_l;&c_je;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_mol;&c_o;&c_r;&c_u; &c_m;&c_o;&c_r;&c_s;&c_mol;&c_k;&c_o;&c_ji; &c_v;&c_o;&c_d;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#34013;&#32511;&#33394; <pr>l&aacute;nl&uuml;s&egrave;</pr></trd>,
    <trd>&#27700;&#40232; <pr>shu&#464;y&amacron;</pr></trd>,
    <trd>&#34013;&#32511; <pr>l&aacute;nl&uuml;</pr></trd>,
    <trd>&#40167;&#32160; <pr>f&uacute;l&uuml;</pr></trd>,
    <trd>&#40232;&#32160;&#33394; <pr>y&amacron;l&uuml;s&egrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="glauxk1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="G"/>a <ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">koloro</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_l;&c_je;&c_r; &c_m;&c_a;&c_r;&c_s;&c_k;&c_o;&c_j; &c_h;&c_v;&c_a;&c_l;&c_ib;</trd>
  <trd lng="ca">color glauc</trd>
  <trd lng="fa">&a_A_madda;&a_b;&f_y; &a_z;&a_n;&f_g;&a_A;&a_r;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>glauque<klr> (subst., bleu-vert)</klr></trd>,
    <trd>bleu-vert<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="pl">kolor morski</trd>
  <trdgrp lng="pt">
   <trd>verde-mar<klr tip="ind"> (subst.)</klr></trd>,
   <trd>verde azulado<klr tip="ind"> (subst.)</klr></trd>,
   <trd>glauco<klr tip="ind"> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_c;&c_v;&c_je;&c_t; &c_m;&c_o;&c_r;&c_s;&c_k;&c_o;&c_j; &c_v;&c_o;&c_l;&c_n;&c_y;</trd>
</drv>
</art>

