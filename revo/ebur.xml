<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc><rad>ebur</rad>/o
</kap>

<drv mrk="ebur.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Blanka kaj malmola substanco, kiu konsistigas la dentegojn de
      elefanto kaj de kelkaj aliaj bestoj.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_l;&c_a;&c_n;&c_o;&c_v;&c_a;&c_ja;
  &c_k;&c_o;&c_s;&c_t;&c_k;&c_a;</trd>
  <trd lng="bg">&c_s;&c_l;&c_o;&c_n;&c_o;&c_v;&c_a;
  &c_k;&c_o;&c_s;&c_t;</trd>
  <trd lng="de">Elfenbein</trd>
  <trd lng="en">ivory</trd>
  <trd lng="es">marfil</trd>
  <trd lng="fr">ivoire<klr tip="ind"> (mati&egrave;re)</klr></trd>
  <trd lng="nl">ivoor</trd>
  <trd lng="hu">elef&aacute;ntcsont</trd>
  <trd lng="pl">ko&sacute;&cacute; <ind>s&lstroke;oniowa</ind></trd>
  <trd lng="pt">marfim</trd>
  <trd lng="ru">&c_s;&c_l;&c_o;&c_n;&c_o;&c_v;&c_a;&c_ja;
  &c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sv">elfenben</trd>
</drv>

<drv mrk="ebur.0a">
  <kap><tld/>a</kap>
  <snc num="1">
    <dif>
      El <tld/>o: <ekz><tld/>a statueto.
      </ekz>
    </dif>
    <trd lng="bg">&c_o;&c_t; <ind>&c_s;&c_l;&c_o;&c_n;&c_o;&c_v;&c_a;
    &c_k;&c_o;&c_s;&c_t;</ind></trd>
    <trd lng="de">elfenbeinern</trd>
    <trd lng="fr">en <ind>ivoire</ind></trd>
    <trd lng="nl">ivoren</trd>
    <trd lng="pl">z ko&sacute;ci <ind>s&lstroke;oniowej</ind></trd>
    <trdgrp lng="pt">
      <trd>eb&uacute;rneo</trd>,
      <trd>de <ind>marfim</ind></trd>,
      <trd>elefantino</trd>,
      <trd>eb&oacute;reo</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <dif>
      Kies koloro a&ubreve; &scirc;ajno similas tiun do <tld/>o:
      <ekz><tld/>aj &scirc;ultroj de virino;</ekz>
      <ekz><tld/>aj dentoj.</ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>ivoire<klr tip="ind"> (adj. de couleur)</klr></trd>,
      <trd>ivoirin</trd></trdgrp>
    <trd lng="nl">ivoorkleurig</trd>
    <trd lng="pl">wygl&aogonek;du <klr>(koloru) </klr>ko&sacute;ci
    <ind>s&lstroke;oniowej</ind></trd>
  </snc>
  <trd lng="de">elfenbeinern</trd>
  <trd lng="en">ivory</trd>
  <trd lng="hu">elef&aacute;ntcsont</trd>
  <trdgrp lng="pt">
    <trd>eb&uacute;rneo</trd>,
    <trd>de <ind>marfim</ind></trd>,
    <trd>eb&oacute;reo</trd>
  </trdgrp>
</drv>

<drv mrk="ebur.0bordo">
  <kap>
    <tld lit="E"/>-Bordo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="ebur.0bordo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la okcidenta <ref tip="malprt"
      cel="afrik.A0o">Afriko</ref>
      kun &ccirc;efurbo Jamusukro.
      Teritorio de Respubliko Ebur-Bordo.
      ISO-3166 kodo: CI.
      &Gcirc;i limas kun
      <ref tip="vid" cel="burkin.0o.GEOG">Burkino</ref>,
      <ref tip="vid" cel="gana.0o.GEOG">Ganao</ref>,
      <ref tip="vid" cel="gvine.0o.GEOG">Gvineo</ref>,
      <ref tip="vid" cel="liberi.0o.GEOG">Liberio</ref>,
      <ref tip="vid" cel="mali.0o.GEOG">Malio</ref>
      kaj <ref tip="vid" cel="atlant.0o.GEOG">Atlantiko</ref> (Gvinea
      Golfo).
    </dif>
    <trdgrp lng="pl">
      <trd>Wybrze&zdot;e Ko&sacute;ci S&lstroke;oniowej</trd>,
      <trd>Republika <ind>Wybrze&zdot;a Ko&sacute;ci
      S&lstroke;oniowej</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>
