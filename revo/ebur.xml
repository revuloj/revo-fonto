<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ebur.xml,v 1.38 2024/10/11 18:36:41 revo Exp $">
<kap>
  <ofc>2</ofc><rad>ebur</rad>/o <fnt><bib>JED</bib></fnt>
</kap>

<drv mrk="ebur.0o">
  <kap><tld/>o</kap>
  <snc mrk="ebur.0o.SUB">
    <dif>
      Blanka kaj malmola substanco, kiu konsistigas la dentegojn de
      elefanto kaj de kelkaj aliaj bestoj:
      <ekz>
        la re&gcirc;o faris grandan tronon el <tld/>o kaj tegis &gcirc;in per pura oro
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 10:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        la granda korporacio de ligna&jcirc;istoj <klr>[&#8230;]</klr> montris se&gcirc;ojn, tablojn,
        kanapojn <klr>[&#8230;]</klr> inkrustitajn per diverskolora ligno, perlamoto kaj
        <tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i estis bela knabino kun grekaj trajtoj kaj viza&gcirc;o kvaza&ubreve; el <tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_l;&c_a;&c_n;&c_o;&c_v;&c_a;&c_ja; &c_k;&c_o;&c_s;&c_t;&c_k;&c_a;</trd>
  <trd lng="bg">&c_s;&c_l;&c_o;&c_n;&c_o;&c_v;&c_a; &c_k;&c_o;&c_s;&c_t;</trd>
  <trd lng="ca">ivori</trd>
  <trdgrp lng="cs">
    <trd>ebur</trd>,
    <trd>slonovina</trd>,
    <trd>slonov&aacute; kost</trd>
  </trdgrp>
  <trd lng="de">Elfenbein</trd>
  <trd lng="en">ivory</trd>
  <trd lng="es">marfil</trd>
  <trd lng="fa">&a_ayn;&a_A;&a_j;</trd>
  <trd lng="fr">ivoire<klr tip="ind"> (mati&egrave;re)</klr></trd>
  <trd lng="fy">ivoar</trd>
  <trd lng="hu">elef&aacute;ntcsont</trd>
  <trd lng="id">gading</trd>
  <trd lng="io">ivoro</trd>
  <trd lng="ja">&#35937;&#29273; <pr>&#12382;&#12358;&#12370;</pr></trd>
  <trd lng="nl">ivoor</trd>
  <trd lng="pl">ko&sacute;&cacute; <ind>s&lstroke;oniowa</ind></trd>
  <trd lng="pt">marfim</trd>
  <trd lng="ru">&c_s;&c_l;&c_o;&c_n;&c_o;&c_v;&c_a;&c_ja; &c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">slonovina</trd>
  <trd lng="sv">elfenben</trd>
  <trd lng="uk">&c_s;&c_l;&c_o;&c_n;&c_o;&c_v;&c_a; &c_k;&c_ib;&c_s;&c_t;&c_mol;</trd>
  <trd lng="vo">vior</trd>
  <trdgrp lng="zh">
    <trd>&#35937;&#29273; <pr>xi&agrave;ngy&aacute;</pr></trd>,
    <trd>&#29273; <pr>y&aacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ebur.0a">
  <kap><tld/>a</kap>
  <snc mrk="ebur.0a.SUB">
    <dif>
      El <tld/>o:
      <ekz>
        <tld/>a statueto;
      </ekz>
      <ekz>
        sub la kapon oni metis <tld/>an apogilon, sur kiu ordinare dormas Egiptanoj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i &scirc;ovis la manon en la po&scirc;on kaj, elpreninte el &gcirc;i sufi&ccirc;e grandan <tld/>an
        monujon, volis jam malfermi &gcirc;in
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_a; &c_s;&c_l;&c_a;&c_n;&c_o;&c_v;&c_a;&c_j; &c_k;&c_o;&c_s;&c_t;&c_k;&c_ib;</trd>
    <trd lng="bg">&c_o;&c_t; <ind>&c_s;&c_l;&c_o;&c_n;&c_o;&c_v;&c_a;
    &c_k;&c_o;&c_s;&c_t;</ind></trd>
    <trd lng="fr">en <ind>ivoire</ind></trd>
    <trd lng="io">ivora</trd>
    <trd lng="ja">&#35937;&#29273;&#12398; <pr>&#12382;&#12358;&#12370;&#12398;</pr></trd>
    <trd lng="nl">ivoren</trd>
    <trd lng="pl">z ko&sacute;ci <ind>s&lstroke;oniowej</ind></trd>
    <trdgrp lng="pt">
      <trd>eb&uacute;rneo</trd>,
      <trd>de <ind>marfim</ind></trd>,
      <trd>elefantino</trd>,
      <trd>eb&oacute;reo</trd>
    </trdgrp>
  </snc>
  <snc mrk="ebur.0a.KOL">
    <dif>
      Kies <ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">koloro</ref> a&ubreve; &scirc;ajno similas
      tiun de <tld/>o:
      <ekz>
        <tld/>aj &scirc;ultroj de virino;
      </ekz>
      <ekz>
        <tld/>aj dentoj.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_o;&c_l;&c_je;&c_r;&c_u; &c_s;&c_l;&c_a;&c_n;&c_o;&c_v;&c_a;&c_j; &c_k;&c_o;&c_s;&c_t;&c_k;&c_ib;</trd>
    <trdgrp lng="de">
      <trd><ind>wei&szlig;</ind> wie Elfenbein</trd>,
      <trd><ind>bleich</ind> wie Elfenbein</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>ivoire<klr tip="ind"> (adj. de couleur)</klr></trd>,
      <trd>ivoirin</trd>
    </trdgrp>
    <trd lng="io">ivorea</trd>
    <trd lng="ja">&#35937;&#29273;&#33394;&#12398; <pr>&#12382;&#12358;&#12370;&#12356;&#12429;&#12398;</pr></trd>
    <trd lng="nl">ivoorkleurig</trd>
    <trd lng="pl">wygl&aogonek;du <klr>(koloru) </klr>ko&sacute;ci
    <ind>s&lstroke;oniowej</ind></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>eburov&yacute;</trd>,
    <trd>slonovinov&yacute;</trd>
  </trdgrp>
  <trd lng="de">elfenbeinern</trd>
  <trd lng="en">ivory</trd>
  <trd lng="hu">elef&aacute;ntcsont</trd>
  <trdgrp lng="pt">
    <trd>eb&uacute;rneo</trd>,
    <trd>de <ind>marfim</ind></trd>,
    <trd>eb&oacute;reo</trd>
  </trdgrp>
  <trd lng="sk">slonovinov&yacute;</trd>
  <trd lng="uk">&c_s;&c_l;&c_o;&c_n;&c_o;&c_v;&c_o;&c_ji; &c_k;&c_o;&c_s;&c_t;&c_ib;</trd>
  <trdgrp lng="zh">
    <trd>&#29273; <pr>y&aacute;</pr></trd>,
    <trd>&#35937;&#29273;&#33394; <pr>xi&agrave;ngy&aacute;s&egrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ebur.E0Bordo">
  <kap>
    <ofc>10</ofc><tld lit="E"/>-Bordo <fnt><vrk>Oficiala Informo de AdE</vrk>,
    <lok>numero 12</lok></fnt>, <var><kap><tld lit="E"/>bordo</kap></var>
  </kap>
  <mlg kod="ISO-3166">CI</mlg>
  <snc mrk="ebur.E0Bordo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la okcidenta <ref tip="malprt" cel="afrik.A0o">Afriko</ref> kun &ccirc;efurbo Jamusukro;
      teritorio de Respubliko <tld lit="E"/>-Bordo limanta kun <ref tip="vid"
      cel="burkin.B0o.GEOG">Burkino</ref>,
      <ref tip="vid" cel="gana.G0o.GEOG">Ganao</ref>, <ref tip="vid"
      cel="gvine.G0o.GEOG">Gvineo</ref>,
      <ref tip="vid" cel="liberi.L0o.GEOG">Liberio</ref>, <ref tip="vid"
      cel="mali.M0o.GEOG">Malio</ref>
      kaj <ref tip="vid" cel="atlant.A0o.GEOG">Atlantiko</ref>
      (Gvinea Golfo):
      <ekz>
        dum la IJK mi instruis mian denaskan lingvon <nom>Ewe</nom>, kiu estas parolata en la suda
        parto de <tld lit="E"/>bordo, Ganao, Togolando kaj Benino
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo &ndash; AEJK</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_K;&c_o;&c_t; &c_d;&#8217;&c_Ib;&c_v;&c_u;&c_a;&c_r;</trd>,
    <trd>&c_B;&c_je;&c_r;&c_a;&c_g; &c_S;&c_l;&c_a;&c_n;&c_o;&c_v;&c_a;&c_j; &c_K;&c_o;&c_s;&c_t;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="af">Ivoorkus</trd>
  <trd lng="ca">Costa d'Ivori</trd>
  <trd lng="da">Elfenbenskysten</trd>
  <trd lng="de">Elfenbeink&uuml;ste</trd>
  <trdgrp lng="en">
    <trd>Ivory Coast</trd>,
    <trd>C&ocirc;te d'Ivoire</trd>,
    <trd>Republic of C&ocirc;te d'Ivoire</trd>
  </trdgrp>
  <trd lng="es">Costa de Marfil</trd>
  <trd lng="eu">Boli Kosta</trd>
  <trd lng="fa">&a_s;&a_A;&a_H;&a_l; &a_ayn;&a_A;&a_j;</trd>
  <trd lng="fr">C&ocirc;te d'Ivoire</trd>
  <trd lng="fy">Ivoarkust</trd>
  <trd lng="he">&shin;&#1473;&#1462;&nun;&he;&#1464;&bet;</trd>
  <trd lng="hu">Elef&aacute;ntcsontpart</trd>
  <trd lng="id">Pantai Gading</trd>
  <trd lng="io">Ivora Rivo</trd>
  <trd lng="it">Costa d'Avorio</trd>
  <trd lng="ja">&#12467;&#12540;&#12488;&#12472;&#12508;&#12527;&#12540;&#12523;
  <pr>&#12371;&#12540;&#12392;&#12376;&#12412;&#12431;&#12540;&#12427;</pr></trd>
  <trd lng="ko">&#53076;&#53944;&#46356;&#48512;&#50500;&#47476;</trd>
  <trd lng="nl">Ivoorkust</trd>
  <trdgrp lng="pl">
    <trd>Wybrze&zdot;e Ko&sacute;ci S&lstroke;oniowej</trd>,
    <trd>Republika <ind>Wybrze&zdot;a Ko&sacute;ci S&lstroke;oniowej</ind></trd>
  </trdgrp>
  <trd lng="pt">Costa do Marfim</trd>
  <trd lng="ru">&c_K;&c_o;&c_t;-&c_d;'&c_I;&c_v;&c_u;&c_a;&c_r;</trd>
  <trd lng="sv">Elfenbenskusten</trd>
  <trd lng="tok">ma Kosiwa</trd>
  <trd lng="vo">Viorajol&auml;n</trd>
  <trdgrp lng="zh">
    <trd>&#38750;&#27954;&#35199;&#37096;&#22269;&#23478; <pr>f&emacron;izh&omacron;ux&imacron;b&ugrave;gu&oacute;ji&amacron;</pr></trd>,
    <trd>&#31185;&#29305;&#36842;&#29926; <pr>k&emacron;t&egrave;d&iacute;w&#462;</pr></trd>,
    <trd>&#35937;&#29273;&#28023;&#23736; <pr>xi&agrave;ngy&aacute;h&#462;i&agrave;n</pr></trd>
  </trdgrp>
</drv>
