<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>delic</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="delic.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Malofta, delikata sensa <ref tip="super" cel="plezur.0o">plezuro</ref>,
      kiun oni intense &gcirc;uas:
      <ekz>
        kun evidenta <tld/>o li glutis kelkajn ostrojn
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 25a, p. 262a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">del&iacute;cia</trd>
  <trd lng="es">delicia</trd>
  <trd lng="fr">d&eacute;lice</trd>
</drv>

<drv mrk="delic.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      &Gcirc;uinda, ka&ubreve;zanta grandan plezuron:
      <ekz>
        bone, ke oni &gcirc;uu la lunon sur la monto,
        tamen multe pli <tld/>e &ccirc;e akvo
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 76a, p. 600a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">delici&oacute;s</trd>
  <trd lng="es">delicioso</trd>
  <trd lng="fr">d&eacute;licieux</trd>
</drv>
</art>

