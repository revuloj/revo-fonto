<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: delic.xml,v 1.14 2024/01/29 09:23:18 revo Exp $">
<kap>
  <rad>delic</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="delic.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Malofta, delikata sensa <ref tip="super" cel="plezur.0o">plezuro</ref>,
      kiun oni intense &gcirc;uas:
      <ekz>
        kun evidenta <tld/>o li glutis kelkajn ostrojn
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 25a, p.
        262a</lok></fnt>;
      </ekz>
      <ekz>
        plorsingulti pro <tld/>o de spontanea, senbrida amorado, kiam &ccirc;iuj sensoj
        koncentri&gcirc;as al tiu unusola kapturniga traviva&jcirc;o
        <fnt>
          <bib>SkandalJozef</bib>
          <lok>Plendoj de Jozefo arestita</lok>
        </fnt>;
      </ekz>
      <ekz>
        gustumante ekzemple pladon da lokaj torteloj, kvadrataj, far&ccirc;itaj per selakto,
        kaj spinacoj, kondimentitaj per tomata sa&ubreve;co, kulminigu tiun <tld/>on per
        botelo da itala vino
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2012/010845.php">Saturnia: Vintra magio</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gxu.0o.plezuro">&gcirc;uado</ref>,
      <ref cel="volupt.0o">volupto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_s;&c_a;&c_l;&c_o;&c_d;&c_a;</trd>,
    <trd>&c_u;&c_c;&c_je;&c_h;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">del&iacute;cia</trd>
  <trdgrp lng="cs">
    <trd>libost</trd>,
    <trd>rozko&scaron;</trd>,
    <trd>slast</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Wonne</trd>,
    <trd>Wollust</trd>,
    <trd>Genuss</trd>,
    <trd>Sinneslust</trd>,
    <trd>sinnliches <ind>Vergn&uuml;gen</ind></trd>
  </trdgrp>
  <trd lng="es">delicia</trd>
  <trd lng="fr">d&eacute;lice</trd>
  <trdgrp lng="pl">
    <trd>delicja</trd>,
    <trd>przyjemno&sacute;&cacute;</trd>
  </trdgrp>
  <trd lng="pt">del&iacute;cia</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_s;&c_l;&c_a;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_o;&c_t;&c_r;&c_a;&c_d;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">rozko&scaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_a;&c_l;&c_o;&c_v;&c_zh;&c_i;&c_v;. &c_n;&c_a;&c_s;&c_o;&c_l;&c_o;&c_d;&c_a;</trd>,
    <trd>&c_u;&c_t;&c_ib;&c_h;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#33394;&#x80C6; <pr>s&#xE8;d&#x1CE;n</pr></trd>,
    <trd>&#x4E50;&#x8DA3; <pr>l&#xE8;q&#xF9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="delic.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="delic.0a.kauxzanta">
    <dif>
      <ref tip="super" cel="gxu.0inda">&Gcirc;uinda</ref>, ka&ubreve;zanta grandan plezuron:
      <ekz>
        bone, ke oni &gcirc;uu la lunon sur la monto,
        tamen multe pli <tld/>e &ccirc;e akvo
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 76a, p.
        600a</lok></fnt>;
      </ekz>
      <ekz>
        enpenetrante mi sentas infanecan &gcirc;ojon &ndash; <tld/>a varmo &ccirc;irka&ubreve;as &ndash; <klr>[&#8230;]</klr>
        kaj subita voluptego konvulsia akre trairas mian korpon
        <fnt>
          <bib>SkandalJozef</bib>
          <lok>Plendoj de Jozefo arestita</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>emo elvokas volupton, kaj la volupto &ccirc;iam estas kruela
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#8470;10</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi estas <tld/>a malgranda vivanta miraklo, nu, reiru do en la vastan maron
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 18</lok>
        </fnt>.
      </ekz>
    </dif>
<ref tip="vid" cel="plezur.0iga">plezuriga</ref>
  </snc>
  <snc mrk="delic.0a.esprimanta">
    <dif>
      Esprimanta &gcirc;uon, plezuron:
      <ekz>
        la jubilo kaj <tld/>aj krioj de la Kooperativestro fendis la transfenestran
        vespersilenton
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 4</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_je;&c_m;&c_n;&c_y;</trd>
  <trd lng="ca">delici&oacute;s</trd>
  <trd lng="cs">lahodn&yacute;</trd>
  <trdgrp lng="de">
    <trd>wohlig</trd>,
    <trd>wonnig</trd>,
    <trd>sinnlich</trd>,
    <trd>delizi&ouml;s</trd>
  </trdgrp>
  <trd lng="es">delicioso</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;licieux</trd>,
    <trd>exquis</trd>
  </trdgrp>
  <trd lng="pl">przyjemny</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_l;&c_je;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_v;&c_o;&c_s;&c_h;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_o;&c_s;&c_h;&c_i;&c_t;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">rozko&scaron;n&yacute;</trd>
  <trd lng="zh">&#x60EC; <pr>qi&#xE8;</pr></trd>
</drv>
</art>

