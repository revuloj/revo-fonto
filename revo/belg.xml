<kap>
  <ofc>8</ofc><rad>belg</rad>/o 
  <fnt><bib>FK</bib></fnt>
</kap>

<drv mrk="belg.0o">
  <kap><ofc>8</ofc><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="belg.B0ujo"><tld lit="B"/>ujo</ref>:
      <ekz>
        la prefero de <tld/>o bone man&gcirc;i kaj trinki trovas sian originon en la &leftquot;Ora&rightquot; 17a jarcento de
        Flandrio
        <fnt>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="&Monato;/2001/007144.php">Bierlando Belgio</url></vrk>
          <bib>Monato</bib>
          <lok>2001/01, p. 18</lok>
        </fnt>;
      </ekz>
      <ekz>
        dume kuzino eklo&gcirc;is en Bruselo, kaj &scirc;i estas de tiam entuziasma <tld/>o &ndash; se tiaj ekzistas
        <fnt>
          <bib>Monato</bib>
          <aut>Istv&aacute;n Ertl</aut>
          <vrk><url ref="&Monato;/2003/008140.html">Pri orienti&gcirc;o kaj okcidenti&gcirc;o</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Flandrio, regiono, en kiu vivas ses milionoj da nederlandlingvaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2005/008502.php">La Blok blokita ... a&ubreve; &ccirc;u?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_e;&c_l;&c_mol;&c_g;&c_ib;&c_je;&c_c;</trd>
  <trd lng="bg">&c_b;&c_je;&c_l;&c_g;&c_i;&c_je;&c_c;</trd>
  <trd lng="cs">Belgi&ccaron;an</trd>
  <trd lng="de">Belgier</trd>
  <trd lng="en">Belgian</trd>
  <trd lng="es">belga</trd>
  <trd lng="fr">Belge<klr> (subst.)</klr></trd>
  <trd lng="he">&bet;&lamed;&gimel;&yod;</trd>
  <trd lng="hu">belga</trd>
  <trdgrp lng="io">
    <trd>Belgiano</trd>,
    <trd>Belgo</trd>
  </trdgrp>
  <trd lng="ja">&#x30D9;&#x30EB;&#x30AE;&#x30FC;&#x4EBA;  <pr>&#x30D9;&#x30EB;&#x30AE;&#x30FC;&#x3058;&#x3093;</pr></trd>
  <trd lng="nl">Belg</trd>
  <trd lng="pl">Belg</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_mol;&c_g;&c_i;&c_je;&c_c;</trd>
  <trd lng="sk">Belgi&ccaron;an</trd>
  <trd lng="tok">jan Pesije</trd>
  <trd lng="uk">&c_b;&c_je;&c_l;&c_mol;&c_g;&c_ib;&c_jeu;&c_c;&c_mol;</trd>
  <trd lng="vo">Belg&auml;nan</trd>
</drv>

<drv mrk="belg.0a">
  <kap><tld/>a
    <fnt>
      <bib>Paroloj</bib>
      <vrk>Parolado anta&ubreve; la Sepa Kongreso Esperantista en Antwerpen en la 21a de a&ubreve;gusto 1911</vrk>
    </fnt>
  </kap>
  <snc>
    <dif>
      Rilata al <tld/>oj:
      <ekz>
        mi esprimas nian koran fratan dankon al tiuj niaj <tld/>aj samideanoj,
        kiuj tiel laboreme kaj zorge pretigis nian grandan feston
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sepa Kongreso Esperantista en Antwerpen en la 21a de a&ubreve;gusto
          1911</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="B"/>a konsulo en <nom>Buenos-Ayres</nom> sendis al sia registaro la sekvantajn
        sciigojn pri tiu &ccirc;i kolonio: <klr>[&#x2026;]</klr>
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u la <tld/>a re&gcirc;o Leopoldo la 2a estis vere <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Serge Zandandu Ntomono Zola/pg</aut>
          <vrk><url ref="&Monato;/2005/008608.php">Kiu estas Kabila?</url></vrk>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_e;&c_l;&c_mol;&c_g;&c_ib;&c_j;&c_s;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_b;&c_je;&c_l;&c_g;&c_i;&c_j;&c_s;&c_k;&c_i;</trd>
  <trd lng="de">belgisch</trd>
  <trd lng="en">Belgian</trd>
  <trd lng="es">belga</trd>
  <trd lng="fr">belge<klr> (adj.)</klr></trd>
  <trd lng="fy">Belgysk</trd>
  <trd lng="he">&bet;&lamed;&gimel;&yod;</trd>
  <trd lng="hu">belga</trd>
  <trdgrp lng="io">
    <trd>Belgiana</trd>,
    <trd>Belga</trd>
  </trdgrp>
  <trd lng="ja">&#x30D9;&#x30EB;&#x30AE;&#x30FC;&#x306E;  <pr>&#x3079;&#x308B;&#x304E;&#x30FC;&#x306E;</pr></trd>
  <trd lng="nl">Belgisch</trd>
  <trd lng="pl">belgijski</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_mol;&c_g;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">Belgick&yacute;</trd>
  <trd lng="uk">&c_b;&c_je;&c_l;&c_mol;&c_g;&c_ib;&c_j;&c_s;&c_mol;&c_k;&c_i;&c_j;</trd>
  <trd lng="vo">Belg&auml;nik</trd>
</drv>

<drv mrk="belg.B0ujo">
  <kap><tld lit="B"/>ujo <fnt><bib>ZR</bib></fnt></kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&Scirc;tato</ref> situanta en
      okcidenta <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref>
      kaj limanta okcidente al Norda Maro, norde al
      <ref tip="vid" cel="nederl.N0o">Nederlando</ref>, oriente
      al <ref tip="vid" cel="german1.G0ujo">Germanujo</ref>, sude al
      <ref tip="vid" cel="franc.F0ujo">Francujo</ref> kun
      &ccirc;efurbo
      <ref tip="prt" cel="brusel.B0o.urb">Bruselo</ref>:
      <ekz>
        Svisujo, <tld lit="B"/>ujo, A&ubreve;strujo, Kanado, <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Dogmoj de Hilelismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; en paca <tld lit="B"/>ujo, en Antverpeno 1911, 
        la kongresa parolado devis esti senkolora pro proksimeco al pli potencaj &scirc;tatoj
        <fnt>
          <bib>VivZam</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="belgi.B0o">Belgio</ref>
    <rim>
      Z en Lingvaj Respondoj skribas:
      <ekz>
        estas vero, ke ekzistas tri E&ubreve;ropaj regnoj, por kiuj la sufikso 
        &leftquot;uj&rightquot; estas ne tute logika (A&ubreve;strujo, <tld lit="B"/>ujo kaj Svisujo), 
        kaj se la Akademio trovus necesa anstata&ubreve;igi en ili la &leftquot;uj&rightquot; per &leftquot;i&rightquot;, mi tion ne malkonsilus;
        sed fari propravole escepton por tiuj tri regnoj mi al niaj verkistoj ne konsilus, 
        &ccirc;ar la Esperantistoj jam tro forte alkutimi&gcirc;is al la &gcirc;isnunaj, neniom &gcirc;enantaj
        nomoj de tiuj tre gravaj kaj ofte citataj regnoj, kaj por fari rompon 
        en tiu alkutimi&gcirc;o mi ne vidas ian gravan neceson
        <fnt>
          <bib>LR</bib>
          <lok>2. Vortuzo</lok>
        </fnt>.
      </ekz>
  </rim>
  </snc>
  <trd lng="af">Belgi&euml;</trd>
  <trd lng="be">&c_B;&c_e;&c_l;&c_mol;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_B;&c_je;&c_l;&c_g;&c_i;&c_ja;</trd>
  <trd lng="ca">B&egrave;lgica</trd>
  <trd lng="cs">Belgie</trd>
  <trd lng="da">Belgien</trd>
  <trd lng="de">Belgien</trd>
  <trd lng="el">&Beta;&epsilon_acute;&lambda;&gamma;&jota;&omikron;</trd>
  <trd lng="en">Belgium</trd>
  <trd lng="es">B&eacute;lgica</trd>
  <trd lng="fi">Belgia</trd>
  <trdgrp lng="fr">
    <trd>Belgique</trd>,
    <trd>Royaume de Belgique</trd>
  </trdgrp>
  <trd lng="fy">Belgje</trd>
  <trd lng="gd">an <ind>Bheilg</ind></trd>
  <trd lng="he">&bet;&lamed;&gimel;&yod;&he;</trd>
  <trd lng="hu">Belgium</trd>
  <trd lng="ia">Belgica</trd>
  <trd lng="id">Belgia</trd>
  <trd lng="io">Belgia</trd>
  <trd lng="is">Belg&iacute;a</trd>
  <trd lng="it">Belgio</trd>
  <trd lng="lat">Belgica</trd>
  <trd lng="lt">Belgija</trd>
  <trd lng="lv">Be&lcommaaccent;&gcommaaccent;ija</trd>
  <trd lng="nl">Belgi&euml;</trd>
  <trd lng="no">Belgia</trd>
  <trd lng="pl">Belgia</trd>
  <trd lng="pt">B&eacute;lgica</trd>
  <trd lng="ru">&c_B;&c_je;&c_l;&c_mol;&c_g;&c_i;&c_ja;</trd>
  <trd lng="sk">Belgicko</trd>
  <trd lng="sv">Belgien</trd>
  <trd lng="tok">ma Pesije</trd>
  <trd lng="tr">Bel&ccedil;ika</trd>
  <trd lng="vo">Belg&auml;n</trd>
</drv>
