<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">

<kap>
  <ofc>8</ofc><rad>belg</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="belg.0o">
  <kap><ofc>8</ofc><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="belg.B0ujo"><tld lit="B"/>ujo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_e;&c_l;&c_mol;&c_g;&c_ib;&c_je;&c_c;</trd>
  <trd lng="bg">&c_b;&c_je;&c_l;&c_g;&c_i;&c_je;&c_c;</trd>
  <trd lng="cs">Belgi&ccaron;an</trd>
  <trd lng="de">Belgier</trd>
  <trd lng="en">Belgian</trd>
  <trd lng="es">belga</trd>
  <trd lng="fr">Belge<klr> (subst.)</klr></trd>
  <trd lng="he">&bet;&lamed;&gimel;&yod;</trd>
  <trd lng="hu">belga</trd>
  <trdgrp lng="io">
    <trd>Belgiano</trd>,
    <trd>Belgo</trd>
  </trdgrp>
  <trd lng="nl">Belg</trd>
  <trd lng="pl">Belg</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_mol;&c_g;&c_i;&c_je;&c_c;</trd>
  <trd lng="sk">Belgi&ccaron;an</trd>
  <trd lng="tp">jan Pesije</trd>
  <trd lng="vo">Belg&auml;nan</trd>
</drv>

<drv mrk="belg.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al la &scirc;tatanoj de <tld lit="B"/>ujo.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_e;&c_l;&c_mol;&c_g;&c_ib;&c_j;&c_s;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_b;&c_je;&c_l;&c_g;&c_i;&c_j;&c_s;&c_k;&c_i;</trd>
  <trd lng="de">belgisch</trd>
  <trd lng="en">Belgian</trd>
  <trd lng="es">belga</trd>
  <trd lng="fr">belge<klr> (adj.)</klr></trd>
  <trd lng="fy">Belgysk</trd>
  <trd lng="he">&bet;&lamed;&gimel;&yod;</trd>
  <trd lng="hu">belga</trd>
  <trdgrp lng="io">
    <trd>Belgiana</trd>,
    <trd>Belga</trd>
  </trdgrp>
  <trd lng="nl">Belgisch</trd>
  <trd lng="pl">belgijski</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_mol;&c_g;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">Belgick&yacute;</trd>
  <trd lng="vo">Belg&auml;nik</trd>
</drv>

<drv mrk="belg.B0ujo">
  <kap><tld lit="B"/>ujo <fnt><bib>ZR</bib></fnt></kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o">&Scirc;tato</ref> situanta en
      okcidenta <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref>
      kaj limanta okcidente al Norda Maro, norde al
      <ref tip="vid" cel="nederl.N0o">Nederlando</ref>, oriente
      al <ref tip="vid" cel="german1.G0ujo">Germanujo</ref>, sude al
      <ref tip="vid" cel="franc.F0ujo">Francujo</ref> kun
      <ref tip="vid" cel="urb.cxef0o">&ccirc;efurbo</ref>
      <ref tip="prt" cel="brusel.B0o.urb">Bruselo</ref>.
    </dif>
    <ref tip="sin" cel="belgi.B0o">Belgio</ref>
  </snc>
  <trd lng="af">Belgi&euml;</trd>
  <trd lng="be">&c_B;&c_e;&c_l;&c_mol;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_B;&c_je;&c_l;&c_g;&c_i;&c_ja;</trd>
  <trd lng="ca">B&egrave;lgica</trd>
  <trd lng="cs">Belgie</trd>
  <trd lng="da">Belgien</trd>
  <trd lng="de">Belgien</trd>
  <trd lng="el">&Beta;&epsilon_acute;&lambda;&gamma;&jota;&omikron;</trd>
  <trd lng="en">Belgium</trd>
  <trd lng="es">B&eacute;lgica</trd>
  <trd lng="fi">Belgia</trd>
  <trdgrp lng="fr">
    <trd>Belgique</trd>,
    <trd>Royaume de Belgique</trd>
  </trdgrp>
  <trd lng="fy">Belgje</trd>
  <trd lng="gd">an <ind>Bheilg</ind></trd>
  <trd lng="he">&bet;&lamed;&gimel;&yod;&he;</trd>
  <trd lng="hu">Belgium</trd>
  <trd lng="ia">Belgica</trd>
  <trd lng="id">Belgia</trd>
  <trd lng="io">Belgia</trd>
  <trd lng="is">Belg&iacute;a</trd>
  <trd lng="it">Belgio</trd>
  <trd lng="lat">Belgica</trd>
  <trd lng="lt">Belgija</trd>
  <trd lng="lv">Be&#316;&#291;ija</trd>
  <trd lng="nl">Belgi&euml;</trd>
  <trd lng="no">Belgia</trd>
  <trd lng="pl">Belgia</trd>
  <trd lng="pt">B&eacute;lgica</trd>
  <trd lng="ru">&c_B;e&c_l;&c_mol;&c_g;&c_i;&c_ja;</trd>
  <trd lng="ru">&c_B;&c_je;&c_l;&c_mol;&c_g;&c_i;&c_ja;</trd>
  <trd lng="sk">Belgicko</trd>
  <trd lng="sv">Belgien</trd>
  <trd lng="tp">ma Pesije</trd>
  <trd lng="tr">Bel&ccedil;ika</trd>
  <trd lng="vo">Belg&auml;n</trd>
</drv>
</art>

