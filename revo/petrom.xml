<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>petromiz</rad>/o
</kap>
<drv mrk="petrom.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la klaso
      <ref tip="super" cel="busx.rond0uloj">rondbu&scirc;uloj</ref>
      (<trd lng="la">Petromyzon</trd>), man&gcirc;ebla angilforma
      marfi&scirc;o kun okuloj kaj sep paroj de brankotruoj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Neunauge</trd>,
    <trd>Bricke</trd>,
    <trd>Lamprete</trd>
  </trdgrp>
  <trd lng="en">lamprey</trd>
  <trdgrp lng="es">
    <trd>lamprea</trd>,
    <trd>morena</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>lamproie</trd>,
    <trd>marbr&eacute;e</trd>
  </trdgrp>
  <trd lng="it">lampreda</trd>
  <trdgrp lng="nl">
    <trd>lamprei</trd>,
    <trd>negenoog</trd>
  </trdgrp>
  <trd lng="pt">lampreia</trd>
  <trd lng="ru">&c_m;&c_i;&c_n;&c_o;&c_g;&c_a;</trd>
</drv>
</art>
