<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: real1.xml,v 1.16 2023/10/20 19:20:37 revo Exp $">
<kap>
  <rad>real</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="real1.0o">
  <kap><tld/>o</kap>
  <snc mrk="real1.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Iu el diversaj antikvaj <ref tip="super" cel="mon.0ero">monero</ref>j 
      uzataj dum multaj jaroj en hispan- kaj portugal-lingvaj landoj kun diversaj valoroj kaj
      diversaj
      specifaj nomoj en diversaj epokoj:
      <ekz>
        mi a&ccirc;etis de la knabo la tuton da paperoj kaj kajeroj por
        duono de <tld/>o
        <fnt>
        <bib>DonKihxoto</bib>,
        <lok>parto 1a, &ccirc;apitro 9a, p. 74a</lok>
        </fnt>.
      </ekz>
    </dif>
    <subsnc mrk="real1.0o.brazila">
      <uzo tip="fak">EKON</uzo>
      <dif>
        <ref tip="super" cel="unu.mon0o">Monunuo</ref> de Brazilo.
      </dif>
    </subsnc>
    <ref tip="hom" cel="real.0o">realo</ref>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_a;&c_l;</trd>
  <trdgrp lng="cs">
    <trd>hmotn&yacute; sv&ecaron;t</trd>,
    <trd>re&aacute;lno</trd>,
    <trd>skute&ccaron;no</trd>
  </trdgrp>
  <trd lng="de">Real</trd>
  <trd lng="en">real</trd>
  <trd lng="es">real</trd>
  <trd lng="fa">&a_r;&a_y_hamza;&a_A;&a_l;</trd>
  <trd lng="fr">r&eacute;al<klr> (monnaie)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x73FE;&#x5B9F;  <pr>&#x3052;&#x3093;&#x3058;&#x3064;</pr></trd>,
    <trd>&#x5B9F;&#x5728;  <pr>&#x3058;&#x3064;&#x3056;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="pl">real</trd>
  <trd lng="pt">real</trd>
  <trd lng="ru">&c_r;&c_je;&c_a;&c_l;</trd>
  <trdgrp lng="sk">
    <trd>hmotn&yacute; svet</trd>,
    <trd>re&aacute;lno</trd>,
    <trd>skuto&ccaron;no</trd>
  </trdgrp>
  <trd lng="uk">&#x43D;&#x443;&#x43C;&#x456;&#x437;&#x43C;. &#x440;&#x435;&#x430;&#x43B;</trd>
</drv>
</art>
