<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>real</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="real1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="unu.mon0o">Monunuo</ref> uzita aux uzata
      en hispan- kaj portugal-lingvaj landoj:
      <ekz>
        mi a&ccirc;etis de la knabo la tuton da paperoj kaj kajeroj por duono de
        <tld/>o<fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 9a, p.
        74a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="hom" cel="real.0o">realo</ref>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_a;&c_l;</trd>
  <trd lng="de">Real</trd>
  <trd lng="es">real</trd>
  <trd lng="fr">r&eacute;al<klr> (monnaie)</klr></trd>
  <trd lng="ru">&c_r;&c_je;&c_a;&c_l;</trd>
</drv>
</art>

