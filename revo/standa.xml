<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>standard</rad>/o
</kap>
<drv mrk="standa.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Flago servanta kiel solena militista, religia, nacia a&ubreve; partia
      simbolo:
      <ekz>
        la verda <tld/>o estas io pli ol simpla signo de
        lingvo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        forte grupi&gcirc;i anta&ubreve; sia <tld/>o<fnt>Z</fnt>
        <klr>(f: tute harmonie kaj interkonsente kunigi siajn
        fortojn)</klr>;
      </ekz>
      <ekz>
        arbaro da <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
     <trd>&c_sh;&c_t;&c_a;&c_n;&c_d;&c_a;&c_r;</trd>, 
     <trd>&c_s;&c_mol;&c_c;&c_ja;&c_g;</trd>, 
     <trd>&c_h;&c_a;&c_r;&c_u;&c_g;&c_v;&c_a;</trd>, 
     <trd>&c_f;&c_l;&c_a;&c_g;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Standarte</trd>,
    <trd>Banner</trd>
  </trdgrp>
  <trd lng="en">flag</trd>
  <trdgrp lng="hu">
    <trd>z&aacute;szl&oacute;</trd>,
    <trd>lobog&oacute;</trd>
  </trdgrp>
  <trd lng="nl">vlag</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_n;&c_a;&c_m;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_ja;&c_g;</trd>,
    <trd>&c_sh;&c_t;&c_a;&c_n;&c_d;&c_a;&c_r;&c_t;</trd>
  </trdgrp>
</drv>
</art>
