<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: standa.xml,v 1.16 2018/06/01 16:10:13 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>standard</rad>/o
</kap>
<drv mrk="standa.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Flago servanta kiel solena militista, religia, nacia a&ubreve;
      partia simbolo:
      <ekz>
        la verda <tld/>o estas io pli ol simpla signo de
        lingvo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        forte grupi&gcirc;i anta&ubreve; sia <tld/>o<fnt>Z</fnt>
        <klr>(f: tute harmonie kaj interkonsente kunigi siajn
        fortojn)</klr>;
      </ekz>
      <ekz>
        arbaro da <tld/>oj.
      </ekz>
    </dif>
    <rim> Ne konfuzu kun <ref cel="norm.0o">normo</ref>.</rim>
  </snc>
  <trdgrp lng="be">
     <trd>&c_sh;&c_t;&c_a;&c_n;&c_d;&c_a;&c_r;</trd>, 
     <trd>&c_s;&c_mol;&c_c;&c_ja;&c_g;</trd>, 
     <trd>&c_h;&c_a;&c_r;&c_u;&c_g;&c_v;&c_a;</trd>, 
     <trd>&c_f;&c_l;&c_a;&c_g;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>korouhev</trd>,
    <trd>prapor</trd>,
    <trd>z&aacute;stava</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Standarte</trd>,
    <trd>Banner</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>banner</trd>,
    <trd>standard</trd>,
    <trd>flag</trd>
  </trdgrp>
  <trd lng="es">estandarte</trd>
  <trd lng="fr">&eacute;tandard</trd>
  <trdgrp lng="hu">
    <trd>z&aacute;szl&oacute;</trd>,
    <trd>lobog&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>banier</trd>,
    <trd>standaard</trd>,
    <trd>vaan</trd>,
    <trd>vaandel</trd>,
    <trd>veldteken</trd>,
    <trd>vendel</trd>,
    <trd>wimpel</trd>,
    <trd>vlag</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>sztandar</trd>,
    <trd>chor&aogonek;giew</trd>,
    <trd>bandera</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_n;&c_a;&c_m;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_ja;&c_g;</trd>,
    <trd>&c_sh;&c_t;&c_a;&c_n;&c_d;&c_a;&c_r;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>z&aacute;stava</trd>,
    <trd>&scaron;tandarda</trd>
  </trdgrp>
</drv>
</art>
