<kap>
  <ofc>2</ofc>
  <rad>petard</rad>/o
</kap>
<drv mrk="petard.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Malforta eksplodilo, konsistanta el pulvo entenata en kartona tubo
      kaj uzata kiel signalo a&ubreve; amuza&jcirc;o:
      <ekz>
        la&ubreve;mite aperas jarfine minacanta besto, &gcirc;i estu forpelita per <tld/>oj:
        eksplodigi fajra&jcirc;ojn fari&gcirc;is do kutima printempfesta amuza&jcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Chen Lina/pg</aut>
          <vrk><url ref="&Monato;/2007/009185.php">&Scirc;an&gcirc;i&gcirc;anta feli&ccirc;o-festo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;io &scirc;ajnas preta, kiam zeloto serba &jcirc;etas <tld/>ojn kaj aliajn objektojn sur la
        kampon <klr>[&#x2026;]</klr> videblas krome serbaj fanatikuloj, kiuj bruligas
        albanan flagon
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2011/010198.php">Futbalejo a&ubreve; &ccirc;u batalejo?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="eksplo.0ajxo">eksploda&jcirc;o</ref>,
       <ref cel="pirote.0ajxo">piroteknika&jcirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_t;&c_a;&c_r;&c_d;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>petarda</trd>,
    <trd>sign&aacute;ln&iacute; raketa</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Knallk&ouml;rper</trd>,
    <trd>Knaller</trd>,
    <trd>B&ouml;ller</trd>,
    <trd>Signalmunition</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>petard</trd>,
    <trd>detonator</trd>,
    <trd>firecracker</trd>
  </trdgrp>
  <trd lng="es">petardo</trd>
  <trd lng="fr">p&eacute;tard</trd>
  <trd lng="hu">pet&aacute;rda</trd>
  <trdgrp lng="ja">
    <trd>&#x7206;&#x7AF9;  [<ind>&#x3070;&#x304F;&#x3061;&#x304F;</ind>]</trd>,
    <trd>&#x767A;&#x96F7;&#x4FE1;&#x53F7;  [<ind>&#x306F;&#x3064;&#x304B;&#x307F;&#x306A;&#x308A;&#x3057;&#x3093;&#x3054;&#x3046;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">voetzoeker</trd>
  <trd lng="pl">petarda</trd>
  <trd lng="ro">petard&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_t;&c_a;&c_r;&c_d;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_r;&c_y;&c_v;&c_n;&c_a;&c_ja; &c_sh;&c_a;&c_sh;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>alebo z&aacute;bavn&aacute; n&aacute;lo&zcaron;ka</trd>,
    <trd>signaliza&ccaron;n&aacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x435;&#x442;&#x430;&#x440;&#x434;&#x430;</trd>,
    <trd>&#x43F;&#x456;&#x434;&#x440;&#x438;&#x432;&#x43D;&#x430; &#x448;&#x430;&#x448;&#x43A;&#x430;</trd>,
    <trd>&#x445;&#x43B;&#x43E;&#x43F;&#x430;&#x432;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>
