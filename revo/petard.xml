<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>petard</rad>/o
</kap>
<drv mrk="petard.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Malforta eksplodilo, konsistanta el pulvo entenata en kartona tubo
      kaj uzata kiel signalo aŭ amuzaĵo.
    </dif>
  </snc>
  <trd lng="be">пэтарда</trd>
  <trd lng="de">Feuerwerkskörper</trd>
  <trdgrp lng="en">
    <trd>petard</trd>,
    <trd>detonator</trd>,
    <trd>firecracker</trd>
  </trdgrp>
  <trd lng="hu">petárda</trd>
  <trd lng="nl">voetzoeker</trd>
  <trdgrp lng="ru">
    <trd>петарда</trd>,
    <trd>подрывная шашка</trd>
  </trdgrp>
</drv>
</art>
