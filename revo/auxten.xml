<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>a&ubreve;tent</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="auxten.0a">
  <kap><tld/>a</kap>
  <snc>
    <ref tip="dif" cel="auxten1.0a">a&ubreve;tentika</ref>
    <ekz>
      en la <tld/>a <ctl>Pinyin</ctl>, tonmarkoj estas integra parto de
      literumado<fnt><aut>Yamasaki Seik&ocirc;</aut>: <vrk>Transskribo de la
      &ccirc;ina</vrk>, <bib>Monato</bib><lok>jaro 1999a, numero 4a, p.
      20a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="de">
    <trd>authentisch</trd>,
    <trd>echt</trd>
  </trdgrp>
  <trd lng="es">aut&eacute;ntico</trd>
  <trd lng="fr">authentique</trd>
  <trdgrp lng="hu">
    <trd>hiteles</trd>,
    <trd>val&oacute;di</trd>,
    <trd>autentikus</trd>
  </trdgrp>
  <trd lng="sk">autentick&yacute;</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_d;&c_l;&c_i;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_d;&c_o;&c_s;&c_t;&c_o;&c_v;&c_je;&c_r;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_a;&c_u;&c_t;&c_je;&c_n;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
</drv>

<drv mrk="auxten.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <ref tip="dif" cel="auxten1.0eco">a&ubreve;tentikeco</ref>
    <ekz>
      estis ege pla&ccirc;a ideo de la a&ubreve;toro, &gcirc;enerale preni la pli originajn
      nomo-formojn: tio vere kontribuas al sento pri
      <tld/>eco<fnt><aut>Gerrit Berveling</aut>: <vrk>Historia romano pri
      Jesuo (Manuel de Seabra, La tago kiam Jesuo perfidis Judason)</vrk>,
      <bib>Monato</bib><lok>jaro 2001a, numero 11a, p. 28a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="de">
    <trd>Authentizit&auml;t</trd>,
    <trd>Echtheit</trd>
  </trdgrp>
  <trd lng="es">autenticidad</trd>
  <trd lng="fr">authenticit&eacute;</trd>
  <trdgrp lng="hu">
    <trd>hiteless&eacute;g</trd>,
    <trd>val&oacute;dis&aacute;g</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_l;&c_i;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_o;&c_s;&c_t;&c_o;&c_v;&c_je;&c_r;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_a;&c_u;&c_t;&c_je;&c_n;&c_t;&c_i;&c_ch;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="auxten.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <ref tip="dif" cel="auxten1.0igi">a&ubreve;tentikigi</ref>
  </snc>
    <trdgrp lng="de">
      <trd>authentifizieren</trd>,
      <trd>Echtheit best&auml;tigen</trd>
    </trdgrp>
    <trd lng="es">autentificar</trd>
    <trd lng="fr">authentifier</trd>
    <trd lng="hu">hiteles&iacute;tr</trd>
    <trdgrp lng="ru">
      <trd>&c_u;&c_d;&c_o;&c_s;&c_t;&c_o;&c_v;&c_je;&c_r;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_u;&c_d;&c_o;&c_s;&c_t;&c_o;&c_v;&c_je;&c_r;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_v;&c_je;&c_r;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_v;&c_je;&c_r;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_s;&c_v;&c_i;&c_d;&c_je;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
</drv>
</art>

