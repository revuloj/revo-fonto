<kap>
   <ofc>9</ofc><rad>tank</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tank.0o">
   <kap><ofc>9</ofc><tld/>o</kap>
   <uzo tip="fak">MIL</uzo>
   <snc>
     <dif>
       Speco de kirasita a&ubreve;tomobilo sin movanta per
       turni&gcirc;antaj lar&gcirc;aj &ccirc;enoj anstata&ubreve; radoj:
       <ekz>
         la unuaj <tld/>oj aperis dum la mondmilito en la jaro 1916.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_t;&c_a;&c_n;&c_k;</trd>
   <trd lng="bg">&c_t;&c_a;&c_n;&c_k;</trd>
   <trd lng="ca">tanc</trd>
   <trd lng="de">Panzer</trd>
   <trd lng="en">tank</trd>
   <trd lng="es">tanque</trd>
   <trd lng="fa">&a_t;&a_A;&a_n;&f_k;</trd>
   <trdgrp lng="hu">
     <trd>harckocsi</trd>,
     <trd>tank</trd>
   </trdgrp>
   <trd lng="it">carro armato</trd>
   <trdgrp lng="ja">
    <trd>&#x6226;&#x8ECA;  <pr>&#x305B;&#x3093;&#x3057;&#x3083;</pr></trd>,
    <trd>&#x30BF;&#x30F3;&#x30AF;  <pr>&#x305F;&#x3093;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">tank</trd>
   <trdgrp lng="pl">
     <trd>czo&lstroke;g</trd>,
     <trd>tank<klr> (rzadkie)</klr></trd>
   </trdgrp>
   <trd lng="pt">tanque</trd>
   <trd lng="ru">&c_t;&c_a;&c_n;&c_k;</trd>
   <trdgrp lng="fr"><trd>char d'assaut</trd><trd>tank</trd></trdgrp>
  <trd lng="uk">&#x442;&#x430;&#x43D;&#x43A;</trd>
</drv>
