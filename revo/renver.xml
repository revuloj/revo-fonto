<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: renver.xml,v 1.27 2021/05/29 14:33:10 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>renvers</rad>/i
</kap>
<drv mrk="renver.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="renver.0i.faligi">
    <dif>
      <ref tip="super" cel="fal.0igi">Faligi</ref> ion a&ubreve; iun starantan, 
      tiel ke la supro estas malsupre:
      <ekz>
        <tld/>i inkujon, meblon, muron;
      </ekz>
      <ekz>
        la tablo staras malrekte kaj kredeble balda&ubreve;
        <tld/>i&gcirc;os
        <fnt>
          <bib>F</bib>
          <lok>&FE; 33</lok>
        </fnt>;
      </ekz>
      <ekz>
        pano ruli&gcirc;is tra <klr>[&#x2026;]</klr> tendo, kaj ekfrapis &gcirc;in tiel, ke &gcirc;i falis, kaj
        <tld/>is &gcirc;in, kaj la tendo tute ku&scirc;is
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 7:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni <tld/>as montojn de ilia bazo
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 28:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi estas tute sendefenda, infano povas min <tld/>i
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni havas gubernian madejron: &gcirc;i ne imponas per ekstera&jcirc;o, sed e&ccirc; elefanton &gcirc;i
        <tld/>os
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        li alsendu vinon la plej bonan, alie mi <tld/>os lian tutan kelon
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        el surprizo pro tiu &ccirc;i demando <klr>[li]</klr> preska&ubreve; <tld/>is la pleton de teo
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la ludantaj delfenoj sin <tld/>adis
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiam la &scirc;ipo <tld/>i&gcirc;as, oni ne havas tempon diskuti pri la remistoj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>.
      </ekz>
<!--
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind><tld/>i ministraron</ind>, re&gcirc;on;
        <trd lng="hu">megd&ouml;nt korm&aacute;nyt</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la veturilo <ind><tld/>i&gcirc;i</ind>s;
        <trd lng="hu">felborul</trd>
      </ekz>
-->
    </dif>
    <refgrp tip="vid">
       <ref cel="sxance.0i.MOV">&scirc;anceli<sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>umkippen</trd>,
      <trd>umwerfen</trd>,
      <trd>kentern</trd>,
      <trd>umst&uuml;rzen</trd>,
    </trdgrp>
    <trdgrp lng="hu">
      <trd>felbor&iacute;t</trd>,
      <trd>feld&ouml;nt</trd>,
      <trd>led&ouml;nt</trd>
    </trdgrp>    
  </snc>
  <snc mrk="renver.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="ruin.0igi.detrui">Ruinigi</ref>, 
      <ref tip="sin" cel="detru.0i">detrui</ref>, <ref cel="ekster1.0i">ekstermi</ref>:
      <ekz>
        Mi ne <tld/>os la urbon
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 19:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malvirtuloj <tld/>i&gcirc;os kaj pereos, sed la domo de virtuloj
        staros forte
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 12:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        du tiaj homoj, kiel mi, ruinige <tld/>us la tutan konstruon de la morala mondo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio &ccirc;i <tld/>as la tre vastigitan opinion
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_r;&c_y;&c_n;&c_u;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_k;&c_ib;&c_n;&c_u;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_mol;&c_v;&c_je;&c_r;&c_g;&c_n;&c_u;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_b;&c_v;&c_je;&c_r;&c_g;&c_n;&c_u;&c_c;&c_mol; <klr>(opinion)</klr></trd>
    </trdgrp>
    <trd lng="de">st&uuml;rzen</trd>
    <trd lng="hu">felford&iacute;t</trd>    
    <trd lng="ru">&c_n;&c_i;&c_s;&c_p;&c_r;&c_o;&c_v;&c_je;&c_r;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="renver.0i.inversigi">
    <dif>
      <ref tip="dif" cel="invers.0igi.KOMUNE">Inversigi</ref>:
      <ekz>
        <tld/>i la ordon de listo;
      </ekz>
      <ekz>
        <tld/>i la randon de &ccirc;apelo.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>umdrehen</trd>,
      <trd>umst&uuml;lpen</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kiford&iacute;t</trd>,
      <trd>megford&iacute;t</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_je;&c_r;&c_a;&c_k;&c_u;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_v;&c_ja;&c_r;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_ja;&c_r;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>klopit</trd>,
    <trd>k&aacute;cet</trd>,
    <trd>podtrhnout</trd>,
    <trd>p&rcaron;eklopit</trd>,
    <trd>p&rcaron;ekl&aacute;p&ecaron;t</trd>,
    <trd>p&rcaron;evrhnout</trd>,
    <trd>p&rcaron;evr&aacute;tit</trd>,
    <trd>rozvr&aacute;tit</trd>,
    <trd>sk&aacute;cet</trd>,
    <trd>svalit</trd>,
    <trd>zbavit moci</trd>,
    <trd>zvr&aacute;tit</trd>
  </trdgrp>  
  <trdgrp lng="en">
    <trd>overturn</trd>
    <trd>upset</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>basculer</trd>,
    <trd>renverser</trd>,
    <trd>retourner</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>omgooien</trd>,
    <trd>ten <ind>val</ind> brengen</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>revirar</trd>,
    <trd>deitar por terra</trd>,
    <trd>derribar</trd>,
    <trd>entornar</trd>, 
    <trd>tombar</trd>    
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_p;&c_r;&c_o;&c_k;&c_i;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>obr&aacute;ti&tcaron;</trd>,
    <trd>preklopi&tcaron;</trd>,
    <trd>prevali&tcaron;</trd>,
    <trd>prevrhn&uacute;&tcaron;</trd>,
    <trd>prevr&aacute;ti&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="renver.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 14</lok></fnt>
  <snc mrk="renver.0ajxo.permuto">
    <dif>
      <klr>(de <ref cel="permut.0o.bijekcio">permuto</ref> 
      <frm am="sigma">&sigma;</frm>
      super <ref cel="ar.orda0o">orda aro</ref>)</klr>
      &Ccirc;iu tia duopo <frm am="(i,j)">(<k>i</k>,<k>j</k>)</frm>, ke 
      <frm am="i lt j"><k>i</k> &lt; <k>j</k></frm>, kaj 
      <frm am="sigma(i) gt sigma(j)">&sigma;(<k>i</k>) &gt; &sigma;(<k>j</k>)</frm>:
      <ekz>
        la permuto <frm am="abc to acb"><k>abc</k>&#x2192;<k>acb</k></frm> enhavas unu 
        <tld/>a&jcirc;on, nome <frm am="(b,c)">(<k>b</k>,<k>c</k>)</frm>.
      </ekz>
    </dif>
    <rim>
      Eblas diri anka&ubreve;, ke temas pri la duopo 
      <frm am="(sigma(i),sigma(j))">(&sigma;(<k>i</k>),&sigma;(<k>j</k>))</frm>,
      kiel faras <nom>Bricard</nom>.
    </rim>
    <trd lng="be">&c_b;&c_je;&c_s;&c_p;&c_a;&c_r;&c_a;&c_d;&c_a;&c_k;</trd>
    <trdgrp lng="de">
      <trd>Inversion</trd>,
      <trd>Derangement</trd>
    </trdgrp>
    <trd lng="en">inversion<klr tip="ind"> (in a
    permutation)</klr></trd>
    <trdgrp lng="fr">
      <trd>inversion<klr tip="ind"> (dans une permutation)</klr></trd>,
      <trd>d&eacute;rangement<klr tip="ind"> (dans une permutation)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>inverzi&oacute;<klr tip="ind"> (permut&aacute;ci&oacute;ban)</klr></trd>,
      <trd>megford&iacute;t&aacute;s<klr tip="ind"> (permut&aacute;ci&oacute;ban)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_je;&c_s;&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_k;</trd>
  </snc>
</drv>

<drv mrk="renver.0igxi">
  <kap>
    <tld/>i&gcirc;i
  </kap>
  <snc>
    <dif>
      Esti <tld/>ata:
      <ekz>
        la tablo staras malrekte kaj kredeble balda&ubreve; <tld/>i&gcirc;os
        <fnt>
          <bib>F</bib>
          <lok>&FE; 33</lok>
        </fnt>;
      </ekz>
      <ekz>
        li kolektas siajn fortojn kaj <klr>[&#x2026;]</klr> pu&scirc;as la veturilon kun tia forto,
        ke &gcirc;i <tld/>i&gcirc;as sur alian flankon
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        lia man&gcirc;a&jcirc;o <tld/>i&gcirc;os en liaj interna&jcirc;oj
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 20:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        per la morto de nia kara frato <tld/>i&gcirc;is la ordo <klr>[&#x2026;]</klr> en nia lando
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;anceli&gcirc;anta konstrua&jcirc;o ne bezonas tertremon, por <tld/>i&gcirc;i
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        Petro ofte ne bone a&ubreve;dadis, la memoro senkontrola ne bone tenadis, kaj tiel la
        vortoj <tld/>i&gcirc;adis kaj akceptadis diversajn sova&gcirc;ajn formojn
        <fnt>
          <bib>LR</bib>
          <lok>Pri la regulo de la akcento</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
<trd lng="bo">&#xF58;&#xF42;&#xF7C;&#xF0B;&#xF62;&#xF9F;&#xF72;&#xF44;&#xF0B;&#xF63;&#xF7C;&#xF42;&#xF0B;</trd>
  <trdgrp lng="de">
    <trd>umfallen</trd>,
    <trd>umst&uuml;rzen <klr>(fallen)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="renver.sxtat0o">
  <kap>&scirc;tat<tld/>o</kap>
  <snc mrk="renver.sxtat0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Subita kaj perforta forigo de re&gcirc;imo, &ccirc;u fare de militistoj per
      <ref tip="sub" cel="pucx.0o">pu&ccirc;o</ref>, &ccirc;u fare de la popolo per
      <ref tip="sub" cel="revolu.0o.POL">revolucio</ref>:
      <ekz>
        Tajlando havas longan tradicion de &scirc;tat<tld/>oj
        <fnt>
          <aut>G. G&auml;llmo</aut>,
          <vrk>Tajlando: denove &scirc;tatrenverso</vrk>,
          <lok><url ref="http://raporto.info">Raporto.info</url>, 
          2006-09-20</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la historio de Ni&gcirc;erio el &ccirc;iuj vo&ccirc;donoj rezultis krizoj kaj poste &scirc;tat<tld/>o
        fare de la ni&gcirc;eria armeo
        <fnt>
          <bib>Monato</bib>
          <aut>Princo Henriko Oguinye</aut>
          <vrk><url ref="https://www.monato.be/2007/009250.php">Elektoj sen elektiloj</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Putsch</trd>,
    <trd>Staatsstreich</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>coup d'&eacute;tat</trd>,
    <trd>putch</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&aacute;llamcs&iacute;ny</trd>,
    <trd>puccs</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>staatsgreep</trd>,
    <trd>putch</trd>
  </trdgrp>
  <trd lng="no">kuppfors&oslash;k</trd>
  <trd lng="pt">golpe de <klr tip="amb">estado</klr></trd> 
  <trd lng="ru">&c_p;&c_u;&c_t;&c_ch;</trd>
</drv>
</art>
