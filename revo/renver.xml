<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>renvers</rad>/i
</kap>
<drv mrk="renver.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Faligi ion a&ubreve; iun starantan:
      <ekz>
        <tld/>i inkujon, meblon, muron;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i ministraron, re&gcirc;on;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la tablo staras malrekte kaj kredeble balda&ubreve;
        <tld/>i&gcirc;os<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la veturilo <tld/>i&gcirc;is;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la &scirc;ipo <tld/>i&gcirc;is sur la rivero.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>umkippen</trd>,
      <trd>umwerfen</trd>,
      <trd>umst&uuml;lpen</trd>,
      <trd>umst&uuml;rzen</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ruinigi, detrui, ekstermi:
      <ekz>
        tio &ccirc;i <tld/>as tre vastigitan opinion;
      </ekz>
      <ekz>
        la malvirtuloj <tld/>i&gcirc;os kaj pereos, sed la domo de virtuloj
        staros forte<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_n;&c_i;&c_s;&c_p;&c_r;&c_o;&c_v;&c_je;&c_r;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Inversigi:
      <ekz>
        <tld/>i la ordon de listo;
      </ekz>
      <ekz>
        <tld/>i la randon de &ccirc;apelo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">umdrehen</trd>
  <trdgrp lng="en">
    <trd>overturn</trd>
    <trd>upset</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>omgooien</trd>,
    <trd>ten <ind>val</ind> brengen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_p;&c_r;&c_o;&c_k;&c_i;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
