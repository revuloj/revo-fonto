<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>renvers</rad>/i
</kap>
<drv mrk="renver.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Faligi ion a&ubreve; iun starantan:
      <ekz>
        <tld/>i inkujon, meblon, muron;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i ministraron, re&gcirc;on;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la tablo staras malrekte kaj kredeble balda&ubreve;
        <tld/>i&gcirc;os<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la veturilo <tld/>i&gcirc;is;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la &scirc;ipo <tld/>i&gcirc;is sur la rivero.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>umkippen</trd>,
      <trd>umwerfen</trd>,
      <trd>umst&uuml;lpen</trd>,
      <trd>umst&uuml;rzen</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ruinigi, detrui, ekstermi:
      <ekz>
        tio &ccirc;i <tld/>as tre vastigitan opinion;
      </ekz>
      <ekz>
        la malvirtuloj <tld/>i&gcirc;os kaj pereos, sed la domo de virtuloj
        staros forte<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_n;&c_i;&c_s;&c_p;&c_r;&c_o;&c_v;&c_je;&c_r;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Inversigi:
      <ekz>
        <tld/>i la ordon de listo;
      </ekz>
      <ekz>
        <tld/>i la randon de &ccirc;apelo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">umdrehen</trd>
  <trdgrp lng="en">
    <trd>overturn</trd>
    <trd>upset</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>omgooien</trd>,
    <trd>ten <ind>val</ind> brengen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_p;&c_r;&c_o;&c_k;&c_i;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="renver.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 14</lok></fnt>
  <snc mrk="renver.0ajxo.permuto">
    <dif>
      <klr>(de <ref cel="permut.0o">permuto</ref> &sigma; super
      <ref cel="ar.orda0o">orda aro</ref>)</klr>
      &Ccirc;iu tia duopo (i,j), ke i&lt;j, kaj &sigma;(i)&gt;&sigma;(j):
      <ekz>
        la permuto abc&#x2192;bca enhavas unu <tld/>a&jcirc;on, nome (b,c).
      </ekz>
    </dif>
    <rim>
      Eblas diri anka&ubreve;, ke temas pri la duopo (&sigma;(i),&sigma;(j)),
      kiel faras Bricard.
    </rim>
    <trdgrp lng="de">
      <trd>Inversion</trd>,
      <trd>Derangement</trd>
    </trdgrp>
    <trd lng="en">inversion<klr tip="ind"> (in a permutation)</klr></trd>
    <trdgrp lng="fr">
      <trd>inversion<klr tip="ind"> (dans une permutation)</klr></trd>,
      <trd>d&eacute;rangement<klr tip="ind"> (dans une permutation)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_je;&c_s;&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_k;</trd>
  </snc>
</drv>

</art>
