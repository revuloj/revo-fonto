<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: markiz.xml,v 1.27 2024/01/25 20:10:34 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>markiz</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="markiz.0o">
  <kap><tld/>o</kap>
  <snc mrk="markiz.0o.rango">
    <dif>
      <ref tip="super" cel="nobel.0o">Nobelo</ref>
      kies rango estas inter tiuj de  
      <ref cel="duk.0o">duko</ref> kaj <ref cel="graf.0o">grafo</ref>:
      <ekz>
        helpon! helpon! sinjoro <tld/>o de <nom>Karaba</nom> dronas
        <fnt>
          <aut>C. Perrault, trad. P. Sarpy</aut>,
          <vrk>La Majstro Kato</vrk>, 
          <lok><url ref="http://eo.wikibooks.org/wiki/Rakontoj_pri_Feinoj">Rakontoj
              pri feinoj</url>, Hachette, 1904</lok></fnt>;
      </ekz>
      <ekz>
        inter ili staris la <tld/>o <nom>Louis de Beaufront</nom>, unua propagandisto en Francujo
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        privata lernejo en kamparo de unu el la vasalaj <tld/>oj sub la &scirc;oguno
        <fnt>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url ref="&Monato;/2000/006909.php">Novjara kla&ccirc;o super konjako</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 25</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    Ne konfuzu kun <ref tip="hom" cel="markez.0o">markezo</ref>.
  </rim>
  <trd lng="be">&c_m;&c_a;&c_r;&c_k;&c_ib;&c_z;</trd>
  <trd lng="bg">&c_m;&c_a;&c_r;&c_k;&c_i;&c_z;</trd>
  <trd lng="br">markiz</trd>
  <trd lng="ca">marqu&eacute;s</trd>
  <trd lng="cs">mark&yacute;z</trd>
  <trdgrp lng="de">
    <trd>Markgraf</trd>,
    <trd>Marquis</trd>
  </trdgrp>
  <trd lng="en">marquis</trd>
  <trd lng="fr">marquis</trd>
  <trdgrp lng="hu">
    <trd>&odblac;rgr&oacute;f</trd>,
    <trd>m&aacute;rki</trd>
  </trdgrp>
  <trd lng="ja">&#x4FAF;&#x7235;  <pr>&#x3053;&#x3046;&#x3057;&#x3083;&#x304F;</pr></trd>
  <trd lng="nl">markies</trd>
  <trd lng="pt">marqu&ecirc;s</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_r;&c_k;&c_i;&c_z;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_k;&c_g;&c_r;&c_a;&c_f;</trd>
  </trdgrp>
  <trd lng="sk">mark&iacute;z</trd>
  <trd lng="uk">&#x43C;&c_a;&c_r;&#x43A;&c_ib;&c_z;</trd>
  <trdgrp lng="zh">
    <trd>&#x4FAF;&#x7235; <pr>h&#xF3;uju&#xE9;</pr></trd>,
    <trd>&#x738B;&#x7237; <pr>w&#xE1;ngye</pr></trd>,
    <trd>&#x738B;&#x723A; <pr>w&#xE1;ngye</pr></trd>
  </trdgrp>
</drv>

<drv mrk="markiz.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="markiz.0ujo.limregiono">
    <dif>
      <ref tip="super" cel="feuxd.0o">Fe&ubreve;da</ref> a&ubreve; administra unuo, 
      kiel tiu estrata de <tld/>o, limregiono de <ref tip="malprt" cel="regn.0o.POL">regno</ref>:
      <ekz>
        la nordaj kristanaj &scirc;tatoj jam estis plivasti&gcirc;antaj el
        siaj unuaj teritorioj en Galicio, Asturio, E&ubreve;skio kaj la
        Hispana <tld/>ujo
        <fnt>
          <vrk><url ref="&Viki;/Al-Andalus">Al-Andalus</url></vrk>. <bib>Viki</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">marquesat</trd>
  <trd lng="de">Mark <klr>(Grenzgebiet)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_r;&c_k;&c_i;&c_z;&c_a;&c_t;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_k;&c_g;&c_r;&c_a;&c_f;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8FB9;&#x7586; <pr>bi&#x101;nji&#x101;ng</pr></trd>,
    <trd>&#x908A;&#x7586; <pr>bi&#x101;nji&#x101;ng</pr></trd>
  </trdgrp>
</drv>

</art>
