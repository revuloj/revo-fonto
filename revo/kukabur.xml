<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kukabur.xml,v 1.7 2023/12/05 19:19:39 revo Exp $">
<kap>
<rad>kukabur</rad>/o
</kap>
<drv mrk="kukabur.0o">
  <kap><tld/>o</kap>
  <snc mrk="kukabur.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda <ref tip="lst" lst="voko:birdoj" cel="bird.rabo0oj">rabobirdo</ref>
      el familio <ref tip="malprt" cel="alcion1.0oj">alcionedoj</ref>
      kun ridosimila bleko vivanta en A&ubreve;stralio kaj Novgvineo
      (<trd lng="la">Dacelo novaeguineae</trd>):
      <ekz>
        la programo komenci&gcirc;is per registrita gakado de <tld/>o
        <fnt>
          <aut>Ralph Harry</aut>,
          <vrk><url ref="http://www.luin.se/inko/X24-6.pdf">La Diplomato
          Kiu Ridis</url></vrk>,
          <lok>1997</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni &gcirc;uis eksterordinaran spektaklon de <klr>[&#8230;]</klr> diversaj konataj
        rabobirdoj, sed anka&ubreve; papagoj, vulturoj, <tld/>o, kiu devenas el A&ubreve;stralio
        kaj Novgvineo kaj iom similas al alciono
        <fnt>
          <aut>Liba Gabalda</aut>
          <vrk><url ref="https://poludnica.esperanto.sk/index.php?akcia=e-vojagoj-2021&amp;co=kvinpetalo"
              >Praktikado de la lingvo en Kvinpetalo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/e/e1/Dacelo_novaeguineae_-Sydney%2C_Australia_-frog_in_beak-8.jpg/634px-Dacelo_novaeguineae_-Sydney%2C_Australia_-frog_in_beak-8.jpg"
      prm="CC BY 2.0"
      ><tld/>o kaptinta ranon, apud Sidnejo, A&ubreve;stralio
        <fnt>
          <aut>Richard Taylor</aut>
          <lok><url ref="&WCW;/File:Dacelo_novaeguineae_-Sydney,_Australia_-frog_in_beak-8.jpg"
              >https://commons.wikimedia.org/wiki/File:Dacelo_novaeguineae_-Sydney,_Australia_-frog_in_beak-8.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="de">J&auml;gerliest</trd>
  <trd lng="en">kookaburra</trd>
  <trdgrp lng="fr">
    <trd>kookaburra</trd>,
    <trd>martin-chasseur g&eacute;ant</trd>
  </trdgrp>
  <trd lng="pl">kukabura</trd>
  <trd lng="pt">kookaburra</trd>
</drv>
</art>
