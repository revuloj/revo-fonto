<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sangri.xml,v 1.11 2022/03/31 08:39:22 revo Exp $">
<kap>
  <rad>sangri</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="sangri.0o">
  <kap><tld/>o</kap>
  <snc mrk="sangri.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="alkoho.0ajxo.KUI">Alkohola&jcirc;o</ref> el ru&gcirc;a
      
      vino, fruktoj a&ubreve; fruktosukoj, eventuale sodakvo, sukero,
      brando... 
      la&ubreve; hispandevena recepto:
      <ekz>
        kia drinkado! La <tld/>o kaj cidro neniam fini&gcirc;is!
        <fnt>
          <aut>Araneo</aut>,
          <vrk><url ref="http://www.esperanto-es.net/forumo/forum_posts.asp?TID=7">HEJS
          renkonti&gcirc;oj...</url></vrk>,
          <lok>2003-12-16</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_n;&c_g;&c_r;&c_y;&c_ja;</trd>
  <trd lng="cs">sangria</trd>
  <trdgrp lng="de">
    <trd>Sangr&iacute;a</trd>,
    <trd>Sangria</trd>
  </trdgrp>
  <trd lng="es">sangr&iacute;a</trd>
  <trd lng="fa">&a_s;&a_A;&a_n;&f_g;&a_r;&f_y;&a_A;</trd>
  <trd lng="fr">sangria</trd>
  <trd lng="it">sangria</trd>
  <trd lng="pl">Sangria</trd>
  <trd lng="sk">sangria</trd>
  <trd lng="uk">&c_s;&c_a;&#x43D;&c_g;&c_r;&c_ib;&#x44F;</trd>
</drv>
</art>

