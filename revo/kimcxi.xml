<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kimcxi.xml,v 1.15 2025/09/08 13:03:38 revo Exp $">
<kap>
  <rad>kim&ccirc;i</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="kimcxi.0o">
  <kap><tld/>o</kap>
  <snc mrk="kimcxi.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Korea man&gcirc;a&jcirc;o el brasiko, ajlo, aliaj legomoj kaj spicoj longe
      fermentintaj en granda vazo:
      <ekz>
        krome trovi&gcirc;as kruda fi&scirc;o, miso-supo, riza&jcirc;o kun koka&jcirc;o kaj
        ova&jcirc;o,
        <tld/>o
        <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>&leftquot;solidareco de
        stomakoj&rightquot;, p.
        46a</lok></fnt>;
      </ekz>
      <ekz>
        la plej konata alman&gcirc;a&jcirc;o estas <tld/>o, kiu estas fermentigita el pekina brasiko, akra
        papriko, ajlo, rafano kaj aliaj spicoj
        <fnt>
          <aut>CHOE Taesok</aut>
          <vrk>Eksciu pri la lando de matena kvieto</vrk>
          <bib>Esperanto</bib>
          <lok>2017:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni bone konis <tld/>on <klr>[...]</klr>, sed dum tradicia man&gcirc;ado oni ofte ricevas multajn
        kromajn teleretojn kun diversaj aliaj legomoj, faboj, algoj, rizoj, nudeloj, supoj ktp por
        dividi
        <fnt>
          <aut>Russ Williams</aut>
          <vrk>Vegana voja&gcirc;o en Koreio &mdash; kaj utilaj metodoj por man&gcirc;i vegetare a&ubreve; vegane</vrk>
          <bib>Kon11</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="brasik.acida_0o">acida brasiko</ref>,
      <ref cel="sauxrk.0o">sa&ubreve;rkra&ubreve;to</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_m;&c_ch;&c_y;</trd>
  <trd lng="bg">&c_k;&c_i;&c_m;&c_ch;&c_i;</trd>
  <trd lng="cs">kim&ccaron;i</trd>
  <trd lng="de">Kimchi</trd>
  <trd lng="en">kimchi</trd>
  <trdgrp lng="es">
    <trd>kimchi</trd>,
    <trd>col fermentada</trd>,
    <trd>chucrut</trd>
  </trdgrp>
  <trd lng="fr">kimchi</trd>
  <trd lng="hu">kimcsi</trd>
  <trd lng="it">kimchi</trd>
  <trd lng="ko">&#44608;&#52824;</trd>
  <trd lng="ja">&#x30AD;&#x30E0;&#x30C1;</trd>
  <trdgrp lng="nl">
    <trd>kimchi</trd>,
    <trd>kimtsji</trd>
  </trdgrp>
  <trd lng="pl">kimchi</trd>
  <trd lng="ro">kimchi</trd>
  <trd lng="ru">&c_k;&c_i;&c_m;&c_ch;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x97D3;&#x5F0F;&#x6CE1;&#x83DC; <pr>H&#xE1;nsh&#xEC;p&#xE0;oc&#xE0;i</pr></trd>,
    <trd>&#x8F9B;&#22855; <pr>x&#x12B;nq&#xED;</pr></trd>,
    <trd>&#x6CE1;&#x83DC; <pr>p&#xE0;oc&#xE0;i</pr></trd>,
    <trd>&#x97D3;&#x570B;&#x6CE1;&#x83DC; <pr>H&#xE1;ngu&#xF3;p&#xE0;oc&#xE0;i</pr></trd>
  </trdgrp>
</drv>
</art>

