    </trdgrp>
  </snc>
  <snc mrk="dec.0e.merite">
    <dif>
      Kiel <tld/>as<sncref ref="dec.0i.meriti"/>:
      <ekz>
        venos al ni kelke da tre eminentaj fremduloj, <klr>[&#8230;]</klr> tial la maljuna
        re&gcirc;o <klr>[&#8230;]</klr> volas <tld/>e sin montri
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi sufi&ccirc;e longe esploris la man&gcirc;on kreska&jcirc;an kaj mi lernis <tld/>e estimi &gcirc;in
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        de tempo tiel longa mi ne manprenis remilojn, ke mi ne scias &ccirc;u mi povos ankora&ubreve;
        <tld/>e remi
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i estis tiel malbona <klr>[&#8230;]</klr>, oni devis &scirc;in <tld/>e
        puni
        <fnt>
          <bib>Fab4</bib>
          <lok>Knabino, kiu pa&scirc;is sur panon</lok>
        </fnt>!
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_y;&c_g;&c_o;&c_d;&c_n;&c_a;</trd>,
      <trd>&c_p;&c_r;&c_y;&c_d;&c_a;&c_t;&c_n;&c_a;</trd>,
