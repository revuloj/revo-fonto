<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: eksklamaci.xml,v 1.6 2022/03/16 15:54:27 revo Exp $">
<kap>
  <rad>eksklamaci</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="eksklamaci.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="eksklamaci.0o.BELE">
    <uzo tip="fak">BELE</uzo>
    <dif>
      <ref tip="vid" cel="patos.0a.BELA">Patosa</ref>
      ekkria, pasia esprimo:
      <ekz>
        li <klr>...</klr> a&ubreve;skultas la amajn <tld/>ojn de sia 
        amiko
        <fnt>
          <aut>A. Pu&scirc;kin, trad. A. Kor&jcirc;enkov</aut>,
          <vrk><url ref="http://www.esperanto-ondo.ru/Libroj/V_bibl01.pdf">
          La pika damo</url></vrk>,
          <lok>2003-2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>exklamace</trd>,
    <trd>zvol&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="fr">exclamation<klr> (rh&eacute;torique)</klr></trd>
  <trdgrp lng="pl">
    <trd>eksklamacja</trd>,
    <trd>okrzyk</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>exklam&aacute;cia</trd>,
    <trd>zvolanie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x438;&#x433;&#x443;&#x43A;</trd>,
    <trd>&#x43E;&#x43A;&#x43B;&#x438;&#x43A; (&#x443; &#x43F;&#x440;&#x43E;&#x43C;&#x43E;&#x432;&#x456;)</trd>
  </trdgrp>
</drv>
</art>

