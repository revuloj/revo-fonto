<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: memora.xml,v 1.16 2022/03/23 14:01:45 revo Exp $">
<kap>
  <rad>memorand</rad>/o
</kap>

<drv mrk="memora.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">POL</uzo>
    <dif>
      Mallongigita <ref tip="super" cel="raport.0o.dokumenta">raporto</ref>
      aldonita al diplomata noto; <ref tip="sin" cel="not.pri0o">prinoto</ref>:
      <ekz>
        la japana ministrejo pri fremdaj aferoj tion konfirmis per sia Detala <tld/>o
        pri la diplomatiaj rilatoj kun Koreio
        <fnt>
          <bib>Monato</bib>
          <aut>Ma Young-tae</aut>
          <vrk><url ref="&Monato;/2005/008651.php">Tokto mankas takto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        oni trovis anka&ubreve; <tld/>on de la <nom>Bush</nom>-teamo, verkitan anta&ubreve; lia elekti&gcirc;o kiel
        prezidanto, kiu asertis: &leftquot;ni bezonas novan <nom>Pearl Harbor</nom>&rightquot;
        <fnt>
          <bib>Monato</bib>
          <aut>Ian Fantom</aut>
          <lok><url ref="&Monato;/2007/009196.php">2007</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o pri kompreno inter la ministerioj
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2014/011304.php">Amikaj registaroj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        subskribita estas <tld/>o inter la financministerioj por likvidi pruntojn
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2015/011492.php">Temoj traktataj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="memuar.0o">memuaro</ref>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_m;&c_a;&c_r;&c_a;&c_n;&c_d;&c_u;&c_m;</trd>
  <trd lng="bg">&c_m;&c_je;&c_m;&c_o;&c_r;&c_a;&c_n;&c_d;&c_u;&c_m;</trd>
  <trdgrp lng="de">
    <trd>Memorandum</trd>,
    <trd>Denkschrift</trd>,
    <trd>Stellungnahme</trd>,
    <trd>Memo</trd>
  </trdgrp>
  <trd lng="en">memorandum</trd>
  <trdgrp lng="fa">
    <trd>&f_y;&a_A;&a_d;&a_d;&a_A;&a_sh;&a_t;<klr tip="amb"> (&a_d;&f_y;&f_p;&a_l;&a_m;&a_A;&a_t;&f_y;&f_k;)</klr></trd>,
    <trd>&a_t;&a_dh;&f_k;&a_A;&a_r;&f_y;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">m&eacute;morandum</trd>
  <trd lng="hu">memorandum</trd>
  <trd lng="nl">memorandum</trd>
  <trdgrp lng="pt">
    <trd>memorando <klr tip="amb">(diplom&aacute;tico)</klr></trd>,
    <trd>memor&acirc;ndum <klr tip="amb">(diplom&aacute;tico)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_je;&c_m;&c_o;&c_r;&c_a;&c_n;&c_d;&c_u;&c_m;</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x435;&#x43C;&#x43E;&#x440;&#x430;&#x43D;&#x434;&#x443;&#x43C;</trd>,
    <trd>&#x434;&#x438;&#x43F;&#x43B;&#x43E;&#x43C;&#x430;&#x442;&#x438;&#x447;&#x43D;&#x430; &#x43D;&#x43E;&#x442;&#x430;</trd>
  </trdgrp>
</drv>
</art>
