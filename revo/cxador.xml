<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cxador.xml,v 1.10 2023/08/31 10:10:27 revo Exp $">
<kap>
  <rad>&ccirc;ador</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>

<drv mrk="cxador.0o">
  <kap><tld/>o</kap>
  <snc mrk="cxador.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Ina eksterdoma <ref tip="super" cel="vest.kap0o.TEKS">kapvesto</ref>
      en Irano kaj aliaj landoj, duonronda tuko, kiu kovras la
      hararon kaj pendas malanta&ubreve;e:
      <ekz>
        &scirc;i kovris sin per <tld/>o kaj eliris en la straton
        <fnt>
          <aut>Mahbube Mirkadiri</aut>,
          <vrk>Donaco el voja&gcirc;o</vrk>,
          <bib>LOdE</bib>,
          <lok>2003:3 (101)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mantil.0o.TEKS">mantilo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_d;&c_r;&c_a;</trd>
  <trd lng="de">Tschador</trd>
  <trd lng="fa">&f_ch;&a_A;&a_d;&a_r;<klr tip="ind"> (&f_p;&a_w;&a_sh;&a_sh;
  &a_z;&a_n;&a_A;&a_n;&a_h;)</klr></trd>
  <trd lng="fr">tchador</trd>
  <trd lng="ja">&#x30C1;&#x30E3;&#x30C9;&#x30EB;</trd>
  <trdgrp lng="pl">
    <trd>czadra</trd>,
    <trd>czador</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>xador (&oacute;)</trd>,
    <trd>chador (&oacute;) (evi)</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_a;&c_d;&c_r;&c_a;</trd>
 <trd lng="it">chador</trd>
  <trd lng="uk">&#x447;&#x430;&#x434;&#x440;&#x430;</trd>
</drv>
</art>

