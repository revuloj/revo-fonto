<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vaux.xml,v 1.8 2024/04/13 22:21:51 revo Exp $">
<kap>
  <rad>va&ubreve;</rad> <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="vaux.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="vaux.0.SON">
    <dif>
      Sono de hunda <ref tip="lst" cel="blek.0o"
      lst="voko:blekoj">bleko</ref>;
      <ref tip="dif" cel="boj.0.SON">boj</ref>:
      <ekz>
         li komencis boji, kaj bojis kiel vera hundo:
         <tld/>-<tld/>-<tld/>-<tld/>
         <fnt>
           <aut>C. Collodi, trad. G. Croatini</aut>,
           <vrk><url
           ref="http://www.vortaro.hu/mindenesszotar/jozohor/pinokjo_elemei/pin22.htm">
           La aventuroj de Pinokjo</url></vrk>,
           <lok>[2002]</lok>
         </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_w;!</trd>,
    <trd>&c_d;&c_z;&c_ja;&c_w;!</trd>,
    <trd>&c_c;&c_ja;&c_w;!</trd>
  </trdgrp>
  <trd lng="fr">ouah !<klr> (chien)</klr></trd>
  <trd lng="hu">vau</trd>
  <trd lng="id">guk</trd>
  <trd lng="pl">hau</trd>
  <trd lng="ru">&c_g;&c_a;&c_v;</trd>
</drv>
</art>

