<art mrk="$Id$">
<kap>
<rad>va&ubreve;</rad>
</kap>
<drv mrk="vaux.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc>
    <dif>
      Sono de hunda <ref tip="lst" cel="blek.0o"
      lst="voko:blekoj">bleko</ref>:
       <ekz><tld/>-<tld/></ekz>
    </dif>
    <ref tip="sin" cel="baux1.0">ba&ubreve;</ref>
  </snc>
  <trd lng="hu">vau</trd>
  <trd lng="ru">&c_g;&c_a;&c_v;</trd>
</drv>
</art>

