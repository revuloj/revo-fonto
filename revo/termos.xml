<kap>
  <rad>termos</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="termos.0o">
  <kap><tld/>o</kap>
  <snc mrk="termos.0o.VAZ">
    <dif>
     <ref tip="dif" cel="botel.varm0o.VAZ">Varmbotelo</ref>:
      <ekz>
        <nom>Hagson</nom> konservas <tld/>on, kiu vintre enhavas varman kafon kaj
        somere malvarman bieron
        <fnt>
          <aut>Franko Luin</aut>,
          <vrk><url ref="http://www.algonet.se/~inko/uzino">Uzino</url></vrk>,
          <lok>&Ccirc;u ekzistas an&gcirc;eloj?, 1, ni prezentas nin</lok>
        </fnt>;
      </ekz>
      <ekz>
        la knabino prenis <tld/>on kaj glason
        <fnt>
          <aut>Shi Chengtai</aut>,
          <vrk>La paro de brilaj okuloj</vrk>,
          <lok><url ref="http://www.elerno.net/penseo/pen62.htm">
          Penseo 1995-08, numero 62a</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_t;&c_je;&c_r;&c_m;&c_o;&c_s;</trd>
    <trd lng="ca">termos</trd>
    <trdgrp lng="de">
      <trd>Thermoskanne</trd>,
      <trd>Thermosflasche</trd>,
      <trd>Thermosbeh&auml;lter</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>vacuum flask</trd>,
      <trd>Dewar bottle</trd>,
      <trd>thermos</trd>
    </trdgrp>
    <trd lng="fr">bouteille <ind>thermos</ind></trd>
    <trd lng="nl">thermoskan</trd>
    <trd lng="pl">termos</trd>
    <trdgrp lng="zh">
      <trd>&#20445;&#28201;&#29942; <pr>b&#462;ow&#275;np&#237;ng</pr></trd>,
      <trd>&#20445;&#28331;&#29942; <pr>b&#462;ow&#275;np&#237;ng</pr></trd>,
      <trd>&#26262;&#29942; <pr>nu&#462;np&#237;ng</pr></trd>,
      <trd>&#26262;&#22774; <pr>nu&#462;nh&#250;</pr></trd>,
      <trd>&#26262;&#22778; <pr>nu&#462;nh&#250;</pr></trd>,
      <trd>&#26262;&#27700;&#29942; <pr>nu&#462;nshu&#464;p&#237;ng</pr></trd>,
      <trd>&#28909;&#27700;&#29942; <pr>r&#232;shu&#464;p&#237;ng</pr></trd>,
      <trd>&#29105;&#27700;&#29942; <pr>r&#232;shu&#464;p&#237;ng</pr></trd>
    </trdgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>varmobotelo</ke>, <ke>varmizola botelo</ke>. 
  </rim>

  <trd lng="be">&c_t;&c_e;&c_r;&c_m;&c_a;&c_s;</trd>
  <trd lng="bo">&#3911;&#3851;&#3921;&#3928;&#3851;</trd>
  <trd lng="cs">termoska</trd>
  <trd lng="ja">&#39764;&#27861;&#12403;&#12435;  <pr>&#12414;&#12411;&#12358;&#12403;&#12435;</pr></trd>
  <trd lng="sk">termoska</trd>
  <trd lng="uk">&c_t;&c_je;&c_r;&#1084;&#1086;&c_s;</trd>
</drv>
