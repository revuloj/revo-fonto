<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>traduk</rad>/i
    </kap>
    <drv mrk="traduk.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Transigi iun tekston a&ubreve; konversacion de iu lingvo en alian,
          anstata&ubreve;igante vortojn kaj esprimojn de la unua lingvo per
          samsencaj vortoj a&ubreve; esprimoj de la alia: <ekz><tld/>i el la
            angla lingvo en la italan; </ekz> <ekz><tld/>i francan
            novelon esp-e<fnt>B</fnt> <klr>(a&ubreve; esp-en)</klr>.</ekz> 
        </dif>
        <refgrp tip="sub">
          <ref cel="interp2.0i">interpreti</ref>.
        </refgrp>
      </snc>
      <trd lng="en">translate</trd>
      <trd lng="fr">traduire</trd>
      <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_v;&c_je;&c_s;&c_t;&c_i;</trd>
    </drv>
    <drv mrk="traduk.0ado">
      <kap><tld/>(ad)o</kap>
      <snc>
        <dif>
          Ago, laboro de tiu, kiu <tld/>as: 
          <ekz>la <tld/>o da&ubreve;ris tri monatojn<fnt>B</fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="fr">traduction</trd>
      <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_v;&c_o;&c_d;</trd>
    </drv>
    <drv mrk="traduk.0ajxo">
      <kap><tld/>(a&jcirc;)o</kap>
      <snc>
        <dif>
          Verko rezultanta el tiu laboro: 
          <ekz>la <tld/>o aperis samtempe kun la originalo<fnt>B</fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="fr">traduction</trd>
      <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_v;&c_o;&c_d;</trd>
    </drv>
    <drv mrk="traduk.0ilo">
      <kap><tld/>ilo</kap>
      <uzo tip="fak">KOMP</uzo>
      <snc>
        <dif>
          Programo <tld/>anta de unu formala a&ubreve; homa lingvo
          (<ref tip="vid" cel="lingv.fonto0o">fontolingvo</ref>)
          en alian (<ref tip="vid" cel="lingv.cel0o">cellingvo</ref>).
        </dif>
        <ref tip="vid" cel="interp2.0ilo">interpretilo</ref>
        <ref tip="sub" cel="asembl.0ilo">asemblilo</ref>
      </snc>
      <trdgrp lng="en">
        <trd>translator</trd>,
        <trd>compiler</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_o;&c_d;&c_ch;&c_i;&c_k;</trd>,
        <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_l;&c_ja;&c_t;&c_o;&c_r;</trd>,
        <trd>&c_k;&c_o;&c_m;&c_p;&c_i;&c_l;&c_ja;&c_t;&c_o;&c_r;</trd>
      </trdgrp>
    </drv>
    <drv mrk="traduk.0tempa">
      <kap><tld/>tempa</kap>
      <uzo tip="fak">KOMP</uzo>
      <snc>
        <dif>
          Rilata al la periodo de program<tld/>o:
          <ekz>
            <tld/>tempa eraro, kontrolo, komputado.
          </ekz>
        </dif>
        <ref tip="ant" cel="rul.0tempa">rultempa</ref>
      </snc>
      <trd lng="en">compile-time</trd>
      <trdgrp lng="ru">
        <trd>&c_s;&c_t;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
        <trd>&c_p;&c_je;&c_r;&c_i;&c_o;&c_d;&c_a; <ind>&c_k;&c_o;&c_m;&c_p;&c_i;&c_l;&c_ja;&c_c;&c_i;&c_i;</ind></trd>
      </trdgrp>
    </drv>
  </art>
