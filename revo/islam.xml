<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>islam</rad>/o
      <fnt>Z</fnt>
    </kap>
    <drv mrk="islam.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">REL</uzo>
      <uzo tip="fak">ISL</uzo>
      <snc>
        <dif>
          Unu el la &ccirc;efaj mondreligioj, fondita de la profeto
          <ref tip="vid" cel="muhama.0o.profeto">Muhamado</ref>,
          kies &ccirc;efa sankta libro estas
          <ref tip="vid" cel="koran.0o">Korano</ref>.
        </dif>
        <rim>
          Arabe la vorto signifas <ctl>akcepto, fido, submeti&gcirc;o</ctl>
          <klr>(al la Dia volo)</klr>.
        </rim>
      </snc>
      <trd lng="ca">islam</trd>
      <trd lng="de">Islam</trd>
      <trd lng="hu">iszl&aacute;m</trd>
      <trd lng="nl">islam</trd>
      <trdgrp lng="ru">
        <trd>&c_i;&c_s;&c_l;&c_a;&c_m;</trd>,
        <trd>&c_m;&c_u;&c_s;&c_u;&c_l;&c_mol;&c_m;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>
      </trdgrp>
    </drv>
    <drv mrk="islam.0ano">
      <kap><tld/>ano</kap>
      <uzo tip="fak">REL</uzo>
      <snc>
        <dif>
          Homo akceptanta la doktrinon kaj praktikon de
          <ref tip="malprt" cel="islam.0o">islamo</ref>.
        </dif>
      </snc>
      <trdgrp lng="ca">
        <trd>musulm&agrave;</trd>,
        <trd>mahomet&agrave;</trd>,
        <trd>muslim</trd>
      </trdgrp>
      <trd lng="de">Moslem</trd>
      <trd lng="hu">moszlim</trd>
      <trd lng="nl">moslim</trd>
    <trd lng="ru">&c_m;&c_u;&c_s;&c_u;&c_l;&c_mol;&c_m;&c_a;&c_n;&c_i;&c_n;</trd>
    </drv>
    <drv mrk="islam.0ismo">
      <kap><tld/>ismo</kap>
      <uzo tip="fak">POL</uzo>
      <snc>
        <dif>
          Politiko celanta rigoran realigon de iu formo de
          islama ideologio.
        </dif>
      </snc>
      <trd lng="ca">islamisme</trd>
      <trd lng="de">Islamismus</trd>
      <trd lng="hu">iszlamizmus</trd>
      <trd lng="nl">islamisme</trd>
 <trd lng="ru">&c_i;&c_s;&c_l;&c_a;&c_m;&c_i;&c_z;&c_m;</trd>
    </drv>
    <drv mrk="islam.0isto">
      <kap><tld/>isto</kap>
      <uzo tip="fak">POL</uzo>
      <snc>
        <dif>
          Adepto de islamismo.
        </dif>
      </snc>
      <trd lng="ca">islamista</trd>
      <trd lng="hu">iszlamista</trd>
      <trd lng="nl">islamist</trd>
     <trd lng="ru">&c_i;&c_s;&c_l;&c_a;&c_m;&c_i;&c_s;&c_t;</trd>
    </drv>
  </art>
