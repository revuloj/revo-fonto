<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>islam</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="islam.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">REL</uzo>
  <uzo tip="fak">ISL</uzo>
  <snc>
    <dif>
      Unu el la &ccirc;efaj mond<ref tip="lst" cel="religi.0o.REL" lst="voko:religioj">religioj</ref>, fondita de la profeto
      <ref tip="vid" cel="muhama.0o.profeto">Muhamado</ref>,
      kies &ccirc;efa sankta libro estas
      <ref tip="vid" cel="koran.0o">Korano</ref>:
      <ekz>
        el 17 milionoj da malagasoj nur pli-malpli 4 milionoj apartenas
        al la kristana komunumo, <tld/>o duarangas kaj la plimulto alkro&ccirc;i&gcirc;as 
        al la tradicia religio
        <fnt>
          <bib>Monato</bib>
          <aut>Fidilalao Henriel</aut>
          <vrk><url ref="http://www.monato.net/2006/008777.php">&Scirc;tatlaikeco minacata</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      Arabe la vorto signifas <ctl>akcepto, fido, submeti&gcirc;o</ctl>
      <klr>(al la Dia volo)</klr>.
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_s;&c_l;&c_a;&c_m;</trd>,
    <trd>&c_m;&c_u;&c_s;&c_u;&c_l;&c_mol;&c_m;&c_a;&c_n;&c_s;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_i;&c_s;&c_l;&c_ja;&c_m;</trd>
  <trd lng="ca">islam</trd>
  <trdgrp lng="cs">
    <trd>isl&aacute;m</trd>,
    <trd>muslimsk&eacute; n&aacute;bo&zcaron;enstv&iacute;</trd>
  </trdgrp>
  <trd lng="de">Islam</trd>
  <trd lng="en">islam</trd>
  <trd lng="es">islam</trd>
  <trd lng="fa">&a_A;&a_s;&a_l;&a_A;&a_m;</trd>
  <trd lng="fr">islam</trd>
  <trd lng="hu">iszl&aacute;m</trd>
  <trd lng="id">Islam</trd>
  <trd lng="nl">islam</trd>
  <trd lng="pl">islam</trd>
  <trd lng="pt">isl&atilde;</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_l;&c_a;&c_m;</trd>,
    <trd>&c_m;&c_u;&c_s;&c_u;&c_l;&c_mol;&c_m;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">islam</trd>
  <trd lng="tr">&Idot;slam</trd>
</drv>

<drv mrk="islam.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o, al &gcirc;iaj spirito, principoj, ritoj:
      <ekz>
        la e&ubreve;ropaj medicino, matematiko, astronomio, fiziko kaj 
        geografio multe profitis de la <tld/>aj esploristoj
        <fnt>
          <bib>Monato</bib>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006903.html">Pruntoj: Arabaj vortoj en la germana kaj esperanta lingvoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        religia aristokratio, aperinta post la <tld/>a revolucio,
        ne volas kaj ne povas kredi, ke &gcirc;i devas transdoni la registaron al reformistoj
        <fnt>
          <bib>Monato</bib>
          <aut>M. Reza torabi</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007017.html">Vundita sed ne morta</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia patro estis muezino kaj instruis la malfacilan arton de <tld/>aj
        religiaj kantoj al sia filino
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007063.html">Um Keltum'</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
   <trdgrp lng="be">
     <trd>&c_ib;&c_s;&c_l;&c_a;&c_m;&c_s;&c_k;&c_ib;</trd>,
     <trd>&c_m;&c_u;&c_s;&c_u;&c_l;&c_mol;&c_m;&c_a;&c_n;&c_s;&c_k;&c_ib;</trd>
   </trdgrp>
   <trd lng="ca">isl&agrave;mic <klr>(adj)</klr></trd>
   <trd lng="de">muslimisch</trd>
  <trd lng="fa">&a_A;&a_s;&a_l;&a_A;&a_m;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>musulman<klr tip="ind"> (adj.)</klr></trd>,
    <trd>mahom&eacute;tan<klr tip="ind"> (adj.)</klr></trd>,
    <trd>islamique</trd>
  </trdgrp>
  <trd lng="id">islami</trd>
  <trdgrp lng="pl">
    <trd>islamski</trd>,
    <trd>muzu&lstroke;ma&nacute;ski</trd>
  </trdgrp>
  <trd lng="pt">islam&iacute;tico</trd>
  <trd lng="tr">&Idot;slam&icirc;</trd>
</drv>

<drv mrk="islam.0ano">
  <kap><tld/>ano</kap>
  <uzo tip="fak">REL</uzo>
  <snc>
    <dif>
      Homo akceptanta la doktrinon kaj praktikon de
      <ref tip="malprt" cel="islam.0o">islamo</ref>:
      <ekz>
        la tri plej gravaj unudiaj religioj (judismo, kristismo 
        kaj <tld/>o) havas komunan trunkon, tiel ke iliajn adeptojn 
        (judojn, kristanojn kaj <tld/>anojn) eblus kunnomi &leftquot;idoj de Abrahamo&rightquot;
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006820.html">Plia libro biblia</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en Francio <klr>[&#8230;]</klr> jam vivas <klr>[&#8230;]</klr> 
        kvin milionoj da <tld/>anoj, kiuj posedas oficiale 1600 luksajn
        moskeojn, kaj <tld/>o fari&gcirc;is la dua influa religio en la lando
        <fnt>
          <bib>Monato</bib>
          <aut>Gilbert Ledon</aut>
          <vrk><url ref="https://www.monato.be/2003/008015.html">Misa mondo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_s;&c_u;&c_l;&c_mol;&c_m;&c_a;&c_n;&c_ib;&c_n;</trd>
  <trdgrp lng="ca">
    <trd>musulm&agrave;</trd>,
    <trd>mahomet&agrave;</trd>,
    <trd>muslim</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>isl&aacute;mec</trd>,
    <trd>mohamed&aacute;n</trd>,
    <trd>muslim</trd>
  </trdgrp>
  <trd lng="de">Moslem</trd>
  <trdgrp lng="en">
    <trd>muslim</trd>,
    <trd>moslem</trd>,
    <trd>mohammedan</trd>
  </trdgrp>
  <trd lng="es">musulm&aacute;n</trd>
  <trd lng="fa">&a_m;&a_s;&a_l;&a_m;&a_A;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>musulman<klr tip="ind"> (subst.)</klr></trd>,
    <trd>mahom&eacute;tan<klr tip="ind"> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="hu">moszlim</trd>
  <trd lng="id">muslim</trd>
  <trd lng="nl">moslim</trd>
  <trdgrp lng="pl">
    <trd>muzu&lstroke;manin</trd>,
    <trd>mahometanin <klr>(przest.)</klr></trd>
  </trdgrp>
  <trd lng="pt">islamita<klr tip="amb"> (pessoa)</klr></trd>
  <trd lng="ru">&c_m;&c_u;&c_s;&c_u;&c_l;&c_mol;&c_m;&c_a;&c_n;&c_i;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>mohamed&aacute;n</trd>,
    <trd>moslim</trd>
  </trdgrp>
  <trd lng="tr">M&uuml;sl&uuml;man</trd>
</drv>

<drv mrk="islam.0ismo">
  <kap><tld/>ismo</kap>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Politiko celanta rigoran realigon de iu formo de
      islama ideologio:
      <ekz>
        kun tia eraro lingva mi trovi&gcirc;as en malbona societo de multaj
        &jcirc;urnalistoj, politikistoj kaj ordinaraj homoj, kiuj ne diferencigas
        <tld/>ismon disde <tld/>o pro tio, ke jam de longa tempo teruras nin 
        teroristoj, kiuj asertas agadi en la nomo de sia dio
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2004/008385.html">Eraro (ne nur) lingva</url></vrk>
        </fnt>.
      </ekz>
      <ekz>
        kial <tld/>ismo en Sudano ne disponas pri oficisto
        pri publikaj rilatoj, kiu eble estus povinta evitigi 
        &ccirc;i tiun interreligian, internacian, &ccirc;efe interhoman katastrofon
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="http://www.monato.net/2008/009392.php">La plu&scirc;urso kaj la profeto</url></vrk>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_s;&c_l;&c_a;&c_m;&c_ib;&c_z;&c_m;</trd>
  <trd lng="ca">islamisme</trd>
  <trdgrp lng="cs">
    <trd>islamizmus</trd>,
    <trd>politick&eacute; zneu&zcaron;&iacute;v&aacute;n&iacute; isl&aacute;mu</trd>
  </trdgrp>
  <trd lng="de">Islamismus</trd>
  <trd lng="en">islamism</trd>
  <trd lng="es">islamismo</trd>
  <trd lng="fa">&a_A;&a_s;&a_l;&a_A;&a_m;&zwnj;&f_g;&a_r;&a_A;&f_y;&f_y;</trd>
  <trd lng="fr">islamisme</trd>
  <trd lng="hu">iszlamizmus</trd>
  <trd lng="nl">islamisme</trd>
  <trd lng="pl">islam</trd>
  <trd lng="pt">islamismo</trd>
  <trd lng="ru">&c_i;&c_s;&c_l;&c_a;&c_m;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">islamizmus</trd>
  <trd lng="tr">&Idot;slamc&inodot;l&inodot;k</trd>
</drv>

<drv mrk="islam.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Adepto de islamismo:
      <ekz>
        la insultoj de s-ro <nom>Gofen</nom> kontra&ubreve; araboj kaj <tld/>o
        havas tamen limojn: li plene konsentas kun la fanatikaj
        <tld/>istoj, kiam ili persekutas samseksemon, 
        &leftquot;pornografion kaj seksajn perversa&jcirc;ojn&#8221;
        <fnt>
          <bib>Monato</bib>
          <aut>Michel Duc Goninaz</aut>
          <vrk><url ref="https://www.monato.be/2004/008254.html">&Jcirc;urnalisma deontologio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_s;&c_l;&c_a;&c_m;&c_ib;&c_s;&c_t;</trd>
  <trd lng="ca">islamista</trd>
  <trd lng="de">Islamist</trd>
  <trd lng="en">islamist</trd>
  <trd lng="es">islamita</trd>
  <trd lng="fa">&a_A;&a_s;&a_l;&a_A;&a_m;&zwnj;&f_g;&a_r;&a_A;</trd>
  <trd lng="fr">islamiste<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="hu">iszlamista</trd>
  <trd lng="nl">islamist</trd>
  <trdgrp lng="pl">
    <trd>muzu&lstroke;manin</trd>,
    <trd>mahometanin <klr>(przest.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_s;&c_l;&c_a;&c_m;&c_i;&c_s;&c_t;</trd>
  <trd lng="tr">&Idot;slamc&inodot;</trd>
</drv>
</art>
