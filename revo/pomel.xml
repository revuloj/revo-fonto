<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pomel.xml,v 1.9 2022/05/19 20:33:39 revo Exp $">
<kap>
  <rad>pomel</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="pomel.0o">
  <kap><tld/>o</kap>
  <snc mrk="pomel.0o.TEK">
    <dif>
      Globforma ekstrema&jcirc;o de <ref tip="super" cel="ten.0ilo">tenilo</ref>:
      <ekz>
        la &scirc;ildisto <klr>...</klr> metis unu manon sur la frontan
        <tld/>on
        de la selo
        <fnt>
          <bib>DonKihxoto</bib>,
          <lok>parto 1a, &ccirc;apitro 20a, p. 139a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">pom</trd>
  <trdgrp lng="cs">
    <trd>dr&#x17E;adlo me&#x10D;e a dyky</trd>,
    <trd>j&#xED;lec</trd>
  </trdgrp>
  <trd lng="en">pommel</trd>
  <trd lng="fr">pommeau</trd>
  <trd lng="nl">knop<klr tip="ind"> (v.paraplu, wandelstok,
  zadel)</klr></trd>
  <trdgrp lng="pl">
    <trd>g&lstroke;owica  <klr>(broni)</klr></trd>,
    <trd>&lstroke;&eogonek;k siod&lstroke;a</trd>
  </trdgrp>
  <trd lng="pt">ar&ccedil;&atilde;o</trd>
  <trd lng="ro">capul  <klr>(arm&abreve;)</klr></trd>
  <trd lng="sk">dr&#x17E;adlo me&#x10D;a</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&#x43E;&#x43B;&#x43E;&c_v;&#x43A;&c_a; &c_je;&c_f;&c_je;&c_s;&c_a; (&c_sh;&c_a;&c_b;&#x43B;&c_ib;</trd>,
    <trd>&c_sh;&#x43F;&c_a;&c_g;&c_i;)</trd>,
    <trd>&c_g;&#x43E;&#x43B;&#x43E;&c_v;&#x43A;&c_a; &#x43F;&c_je;&c_r;&c_je;&c_d;&#x43D;&#x44C;&#x43E;&c_ji; &#x43B;&c_u;&#x43A;&c_i;
    &c_s;&c_ib;&c_d;&#x43B;&c_a;</trd>
  </trdgrp>
</drv>
</art>

