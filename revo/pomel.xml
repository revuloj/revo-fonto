<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pomel.xml,v 1.7 2015/07/04 08:37:41 revo Exp $">
<kap>
  <rad>pomel</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="pomel.0o">
  <kap><tld/>o</kap>
  <snc mrk="pomel.0o.TEK">
    <dif>
      Globforma ekstrema&jcirc;o de <ref tip="super"
      cel="ten.0ilo">tenilo</ref>:
      <ekz>
        la &scirc;ildisto <klr>...</klr> metis unu manon sur la frontan
        <tld/>on
        de la selo
        <fnt>
          <bib>DonKihxoto</bib>,
          <lok>parto 1a, &ccirc;apitro 20a, p. 139a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">pom</trd>
  <trdgrp lng="cs">
    <trd>držadlo meče a dyky</trd>,
    <trd>jílec</trd>
  </trdgrp>
  <trd lng="en">pommel</trd>
  <trd lng="fr">pommeau</trd>
  <trd lng="nl">knop<klr tip="ind"> (v.paraplu, wandelstok,
  zadel)</klr></trd>
  <trd lng="pt">ar&ccedil;&atilde;o</trd>
  <trd lng="sk">držadlo meča</trd>
</drv>
</art>

