<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pomel.xml,v 1.13 2024/02/17 07:52:34 revo Exp $">
<kap>
  <rad>pomel</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="pomel.0o">
  <kap><tld/>o</kap>
  <snc mrk="pomel.0o.TEK">
    <dif>
      Globforma ekstrema&jcirc;o de <ref tip="super" cel="ten.0ilo">tenilo</ref>:
      <ekz>
        speciala selo, kun pli alta <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Judit Felszeghy</aut>
          <vrk><url ref="&Monato;/2008/009409.php">Ne nur sporto, anka&ubreve; vivoformo</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;ildisto <klr>[&#x2026;]</klr> metis unu manon sur la frontan
        <tld/>on de la selo
        <fnt>
          <bib>DonKihxoto</bib>,
          <lok>parto 1a, &ccirc;apitro 20a, p. 139a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="kap.0eto.klinko">kapeto<sncref/></ref>
    <refgrp tip="vid">
      <ref cel="ans.0o.tenilo">anso</ref>,
      <ref cel="buton.0o.KOMUNE">butono</ref>
    </refgrp>
  </snc>
  <trd lng="ca">pom</trd>
  <trdgrp lng="cs">
    <trd>dr&zcaron;adlo me&ccaron;e a dyky</trd>,
    <trd>j&iacute;lec</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Knauf</trd>,
    <trd>Knopf <klr>(Knauf)</klr></trd>
  </trdgrp>
  <trd lng="en">pommel</trd>
  <trd lng="fr">pommeau</trd>
  <trd lng="nl">knop<klr tip="ind"> (v.paraplu, wandelstok,
  zadel)</klr></trd>
  <trdgrp lng="pl">
    <trd>g&lstroke;owica  <klr>(broni)</klr></trd>,
    <trd>&lstroke;&eogonek;k siod&lstroke;a</trd>
  </trdgrp>
  <trd lng="pt">ar&ccedil;&atilde;o</trd>
  <trd lng="ro">capul  <klr>(arm&abreve;)</klr></trd>
  <trd lng="sk">dr&zcaron;adlo me&ccaron;a</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_o;&c_l;&c_o;&c_v;&c_k;&c_a; &c_je;&c_f;&c_je;&c_s;&c_a; (&c_sh;&c_a;&c_b;&c_l;&c_ib;</trd>,
    <trd>&c_sh;&c_p;&c_a;&c_g;&c_i;)</trd>,
    <trd>&c_g;&c_o;&c_l;&c_o;&c_v;&c_k;&c_a; &c_p;&c_je;&c_r;&c_je;&c_d;&c_n;&c_mol;&c_o;&c_ji; &c_l;&c_u;&c_k;&c_i;
    &c_s;&c_ib;&c_d;&c_l;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#x628A;&#x624B; <pr>b&#x1CE;shou</pr></trd>
</drv>
</art>

