<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>konifer</rad>/o
</kap>
<drv mrk="konifer.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Lignoplanto kun ledecaj, pinglo- a&ubreve;
      skvamformaj, ofte plurjaraj, folioj kaj plej ofte konusformaj
      fruktoj.
    </dif>
  </snc>
  <adm>
    La difino apogita al tiu de PIV, sed ver&scirc;ajne
    bezonas aktualigon la&ubreve; nuna klasifiko.
    <aut>Wolfram Diestel</aut>
  </adm>
  <trd lng="be">&c_h;&c_v;&c_o;&c_j;&c_n;&c_a;&c_je; &c_d;&c_r;&c_e;&c_v;&c_a;</trd>
  <trd lng="en">conifer</trd>
  <trd lng="hu">t&udblac;level&udblac; fa</trd>
  <trd lng="nl">conifeer</trd>
  <trd lng="pl">drzewo iglaste</trd>
  <trd lng="ru">&c_h;&c_v;&c_o;&c_j;&c_n;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
</drv>
<drv mrk="konifer.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Klaso de <ref tip="super" cel="konifer.0oplantoj"><tld/>oplantoj</ref>
      (<trd lng="la">Coniferopsida</trd>).
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>tobozterm&odblac;k</trd>,
    <trd>tobozos nyitvaterm&odblac;k</trd>
  </trdgrp>
  <trd lng="ru">&c_h;&c_v;&c_o;&c_j;&c_n;&c_y;&c_je;</trd>
</drv>
<drv mrk="konifer.0oplantoj">
  <kap>
    <tld/>oplantoj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Grupo de <ref tip="super" cel="plant.0oj">plantoj</ref>,
      reprodukti&gcirc;antaj per semgrajnoj, kiuj ne estas
      enfermitaj en ovario.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Nacktsamige Pflanzen</trd>,
    <trd>Nacktsamer</trd>
  </trdgrp>
  <trd lng="fr">conif&egrave;re</trd>
  <trd lng="hu">nyitvaterm&odblac;k</trd>
  <trdgrp lng="la">
    <trd>Coniferophyta</trd>,
    <trd>Gymnospermae</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_o;&c_l;&c_o;&c_s;&c_je;&c_m;&c_je;&c_n;&c_n;&c_y;&c_je;</trd>
  <trd lng="sv">barrv&auml;xter</trd>
</drv>
</art>
