<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>konifer</rad>/o
</kap>
<drv mrk="konifer.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Iu el <tld/>oj, lignoplanto kun ledecaj, pinglo- a&ubreve;
      skvamformaj, ofte plurjaraj, folioj kaj plej ofte konusformaj
      fruktoj.
    </dif>
  </snc>
</drv>
<drv mrk="konifer.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Klaso de <ref tip="super" cel="konifer.0oplantoj"><tld/>oplantoj</ref>
      (<trd lng="la">Coniferopsida</trd>).
    </dif>
  </snc>
  <trd lng="de">Kiefern&auml;hnliche Pflanzen</trd>
</drv>
<drv mrk="konifer.0oplantoj">
  <kap><tld/>oplantoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Grupo de <ref tip="super" cel="plant.0oj">plantoj</ref>,
      kies semgrajnoj sidas sur nefermitaj fruktofolioj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Nacktsamige Pflanzen</trd>,
    <trd>Nacktsamer</trd>
  </trdgrp> 
  <trdgrp lng="la">                     
    <trd>Coniferophyta</trd>,
    <trd>Gymnospermae</trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
-->
</vortaro>