<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>konifer</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="konifer.0o">
  <kap><tld/>o</kap>
  <snc mrk="konifer.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Lignoplanto kun ledecaj, pinglo- a&ubreve;
      skvamformaj, ofte plurjaraj, folioj kaj plej ofte konusformaj
      fruktoj.
    </dif>
  </snc>
  <adm>
    La difino estas apogita al tiu de PIV, sed ver&scirc;ajne
    bezonas aktualigon la&ubreve; nuna klasifiko.
    <aut>W. Diestel</aut>
  </adm>
  <trd lng="be">&c_h;&c_v;&c_o;&c_j;&c_n;&c_a;&c_je; &c_d;&c_r;&c_e;&c_v;&c_a;</trd>
  <trd lng="ca">con&iacute;fera</trd>
  <trd lng="en">conifer</trd>
  <trdgrp lng="fr">
    <trd>conif&egrave;re</trd>,
    <trd>r&eacute;sineux<klr> (conif&egrave;re)</klr></trd>
  </trdgrp>
  <trd lng="hu">t&udblac;level&udblac; fa</trd>
  <trd lng="nl">conifeer</trd>
  <trd lng="pl">drzewo iglaste</trd>
  <trd lng="pt">con&iacute;fera</trd>
  <trd lng="ru">&c_h;&c_v;&c_o;&c_j;&c_n;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  <trd lng="sv">barrtr&auml;d</trd>
</drv>

<drv mrk="konifer.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="konifer.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="klas.0o.BIO"
      lst="voko:botanikaj_klasoj">Klaso</ref> de <ref tip="malprt"
      cel="flor.0oplantoj">floroplantoj</ref>
      (<trd lng="la">Coniferopsida</trd>).
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>pin&ograve;psids</trd>,
    <trd>con&iacute;feres</trd>
  </trdgrp>
  <trd lng="de">Koniferen</trd>
  <trdgrp lng="hu">
    <trd>tobozterm&odblac;k</trd>,
    <trd>tobozos nyitvaterm&odblac;k</trd>
  </trdgrp>
  <trd lng="nl">coniferen</trd>
  <trd lng="ru">&c_h;&c_v;&c_o;&c_j;&c_n;&c_y;&c_je;</trd>
</drv>

</art>
