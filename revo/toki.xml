<art mrk="$Id: toki.xml,v 1.17 2022/05/05 17:08:11 revo Exp $">
<kap><rad>Toki</rad>/o</kap>
<drv mrk="toki.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>ZR</bib></fnt>
  <fnt><bib>PIV1</bib></fnt>
  <fnt><bib>KrED</bib></fnt>
  <snc>
    <dif>
      <ref tip="lst" cel="urb.cxef0o" lst="voko:&ccirc;efurboj">&Ccirc;efurbo</ref> 
      kaj plej granda urbo de
      <ref tip="malprt" cel="japan.0ujo">Japanujo</ref>,
      kun origina nomo <ctl>Edo</ctl>:
      <ekz>
        <tld/>o signifas <ctl>orienta
        &ccirc;efurbo</ctl> kaj iam oficiale anstata&ubreve;is la nomon Edo;
      </ekz>
      <ekz>
        la golfo de <tld/>o
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/006909.html">Japanio: Takasugi Sinsaku: Novjara kla&ccirc;o super konjako</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        okaze de la tertremo en <tld/>o en 1855 estas raportite, ke multe tumultis
        siluroj
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Yasuo</aut>
          <vrk><url ref="&Monato;/2003/008060.html">Siluroj, &ccirc;u supersti&ccirc;o a&ubreve; scienco?</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;intoisma templo en <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Igarasi Takeo</aut>
          <vrk><url ref="&Monato;/2004/008049.html">Sum(o)o plej alta</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    </snc>
    <trd lng="be">&c_T;&c_o;&c_k;&c_ib;&c_jo;</trd>
    <trd lng="ca">T&ograve;quio</trd>
    <trd lng="de">Tokio</trd>
    <trd lng="fa">&a_t;&a_w;&f_k;&f_y;&a_w;</trd>
    <trd lng="fr">Tokyo</trd>
    <trd lng="hu">Toki&oacute;</trd>
    <trd lng="nl">Tokio</trd>
    <trd lng="pl">Tokio</trd>
    <trd lng="pt">T&oacute;quio</trd>
    <trd lng="ru">&c_T;&c_o;&c_k;&c_i;&c_o;</trd>
    <trd lng="uk">&#x422;&#x43E;&#x43A;&#x456;&#x43E; (&#x441;&#x442;&#x43E;&#x43B;&#x438;&#x446;&#x44F;)</trd>
  <trd lng="zh">&#x4E1C;&#x4EAC; [<ind>d&#x14D;ng j&#x12B;ng</ind>]</trd>
</drv>
</art>
