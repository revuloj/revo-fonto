<vortaro>
<art mrk="$Id$">
<kap><rad>Toki</rad>o<fnt>Z</fnt></kap>
<drv mrk="toki.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>ZR</bib></fnt>
  <fnt><bib>PIV1</bib></fnt>
  <fnt><bib>KrED</bib></fnt>
  <snc>
    <dif>
      &Ccirc;efurbo kaj plej granda <ref tip="super" cel="urb.0o">urbo</ref> de
      Japanujo. La origina nomo estis Edo. <tld/>o signifas orienta
      &ccirc;efurbo kaj iam oficiale anstata&ubreve;is la nomon.
    </dif>
    <trd lng="be">&c_T;&c_o;&c_k;&c_ib;&c_jo;</trd>
    <trd lng="de">Tokio</trd>
    <trd lng="fr">Tokyo</trd>
    <trd lng="hu">Toki&oacute;</trd>
    <trd lng="nl">Tokio</trd>
    <trd lng="ru">&c_T;&c_o;&c_k;&c_i;&c_o;</trd>
  </snc>
</drv>
</art>
