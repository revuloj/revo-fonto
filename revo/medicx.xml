<vortaro>
  <art mrk="$Id: medicx.xml,v 1.2 2020/07/22 22:10:33 afido Exp $">
    <kap>
      <rad>Medi&ccirc;</rad>/oj
    </kap>

<drv mrk="medicx.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">HIS</uzo>
  <snc mrk="medicx.0oj.familio">
    <dif>
      <ref tip="malprt" cel="florenc.0o">Florenc</ref>a influhava familio 
      inter 15a kaj 18a jc, al kiu devenis i.a. tri papoj:
      <ekz>
        Juliano de <tld/>oj ordonis, ke Leonardo <nom>da Vin&ccirc;i</nom> akceptu Paolon kiel
        lernoservanton
        <fnt>
          <bib>Monato</bib>
          <aut>Anneke Buys</aut>
          <vrk><url ref="https://www.monato.be/2009/009743.php"
              >Aventuroplena historia romano</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Louis Marie Anne Couperus</nom> <klr>[estis]</klr> esploranto de la itala <ref
        tip="malprt" cel="renesa.0o">renesanco</ref>,
        &scirc;atanto de la florencaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Nicolino Rossi</aut>
          <vrk><url ref="https://www.monato.be/2013/011144.php"
              >Familio-sagao, kun lar&gcirc;e variigitaj psikologiaj portretoj</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Medici</trd>
    <trd lng="en">House of Medici</trd>
    <trd lng="fr">Maison de M&eacute;dicis</trd>
    <trd lng="pl">Medyceusze</trd>     
    <trd lng="ro">Familia <ind>Medici</ind></trd>
  </snc>
</drv>

  </art>

