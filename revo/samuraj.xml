<kap>
  <rad>samuraj</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="samuraj.0o">
  <kap><tld/>o</kap>
  <snc mrk="samuraj.0o.MIL">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Ano de la militista klaso en malnova Japanio:
      <ekz>
        juna <tld/>o kaj lia edzino <klr>...</klr> vivis en malfrua
        periodo el la Militregna Epoko (1467-1568) de Japanio
        <fnt>
        <aut>Vastalto</aut>,
        <vrk><url ref="http://www.vastalto.com/blog">Akvosfero</url></vrk>
        <lok>La Militregna Epoko..., 2006-02-26</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_m;&c_u;&c_r;&c_a;&c_j;</trd>
  <trd lng="bg">&c_s;&c_a;&c_m;&c_u;&c_r;&c_a;&c_j;</trd>
  <trd lng="cs">samuraj</trd>
  <trd lng="de">Samurai</trd>
  <trd lng="en">samurai</trd>
  <trd lng="fa">&a_s;&a_A;&a_m;&a_w;&a_r;&a_A;&a_y_hamza;&f_y;</trd>
  <trd lng="fr">samoura&iuml;</trd>
  <trd lng="it">samurai</trd>
  <trdgrp lng="ja">
    <trd>&#x6B66;&#x58EB;  <pr>&#x3082;&#x306E;&#x306E;&#x3075;</pr></trd>,
    <trd>&#x4F8D;  <pr>&#x3055;&#x3080;&#x3089;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="pl">samuraj</trd>
  <trd lng="pt">samurai</trd>
  <trd lng="sk">samuraj</trd>
  <trd lng="uk">&c_s;&c_a;&#x43C;&c_u;&c_r;&c_a;&c_j;</trd>
</drv>
