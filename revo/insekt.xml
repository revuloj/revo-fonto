<drv mrk="insekt.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="insekt.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Senskeletaj sespiedaj bestetoj,
      kiuj preska&ubreve; senescepte ricevas la definitivan formon
      nur post pluraj metamorfozoj;
      klaso de <ref tip="super" cel="artik.0uloj">artikuloj</ref>
      (<trd lng="la">Insecta</trd>):
      <ekz>
        mu&scirc;o, papilio, formiko, akrido estas <tld/>oj, sed araneo ne
        estas <tld/>o.
      </ekz>
    </dif>
  </snc>
    <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_s;&c_ja;&c_k;&c_o;&c_m;&c_y;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_z;&c_ju;&c_r;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="ca">insectes</trd>
  <trdgrp lng="cs">
    <trd>hmyz</trd>,
    <trd>insekt</trd>
  </trdgrp>
  <trd lng="de">Insekten</trd>
  <trd lng="en">insects</trd>
  <trd lng="es">insectos</trd>
  <trd lng="fr">insectes</trd>
  <trd lng="hu">rovarok</trd>
  <trd lng="id">insekta</trd>
  <trd lng="it">insetti</trd>
  <trd lng="ja">&#26118;&#x866B;&#x985E;  <pr>&#12371;&#12435;&#12385;&#12421;&#12358;&#x308B;&#12356;</pr></trd>
  <trd lng="nl">insecten</trd>
  <trd lng="pt">insetos<klr tip="amb"> (classe)</klr></trd>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_je;&c_k;&c_o;&c_m;&c_y;&c_je;</trd>
  <trd lng="sk">hmyz</trd>
  <trd lng="uk">&#x43A;&#x43E;&#x43C;&c_a;&c_h;&c_i;</trd>
  <trd lng="zh">&#26118;&#x866B; <pr>k&#x16B;nch&#xF3;ng</pr></trd>
</drv>

<drv mrk="insekt.fi0o">
  <kap>fi<tld/>o, <var><kap>trud<tld/>o</kap></var>, <var><kap><tld/>a&ccirc;o</kap></var></kap>
  <snc mrk="insekt.fi0o.KOS">
    <dif>
      <tld lit="I"/>o dama&gcirc;a por la homo (t.e. <ref tip="super" cel="parazi.0o.EKOL">parazito</ref>)
      a&ubreve; la rikolto:  pulo, pediko kc:
      <ekz>
        aperis multego da fi<tld/>oj en la domo
        <fnt>
          <bib>MT</bib>
          <lok>&Eli; 8:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ja povus <klr>[&#8230;]</klr> diskuti kun la lo&gcirc;antoj pri humido kaj fi<tld/>oj
        <klr>[&#8230;]</klr> dum pasas la pluvo
        <fnt>
          <bib>Kiso</bib>
        </fnt>;
      </ekz>
      <ekz>
        pro <klr>[&#8230;]</klr> fi<tld/>oj ktp, en 2006 anka&ubreve; la produktokvanto de rizo iom
        falis
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="&Monato;/2007/009154.php">Mondaj grenprezoj levi&gcirc;as</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        multaj specioj <klr>[de akrido]</klr> estas trud<tld/>oj dama&gcirc;antaj agrojn
        <fnt>
          <aut>-</aut>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Akrido">Akrido</url></vrk>
          <lok>Vikipedio, 2023-02-23</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_z;&c_y;&c_t; <klr>(&c_n;&c_a;&c_s;&c_ja;&c_k;&c_o;&c_m;&c_a;&c_je;)</klr></trd>
  <trdgrp lng="ca">
    <trd>pu&ccedil;a</trd>,
    <trd>poll</trd>,
    <trd>par&agrave;sit <klr>(en general)</klr></trd>
  </trdgrp>
  <trd lng="de">Ungeziefer</trd>
  <trd lng="fr">vermine</trd>
  <trd lng="id">hama</trd>
  <trd lng="it">parassita<klr> (insetto)</klr></trd>
  <trd lng="ja">&#x5BB3;&#x866B;  <pr>&#x304C;&#12356;&#12385;&#12421;&#12358;</pr></trd>
  <trd lng="pt">parasita<klr tip="amb"> (inseto)</klr></trd>
  <trd lng="uk">(&#x43A;&#x43E;&#x43C;&c_a;&c_h;&c_a;-)&#x43F;&c_a;&c_r;&c_a;&c_z;&c_i;&c_t;</trd>
  <trd lng="zh">&#x5BB3;&#x866B; <pr>h&#xE0;ich&#xF3;ng</pr></trd>
</drv>
