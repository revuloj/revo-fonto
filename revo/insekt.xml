<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>insekt</rad>/o
</kap>
<drv mrk="insekt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Malgranda besto apartenanta al la klaso
      <ref tip="malprt" cel="insekt.0oj"><tld/>oj</ref>.
    </dif>
  </snc>
  <trd lng="de">Insekt</trd>
  <trd lng="nl">insect</trd>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_je;&c_k;&c_o;&c_m;&c_o;&c_je;</trd>
  <trd lng="sv">insekt</trd>
  <trd lng="tr">b&ouml;cek</trd>
</drv>
<drv mrk="insekt.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Klaso de <ref tip="super" cel="artik.0pieduloj">artikpieduloj</ref>
      (<trd lng="la">Insecta</trd>), kun preska&ubreve;
      unu miliono da specioj. <tld lit="I"/>oj estas senskeletaj sespiedaj
      bestetoj, kiuj preska&ubreve; senescepte ricevas la definitivan formon
      nur post pluraj metamorfozoj:
      <ekz>
        mu&scirc;o, papilio, formiko, akrido estas <tld/>oj, sed araneo ne
        estas <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Insekten</trd>
  <trd lng="en">insects</trd>
  <trd lng="nl">insecten</trd>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_je;&c_k;&c_o;&c_m;&c_y;&c_je;</trd>
</drv>
<drv mrk="insekt.fi0o0acxo">
  <kap>fi<tld/>o, <tld/>a&ccirc;o</kap>
  <snc>
    <dif>
      Parazita malpura besteto (pulo, pediko kc).
    </dif>
  </snc>
</drv>
</art>
