<drv mrk="insekt.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="insekt.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Senskeletaj sespiedaj bestetoj,
      kiuj preska&ubreve; senescepte ricevas la definitivan formon
      nur post pluraj metamorfozoj;
      klaso de <ref tip="super" cel="artik.0uloj">artikuloj</ref>
      (<trd lng="la">Insecta</trd>):
      <ekz>
        mu&scirc;o, papilio, formiko, akrido estas <tld/>oj, sed araneo ne
        estas <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_s;&c_ja;&c_k;&c_o;&c_m;&c_y;&c_ja;</trd>
  <trd lng="ca">insectes</trd>
  <trdgrp lng="cs">
    <trd>hmyz</trd>,
    <trd>insekt</trd>
  </trdgrp>
  <trd lng="de">Insekten</trd>
  <trd lng="en">insects</trd>
  <trd lng="es">insectos</trd>
  <trd lng="fr">insectes</trd>
  <trd lng="hu">rovarok</trd>
  <trd lng="it">insetti</trd>
  <trd lng="nl">insecten</trd>
  <trd lng="pt">insetos<klr tip="amb"> (classe)</klr></trd>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_je;&c_k;&c_o;&c_m;&c_y;&c_je;</trd>
  <trd lng="sk">hmyz</trd>
</drv>

<drv mrk="insekt.fi0o">
  <kap>fi<tld/>o, <var><kap><tld/>a&ccirc;o</kap></var></kap>
  <snc mrk="insekt.fi0o.KOS">
    <dif>
      Parazita malpura besteto: pulo, pediko kaj
      ceteraj.
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>pu&ccedil;a</trd>,
    <trd>poll</trd>,
    <trd>par&agrave;sit <klr>(en general)</klr></trd>
  </trdgrp>
  <trd lng="fr">vermine</trd>
  <trd lng="it">parassita<klr> (insetto)</klr></trd>
  <trd lng="pt">parasita<klr tip="amb"> (inseto)</klr></trd>
</drv>
