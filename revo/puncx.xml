<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: puncx.xml,v 1.14 2009/06/27 16:31:20 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pun&ccirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="puncx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="puncx.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Varma a&ubreve; malvarma <ref tip="super" 
      cel="alkoho.0ajxo.KUI">alkohola&jcirc;o</ref> kun 
      sukero, akvo kaj citrono a&ubreve; aliaj aroma&jcirc;oj:
      <ekz>
        kial oni ne preparus vin<tld/>on por &ccirc;iuj
        <fnt><bib>KielAkvo</bib>, <lok>1:3</lok></fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_n;&c_sh;</trd>
  <trd lng="bg">&c_p;&c_u;&c_n;&c_sh;</trd>
  <trd lng="de">Punsch</trd>
  <trd lng="en">punch<klr> (alcohol and fruit drink)</klr></trd>
  <trd lng="fr">punch<klr> (boisson)</klr></trd>
  <trd lng="hu">puncs</trd>
  <trd lng="nl">punch</trd>
  <trd lng="pt">ponche</trd>
  <trd lng="ru">&c_p;&c_u;&c_n;&c_sh;</trd>
</drv>
</art>
