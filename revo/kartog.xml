<vortaro>
<art mrk="$Id: kartog.xml,v 1.19 2020/03/20 23:11:14 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>kartografi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kartog.0o">
  <kap><tld/>o</kap>
  <snc mrk="kartog.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Arto verki geografiajn mapojn:
      <ekz>
        la observatorio de la franca &ccirc;efurbo havis gravan rolon 
        por okcidenta astronomio, tie naski&gcirc;is kelkaj gravaj 
        sciencoj kiel geodezio, <tld/>o a&ubreve; meteologio
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref="https://eo.wikipedia.org/?curid=125889">Observatorio de
          Parizo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la fosilia&jcirc;o estis malkovrita hazarde okaze de <tld/>aj laboroj
        en tiu regiono 
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2010/010029.php">Fosilia
          trova&jcirc;o</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd
  lng="be">&c_k;&c_a;&c_r;&c_t;&c_a;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>kartografie</trd>,
    <trd>mapopis</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Kartographie</trd>,
     <trd>Kartenzeichnen</trd>,
     <trd>Kartenherstellung</trd>
  </trdgrp>
  <trd lng="en">cartography</trd>
  <trd lng="es">cartograf&iacute;a</trd>
  <trd lng="fr">cartographie</trd>
  <trdgrp lng="hu">
     <trd>t&eacute;rk&eacute;p&eacute;szet</trd>,
     <trd>kartogr&aacute;fia</trd>
  </trdgrp>
  <trd lng="nl">cartografie</trd>
  <trd lng="pl">kartografia</trd>     
  <trd lng="pt">cartografia</trd>
  <trd lng="ro">cartografie</trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
  <trd lng="sk">kartografia</trd>
</drv>
</art>
