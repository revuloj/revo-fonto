<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: orgojl.xml,v 1.18 2015/07/04 08:37:40 revo Exp $">
<kap>
  <rad>orgojl</rad>/o
</kap>
<drv mrk="orgojl.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Granda sento de mem&scirc;ato, ofte akompanata de mal&scirc;ato
      de la aliaj:
      <ekz>
        talentaj &ccirc;iuj kvar estas, tamen iom da <tld/>o ne estas fremda
        al
        la pli junaj<fnt><aut>Christian Declerck</aut>: <vrk>Dua
        kvaropo,
        sproniva kiel la unua</vrk>, <bib>Monato</bib><lok>jaro 1994a,
        numero 4a, p. 22a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="lat">
    <trd>fastus, us</trd>,
    <trd>ferocia, ae</trd>,
    <trd>superbia, ae</trd>
  </trdgrp>
  <trdgrp lng="be">
    <trd>&c_p;&c_y;&c_h;&c_a;</trd>,
    <trd>&c_f;&c_a;&c_n;&c_a;&c_b;&c_e;&c_r;&c_y;&c_ja;</trd>,
    <trd>&c_g;&c_o;&c_n;&c_a;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nadutost</trd>,
    <trd>pýcha</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hochmut</trd>,
    <trd>D&uuml;nkel</trd>,
    <trd>&Uuml;berheblichkeit</trd>,
    <trd>Hochn&auml;sigkeit</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>arrogance</trd>,
    <trd>pride</trd>,
    <trd>vaingloriousness</trd>
  </trdgrp>
  <trd lng="es">arrogancia</trd>
  <trdgrp lng="fr">
    <trd>fatuit&eacute;</trd>,
    <trd>orgueil</trd>,
    <trd>suffisance</trd>,
    <trd>vanit&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>g&odblac;g</trd>,
    <trd>d&ouml;lyf</trd>,
    <trd>fennh&eacute;j&aacute;z&aacute;s</trd>,
    <trd>&ouml;ntelts&eacute;g</trd>
  </trdgrp>
  <trd lng="nl">hoogmoed</trd>
  <trdgrp lng="pt">
    <trd>orgulho</trd>,
    <trd>arrog&acirc;ncia</trd>,
    <trd>soberba</trd>,
    <trd>imod&eacute;stia</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_d;&c_m;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_je;&c_s;&c_mol;</trd>,
    <trd>&c_g;&c_o;&c_n;&c_o;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nadutosť</trd>,
    <trd>nafúkanosť</trd>,
    <trd>spupnosť</trd>
  </trdgrp>
</drv>

<drv mrk="orgojl.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Sentanta grandan memestimon.
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="arogan.0a">aroganta</ref>,
    <ref cel="impert.0a">impertinenta</ref>,
    <ref cel="humil.mal0a">malhumila</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_p;&c_y;&c_h;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_f;&c_a;&c_n;&c_a;&c_b;&c_e;&c_r;&c_y;&c_s;&c_t;&c_y;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_y;&c_sh;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_g;&c_a;&c_n;&c_a;&c_r;&c_y;&c_s;&c_t;&c_y;</trd>,
    <trd>&c_g;&c_a;&c_n;&c_a;&c_r;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">arrogant</trd>
  <trdgrp lng="de">
    <trd>hochm&uuml;tig</trd>,
    <trd>d&uuml;nkelhaft</trd>,
    <trd>&uuml;berheblich</trd>,
    <trd>hochn&auml;sig</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>arrogant</trd>,
    <trd>proud</trd>,
    <trd>vain</trd>
  </trdgrp>
  <trd lng="es">arrogante</trd>
  <trdgrp lng="fr">
    <trd>fat</trd>,
    <trd>fier<klr> (de soi)</klr></trd>,
    <trd>orgueilleux</trd>,
    <trd>vaniteux</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>hoogmoedig</trd>,
    <trd>arrogant</trd>
  </trdgrp>
  <trd lng="it">arrogante</trd>
  <trd lng="pt">arrogante</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_d;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_p;&c_je;&c_s;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>domýšľavý</trd>,
    <trd>nafúkaný</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>domýšlivý</trd>,
    <trd>ješitný</trd>,
    <trd>nadutý</trd>,
    <trd>nafoukaný</trd>
  </trdgrp>
</drv>

<drv mrk="orgojl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Senti troan memfieron:
      <ekz>
        mi, kiel aliaj &leftquot;bonaj a&ubreve;toroj&rightquot; (la
        citiloj malhelpas, ke mi tro <tld/>u), reagos paranoje
        <fnt><aut>Trevor Steele</aut>: <vrk>Paranoja reago
        (1)</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 4a, p.
        4a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_n;&c_a;&c_b;&c_e;&c_r;&c_y;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol;
<ind>&c_p;&c_y;&c_h;&c_l;&c_ib;&c_v;&c_y;&c_m;</ind></trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol;
<ind>&c_f;&c_a;&c_n;&c_a;&c_b;&c_e;&c_r;&c_y;&c_s;&c_t;&c_y;&c_m;</ind></trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol;
<ind>&c_g;&c_a;&c_n;&c_a;&c_r;&c_l;&c_ib;&c_v;&c_y;&c_m;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hochmut zeigen</trd>,
    <trd>hochm&uuml;tig sein</trd>,
    <trd>d&uuml;nkelhaft sein</trd>,
    <trd>&uuml;berheblich sein</trd>,
    <trd>hochn&auml;sig sein</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>infatuer</ind><klr> (de soi)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd><ind>hoogmoedig</ind> zijn</trd>,
    <trd><ind>arrogant</ind> zijn</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_y;&c_t;&c_mol;
<ind>&c_n;&c_a;&c_d;&c_m;&c_je;&c_n;&c_n;&c_y;&c_m;</ind></trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol;
<ind>&c_s;&c_p;&c_je;&c_s;&c_i;&c_v;&c_y;&c_m;</ind></trd>
  </trdgrp>
</drv>
</art>
