<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc><rad>&jcirc;</rad> <fnt><bib>PIV1</bib></fnt>
    </kap>
    <subart>
      <drv mrk="jx.0.konsonanto">
        <kap><tld/></kap>
        <uzo tip="fak">LIN</uzo>
        <uzo tip="fak">GRA</uzo>
        <snc mrk="jx.0.litero">
          <dif>
            La dek kvara
            <ref tip="super" cel="liter.0o.propra">litero</ref> de la
            Esperanta alfabeto (&Jcirc;, &jcirc;).
          </dif>
          <trd lng="fr">&jcirc;<klr> (lettre)</klr></trd>
        </snc>  
        <snc mrk="jx.0.FON">    
          <uzo tip="fak">FON</uzo>
          <dif>
            <ref tip="dif" cel="sxusx.0a.konsonanto">&Scirc;u&scirc;a
            konsonanto</ref>
            vo&ccirc;a, IFAe /&#658;/.
          </dif>
          <refgrp tip="super">
            <ref cel="konson1.0o">konsonanto</ref>,
            <ref cel="frot.0sono">frotsono</ref>
          </refgrp>
          <trd lng="fr">&jcirc;<klr> (consonne)</klr></trd>  
        </snc>
      </drv>
    </subart>

    <subart>
      <drv mrk="jx.0.nomo">
        <kap><ofc>* 7</ofc> <tld/>o</kap>
        <snc>   
          <dif>
            La nomo de tiu
            <ref tip="super" cel="liter.0o.propra">litero</ref>.
          </dif>
          <trd lng="fr">j</trd>
        </snc>
      </drv>
    </subart>
  </art>

