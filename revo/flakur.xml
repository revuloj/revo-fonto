<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>flakurti</rad>/o
</kap>
<drv mrk="flakur.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super"
      cel="flakur1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Flacourtia</trd>), tropikaj vegeta&jcirc;oj kulturataj
      pro la dol&ccirc;aj fruktoj.
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_l;&c_ja;&c_k;&c_u;&c_r;&c_t;&c_y;&c_ja;</trd>
  <trd lng="de">Flacourtia</trd>
  <trdgrp lng="hu">
    <trd>maronszilva</trd>,
    <trd>kormy&aacute;nyz&oacute;szilva</trd>,
    <trd>batok&oacute;szilva</trd>,
    <trd>ramoncsi</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_l;&c_a;&c_k;&c_u;&c_r;&c_t;&c_i;&c_ja;</trd>
</drv>
</art>
