<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: flakur.xml,v 1.18 2019/03/20 21:10:14 revo Exp $">
<kap>
  <rad>flakurti</rad>/o
</kap>
<drv mrk="flakur.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el la familio 
      <ref tip="malprt" cel="salikac.0oj">salikacoj</ref>
      (<trd lng="la">Flacourtia</trd>), tropikaj vegeta&jcirc;oj kun unu specio kulturata
      pro la dol&ccirc;aj fruktoj:
      <ekz>
        la 15 &gcirc;is 17 specioj de <tld/>o kreskas en la tropika Afriko kaj Azio.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/7/72/Flacourtia_.jpg/360px-Flacourtia_.jpg"
      prm="CC BY-SA 3.0"
      ><tld/>o (Flacourtia)
        <fnt>
          <aut>Challiyan, propra verko</aut>
          <lok><url ref="https://commons.wikimedia.org/wiki/File:Flacourtia_.jpg"
              >https://commons.wikimedia.org/wiki/File:Flacourtia_.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_f;&c_l;&c_ja;&c_k;&c_u;&c_r;&c_t;&c_y;&c_ja;</trd>
  <trd lng="ca">g&egrave;nere Flacourtia</trd>
  <trd lng="de">Flacourtia</trd>
  <trdgrp lng="hu">
    <trd>maronszilva</trd>,
    <trd>kormy&aacute;nyz&oacute;szilva</trd>,
    <trd>batok&oacute;szilva</trd>,
    <trd>ramoncsi</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>flacourtia</trd>,
    <trd>flac&uacute;rtia</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_l;&c_a;&c_k;&c_u;&c_r;&c_t;&c_i;&c_ja;</trd>
</drv>
</art>
