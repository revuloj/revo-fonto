<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: purin.xml,v 1.2 2023/08/30 13:29:33 afido Exp $">
    <kap>
      <rad>purin</rad>/o
      <fnt><bib>PIV2</bib></fnt>
    </kap>

<drv mrk="purin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      <frm>C<sub>5</sub>H<sub>4</sub>N<sub>4</sub></frm>, hetero-ducikla aromata amino, kies
      molekulo havas kvar nitrogenatomojn:
      <ekz>
        nukleaj acidoj <klr>[&#8230;]</klr> konstrui&gcirc;as el fosfata acido, el deriva&jcirc;oj de
        pirimidino kaj <tld/>o kaj el kvinkarbon-enhava molekulo de sukero (ribozo
        kaj dezoksiribozo)
        <fnt>
          <vrk><url ref="http://www.eventoj.hu/steb/biologio/enkond-fokuso/f874-p08.htm"
              >Enkonduko en la Biologion</url></vrk>
          <lok>Fokuso, n-ro 1987/4, p. 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o <klr>[&#8230;]</klr> konsistas <klr>[el]</klr> unu pirimidina ringo kaj unu
        imidazola ringo
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Purino"
              >Purino - Vikipedio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/1/12/7H-Purine.svg"
      prm="PD"
      lrg="320"
      >strukturformulo de 7H-<tld/>o
        <fnt>
          <aut>Sponk, propra verko</aut>
          <lok><url ref="&WCW;/File:7H-Purine.svg"
              >https://commons.wikimedia.org/wiki/File:7H-Purine.svg</url></lok>
        </fnt>
    </bld>
    <trd lng="be">&c_p;&c_u;&c_r;&c_y;&c_n;</trd>
    <trd lng="de">Purin</trd>
    <trd lng="en">purine</trd>
    <trd lng="fr">purine</trd>
    <trd lng="ru">&c_p;&c_u;&c_r;&c_i;&c_n;</trd>
  </snc>
</drv>

  </art>

