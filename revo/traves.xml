<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: traves.xml,v 1.22 2024/10/28 14:31:20 revo Exp $">
<kap>
  <rad>travesti</rad>/i
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="traves.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="traves.0i.KOMUNE">
     <dif>
      <ref tip="dif" cel="parodi.0i">Parodii</ref>:
      <ekz>
        <tld/>i Eneidon;
      </ekz>
      <ekz>
        <tld/>ante dira&jcirc;on de la pola verkistino <nom>Zofia Nalkowska</nom>, oni povas diri, ke
        &leftquot;esperantistoj al esperantistoj pretigis &ccirc;i-sorton&#8221;
        <fnt>
          <bib>Monato</bib>
          <aut>Zofia Banet-Fornalowa</aut>
          <vrk><url ref="&Monato;/2005/008642.php">Tre dan&gcirc;era Monato</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mask.0i">maski</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_d;&c_y;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>verkleiden</trd>,
    <trd>parodieren</trd>,
    <trd>kost&uuml;mieren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>travesty</trd>,
    <trd>parody</trd>,
    <trd>burlesque</trd>
  </trdgrp>
  <trd lng="es">parodiar</trd>
  <trdgrp lng="fr">
    <trd>parodier</trd>,
    <trd>pasticher</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>parodiz&aacute;l</trd>,
    <trd>kifigur&aacute;z</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12418;&#12376;&#12427;</trd>,
    <trd>&#33590;&#21270;&#12377;  <pr>&#12385;&#12419;&#12363;&#12377;</pr></trd>,
    <trd>&#20206;&#35013;&#12377;&#12427;  <pr>&#12363;&#12381;&#12358;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>travesteren</trd>,
    <trd>parodi&euml;ren</trd>,
    <trd>verkleden</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>trawestowa&cacute;</trd>,
    <trd>przerabia&cacute; <klr>(utw&oacute;r literacki)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_o;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_r;&c_a;&c_v;&c_je;&c_s;&c_t;&c_ib;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_d;&c_ja;&c_g;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#25103;&#20223; <pr>x&igrave;f&#462;ng</pr></trd>,
    <trd>&#25138;&#20223; <pr>x&igrave;f&#462;ng</pr></trd>,
    <trd>&#25140;&#19978;&#38754;&#20855; <pr>d&agrave;ish&agrave;ngmi&agrave;nj&ugrave;</pr></trd>,
    <trd>&#33945;&#38754; <pr>m&eacute;ngmi&agrave;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="traves.0o">
  <kap><tld/>o</kap>
  <snc mrk="traves.0o.KOMUNE">
    <dif>
      <ref tip="dif" cel="parodi.0o">Parodio</ref>:
      <ekz>
        plej eleganta vira formo estas mizera <tld/>o de la ina
        <fnt><aut>J. Francis</aut>, <vrk>Misio sen Alveno</vrk>,
        <lok>tk-stafeto, 1982</lok></fnt>;
      </ekz>
      <ekz>
        vo&ccirc;o solenatone kun skriba sublinio prezentis la <tld/>on, kiu sonus
        esperantlingve tiel: &leftquot;Kara Futbal-dio, venu Via pilko. Fari&gcirc;u Via ludo. Niajn
        golojn donu al ni hodia&ubreve;. <klr>[&#8230;]</klr>&rightquot;
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2012/010769.php">Patro nia parodia</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="bal.kostum0o">kostumbalo</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_o;&c_d;&c_y;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>kost&yacute;m</trd>,
    <trd>maska</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Verkleidung</trd>,
    <trd>Parodie</trd>,
    <trd>Maskerade</trd>
  </trdgrp>
  <trd lng="en">parody</trd>
  <trd lng="fr">parodie</trd>
  <trdgrp lng="ja">
    <trd>&#12418;&#12376;&#12426;</trd>,
    <trd>&#20206;&#35013;  <pr>&#12363;&#12381;&#12358;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>trawestacja</trd>,
    <trd>przer&oacute;bka <klr>(utworu literackiego)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kost&yacute;m</trd>,
    <trd>maska</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_r;&c_a;&c_v;&c_je;&c_s;&c_t;&c_ib;&c_ja;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_o;&c_d;&c_ib;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#24694;&#25630; <pr>&egrave;g&#462;o</pr></trd>,
    <trd>&#24801;&#25630; <pr>&egrave;g&#462;o</pr></trd>,
    <trd>&#20551;&#38754;&#30446; <pr>ji&#462;mi&agrave;nm&ugrave;</pr></trd>,
    <trd>&#38544;&#34255;&#36523;&#20221;&#30340;&#35013;&#26463; <pr>y&#464;nc&aacute;ngsh&emacron;nf&egrave;ndezhu&amacron;ngsh&ugrave;</pr></trd>,
    <trd>&#38577;&#34255;&#36523;&#20221;&#30340;&#35037;&#26463; <pr>y&#464;nc&aacute;ngsh&emacron;nf&egrave;ndezhu&amacron;ngsh&ugrave;</pr></trd>,
    <trd>&#39166;&#38342; <pr>sh&igrave;b&#462;n</pr></trd>,
    <trd>&#39280;&#26495; <pr>sh&igrave;b&#462;n</pr></trd>,
    <trd>&#21270;&#22918;&#33310;&#20250; <pr>hu&agrave;zhu&amacron;ngw&#468;hu&igrave;</pr></trd>,
    <trd>&#21270;&#22941;&#33310;&#26371; <pr>hu&agrave;zhu&amacron;ngw&#468;hu&igrave;</pr></trd>,
    <trd>&#25103;&#20223; <pr>x&igrave;f&#462;ng</pr></trd>,
    <trd>&#25138;&#20223; <pr>x&igrave;f&#462;ng</pr></trd>
  </trdgrp>
</drv>
</art>
