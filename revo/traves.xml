<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>travesti</rad>/i
  <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="traves.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="traves.0i.KOMUNE">
     <ref tip="dif" cel="parodi.0i">parodii</ref>
      <ekz>
        <tld/>i Eneidon.
      </ekz>
  </snc>
  <trdgrp lng="de">
    <trd>travestieren</trd>,
    <trd>parodieren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>travesty</trd>,
    <trd>parody</trd>,
    <trd>burlesque</trd>
  </trdgrp>
  <trd lng="es">parodiar</trd>
  <trdgrp lng="fr">
    <trd>parodier</trd>,
    <trd>pasticher</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>parodiz&aacute;l</trd>,
    <trd>kifigur&aacute;z</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>travesteren</trd>,
    <trd>parodi&euml;ren</trd>,
    <trd>verkleden</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_o;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="traves.0o">
  <kap><tld/>o</kap>
  <snc mrk="traves.0o.KOMUNE">
    <ref tip="dif" cel="parodi.0i">parodii</ref>
    <ekz>
      plej eleganta vira formo estas mizera <tld/>o de la ina
      <fnt><aut>J. Francis</aut>, <vrk>Misio sen Alveno</vrk>, 
      <lok>tk-stafeto, 1982</lok></fnt>.      
    </ekz>
  </snc>
  <trd lng="fr">parodie</trd>
</drv>
</art>
