<vortaro>
<art mrk="$Id$">
<kap>
<rad>travesti</rad>/i
<fnt>Z</fnt>
</kap>
<drv mrk="traves.0i">
<kap><tld/>i</kap>
<gra><vspec>tr</vspec></gra>
<snc>
<dif>
Aliigi seriozan verkon en burleskan: <ekz><tld/>i Eneidon.
</ekz>
</dif>
<refgrp tip="sin">
<ref cel="parodi.0o">parodio</ref>.
</refgrp>
</snc>
<trdgrp lng="de">
   <trd>travestieren</trd>,
   <trd>parodieren</trd>
</trdgrp>
<trdgrp lng="en">
   <trd>travesty</trd>,
   <trd>parody</trd>,
   <trd>burlesque</trd>
</trdgrp>
<trdgrp lng="nl">
    <trd>travesteren</trd>,
    <trd>parodi&euml;ren</trd>,
    <trd>verkleden</trd>
  </trdgrp>
</drv>
</art>
