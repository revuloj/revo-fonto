<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>respublik</rad>/o
    </kap>
    <drv mrk="respub.0o">
      <kap><tld/>o</kap>
      <snc mrk="respub.0o.regximo">
        <uzo tip="fak">JUR</uzo>
        <dif>
          Polilika <ref tip="super" cel="regxim.0o.POL">re&gcirc;imo</ref>
          en kiu <ref>suvereno</ref> estas (konsiderinda parto de)
          la popolo kaj en kiu regas elektitaj reprezentantoj de
          la popolo. 
        </dif>
        <refgrp tip="vid">
          <ref cel="demokr1.0o">demokratio</ref>.
        </refgrp>
      </snc>
      <snc mrk="respub.0o.sxtato">
        <uzo tip="fak">POL</uzo>
        <dif>
          &Scirc;tato kun <tld/>a re&gcirc;imo:
          <ekz>
            la kvina franca <tld/>o.
          </ekz>
        </dif>
      </snc>
      <trd lng="nl">republiek</trd>
      <trd lng="ru">&c_r;&c_je;&c_s;&c_p;&c_u;&c_b;&c_l;&c_i;&c_k;&c_a;</trd>
    </drv>
    <drv mrk="respub.0ano">
      <kap><tld/>ano</kap>
      <snc mrk="respub.0o.sxtatano">
        <uzo tip="fak">JUR</uzo>
        <dif>
          Civitano de <tld/>o.
        </dif>
      </snc>
      <snc mrk="respub.0o.partiano">
        <uzo tip="fak">POL</uzo>
        <dif>
          Membro de la <tld lit="R"/>a partio de Usono.
        </dif>
      </snc>
      <trd lng="ru">&c_r;&c_je;&c_s;&c_p;&c_u;&c_b;&c_l;&c_i;&c_k;&c_a;&c_n;&c_je;&c_c;</trd>
    </drv>
    <drv mrk="respub.0isto">
      <kap><tld/>isto</kap>
      <snc>
        <uzo tip="fak">POL</uzo>
        <dif>
          Adepto de <tld/>ismo.
        </dif>
        <ref tip="ant" cel="monarh.0isto">monarkisto</ref>
      </snc>
      <trd lng="ru">&c_r;&c_je;&c_s;&c_p;&c_u;&c_b;&c_l;&c_i;&c_k;&c_a;&c_n;&c_je;&c_c;</trd>
    </drv>
  </art>
  <!--
  $Log$
  Revision 1.4  2000/11/09 17:11:34  revo
  Joost Witteveen: +trd nl

  -->
</vortaro>
