<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: genera1.xml,v 1.35 2025/11/16 18:38:00 revo Exp $">
<kap><ofc>*</ofc><ofc>1</ofc><ofc>8</ofc>
<rad>general</rad>/o<fnt><bib>JED</bib></fnt></kap>
<drv mrk="genera1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      En terarmeo kaj aerarmeo,
      <ref tip="super" cel="oficir.0o.MIL">oficiro</ref>, kiu rangas
      super <ref cel="kolone.0o">kolonelo</ref>:
      <ekz>
        <ind>brigada <tld/>o</ind>,
        <trd lng="ca">general de brigada</trd>
        <trd lng="fr">g&eacute;n&eacute;ral de brigade</trd>
        <trd lng="it">generale di brigata<klr> (OF6)</klr></trd>
      </ekz>
      <ekz>
        <ind>divizia <tld/>o</ind>,
        <trd lng="ca">general de divisi&oacute;</trd>
        <trd lng="fr">g&eacute;n&eacute;ral de division</trd>
        <trd lng="it">generale di divisione<klr> (OF7)</klr></trd>
      </ekz>
      <ekz>
        <ind>korpusa <tld/>o</ind>,
        <trd lng="ca">general de cos d'ex&egrave;rcit</trd>
        <trd lng="fr">g&eacute;n&eacute;ral de corps d'arm&eacute;e</trd>
        <trdgrp lng="it">
        <trd>generale di corpo d'armata<klr> (OF8)</klr></trd>,
        <trd>generale di squadra<klr> (aeronautica - OF8)</klr></trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind>armea <tld/>o</ind>
        <trd lng="ca">general d'ex&egrave;rcit</trd>
        <trdgrp lng="it">
  <trd>generale <klr>(OF9 - dal 1997)</klr></trd>,
  <trd>generale d'armata <klr>(OF9 - prima del 1997)</klr></trd>
        </trdgrp>
        <klr>(komandantaj respektive:
        <refgrp tip="vid">
          <ref cel="brigad.0o.MIL">brigadon</ref>,
          <ref cel="divizi.0o.MIL">divizion</ref>,
          <ref cel="korpus.0o.MIL">korpuson</ref> kaj
          <ref cel="arme.0o.sxtata">armeon</ref></refgrp>)</klr>;
        <trd lng="fr">g&eacute;n&eacute;ral d'arm&eacute;e</trd>
      </ekz>
      <ekz>
        inter <tld/>oj parolu pri bataloj
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        bona kaporalo revas esti <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        li estas eks<tld/>o, servinte en la militistaro tridek jarojn, li eksi&gcirc;is
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        kuras la famo, ke la <tld/>o anta&ubreve; unu horo falis
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiamaniere vi estas &ccirc;i tie, granda <tld/>o, kiam viaj &ccirc;efaj fortoj trovi&gcirc;as
        anta&ubreve; nia armeo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_n;&c_e;&c_r;&c_a;&c_l;</trd>
  <trd lng="ca">general <klr tip="amb">(grau militar)</klr></trd>
  <trdgrp lng="cs">
    <trd>gener&aacute;l</trd>,
    <trd>jener&aacute;l</trd>,
    <trd>velmistr &rcaron;&aacute;du</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>General</trd>,
    <trd>Heerf&uuml;hrer</trd>
  </trdgrp>
  <trd lng="en">general <klr>(officer)</klr></trd>
  <trd lng="es">general</trd>
  <trd lng="fr">g&eacute;n&eacute;ral<klr tip="ind"> (militaire)</klr></trd>
  <trdgrp lng="hu">
    <trd>t&aacute;bornok</trd>,
    <trd>gener&aacute;lis</trd>
  </trdgrp>
  <trd lng="it">generale<klr tip="ind"> (grado)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x5C06;&#x8ECD;  <pr>&#x3057;&#x3087;&#x3046;&#x3050;&#x3093;</pr></trd>,
    <trd>&#x5C06;&#x5B98;  <pr>&#x3057;&#x3087;&#x3046;&#x304B;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">generaal</trd>
  <trd lng="no">general</trd>
  <trd lng="pt">general</trd>
  <trd lng="ru">&c_g;&c_je;&c_n;&c_je;&c_r;&c_a;&c_l;</trd>
  <trd lng="sk">gener&aacute;l</trd>
  <trd lng="sv">general</trd>
  <trd lng="tr">general</trd>
  <trd lng="uk">&c_gu;&c_je;&c_n;&c_je;&c_r;&c_a;&c_l;</trd>
  <trdgrp lng="zh">
    <trd>&#x5927;&#x5C06; <pr>d&agrave;ji&amacron;ng</pr></trd>,
    <trd>&#x5C06;&#x519B; <pr>ji&amacron;ngj&umacron;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="genera1.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="genera1.0a.rilata">
    <dif>
      Rilata al la generalo:
      <ekz>
        via <tld/>a mo&scirc;to
        <fnt><bib>F</bib>, <lok>Universala Vortaro, &ccirc;e
        mo&scirc;to</lok></fnt>;
      </ekz>
      <ekz>
        li tie ricevos <tld/>an rangon
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    Ne konfuzu kun <ref tip="hom" cel="gxener.0a">&gcirc;enerala</ref>.
  </rim>
  <trd lng="be">&c_g;&c_je;&c_n;&c_e;&c_r;&c_a;&c_l;&c_mol;&c_s;&c_k;&c_ib;</trd>
  <trd lng="en">general <klr>(officer)</klr></trd>
  <trd lng="uk">&c_gu;&c_je;&c_n;&c_je;&c_r;&c_a;&c_l;&c_mol;&c_s;&c_mol;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="genera1.cxef0o">
  <kap>&ccirc;ef<tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Plej supera <tld/>o, kiu komandas la tutan militistaron kaj
      &ccirc;iujn aliajn <tld/>ojn de lando a&ubreve; liganaro:
      <ekz>
        la &ccirc;efa <tld/>o ekspedis spionojn al la proksimi&gcirc;antaj Libianoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">
&c_g;&c_je;&c_n;&c_e;&c_r;&c_a;&c_l;&c_ib;&c_s;&c_ib;&c_m;&c_u;&c_s;</trd>
  <trdgrp lng="ca">
    <trd>general en cap</trd>,
    <trd>cap de l&#x2019;ex&egrave;rcit</trd>,
    <trd>capit&agrave; general</trd>,
    <trd>general&iacute;ssim</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Feldmarschall</trd>,
    <trd>Generalfeldmarschall</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd><ind>commander</ind> in chief</trd>,
    <trd>supreme <ind>commander</ind></trd>,
    <trd><ind>general</ind>issimo</trd>
  </trdgrp>
  <trd lng="es">capit&aacute;n general</trd>
  <trd lng="fr">g&eacute;n&eacute;ral en chef</trd>
  <trd lng="ja">&#x7DCF;&#x53F8;&#x4EE4;&#x5B98;  <pr>&#x305D;&#x3046;&#x3057;&#x308C;&#x3044;&#x304B;&#x3093;</pr></trd>
  <trd lng="nl">veldmaarschalk</trd>
  <trd lng="pt">general&iacute;ssimo</trd>
  <trd lng="ru">
&c_g;&c_je;&c_n;&c_je;&c_r;&c_a;&c_l;&c_i;&c_s;&c_s;&c_i;&c_m;&c_u;&c_s;</trd>
  <trdgrp lng="it">
  <trd>capo di Stato Maggiore della Difesa<klr> (dal 1997)</klr></trd>,
  <trd>capo di Stato Maggiore<klr> (dal 1948 al 1997)</klr></trd>,
  <trd>Maresciallo<klr> (durante Regno d'Italia)</klr></trd>,
  </trdgrp>
  <trd lng="uk">&c_gu;&c_je;&c_n;&c_je;&c_r;&c_a;&c_l;&c_ib;&c_s;&c_i;&c_m;&c_u;&c_s;</trd>
  <trd lng="zh">&#x9322;&#x6C5F;&#x4E00;&#x6A4B; <pr>qi&aacute;nji&amacron;ngy&imacron;qi&aacute;o</pr></trd>
</drv>
</art>
