<kap>
  <rad>drelik</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="drelik.0o">
  <kap><tld/>o</kap>
  <snc mrk="drelik.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Maldelikata <ref tip="super" cel="sxtof.0o.TEKS">&scirc;tofo</ref>, tola&jcirc;o por sakoj,
      laborvestoj kaj aliaj:
      <ekz>
        li portis uniformon el ordinara <tld/>o, tute sen insignoj
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de
        Diego</aut>, <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>;
      </ekz>
      <ekz>
        kun nenio vestita krom blua <tld/>a kalsono      
        <fnt>
          <aut>Rudyard Kipling, Trad. Wouter F. Pilger</aut>,
          <vrk>Tiel do: Rakontoj por malgrandaj infanoj</vrk>,
          <lok><url ref="&Gutenberg;ebooks/48900">gutenberg.org</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        Irante en <tld/>aj, a&ubreve; perkal&#x2019;-robetoj, Ili brut-pa&scirc;tas ne en bast&#x2019;-, sed
        led-&scirc;uetoj
        <fnt>
          <bib>SrT</bib>
          <lok>Libro VI.</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_d;&c_n;&c_o;</trd>,
    <trd>&c_r;&c_a;&c_d;&c_n;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_d;&c_e;&c_n;&c_ib;&c_m;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_a;&c_m;&c_ja;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hrub&aacute; l&aacute;tka</trd>,
    <trd>pytlovina</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Drillich</trd>,
    <trd>Drell</trd>
  </trdgrp>
  <trd lng="en">denim</trd>
  <trd lng="es">tela <ind>vaquera</ind></trd>
  <trd lng="fr">treillis<klr> (toile de chanvre)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x7C97;&#x5E03;  <pr>&#x305D;&#x3075;</pr></trd>,
    <trd>&#x30BA;&#x30C3;&#x30AF;&#x5730;  <pr>&#x30BA;&#x30C3;&#x30AF;&#x3061;</pr></trd>,
    <trd>&#x30C7;&#x30CB;&#x30E0;&#x5730;  <pr>&#x30C7;&#x30CB;&#x30E0;&#x3061;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>drelich</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>lona</trd>,
    <trd>riscado grosso</trd>,
    <trd>pano para saco</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_r;&c_ju;&c_g;&c_a;</trd>,
    <trd>&c_r;&c_o;&c_g;&c_o;&c_zh;&c_a;</trd>,
    <trd>&c_b;&c_r;&c_je;&c_z;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hrub&aacute; l&aacute;tka</trd>,
    <trd>vrecovina</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_i;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_o;&c_t;&c_n;&c_o;</trd>,
    <trd>&c_b;&c_r;&c_je;&c_z;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
</drv>
