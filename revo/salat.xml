<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>salat</rad>/o
</kap>
<drv mrk="salat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="salat.0o.legomo">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;a&jcirc;o konsistanta el fre&scirc;aj a&ubreve; boligitaj herboj a&ubreve; legomoj
      spicitaj per salo, vinagro kaj oleo.
    </dif>
    <trd lng="ru">&c_v;&c_i;&c_n;&c_je;&c_g;&c_r;&c_je;&c_t;</trd>
  </snc>
  <snc mrk="salat.0o.trancxitaj">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Miksita man&gcirc;a&jcirc;o &gcirc;enerale, konsistanta el diversaj tran&ccirc;itaj eroj:
      <ekz>
        frukto<tld/>o;
      </ekz>
      <ekz>
        haringa <tld/>o;
      </ekz>
      <ekz>
        por hungara viando-<tld/>o oni bezonas 300 g da sengrasa bova&jcirc;o,
        300 g da salamo, 200 g da cepo, 100 g da verda papriko kaj
        1 kuleron da ru&gcirc;a pipro.
        <fnt><bib>IntKui</bib></fnt>
      </ekz>
    </dif>
  </snc>
  <snc mrk="salat.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senorda konfuza miksa&jcirc;o da tre diversaj a&jcirc;oj:
      <ekz>
        tiu libro estas vera <tld/>o el klarigoj, konsiloj, versoj, &scirc;ercoj.
      </ekz>
    </dif>
    <trd lng="ru">&c_m;&c_je;&c_sh;&c_a;&c_n;&c_i;&c_n;&c_a;</trd>
  </snc>
  <trd lng="de">Salat</trd>
  <trd lng="en">salad</trd>
  <trd lng="es">ensalada</trd>
  <trd lng="he">&samekh;&lamed;&tet;</trd>
  <trd lng="hr">salata</trd>
  <trdgrp lng="nl">
    <trd>salade</trd>,
    <trd>sla</trd>,
    <trd>slaatje</trd>
  </trdgrp>
  <trd lng="pl">sa&lstroke;atka</trd>
  <trd lng="ru">&c_s;&c_a;&c_l;&c_a;&c_t;</trd>
</drv>
</art>
