<drv mrk="elektr.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="elektr.0ejo.TEK">
    <uzo tip="fak">ELE</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Fabriko, kie oni produktas <tld/>an energion por lumigado,
      <tld/>aj publikaj transportoj, hejmaj kaj industriaj bezonoj;
      elektro<ref tip="super" cel="centra1.0o.ELE">centralo</ref>:
      <ekz>
        &ccirc;e&hcirc;a opinio pri <ind>atom<tld/>ejo</ind> en
        <nom>Temel&iacute;n</nom>
        <fnt><bib>Monato</bib> <lok>2002</lok></fnt>;
        <trdgrp lng="be">
          <trd>&c_A;&c_E;&c_S;</trd>,
          <trd>&c_a;&c_t;&c_a;&c_m;&c_n;&c_a;&c_ja; &c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_a;&c_s;&c_t;&c_a;&c_n;&c_c;&c_y;&c_ja;</trd>
        </trdgrp>
        <trdgrp lng="de">
           <trd>Atomkraftwerk</trd>
           <trd>Kernkraftwerk</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_A;&c_E;&c_S;</trd>,
          <trd>&c_a;&c_t;&c_o;&c_m;&c_n;&c_a;&c_ja; &c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_ja;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        po 0,89 &scirc;ilingoj por kilovathoro da <tld/>a energio el <ind>vent<tld/>ejo</ind>j
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2001/007271.php">Ventelektrejoj en
          ofensivo</url></vrk>
        </fnt>.
        <trdgrp lng="de">
          <trd>Windkraftwerk</trd>,
          <trd>Windrad</trd>,
          <trd>Windgenerator</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_a;&c_s;&c_t;&c_a;&c_n;&c_c;&c_y;&c_ja;</trd>
  <trd lng="ca">central <ind>el&egrave;ctric</ind>a</trd>
  <trd lng="cs">elektr&aacute;rna</trd>
  <trd lng="de">Kraftwerk</trd>
  <trd lng="es">central <ind>electrica</ind></trd>
  <trd lng="fr"><ind>centrale</ind> &eacute;lectrique</trd>
  <trd lng="he">&tav;&het;&nun;&tav; &kaf;&vav;&het;</trd>
  <trd lng="hu">villamos <ind>er&odblac;m&udblac;</ind></trd>
  <trd lng="id">pem<ind>bangkit</ind> listrik</trd>
  <trd lng="ja">&#x767A;&#x96FB;&#x6240;  <pr>&#x306F;&#x3064;&#x3067;&#x3093;&#x3057;&#x3087;</pr></trd>
