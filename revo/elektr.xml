<drv mrk="elektr.0akampintenso">
  <kap><tld/>a kampintenso</kap>
  <fnt><bib>PIV1</bib>, <lok>artikolo: kampo</lok></fnt>
  <snc>
    <uzo tip="fak">ELE</uzo>
    <dif>
      <ref tip="super" cel="grand.fizika0o">Fizika grando</ref>,
      prezentebla per vektora <ref cel="kamp.0o.MAT">kampo</ref> kaj
      esprimanta la direkton kaj intenson de la efiko de <tld/>aj
      &scirc;argoj unuj super aliaj; simbolo E:
      <ekz>
        la <ref cel="dimens.0o.FIZ">dimensioj</ref> de
        <tld/>a kampo estas
        <frm>M<sup>1</sup>L<sup>1</sup>T<sup>-3</sup>I<sup>-1</sup></frm>;
      </ekz><ekz>
        en <tld/>a kampo E korpo kun <tld/>a &scirc;argo q estas
        almetita
        al <ref cel="fort.0o.FIZ">forto</ref> egala al q.E
        (vektore).
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="volt.v0ojemetro">volto je metro</ref>;
    </refgrp>
    <refgrp tip="sin">
      <ref cel="elektr.0akampo.grando"><tld/>a kampo <sncref ref="elektr.0akampo.grando"/></ref>.
    </refgrp>
    <rim>
      &Ccirc;ar oni prezentas tiun grandon per matematika kampo, oni
      ofte
      skribas <ctl><tld/>a kampo</ctl> anstata&ubreve; la pli pedantan
      <ctl><tld/>a kampintenso</ctl>.
    </rim>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_p;&c_r;&c_u;&c_zh;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol; &c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_g;&c_a; &c_p;&c_o;&c_l;&c_ja;</trd>
  <trd lng="ca">intensitat del camp el&egrave;ctric</trd>
  <trd lng="es"><ind>intensidad</ind> del campo el&eacute;ctrico</trd>
  <trd lng="de">elektrische Feldst&auml;rke</trd>
  <trd lng="fr"><klr tip="ind">(intensit&eacute; du) </klr>champ
    &eacute;lectrique</trd>
  <trd lng="he">&het;&vav;&zayin;&qof; &shin;&dalet;&he;
  &het;&shin;&mem;&lamed;&yod;</trd>
  <trd lng="hu">villamos
    <ind>t&eacute;rer&odblac;ss&eacute;g</ind></trd>
  <trd lng="id"><ind>intensitas</ind> medan listrik</trd>
  <trdgrp lng="ja">
    <trd>&#x96FB;&#x5834;&#x306E;&#x5F37;&#x3055;  <pr>&#x3044;&#x306A;&#x305A;&#x307E;&#x3058;&#x3087;&#x3046;&#x306E;&#x3064;&#x3088;&#x3055;</pr></trd>,
    <trd>&#x96FB;&#x754C;&#x306E;&#x5F37;&#x3055;  <pr>&#x3044;&#x306A;&#x305A;&#x307E;&#x304B;&#x3044;&#x306E;&#x3064;&#x3088;&#x3055;</pr></trd>
  </trdgrp>
  <trd lng="pl">nat&eogonek;&zdot;enie pola elektrycznego</trd>
  <trd lng="ru">&c_n;&c_a;&c_p;&c_r;&c_ja;&c_zh;&c_jo;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol; &c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_g;&c_o; &c_p;&c_o;&c_l;&c_ja;</trd>
</drv>

