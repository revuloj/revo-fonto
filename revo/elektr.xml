    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_d;&c_r;&c_a;&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
