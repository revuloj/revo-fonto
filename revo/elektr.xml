<drv mrk="elektr.akv0a">
  <kap>
    akv<tld/>a, <var><kap>akvo<tld/>a</kap></var>
  </kap>
  <snc mrk="elektr.akv0a.TEK">
    <uzo tip="fak">ELE</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Rilata al <tld/>o produktata per la
      <ref tip="vid" cel="hidrau.0a">hidra&ubreve;lika</ref>
      energio de fluanta akvo;
      <ref tip="sin" cel="hidroe.0a.TEK">hidroelektra</ref>:
      <ekz>
        akv<tld/>aj centraloj
        <fnt>
          <aut>-</aut>
          <vrk><url ref="&Viki;/Bara%C4%B5o_Tri_Gor%C4%9Doj">Bara&jcirc;o Tri Gor&gcirc;oj</url></vrk>
          <lok>Vikipedio, 2022-05-20</lok>
        </fnt>;
      </ekz>
      <ekz>
        la akvo<tld/>a stacio <nom>Hinckley</nom>, konstruita de &ccirc;ina
        elektrokonstruada kompanio <nom>Power China</nom>,
        ne havis dama&gcirc;on en tiu &ccirc;i tertremo
        <fnt>
          <aut>Zhang Xiaotong</aut>
          <vrk><url ref="http://espero.chinareports.org.cn/2016-04/25/content_38320430.htm">Konstrua&jcirc;oj faritaj de &Ccirc;inio ne estis dama&gcirc;itaj en la 7.8-magnituda tertremo en
              Ekvadoro</url></vrk>
