<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>elektr</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="elektr.0o">
  <kap><tld/>o</kap>
  <snc mrk="elektr.0o.FIZ">
    <uzo tip="fak">ELE</uzo>
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Unu el la formoj de fizika energio, kiu montri&gcirc;as per
      lumaj, termikaj kaj me&hcirc;anikaj fenomenoj.
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_je;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="de">Elektrizit&auml;t</trd>
  <trd lng="en">electricity</trd>
  <trd lng="es">electricidad</trd>
  <trd lng="fr">&eacute;lectricit&eacute;</trd>
  <trdgrp lng="hu">
    <trd>villamoss&aacute;g</trd>,
    <trd>elektromoss&aacute;g</trd>
  </trdgrp>
  <trd lng="nl">elektriciteit</trd>
  <trd lng="pl">elektryczno&sacute;&cacute;</trd>
  <trd lng="pt">eletricidade</trd>
  <trd
  lng="ru">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="sv">elektricitet</trd>
</drv>

<drv mrk="elektr.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Produktata de <tld/>o:
      <ekz>
        <tld/>a fajrero, lumo;
      </ekz>
      <ekz>
        <tld/>aj fenomenoj.
      </ekz>
    </dif>
    <trd lng="pl">elektryczny</trd>
  </snc>
  <snc>
    <dif>
      Rilatanta al <tld/>o:
      <ekz>
        <tld/>a ma&scirc;ino, kondukilo;
      </ekz>
      <ekz>
        <ind><tld/>a forno</ind>;
        <trd lng="de">Elektroherd</trd>
        <trdgrp lng="hu">
          <trd>villamos <ind>kemence</ind></trd>,
          <trd>elektromos <ind>kemence</ind></trd>
        </trdgrp>
        <trd lng="pl">piec elektryczny</trd>
      </ekz>
      <ekz>
        <ind><tld/>aj unuoj</ind>
        <klr>(ampero, farado, &jcirc;ulo, volto, vato ktp)</klr>.
        <trd lng="de">elektrische Einheiten</trd>
        <trd lng="hu">villamos
        <ind>m&eacute;rt&eacute;kegys&eacute;gek</ind></trd>
      </ekz>
    </dif>
    <trd lng="pl">elektryczny</trd>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="bg">&c_je;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;</trd>
  <trd lng="de">elektrisch</trd>
  <trd lng="es">el&eacute;ctrico</trd>
  <trd lng="fr">&eacute;lectrique</trd>
  <trdgrp lng="hu">
    <trd>villamos</trd>,
    <trd>elektromos</trd>
  </trdgrp>
  <trd lng="nl">elektrisch</trd>
  <trd
  lng="ru">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sv">elektrisk</trd>
</drv>

<drv mrk="elektr.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi <tld/>a, funkciigi per <tld/>o:
      <ekz>
        oni <tld/>igis la fervojon en nia lando.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_f;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_je;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_f;&c_i;&c_c;&c_i;&c_r;&c_a;&c_m;</trd>
  <trd lng="de">elektrifizieren</trd>
  <trd lng="es">electrificar</trd>
  <trd lng="fr">&eacute;lectrifier</trd>
  <trd lng="hu">villamos&iacute;t</trd>
  <trd lng="nl">elektrificeren</trd>
  <trd lng="pl">elektryfikowa&cacute;</trd>
  <trd lng="ru">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_f;&c_i;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;
  <klr>(&c_o;&c_b;&c_je;&c_s;&c_p;&c_je;&c_ch;&c_i;&c_t;&c_mol; &c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;&c_m;)</klr></trd>
</drv>

<drv mrk="elektr.0umi">
  <kap><tld/>umi, <var><kap><tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Igi korpon kapabla produkti <tld/>ajn fenomenojn:
      <ekz>
        <tld/>(um)i sukcenon per frotado.
      </ekz>
    </dif>
    <trd lng="hu">felt&ouml;lt elektromoss&aacute;ggal</trd>
    <trdgrp lng="pl">
      <trd>elektryzowa&cacute;</trd>,
      <trd>naelektryzowa&cacute;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Meti sub la influo de <tld/>o, kuraci per <tld/>o:
      <ekz>
        en la <tld/>oterapio oni <tld/>(um)as la malsanulojn.
      </ekz>
    </dif>
    <trd lng="hu">elektromosan kezel</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Eksciti, entuziasmigi, movigi, skui:
      <ekz>
        Zamenhof <tld/>(um)adis la kongresanaron per siaj paroladoj.
      </ekz>
    </dif>
    <trd lng="hu">felvillanyoz</trd>
    <trdgrp lng="pl">
      <trd>elektryzowa&cacute;</trd>,
      <trd>naelektryzowa&cacute;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_n;&c_a;&c_je;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_z;&c_i;&c_r;&c_v;&c_a;&c_m;</trd>
  <trd lng="de">elektrisieren</trd>
  <trd lng="fr">&eacute;lectriser</trd>
  <trd lng="nl">elektriceren</trd>
  <trd lng="ru">&c_n;&c_a;&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="elektr.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="elektr.0ejo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Fabriko, kie oni produktas <tld/>an energion por lumigado,
      <tld/>aj publikaj transportoj, hejmaj kaj industriaj bezonoj
      ktp:
      <ekz>
        &leftquot;&Ccirc;e&hcirc;a opinio pri <ind>atom<tld/>ejo</ind>
        en Temel&iacute;n&rightquot;
        <fnt><lok>Monato, 2002</lok></fnt>.
        <trdgrp lng="ru">
          <trd>&c_A;&c_E;&c_S;</trd>,
          <trd>&c_a;&c_t;&c_o;&c_m;&c_n;&c_a;&c_ja;
          &c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_ja;</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_a;&c_s;&c_t;&c_a;&c_n;&c_c;&c_y;&c_ja;</trd>
  <trd lng="de">Kraftwerk</trd>
  <trd lng="es">central <ind>electrica</ind></trd>
  <trd lng="fr"><ind>centrale</ind> &eacute;lectrique</trd>
  <trd lng="hu">villamos <ind>er&odblac;m&udblac;</ind></trd>
  <trd lng="nl">elektriciteitscentrale</trd>
  <trd lng="pl">elektrownia</trd>
  <trd lng="ru">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_ja;</trd>
</drv>

<drv mrk="elektr.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="elektr.0isto.ELE">
    <dif>
      Specialisto en la <tld/>oscienco a&ubreve; <tld/>otekniko.
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_k;</trd>
  <trd lng="bg">&c_je;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_t;&c_je;&c_h;&c_n;&c_i;&c_k;</trd>
  <trd lng="de">Elektriker</trd>
  <trd lng="es">electricista</trd>
  <trd lng="fr">&eacute;lectricien</trd>
  <trdgrp lng="hu">
    <trd>elektrikus</trd>,
    <trd>elektrotechnikus</trd>
  </trdgrp>
  <trd lng="nl">elektricien</trd>
  <trdgrp lng="pl">
    <trd>elektryk</trd>,
    <trd>elektrotechnik</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_k;</trd>
  <trd lng="sv">elektriker</trd>
</drv>

<drv mrk="elektr.0afajrero">
  <kap><tld/>a fajrero</kap>
  <snc>
    <uzo tip="fak">ELE</uzo>
    <dif>
      Fajrero, kiu esti&gcirc;as inter elektrodoj apartigitaj per
      dielektriko, se la tensio estas sufi&ccirc;e alta:
      <ekz>
        por estigi
        fajreron en la aero &ccirc;e la distanco de elektrodoj je 1,5
        cm,
        estas necesa tensio je &ccirc;irka&ubreve; 17&nbsp;000 voltoj,
        krome
        dependa de la formo de elektrodoj.
      </ekz>
    </dif>
    <adm>
      &Ccirc;u vere temas pri tio? Precizigi la nuancon disde "arko".
      Referenci "sparko". <aut>MB</aut>
    </adm>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_s;&c_k;&c_r;&c_a; <klr
    tip="amb">(&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_y;)</klr></trd>,
    <trd>&c_r;&c_a;&c_z;&c_r;&c_a;&c_d; <klr
    tip="amb">(&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_y;)</klr></trd>
  </trdgrp>
  <trd lng="de">elektrischer Lichtbogen</trd>
  <trd lng="es">chispa el&eacute;ctrica</trd>
  <trdgrp lng="hu">
    <trd>elektromos <ind>szikra</ind></trd>,
    <trd>villamos <ind>szikra</ind></trd>
  </trdgrp>
  <trd lng="pl">iskra elektryczna</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_k;&c_r;&c_a;<klr tip="amb">
    (&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja;)</klr></trd>,
    <trd>&c_r;&c_a;&c_z;&c_r;&c_ja;&c_d;<klr tip="amb">
    (&c_t;&c_o;&c_k;&c_a;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="elektr.0afluo">
  <kap><tld/>a fluo</kap>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <uzo tip="fak">ELE</uzo>
    <ref tip="dif" cel="kurent.0o">kurento</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_y; &c_t;&c_o;&c_k;</trd>,
    <trd>&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_a;&c_t;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_t;&c_o;&c_k;</trd>
  <trd lng="de">Strom</trd>
  <trd lng="es">corriente el&eacute;ctrica</trd>
  <trd lng="fr"><ind>courant</ind> &eacute;lectrique</trd>
  <trd lng="hu">villamos <ind>&aacute;ram</ind></trd>
  <trd lng="nl">stroom</trd>
  <trd lng="pl">pr&aogonek;d elektryczny</trd>
  <trd lng="ru">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; <ind>&c_t;&c_o;&c_k;</ind></trd>
</drv>

<drv mrk="elektr.0akampo">
  <kap><tld/>a kampo</kap>
  <snc mrk="elektr.0akampo.spaco">
    <uzo tip="fak">ELE</uzo>
    <fnt><bib>PIV1</bib>, <lok>artikolo: kampo</lok></fnt>
    <dif>
      Parto de spaco, kie estas sentebla la efiko de <tld/>aj
      &scirc;argoj super &scirc;argitaj korpoj.
    </dif>
  </snc>
  <snc mrk="elektr.0akampo.grando">
    <fnt><bib>PIV1</bib>, <lok>artikolo: tensio</lok></fnt>
    <ref tip="dif" cel="elektr.0akampintenso"><tld/>a kampintenso.</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_je; &c_p;&c_o;&c_l;&c_je;</trd>,
    <trd>&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_a;&c_s;&c_t;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_je; &c_p;&c_o;&c_l;&c_je;</trd>
  </trdgrp>
  <trd lng="es">campo el&eacute;ctrico</trd>
  <trd lng="fr">champ &eacute;lectrique</trd>
  <trd lng="hu">villamos t&eacute;r</trd>
  <trd lng="de">elektrisches Feld</trd>
  <trdgrp lng="pl">
    <trd>pole elektryczny</trd>,
    <trd>pole elektrostatyczne</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; &c_p;&c_o;&c_l;&c_je;</trd>,
    <trd>&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_s;&c_t;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; &c_p;&c_o;&c_l;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="elektr.0akampintenso">
  <kap><tld/>a kampintenso</kap>
  <fnt><bib>PIV1</bib>, <lok>artikolo: kampo</lok></fnt>
  <snc>
    <uzo tip="fak">ELE</uzo>
    <dif>
      <ref tip="super" cel="grand.fizika0o">Fizika grando</ref>,
      prezentebla per vektora <ref cel="kamp.0o.MAT">kampo</ref> kaj
      esprimanta la direkton kaj intenson de la efiko de <tld/>aj
      &scirc;argoj unuj super aliaj; simbolo E:
      <ekz>
        la <ref cel="dimens.0o.FIZ">dimensioj</ref> de
        <tld/>a kampo estas
        <frm>M<sup>1</sup>L<sup>1</sup>T<sup>-3</sup>I<sup>-1</sup></frm>;
      </ekz>
      <ekz>
        en <tld/>a kampo E korpo kun <tld/>a &scirc;argo q estas
        almetita
        al <ref cel="fort.0o.FIZ">forto</ref> egala al q.E (vektore).
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="volt.v0ojemetro">volto je metro</ref>;
    </refgrp>
    <refgrp tip="sin">
      <ref cel="elektr.0akampo.grando"><tld/>a kampo <sncref
      ref="elektr.0akampo.grando"/></ref>.
    </refgrp>
    <rim>
      &Ccirc;ar oni prezentas tiun grandon per matematika kampo, oni
      ofte
      skribas <ctl><tld/>a kampo</ctl> anstata&ubreve; la pli pedantan
      <ctl><tld/>a kampintenso</ctl>.
    </rim>
  </snc>
  <trd
  lng="be">&c_n;&c_a;&c_p;&c_r;&c_u;&c_zh;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol; &c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_g;&c_a; &c_p;&c_o;&c_l;&c_ja;</trd>
  <trd lng="es"><ind>intensidad</ind> del campo el&eacute;ctrico</trd>
  <trd lng="de">elektrische Feldst&auml;rke</trd>
  <trd lng="fr"><klr tip="ind">(intensit&eacute; du) </klr>champ
  &eacute;lectrique</trd>
  <trd lng="hu">villamos
  <ind>t&eacute;rer&odblac;ss&eacute;g</ind></trd>
  <trd lng="pl">nat&eogonek;&zdot;enie pola elektrycznego</trd>
  <trd
  lng="ru">&c_n;&c_a;&c_p;&c_r;&c_ja;&c_zh;&c_jo;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol; &c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_g;&c_o; &c_p;&c_o;&c_l;&c_ja;</trd>
</drv>

<drv mrk="elektr.0astrecxo">
  <kap><tld/>a stre&ccirc;o</kap>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <uzo tip="fak">ELE</uzo>
    <ref tip="dif" cel="tensi.0o">tensio</ref>
  </snc>
  <trd
  lng="be">&c_n;&c_a;&c_p;&c_r;&c_u;&c_zh;&c_a;&c_n;&c_mol;&c_n;&c_je; <klr tip="amb">(&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_je;)</klr></trd>
  <trd lng="de">elektrische Spannung</trd>
  <trd lng="es">voltaje</trd>
  <trd lng="fr"><ind>tension</ind> &eacute;lectrique</trd>
  <trd lng="hu">villamos <ind>fesz&uuml;lts&eacute;g</ind></trd>
  <trd lng="nl">spanning</trd>
  <trd lng="pl">napi&eogonek;cie elektryczne</trd>
  <trd lng="ru">&c_n;&c_a;&c_p;&c_r;&c_ja;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="elektr.0okvanto">
  <kap><tld/>okvanto</kap>
  <snc>
    <uzo tip="fak">ELE</uzo>
    <ref tip="dif" cel="sxarg.0o.grando">&scirc;argo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_o;&c_l;&c_mol;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;
    &c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_y;
    <ind>&c_z;&c_a;&c_r;&c_a;&c_d;</ind></trd>
  </trdgrp>
  <trd lng="hu">villamos <ind>t&ouml;lt&eacute;s</ind></trd>
  <trd lng="pl">&lstroke;adunek elektryczny</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;
    &c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;
