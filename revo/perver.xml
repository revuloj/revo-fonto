<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>pervers</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="perver.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Inklina al malbonfarado, malica:
      <ekz>
        pruntado de mono kontra&ubreve; interezo estas <tld/>a maniero akiri
        hava&jcirc;on, la&ubreve; Aristotelo<fnt><aut>Bo Sandelin</aut>:
        <vrk>Anta&ubreve;merkantilisma ekonomia pensado</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 2a, p. 16a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>pernicieux</trd>
    </trdgrp>
    <trd lng="hu">elvetem&uuml;lt</trd>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_v;&c_r;&c_a;&c_shch;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc>
    <dif>
      Inklina al nenormalaj voluptoj.
    </dif>
    <trdgrp lng="fr">
      <trd>d&eacute;prav&eacute;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>perverz</trd>,
      <trd>fajtalan</trd>
    </trdgrp>
    <trd lng="ru">&c_i;&c_z;&c_v;&c_r;&c_a;&c_shch;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trd lng="de">pervers</trd>
  <trdgrp lng="fr">
    <trd>pervers</trd>,
    <trd>vicieux</trd>
  </trdgrp>
  <trd lng="nl">pervers</trd>
</drv>

<drv mrk="perver.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      &Gcirc;ue agante kun malico:
      <ekz>
        la emo de a&jcirc;oj kaj aferoj misiri kvaza&ubreve; <tld/>e<fnt><aut>Marjorie
        Boulton</aut>: <vrk>Tiu le&gcirc;o de Murphy</vrk>,
        <bib>Monato</bib><lok>jaro 1997a, numero 4a, p. 26a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>pernicieusement</trd>,
    <trd>vicieusement</trd>
  </trdgrp>
  <trd lng="hu">perverz m&oacute;don</trd>
</drv>
</art>
