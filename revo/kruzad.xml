<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kruzad.xml,v 1.8 2015/07/04 08:37:38 revo Exp $">
<kap>
  <rad>kruzad</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="kruzad.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Antikva portugala <ref tip="super" cel="mon.0ero">monero</ref> el
      oro:
      <ekz>
        li postlasis <klr>[...]</klr> iom da mono en <tld/>oj kaj oraj
        eskudoj
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 63a, p.
        760a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_u;&c_z;&c_a;&c_d;&c_a;</trd>
  <trd lng="ca">cruzado</trd>
  <trd lng="cs">starý portugalský zlatý peníz</trd>
  <trd lng="es">cruzado</trd>
  <trd lng="fr">cruzade</trd>
  <trd lng="pt">cruzado <klr tip="amb">(moeda)</klr></trd>
  <trd lng="ru">&c_k;&c_r;&c_u;&c_z;&c_a;&c_d;&c_o;</trd>
  <trd lng="sk">starý portugalský zlatý peniaz</trd>
</drv>
</art>

