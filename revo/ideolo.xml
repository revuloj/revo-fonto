<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ideolo.xml,v 1.30 2023/11/19 10:22:02 revo Exp $">
<kap>
  <ofc>9</ofc><rad>ideologi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ideolo.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="ideolo.0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Scienco pri la deveno kaj formi&gcirc;o de la ideoj.
    </dif>
    <trd lng="nl">ideologie</trd>
  </snc>
  <snc mrk="ideolo.0o.KOMUNE">
    <dif>
      <ref tip="dif" cel="ide.0aro.FIL">Idearo</ref>:
      <ekz>
        politika <tld/>o
        <fnt>
          <aut>Guido van Damme</aut>
            <vrk><url ref="&Monato;/2001/007242.php">Supra&jcirc;e rozkolora</url></vrk>
            <bib>Monato</bib>
            <lok>2001/02, p. 29</lok>
          </fnt>;
      </ekz>
      <ekz>
        liberalisma <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Claude PIRON</aut>
          <vrk><url ref="&Monato;/2003/008141.html">Francio invitas, Svislando suferas</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        imperiisma <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolai Gudskov</aut>
          <vrk><url ref="&Monato;/2003/008029.html">Ordinara fa&scirc;ismo</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Romm</nom> montras la terura&jcirc;ojn de la fa&scirc;isma-nazia <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolai Gudskov</aut>
          <vrk><url ref="&Monato;/2003/008029.html">Ordinara fa&scirc;ismo</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estas anta&ubreve; &ccirc;io interpreto pri la sociaj kaj similaj
        aferoj
        <fnt>
          <aut>E. Pauli</aut>,
          <vrk><url ref="http://www.cfh.ufsc.br/~simpozio/">Enciklopedio
          simpozio</url></vrk>,
          <lok>artikolo &leftquot;ideologo&rightquot;, 1996-05-12</lok>
        </fnt>;
      </ekz>
      <ekz>
        la reganta egoisma kaj avara kapitalisma libermerkata <tld/>o estas ido de la
        protestanta etiko de la &gcirc;eneva civito
        <fnt>
       <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2000/006847.php">Sobraj pensoj de sobrulo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 22</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Ideologie</trd>
    <trd lng="nl">idee&euml;ngoed</trd>
  </snc>
  <trd lng="be">&c_ib;&c_d;&c_e;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_i;&c_d;&c_je;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="ca">ideologia</trd>
  <trd lng="en">ideology</trd>
  <trd lng="fr">id&eacute;ologie</trd>
  <trd lng="hu">ideol&oacute;gia</trd>
  <trdgrp lng="ja">
    <trd>&#x30A4;&#x30C7;&#x30AA;&#x30ED;&#x30AE;&#x30FC;  <pr>&#x3044;&#x3067;&#x304A;&#x308D;&#x304E;&#x30FC;</pr></trd>,
    <trd>&#x89B3;&#x5FF5;&#x5F62;&#x614B;  <pr>&#x304B;&#x3093;&#x306D;&#x3093;&#x3051;&#x3044;&#x305F;&#x3044;</pr></trd>,
    <trd>&#x89B3;&#x5FF5;&#x5B66;  <pr>&#x304B;&#x3093;&#x306D;&#x3093;&#x304C;&#x304F;</pr></trd>,
    <trd>&#x7A7A;&#x7406;&#x7A7A;&#x8AD6;  <pr>&#x304F;&#x3046;&#x308A;&#x304F;&#x3046;&#x308D;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="pl">ideologia</trd>
  <trd lng="pt">ideologia</trd>
  <trd lng="ru">&c_i;&c_d;&c_je;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="uk">&c_ib;&c_d;&c_je;&#x43E;&#x43B;&#x43E;&c_g;&c_ib;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x601D;&#x60F3; <pr>s&#x12B;xi&#x1CE;ng</pr></trd>,
    <trd>&#x610F;&#x8B58;&#x5F62;&#x614B; <pr>y&#xEC;sh&#xED;x&#xED;ngt&#xE0;i</pr></trd>,
    <trd>&#x653F;&#x6CBB;&#x601D;&#x60F3; <pr>zh&#xE8;ngzh&#xEC;s&#x12B;xi&#x1CE;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ideolo.0a">
  <kap><tld/>a</kap>
  <snc mrk="ideolo.0a.KOMUNE">
    <dif>
      Propra al aparta idearo, al la tipaj ideoj de iu epoko a&ubreve; socio:
      <ekz>
        <tld/>a hegemonio
        <fnt>
          <bib>Monato</bib>
          <aut>Gilbert Ledon</aut>
          <vrk><url ref="&Monato;/2003/008040.html">Kvina povo</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj diferencoj
        <fnt>
          <bib>Monato</bib>
          <aut>Josef Mendl</aut>
          <vrk><url ref="&Monato;/2005/008560.php">De la tria E&ubreve;ropa al la kvina Monda Sociala
          Forumo</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>aj faktoroj ludas grandan rolon en la historio;
      </ekz>
      <ekz>
        tiuj, kiuj estas esperantistoj en <tld/>a senco, apogu kaj defendu la lingvajn
        rajtojn de niaj surdaj kunhomoj
        <fnt>
       <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2000/006880.php">Ne &ccirc;iuj lingvoj estas langaj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_d;&c_e;&c_a;&c_l;&c_ja;&c_g;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="bg">&c_i;&c_d;&c_je;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;</trd>
  <trd lng="ca">ideol&ograve;gic</trd>
  <trd lng="cs">ideologick&yacute;</trd>
  <trd lng="de">ideologisch</trd>
  <trd lng="en">ideology</trd>
  <trd lng="fr">id&eacute;ologique</trd>
  <trdgrp lng="ja">
    <trd>&#x30A4;&#x30C7;&#x30AA;&#x30ED;&#x30AE;&#x30FC;&#x306E;  <pr>&#x3044;&#x3067;&#x304A;&#x308D;&#x304E;&#x30FC;&#x306E;</pr></trd>,
    <trd>&#x89B3;&#x5FF5;&#x5F62;&#x614B;&#x306E;  <pr>&#x304B;&#x3093;&#x306D;&#x3093;&#x3051;&#x3044;&#x305F;&#x3044;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">ideologisch</trd>
  <trd lng="pl">ideologiczny</trd>
  <trd lng="ro">ideologic</trd>
  <trd lng="ru">&c_i;&c_d;&c_je;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">ideologick&yacute;</trd>
  <trd lng="uk">&c_ib;&c_d;&c_je;&#x43E;&#x43B;&#x43E;&c_g;&c_ib;&c_ch;&#x43D;&c_i;&c_j;</trd>
  <trd lng="zh">&#x601D;&#x60F3;&#x4E0A; <pr>s&#x12B;xi&#x1CE;ngsh&#xE0;ng</pr></trd>
</drv>

<drv mrk="ideolo.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="ideolo.0isto.KOMUNE">
    <dif>
      Porparolanto de iu idearo:
      <ekz>
        partiaj <tld/>istoj volis difini tiun enhavon
        <fnt>
          <aut>Pilch H.</aut>,
          <vrk>Honkongo redonita al &Ccirc;inujo</vrk>,
          <lok><url ref="&SCE;">soc.culture.esperanto</url>,
          1997-07-27</lok>
        </fnt>;
      </ekz>
      <ekz>
        la nazia &ccirc;ef-<tld/>isto <nom>Alfred Rosenberg</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=450517">Hans Joachim Moser</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_d;&c_e;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="bg">&c_i;&c_d;&c_je;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="ca">ide&ograve;leg</trd>
  <trd lng="cs">ideolog</trd>
  <trd lng="de">Ideologe</trd>
  <trd lng="en">ideologue</trd>
  <trd lng="fr">id&eacute;ologue</trd>
  <trd lng="pl">ideolog</trd>
  <trd lng="sk">ideol&oacute;g</trd>
</drv>
