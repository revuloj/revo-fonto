<drv mrk="oksige.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="oksige.0i.provizi">
    <dif>
      Provizi per <tld/>o:
      <ekz>
        la pulmoj <tld/>as la sangon;
      </ekz>
      <ekz>
        la efiko estas plilar&gcirc;igi arteriojn, kaj tio plifaciligas la
        fluon de <tld/>ita sango al la koro
        <fnt>
          <aut>David Curtis</aut>
          <vrk><url ref="&Monato;/2002/pdf/monato200201.pdf#page=28">Koratako</url></vrk>
          <bib>Monato</bib>
          <lok>2002/01, p. 28-34</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
<!--
  <rim>
    Temas ne pri &hcirc;emia kombina&jcirc;o de <tld/>o kaj alia elemento
  </rim>
-->
  <trd lng="be">&c_n;&c_a;&c_s;&c_y;&c_ch;&c_a;&c_c;&c_mol; <ind>&c_k;&c_ib;&c_s;&c_l;&c_a;&c_r;&c_o;&c_d;&c_a;&c_m;</ind></trd>
  <trd lng="br">oksigena&ntilde;</trd>
  <trd lng="ca">oxigenar</trd>
  <trd lng="de">mit Sauerstoff <ind>anreichern</ind></trd>
  <trd lng="en">oxigenate</trd>
  <trd lng="es">oxigenar</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&f_k;&a_s;&f_y;&f_zh;&a_n;&#8204;&a_d;&a_A;&a_r; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_A;&f_k;&a_s;&f_y;&f_zh;&a_n; &a_d;&a_A;&a_d;&a_n;<klr> (&a_b;&a_h;)</klr></trd>
  </trdgrp>
  <trd lng="fr">oxyg&eacute;ner</trd>
  <trdgrp lng="hu">
    <trd>oxig&eacute;nez</trd>,
    <trd>oxig&eacute;nnel d&uacute;s&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">met <klr tip="ind">zuurstof</klr> verrijken</trd>
  <trdgrp lng="pl">
    <trd>nasyca&cacute; tlenem</trd>,
    <trd>natlenia&cacute;</trd>,
    <trd>dotlenia&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_y;&c_shch;&c_a;&c_t;&c_mol; <ind>&c_k;&c_i;&c_s;&c_l;&c_o;&c_r;&c_o;&c_d;&c_o;&c_m;</ind></trd>
  <trd lng="sv">syres&auml;tta</trd>
