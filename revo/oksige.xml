  <trd lng="sv">syres&auml;tta</trd>
  <trd lng="sk">okysli&ccaron;ova&tcaron;</trd>
  <trdgrp lng="cs">
    <trd>okysli&ccaron;ovat</trd>,
    <trd>oxygenovat</trd>
  </trdgrp>
  <trdgrp lng="uk">
