<art mrk="$Id: afust.xml,v 1.26 2025/12/17 14:37:22 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>afust</rad>/o
<fnt><bib>JED</bib></fnt></kap>
<drv mrk="afust.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="port.0ilo">Portilo</ref>
      de pezaj pafiloj kiel kanono kc:
      <ekz>
        ses generaloj metis la &ccirc;erkon sur <tld/>on de ha&ubreve;bizo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4256">Tom&aacute;&scaron; Garrigue Masaryk</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="postam.0o">postamento</ref>,
      <ref cel="stativ.0o">stativo</ref>
    </refgrp>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Anka&ubreve; eblas diri <ke>pafilportilo</ke>, <ke>pafilsubtenilo</ke>.
  </rim>
  <trd lng="be">&c_l;&c_a;&c_f;&c_je;&c_t;</trd>
  <trd lng="cs">lafeta</trd>
  <trdgrp lng="de">
     <trd>Lafette</trd>,
     <trd>Selbstfahrlafette</trd>
  </trdgrp>
  <trd lng="en">gun carriage</trd>
  <trd lng="fr">aff&ucirc;t<klr tip="amb"> (de canon)</klr></trd>
  <trdgrp lng="he">
    <trd>&kaf;&fnun;</trd>,
    <trd>&het;&tsadi;&vav;&bet;&he;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&aacute;gy&uacute;talp</trd>,
    <trd>l&ouml;vegtalp</trd>,
    <trd>lafetta</trd>
  </trdgrp>
  <trd lng="it">affusto</trd>
  <trdgrp lng="ja">
    <trd>&#x7832;&#x67B6;  <pr>&#x307B;&#x3046;&#x304B;</pr></trd>,
    <trd>&#x9283;&#x67B6;  <pr>&#x3058;&#x3085;&#x3046;&#x304B;</pr></trd>
  </trdgrp>
  <trd lng="nl">affuit</trd>
  <trd lng="pt">carreta<klr tip="amb"> (de canh&atilde;o ou
  metralhadora)</klr></trd>
  <trd lng="ru">&c_l;&c_a;&c_f;&c_je;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>lafeta</trd>,
    <trd>lafeta <klr>(voj.)</klr></trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_ib;&c_j;&c_s;&c_mol;&c_k;. &c_l;&c_a;&c_f;&c_je;&c_t;</trd>
</drv>
</art>
