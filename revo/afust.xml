<kap>
  <ofc>1</ofc>
  <rad>afust</rad>/o
<fnt><bib>JED</bib></fnt></kap>
<drv mrk="afust.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="port.0ilo">Portilo</ref>
      de pezaj pafiloj kiel kanono kc:
      <ekz>
        ses generaloj metis la &ccirc;erkon sur <tld/>on de ha&ubreve;bizo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4256">Tom&aacute;&scaron; Garrigue Masaryk</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Anka&#365; eblas diri <ke>pafilportilo</ke>, <ke>pafilsubtenilo</ke>.
  </rim>

  <trd lng="be">&c_l;&c_a;&c_f;&c_je;&c_t;</trd>
  <trd lng="cs">lafeta</trd>
  <trdgrp lng="de">
     <trd>Lafette</trd>,
     <trd>Selbstfahrlafette</trd>
  </trdgrp>
  <trd lng="en">gun carriage</trd>
  <trd lng="fr">aff&ucirc;t<klr tip="amb"> (de canon)</klr></trd>
  <trdgrp lng="he">
    <trd>&#1499;&#1503;</trd>,
    <trd>&#1495;&#1510;&#1493;&#1489;&#1492;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&aacute;gy&uacute;talp</trd>,
    <trd>l&ouml;vegtalp</trd>,
    <trd>lafetta</trd>
  </trdgrp>
  <trd lng="it">affusto</trd>
  <trdgrp lng="ja">
    <trd>&#30770;&#26550;  <pr>&#12411;&#12358;&#12363;</pr></trd>,
    <trd>&#37507;&#26550;  <pr>&#12376;&#12421;&#12358;&#12363;</pr></trd>
  </trdgrp>
  <trd lng="nl">affuit</trd>
  <trd lng="pt">carreta<klr tip="amb"> (de canh&atilde;o ou
  metralhadora)</klr></trd>
  <trd lng="ru">&c_l;&c_a;&c_f;&c_je;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>lafeta</trd>,
    <trd>lafeta <klr>(voj.)</klr></trd>
  </trdgrp>
  <trd lng="uk">&#1074;&#1110;&#1081;&#1089;&#1100;&#1082;. &#1083;&#1072;&#1092;&#1077;&#1090;</trd>
</drv>
