<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: benzin.xml,v 1.35 2023/10/29 19:11:01 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>benzin</rad>/o
</kap>
<drv mrk="benzin.0o">
  <kap><tld/>o</kap>
  <snc mrk="benzin.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Nepura substanco <ref tip="super" cel="petrol.0o">petrola</ref>,
      miksa&jcirc;o el malpezaj
      <ref cel="hidrok.0o">hidrokarbidoj</ref> (bolantaj &ccirc;e
      30&#x2010;205&#x2DA;C),
      ekstraktata precipe el <ref tip="malprt" cel="naft.0o">nafto</ref>
      post distilado kaj uzata kiel <ref tip="super" cel="solv.0ilo">solvilo</ref> a&ubreve; 
      kiel <ref tip="super" cel="brul.0igajxo">bruliga&jcirc;o</ref>
      en motoroj:
      <ekz>
        post trisemajna traktado la komercistoj akceptis vendi <tld/>on
        je normala prezo
        <fnt>
        <aut>Zandandu Ntomono Zola</aut>
        <vrk><url ref="&Monato;/2001/006332.html">Ekonomio dum la milito</url></vrk>
        <bib>Monato</bib>
        <lok>1999/03, p. 9</lok></fnt>;
      </ekz>
      <ekz>
        tiu transportilo en ebena vojo, veturante en rapideco de 70-80
        km/h, konsumas je 200 km &ccirc;irka&ubreve; 18 litrojn da <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius STRA&Zcaron;NICKAS</aut>
          <vrk><url ref="&Monato;/2000/006877.php">Nekutimaj
          transportiloj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        de pluraj monatoj la prezo de kruda petrolo, kaj sekve de &gcirc;iaj
        derivatoj kiel <tld/>o kaj 
        <ref tip="vid" cel="dizel.0o">dizeloleo</ref>, da&ubreve;re kreskas
        <fnt>
          <bib>Monato</bib>
          <aut>Roland ROTSAERT</aut>
          <vrk><url ref="&Monato;/2001/007184.php">Petrolo:
          Nehaltigeblaj prezalti&gcirc;oj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x8CBB;&#x6CB9; <pr>f&#xE8;iy&#xF3;u</pr></trd>,
    <trd>&#x6C7D;&#x6CB9; <pr>q&#xEC;y&#xF3;u</pr></trd>,
    <trd>&#x77F3;&#x6CB9;&#x919A; <pr>sh&#xED;y&#xF3;um&#xED;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_b;&c_e;&c_n;&c_z;&c_y;&c_n;</trd>
  <trd lng="bo">&t_sa;&t_nata;&t_shabchu;&t_ma;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>benzin</trd>,
    <trd>benz&iacute;n</trd>
  </trdgrp>
  <trd lng="de">Benzin</trd>
  <trdgrp lng="en">
    <trd>gasoline<klr> (usone)</klr></trd>,
    <trd>petrol<klr> (brite)</klr></trd>
  </trdgrp>
  <trd lng="es">bencina</trd>
  <trd lng="fr">essence</trd>
  <trd lng="he">&bet;&#x5BC;&#x5B6;&nun;&zayin;&#x5B4;&yod;&fnun;</trd>
  <trdgrp lng="ja">
    <trd>&#x30AC;&#x30BD;&#x30EA;&#x30F3;  <pr>&#x304C;&#x305D;&#x308A;&#x3093;</pr></trd>,
    <trd>&#x30D9;&#x30F3;&#x30B8;&#x30F3;</trd>
  </trdgrp>
  <trd lng="pl">benzyna</trd>
  <trd lng="pt">gasolina</trd>
  <trd lng="ru">&c_b;&c_je;&c_n;&c_z;&c_i;&c_n;</trd>
  <trd lng="sk">benz&iacute;n</trd>
  <trd lng="sv">bensin</trd>
  <trd lng="uk">&c_b;&c_je;&c_n;&c_z;&c_i;&c_n;</trd>
</drv>

<drv mrk="benzin.0ejo">
  <kap><tld/>ejo,
  <var><kap><tld/>stacio</kap></var></kap>
  <snc mrk="benzin.0ejo.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      Loko, <ref tip="super" cel="staci.0o.TEK">stacio<sncref/></ref>
      kie a&ubreve;tistoj povas provizi sin per <tld/>o:
      <ekz>
        mi haltis en a&ubreve;to&scirc;osea <tld/>ejo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Monato;/2000/007052.php">Verda
          nature, science, socie</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la a&ubreve;to pasis preter kelkaj <tld/>ejoj, sed la viro ne haltis
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=353100">Zodiako
          (seria murdisto)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>LUKoil</nom> planas &gcirc;is 2005 estigi en Bulgario distribuan reton
        de 150 <tld/>stacioj
        <fnt>
        <aut>E. Georgiev</aut>
        <vrk>Pliigita aktivado de LUKoil</vrk>
        <bib>Monato</bib>
        <lok>2001/02, p. 10</lok>
        </fnt>;
      </ekz>
      <ekz>
        urba apoteko <nom>Wiesloch</nom>, unua <tld/>stacio de la mondo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=15204">Karl
          Benz</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        restoracio <tld/>stacia.
        <fnt>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2001/007297.php">Kolekti
          sonorilojn</url></vrk>
          <bib>Monato</bib>
          <lok>2001/02, p. 24</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x52A0;&#x6CB9;&#x7AD9; <pr>ji&#x101;y&#xF3;uzh&#xE0;n</pr></trd>
</snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_w;&c_t;&c_a;&c_z;&c_a;&c_p;&c_r;&c_a;&c_w;&c_n;&c_a;&c_ja; <ind>&c_s;&c_t;&c_a;&c_n;&c_c;&c_y;&c_ja;</ind></trd>,
    <trd>&c_b;&c_e;&c_n;&c_z;&c_a;&c_s;&c_t;&c_a;&c_n;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_A;&c_Z;&c_S;</trd>
  </trdgrp>
  <trd lng="cs">benzinov&aacute; stanice</trd>
  <trd lng="de">Tankstelle</trd>
  <trdgrp lng="en">
    <trd>gas station<klr> (usone)</klr></trd>,
    <trd>filling station<klr> (usone)</klr></trd>,
    <trd>petrol station<klr> (brite)</klr></trd>
  </trdgrp>
  <trd lng="fr">station-service</trd>
  <trd lng="he">&tav;&het;&nun;&tav; &dalet;&lamed;&qof;</trd>
  <trd lng="ja">&#x30AC;&#x30BD;&#x30EA;&#x30F3;&#x30B9;&#x30BF;&#x30F3;&#x30C9;  <pr>&#x304C;&#x305D;&#x308A;&#x3093;&#x3059;&#x305F;&#x3093;&#x3069;</pr></trd>
  <trd lng="pl">stacja <ind>benzynowa</ind></trd>
  <trd lng="pt">posto de gasolina</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_n;&c_z;&c_o;&c_z;&c_a;&c_p;&c_r;&c_a;&c_v;&c_o;&c_ch;&c_n;&c_a;&c_ja;
    &c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_b;&c_je;&c_n;&c_z;&c_o;&c_z;&c_a;&c_p;&c_r;&c_a;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">benz&eacute;nov&aacute; stanica</trd>
  <trdgrp lng="sv">
    <trd>bensinmack</trd>,
    <trd>bensinstation</trd>
  </trdgrp>
</drv>

<drv mrk="benzin.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="benzin.0ujo.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      Parto de la a&ubreve;to, avio ktp, kie estas tenata la uzota
      <tld/>o:
      <ekz>
        senpaga plenigo de a&ubreve;ta <tld/>ujo
        <fnt>
          <bib>Monato</bib>
        </fnt>;
      </ekz>
      <ekz>
        usona aviadilo kun sia voksigno pentrita &ccirc;e la <tld/>ujo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=332050">Flugkompania
          voksigno</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x6C7D;&#x6CB9;&#x7BB1; <pr>q&#xEC;y&#xF3;uxi&#x101;ng</pr></trd>,
    <trd>&#x6CB9;&#x7BB1; <pr>y&#xF3;uxi&#x101;ng</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_b;&c_e;&c_n;&c_z;&c_a;&c_b;&c_a;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>n&aacute;dr&zcaron; na benzin</trd>,
    <trd>tank</trd>
  </trdgrp>
  <trd lng="de">Benzintank</trd>
  <trdgrp lng="en">
    <trd>gas tank<klr> (usone)</klr></trd>,
    <trd>petrol tank<klr> (brite)</klr></trd>
  </trdgrp>
  <trd lng="fr">r&eacute;servoir &agrave; essence</trd>
  <trd lng="he">&mem;&yod;&kaf;&lamed; &dalet;&lamed;&qof;</trd>
  <trd lng="ja">&#x30AC;&#x30BD;&#x30EA;&#x30F3;&#x30BF;&#x30F3;&#x30AF;</trd>
  <trdgrp lng="pl">
    <trd>bak</trd>,
    <trd>zbiornik <ind>paliwa</ind></trd>
  </trdgrp>
  <trd lng="pt">tanque de gasolina</trd>
  <trd lng="ru">&c_b;&c_je;&c_n;&c_z;&c_o;&c_b;&c_a;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>benz&iacute;nov&aacute; n&aacute;dr&zcaron;</trd>,
    <trd>n&aacute;dr&zcaron;</trd>
  </trdgrp>
  <trd lng="sv">bensintank</trd>
  <trd lng="uk">&c_k;&c_a;&c_n;&c_ib;&c_s;&c_t;&c_r;&c_a;</trd>
</drv>

<drv mrk="benzin.0umi">
  <kap>
    <tld/>umi
  </kap>
  <snc mrk="benzin.0umi.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      Provizi eksplod-motoran veturilon per <tld/>o:
      <ekz>
        ni haltas por <tld/>umi &ccirc;e butiko kiu ne plu akceptas kreditkartojn
        <fnt>
          <aut>K. Kniivil&auml;</aut>
          <vrk>Krimeo estas nia</vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_p;&c_r;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol; &c_b;&c_e;&c_n;&c_z;&c_y;&c_n;&c_a;&c_m;</trd>
<trdgrp lng="fr">
  <trd>prendre de l'essence</trd>,
  <trd>faire le plein</trd>
</trdgrp>
</drv>

<drv mrk="benzin.rafinita0">
  <kap>rafinita <tld/>o</kap>
  <snc mrk="benzin.rafinita0.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      <tld lit="B"/>o vendata en komerco por forigado de grasaj makuloj.
    </dif>
    <adm>
      Pri kio temas precize? Estas multaj diversaj specoj de benzino
      troveblaj en butikoj por diversaj hejmaj a&ubreve; kampadaj uzoj.
      Estus bone plidetaligi la temon.(MB)
    </adm>
  </snc>
  <trd lng="be">&c_b;&c_e;&c_n;&c_z;&c_y;&c_n;&c_a;&c_v;&c_y; &c_r;&c_a;&c_s;&c_t;&c_v;&c_a;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trd lng="cs">rafinovan&yacute; benzin</trd>
  <trd lng="de">Reinigungsbenzin</trd>
  <trd lng="fr">essence F</trd>
  <trdgrp lng="ja">
    <trd>&#x77F3;&#x6CB9;&#x30D9;&#x30F3;&#x30B8;&#x30F3;  <pr>&#x305B;&#x304D;&#x3086;&#x30D9;&#x30F3;&#x30B8;&#x30F3;</pr></trd>,
    <trd>&#x7CBE;&#x88FD;&#x30D9;&#x30F3;&#x30B8;&#x30F3;  <pr>&#x305B;&#x3044;&#x305B;&#x3044;&#x30D9;&#x30F3;&#x30B8;&#x30F3;</pr></trd>
  </trdgrp>
  <trd lng="sk">rafinovan&yacute; benz&iacute;n</trd>
  <adm>ne trovis ekzemplon de uzo <aut>WD</aut></adm>
</drv>
</art>
