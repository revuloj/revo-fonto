<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>benzin</rad>/o
</kap>
<drv mrk="benzin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Nepura substanco <ref tip="super" cel="petrol.0o">petrola</ref>,
      miksa&jcirc;o el malpezaj
      <ref cel="hidrok.0o">hidrokarbidoj</ref> (bolantaj &ccirc;e
      30&dash;205&ring;C),
      ekstraktata precipe el <ref tip="malprt" cel="naft.0o">nafto</ref>
      post distilado kaj uzata kiel solvilo a&ubreve; kiel brula&jcirc;o
      en motoroj:
      <ekz>
        post trisemajna traktado la komercistoj akceptis vendi <tld/>on
        je
        normala prezo
        <fnt>
        <aut>Zandandu Ntomono Zola</aut>: <vrk>Ekonomio
        dum la milito</vrk>, <bib>Monato</bib><lok>jaro 1999a, n-ro 3a,
        p. 9a</lok></fnt>.
      </ekz>
    </dif>
    <adm>
      Enkonduki deriva&jcirc;on "~ilo" sub "solvi". (MB)
    </adm>
  </snc>
  <trd lng="de">Benzin</trd>
  <trdgrp lng="en">
    <trd>gasoline<klr> (usone)</klr></trd>,
    <trd>petrol<klr> (brite)</klr></trd>
  </trdgrp>
  <trd lng="es">bencina</trd>
  <trd lng="fr">essence</trd>
  <trd lng="pl">benzyna</trd>
  <trd lng="ru">&c_b;&c_je;&c_n;&c_z;&c_i;&c_n;</trd>
  <trd lng="sk">benz&iacute;n</trd>
</drv>

<drv mrk="benzin.rafinita0">
  <kap>rafinita <tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      <tld lit="B"/>o vendata en komerco por forigado de grasaj makuloj.
    </dif>
    <adm>
      Pri kio temas precize? Estas multaj diversaj specoj de benzino
      troveblaj en butikoj por diversaj hejmaj a&ubreve; kampadaj uzoj.
      Estus bone plidetaligi la temon.(MB)
    </adm>
  </snc>
  <trd lng="de">Reinigungsbenzin</trd>
</drv>

<drv mrk="benzin.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Loko, kie a&ubreve;tistoj povas provizi sin per <tld/>o.
    </dif>
    <refgrp tip="sin">
      <ref cel="benzin.0stacio"><tld/>stacio</ref>
    </refgrp>
  </snc>
  <trd lng="de">Tankstelle</trd>
  <trdgrp lng="en">
    <trd>gas station<klr> (usone)</klr></trd>,
    <trd>filling station<klr> (usone)</klr></trd>,
    <trd>petrol station<klr> (brite)</klr></trd>
  </trdgrp>
  <trd lng="fr">station-service</trd>
  <trd lng="pl">stacja <ind>benzynowa</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_n;&c_z;&c_o;&c_z;&c_a;&c_p;&c_r;&c_a;&c_v;&c_o;&c_ch;&c_n;&c_a;&c_ja;
    &c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_b;&c_je;&c_n;&c_z;&c_o;&c_z;&c_a;&c_p;&c_r;&c_a;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="benzin.0stacio">
  <kap><tld/>stacio</kap>
  <snc>
    <dif>
      = <ref tip="dif" cel="benzin.0ejo"><tld/>ejo</ref>:
      <ekz>
        LUKoil planas &gcirc;is 2005 estigi en Bulgario distribuan reton
        de 150
        <tld/>stacioj
        <fnt><aut>Evgeni Georgiev</aut>:
        <vrk>Pliigita aktivado de LUKoil</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 2a, p. 10a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Tankstelle</trd>
  <trdgrp lng="en">
    <trd>gas station<klr> (usone)</klr></trd>,
    <trd>filling station<klr> (usone)</klr></trd>,
    <trd>petrol station<klr> (brite)</klr></trd>
  </trdgrp>
  <trd lng="fr">station-service</trd>
  <trd lng="pl">stacja <ind>benzynowa</ind></trd>
  <trd
  lng="ru">&c_b;&c_je;&c_n;&c_z;&c_o;&c_z;&c_a;&c_p;&c_r;&c_a;&c_v;&c_o;&c_ch;&c_n;&c_a;&c_ja;
  &c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_ja;</trd>
</drv>

<drv mrk="benzin.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <dif>
      Parto de la a&ubreve;to, avio ktp, kie estas tenata la uzota
      <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Benzintank</trd>
  <trdgrp lng="en">
    <trd>gas tank<klr> (usone)</klr></trd>,
    <trd>petrol tank<klr> (brite)</klr></trd>
  </trdgrp>
  <trd lng="fr">r&eacute;servoir &agrave; essence</trd>
  <trdgrp lng="pl">
    <trd>bak</trd>,
    <trd>zbiornik <ind>paliwa</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_n;&c_z;&c_o;&c_b;&c_a;&c_k;</trd>
  <trd lng="sk">n&aacute;dr&zcaron;</trd>
</drv>

<drv mrk="benzin.0krucxo">
  <kap><tld/>kru&ccirc;o</kap>
  <snc>
    <dif>
      Metallada kru&ccirc;o por transporto de la <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Benzinkanister</trd>
  <trdgrp lng="en">
    <trd>gas can<klr> (usone)</klr></trd>,
    <trd>petrol can<klr> (brite)</klr></trd>
  </trdgrp>
  <trd lng="fr">jerricane</trd>
  <trd lng="pl">kanister</trd>
  <trd lng="ru">&c_k;&c_a;&c_n;&c_i;&c_s;&c_t;&c_r;&c_a;</trd>
</drv>
</art>
