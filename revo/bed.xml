<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>bed</rad>/o
</kap>
<drv mrk="bed.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      &Gcirc;ardena strio da tero por kulturi florojn a&ubreve; legomojn.  
    </dif>
  </snc>
</drv>
<drv mrk="bed.herbo0o">
  <kap><tld/>obedo</kap>
  <snc>
    <dif>
      Belaran&gcirc;ita bedo kun <tld/>o en parko, &gcirc;ardeno ktp. 
    </dif>
    <refgrp tip="sin">
      <ref cel="razen.0o">razeno</ref>,
      <ref cel="gramine.0oj">gramineoj</ref>.
    </refgrp>
  </snc>
</drv>
</art>
