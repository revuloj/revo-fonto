<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bed.xml,v 1.31 2024/03/11 09:22:20 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bed</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="bed.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bed.0o.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      &Gcirc;ardena strio da tero por kulturi florojn a&ubreve;
      legomojn:
      <ekz>
        nelar&gcirc;a, bele aran&gcirc;ita <tld/>o la&ubreve; la tuta longo de la domo
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i saltis inter la <tld/>ojn, ser&ccirc;is kaj ser&ccirc;is, sed &scirc;i ne povis trovi e&ccirc; unu
        rozon
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        homoj <klr>[&#8230;]</klr> &ccirc;erpis la akvon per kru&ccirc;oj kaj surver&scirc;is <tld/>ojn de
        legomoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        en regulaj brunaj ondoj ku&scirc;as &jcirc;us fositaj <tld/>oj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        liaj vangoj estas kiel <tld/>oj de aroma&jcirc;oj
        <fnt><bib>MT</bib>, <lok>&Alt; 5:13</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="parcel.0o">parcelo</ref>,
      <ref cel="skvar.0o">skvaro</ref>
    </refgrp>
    <trdgrp lng="th">
      <trd>&#3649;&#3611;&#3621;&#3591;</trd>,
      <trd>&#3619;&#3656;&#3629;&#3591;</trd>
    </trdgrp>
    <trdgrp lng="zh">
      <trd>&#33495;&#24202; <pr>mi&aacute;ochu&aacute;ng</pr></trd>,
      <trd>&#30054; <pr>q&iacute;</pr></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_d;&c_k;&c_a;</trd>
  <trd lng="br">ervenn</trd>
  <trd lng="cs">z&aacute;hon</trd>
  <trd lng="de">Beet</trd>
  <trdgrp lng="en">
    <trd>bed</trd>,
    <trd>plot</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>parterre</trd>,
    <trd>arriate</trd>,
    <trd>era</trd>
  </trdgrp>
  <trd lng="fr">plate-bande</trd>
  <trd lng="he">&ayin;&resh;&vav;&gimel;&he;</trd>
  <trd lng="hu">&aacute;gy&aacute;s</trd>
  <trdgrp lng="ja">
    <trd>&#37326;&#33756;&#30033;  <pr>&#12420;&#12373;&#12356;&#12400;&#12383;&#12369;</pr></trd>,
    <trd>&#33457;&#22727;  <pr>&#12363;&#12384;&#12435;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>grz&aogonek;dka</trd>,
    <trd>zagon</trd>
  </trdgrp>
  <trd lng="pt">canteiro</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_ja;&c_d;&c_a;</trd>,
    <trd>&c_g;&c_r;&c_ja;&c_d;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hriadka</trd>,
    <trd>z&aacute;hon</trd>
  </trdgrp>
  <trd lng="uk">&c_g;&c_r;&c_ja;&c_d;&c_k;&c_a;</trd>
</drv>

<drv mrk="bed.flor0o">
  <kap>flor<tld/>o</kap>
  <snc mrk="bed.flor0o.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      Ornama <tld/>o priplantita per floroj:
      <ekz>
        monumento, &ccirc;irka&ubreve;ita de flor<tld/>oj
        <fnt>
          <bib>IdO</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i haltis, rigardis la fazeolon kaj la flor<tld/>on
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        malgranda, artefarita ebena&jcirc;o kun multekoloraj kaj
        simetrie aran&gcirc;itaj flor<tld/>oj
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 5a, Lemuel
        aktivi&gcirc;as</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="flor.0ejo">florejo</ref>
    <trdgrp lng="zh">
      <trd>&#33457;&#24202; <pr>hu&amacron;chu&aacute;ng</pr></trd>,
      <trd>&#33457;&#22275; <pr>hu&amacron;p&#468;</pr></trd>,
      <trd>&#33457;&#22363; <pr>hu&amacron;t&aacute;n</pr></trd>,
      <trd>&#33495;&#22275; <pr>mi&aacute;op&#468;</pr></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_ju;&c_m;&c_b;&c_a;</trd>
  <trd lng="cs">kv&ecaron;tinov&yacute; z&aacute;hon</trd>
  <trd lng="de">Blumenbeet</trd>
  <trd lng="en">flower <ind>bed</ind></trd>
  <trd lng="es">arriate</trd>
  <trdgrp lng="fr">
    <trd>massif de fleurs</trd>,
    <trd>parterre de fleurs</trd>
  </trdgrp>
  <trd lng="ja">&#33457;&#22727;  <pr>&#12363;&#12384;&#12435;</pr></trd>
  <trd lng="pl">klomb</trd>
  <trd lng="sk">kvetinov&yacute; z&aacute;hon</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_l;&c_u;&c_m;&c_b;&c_a;</trd>,
    <trd>&c_k;&c_v;&c_ib;&c_t;&c_k;&c_o;&c_v;&c_a; &c_g;&c_r;&c_ja;&c_d;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="bed.herbo0o">
  <kap>herbo<tld/>o</kap>
  <snc mrk="bed.herbo0o.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      Belaran&gcirc;ita <tld/>o kun herbo en parko, &gcirc;ardeno...:
      <ekz>
        fre&scirc;a herbo<tld/>o
        <fnt><bib>Fab2</bib>, <lok>sambuka virineto</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="gazon.0o.HOR">gazono</ref>
    <refgrp tip="vid">
      <ref cel="gramine.0oj">gramineoj</ref>,
      <ref cel="razen.0o">razeno</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_z;&c_o;&c_n;</trd>
  <trdgrp lng="br">
    <trd>letonenn</trd>,
    <trd>glazenn</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pa&zcaron;it</trd>,
    <trd>tr&aacute;vn&iacute;k</trd>
  </trdgrp>
  <trd lng="de">Rasenfl&auml;che</trd>
  <trd lng="en">lawn</trd>
  <trd lng="fr">pelouse</trd>
  <trdgrp lng="hu">
    <trd>p&aacute;zsit</trd>,
    <trd>gyep</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#33437;&#29983;  <pr>&#12375;&#12400;&#12405;</pr></trd>,
    <trd>&#33609;&#22320;  <pr>&#12367;&#12373;&#12385;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>gazon</trd>,
    <trd>trawnik</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_a;&c_z;&c_o;&c_n;</trd>
  <trd lng="sk">hriadka</trd>
  <adm>
    Kiel oni nombas ~on de speicherboj?
    <aut>WD</aut>
  </adm>
</drv>
</art>
