<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>bed</rad>/o
</kap>
<drv mrk="bed.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      &Gcirc;ardena strio da tero por kulturi florojn a&ubreve; legomojn.  
    </dif>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_ja;&c_d;&c_a;</trd>,
    <trd>&c_g;&c_r;&c_ja;&c_d;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
<drv mrk="bed.herbo0o">
  <kap>herbo<tld/>o</kap>
  <snc>
    <dif>
      Belaran&gcirc;ita <tld/>o kun herbo en parko, &gcirc;ardeno ktp. 
    </dif>
    <refgrp tip="sin">
      <ref cel="razen.0o">razeno</ref>,
      <ref cel="gramine.0oj">gramineoj</ref>.
    </refgrp>
  </snc>
  <trd lng="ru">&c_g;&c_a;&c_z;&c_o;&c_n;</trd>
</drv>
</art>
