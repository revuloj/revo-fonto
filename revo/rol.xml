<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>rol</rad>/o
</kap>
<drv mrk="rol.0o">
  <kap><tld/>o</kap>
  <snc mrk="rol.0o.TEA">
    <uzo tip="fak">KIN</uzo>
    <uzo tip="fak">TEA</uzo>
    <dif>
      &Ccirc;iu parto de teatra verko, ludata de &ccirc;iu aparta aktoro:
      <ekz>
        ludi la <tld/>on de Hamleto.
      </ekz>
    </dif>
  </snc>
  <snc mrk="rol.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Agado, funkcio de iu a&ubreve; io, konsiderata en siaj efikoj:
      <ekz>
        ludi gravan <tld/>on en entrepreno<fnt>B</fnt>
      </ekz>
      <ekz>
        <tld/>o tre flata sed tre &scirc;ar&gcirc;a<fnt>Z</fnt>;
      </ekz>
      <ekz>
        Esperanto montri&gcirc;is tute ta&ubreve;ga por sia <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la kutimo ludas gravan <tld/>on en la vivo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        plenumi <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        okupi <tld/>on en ia afero<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ofic.0o">ofico</ref>,
      <ref cel="okup.0o">okupo</ref>,
      <ref cel="profes.0o">profesio</ref>,
      <ref cel="task.0o">tasko</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_o;&c_l;&c_ja;</trd>
  <trd lng="de">Rolle</trd>
  <trd lng="en">role</trd>
  <trdgrp lng="fr">
    <trd>r&ocirc;le</trd>
  </trdgrp>
  <trd lng="hu">szerep</trd>
  <trd lng="nl">rol</trd>
  <trd lng="ru">&c_r;&c_o;&c_l;&c_mol;</trd>
</drv>

<drv mrk="rol.0i">
  <kap><tld/>i</kap>
  <snc mrk="rol.0i.TEA">
    <dif>
      Surpreni teatran, kinan, fikcian <tld/>on:
      <ekz>
        membroj studas proksime al la operejo kaj vespere <tld/>as a&ubreve;
        kantas en operaj prezentadoj
        <fnt>
        <aut>Juh&aacute;sz K&aacute;roly</aut>,
        <vrk>Korusa konkurso</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2000a, numero 12a, p. 14a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aktor.0i">aktori</ref>
    </refgrp>
  </snc>
  <snc mrk="rol.0i.FIG">
    <dif>
      Partopreni en situacio, agi konforme al iu funkcio:
      <ekz>
        fekundiga polvo de floroj, kiu <tld/>as kiel planta
        spermo
        <fnt>
        <aut>Robert Roze</aut>,
        <vrk>Vivisekcio de polenero</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 1997a, numero 11a, p. 12a</lok>
        </fnt>.
      </ekz>
      <ekz>
        en amba&ubreve; verkoj grave <tld/>as anka&ubreve; sociaj rilatoj kaj
        kondi&ccirc;oj
        <fnt>
        <aut>Gerrit Berveling</aut>,
        <vrk>Iom tro predikema romano (Lev Tolstoj, Resurekto)</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2001a, numero 5a, p. 30a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">play a <ind>role</ind></trd>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_c;&c_mol; <ind>&c_r;&c_o;&c_l;&c_ju;</ind></trd>
  <trdgrp lng="fr">
    <trd>jouer un <ind>r&ocirc;le</ind></trd>,
    <trd>&ecirc;tre <ind>impliqu&eacute;</ind> dans</trd>
  </trdgrp>
  <trd lng="hu">szerepel</trd>
  <trd lng="ru">&c_i;&c_g;&c_r;&c_a;&c_t;&c_mol; <ind>&c_r;&c_o;&c_l;&c_mol;</ind></trd>
</drv>

<drv mrk="rol.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="rol.0isto.TEA">
    <uzo tip="fak">KIN</uzo>
    <uzo tip="fak">TEA</uzo>
    <ref tip="dif" cel="aktor.0o.TEA">aktoro</ref>
    <ekz>
      estas granda diferenco inter la prononckapablo de mezuma popolano
      kaj tiu de <klr>...</klr> teatra <tld/>isto
      <fnt>
      <aut>Pejno S.</aut>,
      <vrk>Re: Duoblaj konsonantoj</vrk>,
      <lok>soc.culture.esperanto, 1998-11-29</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="fr">acteur</trd>
</drv>

<drv mrk="rol.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="rol.0ulo.BELE">
    <uzo tip="fak">BELE</uzo>
    <uzo tip="fak">KIN</uzo>
    <uzo tip="fak">TEA</uzo>
    <dif>
      Persono en fikcia&jcirc;o, teatra&jcirc;o, kina&jcirc;o, rakonto:
      <ekz>
        entute la <tld/>uloj de Piron &scirc;ajnas pupoj sen propra
        vivo
        <fnt>
        <aut>Sten Johansson</aut>,
        <vrk>Novelaro duepoka (Bako Sandor J., Claude Piron: La dorsosako
        de Panjo Rut')</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 1998a, numero 2a, p. 16a</lok>
        </fnt>.
      </ekz>
      <ekz>
        libro bezonas anka&ubreve; bonajn <tld/>ulojn
        <fnt>
        <aut>Familio Macgill</aut>,
        <vrk>June, nejune, pretertempe (John Ronald Reuel Tolkien, La
        Hobito a&ubreve; tien kaj reen)</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2001a, numero 7a, p. 26a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">character</trd>
  <trdgrp lng="fr">
    <trd>personnage<klr> (de fiction)</klr></trd>,
    <trd>protagoniste</trd>
  </trdgrp>
  <trd lng="hu">szerepl&odblac;</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_u;&c_ju;&c_shch;&c_je;&c_je; &c_l;&c_i;&c_c;&c_o;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_s;&c_o;&c_n;&c_a;&c_zh;</trd>,
    <trd>&c_g;&c_je;&c_r;&c_o;&c_j;<klr tip="amb"> (&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="rol.cxef0ulo">
  <kap>&ccirc;ef<tld/>ulo</kap>
  <snc mrk="rol.cxef0ulo.BELE">
    <uzo tip="fak">BELE</uzo>
    <uzo tip="fak">KIN</uzo>
    <uzo tip="fak">TEA</uzo>
    <dif>
      Iu el la &ccirc;efaj <ref tip="super" cel="rol.0ulo.BELE"><tld/>uloj</ref>
      en fikcia&jcirc;o, ofte aperanta, ofte aganta.
    </dif>
    <refgrp tip="sin">
      <ref cel="hero.0o.BELE">heroo <sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="hu">f&odblac;szerepl&odblac;</trd>
  <trd lng="nl">hoofdrolspeler</trd>
  <trd lng="fr">h&eacute;ros<klr tip="amb"> (de r&eacute;cit)</klr></trd>
</drv>
</art>
