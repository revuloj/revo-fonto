</kap>
<drv mrk="rol.0o">
  <kap><tld/>o</kap>
  <snc mrk="rol.0o.TEA">
    <uzo tip="fak">KIN</uzo>
    <uzo tip="fak">TEA</uzo>
    <dif>
      &Ccirc;iu parto de teatra verko, ludata de &ccirc;iu aparta aktoro:
      <ekz>
        ludi la <tld/>on de Hamleto;
      </ekz>
      <ekz>
        <nom>Hedwig Reicher</nom> ludas la <tld/>on de Ifigenio
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>role<klr tip="ind"> (in theatre)</klr></trd>,
      <trd>part<klr tip="amb"> (in theatre)</klr></trd>
    </trdgrp>
    <trdgrp lng="ja">
      <trd>&#24441; <pr>&#12420;&#12367;</pr></trd>,
      <trd>&#24441;&#26564; <pr>&#12420;&#12367;&#12364;&#12425;</pr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>rola<klr tip="ind"> (teatr)</klr></trd>,
      <trd>partia<klr tip="amb"> (teatr)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="rol.0o.FIG">
     <uzo tip="stl">FIG</uzo>
     <dif>
       Agado, funkcio de iu a&ubreve; io, konsiderata en siaj efikoj:
      <ekz>
        mi staras anta&ubreve; vi ankora&ubreve; en <tld/>o oficiala
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Oka Kongreso Esperantista en Krak&oacute;w en
          la 11a de a&ubreve;gusto 1912</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la sorto donis al mi <tld/>on kvankam tre flatan, tamen samtempe
        anka&ubreve; tre &scirc;ar&gcirc;an
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvina Kongreso Esperantista en
          Barcelona en la 6a de septembro 1909</vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi formetas de mi &ccirc;ian oficialan <tld/>on
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Oka Kongreso Esperantista en Krak&oacute;w en
          la 11a de a&ubreve;gusto 1912</vrk>
        </fnt>;
      </ekz>
      <ekz>
        por la <tld/>o de lingvo internacia Esperanto estas nun la sola
        kandidato
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
       <ekz>
         la kutimo ludas gravan <tld/>on en la vivo
        <fnt>
          <bib>LR</bib>
          <lok>4. Fonetiko</lok>
        </fnt>;
       </ekz>
      <ekz>
        ideoj <klr>[&#8230;]</klr> ludontaj gravan <tld/>on en la historio
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        fi, vi ja ne volos ludi la <tld/>on de erarinta filo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>?
      </ekz>
      <ekz>
        amba&ubreve; postenoj estas efektive nur duontempaj, <klr>[do]</klr>
        eblas plenumi
        samtempe du <tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2015/011468.php">Unu
          regulo por ri&ccirc;uloj, dua regulo por malri&ccirc;uloj</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="ofic.0o">ofico</ref>,
       <ref cel="okup.0o">okupo</ref>,
       <ref cel="profes.0o">profesio</ref>,
       <ref cel="task.0o">tasko</ref>
     </refgrp>
    <trd lng="en">role</trd>
    <trdgrp lng="es">
      <trd>funci&oacute;n</trd>,
      <trd>cometido</trd>
    </trdgrp>
    <trd lng="ja">&#24441;&#21106; <pr>&#12420;&#12367;&#12431;&#12426;</pr></trd>
    <trdgrp lng="pl">
      <trd>rola</trd>,
      <trd>funkcja</trd>
    </trdgrp>
    <trd lng="pt">papel</trd>
  </snc>
  <trd lng="be">&c_r;&c_o;&c_l;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>role</trd>,
    <trd>&uacute;loha</trd>
  </trdgrp>
  <trd lng="de">Rolle <klr>(Figur)</klr></trd>
  <trdgrp lng="es">
    <trd>rol</trd>,
    <trd>papel</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>r&ocirc;le</trd>
  </trdgrp>
  <trd lng="hu">szerep</trd>
  <trd lng="io">rolo</trd>
  <trd lng="it">ruolo</trd>
  <trd lng="nl">rol</trd>
  <trd lng="pl">rola</trd>
  <trdgrp lng="pt">
    <trd>parte<klr tip="amb"> (que cada ator desempenha)</klr></trd>,
    <trd>papel</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_o;&c_l;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>rola</trd>,
    <trd>&uacute;loha</trd>
  </trdgrp>
  <trd lng="uk">&c_r;&c_o;&c_l;&c_mol;</trd>
  <trdgrp lng="zh">
    <trd>&#35282;&#33394; <pr>ju&eacute;s&egrave;</pr></trd>,
    <trd>&#35282; <pr>ju&eacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rol.0i">
  <kap><tld/>i</kap>
  <snc mrk="rol.0i.TEA">
    <dif>
      Surpreni teatran, kinan, fikcian <tld/>on:
      <ekz>
        membroj studas proksime al la operejo kaj vespere <tld/>as a&ubreve;
        kantas en operaj prezentadoj
        <fnt>
          <aut>Juh&aacute;sz K.</aut>,
          <vrk>Korusa konkurso</vrk>,
          <bib>Monato</bib>,
          <lok>2000/12, p. 14</lok>
        </fnt>;
      </ekz>
      <ekz>
        la detektivo, Inspektoro <nom>Kramer</nom>, <tld/>as nur kiel
        preteksto por reliefigi la
        verajn &ccirc;ef<tld/>antojn en la romano
        <fnt>
          <aut>Donald Broadribb</aut>
            <vrk><url ref="&Monato;/2000/007079.php">Inter la dek plej bonaj romanoj</url></vrk>
            <bib>Monato</bib>
            <lok>2000/11, p. 26</lok>
          </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aktor.0i">aktori</ref>
    </refgrp>
    <trd lng="es">interpretar <klr tip="amb">(un personaje)</klr></trd>
    <trd lng="ja">&#24441;&#12434;&#28436;&#12376;&#12427; <pr>&#12420;&#12367;&#12434;&#12360;&#12435;&#12376;&#12427;</pr></trd>
  </snc>
  <snc mrk="rol.0i.FIG">
    <dif>
      <ref tip="sin" cel="part.0opreni.kunhelpi">Partopreni</ref>
      en situacio, agi konforme al iu funkcio:
      <ekz>
        kial Sud-Ameriko ne <tld/>as pli aktive en Unui&gcirc;intaj Nacioj
        <fnt>
          <aut>Maritza Guti&eacute;rrez</aut>
          <vrk><url ref="&Monato;/1993/A931202.php">A&ubreve;di&gcirc;as komuna vo&ccirc;o</url></vrk>
           <bib>Monato</bib>
          <lok>1993/12, p. 10</lok>
        </fnt>;
      </ekz>
      <ekz>
        pri la kolektiva konduto de homoj tute ne rolas genoj
       <fnt>
        <aut>Michel Duc Goninaz</aut>
        <vrk><url ref="&Monato;/2000/006947.html">Eks-Jugoslavio:
          kiu kiun mistifikas?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        fekundiga polvo de floroj, kiu <tld/>as kiel planta
        spermo
        <fnt>
          <aut>R. Roze</aut>,
          <vrk>Vivisekcio de polenero</vrk>,
          <bib>Monato</bib>,
          <lok>1997:11, p. 12a</lok>
        </fnt>;
      </ekz>
      <ekz>
        en amba&ubreve; verkoj grave <tld/>as anka&ubreve; sociaj rilatoj kaj
        kondi&ccirc;oj
        <fnt>
          <aut>G. Berveling</aut>,
          <vrk>Iom tro predikema romano</vrk>,
          <bib>Monato</bib>,
          <lok>2001:5, p. 30a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">agieren <klr tip="amb">(als)</klr></trd>
    <trd lng="ja">&#24441;&#21106;&#12434;&#26524;&#12383;&#12377;
    <pr>&#12420;&#12367;&#12431;&#12426;&#12434;&#12399;&#12383;&#12377;</pr></trd>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_c;&c_mol; <ind>&c_r;&c_o;&c_l;&c_ju;</ind></trd>
  <trd lng="cs">&uacute;&ccaron;inkovat</trd>
  <trd lng="de">eine <ind>Rolle</ind> spielen</trd>
  <trd lng="en">play a <ind>role</ind></trd>
  <trdgrp lng="fr">
    <trd>jouer un <ind>r&ocirc;le</ind></trd>,
    <trd>&ecirc;tre <ind>impliqu&eacute;</ind> dans</trd>
  </trdgrp>
  <trd lng="hu">szerepel</trd>
  <trdgrp lng="it">
    <trd>recitare</trd>,
    <trd>impersonare</trd>,
    <trd>figurare</trd>,
  </trdgrp>
  <trd lng="nl">een rol spelen</trd>
  <trdgrp lng="pl">
    <trd>gra&cacute;  <ind>rol&eogonek;</ind></trd>,
    <trd>odgrywa&cacute;  <ind>rol&eogonek;</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_g;&c_r;&c_a;&c_t;&c_mol; <ind>&c_r;&c_o;&c_l;&c_mol;</ind></trd>
  <trdgrp lng="sk">
    <trd>hra&tcaron;</trd>,
    <trd>vyst&uacute;pi&tcaron; <klr tip="amb">(v &uacute;lohe)</klr></trd>,
    <trd>&uacute;&ccaron;inkova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#25198;&#28436; <pr>b&agrave;ny&#462;n</pr></trd>,
    <trd>&#20986;&#37857; <pr>ch&umacron;j&igrave;ng</pr></trd>,
    <trd>&#20986;&#38236; <pr>ch&umacron;j&igrave;ng</pr></trd>,
    <trd>&#25198;&#28436;&#35282;&#33394; <pr>b&agrave;ny&#462;nji&#462;os&egrave;</pr></trd>,
    <trd>&#35013; <pr>zhu&amacron;ng</pr></trd>,
    <trd>&#35037; <pr>zhu&amacron;ng</pr></trd>
  </trdgrp>
</drv>

