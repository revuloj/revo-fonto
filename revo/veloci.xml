<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>velociped</rad>/o
</kap>
<drv mrk="veloci.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="vetur.0ilo">Veturilo</ref> kun du a&ubreve;
      tri <ref tip="prt" cel="rad.0o">radoj</ref>, kiun oni movas, premante
      la piedojn sur <ref tip="prt" cel="pedal.0o">pedaloj</ref>.
    </dif>
    <ref tip="super" cel="cikl.0o.veturilo">ciklo</ref>
    <refgrp tip="sub">
      <ref cel="bicikl.0o">biciklo</ref>,
      <ref cel="tandem.0o">tandemo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_o;&c_v;&c_a;&c_r;</trd>, 
    <trd>&c_v;&c_e;&c_l;&c_ja;&c_s;&c_y;&c_p;&c_e;&c_d;</trd>
  </trdgrp>  
  <trd lng="fr">cycle (sans moteur)</trd>
  <trd lng="nl">velocipede</trd>
  <trd lng="pt">veloc&iacute;pede</trd>
  <trd lng="ru">&c_v;&c_je;&c_l;&c_o;&c_s;&c_i;&c_p;&c_je;&c_d;</trd>
</drv>
</art>
