<vortaro>
  <art mrk="$Id: veloci.xml,v 1.17 2015/07/04 08:37:45 revo Exp $">
    <kap>
      <ofc>1</ofc>
      <rad>velociped</rad>/o
    </kap>

    <drv mrk="veloci.0o">
      <kap><tld/>o</kap>
      <snc mrk="veloci.0o.CIK">
        <dif>
          <ref tip="super" cel="vetur.0ilo">Veturilo</ref> kun unu, du
          a&ubreve; tri <ref tip="prt" cel="rad.0o">radoj</ref>, kiun
          oni
          movas, premante <ref tip="prt" cel="pedal.0o">pedalojn</ref>
          per la piedoj:
          <ekz>
            tute tian saman frazon oni ja povus uzi anka&ubreve;
            kontra&ubreve;
            <klr>...</klr> la arta veturado per helpo de vaporo
            a&ubreve; <tld/>o, kaj kontra&ubreve; nia tuta arta
            civilizacio!
            <fnt><bib>EE</bib>, <lok>&ccirc;ap. 1a</lok></fnt>.
          </ekz>
        </dif>
        <ref tip="super" cel="cikl.0o.veturilo">ciklo</ref>
        <refgrp tip="sub">
          <ref cel="bicikl.0o">biciklo</ref>,
          <ref cel="tandem.0o">tandemo</ref>.
        </refgrp>
      </snc>
  <trdgrp lng="be">
        <trd>&c_r;&c_o;&c_v;&c_a;&c_r;</trd>, 
        <trd>&c_v;&c_e;&c_l;&c_ja;&c_s;&c_y;&c_p;&c_e;&c_d;</trd>
      </trdgrp>
  <trd lng="ca">veloc&iacute;pede</trd>
  <trd lng="cs">velociped</trd>
  <trdgrp lng="de">
        <trd>Fahrrad</trd>,
        <trd>Veloziped<klr> [ark]</klr></trd>
      </trdgrp>
  <trd lng="fr">cycle (sans moteur)</trd>
  <trdgrp lng="hu">
        <trd>ker&eacute;kp&aacute;r</trd>,
        <trd>velocip&eacute;d</trd>
      </trdgrp>
  <trd lng="nl">velocipede</trd>
  <trd lng="pl">welocyped</trd>
  <trd lng="pt">veloc&iacute;pede</trd>
  <trd
      lng="ru">&c_v;&c_je;&c_l;&c_o;&c_s;&c_i;&c_p;&c_je;&c_d;</trd>
  <trd lng="sk">velocip√©d</trd>
</drv>
