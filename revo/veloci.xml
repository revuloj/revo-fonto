<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: veloci.xml,v 1.19 2022/03/28 11:08:54 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>velociped</rad>/o
</kap>

<drv mrk="veloci.0o">
  <kap><tld/>o</kap>
  <snc mrk="veloci.0o.CIK">
    <dif>
      <ref tip="super" cel="vetur.0ilo">Veturilo</ref> kun unu, du
      a&ubreve; tri <ref tip="prt" cel="rad.0o">radoj</ref>, kiun
      oni movas, premante <ref tip="prt" cel="pedal.0o">pedalojn</ref>
      per la piedoj:
      <ekz>
        la arta veturado per <klr>[&#x2026;]</klr> <tld/>o
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        se <ind><tld/>isto</ind> bezonas la vorton &leftquot;rado&rightquot;, li kompreneble ne kreos novan vorton
        <fnt>
          <bib>LR</bib>
          <lok>4. Vortaroj</lok>
        </fnt>;
        <trd lng="de">Fahrradfahrer</trd>
      </ekz>
      <ekz>
        tra Finnujo <tld/>e, tra la plej bela, plej &ccirc;arma lando
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
        </fnt>.
      </ekz>
    </dif>
    <!-- ref tip="super" cel="cikl.0o.veturilo">ciklo</ref -->
    <refgrp tip="sub">
      <ref cel="bicikl.0o">biciklo</ref>,
      <ref cel="tricik.0o">triciklo</ref>,
      <ref cel="tandem.0o.BIC">tandemo<sncref/></ref>
    </refgrp>
    <refgrp tip="vid">
       <ref cel="moped.0o">mopedo</ref>,
       <ref cel="motoci.0o">motociklo</ref>,
       <ref cel="skoter.0o">skotero</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_o;&c_v;&c_a;&c_r;</trd>, 
    <trd>&c_v;&c_e;&c_l;&c_ja;&c_s;&c_y;&c_p;&c_e;&c_d;</trd>
  </trdgrp>
  <trd lng="ca">veloc&iacute;pede</trd>
  <trd lng="cs">velociped</trd>
  <trdgrp lng="de">
    <trd>Fahrrad</trd>,
    <trd>Veloziped<klr> (ark)</klr></trd>
  </trdgrp>
  <trd lng="fr">cycle (sans moteur)</trd>
  <trdgrp lng="hu">
    <trd>ker&eacute;kp&aacute;r</trd>,
    <trd>velocip&eacute;d</trd>
  </trdgrp>
  <trd lng="nl">velocipede</trd>
  <trd lng="pl">welocyped</trd>
  <trd lng="pt">veloc&iacute;pede</trd>
  <trd lng="ru">&c_v;&c_je;&c_l;&c_o;&c_s;&c_i;&c_p;&c_je;&c_d;</trd>
  <trd lng="sk">velocip&eacute;d</trd>
  <trd lng="uk">&#x432;&#x435;&#x43B;&#x43E;&#x441;&#x438;&#x43F;&#x435;&#x434;</trd>
</drv>
