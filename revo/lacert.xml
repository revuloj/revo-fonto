<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lacert</rad>/o
</kap>
<drv mrk="lacert.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="lacert1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Lacerta</trd>),
      malgranda ovnaska rampulo kun kvar mallongaj piedoj kaj longa
      maldika
      vosto.
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_sh;&c_ch;&c_a;&c_r;&c_k;&c_a;</trd>
  <trd lng="bg">&c_g;&c_u;&c_shch;&c_je;&c_r;</trd>
  <trd lng="br">glazard</trd>
  <trdgrp lng="ca">
    <trd>llangardaix</trd>,
    <trd>sargantana</trd>
  </trdgrp>
  <trd lng="de">Eidechse</trd>
  <trd lng="el">&sigma;&alfa;&ypsilon_ton;&rho;&alfa;</trd>
  <trd lng="en">lizard</trd>
  <trd lng="es">lagarto</trd>
  <trd lng="fr">l&eacute;zard</trd>
  <trd lng="hu">gy&iacute;k</trd>
  <trd lng="nl">hagedis</trd>
  <trd lng="pt">lagarto</trd>
  <trd lng="ru">&c_ja;&c_shch;&c_je;&c_r;&c_i;&c_c;&c_a;</trd>
  <trd lng="tr">kertenkele</trd>
</drv>
</art>
