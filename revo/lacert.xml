<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>lacert</rad>/o
</kap>
<drv mrk="lacert.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="lacert.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Lacerta</trd>),
      malgranda ovnaska rampulo kun kvar mallongaj piedoj kaj longa maldika
      vosto.
    </dif>
  </snc>
  <trd lng="de">Eidechse</trd>
  <trd lng="tr">kertenkele</trd>
</drv>
<drv mrk="lacert.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="skvam.0uloj">skvamuloj</ref>
      (<trd lng="la">Lacertidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Eidechsen <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>
