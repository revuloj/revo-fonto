<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lacert</rad>/o
</kap>
<drv mrk="lacert.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="lacert1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Lacerta</trd>),
      malgranda ovnaska rampulo kun kvar mallongaj piedoj kaj longa maldika
      vosto.
    </dif>
  </snc>
  <trd lng="de">Eidechse</trd>
  <trd lng="en">lizard</trd>
  <trd lng="nl">hagedis</trd>
  <trd lng="ru">&c_ja;&c_shch;&c_je;&c_r;&c_i;&c_c;&c_a;</trd>
  <trd lng="tr">kertenkele</trd>
</drv>
</art>
