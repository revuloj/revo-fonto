<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: koteri.xml,v 1.17 2021/02/09 18:41:54 revo Exp $">
<kap>
  <rad>koteri</rad>/o
</kap>
<drv mrk="koteri.0o">
  <kap><tld/>o</kap>  
  <snc>
    <dif>
      <ref tip="super" cel="grup.0acxo">Grupa&ccirc;o</ref> da homoj, kiuj
      &scirc;atas kaj la&ubreve;das nur tion, kion iu el ili faris; ekskluziva
      partieta&ccirc;o:
      <ekz>
        politika, beletra <tld/>o;
      </ekz>
      <ekz>
        &ccirc;irka&ubreve;rigardo por sorbi la aprobajn rigardojn de la <tld/>o
        <fnt>
          <aut>Cezaro Rossetti</aut>
          <vrk><url ref="https://tekstaro.com/t?nomo=kredu-min-sinjorino">Kredu min, sinjorino!</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;rapnela respondo al la tuta naiva <tld/>o: kleristoj, humanistoj <klr>[&#8230;]</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Filippo Franceschi</aut>
          <vrk><url ref="https://www.monato.be/2006/009045.php">Mussolini, Hitler, Stalin kaj Bush</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        la organizantoj <klr>[&#8230;]</klr> strebas fari bonkvalitan forumon por
        &leftquot;inteligentaj homoj&rightquot;, ne por bruaktivula memsufi&ccirc;a <tld/>o
        <fnt>
          <vrk><url ref="https://tekstaro.com/t?nomo=ondo-de-esperanto">ARKONES: &ccirc;iam pli sukcesa</url></vrk>
          <lok>La Ondo de Esperanto 2001 &ndash; 2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="klik.0o.grupacxo">kliko<sncref/></ref>
    <refgrp tip="vid">
      <ref cel="elit.0o">elito</ref>,
      <ref cel="klub.0o">klubo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_r;&c_u;&c_p;&c_o;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_h;&c_a;&c_w;&c_r;&c_u;&c_s;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_k;&c_n;&c_u;&c_t;&c_a;&c_je; &c_k;&c_o;&c_l;&c_a; <klr>(&c_l;&c_ju;&c_d;&c_z;&c_je;&c_j;)</klr></trd>
  </trdgrp>
  <trd lng="ca">camarilla</trd>
  <trd lng="cs">koterie</trd>
  <trdgrp lng="de">
    <trd>Kl&uuml;ngel</trd>,
    <trd>Koterie</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>coterie</trd>,
    <trd>exculsive <ind>circle</ind></trd>,
    <trd>set</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>camarilla</trd>,
    <trd>argolla</trd>,
  </trdgrp>
  <trd lng="fr">coterie</trd>
  <trdgrp lng="hu">
    <trd>klikk</trd>,
    <trd>&eacute;rdeksz&ouml;vets&eacute;g</trd>,
    <trd>kot&eacute;ria</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>coterie</trd>,
    <trd>kliek</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>koteria</trd>,
    <trd>sitwa</trd>,
    <trd>uk&lstroke;ad</trd>,
    <trd>Grupa Trzymaj&aogonek;ca W&lstroke;adz&eogonek; <klr>(pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>panelinha</trd>,
    <trd>igrejinha</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_u;&c_p;&c_p;&c_i;&c_r;&c_o;&c_v;&c_k;&c_a; <klr>(&c_n;&c_je;&c_o;&c_d;&c_o;&c_b;&c_r;.)</klr></trd>,
    <trd>&c_z;&c_a;&c_m;&c_k;&c_n;&c_u;&c_t;&c_y;&c_j; &c_k;&c_r;&c_u;&c_zh;&c_o;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>klika</trd>,
    <trd>kot&eacute;ria</trd>
  </trdgrp>
</drv>
</art>
