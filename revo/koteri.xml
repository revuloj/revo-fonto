<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>koteri</rad>/o
</kap>
<drv mrk="koteri.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Aro da personoj, kiuj &scirc;atas kaj la&ubreve;das nur tion, kion iu el ili
      faris; ekskluziva partieta&ccirc;o:
      <ekz>
        politika, beletra <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="klan.0o">klano 2</ref>,
      <ref cel="kamari.0o">kamarilo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="en">
    <trd>coterie</trd>,
    <trd>exculsive <ind>circle</ind></trd>,
    <trd>set</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_u;&c_p;&c_p;&c_i;&c_r;&c_o;&c_v;&c_k;&c_a; <klr>(&c_n;&c_je;&c_o;&c_d;&c_o;&c_b;&c_r;.)</klr></trd>,
    <trd>&c_z;&c_a;&c_m;&c_k;&c_n;&c_u;&c_t;&c_y;&c_j; &c_k;&c_r;&c_u;&c_zh;&c_o;&c_k;</trd>
  </trdgrp>
</drv>
</art>
