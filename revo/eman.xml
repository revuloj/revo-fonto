<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: eman.xml,v 1.22 2025/09/15 22:29:46 revo Exp $">
<kap>
  <rad>eman</rad>/i <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="eman.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="eman.0i.KEM">
    <uzo tip="fak">KEM</uzo>
    <uzo tip="klr">(plej ofte pri gasoj kaj radio)</uzo>
    <dif>
      Eli&gcirc;i nevideble.
    </dif>
  </snc>
  <rim>
    en nefaka uzo, la&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>eli&gcirc;i</ke>, <ke>elveni</ke>.
  </rim>

  <trd lng="be">&c_v;&c_y;&c_d;&c_z;&c_ja;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>emanar</trd>,
    <trd>exhalar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>emanovat</trd>,
    <trd>uvol&ncaron;ovat se</trd>,
    <trd>vylu&ccaron;ovat se</trd>,
    <trd>vyv&iacute;jet se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ausstr&ouml;men</trd>,
    <trd>entweichen</trd>,
    <trd>hervorgehen</trd>,
    <trd>ausstrahlen</trd>
  </trdgrp>
  <trd lng="en">emanate</trd>
  <trdgrp lng="fr">
    <trd>se <ind>d&eacute;gager</ind><klr tip="ind">
    (&eacute;maner)</klr></trd>,
    <trd>&eacute;maner</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#25918;&#23556;&#12377;&#12427;  <pr>&#12411;&#12358;&#12375;&#12419;&#12377;&#12427;</pr></trd>,
    <trd>&#30330;&#25955;&#12377;&#12427;  <pr>&#12399;&#12387;&#12373;&#12435;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>emanowa&cacute;</trd>,
    <trd>promieniowa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">emanar</trd>
  <trd lng="ru">&c_v;&c_y;&c_d;&c_je;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>emanova&tcaron;</trd>,
    <trd>unika&tcaron;</trd>,
    <trd>vy&zcaron;arova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_t;&c_ib;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_p;&c_r;&c_o;&c_m;&c_ib;&c_n;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#28431;&#20986; <pr>l&ograve;uch&umacron;</pr></trd>,
    <trd>&#27900;&#20986; <pr>p&omacron;ch&umacron;</pr></trd>,
    <trd>&#27969;&#20986; <pr>li&uacute;ch&umacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="eman.0o">
  <kap><tld/>o</kap>
  <snc mrk="eman.0o.KEM">
    <dif>
      Ago, per kiu eli&gcirc;emaj (vapori&gcirc;emaj) substancoj
      <tld/>as el la korpoj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_d;&c_z;&c_ja;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_e;&c_m;&c_a;&c_n;&c_a;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>emanaci&oacute;</trd>,
    <trd>efluvi</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>emanace</trd>,
    <trd>inkluze</trd>,
    <trd>izolace</trd>,
    <trd>odd&ecaron;len&iacute; se</trd>,
    <trd>tok</trd>,
    <trd>tvorba</trd>,
    <trd>uvoln&ecaron;n&iacute; se</trd>,
    <trd>vylou&ccaron;en&iacute; se</trd>,
    <trd>v&yacute;ron</trd>,
    <trd>v&yacute;vin</trd>,
    <trd>z&aacute;&rcaron;en&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ausstr&ouml;men</trd>,
    <trd>Entweichen</trd>,
    <trd>Emanation</trd>,
    <trd>Ausstrahlen</trd>
  </trdgrp>
  <trd lng="en">emanation</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;gagement<klr tip="ind">
    (&eacute;manation)</klr></trd>,
    <trd>&eacute;manation<klr tip="ind"> (action)</klr></trd>
  </trdgrp>
  <trd lng="he">&gimel;&zayin; &resh;&dalet;&yod;&vav; &alef;&qof;&tet;&yod;&bet;&yod;</trd>
  <trdgrp lng="ja">
    <trd>&#25918;&#23556;  <pr>&#12411;&#12358;&#12375;&#12419;</pr></trd>,
    <trd>&#30330;&#25955;  <pr>&#12399;&#12387;&#12373;&#12435;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>emanacja</trd>,
    <trd>promieniowanie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_m;&c_a;&c_n;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_v;&c_y;&c_d;&c_je;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>izol&aacute;cia</trd>,
    <trd>oddelenie</trd>,
    <trd>tvorba</trd>,
    <trd>vyl&uacute;&ccaron;enie sa</trd>,
    <trd>&zcaron;iarenie</trd>
  </trdgrp>
</drv>

<drv mrk="eman.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="eman.0ajxo.KOMUNE">
    <dif>
      Tio, kio <tld/>as.
    </dif>
  </snc>
  <snc mrk="eman.0ajxo.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      <tld lit="E"/>anta gaso:
      <ekz>
        nitono estas radiuma <tld/>a&jcirc;o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_d;&c_z;&c_ja;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_e;&c_m;&c_a;&c_n;&c_a;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
  <trd lng="ca">emanaci&oacute;</trd>
  <trdgrp lng="de">
    <trd>Ausstr&ouml;mendes</trd>,
    <trd>entweichendes <ind>Gas</ind></trd>,
    <trd>entweichende <ind>Strahlung</ind></trd>
  </trdgrp>
  <trd lng="en">emanation</trd>
  <trd lng="fr">&eacute;manation<klr tip="ind"> (ce qui
  &eacute;mane)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#25918;&#23556;&#29289;  <pr>&#12411;&#12358;&#12375;&#12419;&#12406;&#12388;</pr></trd>,
    <trd>&#30330;&#25955;&#29289;  <pr>&#12399;&#12387;&#12373;&#12435;&#12406;&#12388;</pr></trd>,
    <trd>&#12456;&#12510;&#12490;&#12481;&#12458;&#12531;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>emanacja</trd>,
    <trd>wyp&lstroke;yw</trd>,
    <trd>substancja wydzielaj&aogonek;ca si&eogonek;</trd>,
    <trd>rzecz zrodzona z bytu wy&zdot;szego <klr>(filozof.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_m;&c_a;&c_n;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_v;&c_y;&c_d;&c_je;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="uk">&c_je;&c_m;&c_a;&c_n;&c_a;&c_c;&c_ib;&c_ja;</trd>
</drv>

<drv mrk="eman.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="eman.0igi.KEM">
    <dif>
      Eligi:
    </dif>
    <ekz>radiumo <tld/>igas energion.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_d;&c_z;&c_ja;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_e;&c_m;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>alliberar</trd>,
    <trd>deslliurar</trd>
  </trdgrp>
  <trd lng="cs">vyvolat emanaci</trd>
  <trdgrp lng="de">
    <trd>ausstr&ouml;men</trd>,
    <trd>aussenden</trd>,
    <trd>ausstrahlen</trd>
  </trdgrp>
  <trd lng="en">emanate</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;gager</trd>,
    <trd>lib&eacute;rer<klr tip="ind"> (une
    &eacute;manation)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#25918;&#23556;&#12377;&#12427;  <pr>&#12411;&#12358;&#12375;&#12419;&#12377;&#12427;</pr></trd>,
    <trd>&#30330;&#25955;&#12377;&#12427;  <pr>&#12399;&#12387;&#12373;&#12435;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>powodowa&cacute; <ind>wypromieniowanie</ind></trd>,
    <trd>powodowa&cacute; <ind>wyp&lstroke;yw</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_y;&c_d;&c_je;&c_l;&c_ja;&c_t;&c_mol;</trd>
  <trd lng="sk">vyvola&tcaron; eman&aacute;ciu</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_m;&c_u;&c_sh;&c_u;&c_v;&c_a;&c_t;&c_i; &c_v;&c_i;&c_p;&c_r;&c_o;&c_m;&c_ib;&c_n;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_k;&c_l;&c_i;&c_k;&c_a;&c_t;&c_i; &c_je;&c_m;&c_a;&c_n;&c_a;&c_c;&c_ib;&c_ju;</trd>
  </trdgrp>
</drv>

<adm>
(1) En la franca ekzistas GEOL, REL k FIL sencoj. Vd ankaux la REL senco
de eligi.
(2) ~ajxo 2: laux la ekz-o temas pli pri FIZ ol KEM. Kio estas nitono?
<aut>MB</aut>
</adm>
</art>

