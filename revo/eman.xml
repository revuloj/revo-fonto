<kap>
  <rad>eman</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="eman.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="eman.0i.KEM">
    <uzo tip="fak">KEM</uzo>
    <uzo tip="klr">(plej ofte pri gasoj kaj radio)</uzo>
    <dif>
      Eli&gcirc;i nevideble. 
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_d;&c_z;&c_ja;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>emanar</trd>,
    <trd>exhalar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>emanovat</trd>,
    <trd>uvol&#x148;ovat se</trd>,
    <trd>vylu&#x10D;ovat se</trd>,
    <trd>vyv&#xED;jet se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ausstr&ouml;men</trd>,
    <trd>entweichen</trd>,
    <trd>hervorgehen</trd>,
    <trd>ausstrahlen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>d&eacute;gager</ind><klr tip="ind">
    (&eacute;maner)</klr></trd>,
    <trd>&eacute;maner</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x653E;&#x5C04;&#x3059;&#x308B;  <pr>&#x307B;&#x3046;&#x3057;&#x3083;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x767A;&#x6563;&#x3059;&#x308B;  <pr>&#x306F;&#x3063;&#x3055;&#x3093;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>emanowa&cacute;</trd>,
    <trd>promieniowa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">emanar</trd>
  <trd lng="ru">&c_v;&c_y;&c_d;&c_je;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>emanova&#x165;</trd>,
    <trd>unika&#x165;</trd>,
    <trd>vy&#x17E;arova&#x165;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x438;&#x442;&#x456;&#x43A;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x438;&#x43F;&#x440;&#x43E;&#x43C;&#x456;&#x43D;&#x44E;&#x432;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6F0F;&#x51FA; <pr>l&#xF2;uch&#x16B;</pr></trd>,
    <trd>&#x6CFC;&#x51FA; <pr>p&#x14D;ch&#x16B;</pr></trd>,
    <trd>&#x6D41;&#x51FA; <pr>li&#xFA;ch&#x16B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="eman.0o">
  <kap><tld/>o</kap>
  <snc mrk="eman.0o.KEM">
    <dif>
      Ago, per kiu eli&gcirc;emaj (vapori&gcirc;emaj) substancoj
      <tld/>as el la korpoj. 
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_d;&c_z;&c_ja;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_e;&c_m;&c_a;&c_n;&c_a;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>emanaci&oacute;</trd>,
    <trd>efluvi</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>emanace</trd>,
    <trd>inkluze</trd>,
    <trd>izolace</trd>,
    <trd>odd&#x11B;len&#xED; se</trd>,
    <trd>tok</trd>,
    <trd>tvorba</trd>,
    <trd>uvoln&#x11B;n&#xED; se</trd>,
    <trd>vylou&#x10D;en&#xED; se</trd>,
    <trd>v&#xFD;ron</trd>,
    <trd>v&#xFD;vin</trd>,
    <trd>z&#xE1;&#x159;en&#xED;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ausstr&ouml;men</trd>,
    <trd>Entweichen</trd>,
    <trd>Emanation</trd>,
    <trd>Ausstrahlen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;gagement<klr tip="ind">
    (&eacute;manation)</klr></trd>,
    <trd>&eacute;manation<klr tip="ind"> (action)</klr></trd>
  </trdgrp>
  <trd lng="he">&#x5D2;&#x5D6; &#x5E8;&#x5D3;&#x5D9;&#x5D5; &#x5D0;&#x5E7;&#x5D8;&#x5D9;&#x5D1;&#x5D9;</trd>
  <trdgrp lng="ja">
    <trd>&#x653E;&#x5C04;  <pr>&#x307B;&#x3046;&#x3057;&#x3083;</pr></trd>,
    <trd>&#x767A;&#x6563;  <pr>&#x306F;&#x3063;&#x3055;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>emanacja</trd>,
    <trd>promieniowanie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_m;&c_a;&c_n;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_v;&c_y;&c_d;&c_je;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>izol&#xE1;cia</trd>,
    <trd>oddelenie</trd>,
    <trd>tvorba</trd>,
    <trd>vyl&#xFA;&#x10D;enie sa</trd>,
    <trd>&#x17E;iarenie</trd>
  </trdgrp>
</drv>

<drv mrk="eman.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="eman.0ajxo.KOMUNE">
    <dif>
      Tio, kio <tld/>as. 
    </dif>
  </snc>
  <snc mrk="eman.0ajxo.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      <tld lit="E"/>anta gaso:
      <ekz>
        nitono estas radiuma <tld/>a&jcirc;o.
      </ekz> 
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_d;&c_z;&c_ja;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_e;&c_m;&c_a;&c_n;&c_a;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
  <trd lng="ca">emanaci&oacute;</trd>
  <trdgrp lng="de">
    <trd>Ausstr&ouml;mendes</trd>,
    <trd>entweichendes <ind>Gas</ind></trd>,
    <trd>entweichende <ind>Strahlung</ind></trd>
  </trdgrp>
  <trd lng="fr">&eacute;manation<klr tip="ind"> (ce qui
  &eacute;mane)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x653E;&#x5C04;&#x7269;  <pr>&#x307B;&#x3046;&#x3057;&#x3083;&#x3076;&#x3064;</pr></trd>,
    <trd>&#x767A;&#x6563;&#x7269;  <pr>&#x306F;&#x3063;&#x3055;&#x3093;&#x3076;&#x3064;</pr></trd>,
    <trd>&#x30A8;&#x30DE;&#x30CA;&#x30C1;&#x30AA;&#x30F3;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_m;&c_a;&c_n;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_v;&c_y;&c_d;&c_je;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="uk">&#x435;&#x43C;&#x430;&#x43D;&#x430;&#x446;&#x456;&#x44F;</trd>
</drv>

<drv mrk="eman.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="eman.0igi.KEM">
    <dif>
      Eligi: 
    </dif>
    <ekz>radiumo <tld/>igas energion.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_d;&c_z;&c_ja;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_e;&c_m;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>alliberar</trd>,
    <trd>deslliurar</trd>
  </trdgrp>
  <trd lng="cs">vyvolat emanaci</trd>
  <trdgrp lng="de">
    <trd>ausstr&ouml;men</trd>,
    <trd>aussenden</trd>,
    <trd>ausstrahlen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;gager</trd>,
    <trd>lib&eacute;rer<klr tip="ind"> (une
    &eacute;manation)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x653E;&#x5C04;&#x3059;&#x308B;  <pr>&#x307B;&#x3046;&#x3057;&#x3083;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x767A;&#x6563;&#x3059;&#x308B;  <pr>&#x306F;&#x3063;&#x3055;&#x3093;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_y;&c_d;&c_je;&c_l;&c_ja;&c_t;&c_mol;</trd>
  <trd lng="sk">vyvola&#x165; eman&#xE1;ciu</trd>
  <trdgrp lng="uk">
    <trd>&#x437;&#x43C;&#x443;&#x448;&#x443;&#x432;&#x430;&#x442;&#x438; &#x432;&#x438;&#x43F;&#x440;&#x43E;&#x43C;&#x456;&#x43D;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x438;&#x43A;&#x43B;&#x438;&#x43A;&#x430;&#x442;&#x438; &#x435;&#x43C;&#x430;&#x43D;&#x430;&#x446;&#x456;&#x44E;</trd>
  </trdgrp>
</drv>

<adm>
(1) En la franca ekzistas GEOL, REL k FIL sencoj. Vd ankaux la REL senco
de eligi.
(2) ~ajxo 2: laux la ekz-o temas pli pri FIZ ol KEM. Kio estas nitono?
<aut>MB</aut>
</adm>
