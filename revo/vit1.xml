<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: vit1.xml,v 1.3 2015/04/25 13:10:16 revo Exp $">
    <kap>
      <rad>Vit</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>
    <drv mrk="vit1.0o">
      <kap><tld/>o</kap>
      <snc>
      <uzo tip="fak">PERS</uzo>
        <dif>
          Vira anta&ubreve;nomo.
        </dif>
      </snc>
      <ref tip="hom" cel="vit.0o">vito</ref>
      <trd lng="en">Vitus</trd>
      <trd lng="fr">Guy</trd>
      <trdgrp lng="pl">
        <trd>Witold</trd>,
        <trd>Wit</trd>
      </trdgrp>
      <trd lng="ru">&c_V;&c_i;&c_t;</trd>
    </drv>

    <drv mrk="vit1.malsanode0o">
      <kap>malsano de sankta <tld/>o</kap>
      <snc>
      <uzo tip="fak">MED</uzo>
        <dif>
          La &#171;malgranda <ref tip="super" cel="hxore.0o">&hcirc;oreo</ref>&#187;,
          malsano karakterizata per rapidaj, nekunordigitaj subitaj
          movoj precipe en la viza&gcirc;o, manoj kaj piedoj:
          <ekz>
            li faras gestojn, kiel &ccirc;e la malsano de sankta <tld/>o
            <fnt><aut>&Scirc;ilero, tr. &Z;</aut>: <vrk>Rabistoj</vrk>,
              <lok>1:2</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="en">
        <trd>Sydenham's chorea</trd>,
        <trd>chorea minor</trd>,
        <trd>Saint Vitus Dance</trd>
      </trdgrp>
      <trd lng="fr">danse de Saint Guy</trd>
      <trd lng="pl">choroba &Sacute;wi&eogonek;tego Wita <klr>(pl&aogonek;sawica)</klr></trd>
      <trd lng="ru">&c_p;&c_l;&c_ja;&c_s;&c_k;&c_a; &c_s;&c_v;&c_ja;&c_t;&c_o;&c_g;&c_o; &c_V;&c_i;&c_t;&c_a;</trd>
    </drv>


  </art>

