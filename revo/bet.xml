<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>bet</rad>/o
</kap>
<drv mrk="bet.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bet.0o.genro">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="kenopodi.0acoj">kenopodiacoj</ref>
      (<trd lng="la">Beta</trd>):
      <ekz>ru&gcirc;a <tld/>o</ekz>; <ekz>suker<tld/>o</ekz>. 
    </dif>
    <trd lng="ru">&c_s;&c_v;&#1105;&c_k;&c_l;&c_a;</trd>
  </snc>
  <snc mrk="bet.0o.radiko">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Dika, karna radiko, kiel &ccirc;e ru&gcirc;a <tld/>o, karoto.
    </dif>
  </snc>
  <trd lng="de">R&uuml;be</trd>
</drv>
</art>
