<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bet.xml,v 1.46 2023/10/29 19:11:01 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bet</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="bet.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bet.0o.genro">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Planto kun man&gcirc;eblaj folioj kaj dika radiko kultivata en
      mezvarmaj landoj, <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref>
      el familio amarantacoj (<trd lng="la">Beta</trd>):
      <ekz>
        la origino de blanka biero estas &ccirc;irka&ubreve; la 14a jarcento en Orienta Brabanto,
        tritiko, aveno kaj <tld/>oj estas de plej frua epoko tie kultivataj
        <fnt>
          <aut>Lode van de Velde</aut>
          <vrk><url ref="&Monato;/2001/007144.html">Tradicioj &dash; Bierlando Belgio</url></vrk>
          <bib>Monato</bib>
          <lok>2001/01, p. 18</lok>
        </fnt>;
      </ekz>
      <ekz>
        geosmino <klr>[&#x2026;]</klr> donas al ru&gcirc;aj <tld/>oj ilian teran guston
        <fnt>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2015/011480.php">Pluveraj veroj</url></vrk>
          <bib>Monato</bib>
          <lok>2015/11, p. 22</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/9/9d/Beta_vulgaris_Pokorny.jpg/333px-Beta_vulgaris_Pokorny.jpg" prm="PD">A. Pokorny, Ordinara <tld lit="B"/>o, botanika tabulo, 19a jc
        <fnt>
          <aut>Alois Pokorny (1826-1886), Botanische Wandtafeln</aut>
          <lok><url ref="&WCW;/File:Beta_vulgaris_Pokorny.jpg">&WCW;/File:Beta_vulgaris_Pokorny.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="be">&c_b;&c_u;&c_r;&c_a;&c_k;</trd>
    <trd lng="br">beterabezenn</trd>
    <trdgrp lng="cs">
    <trd>&rcaron;epa</trd>,
    <trd>&rcaron;&iacute;pa</trd>
    </trdgrp>
    <trd lng="de">R&uuml;be</trd>
    <trd lng="en">beet</trd>
    <trd lng="es">remolacha</trd>
    <trdgrp lng="fr">
      <trd>betterave</trd>,
      <trd>bette</trd>
    </trdgrp>
    <trd lng="hu">r&eacute;pa</trd>
    <trd lng="nl">biet</trd>
    <trd lng="pl">burak</trd>
    <trd lng="pt">beterraba</trd>
    <trd lng="ru">&c_s;&c_v;&c_jo;&c_k;&c_l;&c_a;</trd>
    <trd lng="sk">repa</trd>
    <trd lng="sv">beta</trd>
    <trd lng="tr">pancar</trd>
    <trd lng="zh">&#x751C;&#x83DC; <pr>ti&#xE1;nc&#xE0;i</pr></trd>
</snc>
  <snc mrk="bet.0o.radiko">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Dika, karna pivot<ref tip="super" cel="radik.0o">radiko</ref>,
      kiel &ccirc;e ru&gcirc;a <tld/>o, karoto.
    </dif>
    <trdgrp lng="cs">
      <trd>&rcaron;epa</trd>,
      <trd>&rcaron;&iacute;pa</trd>
    </trdgrp>
    <trd lng="de">R&uuml;be</trd>
    <trd lng="en">beet</trd>
    <trd lng="es">remolacha</trd>
    <trd lng="hu">r&eacute;pa</trd>
    <trd lng="id">bit</trd>
    <trd lng="nl">biet</trd>
    <trd lng="pl">burak</trd>
    <trd lng="pt">beterraba</trd>
    <trd lng="sk">repa</trd>
    <trd lng="sv">beta</trd>
    <adm>
      La difino sugestas, ke la man&gcirc;ebla parto de karoto, rafaneto
      kaj multaj aliaj legomoj nomi&gcirc;as "beto". Tio mirigas min.
      PIV1
      konas tian sencon nur por "ru&gcirc;a beto". <aut>MB</aut>
    </adm>
    <trdgrp lng="zh">
    <trd>&#x8451; <pr>f&#x113;ng</pr></trd>,
    <trd>&#x8513;&#x83C1; <pr>m&#xE1;njing</pr></trd>,
    <trd>&#x829C;&#x83C1; <pr>w&#xFA;j&#x12B;ng</pr></trd>,
    <trd>&#x751C;&#x83DC;&#x6839; <pr>ti&#xE1;nc&#xE0;ig&#x113;n</pr></trd>,
    <trd>&#x751C;&#x83DC; <pr>ti&#xE1;nc&#xE0;i</pr></trd>,
    <trd>&#x539A;&#x984F;&#x7121;&#x6065; <pr>h&#xF2;uy&#xE1;nw&#xFA;ch&#x1D0;</pr></trd>
  </trdgrp>
</snc>
  <ref tip="hom" cel="bet1.0o">beto (greka litero)</ref>
  <trd lng="bo">&#xF63;&#xF0B;&#xF55;&#xF74;&#xF42;&#xF0B;</trd>
  <trd lng="he">&samekh;&lamed;&qof; </trd>
  <trdgrp lng="ja">
    <trd>&#x30D3;&#x30FC;&#x30C8;  <pr>&#x3073;&#x30FC;&#x3068;</pr></trd>,
    <trd>&#x30C6;&#x30F3;&#x30B5;&#x30A4;</trd>,
    <trd>&#x30B5;&#x30C8;&#x30A6;&#x30C0;&#x30A4;&#x30B3;&#x30F3;</trd>
  </trdgrp>
  <trd lng="uk">&c_b;&c_u;&c_r;&#x44F;&#x43A;</trd>
</drv>

<drv mrk="bet.foli0o">
  <kap>foli<tld/>o</kap> <fnt><bib>HejmVort</bib></fnt>
  <snc mrk="bet.foli0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Speco de <ref tip="super" cel="bet.0o.genro"><tld/>o</ref> kun
      man&gcirc;eblaj folioj (<trd lng="la">Beta vulgaris ssp. vulgaris</trd>):
      <ekz>
        kultiva&jcirc;oj <klr>[estas]</klr> melongeno, spinaco kaj foli<tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=7582">Agrikulturo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/23/Gelber_und_roter_Mangold.JPG/640px-Gelber_und_roter_Mangold.JPG" prm="CC BY-SA 3.0">flava kaj ru&gcirc;a foli<tld/>o
        <fnt>
          <aut>Claus Ableiter, propra verko</aut>
          <lok><url ref="&WCW;/File:Gelber_und_roter_Mangold.JPG">&WCW;/File:Gelber_und_roter_Mangold.JPG</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="zh">
    <trd>&#x539A;&#x76AE;&#x83DC; <pr>h&#xF2;up&#xED;c&#xE0;i</pr></trd>,
    <trd>&#x8399;&#x8359;&#x83DC; <pr>j&#x16B;nd&#xE1;c&#xE0;i</pr></trd>,
    <trd>&#x7CD6;&#x83B4;&#x82E3; <pr>t&#xE1;ngw&#x14D;j&#xF9;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="cs">
    <trd>cvikla</trd>,
    <trd>mangold</trd>
  </trdgrp>
  <trd lng="de">Mangold</trd>
  <trd lng="en">Chard</trd>
  <trdgrp lng="fr">
    <trd>bette</trd>,
    <trd>blette</trd>
  </trdgrp>
  <trd lng="id"><ind>bit</ind> daun</trd>
  <trdgrp lng="ja">
    <trd>&#x30D5;&#x30C0;&#x30F3;&#x30BD;&#x30A6;</trd>,
    <trd>&#x30C8;&#x30A6;&#x30C2;&#x30B7;&#x30E3;</trd>
  </trdgrp>
  <trd lng="pl">bo&cacute;wina</trd>
  <trd lng="sk">cvikla</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_r;&#x44F;&#x43A; &#x43B;&c_i;&c_s;&c_t;&#x43A;&#x43E;&c_v;&c_i;&c_j;</trd>,
    <trd>&#x43C;&c_a;&#x43D;&c_gu;&#x43E;&#x43B;&#x44C;&c_d;</trd>
  </trdgrp>
</drv>

<drv mrk="bet.furagx0o">
  <kap>fura&gcirc;<tld/>o</kap>
  <snc mrk="bet.furagx0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      <ref tip="super" cel="bet.0o.genro"><tld lit="B"/>o</ref> el
      specoj uzataj por nutri brutojn (<trd lng="la">Beta
      vulgaris</trd>):
      <ekz>
        ekde la oka jaro de mia vivo mi devis multe helpi en la
        rikoltado de la terpomoj kaj fura&gcirc;<tld/>oj sur la bieno de
        mia onklo
        <fnt>
          <bib>FSI</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Futterr&uuml;be</trd>,
      <trd>Runkelr&uuml;be</trd>,
      <trd>Vieh-Mangold</trd>
    </trdgrp>
    <trd lng="fr">bettrave fourrag&egrave;re</trd>
    <trd lng="pl">burak pastewny</trd>
  </snc>
  <trd lng="uk">&#x43A;&#x43E;&c_r;&#x43C;&#x43E;&c_v;&c_i;&c_j; &c_b;&c_u;&c_r;&#x44F;&#x43A;</trd>
</drv>

<drv mrk="bet.rugxa0o">
  <kap>ru&gcirc;a <tld/>o</kap> <fnt><bib>HejmVort</bib></fnt>
  <snc mrk="bet.rugxa0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Speco de <ref tip="super" cel="bet.0o.genro"><tld/>o</ref> kun
      malhele ru&gcirc;a radiko man&gcirc;ebla post kuirado:
      <ekz>
        <ref tip="malprt" cel="barcx.0o">bar&ccirc;o</ref> <klr>[&#x2026;]</klr> estas kuirata
        el ru&gcirc;a <tld/>o, brasiko, cepo, karoto kaj diversaj specoj de viando
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=124731">Bar&ccirc;o</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x751C;&#x83DC;&#x6839; <pr>ti&#xE1;nc&#xE0;ig&#x113;n</pr></trd>
</snc>
  <trd lng="br">beterabezenn ruz</trd>
  <trd lng="de">Rote Bete</trd>
  <trd lng="hu">c&eacute;kla</trd>
  <trd lng="fr">betterave rouge</trd>
  <trd lng="id"><ind>bit</ind> merah</trd>
  <trd lng="ja">&#x8D64;&#x304B;&#x3076;  <pr>&#x3042;&#x304B;&#x304B;&#x3076;</pr></trd>
  <trd lng="nl">rode <ind>biet</ind></trd>
  <trd lng="pl">burak<klr> czerwony</klr></trd>
  <trd lng="ru">&c_s;&c_t;&c_o;&c_l;&c_o;&c_v;&c_a;&c_ja;
  <ind>&c_s;&c_v;&c_jo;&c_k;&c_l;&c_a;</ind></trd>
</drv>

<drv mrk="bet.suker0o">
  <kap>suker<tld/>o</kap>
  <snc mrk="bet.suker0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      <ref tip="super" cel="bet.0o.genro"><tld lit="B"/>o</ref> kun
      sukerri&ccirc;a radiko, uzata por industrie produkti <ref tip="prt" cel="suker.0o">sukero</ref>n:
      <ekz>
        pulpo de suker<tld/>oj
        <fnt>
          <aut>Akademio de Esperanto</aut>,
          <vrk><url ref="&AdE;/oficialaj_informoj/oficialaj_informoj_8_2007.html">
          Oficialaj informoj, 8</url></vrk>,
          <lok>2007-03-10</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la malsupra fluo de Mekongo, oni kultivas <klr>[&#x2026;]</klr> anka&ubreve; maizon,
        suker<tld/>on, tabakon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=29586">Mekongo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kan.suker0o">sukerkano</ref>
    <trdgrp lng="zh">
    <trd>&#x83FE; <pr>ti&#xE1;n</pr></trd>,
    <trd>&#x751C;&#x83DC; <pr>ti&#xE1;nc&#xE0;i</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_c;&c_u;&c_k;&c_r;&c_o;&c_v;&c_y;
  &c_b;&c_u;&c_r;&c_a;&c_k;</trd>
  <trd lng="br">beterabezenn sukr</trd>
  <trd lng="cs">&rcaron;epa cukrovka</trd>
  <trd lng="de">Zuckerr&uuml;be</trd>
  <trd lng="fr">betterave sucri&egrave;re</trd>
  <trd lng="hu">cukorr&eacute;pa</trd>
  <trd lng="id"><ind>bit</ind> gula</trd>
  <trd lng="ja">&#x30B5;&#x30C8;&#x30A6;&#x30C0;&#x30A4;&#x30B3;&#x30F3;</trd>
  <trd lng="nl">suikerbiet</trd>
  <trd lng="pl">burak cukrowy</trd>
  <trd lng="ru">&c_s;&c_a;&c_h;&c_a;&c_r;&c_n;&c_a;&c_ja;
  <ind>&c_s;&c_v;&c_jo;&c_k;&c_l;&c_a;</ind></trd>
  <trd lng="sk">cukrov&aacute; repa</trd>
  <trd lng="tr">&scedil;eker pancar&inodot;</trd>
</drv>
</art>
