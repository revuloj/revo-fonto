<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: uvul.xml,v 1.22 2023/10/21 16:50:28 revo Exp $">
<kap>
  <rad>uvul</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="uvul.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Karna apendico, pendanta &ccirc;e la mezo de la malanta&ubreve;a rando de
      la palata <ref cel="vel.0o.ANA">velo</ref>:
      <ekz>
        la komenca sono de kuzo estas farata per la malanta&ubreve;a parto de la langodorso
        kontra&ubreve; la malanta&ubreve;a parto de la velo, &ccirc;e la <tld/>o
        <fnt>
          <bib>LAE</bib>
          <lok>2. Fonetiko</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>uvula</trd>,
    <trd>&ccaron;ip</trd>,
    <trd>&ccaron;&iacute;pek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Z&auml;pfchen</trd>,
    <trd>Uvula</trd></trdgrp>
  <trd lng="en">uvula</trd>
  <trd lng="es">&uacute;vula</trd>
  <trd lng="fi">kitakieleke</trd>
  <trd lng="fr">luette</trd>
  <trdgrp lng="hu">
    <trd>nyelvcsap</trd>,
    <trd>uvula</trd>
  </trdgrp>
  <trd lng="it">ugola</trd>
  <trdgrp lng="ja">
    <trd>&#x53E3;&#x84CB;&#x5782;  <pr>&#x3053;&#x3046;&#x304C;&#x3044;&#x3059;&#x3044;</pr></trd>,
    <trd>&#x306E;&#x3069;&#x3073;&#x3053;</trd>
  </trdgrp>
  <trd lng="nl">huig</trd>
  <trd lng="pl">j&eogonek;zyczek podniebienny</trd>
  <trd lng="pt">&uacute;vula</trd>
  <trdgrp lng="ru">
    <trd>&c_ja;&c_z;&c_y;&c_ch;&c_o;&c_k;</trd>,
    <trd>&c_u;&c_v;&c_u;&c_l;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>uvula</trd>,
    <trd>&ccaron;ap&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ja;&c_z;&c_i;&c_ch;&c_o;&c_k;</trd>,
    <trd>&c_u;&c_v;&c_u;&c_l;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="uvul.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <uzo tip="fak">FON</uzo>
        <tld/>a ro
        <klr>(= ro produktata de la vibrado de la <tld/>o)</klr>
        <ref tip="vid" cel="kartav.0i">kartava</ref>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">uvular</trd>
  <trd lng="en">uvular</trd>
  <trd lng="ja">&#x53E3;&#x84CB;&#x5782;&#x306E;  <pr>&#x3053;&#x3046;&#x304C;&#x3044;&#x3059;&#x3044;&#x306E;</pr></trd>
  <trdgrp lng="ru">
    <trd>&c_ja;&c_z;&c_y;&c_ch;&c_k;&c_o;&c_v;&c_y;&c_j;</trd>
    <trd>&c_u;&c_v;&c_u;&c_l;&c_ja;&c_r;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ja;&c_z;&c_i;&c_ch;&c_k;&c_o;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_u;&c_v;&c_u;&c_l;&c_ja;&c_r;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

</art>
