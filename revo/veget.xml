<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>veget</rad>/i
</kap>
<drv mrk="veget.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="veget.0i.BIO">
    <dif>
      Plenumi la funkciojn, el kiuj konsistas la vivo de <tld/>a&jcirc;o:
      <ekz>
        &ccirc;io, kio <tld/>as en la naturo;
      </ekz>
      <ekz>
        la normala <tld/>ado de la arboj.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ib;</trd>,
      <trd>&c_v;&c_y;&c_r;&c_a;&c_s;&c_t;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="el">&phi;&ypsilon;&tau;&rho;&omega_ton;&ny;&omega;</trd>
    <trdgrp lng="hu">
      <trd>n&ouml;vekszik</trd>,
      <trd>teny&eacute;szik</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_i;&c_z;&c_r;&c_a;&c_s;&c_t;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="veget.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <uzo tip="klr">(pri homo)</uzo>
    <dif>
      Vivi senemocian, inertan vivon, kiel <tld/>a&jcirc;o:
      <ekz>
        en Parizo oni vivas, en provinco oni <tld/>as.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_g;&c_ib;&c_b;&c_je;&c_c;&c_mol;</trd>,
      <trd>&c_m;&c_a;&c_r;&c_n;&c_je;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">vegetieren</trd>
    <trd lng="el">&phi;&ypsilon;&tau;&omikron;&zeta;&omega;&omega_ton;</trd>
    <trd lng="fr">v&eacute;g&eacute;ter</trd>
    <trdgrp lng="hu">
      <trd>teng&odblac;dik</trd>,
      <trd>veget&aacute;l</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_z;&c_ja;&c_b;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="en">vegetate</trd>
  <trdgrp lng="nl">
    <trd>vegeteren</trd>,
    <trd>een <ind>plantenleven</ind> leiden</trd>
  </trdgrp>
  <trd lng="pt">vegetar</trd>
</drv>

<drv mrk="veget.0ajxo">
  <kap><tld/>a&jcirc;o<fnt>Z</fnt></kap>
  <snc mrk="veget.0ajxo.BIO">
    <tezrad fak="BOT"/>
    <dif>
      <ref tip="super" cel="viv.0ajxo.BIO">Viva&jcirc;o</ref> (<ref
      tip="super" cel="organi1.0o">organismo</ref>),      havanta la
      funkciojn de sinnutrado kaj      reproduktado; malkiel la <ref
      cel="animal.0o">animaloj</ref>,      pliparto da <tld/>a&jcirc;oj ne
      povas sin transporti, sed      kapablas sintezi organikajn
      substancojn:
      <ekz>
        <ind>&ccirc;ambra <tld/>a&jcirc;o</ind>
        <klr>(kiun oni tenas en &ccirc;ambro kiel ornamon)</klr>;
        <trd lng="fr">plante d'int&eacute;rieur</trd>
        <trd lng="hu">szoban&ouml;v&eacute;ny</trd>
        <trd lng="ru">&c_k;&c_o;&c_m;&c_n;&c_a;&c_t;&c_n;&c_o;&c_je; &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;</trd>
      </ekz>
      <ekz>
        <ind>kultura <tld/>a&jcirc;o</ind> <klr>(greno ktp)</klr>;
        <trd lng="hu">kult&uacute;rn&ouml;v&eacute;ny</trd>
        <trd lng="ru">&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_n;&c_o;&c_je; &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;</trd>       
          
      </ekz>
      <ekz>
        <ind>sova&gcirc;a <tld/>a&jcirc;o</ind>;
        <trd lng="hu">dudva</trd>        
        <trd lng="ru">&c_d;&c_i;&c_k;&c_o;&c_je; &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;</trd>
      </ekz>
      <ekz>
        &ccirc;ar simile al herbo ili rapide dehaki&gcirc;as,
        kaj kiel verda <tld/>a&jcirc;o ili forvelkas
        <fnt><bib>MT</bib>, <lok>&Psa; 37:2.</lok></fnt>;
      </ekz>
      <ekz>
        PIV informas, ke nomenklaturo estas terminaro de iu
        scienco a&ubreve; arto, en zoologio kaj botaniko speciale la
        nomaro de animaloj kaj <tld/>a&jcirc;oj
        <fnt><bib>TermKurs</bib>, <lok>&para; 4.2</lok></fnt>.
      </ekz>
      <ekz>
        ni parolu pri la <tld/>a&jcirc;oj &dash; &ccirc;u vi vidis viton?
        <fnt><bib>SatirRak</bib>, <lok>supersti&ccirc;o</lok></fnt>;
      </ekz>
      <ekz>
        por <tld/>a&jcirc;o a&ubreve; &scirc;tono, esti natura ne estas malfacile
        <fnt>
        <aut>&Scirc;unrju Suzuki, trad. Danjelo Gibbons</aut>,
        <lok><url ref="http://www.angelfire.com/mo/dhgibbons/#25"
        >Zena Menso, Komencanta Menso</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        vi estas <tld/>a&jcirc;oj, kies trunketoj, edukitaj en
        oran&gcirc;erioj, ne havas la forton, por kontra&ubreve;stari al ventoj
        kaj fulmotondroj!
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kresk.0ajxo.vegetajxo">kreska&jcirc;o <sncref/></ref>,
      <ref cel="plant.0o.vegetajxo">planto <sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Gew&auml;chs</trd>,
      <trd>Pflanze</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;</trd>
  <trd lng="el">&phi;&ypsilon;&tau;&omikron_ton;</trd>
  <trd lng="fr">v&eacute;g&eacute;tal <klr>(un)</klr></trd>
  <trd lng="hu">n&ouml;v&eacute;ny</trd>  
  <trd lng="nl">plant</trd>
  <trd lng="pt">vegetal</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="veget.0ajxa">
  <kap><tld/>a&jcirc;a</kap>
  <snc>
    <dif>
      Rilata al <tld/>a&jcirc;oj:
      <ekz>
        la <tld/>a&jcirc;a <ref cel="regn.0o.BIO">regno</ref>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        e&ccirc; la pereo en la batalo estas pli agrabla ol tiu velka,
        dormema
        kaj <tld/>a vivo en peza enfermita aero.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_n;&c_y;</trd>
  <trd lng="el">&phi;&ypsilon;&tau;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trdgrp lng="fr">
    <trd>v&eacute;g&eacute;tal <klr>(adj.)</klr></trd>,
    <trd>v&eacute;g&eacute;tatif</trd>
  </trdgrp>
  <trd lng="hu">n&ouml;v&eacute;nyi</trd>  
  <trd lng="nl">plantaardig</trd>
  <trd lng="pt">vegetal</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_t;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="veget.0ajxaro">
  <kap><tld/>a&jcirc;aro</kap>
  <snc>
    <dif>
      Tuto de la <tld/>a&jcirc;oj de la mondo a&ubreve; regiono:
      <ekz>
        la parko trovi&gcirc;as je 12 km for de Ushuaia kaj havas
        <tld/>a&jcirc;aron, kiu integras la sektoron pli sudan ol
        la magalhana distrikto
        <fnt>
        <aut>Jos&eacute; de N. MARQUES</aut>,
        <vrk>Argentino: Voja&gcirc;o al Ushuaia</vrk>, <bib>Monato</bib>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_f;&c_l;&c_jo;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="fr">v&eacute;g&eacute;tation</trd>
  <trdgrp lng="hu">
    <trd>veget&aacute;ci&oacute;</trd>,
    <trd>fl&oacute;ra</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vegetatie</trd>,
    <trd>flora</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>flora</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_t;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_f;&c_l;&c_o;&c_r;&c_a;</trd>
  </trdgrp>  
</drv>
</art>
