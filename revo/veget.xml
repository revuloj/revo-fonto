<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: veget.xml,v 1.40 2024/08/03 15:28:25 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>veget</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="veget.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="veget.0i.BIO">
    <dif>
      Plenumi la funkciojn, el kiuj konsistas la vivo de <tld/>a&jcirc;o:
      <ekz>
        &ccirc;io, kio <tld/>as en la naturo;
      </ekz>
      <ekz>
        du-tri <tld/>antaj pomarboj kaj vera densejo de siringoj
        <fnt>
          <bib>Tor</bib>
          <lok>I.</lok>
        </fnt>;
      </ekz>
      <ekz>
        pro la malvarma klimato vito ne <tld/>as en Irlando
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006878.html">Dommastrumo:
          &Gcirc;ardenumi</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kresk.0i.vegetajxo">kreski<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ib;</trd>,
      <trd>&c_v;&c_y;&c_r;&c_a;&c_s;&c_t;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>wachsen <klr>(vorkommen)</klr></trd>,
      <trd>vegetieren</trd>
    </trdgrp>
    <trd lng="el">&phi;&ypsilon;&tau;&rho;&omega_ton;&ny;&omega;</trd>
    <trd lng="fa">&a_r;&a_w;&f_y;&f_y;&a_d;&a_n;</trd>
    <trdgrp lng="hu">
      <trd>n&ouml;vekszik</trd>,
      <trd>teny&eacute;szik</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_i;&c_z;&c_r;&c_a;&c_s;&c_t;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="veget.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <uzo tip="klr">(pri homo k.a.)</uzo>
    <dif>
      Vivi senemocian, inertan vivon, kiel <tld/>a&jcirc;o:
      <ekz>
        en Parizo oni vivas, en provinco oni <tld/>as;
      </ekz>
      <ekz>
        <klr>[min]</klr> lasu <tld/>i en la sengloreco
        <fnt>
          <bib>VaK</bib>
          <lok>Deksesa &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la diritaj asocioj apena&ubreve; <tld/>is, baraktante kontra&ubreve; organizaj, financaj kaj
        internaj malfacila&jcirc;oj
        <fnt>
          <bib>Kon11</bib>
          <lok>Kiel naski&gcirc;is la nuna TEJO (G. C. Fighiera, Honora Membro de UEA)</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum multaj E-organizoj <tld/>as, la brazila Esperanto-movado bolegas
        <fnt>
          <bib>Kon11</bib>
          <lok>Esperanta kinarto vivas malgra&ubreve; la esperantistoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        bona fil' de bona sklavo &#x2AFD; modeste en angul' <tld/>is
        <fnt>
          <bib>LSB</bib>
          <lok>Tre Respekte</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_g;&c_ib;&c_b;&c_je;&c_c;&c_mol;</trd>,
      <trd>&c_m;&c_a;&c_r;&c_n;&c_je;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">vor sich hin <ind>vegetieren</ind></trd>
    <trd lng="el">&phi;&ypsilon;&tau;&omikron;&zeta;&omega;&omega_ton;</trd>
    <trdgrp lng="fa">
      <trd>&f_g;&f_y;&a_A;&a_h;&#x200C;&a_w;&a_A;&a_r; &a_z;&f_y;&a_s;&a_t;&a_n;</trd>,
      <trd>&a_kh;&a_w;&a_r;&a_d;&a_n; &a_w; &a_kh;&a_w;&a_A;&a_b;&f_y;&a_d;&a_n;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>teng&odblac;dik</trd>,
      <trd>veget&aacute;l</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_z;&c_ja;&c_b;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <refgrp tip="super">
    <ref cel="ekzist.0i">ekzisti</ref>,
    <ref cel="viv.0i">vivi</ref>
  </refgrp>
  <trd lng="ca">vegetar</trd>
  <trd lng="cs">r&uring;st <klr>(jen o rostlin&aacute;ch)</klr></trd>
  <trd lng="en">vegetate</trd>
  <trd lng="es">vegetar</trd>
  <trd lng="fr">v&eacute;g&eacute;ter</trd>
  <trdgrp lng="ja">
    <trd>&#x751F;&#x80B2;&#12377;&#x308B;  <pr>&#x305B;&#12356;&#12356;&#x304F;&#12377;&#x308B;</pr></trd>,
    <trd>&#x751F;&#12356;&#33538;&#x308B;  <pr>&#x304A;&#12356;&#12375;&#12370;&#x308B;</pr></trd>,
    <trd>&#x7D30;&#12293;&#12392;&#x66AE;&#12425;&#12377;  <pr>&#12373;&#12356;&#12373;&#12356;&#12392;&#x304F;&#12425;&#12377;</pr></trd>,
    <trd>&#28961;&#x70BA;&#x306B;&#x904E;&#12372;&#12377;  <pr>&#12416;&#12356;&#x306B;&#12377;&#12372;&#12377;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vegeteren</trd>,
    <trd>een <ind>plantenleven</ind> leiden</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wegetowa&cacute;</trd>,
    <trd>wzrasta&cacute;<klr>  (o ro&sacute;linach)</klr></trd>
  </trdgrp>
  <trd lng="pt">vegetar</trd>
  <trd lng="sk">r&aacute;s&tcaron; <klr>(rastliny)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_r;&c_o;&c_s;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_v;&c_i;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>(&c_l;&c_je;&c_d;&c_v;&c_je;) &c_zh;&c_i;&c_v;&c_o;&c_t;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_n;&c_i;&c_d;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_l;&c_i;&c_d;&c_a;&c_r;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#38263;&#x8D05;&#x75A3; <pr>zh&#x1CE;ngzhu&#xEC;y&#xF3;u</pr></trd>,
    <trd>&#x957F;&#x8D58;&#x75A3; <pr>zh&#x1CE;ngzhu&#xEC;y&#xF3;u</pr></trd>
  </trdgrp>
</drv>

<drv mrk="veget.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="veget.0ajxo.BIO">
    <uzo tip="fak">BOT</uzo>
    <tezrad fak="BOT"/>
    <dif>
      E&ubreve;kariota <ref tip="super" cel="viv.0ajxo.BIO">viva&jcirc;o</ref>,
      kies &ccirc;eloj havas firman vandon el celulozo a&ubreve; kitino, ofte klorofilon
      por a&ubreve;totrofa nutri&gcirc;o, sed kiuj, alie ol <ref cel="animal.0o">animaloj</ref>,
      ordinare ne povas transporti sin:
      <ekz>
        la bestoj kaj <tld/>a&jcirc;oj vivas, la mineraloj ne vivas
        <fnt><bib>VdE</bib></fnt>;
      </ekz>
      <ekz>
        <ind>grimpanta <tld/>a&jcirc;o</ind>
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>;
        <trd lng="de">Kletterpflanze</trd>
      </ekz><ekz>
         figuroj de bestoj (falko, hundo, lupo, skorpio ktp) a&ubreve; de
         <tld/>a&jcirc;oj (kano, palmo, sikomoro ktp)
         <fnt><aut>Lanti</aut>: <vrk>Naciismo</vrk>.
           <lok>SAT: Lepsiko, 1930. 1:1.</lok></fnt>;
      </ekz><ekz>
        <ind>kultura <tld/>a&jcirc;o</ind> <klr>(greno ktp)</klr>;
        <trd lng="ca"><ind>planta</ind> de conreu</trd>
        <trd lng="hu">kult&uacute;rn&ouml;v&eacute;ny</trd>
        <trd lng="ru">&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_n;&c_o;&c_je; &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;</trd>
      </ekz><ekz>
        <ind>sova&gcirc;a <tld/>a&jcirc;o</ind>;
        <trd lng="ca">mala <ind>herba</ind></trd>
        <trd lng="hu">dudva</trd>
        <trd lng="ru">&c_d;&c_i;&c_k;&c_o;&c_je; &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;</trd>
      </ekz>
      <ekz>
        kanabo &ndash; populara <ind>droga <tld/>a&jcirc;o</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Antanas Grincevi&ccaron;ius</aut>
          <vrk><url ref="https://www.monato.be/2003/007879.html">&Ccirc;okolada konflikto</url></vrk>
          <lok>2003</lok>
        </fnt>;
        <trd lng="de">Drogenpflanze</trd>
      </ekz><ekz>
        &ccirc;ar simile al herbo ili rapide dehaki&gcirc;as,
        kaj kiel verda <tld/>a&jcirc;o ili forvelkas
        <fnt><bib>MT</bib>, <lok>&Psa; 37:2.</lok></fnt>;
      </ekz><ekz>
        nomenklaturo estas <klr>[&#8230;]</klr> en zoologio kaj
        botaniko speciale la nomaro de animaloj kaj <tld/>a&jcirc;oj
        <fnt><bib>TermKurs</bib>, <lok>&para; 4.2</lok></fnt>.
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        vi estas <tld/>a&jcirc;oj, kies trunketoj, edukitaj en
        oran&gcirc;erioj, ne havas la forton, por kontra&ubreve;stari al ventoj
        kaj fulmotondroj!
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kresk.0ajxo.vegetajxo">kreska&jcirc;o<sncref/></ref>,
      <ref cel="plant1.0o.vegetajxo">planto</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Gew&auml;chs</trd>,
      <trd>Pflanze</trd>
    </trdgrp>
    <trd lng="es">vegetal</trd>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;</trd>
  <trd lng="bo">&t_ra;&t_tsata;&t_gigu;&t_tsheg;&t_sha;&t_gigu;&t_nga;&t_tsheg;</trd>
  <trdgrp lng="ca">
    <trd>vegetal</trd>,
    <trd>planta</trd>
  </trdgrp>
  <trd lng="el">&phi;&ypsilon;&tau;&omikron_ton;</trd>
  <trd lng="en">plant</trd>
  <trdgrp lng="fa">
    <trd>&f_g;&f_y;&a_A;&a_h;</trd>,
    <trd>&a_r;&a_s;&a_t;&a_n;&f_y;</trd>,
    <trd>&a_n;&a_b;&a_A;&a_t;</trd>
  </trdgrp>
  <trd lng="fr">v&eacute;g&eacute;tal <klr>(subst.)</klr></trd>
  <trd lng="hu">n&ouml;v&eacute;ny</trd>
  <trd lng="ja">&#x690D;&#29289;  <pr>&#12375;&#12423;&#x304F;&#12406;&#12388;</pr></trd>
  <trd lng="nl">plant</trd>
  <trd lng="pl">ro&sacute;lina</trd>
  <trd lng="pt">vegetal</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="uk">&c_r;&c_o;&c_s;&c_l;&c_i;&c_n;&c_a;</trd>
  <trd lng="zh">&#x690D;&#29289; <pr>zh&#xED;w&#xF9;</pr></trd>
</drv>

<drv mrk="veget.0ajxa">
  <kap><tld/>a&jcirc;a</kap>
  <snc mrk="veget.0ajxa.BOT">
    <dif>
      Rilata al <tld/>a&jcirc;oj:
      <ekz>
        la <tld/>a&jcirc;a <ref cel="regn.0o.BIO">regno</ref>;
      </ekz>
      <ekz>
        &ccirc;u la vegana (pure <tld/>a&jcirc;a) dieto estas bongusta kaj sufi&ccirc;a por la korpaj
        bezonoj
        <fnt>
          <bib>Kon11</bib>
          <lok>&ndash; Reagoj &ndash;</lok>
        </fnt>?
      </ekz>
      <ekz>
        braceletoj faritaj el <tld/>a&jcirc;aj materialoj
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikoloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel <tld/>a&jcirc;a tapi&scirc;o tie kreskis nur ranunkoloj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006878.html">Dommastrumo:
          &Gcirc;ardenumi</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        dum la ses jaroj de la unutona kaj preska&ubreve; <tld/>a&jcirc;a ekzistado, kiun Fernando
        pasigis en <nom>Hongkong</nom> li vivis nur per siaj memoroj
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekkvina</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_n;&c_y;</trd>
  <trd lng="ca">vegetal<klr> (adj.)</klr></trd>
  <trdgrp lng="de">
    <trd>pflanzlich</trd>,
    <trd>Pflanzen-</trd>,
    <trd>vegetativ</trd>
  </trdgrp>
  <trd lng="el">&phi;&ypsilon;&tau;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">vegetable</trd>
  <trdgrp lng="fa">
    <trd>&f_g;&f_y;&a_A;&a_h;&f_y;</trd>,
    <trd>&a_n;&a_b;&a_A;&a_t;&f_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>v&eacute;g&eacute;tal <klr>(adj.)</klr></trd>,
    <trd>v&eacute;g&eacute;tatif</trd>
  </trdgrp>
  <trd lng="hu">n&ouml;v&eacute;nyi</trd>
  <trdgrp lng="ja">
    <trd>&#x690D;&#29289;&#x306E;  <pr>&#12375;&#12423;&#x304F;&#12406;&#12388;&#x306E;</pr></trd>,
    <trd>&#x690D;&#29289;&#24615;&#x306E;  <pr>&#12375;&#12423;&#x304F;&#12406;&#12388;&#x305B;&#12356;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">plantaardig</trd>
  <trd lng="pl">warzywny</trd>
  <trd lng="pt">vegetal</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_t;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trd lng="zh">&#x690D;&#29289;&#24615; <pr>zh&#xED;w&#xF9;x&#xEC;ng</pr></trd>
</drv>

<drv mrk="veget.0ajxaro">
  <kap><tld/>a&jcirc;aro</kap>
  <snc mrk="veget.0ajxaro.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Tuto de la <tld/>a&jcirc;oj de regiono a&ubreve; de la mondo:
      <ekz>
        la <tld/>a&jcirc;aro de la Insuloj de <nom>Santa Cruz</nom> estas
        malalta&jcirc;a pluvarbaro
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Vanuatuaj_pluvarbaroj">
          Vanuatuaj pluvarbaroj</url>, 2011-12-29</lok>
        </fnt>;
      </ekz>
      <ekz>
        sciencistoj <klr>[&#8230;]</klr> studas la rilatojn inter la <tld/>a&jcirc;aro, fa&ubreve;no kaj la
        homoj de tiu ankora&ubreve; ne esplorata regiono
        <fnt>
          <bib>Kon11</bib>
          <lok>Intervjuo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;atata turisma alvenpunkto <klr>[pro la]</klr> belega maro kaj ri&ccirc;a <tld/>a&jcirc;aro
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.be/2011/010261.php">Estis iam alfabeto ...</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_f;&c_l;&c_jo;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">vegetaci&oacute;</trd>
  <trd lng="de">Vegetation</trd>
  <trdgrp lng="de">
    <trd>Pflanzenwelt</trd>,
    <trd>Vegetation</trd>,
    <trd>Flora</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>vegetation</trd>,
    <trd>flora</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&f_g;&f_y;&a_A;&a_h;&a_A;&a_n;</trd>,
    <trd>&a_n;&a_b;&a_A;&a_t;&a_A;&a_t;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>flore</trd>,
    <trd>v&eacute;g&eacute;tation</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>veget&aacute;ci&oacute;</trd>,
    <trd>fl&oacute;ra</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x690D;&#29289;&#x76F8;  <pr>&#12375;&#12423;&#x304F;&#12406;&#12388;&#x305D;&#12358;</pr></trd>,
    <trd>&#x30D5;&#x30ED;&#x30E9;</trd>,
    <trd/>,
    <trd>&#x690D;&#x751F;  <pr>&#12375;&#12423;&#x304F;&#x305B;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vegetatie</trd>,
    <trd>flora</trd>
  </trdgrp>
  <trd lng="pl">&sacute;wiat<ind> ro&sacute;linny</ind></trd>
  <trdgrp lng="pt">
   <trd>flora</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_t;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_f;&c_l;&c_o;&c_r;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x690D;&#x88AB; <pr>zh&#xED;b&#xE8;i</pr></trd>,
    <trd>&#33609;&#26408; <pr>c&#x1CE;om&#xF9;</pr></trd>,
    <trd>&#x690D;&#29289;&#x7FA4; <pr>zh&#xED;w&#xF9;q&#xFA;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="veget.cxambra0ajxo">
  <kap>&ccirc;ambra <tld/>a&jcirc;o</kap>
  <snc mrk="veget.cxambra0ajxo.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      <tld lit="V"/>a&jcirc;o kiun oni tenas en &ccirc;ambro kiel ornamon.
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_t;&c_n;&c_ja;&c_ja; <ind>&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;</ind></trd>
  <trd lng="ca"><ind>planta</ind> d'interior</trd>
  <trd lng="de">Zimmerpflanze</trd>
  <trd lng="fr">plante d'int&eacute;rieur</trd>
  <trd lng="hu">szoban&ouml;v&eacute;ny</trd>
  <trd lng="pl">ro&sacute;liny pokojowe</trd>
