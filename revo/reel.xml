<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>reel</rad>o<fnt>SPIV</fnt></kap>
    <drv mrk="reel.0o">
      <kap><tld/>o</kap>
      <snc mrk="reel.0o.mat">
        <uzo tip="fak">MAT</uzo>
        <dif>
          &Ccirc;ia <ref tip="super" cel="nombr.0o.mat">nombro</ref>
          prezentebla per sumo de entjero kaj
          <ref cel="frakci.0o.pozicia">pozicia frakcio</ref>:
          <ekz><frm>&#x221a;2</frm> <klr>(la kvadrata radiko de 2)</klr>
              estas <tld/>o neraciona;
          </ekz><ekz>
            la nombro <frm>&#x3c0;</frm> estas <tld/>o kies prezento per
            senfina <ref cel="dek.0uma.frakcio">dekuma frakcio</ref>
            komenci&gcirc;as tiel: 3,14159...
          </ekz>
        </dif>
        <rim>
          PIV proponas por tiu senco <ctl>reala nombro</ctl> &dash; &gcirc;i
          ne estas uzata; PIV-2 havos <ctl>reelo</ctl>n.
        </rim>
        <ref tip="sub" cel="raci5.0o">raciono</ref>
      <trdgrp lng="ru"><trd>&#x432;&#x435;&#x449;&#x435;&#x441;&#x442;&#x432;&#x435;&#x43d;&#x43d;&#x43e;&#x435; &#x447;&#x438;&#x441;&#x43b;&#x43e;</trd>,
        <trd>&#x434;&#x435;&#x439;&#x441;&#x442;&#x432;&#x438;&#x442;&#x435;&#x43b;&#x44c;&#x43d;&#x43e;&#x435; &#x447;&#x438;&#x441;&#x43b;&#x43e;</trd></trdgrp>
      </snc>
      <snc mrk="reel.0o.komp">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          En programlingvoj, <ref cel="racio5.0o">raciono</ref> uzata
          por aproksimi reelan grandon.
        </dif>
        <trd lng="ru">&#x432;&#x435;&#x449;&#x435;&#x441;&#x442;&#x432;&#x435;&#x43d;&#x43d;&#x43e;&#x435; &#x447;&#x438;&#x441;&#x43b;&#x43e;</trd>
      </snc>
      <trd lng="en">real number</trd>
      <trd lng="fr"><klr>(nombre)</klr> r&eacute;el</trd>
    </drv>
  </art>

