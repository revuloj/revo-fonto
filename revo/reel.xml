<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>reel</rad>/o<fnt>SPIV</fnt></kap>
<drv mrk="reel.0o">
  <kap><tld/>o</kap>
  <snc mrk="reel.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <dif>
      &Ccirc;ia <ref tip="super" cel="nombr.0o.MAT">nombro</ref>
      prezentebla per sumo de entjero kaj
      <ref cel="frakci.0o.pozicia">pozicia frakcio</ref>:
      <ekz>
        <frm>&#x221a;2</frm> <klr>(la kvadrata radiko de 2)</klr>
        estas <tld/>o neraciona;
      </ekz>
      <ekz>
        la nombro <frm>&#x3c0;</frm> estas <tld/>o, kies prezento per
        senfina <ref cel="dek.0uma.frakcio">dekuma frakcio</ref>
        komenci&gcirc;as tiel: 3,14159...
      </ekz>
    </dif>
    <rim>
      La unua eldono de PIV havis por tiu senco <ctl>reala nombro</ctl>, sed
      <ctl>reelo</ctl> anstata&ubreve;is &gcirc;in en SPIV.
    </rim>
    <ref tip="sub" cel="racio5.0o">raciono</ref>
    <trd lng="en">real <ind>number</ind></trd>
    <trd lng="de">reelle <ind>Zahl</ind></trd>
    <trdgrp lng="fr">
      <trd>r&eacute;&eacute;l<klr tip="ind"> (nombre)</klr></trd>,
      <trd>nombre r&eacute;el</trd>
    </trdgrp>
    <trd lng="nl">re&euml;l <ind>getal</ind></trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_je;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;&c_je;
      <ind>&c_ch;&c_i;&c_s;&c_l;&c_o;</ind></trd>,
      <trd>&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je;
      <ind>&c_ch;&c_i;&c_s;&c_l;&c_o;</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="reel.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      En programlingvoj, <ref cel="racio5.0o">raciono</ref> uzata
      por aproksimi reelan grandon.
    </dif>
    <ref tip="super" cel="tip.datum0o">datumtipo</ref>
    <trd lng="fr">r&eacute;el<klr tip="ind"> (subst., informatique)</klr></trd>
    <trd
    lng="ru">&c_v;&c_je;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;&c_je;
    &c_ch;&c_i;&c_s;&c_l;&c_o;</trd>
  </snc>
</drv>
<drv mrk="reel.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <uzo tip="fak">KOMP</uzo>
  <snc>
    <dif>
      Rilata al <tld/>o
      <sncref ref="reel.0o.MAT"/> a&ubreve; <sncref ref="reel.0o.KOMP"/>;
      estanta <tld/>o:
      <ekz>
        <tld/>a nombro <klr>(<tld/>o)</klr>, variablo, funkcio <klr>(kies
        valoroj estas <tld/>aj)</klr>, rekto <klr>(rekto en la kompleksa
        ebeno, reprezentanta la <tld/>aj nombroj)</klr>, <ref
        cel="spac.vektora0o">vektora spaco</ref> <klr>(super la korpo de
        <tld/>oj)</klr>;
      </ekz>
      <ekz>
        <ref cel="part.reela0o">reela parto</ref> de komplekso.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">r&eacute;el<klr tip="ind"> (adj., math., informatique)</klr></trd>
<trd lng="nl">re&euml;l</trd>
    </drv>
</art>
