<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>alumet</rad>/o
</kap>
<drv mrk="alumet.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Ligna peceto &scirc;mirita per fosforo a&ubreve; speciala substanco, kiu
      ekbrulas per frotado.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_p;&c_a;&c_l;&c_k;&c_a;</trd>
  <trd lng="cs">z&aacute;palka</trd>
  <trdgrp lng="de">
    <trd>Streichholz</trd>,
    <trd>Z&uuml;ndholz</trd>,
    <trd>Schwefelholz [arkaika]</trd>
  </trdgrp>
  <trd lng="en">match</trd>
  <trd lng="fr">allumette</trd>
  <trd lng="nl">lucifer</trd>
  <trd lng="ru">&c_s;&c_p;&c_i;&c_ch;&c_k;&c_a;</trd>
  <trd lng="sv">t&auml;ndsticka</trd>
</drv>
</art>
