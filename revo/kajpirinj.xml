<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kajpirinj.xml,v 1.5 2013/08/01 18:10:12 revo Exp $">
<kap>
  <rad>kajpirinj</rad>/o
</kap>
<drv mrk="kajpirinj.0o">
  <kap><tld/>o</kap>
  <snc mrk="kajpirinj.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Brazildevena alkohola trinka&jcirc;o el sukerkana brando, sukero, 
      glacio kaj pistitaj limeopecoj:
      <ekz>
        oni denove paroladis pri Brazilo kaj pri &gcirc;iaj kulturo kaj 
        lingvo, kaj e&ccirc; povis drinki <tld/>on
        <fnt>
          <aut>-</aut>,
          <vrk>Prelego</vrk>,
          <lok><url
          ref="http://esperanto.toulouse.free.fr/nova/kro/j2007/krokodil200703.pdf">
          La Krokodil&#8217;, 2007-03</url></lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_k;&c_a;&c_j;&c_p;&c_i;&c_r;&c_i;&c_n;&c_ja;</trd>
  <trd lng="de">Caipirinha</trd>
  <trd lng="fr">caipirinha</trd>
  <trd lng="nl">caipirinha</trd>
  <trd lng="pl">caipirinha</trd>
</drv>
</art>

