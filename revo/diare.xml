<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>diare</rad>/o
</kap>
<drv mrk="diare.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="laks.0o">Lakso</ref>:
      <ekz>
        <klr>[&ccirc;e]</klr> netipa pne&ubreve;monio <klr>[&#8230;]</klr>
        post inkubacia periodo <klr>[&#8230;]</klr> &ccirc;e malsanuloj aperas gripsimilaj simptomoj: 
        la temperaturo plialti&gcirc;as &gcirc;is 38 gradoj celsiaj kaj jen venas nazkataro, kap- 
        kaj muskol-doloroj, seka tusado ktp., &ccirc;e multaj aperas <tld/>o, kio ne estas karakterize gripa
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="http://www.monato.net/2005/008721.php">De kie
          venis kaj kial &ccirc;esis netipa pne&ubreve;monio?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en gravaj kazoj pro <tld/>o la &hcirc;olera malsanulo eligas dum unu tagnokto
        kun feka&jcirc;o kaj voma&jcirc;o &gcirc;is 30 litrojn da likva&jcirc;o, entenanta multajn milionojn da &hcirc;oleraj bakterioj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="http://www.monato.net/2010/010071.php">Antikva, sed ankora&ubreve; malica gasto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ja;&c_k;&c_s;&c_a;</trd>,
    <trd>&c_d;&c_y;&c_ja;&c_r;&c_e;&c_ja;</trd>
  </trdgrp>
  <trd lng="ca">diarrea</trd>
  <trdgrp lng="cs">
    <trd>diar&eacute;</trd>,
    <trd>pr&uring;jem</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Diarrh&ouml;</trd>,
     <trd>Durchfall</trd>
  </trdgrp>
  <trd lng="en">diarrhea</trd>
  <trd lng="es">diarrea</trd>
  <trd lng="fr">diarrh&eacute;e</trd>
  <trdgrp lng="hu">
     <trd>hasmen&eacute;s</trd>,
     <trd>diar&eacute;</trd>,
     <trd>diarrhoea</trd>
  </trdgrp>
  <trd lng="nl">diarree</trd>
  <trdgrp lng="pl">
    <trd>biegunka</trd>,
    <trd>rozwolnienie<klr> (pot.)</klr></trd>
  </trdgrp>
  <trd lng="pt">diarr&eacute;ia</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_i;&c_a;&c_r;&c_je;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_n;&c_o;&c_s;</trd>
  </trdgrp>
  <trd lng="sk">hna&ccaron;ka</trd>
</drv>
</art>
