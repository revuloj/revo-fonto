<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>strigl</rad>/i
</kap>
<drv mrk="strigl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Brosi kaj purigi ha&ubreve;ton kaj harojn de &ccirc;evalo a&ubreve; hejma besto:
      <ekz>ili venu &ccirc;i tien <klr>...</klr>, <tld/>u al mi la barbon
      <fnt><bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 32a, p. 580a</lok></fnt></ekz>
    </dif>
  </snc>
  <trd lng="de">striegeln</trd>
  <trd lng="fr">&eacute;triller</trd>
  <trdgrp lng="hu">
    <trd>csutakol</trd>,
    <trd>vakar<klr tip="ind">(csutakol</klr>)</trd>
  </trdgrp>
  <trd lng="nl">roskammen</trd>
  <trd lng="ru">&c_ch;&c_i;&c_s;&c_t;&c_i;&c_t;&c_mol; &c_s;&c_k;&c_r;&c_je;&c_b;&c_n;&c_i;&c_c;&c_je;&c_j;</trd>
</drv>

<drv mrk="strigl.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Speciala ilo por <tld/>i konsistanta el kelkaj paralelaj metalaj
      kombiloj.
    </dif>
  </snc>
  <trd lng="de">Striegel</trd>
  <trd lng="fr">&eacute;trille</trd>
  <trdgrp lng="hu">
    <trd>csutak</trd>,
    <trd>l&oacute;vakar&oacute;</trd>
  </trdgrp>
  <trd lng="nl">roskam</trd>
  <trd lng="ru">&c_s;&c_k;&c_r;&c_je;&c_b;&c_n;&c_i;&c_c;&c_a;</trd>
</drv>
</art>
