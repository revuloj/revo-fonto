<art mrk="$Id: karoseri.xml,v 1.10 2023/09/01 13:58:56 revo Exp $">
<kap>
  <ofc>9</ofc><rad>karoseri</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="karoseri.0o">
  <kap><tld/>o</kap>
  <snc mrk="karoseri.0o.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      La supra parto de <ref tip="malprt" cel="vetur.0ilo">veturilo</ref> muntita sur la 
      <ref tip="vid" cel="cxasi.0o">&ccirc;asio</ref>:
      <ekz>
        la <tld/>o ricevis novan lakon
        <fnt>
          <aut>S. Mar&ccirc;ek</aut>,
          <vrk>Vi trovos...</vrk>,
          <bib>Esperanto</bib>,
          <lok><url ref="">
          2002:6 (1148)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;ian <tld/>on jam korodis la tiea mara vento
        <fnt>
          <aut>H. Fidilalao</aut>,
          <vrk>A&ubreve;tokomerco pli strikte regulita</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2007a, numero 1a, p. 12a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o plena de skrapa&jcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="&Monato;/2011/010455.php">Memsanigaj materialoj?</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <ref cel="fram.0o.TEK">framo</ref> kaj <tld/>o 
        de la tripersona veturilo estas konstruitaj el tre malpeza
        miksita karbonfibro
        <fnt>
          <bib>Monato</bib>
          <aut>Tam&aacute;s Slez&aacute;k</aut>
          <vrk><url ref="&Monato;/2008/009631.php">Pli ol a&ubreve;to: perspektivo kaj sinteno</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_k;&c_a;&c_r;&c_o;&c_s;&c_je;&c_r;&c_i;&c_ja;</trd>
  <trd lng="br">karro&ntilde;sadur</trd>
  <trd lng="ca">carrosseria</trd>
  <trd lng="cs">karos&eacute;rie</trd>
  <trd lng="de">Karosserie</trd>
  <trd lng="en">coachwork</trd>
  <trd lng="fa">&a_b;&a_d;&a_n;&a_h;</trd>
  <trd lng="fr">carrosserie</trd>
  <trdgrp lng="hu">
    <trd>karossz&eacute;ria</trd>,
    <trd>kocsiszekr&eacute;ny</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>carrozzeria</trd>,
    <trd>scocca</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8ECA;&#x4F53;  [<ind>&#x3057;&#x3083;&#x305F;&#x3044;</ind>]</trd>,
    <trd>&#x30DC;&#x30C7;&#x30A3;&#x30FC;</trd>
  </trdgrp>
  <trd lng="nl">carosserie</trd>
  <trd lng="pl">karoseria</trd>
  <trd lng="sk">karos&eacute;ria</trd>
  <trd lng="uk">&#x43A;&#x443;&#x437;&#x43E;&#x432;</trd>
</drv>
