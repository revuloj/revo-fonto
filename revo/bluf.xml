<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>bluf</rad>/i<fnt>Z</fnt>
</kap>
<drv mrk="bluf.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Fanfaroni por trompi, &ccirc;arlatani:
      <ekz>
        &ccirc;io estas nur <tld/>o
        <fnt>(<aut>Zamenhof</aut>: <vrk>Fabeloj de Andersen,
        Ne&gcirc;a re&gcirc;ino</vrk>)</fnt>.
      </ekz> 
    </dif>
    <refgrp tip="sin">
      <ref cel="blag.0i">blagi</ref>,
      <ref cel="bombas.0o">bombasto</ref>,
      <ref cel="emfaz.0i">emfazi</ref>,
      <ref cel="mistif">mistifiki</ref>,
      <ref cel="blind">brilblindigi</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="polv.0o">polvo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>bluffen</trd>,
    <trd>vorspiegeln</trd>,
    <trd>vort&auml;uschen</trd>
  </trdgrp>
</drv>
</art>
<!--
$Log$
-->
</vortaro>