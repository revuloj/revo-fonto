<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bluf.xml,v 1.25 2023/10/14 08:39:48 revo Exp $">
<kap>
  <ofc>9</ofc>
  <rad>bluf</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="bluf.0i">
  <kap><ofc>9</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="sxajn.0igi">&Scirc;ajnigi</ref> sin supera al kontra&ubreve;ulo por konduki tiun al
      eraro:
      <!-- Fanfaroni por trompi, &ccirc;arlatani -->
      <ekz>
        &ccirc;io estas nur <ind><tld/>o</ind>
        <fnt><bib>Fab2</bib>, <lok>Ne&gcirc;a re&gcirc;ino</lok></fnt>;
        <trd lng="be">&c_b;&c_l;&c_je;&c_f;</trd>
        <trd lng="de">Bluff</trd>
        <trdgrp lng="fa">
          <trd>&a_kh;&a_A;&a_l;&f_y;&zwnj;&a_b;&a_n;&a_d;&f_y;</trd>,
          <trd>&a_b;&a_l;&a_w;&a_f;</trd>
        </trdgrp>
        <trd lng="fr">bluff</trd>
      </ekz>
      <ekz>
        <klr>[reklamante]</klr> &leftquot;tujan kompreneblecon&rightquot;, <klr>[&#x2026;]</klr> kelkaj
        pse&ubreve;dosciencaj fabrikantoj de novaj projektoj <tld/>as personojn jam
        parolantajn plurajn okcidente&ubreve;ropajn lingvojn
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne timu lian minacon, &ccirc;ar li <tld/>as
        <fnt><bib>OA9</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="blag.0i">blagi</ref>,
      <ref cel="bombas.0o">bombasto</ref>,
      <ref cel="blind">brilblindigi</ref>,
      <ref cel="emfaz.0i">emfazi</ref>,
      <ref cel="mistifik.0i">mistifiki</ref>,
      <ref cel="tromp.0i">trompi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_l;&c_je;&c_f;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_b;&c_l;&c_malmol;&c_f;&c_i;&c_r;&c_a;&c_m;</trd>
  <trdgrp lng="de">
    <trd>bluffen</trd>,
    <trd>vorspiegeln</trd>,
    <trd>vort&auml;uschen</trd>
  </trdgrp>
  <trd lng="en">bluff</trd>
  <trdgrp lng="es">
    <trd>bluf</trd>,
    <trd>embaucar</trd>,
    <trd>embuste</trd>,
    <trd>marcarse un farol</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_b;&a_l;&a_w;&a_f; &a_z;&a_d;&a_n;</trd>,
    <trd>&a_kh;&a_A;&a_l;&f_y; &a_b;&a_s;&a_t;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bluffer</trd>,
    <trd>leurrer</trd>,
    <trd>tromper <klr tip="ind">(bluffer)</klr></trd>
  </trdgrp>
  <trd lng="he">&lamed;&shin;&qof;&resh;</trd>
  <trdgrp lng="hu">
    <trd>bl&ouml;ff&ouml;l</trd>,
    <trd>l&oacute;d&iacute;t</trd>
  </trdgrp>
  <trd lng="it">bluffare</trd>
  <trdgrp lng="ja">
    <trd>&#x306F;&#x3063;&#x305F;&#x308A;&#x3092;&#x304B;&#x3051;&#x308B;</trd>,
    <trd>&#x865A;&#x52E2;&#x3092;&#x5F35;&#x3063;&#x3066;&#x3060;&#x307E;&#x3059;  <pr>&#x304D;&#x3087;&#x305B;&#x3044;&#x3092;&#x306F;&#x3063;&#x3066;&#x3060;&#x307E;&#x3059;</pr></trd>
  </trdgrp>
  <trd lng="pl">blefowa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_l;&c_je;&c_f;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol; &c_v;
    &c_z;&c_a;&c_b;&c_l;&c_u;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_u;&c_s;&c_k;&c_a;&c_t;&c_mol; &c_p;&c_y;&c_l;&c_mol;
    &c_v; &c_g;&c_l;&c_a;&c_z;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_l;&c_je;&c_f;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_v;&c_o;&c_d;&c_i;&c_t;&c_i; &c_v; &c_o;&c_m;&c_a;&c_n;&c_u;</trd>,
    <trd>&c_p;&c_u;&c_s;&c_k;&c_a;&c_t;&c_i; &c_p;&c_i;&c_l; &c_v;&nbsp;&c_o;&c_ch;&c_ib;</trd>
  </trdgrp>
</drv>
</art>
