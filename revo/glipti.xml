<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gliptik</rad>/o
</kap>
<drv mrk="glipti.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Arto gravuri sur multekostaj &scirc;tonoj:
      <ekz>egiptoj estis en la antikvaj tempoj majstroj en la <tld/>o;</ekz>
      <ekz>ekzistas duspecaj <tld/>oj: intajlo kaj kameo.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gem.0o">gemo</ref>,
      <ref cel="fotogl.0o">fotogliptiko</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Glyptik</trd>,
     <trd>Steinschneidekunst</trd>,
     <trd>Edelsteingravur</trd></trdgrp>
  <trd lng="fr">glyptique</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_l;&c_i;&c_p;&c_t;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_je;&c_z;&c_mol;&c_b;&c_a; &c_p;&c_o; &c_k;&c_a;&c_m;&c_n;&c_ju;</trd>
  </trdgrp>
</drv>
</art>
