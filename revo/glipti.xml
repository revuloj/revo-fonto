<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: glipti.xml,v 1.16 2022/10/04 16:54:32 revo Exp $">
<kap>
  <rad>gliptik</rad>/o
</kap>
<drv mrk="glipti.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Arto <ref cel="gravur.0i">gravuri</ref> sur multekostaj &scirc;tonoj:
      <ekz>
        egiptoj estis en la antikvaj tempoj majstroj en la <tld/>o;
      </ekz>
      <ekz>
        ekzistas duspecaj <tld/>a&jcirc;oj: 
        <ref cel="intajl.0o">intajlo</ref> kaj 
        <ref cel="kame.0o">kameo</ref>;
      </ekz>
      <ekz>
        en la &ccirc;e&hcirc;a urbo <nom>Pod&ecaron;brady</nom> lo&gcirc;is kaj 
        kreis fama &ccirc;e&hcirc;a <tld/>isto <nom>Ladislav Havlas</nom>,
        per metodo de <tld/>o li kreis portretojn de eminentaj personoj
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#8470;6</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gem.0o">gemo</ref>,
      <ref cel="fotogl.0o">fotogliptiko</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_l;&c_ib;&c_p;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="ca">gl&iacute;ptica</trd>
  <trdgrp lng="cs">
    <trd>glyptika</trd>,
    <trd>kamenorytectv&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Glyptik</trd>,
    <trd>Steinschneidekunst</trd>,
    <trd>Edelsteingravur</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>glyptics</trd>,
    <trd>glyptography</trd>
  </trdgrp>
  <trd lng="fr">glyptique</trd>
  <trd lng="nl">glyptiek</trd>
  <trd lng="pt">gl&iacute;ptica</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_l;&c_i;&c_p;&c_t;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_je;&c_z;&c_mol;&c_b;&c_a; &c_p;&c_o; &c_k;&c_a;&c_m;&c_n;&c_ju;</trd>
  </trdgrp>
  <trd lng="sk">kame&ncaron;orytectvo</trd>
  <trd lng="uk">&c_g;&c_l;&c_ib;&c_p;&c_t;&c_i;&c_k;&c_a;&nbsp;(&c_r;&c_ib;&c_z;&c_mol;&c_b;&c_l;&c_je;&c_n;&c_n;&c_ja; &c_p;&c_o; &c_k;&c_a;&c_m;&c_je;&c_n;&c_ju;)</trd>
</drv>
</art>
