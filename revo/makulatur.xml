<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: makulatur.xml,v 1.12 2024/01/25 20:10:33 revo Exp $">
<kap>
  <rad>makulatur</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="makulatur.0o">
  <kap><tld/>o</kap>
  <snc mrk="makulatur.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Provpapero, <ref tip="super" cel="paper.0o.materialo">papero</ref> surpresita kaj reuzata:
      <ekz>
        mi vendis ilin kiel <tld/>on, &mdash; &scirc;i triumfe deklaris, &mdash; neniu
        brokantisto konsenti<klr>s</klr> pagi e&ccirc; unu gro&scirc;on
        <fnt>
          <aut>P. Gnedi&ccirc;, trad. Traduka rondo de UES</aut>,
          <vrk>Libra polvo</vrk>,
          <bib>LOdE</bib>,
          <lok>2000:6 (68)</lok>
        </fnt>;
      </ekz>
      <ekz>
        la monon por eldono li ricevis, vendinte kiel <tld/>on stokojn da siaj anta&ubreve;aj
        libroj
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#x2116;12</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Altpapier</trd>
    <trd lng="en">scrap paper</trd>
    <trdgrp lng="fr">
      <trd>g&acirc;che</trd>,
      <trd>macule</trd>
    </trdgrp>
  </snc>
  <snc mrk="makulatur.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senvalora skriba&jcirc;o:
      <ekz>
        <klr>[&#x2026;]</klr> kio estas grava kaj kio estas nur <tld/>o
        <fnt>
          <aut>J. S. Skorupski</aut>,
          <vrk><url ref="http://www.skorupski.com/cdeli.htm">
          Portalo de Plezuro</url></vrk>,
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[sur ceteraj pa&gcirc;oj]</klr> plejparte estas piaj legendoj kaj erudiciaj mitoj, kiujn probable en&scirc;ovis
        <klr>[&#x2026;]</klr> okcidenta klerulo, kiu ne bezonis forlasi sian Parizan
        kabineton por produkti tiun <tld/>on
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#x2116;3</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>worthless scribble</trd>,
      <trd>scrawl</trd>
    </trdgrp>
    <trd lng="fr">torchon<klr> (mauvais &eacute;crit)</klr></trd>
    <trdgrp lng="zh">
    <trd>&#x5E9F;&#x54C1; <pr>f&#xE8;ip&#x1D0;n</pr></trd>,
    <trd>&#x5EE2;&#x54C1; <pr>f&#xE8;ip&#x1D0;n</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_m;&c_a;&c_k;&c_u;&c_l;&c_ja;&c_t;&c_u;&c_r;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>arch tisku nane&ccaron;isto</trd>,
    <trd>makulatura</trd>
  </trdgrp>
  <trd lng="de">Makulatur</trd>
  <trdgrp lng="ja">
    <trd>&#x640D;&#x7D19;  <pr>&#x305D;&#x3093;&#x3057;</pr></trd>,
    <trd>&#x5237;&#x308A;&#x640D;&#x3058;  <pr>&#x3059;&#x308A;&#x305D;&#x3093;&#x3058;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>makulatura</trd>,
    <trd>&sacute;mieci</trd>
  </trdgrp>
<trdgrp lng="ro">
    <trd>reciclarea h&acirc;rtiei</trd>,
    <trd>gunoi</trd>
  </trdgrp>
  <trd lng="sk">tla&ccaron; str&aacute;nky na ne&ccaron;isto</trd>
  <trd lng="uk">&c_m;&c_a;&c_k;&c_u;&c_l;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
</drv>
</art>

