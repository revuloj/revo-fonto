<drv mrk="fin.0e">
  <kap><tld/>e</kap>
  <snc mrk="fin.0e.laste">
    <dif>
      &Ccirc;e la <tld/>o <klr>(tempe a&ubreve; loke)</klr>, laste:
      <ekz>
        <ind>longe &scirc;telas &scirc;telisto, tamen <tld/>e li
        pendos</ind>
        <fnt><bib>PrV</bib></fnt>
        <trd lng="hu">addig h&aacute;ny cselt a r&oacute;ka, am&iacute;g
        cs&aacute;v&aacute;ba nem ker&uuml;l</trd>
      </ekz>
      <ekz>
        unue mar&scirc;is muzikistaro, poste la urbestraro
        kaj <tld/>e la kongresanoj;
      </ekz>
      <ekz>
        <tld/>e, sed certe ne malplej grave
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Dua Kongreso Esperantista en Gen&egrave;ve en
          la 28a de a&ubreve;gusto 1906</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="fin.0e.finfine">
    <dif>
      Post longa atendado:
      <ekz>
        la infano <klr>[...]</klr> <tld/>e alvenis &ccirc;i tien
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e vi alvenas!
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_r;&c_e;&c_sh;&c_c;&c_je;</trd>,
    <trd>&c_n;&c_a;&c_r;&c_e;&c_sh;&c_c;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>al final</trd>,
    <trd>a la fi</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>koncem</trd>,
    <trd>kone&ccaron;n&ecaron;</trd>,
    <trd>kone&ccaron;n&ecaron;</trd>,
    <trd>nakonec</trd>,
    <trd>z&aacute;v&ecaron;rem</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>endlich</trd>,
    <trd>schlie&szlig;lich</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>finalmente</trd>,
    <trd>al fin</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>au <ind>final</ind></trd>,
    <trd>finalement</trd>,
    <trd>enfin<klr tip="ind"> (finalement)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>v&eacute;g&uuml;l</trd>,
    <trd>v&eacute;gre</trd>
  </trdgrp>
  <trd lng="id"><ind>akhir</ind>nya</trd>
  <trd lng="nl">eindelijk</trd>
  <trd lng="oc">a la fin</trd>
  <trd lng="pl">nareszcie</trd>
  <trd lng="ro">&icirc;n sf&acirc;r&scedil;it</trd>
  <trd lng="ru">&c_n;&c_a;&c_k;&c_o;&c_n;&c_je;&c_c;</trd>
