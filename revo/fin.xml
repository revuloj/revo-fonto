    <dif>
      <ref tip="malprt" cel="dum.0.prep">Dum</ref> la <tld/>a tempo de:
      <ekz>
        <tld/>e de la liberi&gcirc;milito
        <fnt>
          <aut>Turisma skabenejo de la urbo Torino</aut>,
          <vrk>Torino</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="fin.0ede.kie">
    <dif>
      <ref tip="malprt" cel="en.0.kie">En</ref> la lasta parto, &ccirc;e la
      ekstremo de:
      <ekz>
        <tld/>e de la eta akvovojo
        <fnt>
        <aut>W. Schad</aut>,
        <vrk>Orinoko-delto...</vrk>,
        <bib>Monato</bib>,
        <lok>2000:11, p. 20a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u; <ind>&c_k;&c_a;&c_n;&c_c;&c_y;</ind></trd>,
    <trd>&c_n;&c_a;&c_p;&c_r;&c_y;&c_k;&c_a;&c_n;&c_c;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>al final de</trd>,
    <trd>a la fi de</trd>
  </trdgrp>
  <trd lng="de">am <ind>Ende</ind> von</trd>
  <trdgrp lng="fr">
    <trd>&agrave; la fin de</trd>,
    <trd>&agrave; l'issue de</trd>
  </trdgrp>
  <trd lng="hu">vminek a <ind>v&eacute;g&eacute;n</ind></trd>
  <trd lng="nl">op het <ind>einde</ind> van</trd>
  <trd lng="oc">a la fin de</trd>
  <trd lng="pl">w ko&nacute;cu</trd>
  <trd lng="ru">&c_v; <ind>&c_k;&c_o;&c_n;&c_c;&c_je;</ind></trd>
