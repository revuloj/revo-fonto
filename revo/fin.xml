<drv mrk="fin.0igxi">
  <kap><ofc>*</ofc><tld/>i&gcirc;i</kap>
  <snc mrk="fin.0igxi.cxesi">
    <dif>
      <ref tip="dif" cel="cxes.0i">&Ccirc;esi</ref>:
      <ekz>
        <ind>kio komenci&gcirc;is, tio anka&ubreve; <tld/>i&gcirc;os</ind>
        <trd lng="fr">tout a une <ind>fin</ind></trd>
        <trd lng="hu">aminek kezdete van, annak <ind>v&eacute;ge</ind> is
