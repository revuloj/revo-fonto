<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>diabol</rad>/o
</kap>
<drv mrk="diabol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      Ludilo konsistanta el bobeno, kiun oni movigas, &jcirc;etas supren
      kaj
      rekaptas per &scirc;nuro havanta tenilon &ccirc;e &ccirc;iu
      ekstremo.
    </dif>
  </snc>
  <trd lng="bg">&c_d;&c_i;&c_a;&c_b;&c_o;&c_l;&c_o;</trd>
  <trd lng="ca">di&agrave;bolo</trd>
  <trd lng="de">Diabolo</trd>
  <trd lng="en">diabolo</trd>
  <trd lng="es">di&aacute;bolo</trd>
  <trd lng="fr">diabolo<klr tip="ind"> (jouet)</klr></trd>
  <trd lng="hu">diabol&oacute;</trd>
  <trd lng="pl">diabolo</trd>
  <trdgrp lng="pt">
    <trd>diabol&ocirc;</trd>,
    <trd>di&aacute;bolo</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_i;&c_a;&c_b;&c_o;&c_l;&c_o;</trd>
</drv>
</art>
