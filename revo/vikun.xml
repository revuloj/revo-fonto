<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: vikun.xml,v 1.2 2025/01/08 18:36:32 afido Exp $">
    <kap>
      <rad>vikun</rad>/o
      <fnt><bib>JED</bib></fnt>
    </kap>

<drv mrk="vikun.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Mamulo el <ref tip="malprt" cel="kameled.0oj">kameledoj</ref> (<trd lng="la">Vicugna
      vicugna</trd>),
      kun tre delikata lano, vivanta en altaj Andoj:
      <ekz>
        sendube <nom>Volter</nom> aludas al la alpako, al la lamao, al la <tld/>o, kaj aliaj
        rema&ccirc;uloj, kun flavru&gcirc;aj haroj, el Sud-Ameriko
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        gvanakoj, lamoj, <tld/>oj kaj andaj vulpoj longavostaj videblas el la vagonaro
        <fnt>
          <bib>Monato</bib>
          <aut>Marta Sol&iacute;s</aut>
          <vrk><url ref="&Monato;/2005/008399.php"
              >Unika fervojo en Andoj</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="alpak.0o.besto">alpako<sncref/></ref>,
      <ref cel="gvanak.0o">gvanako</ref>,
      <ref cel="lam1.0o">lamo</ref>
    </refgrp>
    <bld
      lok="&WCU;/thumb/a/ab/Vicu&ntilde;a_by_Rico_H&uuml;bner.jpg/397px-Vicu&ntilde;a_by_Rico_H&uuml;bner.jpg"
      prm="CC BY-SA 3.0"
      ><tld/>ino
        <fnt>
          <aut>foto de Rixxo, Rico H&uuml;bner</aut>
          <lok><url ref="&WCW;/File:Vicu%C3%B1a_by_Rico_H%C3%BCbner.jpg"
              >https://commons.wikimedia.org/wiki/File:Vicu&ntilde;a_by_Rico_H&uuml;bner.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="bg">&c_v;&c_i;&c_k;&c_u;&c_n;&c_ja;</trd>
    <trdgrp lng="de">
      <trd>Vikunja</trd>,
      <trd>Vicu&ntilde;a</trd>
    </trdgrp>
    <trd lng="en">vicu&ntilde;a</trd>
    <trd lng="es">vicu&ntilde;a</trd>
    <trdgrp lng="fa">
      <trd>&a_sh;&a_t;&a_r;&f_ch;&a_h;</trd>,
      <trd>&a_w;&f_y;&f_k;&a_w;&a_n;&f_y;&a_A;</trd>
    </trdgrp>
    <trd lng="fr">vigogne</trd>
    <trd lng="hu">vikunyna</trd>
    <trd lng="nl">vicu&ntilde;a</trd>
    <trd lng="ru">&c_v;&c_i;&c_k;&c_u;&c_n;&c_mol;&c_ja;</trd>
  </snc>
</drv>

  </art>

