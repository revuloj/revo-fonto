<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sutur.xml,v 1.7 2022/05/18 07:31:06 revo Exp $">
<kap>
  <rad>sutur</rad>/i <fnt><bib>PIV2</bib></fnt>
</kap>

<drv mrk="sutur.0i">
  <kap><tld/>i</kap>
  <snc mrk="sutur.0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Fermi vundon per speciala fadeno kaj kudro:
      <ekz>
        necesis <tld/>i vundojn en diversaj partoj de la korpo
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="">
          Membro de Strik-Komitato...</url></vrk>,
          <lok>2006-09-30</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_sh;&c_y;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_r;&c_a;&c_n;&c_u;)</klr></trd>,
    <trd>&c_n;&c_a;&c_k;&c_l;&c_a;&c_d;&c_v;&c_a;&c_c;&c_mol; <ind>&c_sh;&c_v;&c_o;</ind> <klr>(&c_n;&c_a; &c_r;&c_a;&c_n;&c_u;)</klr></trd>
  </trdgrp>
  <trd lng="cs">se&scaron;&#xED;t tk&#xE1;&ncaron;</trd>
  <trdgrp lng="en"><trd>stitch</trd>, <trd>suture</trd></trdgrp>
  <trdgrp lng="fr">
    <trd>suturer</trd>,
    <trd>recoudre<klr> (une plaie)</klr></trd>
  </trdgrp>
  <trd lng="sk">zo&scaron;i&tcaron; tkanivo</trd>
  <trd lng="uk">&#x43D;&c_a;&#x43A;&#x43B;&c_a;&c_d;&c_a;&c_t;&c_i; &c_sh;&#x43E;&c_v;</trd>
</drv>
</art>

