<vortaro>
<art mrk="$Id: tehxnikum.xml,v 1.1 2022/05/22 20:30:28 afido Exp $">
<kap>
  <rad>te&hcirc;nikum</rad>/o,
  <var><kap><rad var="k">teknikum</rad>/o <fnt><bib>Viki</bib></fnt></kap></var>
</kap>

<drv mrk="tehxnikum.0o">
  <kap><tld/>o</kap>
  <snc mrk="tehxnikum.0o.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      <ref tip="super" cel="lern.mez0ejo,PED">Mezlernejo</ref>
      kun studoj dedi&ccirc;itaj grandparte al difinita te&hcirc;nika
      a&ubreve; industria fako:
      <ekz>
        en la a&ubreve;lo de la pedagogia <tld/>o
        <fnt>
          <aut>J. German, trad. J. Finkel</aut>
          <vrk><url ref="https://www.esperanto.mv.ru/German/kara-05-2.html"
              >Kara mia homo</url></vrk>
          <lok>2016-2017</lok>
        </fnt>;
      </ekz>
      <ekz>
        en 1972 <klr>[&#8230;]</klr> mi eniris en la aviad-<tld var="k"/>on, kie mi ricevis la
        specialecon pri aviad-aparatoj
        <fnt>
          <aut>N. Lozga&ccirc;ev</aut>
          <vrk><url ref="https://sezonoj.ru/2022/05/lozgachev"
              >A&ubreve;tobiografiaj skizoj</url></vrk>
          <lok>La Ondo de Esperanto, 2017:5</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>lyc&eacute;e technologique</trd>,
    <trd>lyc&eacute;e professionnel</trd>
  </trdgrp>
</drv>
</art>

