<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sxpruc.xml,v 1.23 2019/05/29 14:10:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;pruc</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="sxpruc.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sxpruc.0i.flui">
    <dif>
      Rapide, salte kaj fortege
      el<ref tip="super" cel="flu.0i.malsupren">flui</ref>
      eksteren:
      <ekz>
        fonto <tld/>as el la roko;
      </ekz>
      <ekz>
        larmoj <tld/>is el liaj okuloj.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>spritzen</trd>,
      <trd>sprudeln</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>brotar</trd>,
      <trd>manar</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>sprizzare</trd>,
      <trd>schizzare<klr> (sprizzare)</klr></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>jorrar</trd>,
      <trd>esguichar</trd>
    </trdgrp>
    <trdgrp lng="th">
      <trd>&#3614;&#3640;&#3656;&#3591;</trd>,
      <trd>&#3652;&#3627;&#3621;&#3629;&#3629;&#3585;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sxpruc.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Rapide, subite montri&gcirc;i:
      <ekz>
        de &ccirc;iu monteto <tld/>is malhela arbareto de akacioj, 
        sikomoroj kaj tamarindoj
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 1a</lok></fnt>;
      </ekz>
      <ekz>
        fine <tld/>is el la horizonto kun siaj nenombreblaj kupoloj
        kaj minaretoj la blanka Konstantinopolo;
      </ekz>
      <ekz>
        &ccirc;ie <tld/>as fre&scirc;a forto kaj nova vivo;
      </ekz>
      <ekz>
        &gcirc;ojo <tld/>is el &ccirc;iuj okuloj;
      </ekz>
      <ekz>
        spasme <tld/>anta gajeco<fnt>Z</fnt>.
      </ekz>
    </dif>
     <trdgrp lng="it">
      <trd>sprizzare</trd>,
      <trd>errompere</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>surgir</trd>,
      <trd>aparecer</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_y;&c_r;&c_s;&c_k;&c_a;&c_c;&c_mol;</trd>
  <trd lng="nl">spuiten</trd>
  <trd lng="en">gush</trd>
  <trdgrp lng="fr">
    <trd>jaillir</trd>,
    <trd>surgir</trd>
  </trdgrp> 
  <trdgrp lng="hu">
    <trd>fr&ouml;cs&ouml;g</trd>,
    <trd>fakad</trd>,
    <trd>buzog</trd>,
    <trd>locsog<klr tip="ind"> (pl. v&iacute;z)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_y;&c_z;&c_g;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_i;&c_t;&c_mol; &c_k;&c_l;&c_ju;&c_ch;&c_o;&c_m;</trd>
  </trdgrp>
</drv>

<drv mrk="sxpruc.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="sxpruc.0.SON">
    <dif>
      <ref tip="lst" cel="son.0o" lst="voko:sonoj">Sono</ref>
      de elpremi&gcirc;anta flua&jcirc;o:
      <ekz>
        <tld/>'! plumingo elsaltis el la fingroj de dormema Zbi&ccirc;jo
        <fnt>
          <aut>A. Rudawcowa, trad. K. Grochocka</aut>,
          <vrk><url
          ref="www.esperanto.cba.pl/images/lernojaro.pdf&lrm;">
          Lernojaro komenci&gcirc;is</url></vrk>,
          <lok>[vidita en 2013]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">&#161;chof!</trd>
  <trd lng="fr">sproutch !</trd>
</drv>


<drv mrk="sxpruc.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxpruc.0igi.KOMUNE">
    <dif>
      Igi ion <tld/>i:
      <ekz>
        fontoj, <tld/>igantaj akvon
        <fnt><bib>MT</bib>, <lok>&Sen; 8:24</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>igu fajron kaj morton el viaj okuloj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="jxet.el0i">el&jcirc;eti</ref>,
      <ref cel="vom.0i">vomi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="es">
    <trd>proyectar<klr tip="amb"> (un l&iacute;quido)</klr></trd>,
    <trd>pulverizar<klr tip="amb"> (un l&iacute;quido)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>asperger</trd>,
    <trd>projeter<klr tip="amb"> (un liquide)</klr></trd>,
    <trd>pulv&eacute;riser<klr tip="amb"> (un liquide)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fr&ouml;ccsent</trd>,
    <trd>locsol</trd>,
    <trd>fakaszt</trd>,
  </trdgrp>
  <trd lng="it">spruzzare</trd>
  <trd lng="nl">sproeien</trd>
  <trd lng="pt">jorrar</trd>
  <trd lng="ru">
&c_r;&c_a;&c_z;&c_b;&c_r;&c_y;&c_z;&c_g;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="sxpruc.0igilo">
  <kap><tld/>igilo</kap>
  <snc mrk="sxpruc.0igilo.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Ilo a&ubreve; aparato por <tld/>igi likva&jcirc;on:
      <ekz>
        kelke da homoj <klr>...</klr> per <tld/>igiloj malsekigis la
        arbojn
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 14a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="th">&#3648;&#3588;&#3619;&#3639;&#3656;&#3629;&#3591;&#3614;&#3656;&#3609;&#3618;&#3634;</trd>
  </snc>
  <trdgrp lng="es">
    <trd>aspersor</trd>,
    <trd>pulverizador</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>aspersoir</trd>,
    <trd>brumisateur</trd>,
    <trd>pulv&eacute;risateur</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>spruzzatore</trd>,
    <trd>irroratore</trd>,
    <trd>lancia<klr> (spruzzatore)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="sxpruc.dis0i">
  <kap>dis<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sxpruc.dis0i.KOMUNE">
    <dif>
      Rapide, forte elflui al &ccirc;iuj direktoj:
      <ekz>
        &ccirc;iuflanken dis<tld/>is homa cerbo; torente fluis la sango
        <fnt><bib>&Ccirc;uk&ccirc;oj</bib>, <lok>la trimova
        &scirc;akproblemo, 
        (tria skizo el la &ccirc;uk&ccirc;a vivo), 3, La e&ubreve;rope 
        edukita homo</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="eksplo.0i">eksplodi</ref>
    </refgrp>
  </snc>
  <trd lng="es">manar</trd>
  <trd lng="fr">gicler</trd>
  <trd lng="it">schizzare</trd>
</drv>

<drv mrk="sxpruc.en0igi">
  <kap>en<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxpruc.en0igi.KOMUNE">
    <dif>
      Rapide kaj forte enigi flua&jcirc;on en ion:
      <ekz>
        tio en<tld/>igis kvaza&ubreve; novan sangon en la vejnoj
        de la &scirc;toni&gcirc;anta societo.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="injekt.0i">injekti</ref>
    </refgrp>
  </snc>
  <trd
  lng="be">&c_u;&c_p;&c_y;&c_r;&c_s;&c_k;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">hineinspritzen</trd>
  <trd lng="es">inyectar</trd>
  <trd lng="fr">injecter</trd>
  <trd lng="hu">befecskendez</trd>
  <trd lng="it">iniettare</trd>
  <trd lng="nl">inspuiten</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_p;&c_r;&c_y;&c_s;&c_k;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_p;&c_r;&c_y;&c_s;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
