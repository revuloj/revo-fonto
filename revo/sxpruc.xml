<drv mrk="sxpruc.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sxpruc.0i.flui">
    <dif>
      El<ref tip="super" cel="jxet.0igxi">&jcirc;eti&gcirc;i</ref> plurdirekten fontane, multgute, mult-ere:
      <ekz>
        &scirc;ia sango <tld/>is sur la muron
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 9:33</lok>
        </fnt>;
      </ekz>
      <ekz>
        la akvo <tld/>as alte super nin
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;talo ekfrapis la fajro&scirc;tonon tiel, ke fajreroj ek<tld/>is
        <fnt>
          <bib>Fab1</bib>
          <lok>Fluganta kofro</lok>
        </fnt>;
      </ekz>
      <ekz>
        purpurru&gcirc;aj angiloj, el kiuj &ccirc;e &ccirc;iu movi&gcirc;o <tld/>is bluaj fajreroj, ludis
        malsupre en la akvo
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la anasido <klr>[&#8230;]</klr> en sia timego enflugis &gcirc;uste en pladon kun lakto, tiel
        ke la lakto <tld/>is &ccirc;irka&ubreve;en
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi turmentas min tiom, ke al mi preska&ubreve; la sango el<tld/>as el sub la ungoj
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        malgranda &gcirc;ardeno, en <klr>[kies]</klr> mezo <tld/>is fontano
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> almetis la tor&ccirc;on, tuj <tld/>is granda flamo
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        se malfermi malgrandan ka&scirc;an flankan pordon, en la okulojn <tld/>as la suno
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>spritzen</trd>,
      <trd>sprudeln</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>brotar</trd>,
      <trd>manar</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>sprizzare</trd>,
      <trd>schizzare<klr> (sprizzare)</klr></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>jorrar</trd>,
      <trd>esguichar</trd>
    </trdgrp>
    <trdgrp lng="th">
      <trd>&#xE1E;&#xE38;&#xE48;&#xE07;</trd>,
      <trd>&#xE44;&#xE2B;&#xE25;&#xE2D;&#xE2D;&#xE01;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sxpruc.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Etendi&gcirc;i, &jcirc;eti&gcirc;i dise plurdirekten kvaza&ubreve; gutoj, flamoj ks:
      <ekz>
        de &ccirc;iu monteto <tld/>is malhela arbareto de akacioj,
        sikomoroj kaj tamarindoj
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 1a</lok></fnt>;
      </ekz>
      <ekz>
        brufermi&gcirc;is la pordoj, <tld/>is la ridoj kaj la &gcirc;ojaj salutaj ekkrioj
        <fnt>
          <bib>Lasta</bib>
          <lok>La Lasta</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi estus feli&ccirc;a, se <klr>[&#8230;]</klr> iom da tiu amemo povus <tld/>i &gcirc;is mia persono
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        li esperis, ke rebrilo de tiu nobela titolo <tld/>os sur lin
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekkvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        na&ubreve;ziga kra&ccirc;a&jcirc;o, kiu <tld/>is sur &scirc;iajn sentojn kaj malpurigis &scirc;iajn memorojn
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>;
      </ekz>
      <ekz>
        konstante sur min <tld/>as la malbeno
        <fnt>
          <bib>Ifigenio</bib>
          <lok>Ifigenio en Ta&ubreve;rido</lok>
        </fnt>!
      </ekz>
      <ekz>
        la spriteco <klr>[&#8230;]</klr> el<tld/>as el li
        <fnt>
          <bib>DKM</bib>
          <lok>&Ccirc;apitro LXII</lok>
        </fnt>.
      </ekz>
    </dif>
     <trdgrp lng="it">
      <trd>sprizzare</trd>,
      <trd>errompere</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>surgir</trd>,
      <trd>aparecer</trd>
    </trdgrp>
  </snc>
  <snc mrk="sxpruc.0i.sxprucigi">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="sxpruc.0igi">&Scirc;prucigi</ref>:
      <ekz>
        Sub benko katon mi ekvidis, &#x2AFD; Sur &gcirc;in mi <tld/>is el botelo
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        balenoj <tld/>is akvon el la truoj de siaj nazoj supren, ke &gcirc;i elrigardis
        &ccirc;irka&ubreve;e kiel centoj da fontanoj
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u sur ilin nur unu solan guton de tiu &ccirc;i trinka&jcirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_y;&c_r;&c_s;&c_k;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="ja">
    <trd>&#22132;&#x304D;&#x51FA;&#x308B;  <pr>&#12405;&#x304D;&#12391;&#x308B;</pr></trd>,
    <trd>&#x308F;&#x304D;&#x51FA;&#x308B;  <pr>&#x308F;&#x304D;&#12391;&#x308B;</pr></trd>,
    <trd>&#x307B;&#12392;&#12400;&#12375;&#x308B;</trd>,
    <trd>&#x4E0D;&#x610F;&#x306B;&#x73FE;&#x308C;&#x308B;
    <pr>&#12405;&#12356;&#x306B;&#12354;&#12425;&#x308F;&#x308C;&#x308B;</pr></trd>,
    <trd>&#x8E8D;&#x308A;&#x51FA;&#x308B;  <pr>&#x304A;&#12393;&#x308A;&#12391;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="en">gush</trd>
  <trdgrp lng="fr">
    <trd>jaillir</trd>,
    <trd>surgir</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fr&ouml;cs&ouml;g</trd>,
    <trd>fakad</trd>,
    <trd>buzog</trd>,
    <trd>locsog<klr tip="ind"> (pl. v&iacute;z)</klr></trd>
  </trdgrp>
  <trd lng="nl">spuiten</trd>
  <trdgrp lng="pl">
    <trd>bryzga&cacute;</trd>,
    <trd>chlupa&cacute;</trd>,
    <trd>pryska&cacute;</trd>,
    <trd>tryska&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_y;&c_z;&c_g;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_i;&c_t;&c_mol; &c_k;&c_l;&c_ju;&c_ch;&c_o;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_r;&c_i;&c_z;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_i;&c_r;&c_s;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_i;&c_t;&c_i; &c_k;&c_l;&c_ju;&c_ch;&c_je;&c_m;</trd>,
    <trd>&c_s;&c_i;&c_p;&c_a;&c_t;&c_i; (&c_p;&c_r;&c_o; &c_ib;&c_s;&c_k;&c_r;&c_i;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6D8C;&#x51FA; <pr>y&#x1D2;ngch&#x16B;</pr></trd>,
    <trd>&#x6E67;&#x51FA; <pr>y&#x1D2;ngch&#x16B;</pr></trd>,
    <trd>&#x55B7; <pr>p&emacron;n</pr></trd>,
    <trd>&#22132; <pr>p&emacron;n</pr></trd>,
    <trd>&#x55B7;&#x5C04; <pr>p&emacron;nsh&#xE8;</pr></trd>,
    <trd>&#22132;&#x5C04; <pr>p&emacron;nsh&#xE8;</pr></trd>,
    <trd>&#x6D8C;&#x73B0; <pr>y&#x1D2;ngxi&#xE0;n</pr></trd>,
    <trd>&#x6E67;&#x73FE; <pr>y&#x1D2;ngxi&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxpruc.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="sxpruc.0.SON">
    <dif>
      <ref tip="lst" cel="son.0o" lst="voko:sonoj">Sono</ref>
      de elpremi&gcirc;anta flua&jcirc;o:
      <ekz>
        <tld/>'! plumingo elsaltis el la fingroj de dormema <nom>Zbi&ccirc;jo</nom>
        <fnt>
          <aut>A. Rudawcowa, trad. K. Grochocka</aut>,
          <vrk><url ref="www.esperanto.cba.pl/images/lernojaro.pdf&#x200E;">
          Lernojaro komenci&gcirc;is</url></vrk>,
          <lok>[vidita en 2013]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_y;&c_r;&c_s;&c_k;!</trd>
  <trd lng="es">&#xA1;chof!</trd>
  <trd lng="fr">sproutch !</trd>
  <trdgrp lng="pl">
    <trd>chlap !</trd>,
    <trd>prysk !</trd>
  </trdgrp>
</drv>

<drv mrk="sxpruc.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxpruc.0igi.KOMUNE">
    <dif>
      Igi ion <tld/>i:
      <ekz>
        fontoj, <tld/>igantaj akvon
        <fnt><bib>MT</bib>, <lok>&Sen; 8:24</lok></fnt>;
      </ekz>
      <ekz>
        duonon de la sango li <tld/>igis sur la altaron
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 24:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fonto el<tld/>igas el la sama aperturo dol&ccirc;an akvon kaj maldol&ccirc;an
        <fnt>
          <bib>NT</bib>
          <lok>Jakobo 3:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;eestantaj virinoj <tld/>igis sur &scirc;in vinagron
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[li]</klr> <tld/>igis tiajn alegoriojn kaj ka&scirc;itajn sencojn, ke &scirc;ajnis, ke
        oni neniam divenos la veron
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igu fajron kaj morton el viaj okuloj &mdash; min amuzas la kolerego de virino
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        en mia plena malespero mi <tld/>igis al ili la gason, esperante, ke mi povas
        ilin fu&gcirc;igi
        <fnt>
          <bib>VaK</bib>
          <lok>Deksepa &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="asperg.0i">aspergi</ref>,
      <ref cel="dusx.0i">du&scirc;i</ref>,
      <ref cel="jxet.el0i">el&jcirc;eti</ref>,
      <ref cel="kracx.0i">kra&ccirc;i</ref>,
      <ref cel="vom.0i">vomi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_p;&c_y;&c_r;&c_s;&c_k;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>verspritzen</trd>,
    <trd>spr&uuml;hen</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>proyectar<klr tip="amb"> (un l&iacute;quido)</klr></trd>,
    <trd>pulverizar<klr tip="amb"> (un l&iacute;quido)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>asperger</trd>,
    <trd>projeter<klr tip="amb"> (un liquide)</klr></trd>,
    <trd>pulv&eacute;riser<klr tip="amb"> (un liquide)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fr&ouml;ccsent</trd>,
    <trd>locsol</trd>,
    <trd>fakaszt</trd>,
  </trdgrp>
  <trd lng="it">spruzzare</trd>
  <trd lng="ja">&#22132;&#x51FA;&#12373;&#x305B;&#x308B;  <pr>&#12405;&#12435;&#12375;&#12421;&#12388;&#12373;&#x305B;&#x308B;</pr></trd>
  <trd lng="nl">sproeien</trd>
  <trd lng="pl">chlapa&cacute;</trd>
  <trd lng="pt">jorrar</trd>
  <trd lng="ru">
&c_r;&c_a;&c_z;&c_b;&c_r;&c_y;&c_z;&c_g;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="uk">&c_o;&c_b;&c_p;&c_r;&c_i;&c_s;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x55B7;&#x6D12; <pr>p&emacron;ns&#x1CE;</pr></trd>,
    <trd>&#22132;&#28753; <pr>p&emacron;ns&#x1CE;</pr></trd>,
    <trd>&#x6E85; <pr>ji&#xE0;n</pr></trd>,
    <trd>&#x6FFA; <pr>ji&#xE0;n</pr></trd>,
    <trd>&#x55B7;&#x5C04;&#x51FA; <pr>p&emacron;nsh&#xE8;ch&#x16B;</pr></trd>,
    <trd>&#22132;&#x5C04;&#x51FA; <pr>p&emacron;nsh&#xE8;ch&#x16B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxpruc.0igilo">
  <kap><tld/>igilo</kap>
  <snc mrk="sxpruc.0igilo.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Ilo a&ubreve; aparato por <tld/>igi likva&jcirc;on:
      <ekz>
        kelke da homoj <klr>[&#8230;]</klr> per <tld/>igiloj malsekigis la
        arbojn
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 14a</lok></fnt>;
      </ekz>
      <ekz>
        la dio de dormo per sia sor&ccirc;a <tld/>igilo aspergis &ccirc;iujn meblojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ie en la &scirc;ipo trovi&gcirc;as kameraoj, <tld/>igiloj, fum- kaj fajro-detektiloj
        <fnt>
          <bib>Monato</bib>
          <aut>Christian Bertin</aut>
          <vrk><url ref="&Monato;/2004/008255.html">Re&gcirc;ino de la maroj</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        kortizonaj naz-<tld/>igiloj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2013/010953.php">Zorga gardisto nenion preteratentas</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam lia <tld/>igilo funkciis tuj, pro troa eksciti&gcirc;o, e&ccirc; anta&ubreve; ol penetri,
        <klr>[&#8230;]</klr> &scirc;i sciis redoni al li kura&gcirc;on, <klr>[&#8230;]</klr> la duan provon
        &scirc;i &scirc;atis: helpante lin sukcesi, &scirc;i faris el li viron
        <fnt>
          <bib>ChM</bib>
          <lok>3</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akv.0umilo">akvumilo</ref>,
      <ref cel="asperg.0ilo">aspergilo</ref>,
      <ref cel="ajut.0o">ajuto</ref>,
      <ref cel="duz.0o">duzo</ref>
    </refgrp>
    <trd
    lng="th">&#xE40;&#xE04;&#xE23;&#xE37;&#xE48;&#xE2D;&#xE07;&#xE1E;&#xE48;&#xE19;&#xE22;&#xE32;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_p;&c_y;&c_r;&c_s;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_p;&c_u;&c_l;&c_mol;&c_v;&c_e;&c_r;&c_y;&c_z;&c_a;&c_t;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="de">Spr&uuml;her</trd>
  <trdgrp lng="es">
    <trd>aspersor</trd>,
    <trd>pulverizador</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>aspersoir</trd>,
    <trd>brumisateur</trd>,
    <trd>pulv&eacute;risateur</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>spruzzatore</trd>,
    <trd>irroratore</trd>,
    <trd>lancia<klr> (spruzzatore)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#22132;&#38695;&#22120;  <pr>&#12405;&#12435;&#12416;&#x304D;</pr></trd>,
    <trd>&#21561;&#x4ED8;&#12369;&#22120;  <pr>&#12405;&#x304D;&#12388;&#12369;&#12358;&#12388;&#x308F;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>nawil&zdot;acz</trd>,
    <trd>sikawka</trd>,
    <trd>spryskiwacz</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_l;&c_i;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_c;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_i;&c_v;&c_a;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_o;&c_b;&c_p;&c_r;&c_i;&c_s;&c_k;&c_u;&c_v;&c_a;&c_ch;</trd>
  </trdgrp>
</drv>

<drv mrk="sxpruc.dis0i">
  <kap>dis<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sxpruc.dis0i.KOMUNE">
    <dif>
      Rapide, forte elflui al &ccirc;iuj direktoj:
      <ekz>
        la akvo dis<tld/>adis sur la korton
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        ilia sango estos dis<tld/>igita kiel polvo
        <fnt>
          <bib>MT</bib>
          <lok>Cefanja 1:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ondoj amase kuris el la profunda&jcirc;oj kaj dis<tld/>is kun &gcirc;emo &ccirc;e la bordoj
        <fnt>
          <bib>Lasta</bib>
          <lok>La Lasta</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuflanken dis<tld/>is homa cerbo, torente fluis la sango
        <fnt><bib>&Ccirc;uk&ccirc;oj</bib>, <lok>la trimova
        &scirc;akproblemo,
        (tria skizo el la &ccirc;uk&ccirc;a vivo), 3, La e&ubreve;rope
