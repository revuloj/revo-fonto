<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;pruc</rad>/i
</kap>
<drv mrk="sxpruc.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Rapide, salte kaj fortege elflui eksteren:
      <ekz>
        fonto <tld/>as el la roko;
      </ekz>
      <ekz>
        larmoj <tld/>is el liaj okuloj
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="torent.0i">torenti</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>spritzen</trd>,
      <trd>sprudeln</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>jorrar</trd>,
      <trd>esguichar</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Rapide, subite montri&gcirc;i:
      <ekz>
        sur tiu monteto <tld/>is malhela arbareto da akacioj kaj
        tamarindoj<fnt>B</fnt>;
      </ekz>
      <ekz>
        fine <tld/>is el la horizonto kun siaj nenombreblaj kupoloj
        kaj minaretoj la blanka Konstantinopolo;
      </ekz>
      <ekz>
        &ccirc;ie <tld/>as fre&scirc;a forto kaj nova vivo;
      </ekz>
      <ekz>
        &gcirc;ojo <tld/>is el &ccirc;iuj okuloj;
      </ekz>
      <ekz>
        spasme <tld/>anta gajeco<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_y;&c_r;&c_s;&c_k;&c_a;&c_c;&c_mol;</trd>
  <trd lng="nl">spuiten</trd>
  <trd lng="en">gush</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_y;&c_z;&c_g;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_i;&c_t;&c_mol; &c_k;&c_l;&c_ju;&c_ch;&c_o;&c_m;</trd>
  </trdgrp>
</drv>

<drv mrk="sxpruc.0igiel0igi">
  <kap><tld/>igi, el<tld/>igi</kap>
  <snc>
    <dif>
      Igi ion <tld/>i:
      <ekz>
        fontoj <tld/>igantaj akvon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>igu fajron kaj morton el viaj okuloj<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="jxet.el0i">el&jcirc;eti</ref>,
      <ref cel="sput">elsputi</ref>,
      <ref cel="vom.0i">vomi</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">sproeien</trd>
  <trd lng="pt">jorrar</trd>
</drv>

<drv mrk="sxpruc.en0igi">
  <kap>en<tld/>igi</kap>
  <snc>
    <dif>
      Rapide kaj forte enigi flua&jcirc;on en ion:
      <ekz>
        tio en<tld/>igis kvaza&ubreve; novan sangon en la vejnoj
        de la &scirc;toni&gcirc;anta societo
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="injekt.0i">injekti</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_u;&c_p;&c_y;&c_r;&c_s;&c_k;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">hineinspritzen</trd>
  <trd lng="nl">inspuiten</trd>
  <trd lng="ru">&c_v;&c_p;&c_r;&c_y;&c_s;&c_n;&c_u;&c_t;&c_mol;</trd>
</drv>
</art>
