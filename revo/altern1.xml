<kap>
  <rad>alternanc</rad>/o
</kap>
<drv mrk="altern1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ELE</uzo>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Duon<ref tip="malprt" cel="period.0o.funkcio">periodo</ref>
      de alterna fenomeno:
      <ekz>
        diodoj estas naturaj &scirc;altiloj por alterna kurento: <tld/>o okazas kun minimuma
        tensioperdo, dum la sekvanta <tld/>o estas blokita
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=298979">&Scirc;altilo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_w;&c_p;&c_e;&c_r;&c_y;&c_ja;&c_d;</trd>
  <trd lng="ca">alternan&ccedil;a <klr tip="amb">(f&iacute;sica)</klr></trd>
  <trdgrp lng="cs">
    <trd>alternace</trd>,
    <trd>alternanta</trd>,
    <trd>polokmit</trd>,
    <trd>st&rcaron;&iacute;d&aacute;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>halbe Schwingungsdauer</trd>,
    <trd>Alternanz</trd>
  </trdgrp>
  <trd lng="en">half-period</trd>
  <trdgrp lng="es">
    <trd>semiperiodo</trd>,
    <trd>semiper&iacute;odo</trd>
  </trdgrp>
  <trd lng="fr">alternance</trd>
  <trd lng="he">&he;&#1460;&tav;&het;&#1463;&lamed;&#1456;&pe;&vav;&#1468;&tav;</trd>
  <trd lng="hu">f&eacute;lperi&oacute;dus</trd>
  <trdgrp lng="ja">
    <trd>&#20132;&#30058;  <pr>&#12371;&#12358;&#12400;&#12435;</pr></trd>,
    <trd>&#21322;&#21608;&#26399;  <pr>&#12399;&#12435;&#12375;&#12421;&#12358;&#12365;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_p;&c_je;&c_r;&c_i;&c_o;&c_d;</trd>
  <trd lng="sk">striedanie</trd>
</drv>
