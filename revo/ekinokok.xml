<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ekinokok.xml,v 1.5 2022/04/27 14:38:53 afido Exp $">
<kap>
  <rad>ekinokok</rad>/o <fnt><bib>PIV1</bib></fnt>,
  <var><kap><rad var="&hcirc;">e&hcirc;inokok</rad>/o <fnt><bib>PIV2</bib></fnt></kap></var>
</kap>

<drv mrk="ekinokok.0o">
  <kap><tld/>o,
  <var><kap><tld var="&hcirc;"/>o</kap></var></kap>
  <snc mrk="ekinokok.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Platvermo kiu parazitas precipe vulpojn, hundojn kaj similajn,
      ka&ubreve;zante kistojn en hepato, pulmoj a&ubreve; aliaj organoj;
      genro el familio teniedoj <trd lng="la">Echinococcus</trd>:
      <ekz>
        sana hundo estas unu el la vivkondi&ccirc;oj de la homoj,
        kaj <klr>[&#x2026;]</klr> tial estas plena skandalo,
        kiom senatentaj iuj homoj estas pri <tld/>oj
        <fnt>
          <aut>H. Laxness</aut>
          <vrk>Sendependaj homoj</vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld var="&hcirc;"/>aj vezikoj &ccirc;e la homo povas atingi diametron &gcirc;is grando de 40 centimetroj
        <fnt>
          <aut>R. Sachs</aut>
          <vrk><url ref="&STEB;medicino/medicina-parazitologio/medicina-parazitologio.htm">Medicina Parazitologio</url></vrk>
          <lok>Scienca Revuo, 2001, printempo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Fuchsbandwurm</trd>
    <trd>Hundebandwurm</trd>
  </trdgrp>
  <trd lng="fr">&eacute;chinocoque</trd>
  <trd lng="uk">&#x435;&#x445;&#x456;&#x43D;&#x43E;&#x43A;&#x43E;&#x43A;</trd>
</drv>
</art>

