<vortaro>
<art mrk="$Id: panace.xml,v 1.16 2019/06/30 18:21:48 revo Exp $">
<kap><rad>panace</rad>/o</kap>
<drv mrk="panace.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">MIT</uzo>
  <uzo tip="fak">MED</uzo>
    <dif>
      &Scirc;ajniga <ref cel="univer1.0a.cxiam_tauga">universala</ref>
      <ref tip="super" cel="kurac.0ilo">kuracilo</ref>
      por &ccirc;iuj malsanoj a&ubreve; malbonoj:
      <ekz>
        melatonino ka&ubreve;zas nek dependon nek iajn riskojn por la
        homa sano <klr>[&#8230;]</klr>, mi ne volas priskribi &gcirc;in
        kiel <tld/>on a&ubreve; eliksiron, sed la &scirc;an&gcirc;oj en la vivkvalito
        de mia kolego mirigis min
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2012/010764.php"
          >Dormu bone, vivu bone (per melatonino)</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_n;&c_a;&c_c;&c_e;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>panacea</trd>,
    <trd>v&scaron;el&eacute;k</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Allheilmittel</trd>,
    <trd>Wundermittel</trd>,
    <trd>Universalmittel</trd>,
    <trd>Panazee</trd>
  </trdgrp>
  <trd lng="en">panacea</trd>
  <trd lng="fr">panac&eacute;e</trd>
  <trdgrp lng="hu">
    <trd>csodaszer</trd>,
    <trd>panacea</trd>
  </trdgrp>
  <trd lng="nl">panacee</trd>
  <trd lng="pt">panac&eacute;ia</trd>
  <trd lng="ru">&c_p;&c_a;&c_n;&c_a;&c_c;&c_je;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>liek na v&scaron;etko</trd>,
    <trd>z&aacute;zra&ccaron;n&yacute; prostriedok</trd>
  </trdgrp>
</drv>
</art>
