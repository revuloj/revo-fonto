<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
   <kap>
      <ofc>1</ofc>
      <rad>miniatur</rad>/o
   </kap>

<drv mrk="miniat.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">BELA</uzo>
   <snc mrk="miniat.0o.bildo">
      <dif>
         Tre malgranda pentra&jcirc;o, plej ofte portreto, farita per akvarelaj
         koloroj sur eburo, metalo, pergameno kc.
      </dif>
      <trd lng="fr">miniature</trd>
   </snc>
   <snc mrk="miniat.0o.artajxo">
      <dif>
         Malgranda arta objekto delikate prilaborita.
      </dif>
   </snc>
   <trd lng="be">&c_m;&c_ib;&c_n;&c_ib;&c_ja;&c_t;&c_u;&c_r;&c_a;</trd>
   <trd lng="de">Miniatur</trd>
   <trd lng="en">miniature</trd>
   <trd lng="hu">miniat&udblac;r</trd>
   <trd lng="nl">miniatuur</trd>
   <trd lng="ru">&c_m;&c_i;&c_n;&c_i;&c_a;&c_t;&c_ju;&c_r;&c_a;</trd>
</drv>

<drv mrk="miniat.0a">
   <kap><tld/>a</kap>
   <snc mrk="miniat.0a.arto">
      <uzo tip="fak">BELA</uzo>
      <dif>
         Rilata al <tld/>o<sncref ref="miniat.0o.bildo"/> a&ubreve;
         <tld/>o<sncref ref="miniat.0o.artajxo"/>.
      </dif>
      <trd lng="hu">miniat&udblac;r-</trd>	
   </snc>
   <snc mrk="miniat.0a.eta">
      <dif>
         Tre malgranda, <ref tip="sin" cel="et.0a">eta</ref>:
         <ekz>
            en la pordo de l' akceptejo aperis miniatura
            figureto de Zoja Ivanovna
            <fnt><bib>Metrop</bib>, <lok>&ccirc;ap. 2</lok></fnt>
         </ekz><ekz>
            <tld/>a lampo, radioricevilo
            <fnt><bib>PIV2</bib>, <lok>p. 744</lok></fnt>.
         </ekz>
      </dif>
      <trdgrp lng="hu">
         <trd>miniat&udblac;r <klr tip="ind">(picike)></klr></trd>,
         <trd>apr&oacute;cska</trd>,
         <trd>picike</trd>
      </trdgrp>
   </snc>
   <trd lng="be">&c_m;&c_ib;&c_n;&c_ib;&c_ja;&c_t;&c_u;&c_r;&c_n;&c_y;</trd>
   <trd 
lng="ru">&c_m;&c_i;&c_n;&c_i;&c_a;&c_t;&c_ju;&c_r;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="miniat.0igi">
   <kap><tld/>igi</kap>
   <snc mrk="miniat.0a.TEK">
      <uzo tip="fak">TEK</uzo>
      <dif>
         La&ubreve;eble malgrandigi la dimensiojn kaj la mason de
         aran&gcirc;a&jcirc;o (precipe elektronika) uzante pli densan muntadon,
         integrajn cirkvitojn ktp.
      </dif>
   </snc>
   <trd lng="be">&c_m;&c_ib;&c_n;&c_ib;&c_ja;&c_t;&c_u;&c_r;&c_y;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
   <trd lng="hu">miniat&uuml;riz&aacute;l</trd>       
   <trd 
lng="ru">&c_m;&c_i;&c_n;&c_i;&c_a;&c_t;&c_ju;&c_r;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
   <trd lng="fr">miniaturiser</trd>
</drv>

</art>
