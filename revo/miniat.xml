<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: miniat.xml,v 1.26 2025/02/19 18:35:31 revo Exp $">
   <kap>
      <ofc>1</ofc>
      <rad>miniatur</rad>/o
   </kap>

<drv mrk="miniat.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">BELA</uzo>
   <snc mrk="miniat.0o.bildo">
      <dif>
         Tre malgranda pentra&jcirc;o, plej ofte portreto, farita per
         akvarelaj
         koloroj sur eburo, metalo, pergameno kc.
      </dif>
      <trd lng="fr">miniature</trd>
   </snc>
   <snc mrk="miniat.0o.artajxo">
      <dif>
         Malgranda arta objekto delikate prilaborita.
      </dif>
   </snc>
  <trd lng="be">&c_m;&c_ib;&c_n;&c_ib;&c_ja;&c_t;&c_u;&c_r;&c_a;</trd>
  <trd lng="ca">miniatura</trd>
  <trd lng="cs">miniatura</trd>
  <trd lng="de">Miniatur</trd>
  <trd lng="en">miniature</trd>
  <trd lng="es">miniatura</trd>
   <trd lng="hu">miniat&udblac;r</trd>
  <trdgrp lng="ja">
    <trd>&#x7D30;&#x5BC6;&#x753B;  <pr>&#12373;&#12356;&#x307F;&#12388;&#x304C;</pr></trd>,
    <trd>&#x5F69;&#x98FE;&#x753B;  <pr>&#12354;&#12420;&#x304B;&#12374;&#x304C;</pr></trd>,
    <trd>&#x7DFB;&#x5BC6;&#x306A;&#x7D30;&#x5DE5;&#29289;  <pr>&#12385;&#x307F;&#12388;&#x306A;&#12373;&#12356;&#x304F;&#12406;&#12388;</pr></trd>,
    <trd>&#x30DF;&#x30CB;&#x30C1;&#x30E5;&#x30A2;</trd>,
    <trd>&#x30B5;&#x30E0;&#x30CD;&#x30A4;&#x30EB;</trd>
  </trdgrp>
  <trd lng="nl">miniatuur</trd>
  <trd lng="ru">&c_m;&c_i;&c_n;&c_i;&c_a;&c_t;&c_ju;&c_r;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>drobnoma&lcaron;ba</trd>,
    <trd>miniat&uacute;ra</trd>
  </trdgrp>
  <trd lng="uk">&#x43C;&c_ib;&#x43D;&c_ib;&c_a;&c_t;&#x44E;&c_r;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x7E2E;&#x5C0F;&#x6A21;&#x578B; <pr>su&#x14D;xi&#x1CE;om&#xF3;x&#xED;ng</pr></trd>,
    <trd>&#x7F29;&#x5C0F;&#x6A21;&#x578B; <pr>su&#x14D;xi&#x1CE;om&#xF3;x&#xED;ng</pr></trd>,
    <trd>&#x7E2E;&#x5F71; <pr>su&#x14D;y&#x1D0;ng</pr></trd>,
    <trd>&#x7F29;&#x5F71; <pr>su&#x14D;y&#x1D0;ng</pr></trd>,
    <trd>&#x5C0F;&#x753B;&#x50CF; <pr>xi&#x1CE;ohu&#xE0;xi&#xE0;ng</pr></trd>,
    <trd>&#x5C0F;&#x756B;&#x50CF; <pr>xi&#x1CE;ohu&#xE0;xi&#xE0;ng</pr></trd>,
    <trd>&#x7E2E;&#22294; <pr>su&#x14D;t&#xFA;</pr></trd>,
    <trd>&#x7F29;&#x56FE; <pr>su&#x14D;t&#xFA;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="miniat.0a">
   <kap><tld/>a</kap>
   <snc mrk="miniat.0a.arto">
      <uzo tip="fak">BELA</uzo>
      <dif>
         Rilata al <tld/>o<sncref ref="miniat.0o.bildo"/> a&ubreve;
         <tld/>o<sncref ref="miniat.0o.artajxo"/>.
      </dif>
      <trdgrp lng="ca">
        <trd>de miniatura</trd>,
        <trd>de miniatures</trd>
      </trdgrp>
      <trd lng="hu">miniat&udblac;r-</trd>
   </snc>
   <snc mrk="miniat.0a.eta">
      <dif>
         Tre malgranda, <ref tip="sin" cel="et.0a">eta</ref>:
         <ekz>
            en la pordo de l' akceptejo aperis miniatura
            figureto de Zoja Ivanovna
            <fnt><bib>Metrop</bib>, <lok>&ccirc;ap. 2</lok></fnt>
         </ekz><ekz>
            <tld/>a lampo, radioricevilo
            <fnt><bib>PIV2</bib>, <lok>p. 744</lok></fnt>.
         </ekz>
      </dif>
      <trd lng="ca">en miniatura</trd>
      <trdgrp lng="hu">
         <trd>miniat&udblac;r <klr tip="ind">(picike)&gt;</klr></trd>,
         <trd>apr&oacute;cska</trd>,
         <trd>picike</trd>
      </trdgrp>
   </snc>
  <trd lng="be">&c_m;&c_ib;&c_n;&c_ib;&c_ja;&c_t;&c_u;&c_r;&c_n;&c_y;</trd>
  <trd lng="cs">miniaturn&iacute;</trd>
  <trd lng="en">miniature</trd>
  <trd lng="en">miniatura</trd>
  <trdgrp lng="ja">
    <trd>&#x7D30;&#x5BC6;&#x753B;&#x306E;  <pr>&#12373;&#12356;&#x307F;&#12388;&#x304C;&#x306E;</pr></trd>,
    <trd>&#x5F69;&#x98FE;&#x753B;&#x306E;  <pr>&#12354;&#12420;&#x304B;&#12374;&#x304C;&#x306E;</pr></trd>,
    <trd>&#x6A21;&#x578B;&#x306E;  <pr>&#12418;&#12369;&#12356;&#x306E;</pr></trd>,
    <trd>&#x30DF;&#x30CB;&#x30C1;&#x30E5;&#x30A2;&#x306E; 
    <pr>&#x307F;&#x306B;&#12385;&#12421;&#12354;&#x306E;</pr></trd>,
    <trd>&#x5C0F;&#x578B;&#x306E;  <pr>&#12371;&#x304C;&#x305F;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_i;&c_n;&c_i;&c_a;&c_t;&c_ju;&c_r;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>drobn&yacute;</trd>,
    <trd>miniat&uacute;rny</trd>
  </trdgrp>
  <trd lng="uk">&#x43C;&c_ib;&#x43D;&c_ib;&c_a;&c_t;&#x44E;&c_r;&#x43D;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x5FAE;&#x7E2E; <pr>w&emacron;isu&#x14D;</pr></trd>,
    <trd>&#x5FAE;&#x7F29; <pr>w&emacron;isu&#x14D;</pr></trd>,
    <trd>&#x5FAE; <pr>w&emacron;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="miniat.0igi">
   <kap><tld/>igi</kap>
   <snc mrk="miniat.0a.TEK">
      <uzo tip="fak">TEK</uzo>
      <dif>
         La&ubreve;eble malgrandigi la dimensiojn kaj la mason de
         aran&gcirc;a&jcirc;o (precipe elektronika) uzante pli densan
         muntadon,
         integrajn cirkvitojn ktp.
      </dif>
   </snc>
  <trd lng="be">&c_m;&c_ib;&c_n;&c_ib;&c_ja;&c_t;&c_u;&c_r;&c_y;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">miniaturitzar</trd>
  <trd lng="cs">miniaturizovat</trd>
  <trd lng="en">miniaturize</trd>
  <trd lng="es">miniaturizar</trd>
  <trd lng="fr">miniaturiser</trd>
  <trd lng="hu">miniat&uuml;riz&aacute;l</trd>
  <trd lng="ru">&c_m;&c_i;&c_n;&c_i;&c_a;&c_t;&c_ju;&c_r;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">miniaturizova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&#x43E;&c_b;&c_i;&c_t;&c_i; &#x43C;&c_ib;&#x43D;&c_ib;&c_a;&c_t;&#x44E;&c_r;&#x43D;&c_i;&#x43C;</trd>,
    <trd>&#x43D;&c_a;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &#x43C;&c_ib;&#x43D;&c_ib;&c_a;&c_t;&#x44E;&c_r;&#x43D;&c_i;&c_h; &c_r;&#x43E;&c_z;&#x43C;&c_ib;&c_r;&c_ib;&c_v;</trd>
  </trdgrp>
</drv>

</art>
