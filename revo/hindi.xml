<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hindi.xml,v 1.7 2023/11/15 17:08:00 revo Exp $">
<kap>
  <rad>hindi</rad>/o
  <fnt><bib>RLN</bib></fnt>
</kap>

<drv mrk="hindi.0o">
  <kap>
    <tld lit="H"/>o
  </kap>
  <snc mrk="hindi.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
   <dif>
      <ref tip="dif" cel="hind.H0ujo">Hindujo</ref>:
      <ekz>
        kalendaroj de Persio, <tld lit="H"/>o, &Ccirc;inio
        <fnt>
          <bib>Monato</bib>
          <aut>Anna kaj Mati Pentus</aut>
          <vrk><url ref="&Monato;/2003/008070.html">Franda&jcirc;o por scivolemuloj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kiam angloj koloniis <tld lit="H"/>on, la oficiala lingvo de <tld lit="H"/>o
        fari&gcirc;is la angla
        <fnt>
          <bib>Monato</bib>
          <aut>Zlatko Ti&scaron;ljar</aut>
          <vrk><url ref="&Monato;/2008/009575.php">Unueco kaj diverseco</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la restintaj antikvaj manskriba&jcirc;oj pruvas, ke budhismo venis al Tohario
        (teritorio de la nuntempa Ta&gcirc;ikio) el norda <tld lit="H"/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Dina Newman kaj Firda&ubreve;s &Scirc;ukurov</aut>
          <vrk><url ref="&Monato;/2008/009332.php">Budhismo en Ta&gcirc;ikio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        detale priskribis la simptomojn de &hcirc;olero kuracistoj el <tld lit="H"/>o, kio
        estas komprenebla, &ccirc;ar la hinda duonkontinento estis konstanta ardejo de
        &hcirc;olero dum kelkaj jarmiloj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2010/010071.php">Antikva, sed ankora&ubreve; malica gasto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="barat.B0o">Barato</ref>
  </snc>
  <trd lng="bg">&c_I;&c_n;&c_d;&c_i;&c_ja;</trd>
  <trd lng="de">Indien</trd>
  <trdgrp lng="fr">
    <trd>Inde</trd>,
    <trd>R&eacute;publique de l'Inde</trd>
  </trdgrp>
  <trd lng="it">India</trd>
  <trdgrp lng="pl">
    <trd>Indie</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5370;&#x5EA6; <pr>Y&#xEC;nd&#xF9;</pr></trd>,
    <trd>&#x5929;&#x7AFA; <pr>ti&#x101;nzh&#xFA;</pr></trd>,
    <trd>&#x897F;&#x7AFA;&#x56FD; <pr>x&#x12B;zh&#xFA;gu&#xF3;</pr></trd>,
    <trd>&#x8EAB;&#x6BD2; <pr>yu&#xE1;nd&#xFA;</pr></trd>,
    <trd>&#x5370;&#x5EA6; <pr>y&#xEC;nd&#xF9;</pr></trd>
  </trdgrp>
</drv>

</art>

