<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: uleks.xml,v 1.25 2024/02/19 21:21:18 revo Exp $">
<kap>
   <rad>uleks</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="uleks.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
       tip="malprt" cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Ulex</trd>), arbusto kun dornoj kaj flavaj floroj,
      kreskanta en seka tero; anka&ubreve; nomata
      &leftquot;dorna <ref tip="sin" cel="genist.0o">genisto</ref>&rightquot;:
      <ekz>
        <tld/>o estas genro <klr>[&#8230;]</klr> de malaltaj arbedoj, kies floroj
        &ndash; ofte duopaj &ndash; havas dulipan kalikon kaj flavan, tipe pizoflorecan korolon,
        la folioj kaj multaj bran&ccirc;opintoj transformi&gcirc;as en vintro al da&ubreve;raj dornoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=36287">Ulekso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/c/c5/Ulex_cantabricus.jpg/600px-Ulex_cantabricus.jpg" prm="CC BY
    3.0">bran&ccirc;o de <tld/>o (Ulex gallii) en la Botanika &Gcirc;ardeno de Gij&oacute;n
        <fnt>
          <aut>Marioarg, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Ulex_cantabricus.jpg">&WCW;/File:Ulex_cantabricus.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_u;&c_l;&c_je;&c_k;&c_s;</trd>
  <trd lng="ca">gatosa</trd>
  <trd lng="cs">hlod&aacute;&scaron;</trd>
  <trd lng="de">Stechginster</trd>
  <trdgrp lng="en">
     <trd>gorse</trd>,
     <trd>furze</trd>,
     <trd>whin</trd>
   </trdgrp>
  <trd lng="es">aulaga</trd>
  <trd lng="fi">piikkiherne</trd>
  <trd lng="fr">ajonc</trd>
  <trdgrp lng="hu">
     <trd>s&uuml;nzan&oacute;t</trd>,
     <trd>s&uuml;lzan&oacute;t</trd>,
     <trd>t&ouml;viszan&oacute;t</trd>,
     <trd>t&uuml;sk&eacute;szan&oacute;t</trd>
   </trdgrp>
  <trdgrp lng="it">
   <trd>ulex</trd>,
   <trd>ginestra<klr> (di specie &amp;leftquotulex&amp;rightquot)</klr></trd>
   </trdgrp>
  <trd lng="ja">&#x30CF;&#x30EA;&#x30A8;&#x30CB;&#x30B7;&#x30C0;</trd>
  <trd lng="nl">gaspeldoorn</trd>
  <trd lng="pl">kolcolist</trd>
  <trd lng="ru">&c_u;&c_t;&c_jo;&c_s;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">kru&ccaron;inka t&racute;nist&aacute;</trd>
  <trd lng="uk">&c_u;&#x43B;&c_je;&#x43A;&c_s;</trd>
</drv>
</art>
