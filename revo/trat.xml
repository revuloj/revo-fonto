<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: trat.xml,v 1.21 2023/10/21 14:37:42 revo Exp $">
<kap>
  <rad>trat</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="trat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Speco de <ref tip="super" cel="kambi.0o">kambio</ref>,
      per kiu iu persono, nomata <tld/>anto, devigas
      alian personon, nomatan <tld/>ato, pagi je difinita tempo, ian
      monsumon la&ubreve; ordono de tria persono, nomata prenanto.
      <ekz>
        por bone funkciigi kambiojn anka&ubreve; internacie oni grandparte normigis kambian juron,
        ekzemple tio rilatas al la nepraj elementoj: <klr>[&#x2026;]</klr>
        en kazo de trato la nomo de la tratato <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=35973">Kambio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">trata</trd>
  <trdgrp lng="de">
    <trd>Tratte</trd>,
    <trd>gezogener <ind>Wechsel</ind></trd>
  </trdgrp>
  <trd lng="es">trato<klr> (comercial)</klr></trd>
  <trdgrp lng="hu">
    <trd>idegen <ind>v&aacute;lt&oacute;</ind></trd>,
    <trd>int&eacute;zv&eacute;ny</trd>,
    <trd>int&eacute;zv&eacute;nyezett <ind>v&aacute;lt&oacute;</ind></trd>
  </trdgrp>
  <trd lng="ja">&#x70BA;&#x66FF;&#x624B;&#x5F62;  <pr>&#x304B;&#x308F;&#x305B;&#x3066;&#x304C;&#x305F;</pr></trd>
  <trdgrp lng="nl">
    <trd>traite</trd>,
    <trd>wissel</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>trata</trd>,
    <trd><ind>weksel</ind> trasowany</trd>
  </trdgrp>
  <trd lng="pt">saque</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_a;&c_t;&c_t;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_o;&c_d;&c_n;&c_o;&c_j; &c_v;&c_je;&c_k;&c_s;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">trata</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x435;&#x440;&#x435;&#x43A;&#x430;&#x437;&#x43D;&#x438;&#x439; &#x432;&#x435;&#x43A;&#x441;&#x435;&#x43B;&#x44C;</trd>,
    <trd>&#x442;&#x440;&#x430;&#x442;&#x442;&#x430;</trd>
  </trdgrp>
</drv>

<drv mrk="trat.0i">
  <kap><tld/>i</kap>
  <snc>
    <dif>
      Fari <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>tratovat sm&ecaron;nku</trd>,
    <trd>vystavit sm&ecaron;nku</trd>
  </trdgrp>
  <trd lng="de">einen <ind>Wechsel</ind> ziehen</trd>
  <trd lng="es">tratar, hacer un trato<klr tip="amb"> (entorno
  comercial)</klr></trd>
  <trdgrp lng="hu">
    <trd>int&eacute;zv&eacute;nyez</trd>,
    <trd>v&aacute;lt&oacute;t kibocs&aacute;t</trd>
  </trdgrp>
  <trd lng="ja">&#x70BA;&#x66FF;&#x624B;&#x5F62;&#x3092;&#x632F;&#x51FA;&#x3059;  <pr>&#x304B;&#x308F;&#x305B;&#x3066;&#x304C;&#x305F;&#x3092;&#x3075;&#x308A;&#x3060;&#x3059;</pr></trd>
  <trd lng="nl">trekken<klr tip="ind"> (een wissel)</klr></trd>
  <trd lng="pl">wystawia&cacute; weksel</trd>
  <trd lng="pt">sacar</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol; &c_v;&c_je;&c_k;&c_s;&c_je;&c_l;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>tratu</trd>,
    <trd>vystavi&tcaron; cudziu zmenku</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x435;&#x440;&#x435;&#x43A;&#x430;&#x437;&#x443;&#x432;&#x430;&#x442;&#x438; &#x432;&#x435;&#x43A;&#x441;&#x435;&#x43B;&#x44C;</trd>
</drv>

<drv mrk="trat.0anto">
  <kap><tld/>anto</kap>
  <snc>
    <dif>
      Tiu, kiu <tld/>as la kambion.
    </dif>
  </snc>
  <trd lng="cs">sm&ecaron;nkovatel</trd>
  <trdgrp lng="de">
    <trd>Trassant</trd>,
    <trd>Aussteller einer <ind>Tratte</ind></trd>
  </trdgrp>
  <trd lng="es">tratante</trd>
  <trdgrp lng="hu">
    <trd>int&eacute;zv&eacute;nyez&odblac;</trd>,
    <trd>kibocs&aacute;t&oacute;<klr tip="ind"> (v&aacute;lt&oacute;&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x632F;&#x51FA;&#x4EBA;  <pr>&#x3075;&#x308A;&#x3060;&#x3057;&#x306B;&#x3093;</pr></trd>
  <trd lng="nl">trekker<klr tip="ind"> (v.e.wissel)</klr></trd>
  <trdgrp lng="pl">
    <trd>trasant</trd>,
    <trd>wystawca weksla</trd>
  </trdgrp>
  <trd lng="pt">sacador</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_k;&c_s;&c_je;&c_l;&c_je;&c_d;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_t;&c_r;&c_a;&c_s;&c_s;&c_a;&c_n;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>trasant</trd>,
    <trd>zmenkovate&lcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="trat.0ato">
  <kap><tld/>ato</kap>
  <snc>
    <dif>
      Tiu, je kies nomo la kambio estas <tld/>ata.
    </dif>
    <refgrp tip="sin">
      <ref cel="kambi.0o">kambio</ref>,
      <ref cel="rimes.0o">rimeso</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">sm&ecaron;ne&ccaron;n&iacute;k</trd>
  <trdgrp lng="de">
    <trd>Trassat</trd>,
    <trd>Bezogener</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>int&eacute;zv&eacute;nyezett</trd>,
    <trd>c&iacute;mzett<klr tip="ind"> (v&aacute;lt&oacute;&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x652F;&#x6255;&#x4EBA;  <pr>&#x3057;&#x306F;&#x3089;&#x3044;&#x3058;&#x3093;</pr></trd>
  <trdgrp lng="nl">
    <trd>betrokkene<klr tip="ind"> (v.e.wissel)</klr></trd>,
    <trd>trassaat</trd>
  </trdgrp>
  <trd lng="pl">trasat</trd>
  <trd lng="pt">sacado</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_shch;&c_i;&c_k;</trd>,
    <trd>&c_t;&c_r;&c_a;&c_s;&c_s;&c_a;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>tras&aacute;t</trd>,
    <trd>zmene&ccaron;n&iacute;k</trd>,
    <trd>zmenkovn&iacute;k</trd>
  </trdgrp>
</drv>
</art>
