<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>katekiz</rad>/i
</kap>
<drv mrk="katekiz.0i">
  <kap><ofc>3</ofc><tld/>i</kap>
  <snc mrk="katekiz.0i.REL">
  <uzo tip="fak">REL</uzo>
    <ref tip="dif" cel="katehx2.0i">kate&hcirc;izi</ref>
  </snc>
  <trd lng="en">catechize</trd>
  <trdgrp lng="fr">
    <trd>cat&eacute;chiser</trd>,
    <trd>faire le <ind>cat&eacute;chisme</ind></trd>
  </trdgrp>
  <trd lng="nl">catechiseren</trd>
</drv>

<drv mrk="katekiz.0o">
  <kap><tld/>o</kap>
  <snc mrk="katekiz.0o.REL">
  <uzo tip="fak">REL</uzo>
    <ref tip="dif" cel="katehx2.0o">kate&hcirc;izo</ref>
  </snc>
  <trdgrp lng="fr">
    <trd>cat&eacute;chisation</trd>,
    <trd>cat&eacute;chisme<klr> (enseignement)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="katekiz.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="katekiz.0isto.REL">
  <uzo tip="fak">REL</uzo>
    <ref tip="dif" cel="katehx2.0isto">kate&hcirc;izisto</ref>
  </snc>
  <trd lng="de">Katechist</trd>
  <trd lng="en">catechist</trd>
  <trdgrp lng="hu">
    <trd>hitoktat&oacute;</trd>,
    <trd>katek&eacute;ta</trd>
  </trdgrp>
  <trd lng="nl">catecheet</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_r;&c_o;&c_u;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_a;&c_v;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>

