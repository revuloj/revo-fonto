<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&ccirc;akon</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="cxakon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="cxakon.0o.danco">
    <dif>
      E&ubreve;ropa <ref tip="super" cel="danc.0o">danco</ref>
hispan-devena,
      tri-takta, jam nur muzike uzata en la 17a jarcento.
    </dif>
  </snc>
  <snc mrk="cxakon.0o.muzikajxo">
    <dif>
      Muzika&jcirc;o tri-takta, malrapida, konsistanta el
      temo kaj varia&jcirc;oj akompanataj de ripeta baso,
      foje uzata kiel parto de <ref tip="malprt" cel="suit.0o">suito</ref>.
    </dif>
  </snc>
  <trd lng="fr">chaconne</trd>
</drv>
</art>

