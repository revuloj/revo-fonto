<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&ccirc;iom</rad>
</kap>
<drv mrk="cxiom.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <uzo tip="klr">(adv)</uzo>
    <dif>
      Tutkvante; &ccirc;io, la&ubreve; kvanta rilato:
      <ekz>
        donu al la lupo
        iom, li postulos <tld/> (<ref tip="sin" cel="avid.0a">avida</ref>);
      </ekz>
      <ekz>
        <tld/> li prenis.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_s;&c_jo;</trd>
  <trdgrp lng="de">
    <trd>alles</trd>,
    <trd>ganz und gar</trd>
  </trdgrp>
  <trd lng="he">&he;&kaf;&lamed;</trd>
  <trdgrp lng="hu">
    <trd>minden</trd>,
    <trd>&ouml;sszes</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>tutto</trd>,
    <trd>tutto<klr> quanto</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_s;&c_jo; <klr>(&c_o; 
&c_k;&c_o;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_je;)</klr></trd>
  <trd lng="sk">v&scaron;etko <klr>(&ccaron;&iacute;slovka)</klr></trd>
  <trd lng="sv">alltsammans</trd>
</drv>

<drv mrk="cxiom.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Tutkvanta, tuta.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_v;&c_je;&c_s;&c_mol;</trd>,
    <trd>&c_c;&c_a;&c_l;&c_k;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>en <ind>tout</ind></trd>,
    <trd>au <ind>total</ind></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&kaf;&vav;&lamed;&lamed;</trd>,
    <trd>&mem;&lamed;&alef;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>teljes</trd>,
    <trd>&ouml;sszes</trd>,
    <trd>tot&aacute;lis</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_s;&c_mol;</trd>,
    <trd>&c_c;&c_je;&c_l;&c_i;&c_k;&c_o;&c_m;</trd>
  </trdgrp>
</drv>
</art>
