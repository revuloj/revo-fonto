<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>motacil</rad>/o
</kap>
<drv mrk="motaci.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="motaci.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Motacilla</trd>).
    </dif>
  </snc>
  <trd lng="cs">konipas</trd>
  <trd lng="en">wagtail</trd>
  <trd lng="de">Bachstelze <klr>(kaj.a.)</klr></trd>
  <trd lng="hu">billeget&odacute;</trd>
  <trd lng="pl">pliszka</trd>
  <trd lng="ru">&c_t;&c_r;&c_ja;&c_s;&c_o;&c_g;&c_u;&c_z;&c_k;&c_a;</trd>
</drv>
<drv mrk="motaci.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="paser.0oformaj">paseroformaj
      birdoj</ref> (<trd lng="la">Motacillidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Stelzen <klr tip="ind">(Vogelfamilie)</klr></trd>
</drv>
</art>
