<drv mrk="posed.0anto">
  <kap>
    <tld/>anto
  </kap>
  <snc>
    <dif>
      Tiu, kiu <tld/>as ion:
      <ekz>
        la domoj, kiujn li konstruas, restas al la <tld/>antoj por eterne
        <fnt><bib>Hamlet</bib></fnt>;
      </ekz><ekz>
        via edzo estas <tld/>anto de la magazeno en rilato jura, sed vi administras la
        aferojn kune kun li
        <fnt><bib>Marta</bib></fnt>;
      </ekz><ekz>
        al <tld/>anto de metio mankas nenio
        <fnt><bib>PrV</bib></fnt>;
      </ekz><ekz>
        <tld/>anto de malsano, / ka&scirc;inda por la mondo
        <fnt><bib>Hamlet</bib></fnt>;
      </ekz>
    </dif>
  </snc>
  <trd lng="bo">&t_ba;&t_da;&t_ga;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>
  <trd lng="de">Besitzer</trd>
  <trd lng="en">possessor</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_l;&c_a;&c_d;&c_je;&c_l;&c_je;&c_c;</trd>,
    <trd>&c_o;&c_b;&c_l;&c_a;&c_d;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="th">&#3648;&#3592;&#3657;&#3634;&#3586;&#3629;&#3591;</trd>
  <trd lng="uk">&c_v;&c_l;&c_a;&c_s;&c_n;&c_i;&c_k;</trd>
</drv>

<drv mrk="posed.0igi">
  <kap>
    <tld/>igi
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="don.trans0i">Transdoni</ref> al ies <tld/>o:
      <ekz>
        li <tld/>igos al ili la landon
        <fnt><bib>MT</bib> <lok>&Rea; 3:28</lok></fnt>;
      </ekz><ekz>
        tiu &ccirc;i sinjoro eble estis siatempe granda a&ccirc;etisto de bienoj, kun &ccirc;iaj
        hipotekoj, depagoj <klr>[&#8230;]</klr> kaj ju&gcirc;aj <tld/>igoj
        <fnt><bib>Hamlet</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>&uuml;bertragen <klr>(von Besitz)</klr></trd>,
    <trd>&uuml;bereignen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_d;&c_je;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_d;&c_a;&c_t;&c_mol; &c_v;&c_o; &c_v;&c_l;&c_a;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_a;&c_d;&c_ib;&c_l;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &c_u; &c_v;&c_l;&c_a;&c_s;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="posed.ek0i">
  <kap>ek<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="posed.ek0i.JUR">
    <dif>
      I&gcirc;i <tld/>anto de:
      <ekz>
        <nom>Izrael</nom> frapis lin per la glavo, kaj ek<tld/>is lian landon
        <fnt><bib>MT</bib> <lok>Nombroj 21:24</lok></fnt>;
      </ekz><ekz>
        ili ek<tld/>is la urbon de Palmoj
        <fnt><bib>MT</bib> <lok>&Jug; 3:13</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="okup.0i">okupi</ref>
    <trd lng="de">besetzen</trd>
  </snc>
  <snc mrk="posed.ek0i.okupi">
    <dif>
      <ref cel="absorb.0i">Absorbi</ref>, okupi, preni la tutan atenton de:
      <ekz>
        &ccirc;i tiu penso ek<tld/>is min tute
        <fnt><bib>VivZam</bib></fnt>;
      </ekz><ekz>
        &ccirc;iujn viajn arbojn kaj la fruktojn de via tero ek<tld/>os la insektoj
        <fnt><bib>MT</bib> <lok>&Rea; 28:42</lok></fnt>;
      </ekz><ekz>
        la frenezo ek<tld/>u &scirc;ian animon!
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>
      </ekz><ekz>
        per la incensoj li tiel ek<tld/>is la poeton, ke &ccirc;i tiu tute perdis la spiritan
        ekvilibron, forgesis sian celon, <klr>[&#8230;]</klr> &ccirc;ion, e&ccirc; sin mem
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz><ekz>
        malbona spirito ek<tld/>is vin
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hant.0i">hanti</ref>,
      <ref cel="obsed.0i">obsedi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>befallen</trd>,
      <trd>heimsuchen</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_a;&c_v;&c_a;&c_l;&c_o;&c_d;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;evz&iacute;t do dr&zcaron;by</trd>,
    <trd>ujmout se dr&zcaron;by</trd>,
    <trd>zmocnit se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Besitz <ind>ergreifen</ind></trd>,
    <trd>in Besitz <ind>nehmen</ind></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd><ind>apropiar</ind>se de</trd>,
    <trd>tomar <ind>posesi&oacute;n</ind> de</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>emparer</ind> de</trd>,
    <trd>prendre <ind>possession</ind> de</trd>
  </trdgrp>
  <trd lng="hu">birtokba vesz</trd>
  <trd lng="nl">bezit nemen</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_v;&c_l;&c_a;&c_d;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_l;&c_a;&c_d;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zmocni&tcaron; sa</trd>
  <trd lng="uk">&c_z;&c_a;&c_v;&c_o;&c_l;&c_o;&c_d;&c_ib;&c_t;&c_i;</trd>
</drv>

<drv mrk="posed.kun0i">
  <kap>kun<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="P"/>i komune kun iu(j):
      <ekz>
        <nom>Zamenhof</nom> efektive tiutempe kun<tld/>is la lernejon por judaj
        knabinoj
        <fnt><aut>Zbigniew Romaniuk, tr. Jaros&lstroke;aw Parzyszek</aut>:
          <vrk>Nekonataj faktoj pri la Zamenhofa familio</vrk>.
          <bib>LOdE</bib>, <lok>&#8470;112 (2004:2)</lok></fnt>;
      </ekz><ekz>
        la <nom>la Mopani Copper Mines (MCM)</nom>, kun<tld/>ata de la
        svisa grupo <nom>Glencore</nom> kaj de la kanada
        <nom>First Quantum</nom> <klr>&#8230;</klr>
        <fnt><lok>Le Monde diplomatique en Esperanto</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="hav.kun0i">kunhavi</ref>
  </snc>
  <trdgrp lng="fr">
    <trd>coposs&eacute;der</trd>,
    <trd>poss&eacute;der en commun</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_v;&c_l;&c_a;&c_d;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_l;&c_a;&c_d;&c_je;&c_t;&c_mol; &c_s;&c_o;&c_v;&c_m;&c_je;&c_s;&c_t;&c_n;&c_o;</trd>
  </trdgrp>
</drv>

