<drv mrk="posed.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>ado:
      <ekz>
        <tld/>a dokumento
        <klr>(pruvanta la <tld/>on)</klr>;
      </ekz><ekz>
        ter<tld/>a aristokratio
        <fnt>
          <bib>MD02</bib>
          <vrk>Kiel la potenculoj fari&gcirc;is vere potencaj</vrk>
        </fnt>;
      </ekz><ekz>
        sklav<tld/>aj koloniistoj
        <fnt>
          <bib>MD11</bib>
          <vrk>La liberalismo &ndash; ideologio de la mastroj</vrk>
        </fnt>;
      </ekz><ekz>
        nia amo ne estas <tld/>a
        <klr>(do libera)</klr>
        <fnt><bib>ChV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_a;&c_jo;&c_m;&c_a;&c_s;&c_n;&c_y;</trd>
  </snc>
  <snc mrk="posed.0a.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      Esprimanta rilatojn de <tld/>o:
      <ekz>
        <ind><tld/>a pronomo</ind>
        <klr>(montranta la <tld/>anton)</klr>;
        <trd lng="be">&c_p;&c_r;&c_y;&c_n;&c_a;&c_l;&c_je;&c_zh;&c_n;&c_y; <ind>&c_z;&c_a;&c_j;&c_m;&c_je;&c_n;&c_mol;&c_n;&c_ib;&c_k;</ind></trd>
        <trd lng="de">Possesivpronomen</trd>
        <trd lng="hu">birtokos n&eacute;vm&aacute;s</trd>
        <trd lng="nl">bezittelijk <ind>voornaamwoord</ind></trd>
        <trd lng="pl">zaimek dzier&zdot;awczy</trd>
        <trd lng="ru">&c_p;&c_r;&c_i;&c_t;&c_ja;&c_zh;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je; &c_m;&c_je;&c_s;&c_t;&c_o;&c_i;&c_m;&c_je;&c_n;&c_i;&c_je;</trd>
      </ekz><ekz>
        la pronomoj <tld/>aj estas formataj per la aldono de la fini&gcirc;o adjektiva
        <fnt><bib>FK</bib></fnt>;
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_r;&c_y;&c_n;&c_a;&c_l;&c_je;&c_zh;&c_n;&c_y;</trd>
  </snc>
  <trd lng="cs">p&rcaron;ivlast&ncaron;ovac&iacute;</trd>
