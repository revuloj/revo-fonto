<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>dut</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="dut.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="dut.0o.intervalo">
    <dif>
      <ref tip="super" cel="interv.0o.MUZ">Intervalo</ref>
      inter du sinsekvaj tonoj de diatona gamo.
    </dif>
  </snc>
  <snc mrk="dut.0o.de.toniko">
    <dif>
      Dua tono de gamo.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>seconde<klr> (intervalle musical)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="dut.duon0o">
  <kap>duon<tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="interv.0o.MUZ">Intervalo</ref>
      egalanta dekduonon de okto, ekzemple E-F en temperita
      okcidenta tonalo.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>demi-ton<klr> (intervalle musical)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="dut.plen0o">
  <kap>plen<tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="interv.0o.MUZ">Intervalo</ref>
      egalanta sesonon de okto, ekzemple C-D en temperita
      okcidenta tonalo.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>ton<klr> (intervalle musical)</klr></trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
-->
</vortaro>