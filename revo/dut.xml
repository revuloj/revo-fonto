<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dut.xml,v 1.15 2025/08/05 22:35:40 revo Exp $">
<kap>
  <rad>dut</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>

<drv mrk="dut.0o">
  <kap><tld/>o</kap>
  <snc mrk="dut.0o.intervalo">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="interv.0o.MUZ">Intervalo</ref>
      inter du sinsekvaj tonoj de diatona gamo:
      <ekz>
        la klavicenisto Ralph Kirkpatrick priskribas tion kiel havanta
        &leftquot;preska&ubreve; nostalgian karesemon&rightquot; &mdash; tion ka&ubreve;zas ver&scirc;ajne
        la aparteco je <tld/>o inter sekvanto kaj kondukanto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=32931">Goldberg-variacioj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj instrumentoj havas kvarto- a&ubreve; <tld/>o-valvon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=32935">Trombono</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="dut.0o.de_toniko">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Dua tono de gamo.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_k;&c_u;&c_n;&c_d;&c_a; <klr tip="amb">(&c_m;&c_u;&c_z;.)</klr></trd>
  <trd lng="bg">&c_S;&c_je;&c_k;&c_u;&c_n;&c_d;&c_a;<klr tip="amb"> (&c_i;&c_n;&c_t;&c_je;&c_r;&c_v;&c_a;&c_l;)</klr></trd>
  <trd lng="cs">sekunda <klr>(hud. interval)</klr></trd>
  <trd lng="de">Sekunde<klr tip="amb"> (Musik)</klr></trd>
  <trd lng="en">second<klr tip="amb"> (musical interval)</klr></trd>
  <trd lng="fr">seconde<klr> (intervalle musical)</klr></trd>
  <trd lng="ja">&#x4E8C;&#x5EA6;&#x97F3;&#x7A0B;  <pr>&#12405;&#x305F;&#12393;&#x304A;&#12435;&#12390;&#12356;</pr></trd>
  <trd lng="pl">sekunda  <klr>(muz.)</klr></trd>
  <trd lng="ru">&c_s;&c_je;&c_k;&c_u;&c_n;&c_d;&c_a;<klr tip="amb"> (&c_m;&c_u;&c_z;. &c_i;&c_n;&c_t;&c_je;&c_r;&c_v;&c_a;&c_l;)</klr></trd>
  <trd lng="sk">sekunda</trd>
  <trd lng="uk">&c_s;&c_je;&#x43A;&c_u;&#x43D;&c_d;&c_a;</trd>
</drv>

<drv mrk="dut.duon0o">
  <kap>duon<tld/>o</kap>
  <snc mrk="dut.duon0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="interv.0o.MUZ">Intervalo</ref>
      egalanta dekduonon de okto, ekzemple E-F en temperita
      okcidenta tonalo:
      <ekz>
        Mario kunpremas la du fingrojn, precizigas la duon<tld/>on,
        kaj ree lia ar&ccirc;o glitas sur liberaj kordoj
        <fnt>
          <aut>&Scirc;. Urban</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr46/papilioj.html">
          Mario kaj la papilioj</url></vrk>,
          <lok>Nica Literatura Revuo, 1959-97 &gcirc;is 08 (4/6)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_w;&c_t;&c_o;&c_n; <klr>(&c_m;&c_u;&c_z;.)</klr></trd>,
    <trd>&c_m;&c_a;&c_l;&c_a;&c_ja; <ind>&c_s;&c_e;&c_k;&c_u;&c_n;&c_d;&c_a;</ind> <klr>(&c_m;&c_u;&c_z;.)</klr></trd>
  </trdgrp>
  <trd lng="cs">nejmen&scaron;&iacute; interval v Z&aacute;padn&iacute; hudb&ecaron;</trd>
  <trdgrp lng="de">
    <trd>kleine <ind>Sekunde</ind></trd>,
    <trd>Halbton</trd>
  </trdgrp>
  <trd lng="en">semitone</trd>
  <trd lng="fr">demi-ton<klr> (intervalle musical)</klr></trd>
  <trdgrp lng="pl">
    <trd>p&oacute;&lstroke;ton</trd>,
    <trd>sekunda ma&lstroke;a <klr>(muz.)</klr></trd>
  </trdgrp>
  <trd lng="pt">segunda</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_t;&c_o;&c_n;<klr tip="amb"> (&c_m;&c_u;&c_z;. &c_i;&c_n;&c_t;&c_je;&c_r;&c_v;&c_a;&c_l;)</klr></trd>
  <trd lng="sk">najmen&scaron;&iacute; interval v Z&aacute;padnej hudbe</trd>
</drv>

<drv mrk="dut.plen0o">
  <kap>plen<tld/>o</kap>
  <snc mrk="dut.plen0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="interv.0o.MUZ">Intervalo</ref>
      egalanta sesonon de okto, ekzemple C-D en temperita
      okcidenta tonalo:
      <ekz>
        la dua parto envenas je plen<tld/>o pli alte ol la unua
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=32931">Goldberg-variacioj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_o;&c_w;&c_n;&c_y; <ind>&c_t;&c_o;&c_n;</ind> <klr>(&c_m;&c_u;&c_z;.)</klr></trd>,
    <trd>&c_c;&c_e;&c_l;&c_y; <ind>&c_t;&c_o;&c_n;</ind> <klr>(&c_m;&c_u;&c_z;.)</klr></trd>,
    <trd>&c_v;&c_ja;&c_l;&c_ib;&c_k;&c_a;&c_ja; <ind>&c_s;&c_e;&c_k;&c_u;&c_n;&c_d;&c_a;</ind> <klr>(&c_m;&c_u;&c_z;.)</klr></trd>
  </trdgrp>
  <trd lng="cs">nejv&ecaron;t&scaron;&iacute; interval</trd>
  <trdgrp lng="de">
    <trd>gro&szlig;e <ind>Sekunde</ind></trd>,
    <trd>Ganzton</trd>
  </trdgrp>
  <trd lng="en">whole tone<klr tip="amb"> (musical interval)</klr></trd>
  <trd lng="fr">ton<klr> (intervalle musical)</klr></trd>
  <trdgrp lng="pl">
    <trd>pe&lstroke;ny <ind>ton</ind> <klr>(muz.)</klr></trd>,
    <trd>sekunda wielka <klr>(muz.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_o;&c_n;<klr tip="amb"> (&c_m;&c_u;&c_z;. &c_i;&c_n;&c_t;&c_je;&c_r;&c_v;&c_a;&c_l;)</klr></trd>
  <trd lng="sk">najv&auml;&ccaron;&scaron;&iacute; interval</trd>
</drv>
</art>

