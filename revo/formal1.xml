<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: formal1.xml,v 1.20 2023/11/10 06:07:59 revo Exp $">
<kap>
  <rad>formaldehid</rad>/o
</kap>
<drv mrk="formal1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      H<sub>2</sub>CO,
      organika <ref tip="super" cel="kombin.0ajxo.KEM">kombina&jcirc;o</ref>, 
      senkolora <ref tip="super" cel="gas.0o.FIZ">gaso</ref> kun neagrabla incitanta odoro:
      <ekz><tld/>o estas forta desinfektilo;</ekz>
      <ekz>
        <klr>[el metanolo]</klr> oni grandkvante produktas <klr>[&#x2026;]</klr> <tld/>on por
        sintezaj rezinoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=88174">Metanolo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_r;&c_m;&c_a;&c_l;&c_mol;&c_d;&c_e;&c_g;&c_ib;&c_d;</trd>
  <trd lng="bg">&c_f;&c_o;&c_r;&c_m;&c_a;&c_l;&c_d;&c_je;&c_h;&c_i;&c_d;</trd>
  <trd lng="ca">formaldehid</trd>
  <trdgrp lng="cs">
    <trd>formaldehyd</trd>,
    <trd>metanal</trd>,
    <trd>metanol</trd>,
    <trd>mraven&ccaron;&iacute; aldehyd</trd>
  </trdgrp>
  <trd lng="de">Formaldehyd</trd>
  <trd lng="es">formaldeh&iacute;do</trd>
  <trd lng="fr">formald&eacute;hyde</trd>
  <trd lng="hu">formaldehid</trd>
  <trd lng="ja">&#x30DB;&#x30EB;&#x30E0;&#x30A2;&#x30EB;&#x30C7;&#x30D2;&#x30C9;</trd>
  <trd lng="nl">formaldehyde</trd>
  <trd lng="pl">formaldehyd</trd>
  <trdgrp lng="pt">
    <trd>alde&iacute;do f&oacute;rmico</trd>,
    <trd>formalde&iacute;do</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_o;&c_r;&c_m;&c_a;&c_l;&c_mol;&c_d;&c_je;&c_g;&c_i;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>formaldehyd</trd>,
    <trd>metanal</trd>
  </trdgrp>
  <trd lng="sv">formaldehyd</trd>
  <trd lng="uk">&c_f;&c_o;&c_r;&c_m;&c_a;&c_l;&c_mol;&c_d;&c_je;&c_g;&c_ib;&c_d;</trd>
  <trd lng="zh">&#x7532;&#x919B; <pr>ji&#x1CE;qu&#xE1;n</pr></trd>
</drv>
</art>
