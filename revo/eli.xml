<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: eli.xml,v 1.12 2023/09/14 10:30:17 revo Exp $">
<kap>
  <rad>Eli</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="eli.0o">
  <kap><tld/>o</kap>
  <snc mrk="eli.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Asimilita vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref>
      responda al la biblia
      Elija:
      <ekz>
        la popolo kredas, ke kiam tondras, la profeto <tld/>o
        promenas en la &ccirc;ielo en kale&scirc;o
        <fnt>
          <bib>PatrojFiloj</bib>,
          <lok>&ccirc;apitro 10a</lok>
        </fnt>;
      </ekz>
      <ekz>
        Elio Izgur (1881&ndash;1937) kaj Viktoro Kol&ccirc;inski
        (1904&ndash;1937) dronis en preska&ubreve; kompleta forgeso,
        &ccirc;ar iliaj verkoj jam
        delonge ne plu estas haveblaj
        <fnt>
          <aut>V. Markov</aut>,
          <vrk><url ref="http://satesperanto.free.fr/satkulturo/article.php3?id_article=278">
          Niaj karaj &leftquot;eminentuloj&rightquot;: kelkaj rimarkindaj
          SAT-anoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Elias</trd>
    <trd lng="en">Elias</trd>
    <trd lng="fr">&Eacute;lie</trd>
    <trd lng="it">Elia</trd>
    <trd lng="pl">Eliasz</trd>
    <trd lng="pt">Elias</trd>
    <trd lng="ru">&c_I;&c_l;&c_mol;&c_ja;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_G;&c_a;&c_l;&c_mol;&c_ja;&c_sh;</trd>,
    <trd>&c_Ib;&c_l;&c_mol;&c_l;&c_ja;</trd>
  </trdgrp>
  <trd lng="cs">Ilja</trd>
  <trd lng="sk">I&#x13E;ja</trd>
  <trd lng="uk">&c_Ib;&#x43B;&#x43B;&#x44F;</trd>
</drv>
</art>

