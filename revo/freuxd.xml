<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>Fre&ubreve;d</rad>/o</kap>
<drv mrk="freuxd.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Germanlingve: Sigmund <trd lng="de">Freud</trd> (1856-1939).
      A&ubreve;stra psikiatro kaj psikologo, la fondinto de la
      <ref tip="vid" cel="psik.0analizo">psikanalizo</ref>.
    </dif>
  </snc>
  <trd lng="fr">Freud</trd>
  <trd lng="nl">Sigmund <ind>Freud</ind></trd>
  <trd lng="ru">&c_F;&c_r;&c_je;&c_j;&c_d;</trd>
</drv>

<drv mrk="freuxd.f0a">
  <kap><tld lit="f"/>a</kap>
  <snc>
    <dif>
      Iel rilatanta kun la konceptoj de <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">freudien</trd>
  <trd lng="nl">freudiaans</trd>
  <trd lng="ru">&c_f;&c_r;&c_je;&c_j;&c_d;&c_o;&c_v;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="freuxd.f0ismo">
  <kap><tld lit="f"/>ismo</kap>
  <snc>
    <dif>
      Doktrino, iniciita de <tld/>o, pri la erotika origino de &ccirc;iaj homaj
      instinktoj kaj sentoj.
    </dif>
    <ref tip="vid" cel="libid.0o">libido</ref>
  </snc>
  <trd lng="fr">freudisme</trd>
  <trd lng="nl">freudianisme</trd>
  <trd lng="ru">&c_f;&c_r;&c_je;&c_j;&c_d;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
