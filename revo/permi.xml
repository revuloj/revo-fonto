      <rad>permi</rad>/o
    </kap>

<drv mrk="permi.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">GEOL</uzo>
    <dif>
      Terhistoria <ref tip="lst" val="299000000" lst="voko:geologiaj_epokoj" cel="epok.0o.periodo">epoko</ref>
      post la Karbonio ekde 299 &gcirc;is 251 milionoj da jaroj, komenco de
      Triaso:
      <ekz>
        ginko aperis en la <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18965">Ginko</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        je la komenco de <tld lit="P"/>o la Tero ankora&ubreve; enhavis nur unu
        grandan
        kontinenton, Pangeon, &ccirc;irka&ubreve;atan de la granda oceano Pantalaso
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=138854">Permio</url></lok>
        </fnt>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_je;&c_r;&c_m;&c_s;&c_k;&c_ib; &c_p;&c_je;&c_r;&c_y;&c_ja;&c_d;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_m;</trd>
    </trdgrp>
    <trd lng="de">Perm</trd>
    <trd lng="fr">Permien</trd>
    <trd lng="pl">Perm</trd>
    <trd lng="ro">Permian</trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x4E8C;&#x7573;&#x7D00;  [<ind>&#x306B;&#x3058;&#x3087;&#x3046;&#x304D;</ind>]</trd>,
    <trd>&#x30DA;&#x30EB;&#x30E0;&#x7D00;  [<ind>&#x30DA;&#x30EB;&#x30E0;&#x304D;</ind>]</trd>,
    <trd>&#x4E8C;&#x7573;&#x7CFB;  [<ind>&#x3075;&#x305F;&#x3058;&#x3087;&#x3046;&#x3051;&#x3044;</ind>]</trd>,
    <trd>&#x30DA;&#x30EB;&#x30E0;&#x7CFB;  [<ind>&#x30DA;&#x30EB;&#x30E0;&#x3051;&#x3044;</ind>]</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x435;&#x440;&#x43C;&#x441;&#x44C;&#x43A;&#x438;&#x439; &#x43F;&#x435;&#x440;&#x456;&#x43E;&#x434;</trd>
</drv>

