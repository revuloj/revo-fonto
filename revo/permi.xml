<drv mrk="permi.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Terhistoria <ref tip="lst" val="299000000" lst="voko:geologiaj_epokoj" cel="epok.0o.periodo">epoko</ref>
      post la Karbonio ekde 299 &gcirc;is 251 milionoj da jaroj, komenco de Triaso:
      <ekz>
        ginko aperis en la <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18965"
              >Ginko</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        je la komenco de <tld lit="P"/>o la Tero ankora&ubreve; enhavis nur unu grandan
        kontinenton, Pangeon, &ccirc;irka&ubreve;atan de la granda oceano Pantalaso
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=138854"
              >Permio</url></lok>
        </fnt>
      </ekz>
    </dif>
    <trd lng="de">Perm</trd>
  </snc>
