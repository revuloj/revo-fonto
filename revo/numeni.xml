<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: numeni.xml,v 1.11 2024/02/07 09:21:23 revo Exp $">
<kap>
  <rad>numeni</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="numeni.0o">
  <kap><tld/>o</kap>
  <snc mrk="numeni.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Mezgranda <ref tip="lst" lst="voko:birdoj" cel="bird.0o.ZOO">birdo</ref> kun longaj kruroj,
      bruna korpo kaj longa malsupren kurbigita beko, vivanta en mezvarmaj a&ubreve; malvarmaj landoj
      en prefere malsekaj senarbejoj; <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio <ref tip="malprt"
      cel="skolop2.0oj.ZOO">skolopedoj</ref> (<trd lng="la">Numenius</trd>):
      <ekz>
        oni a&ubreve;dis la fajfadon de <tld/>oj sur la senarba
        montoflanko
        <fnt><aut>R. L. Stevenson, trad. A. Goodheir</aut>,
        <vrk>Forkaptita</vrk>, <lok>Kardo, 1976</lok></fnt>;
      </ekz>
      <ekz>
        la malgrandaj <tld/>oj vivas en marbordoj, stepoj proksimaj al
        maro, mar&ccirc;ejoj, lagetoj kaj deltoj
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url ref="&CRI;1/2005/09/30/1@29722.htm">
          Malgranda numenio</url></vrk>,
          <lok>2005-09</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/0/09/Curlew_(Numenius_arquata)_(12).jpg/640px-Curlew_(Numenius_arquata)_(12).jpg"
      prm="CC BY-SA 3.0"
      ><tld/>o (Numenius arquata), Irlando
        <fnt>
          <aut>Ken Billington, propra verko</aut>
          <lok><url ref="&WCW;/File:Curlew_(Numenius_arquata)_(12).jpg"
              >https://commons.wikimedia.org/wiki/File:Curlew_(Numenius_arquata)_(12).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_l;&c_jo;&c_n; <klr tip="amb">(&c_p;&c_t;&c_u;&c_sh;&c_k;&c_a;)</klr></trd>
  <trd lng="lat">Numenius</trd>
  <trd lng="cs">kil&iacute;k</trd>
  <trd lng="de">Brachvogel</trd>
  <trd lng="en">curlew</trd>
  <trd lng="fr">courlis</trd>
  <trd lng="nl">wulp</trd>
  <trd lng="pl">kulik</trd>
  <trd lng="pt">ma&ccedil;arico</trd>
  <trd lng="sk">hvizd&aacute;k</trd>
  <trd lng="uk">&c_k;&c_u;&c_l;&c_mol;&c_o;&c_n;</trd>
</drv>
</art>

