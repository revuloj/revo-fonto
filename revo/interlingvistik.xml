<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>interlingvistik</rad>/o
</kap>
<drv mrk="interlingvistik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">LIN</uzo>
  <fnt><bib>KrED</bib></fnt>
  <snc>
    <dif>
      Malvaste: Scienco pri la internaciaj planlingvoj; vaste: scienco pri
      la internacia lingva komunikado inkluzive la rolon de la
      etnaj kaj planitaj lingvoj.
    </dif>
    <refgrp tip="super">
      <ref cel="lingvi.0o">lingvistiko</ref>;
    </refgrp>
  </snc>
  <trd
  lng="be">&c_ib;&c_n;&c_t;&c_e;&c_r;&c_l;&c_ib;&c_g;&c_v;&c_ib;&c_s;&c_t;&c_y;&c_k;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Interlinguistik</trd>
  </trdgrp>
  <trd lng="en">Interlinguistics</trd>
  <trd lng="es">interling&uuml;&iacute;stica</trd>
  <trd lng="fr">interlinguistique</trd>
  <trd lng="hu">interlingvisztika</trd>
  <trd lng="pt">interling&#252;&#237;stica</trd>
  <trd
  lng="ru">&c_i;&c_n;&c_t;&c_je;&c_r;&c_l;&c_i;&c_g;&c_v;&c_i;&c_s;&c_t;&c_i;&c_k;&c_a;</trd>
</drv>
</art>
