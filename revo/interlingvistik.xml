  <art mrk="$Id$">
   <kap>
    <rad>interlingvistik</rad>/o<fnt>Z</fnt>
   </kap>

   <drv mrk="interlingvistik.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">LIN</uzo>
    <fnt><bib>KrED</bib></fnt>
    <snc>
     <dif>
       Malvaste: Scienco pri la internaciaj planlingvoj; vaste: scienco pri
la internacia lingva komunikado inkluzive la rolo de la etnaj kaj planitaj
lingvoj.
     </dif>
     <refgrp tip="super">
       <ref cel="lingvistik.0o">lingvistiko</ref>;
     </refgrp>
    </snc>
    <trdgrp lng="de">
       <trd>Interlinguistik</trd>
    </trdgrp>
   </drv>
  </art>

