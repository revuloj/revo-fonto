<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sagac.xml,v 1.25 2024/03/24 19:59:58 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sagac</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sagac.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="sagac.0a.iu">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Spirite akrevida kaj klarju&gcirc;a, singarde elturni&gcirc;ema, subtile
      eltrovema:
      <ekz>
        &ccirc;io en la &gcirc;ardeno de la imperiestro estis aran&gcirc;ita plej
        <tld/>e
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="inteli.0a">inteligenta</ref>,
      <ref cel="penetr">penetrema</ref>,
      <ref cel="sagx.0a">sa&gcirc;a</ref>,
      <ref cel="sprit.0a">sprita</ref>,
      <ref cel="subtil.0a">subtila</ref>.
    </refgrp>
  </snc>
  <snc mrk="sagac.0a.io">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Subtile, lerte eltrovita:
      <ekz>
        neniaj <tld/>aj politikaj kontraktoj donos pacon al la homaro
        <fnt>
          <aut>L.L. Zamenhof</aut>
          <vrk>Gentoj kaj Lingvo Internacia</vrk>
          <lok>1911</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_n;&c_ib;&c_k;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_z;&c_o;&c_r;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">saga&ccedil;</trd>
  <trdgrp lng="cs">
    <trd>chytr&yacute;</trd>,
    <trd>vychytral&yacute;</trd>
  </trdgrp>
  <trd lng="de">scharfsinnig</trd>
  <trdgrp lng="en">
    <trd>sagacious</trd>,
    <trd>astute</trd>,
    <trd>shrewd</trd>,
    <trd>sharp</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>sagace</trd>,
    <trd>perspicace</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;les esz&udblac;</trd>,
    <trd>lelem&eacute;nyes</trd>,
    <trd>tal&aacute;l&eacute;kony</trd>
  </trdgrp>
  <trd lng="it">sagace</trd>
  <trdgrp lng="ja">
    <trd>&#x660E;&#x654F;&#x306A;  <pr>&#x3081;&#x3044;&#x3073;&#x3093;&#x306A;</pr></trd>,
    <trd>&#x8CE2;&#x660E;&#x306A;  <pr>&#x3051;&#x3093;&#x3081;&#x3044;&#x306A;</pr></trd>,
    <trd>&#x5DE7;&#x5999;&#x306A;  <pr>&#x3053;&#x3046;&#x307F;&#x3087;&#x3046;&#x306A;</pr></trd>,
    <trd>&#x629C;&#x3051;&#x76EE;&#x306E;&#x306A;&#x3044;  <pr>&#x306C;&#x3051;&#x3081;&#x306E;&#x306A;&#x3044;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schrander</trd>,
    <trd>scherpzinnig</trd>,
    <trd>vernuftig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bystry</trd>,
    <trd>przebieg&lstroke;y</trd>,
    <trd>przenikliwy</trd>,
    <trd>sprytny</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>sagaz</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_n;&c_i;&c_c;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_z;&c_o;&c_r;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bystrohlav&yacute;</trd>,
    <trd>bystr&yacute;</trd>,
    <trd>d&ocirc;vtipn&yacute;</trd>,
    <trd>pref&iacute;kan&yacute;</trd>,
    <trd>umn&yacute;</trd>,
    <trd>vynaliezav&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&#x43E;&#x43D;&c_i;&#x43A;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_r;&#x43E;&c_z;&c_ib;&c_r;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_r;&#x43E;&c_z;&#x43E;&c_r;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6167; <pr>hu&#xEC;</pr></trd>,
    <trd>&#x660E;&#x667A; <pr>m&#xED;ngzh&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sagac.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="sagac.0ulo.KOMUNE">
    <dif>
      Homo <tld/>a, bone ju&gcirc;anta, lerte aganta:
      <ekz>
        &leftquot;Korvino estas <tld/>ulino; mi &scirc;in admiras pro &scirc;ia
        klara ju&gcirc;kapablo&rightquot;
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
    <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_n;&c_ib;&c_k;&c_l;&c_ib;&c_v;&c_y; &c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;</trd>,
    <trd>&c_s;&c_p;&c_r;&c_y;&c_t;&c_n;&c_ju;&c_g;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>malin<klr> (subst.)</klr></trd>,
    <trd>perspicace<klr> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>cwaniak</trd>,
    <trd>chytrus</trd>
  </trdgrp>
</drv>
</art>
