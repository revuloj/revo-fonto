<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc><rad>Teodor</rad>/o
</kap>
<drv mrk="teodor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">NOM</uzo>
  <snc>
    <dif>
      Vira anta&ubreve;nomo grekdevena:
      <ekz>
        la rozo apartenas al <tld/>o
        <fnt><bib>F</bib>, <lok>&FE; 5</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">Teodoro</trd>
  <trd lng="fr">Th&eacute;odore</trd>
  <trdgrp lng="hu">
    <trd>Tivadar</trd>,
    <trd>Teodor</trd>,
    <trd>T&oacute;dor</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_F;&c_jo;&c_d;&c_o;&c_r;</trd>,
    <trd>&c_T;&c_je;&c_o;&c_d;&c_o;&c_r;<klr> (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>
  </trdgrp>
</drv>
</art>

