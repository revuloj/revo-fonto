<vortaro>
  <art mrk="$Id: fukusxim.xml,v 1.6 2023/11/10 06:08:01 afido Exp $">
    <kap>
      <rad>Fuku&scirc;im</rad>/o
    </kap>

<drv mrk="fukusxim.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Japana <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>, 240&#x202F;km norde de Tokio:
      <ekz>
        tertremego kun tuj posta cunamo &ndash; kaj la tuj sekvinta nuklea katastrofo en
        <tld/>o rezulte de la cunamo
        <fnt>
          <aut>Pejno Simono</aut>
          <vrk><url ref="https://www.monato.be/2011/010370.php">Moderna minaco: radioaktiveco</url></vrk>
              <bib>Monato</bib>
          <lok>2011/06, p. 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        pro forlaso de nuklea energio post <tld/>o kaj novaj energilineoj tra la lando,
        la prezo de elektro plialti&gcirc;is
        <fnt>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="https://www.monato.be/2012/010844.php">Sunelektraj kamparanoj</url></vrk>
              <bib>Monato</bib>
          <lok>2012/11, p. 11</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_F;&c_u;&c_k;&c_u;&c_s;&c_ib;&c_m;&c_a;</trd>
    <trd lng="de">Fukushima</trd>
    <trd lng="en">Fukushima</trd>
    <trd lng="fr">Fukushima</trd>
    <trd lng="nl">Fukushima</trd>
    <trd lng="ja">&#x798F;&#x5CF6;&#x5E02; <pr>&#x3075;&#x304F;&#x3057;&#x307E;&#x3057;</pr></trd>
  </snc>
  <trd lng="zh">&#x798F;&#x5C9B;&#x5E02; <pr>f&#xFA;d&#x1CE;osh&#xEC;</pr></trd>
</drv>

  </art>

