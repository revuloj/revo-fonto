<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fuort.xml,v 1.18 2023/10/14 19:15:46 revo Exp $">
<kap>
  <rad>fuort</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="fuort.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="super" cel="fortik.0ajxo">Fortika&jcirc;o</ref>
      por la gvato a&ubreve; defendo de pasejo, rivero a&ubreve; simila loko:
      <ekz>
        la na&ubreve; <tld/>oj de Ka&ubreve;no
        <fnt>
          <bib>LOdE</bib>
          <vrk><url ref="&Tekstaro;?nomo=ondo-de-esperanto">Ka&ubreve;no: urbo-fortika&jcirc;o</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        interligitajn <tld/>ojn kaj alarmajn signalturojn la centra
        registaro utiligis por kontakti&gcirc;i kun plej bazaj militaj
        organizoj
        <fnt><bib>EncCxi</bib>, <lok>&ccirc;apitro &leftquot;La Granda Muro&rightquot;, p.
        81a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="bastio.0o.fortikajxo">bastiono<sncref/></ref>,
       <ref cel="bunkr.0o">bunkro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_f;&c_o;&c_r;&c_t;</trd>
  <trd lng="ca">fort<klr> (constr. mil.)</klr></trd>
  <trd lng="cs">pevnost</trd>
  <trdgrp lng="de">
    <trd>Fort</trd>,
    <trd>Befestigungsanlage</trd>,
    <trd>Festung</trd>
  </trdgrp>
  <trd lng="fr">fort<klr> (ouvrage mil. strat&eacute;gique)</klr></trd>
  <trdgrp lng="it">
    <trd>fortezza</trd>,
    <trd>forte<klr> (mil.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7826;  <pr>&#x3068;&#x308A;&#x3067;</pr></trd>,
    <trd>&#x5821;&#x5841;  <pr>&#x307B;&#x3046;&#x308B;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">fort</trd>
  <trd lng="pl">fortyfikacja</trd>
  <trdgrp lng="pt">
    <trd>fortaleza</trd>,
    <trd>fortifica&ccedil;&atilde;o</trd>,
    <trd>forte</trd>
  </trdgrp>
  <trd lng="ro">fortifica&#x21B;ie</trd>
  <trd lng="ru">&c_f;&c_o;&c_r;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>bunker</trd>,
    <trd>pevnos&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_f;&#x43E;&c_r;&c_t;</trd>
</drv>

<drv mrk="fuort.0eto">
  <kap><tld/>eto</kap>
  <snc>
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Malgranda <tld/>o, en kiu de&jcirc;oras nur kelkaj homoj:
      <ekz>
        <tld/>eto &ccirc;e la marbordo
        <fnt>
          <bib>Monato</bib>
          <aut>Paulo Silas</aut>
          <vrk><url ref="&Monato;/2015/011507.php">La lastaj tagoj de Lillo</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_o;&c_r;&c_c;&c_ib;&c_k;</trd>,
    <trd>&c_r;&c_e;&c_d;&c_u;&c_t;</trd>
  </trdgrp>
  <trd lng="ca">fort&iacute;</trd>
  <trd lng="cs">pevn&uring;stka</trd>
  <trdgrp lng="de">
    <trd>Fort</trd>,
    <trd>Vorwerk</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fortin</trd>,
    <trd>redoute</trd>
  </trdgrp>
  <trd lng="it">fortino</trd>
  <trd lng="nl">fortje</trd>
  <trd lng="pl">reduta</trd>
  <trd lng="pt">fortim</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_l;&c_y;&c_j; <ind>&c_f;&c_o;&c_r;&c_t;</ind></trd>,
    <trd>&c_r;&c_je;&c_d;&c_u;&c_t;</trd>,
    <trd>&c_o;&c_p;&c_o;&c_r;&c_n;&c_y;&c_j; &c_p;&c_u;&c_n;&c_k;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">pevn&ocirc;stka</trd>
  <trd lng="uk">&#x43D;&c_je;&c_v;&c_je;&#x43B;&c_i;&#x43A;&c_i;&c_j; &c_f;&#x43E;&c_r;&c_t;</trd>
</drv>
</art>

