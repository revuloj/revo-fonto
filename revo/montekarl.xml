<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Montekarl</rad>/o
    </kap>

<drv mrk="montekarl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POL</uzo><uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Kvartalo de <ref tip="malprt" cel="monak1.M0o">Monako</ref>:
      <ekz>
        83-jara li vizitis kongresojn en Dublino, Helsinko, <tld/>o kaj Komo, &ccirc;io &ccirc;i en
        da&ubreve;ro sesmonata
        <fnt>
          <vrk><url ref="https://biblbut.wordpress.com/kio-estas-kie/la-britaj-esperantistoj/fred-parker/"
              >Fred Parker | Bonvenon al la Biblioteko Butler!</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la Operejo de <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=412157"
              >Operejo de Montekarlo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Monte-Carlo</trd>
    <trd lng="fr">Monte Carlo</trd>
  </snc>
    <rim>
      Ekzistas pluraj samnomaj urboj en Italujo kaj Argentino.
    </rim>
</drv>

  </art>

