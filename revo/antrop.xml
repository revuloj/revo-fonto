<art mrk="$Id: antrop.xml,v 1.18 2021/12/03 08:36:09 revo Exp $">
<kap>
  <rad>antropoid</rad>/o
</kap>
<drv mrk="antrop.0o">
  <kap><tld/>o</kap>
  <snc mrk="antrop.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>  
    <dif>
      Homformulo, precipe iu el la 
      <ref tip="vid" cel="homed.0oj.ZOO">homedoj</ref>
      a&ubreve; el la prahistoriaj formoj:
      <ekz>
        driopiteko <klr>[estas]</klr> primitiva <tld/>o
        kiu vivis en E&ubreve;razio
        <fnt>
          <aut>Ri&scirc;o</aut>,
          <vrk><url ref="http://www.ipernity.com/doc/risho/12280535">
          Driopiteko</url></vrk>,
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_n;&c_t;&c_r;&c_a;&c_p;&c_o;&c_ib;&c_d;</trd>,
    <trd>&c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_a;&c_ja; &c_ib;&c_s;&c_t;&c_o;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="fr">anthropo&iuml;de<klr> (subst.)</klr></trd>
</drv>

<drv mrk="antrop.0a">
  <kap><tld/>a<fnt><bib>PV</bib></fnt></kap>
  <snc mrk="antrop.0a.KOMUNE">
    <dif>
      Homsimila:
      <ekz>
        <tld/>a simio.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;</trd>
  <trd lng="cs">antropoidn&iacute;</trd>
  <trdgrp lng="de">
       <trd>menschenartig</trd>,
       <trd>Menschen-</trd>
  </trdgrp>
  <trd lng="en">anthropoidal</trd>
  <trd lng="es">antropoide</trd>
  <trd lng="fr">anthropo&iuml;de<klr> (adj.)</klr></trd>
  <trd lng="he">&#x5D3;&#x5B0;&#x5DE;&#x5D5;&#x5BC;&#x5D9; &#x5D0;&#x5B8;&#x5D3;&#x5B8;&#x5DD;</trd>
  <trd lng="hu">emberszab&aacute;s&uacute;</trd>
  <trd lng="ru">&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">antropoidn&yacute;</trd>
</drv>
</art>
