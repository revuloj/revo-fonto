<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>hidrarg</rad>/o
</kap>
<drv mrk="hidrar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="hidrar.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 80, atompezo 200,59, simbolo Hg; <ref tip="super"
      cel="metal.0o">metalo</ref> ar&gcirc;entosimila,
      fluida je la ordinara temperaturo, kaj
      uzata por fabrikado de termometroj kaj speguloj:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>li estas vera <tld/>o</ind>
        <trd lng="fr">c'est un <ind>bougeon</ind></trd>
        <klr>(li sen&ccirc;ese movi&gcirc;as)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_t;&c_u;&c_c;&c_mol;</trd>          
  <trd lng="br">merkur</trd>
  <trd lng="de">Quecksilber</trd>
  <trd lng="el">&ypsilon;&delta;&rho;&alfa_ton;&rho;&gamma;&ypsilon;&rho;&omikron;&sigma_fina;</trd>
  <trd lng="en">mercury</trd>
  <trd lng="es">mercurio</trd>
  <trd lng="fr">mercure</trd>
  <trd lng="hu">higany</trd>
  <trd lng="nl">kwikzilver</trd>
  <trd lng="pl">rt&eogonek;&cacute;</trd>
  <trd lng="pt">merc&uacute;rio</trd>
  <trd lng="ru">&c_r;&c_t;&c_u;&c_t;&c_mol;</trd>
</drv>
</art>
