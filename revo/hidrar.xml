<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hidrar.xml,v 1.32 2023/11/15 17:08:00 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>hidrarg</rad>/o
</kap>
<drv mrk="hidrar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <mlg kod="IUPAC">Hg</mlg>
  <snc mrk="hidrar.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="80">elemento</ref> kun
      atomnumero 80, atompezo 200,59; <ref tip="super" cel="metal.0o">metalo</ref> ar&gcirc;entosimila,
      fluida je ordinaraj temperaturoj, kaj
      uzata por fabriki termometrojn kaj spegulojn:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>li estas vera <tld/>o</ind>
        <trd lng="fr">c'est un <ind>bougeon</ind></trd>
        <klr>(li sen&ccirc;ese movi&gcirc;as)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_t;&c_u;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_zh;&c_i;&c_v;&c_a;&c_k;</trd>
  <trd lng="br">merkur</trd>
  <trdgrp lng="cs">
    <trd>Hg</trd>,
    <trd>hydrargyrum</trd>,
    <trd>rtu&#x165;</trd>
  </trdgrp>
  <trd lng="de">Quecksilber</trd>
  <trd lng="el">&ypsilon;&delta;&rho;&alfa_ton;&rho;&gamma;&ypsilon;&rho;&omikron;&sigma_fina;</trd>
  <trd lng="en">mercury</trd>
  <trdgrp lng="es">
    <trd>mercurio</trd>,
    <trd>azogue</trd>
  </trdgrp>
  <trd lng="fa">&a_j;&f_y;&a_w;&a_h;</trd>
  <trd lng="fr">mercure</trd>
  <trd lng="hu">higany</trd>
  <trdgrp lng="id">
    <trd>merkurium</trd>,
    <trd>raksa</trd>
  </trdgrp>
  <trd lng="it">mercurio<klr> (ch.)</klr></trd>
  <trd lng="ja">&#x6C34;&#x9280;  <pr>&#x3059;&#x3044;&#x304E;&#x3093;</pr></trd>
  <trdgrp lng="nl">
    <trd>kwik</trd>,
    <trd>kwikzilver</trd>
  </trdgrp>
  <trd lng="pl">rt&eogonek;&cacute;</trd>
  <trd lng="pt">merc&uacute;rio</trd>
  <trd lng="ru">&c_r;&c_t;&c_u;&c_t;&c_mol;</trd>
  <trd lng="sk">ortu&#x165;</trd>
  <trd lng="sv">kvicksilver</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x435;&#x440;&#x43A;&#x443;&#x440;&#x456;&#x439; (&#x445;&#x456;&#x43C;&#x456;&#x447;&#x43D;&#x438;&#x439; &#x435;&#x43B;&#x435;&#x43C;&#x435;&#x43D;&#x442;)</trd>,
    <trd>&#x440;&#x442;&#x443;&#x442;&#x44C;</trd>,
    <trd>&#x436;&#x438;&#x432;&#x435; &#x441;&#x440;&#x456;&#x431;&#x43B;&#x43E; (&#x43F;&#x440;&#x43E;&#x441;&#x442;&#x430; &#x440;&#x435;&#x447;&#x43E;&#x432;&#x438;&#x43D;&#x430;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6C34;&#x9280; <pr>shu&#x1D0;y&#xED;n</pr></trd>,
    <trd>&#x6C5E; <pr>g&#x1D2;ng</pr></trd>,
    <trd>&#x6C5E; <pr>h&#xF2;ng</pr></trd>
  </trdgrp>
</drv>
</art>
