<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bovl.xml,v 1.23 2023/10/29 19:11:03 revo Exp $">
<kap>
  <ofc>8</ofc><rad>bovl</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="bovl.0o">
  <kap><tld/>o</kap>
  <snc mrk="bovl.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="vaz.0o">Vazo</ref> farita el argilo,
      fajenco, porcelano, metalo a&ubreve; alia materialo, kun a&ubreve; sen anso,
      kaj ne havanta kolon:
      <ekz>
        en la mateno E&ubreve;tikia envenis por porti al mi <tld/>on da lakto
        <fnt><aut>Anna Lowenstein</aut><vrk>La &Scirc;tona Urbo</vrk>
        <lok>jaro 1999a</lok></fnt>;
      </ekz><ekz>
        &scirc;i vidis, ke kato faligis <tld/>on sur la plankon
        <fnt><aut>Machado de Assis, trad. Paulo Viana</aut>
        <vrk>La divenistino kaj aliaj rakontoj</vrk><lok>Oportuno, jaro
        2005a, p.
        52a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_m;&c_ib;&c_s;&c_a;</trd>,
      <trd>&c_m;&c_ib;&c_s;&c_k;&c_a;</trd>,
      <trd>&c_m;&c_a;&c_k;&c_ib;&c_t;&c_r;&c_a;</trd>,
      <trd>&c_l;&c_a;&c_t;&c_k;&c_a; <klr tip="amb">(&c_p;&c_o;&c_s;&c_u;&c_d;)</klr></trd>
    </trdgrp>
    <trdgrp lng="cs">
      <trd>bowle <klr>(n&aacute;doba i n&aacute;poj)</klr></trd>,
      <trd>m&iacute;sa hlubok&aacute;</trd>
    </trdgrp>
    <trd lng="de">Schale</trd>
    <trd lng="en">bowl</trd>
    <trdgrp lng="es">
      <trd>cuenco</trd>,
      <trd>bol</trd>
    </trdgrp>
    <trd lng="fr">bol</trd>
    <trdgrp lng="hu">
      <trd>csupor</trd>,
      <trd>t&aacute;lka</trd>,
      <trd>iv&oacute;cs&eacute;sze</trd>,
      <trd>szilke</trd>
    </trdgrp>
    <trd lng="nl">kom</trd>
    <trd lng="pl">miska</trd>
    <trd lng="pt">tigela</trd>
    <trd lng="ro">bol</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_o;&c_sh;&c_k;&c_a;</trd>,
      <trd>&c_m;&c_i;&c_s;&c_k;&c_a;</trd>,
      <trd>&c_ch;&c_a;&c_sh;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>misa</trd>,
      <trd>okr&uacute;hla v&aacute;za</trd>
    </trdgrp>
  </snc>
  <snc mrk="bovl.0o.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="super" cel="sign.super0o">Supersigno</ref>
      <tld/>oforma, kia aperas i.a. super la esperanta litero &#x2039;&ubreve;&#x203A;:
      <ekz>
        En la etnaj lingvoj la <tld/>o aperas en la ortografio de la
        rumana kaj la vjetnama &ccirc;e la litero &abreve;
        <fnt>
          <aut>J.C.Wells</aut>:
          <vrk>Supersignoj, trasignoj kaj subsignoj</vrk>.
          <lok><url ref="https://www.phon.ucl.ac.uk/home/wells/dia/supersignoj.htm">Internacia
          Kongresa Universitato, Prago 1996 07 26</url>
          </lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_b;&c_r;&c_e;&c_v;&c_ib;&c_s;</trd>,
      <trd>&c_b;&c_r;&c_e;&c_v;&c_e;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_a;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_a;&c_ja; <ind>&c_k;&c_a;&c_m;&c_o;&c_r;&c_a;</ind></trd>
    </trdgrp>
    <trd lng="en">breve</trd>
    <trd lng="nl">breve</trd>
    <trd lng="pl">brewis</trd>
    <trd lng="ro">breve</trd>
    <trd lng="ru">&c_k;&c_r;&c_a;&c_t;&c_k;&c_a;</trd>
  </snc>
  <trd lng="he">&qof;&#x5B0;&ayin;&#x5B8;&resh;&#x5B8;&he;</trd>
  <trdgrp lng="ja">
    <trd>&#x6DF1;&#x76BF;  <pr>&#x3075;&#x304B;&#x3056;&#x3089;</pr></trd>,
    <trd>&#x7897;  <pr>&#x308F;&#x3093;</pr></trd>,
    <trd>&#x4E3C;  <pr>&#x3069;&#x3093;&#x3076;&#x308A;</pr></trd>,
    <trd>&#x30DC;&#x30A6;&#x30EB;  <pr>&#x307C;&#x3046;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_m;&c_i;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_ch;&c_a;&c_sh;&c_a;</trd>,
    <trd>&c_ch;&c_a;&c_sh;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x76CE; <pr>&#xE0;ng</pr></trd>,
    <trd>&#x7897; <pr>w&#x1CE;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="bovl.te0o">
  <kap>te<tld/>o <fnt><bib>PIV2</bib></fnt></kap>
  <snc>
    <dif>
      <tld lit="B"/>o por tetrikado (senansa
      <ref cel="tas.0o.ujo">taso<sncref/></ref>):
      <ekz>
        Mia te<tld/>o estas blua, kun foliaj desegna&jcirc;oj &mdash; &ccirc;ar estas
        somero, &gcirc;i havas tiajn florajn motivojn
        <fnt>
          <aut>&Jcirc;enja Amis</aut>:
          <vrk>Mia malkovro de Japanio</vrk>.
          <lok>Kontakto, n-ro 254 (2013:2)</lok>.
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="trink.0ujo">trinkujo</ref>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_ja;&c_l;&c_a;</trd>
  <trd lng="pl">dzbanek na  <ind>herbat&eogonek;</ind></trd>
  <trd lng="ru">&c_p;&c_i;&c_a;&c_l;&c_a;</trd>
  <trd lng="uk">&c_p;&c_ib;&c_a;&c_l;&c_a;</trd>
</drv>

</art>
