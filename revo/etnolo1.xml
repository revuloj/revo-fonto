<vortaro>
<art mrk="$Id$">
<kap><ofc>9</ofc><rad>etnologi</rad>/o
<fnt>Z</fnt></kap>
<drv mrk="etnolo1.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Scienco studanta kiel <ref tip="prt" cel="etn.0o">etnoj</ref>
      esti&gcirc;as (<ref tip="prt" cel="etn.0ogenezo">etnogenezo</ref>),
      evoluas, sin strukturas, interrilatas kun la aliaj etnoj kaj
      la naturmedio.
    </dif>
    <ref tip="sub" cel="etnogr.0o">etnografio</ref>
  </snc>
  <trd lng="be">&c_e;&c_t;&c_n;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Ethnologie</trd>,
    <trd>V&ouml;lkerkunde</trd>
  </trdgrp>
  <trd lng="en">ethnology</trd>
  <trd lng="es">etnolog&iacute;a</trd>
  <trd lng="fa">&a_q;&a_w;&a_m;&zwnj;&a_sh;&a_n;&a_A;&a_s;&f_y;</trd>
  <trd lng="fr">ethnologie</trd>
  <trdgrp lng="hu">
    <trd>egyetemes <ind>n&aacute;prajz</ind></trd>,
    <trd>&aacute;ltal&aacute;nos <ind>n&aacute;prajz</ind></trd>,
    <trd>n&aacute;ptudom&aacute;ny</trd>,
    <trd>etnol&oacute;gia</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>etnologie</trd>,
    <trd>volkenkunde</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>etnologia</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_t;&c_n;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="sv">etnologi</trd>
</drv>

<drv mrk="etnolo1.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Fakulo pri <tld/>o.
    </dif>
    <refgrp tip="sin">
      <ref cel="etnolo.0o">etnologo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_t;&c_n;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trdgrp lng="de">
    <trd>Ethnologe</trd>,
    <trd>V&ouml;lkerkundler</trd>
  </trdgrp>
  <trd lng="en">ethnologist</trd>
  <trd lng="es">etn&oacute;logo</trd>
  <trd lng="fa">&a_q;&a_w;&a_m;&zwnj;&a_sh;&a_n;&a_A;&a_s;</trd>
  <trd lng="fr">ethnologue</trd>
  <trd lng="hu">etnol&oacute;gus</trd>
  <trd lng="nl">etnoloog</trd>
  <trd lng="ru">&c_e;&c_t;&c_n;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="sv">etnolog</trd>
</drv>
</art>
