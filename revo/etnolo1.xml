<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>etnologi</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="etnolo1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SCI</uzo>
  <snc>
    <dif>
      Scienco studanta kiel <ref tip="prt" cel="etn.0o">etnoj</ref>
      esti&gcirc;as (<ref tip="prt" cel="etn.0ogenezo">etnogenezo</ref>),
      evoluas, sin strukturas, interrilatas kun la aliaj etnoj kaj
      la naturmedio.
    </dif>
    <ref tip="sub" cel="etnogr.0o">etnografio</ref>
  </snc>
  <trd lng="en">ethnology</trd>
  <trd lng="fr">ethnologie</trd>
  <trd lng="ru">&c_e;&c_t;&c_n;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="sv">etnologi</trd>
</drv>

<drv mrk="etnolo1.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Fakulo pri <tld/>o.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="etnolo.0o">etnologo</ref>
  </refgrp>
  <trd lng="fr">ethnologue</trd>
  <trd lng="sv">etnolog</trd>
</drv>
</art>
