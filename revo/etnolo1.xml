<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: etnolo1.xml,v 1.29 2023/11/07 20:34:12 revo Exp $">
<kap>
  <ofc>9</ofc>
  <rad>etnologi</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="etnolo1.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="etnolo1.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="lst" lst="voko:sciencoj" cel="scienc.0o.fako">Scienco</ref>
      studanta kiel <ref tip="prt" cel="etn.0o">etnoj</ref>
      esti&gcirc;as (<ref tip="prt" cel="etn.0ogenezo">etnogenezo</ref>),
      evoluas, sin strukturas, interrilatas kun la aliaj etnoj kaj
      la naturmedio:
      <ekz>
        diversajn muzeojn pri artoj kaj pri <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Mireille Grosjean</aut>
          <vrk><url ref="&Monato;/2015/011395.php">Urbo kun forta studemo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Jean-Luc Godard</nom> <klr>[&#x2026;]</klr> &ccirc;e la <nom>Sorbonne</nom> en Parizo <klr>[&#x2026;]</klr>studis
        <tld/>on
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=241161">Jean-Luc Godard</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="etnogr.0o">etnografio</ref>
  </snc>
  <trd lng="be">&c_e;&c_t;&c_n;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_je;&c_t;&c_n;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Ethnologie</trd>,
    <trd>V&ouml;lkerkunde</trd>
  </trdgrp>
  <trd lng="en">ethnology</trd>
  <trd lng="es">etnolog&iacute;a</trd>
  <trd lng="fa">&a_q;&a_w;&a_m;&zwnj;&a_sh;&a_n;&a_A;&a_s;&f_y;</trd>
  <trd lng="fr">ethnologie</trd>
  <trdgrp lng="hu">
    <trd>egyetemes <ind>n&aacute;prajz</ind></trd>,
    <trd>&aacute;ltal&aacute;nos <ind>n&aacute;prajz</ind></trd>,
    <trd>n&aacute;ptudom&aacute;ny</trd>,
    <trd>etnol&oacute;gia</trd>
  </trdgrp>
  <trd lng="ja">&#x6C11;&#x65CF;&#x5B66;  <pr>&#x307F;&#x3093;&#x305E;&#x304F;&#x304C;&#x304F;</pr></trd>
  <trdgrp lng="nl">
    <trd>etnologie</trd>,
    <trd>volkenkunde</trd>
  </trdgrp>
  <trd lng="pl">etnologia</trd>
  <trdgrp lng="pt">
   <trd>etnologia</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_t;&c_n;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="sv">etnologi</trd>
  <trd lng="uk">&#x435;&#x442;&#x43D;&#x43E;&#x43B;&#x43E;&#x433;&#x456;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x6C11;&#x4FD7; <pr>m&#xED;ns&#xFA;</pr></trd>,
    <trd>&#x4EBA;&#x79CD;&#x5B66; <pr>r&#xE9;nzh&#x1D2;ngxu&#xE9;</pr></trd>,
    <trd>&#x6C11;&#x65CF;&#x5B66; <pr>m&#xED;nz&#xFA;xu&#xE9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="etnolo1.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="etnolo1.0isto.SCI">
    <dif>
      Fakulo pri <tld/>o.
    </dif>
    <refgrp tip="sin">
      <ref cel="etnolo.0o">etnologo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_t;&c_n;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="bg">&c_je;&c_t;&c_n;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="cs">etnolog</trd>
  <trdgrp lng="de">
    <trd>Ethnologe</trd>,
    <trd>V&ouml;lkerkundler</trd>
  </trdgrp>
  <trd lng="en">ethnologist</trd>
  <trd lng="es">etn&oacute;logo</trd>
  <trd lng="fa">&a_q;&a_w;&a_m;&zwnj;&a_sh;&a_n;&a_A;&a_s;</trd>
  <trd lng="fr">ethnologue</trd>
  <trd lng="hu">etnol&oacute;gus</trd>
  <trd lng="nl">etnoloog</trd>
  <trd lng="pl">etnolog</trd>
  <trd lng="ru">&c_e;&c_t;&c_n;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="sk">etnol&oacute;g</trd>
  <trd lng="sv">etnolog</trd>
  <trd lng="zh">&#x6C11;&#x65CF;&#x5B66;&#x8005; <pr>m&#xED;nz&#xFA;xu&#xE9;zh&#x11B;</pr></trd>
</drv>
</art>
