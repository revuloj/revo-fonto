<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: j.xml,v 1.4 2021/05/21 20:51:45 revo Exp $">
<kap>
    <ofc>*</ofc>-<rad>j</rad> 
</kap>
<drv mrk="j.0">
  <kap>-<tld/></kap>
  <snc mrk="j.0.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      <ref tip="super" cel="fin.0ajxo">Fina&jcirc;o</ref> markanta la 
      <ref cel="plural.0o">pluralon</ref> &ccirc;e adjektivoj, substantivoj, 
      pronomoj kaj kelkaj <ref cel="tabel.0vorto">tabelvortoj</ref>:
      <ekz>
        anta&ubreve; kelka<tld/> tago<tld/>
        <fnt>
          <bib>UL</bib>
        </fnt>;
      </ekz>
      <ekz>
        inter tiu<tld/> peco<tld/> kreskis plej bela<tld/> granda<tld/> blanka<tld/>
        floro<tld/>
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        milo<tld/> da milo<tld/>
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 24:60</lok>
        </fnt>;
      </ekz>
      <ekz>
        ia<tld/> bagatela<tld/> ekstera<tld/> detala&jcirc;o<tld/>
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en Dresden en la 17a de a&ubreve;gusto 1908</vrk>
        </fnt>;
      </ekz>
      <ekz>
        jen venis la alta<tld/>, ne&gcirc;kovrita<tld/> Alpo<tld/>, kun nubo<tld/> kaj
        malluma<tld/> abio<tld/>
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        drinku tuta<tld/>n tago<tld/>n, sed kontrolu via<tld/>n ago<tld/>n
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="hom">
    <ref cel="j2.0">-j (neoficiala nomsufikso)</ref>,
    <ref cel="j1.0">jo (liternomo)</ref>
  </refgrp>
  <trd lng="de">Pluralendung</trd>
  <trd lng="en">ending to mark the <ind>plural</ind> in Esperanto</trd>
  <trd lng="fr">marque du <ind>pluriel</ind></trd>
</drv>
</art>

