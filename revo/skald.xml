<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: skald.xml,v 1.10 2023/12/15 08:31:35 revo Exp $">
<kap>
  <rad>skald</rad>/o
  <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="skald.0o">
  <kap><tld/>o</kap>
  <snc mrk="skald.0o.HIS">
    <uzo tip="fak">BELE</uzo>
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">POE</uzo>
    <dif>
      Antikva norde&ubreve;ropa poeto-<ref tip="super" cel="kant.0isto.poeto">kantisto</ref>:
      <ekz>
        <nom>Egill Skallagr&iacute;msson</nom> estas fama <tld/>o, &ccirc;efo kaj vikingo, naski&gcirc;is &ccirc;irka&ubreve; la
        jaro 910 en Islando kaj vivis &gcirc;is proksimume 990
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=27889">Egill Skallagr&iacute;msson</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="aed.0o.HIS">aedo</ref>,
      <ref cel="bard.0o.HIS">bardo</ref>,
      <ref cel="menestre.0o.HIS">menestrelo</ref>,
      <ref cel="rapsod.0o.HIS">rapsodo</ref>,
      <ref cel="trobad.0o">trobadoro</ref>
    </refgrp>
      <trd lng="de">Skalde</trd>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_a;&c_l;&c_mol;&c_d;</trd>
  <trdgrp lng="ca">
      <trd>escald</trd>,
      <trd>skald</trd>
    </trdgrp>
  <trd lng="cs">skald</trd>
  <trd lng="en">skald</trd>
  <trd lng="fr">scalde</trd>
  <trd lng="nl">skald</trd>
  <trd lng="pl">skald</trd>
  <trd lng="sk">skald</trd>
  <trd lng="sv">skald</trd>
  <trd lng="uk">&c_s;&#x43A;&c_a;&#x43B;&#x44C;&c_d;</trd>
</drv>
</art>

