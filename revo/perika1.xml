<kap>
  <rad>perikarp</rad>/o
</kap>
<drv mrk="perika1.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      &Ccirc;io de la <ref tip="malprt" cel="frukt.0o.BOT">frukto</ref>,
      kio envolvas unuopan a&ubreve; plurajn grajnojn:
      <ekz>
        <tld/>o konsistas el epikarpo, sarkokarpo a&ubreve; mezokarpo kaj
        endokarpo.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="grajn.0umo">grajnumo</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_p;&c_e;&c_r;&c_y;&c_k;&c_a;&c_r;&c_p;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_ja;&c_p;&c_l;&c_o;&c_d;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">pericarpi</trd>
  <trdgrp lng="de">
    <trd>Fruchtschale</trd>,
    <trd>Pericarp</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>pericarp</trd>,
    <trd>seed case</trd>,
    <trd>seed vessel</trd>
  </trdgrp>
  <trd lng="es">pericarpio</trd>
  <trdgrp lng="fr">
    <trd>p&eacute;ricarpe</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>term&eacute;sfal</trd>,
    <trd>perikarpium</trd>
  </trdgrp>
  <trd lng="ja">&#x679C;&#x76AE;  <pr>&#x304B;&#x3072;</pr></trd>
  <trd lng="la">pericarpium</trd>
  <trd lng="nl">pericarpium</trd>
  <trd lng="pl">owocnia</trd>
  <trdgrp lng="pt">
    <trd>pericarpo</trd>,
    <trd>peric&aacute;rpio</trd>
  </trdgrp>
  <trd lng="ro">pericarp</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_i;&c_k;&c_a;&c_r;&c_p;&c_i;&c_j;</trd>,
    <trd>&c_o;&c_k;&c_o;&c_l;&c_o;&c_p;&c_l;&c_o;&c_d;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x435;&#x440;&#x438;&#x43A;&#x430;&#x440;&#x43F;&#x456;&#x439;</trd>,
    <trd>&#x43E;&#x43F;&#x43B;&#x43E;&#x434;&#x435;&#x43D;&#x44C;</trd>
  </trdgrp>
  <trd lng="zh">&#x679C;&#x76AE; <pr>gu&#x1D2;p&#xED;</pr></trd>
</drv>
