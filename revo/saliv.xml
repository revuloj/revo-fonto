<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: saliv.xml,v 1.32 2024/12/14 08:31:27 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>saliv</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="saliv.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Senkolora <ref tip="super" cel="flu.0ajxo">flua&jcirc;o</ref>
      kiu malsekigas la <ref tip="malprt" cel="busx.0o.ANA">bu&scirc;on</ref>,
      sekreciata de specialaj glandoj, kaj kiu enhavas <ref tip="prt" cel="enzim.0o">enzimojn</ref>
      kunefikantajn en digestado:
      <ekz>
        li <klr>[&#8230;]</klr> furiozis inter iliaj manoj <klr>[&#8230;]</klr> kaj fluigis <tld/>on sur sian
        barbon
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 21:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la sola a&ubreve;dado jam kolekti&gcirc;as al mi <tld/>o en la bu&scirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>!
      </ekz>
      <ekz>
        <nom>Pasteur</nom> posedis eksterordinaran kura&gcirc;on, &ccirc;ar li konstante
        riskis sian vivon, kiam li forprenis <tld/>on el la bu&scirc;ego
        de rabia hundo por eksperimenta infektado de kunikloj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2008/009550.php">Batalo kontra&ubreve; mortiga malsano</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ne plu necesas la propra salivo, memgluaj <klr>[&#8230;]</klr> la markoj ja estas
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2012/010694.php">Dumvivaj rabatoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;</trd>
  <trd lng="cs">slina</trd>
  <trdgrp lng="de">
    <trd>Speichel</trd>,
    <trd>Spucke</trd>
  </trdgrp>
  <trd lng="en">saliva</trd>
  <trd lng="es">saliva</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_z;&a_A;&a_q;</trd>,
    <trd>&a_A_madda;&a_b; &a_d;&a_h;&a_A;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">salive</trd>
  <trd lng="hu">ny&aacute;l</trd>
  <trd lng="it">saliva</trd>
  <trdgrp lng="ja">
    <trd>&#21822;&#28082;  <pr>&#12384;&#12360;&#12365;</pr></trd>,
    <trd>&#12388;&#12400;</trd>
  </trdgrp>
  <trd lng="nl">speeksel</trd>
  <trd lng="pl">&sacute;lina</trd>
  <trdgrp lng="pt">
   <trd>saliva</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_l;&c_ju;&c_n;&c_a;</trd>
  <trd lng="sk">slina</trd>
  <trd lng="sv">saliv</trd>
  <trd lng="uk">&c_s;&c_l;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#21475;&#27700; <pr>k&#466;ushu&#464;</pr></trd>,
    <trd>&#21822; <pr>tu&ograve;</pr></trd>,
    <trd>&#21822;&#27819; <pr>tu&ograve;mo</pr></trd>,
    <trd>&#27941; <pr>j&imacron;n</pr></trd>,
    <trd>&#28046; <pr>xi&aacute;n</pr></trd>,
    <trd>&#21822;&#28082; <pr>tu&ograve;y&egrave;</pr></trd>,
    <trd>&#21520;&#27819; <pr>t&ugrave;mo</pr></trd>,
    <trd>&#28046;&#27700; <pr>xi&aacute;nshu&#464;</pr></trd>,
    <trd>&#21475;&#27819; <pr>k&#466;um&ograve;</pr></trd>,
    <trd>&#30192;&#28082; <pr>t&aacute;ny&egrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="saliv.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Eligi <tld/>on; kovri per <tld/>o:
      <ekz>
        &ccirc;iaspecaj delikata&jcirc;oj igis la gastojn <tld/>i
        <fnt>
          <bib>CKv</bib>
          <lok>19</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sulki&gcirc;inta ha&ubreve;to de la salamandro jukis, <klr>[&#8230;]</klr> &gcirc;i salivis iomete la
        ha&ubreve;ton sub sia lar&gcirc;a makzeleto
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu viro enrampis vian bruston kaj <tld/>is &gcirc;in per la veneno de sia malnobleco
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXVII</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kracx.0i">kra&ccirc;i</ref>,
      <ref cel="saliv.0umi"><tld/>umi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_s;&c_k;&c_a;&c_c;&c_mol;
    <ind>&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_u;</ind></trd>
  <trdgrp lng="cs">
    <trd>naslinit</trd>,
    <trd>slinit</trd>,
    <trd>vylu&ccaron;ovat sliny</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>spucken</trd>,
    <trd>speicheln</trd>,
    <trd>einspeicheln</trd>
  </trdgrp>
  <trd lng="en">salivate</trd>
  <trdgrp lng="es">
     <trd>salivar</trd>,
     <trd>escupir</trd>
  </trdgrp>
  <trd lng="fr">saliver</trd>
  <trdgrp lng="hu">
    <trd>ny&aacute;laz</trd>,
    <trd>ny&aacute;ladzik</trd>
  </trdgrp>
  <trd lng="it">salivare</trd>
  <trd lng="ja">&#12388;&#12400;&#12364;&#20986;&#12427;  <pr>&#12388;&#12400;&#12364;&#12391;&#12427;</pr></trd>
  <trd lng="nl">speeksel afscheiden</trd>
  <trd lng="pl">&sacute;lini&cacute; si&eogonek;</trd>
  <trd lng="pt">salivar</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_d;&c_je;&c_l;&c_ja;&c_t;&c_mol; <ind>&c_s;&c_l;&c_ju;&c_n;&c_u;</ind></trd>,
    <trd>&c_i;&c_s;&c_t;&c_je;&c_k;&c_a;&c_t;&c_mol; <ind>&c_s;&c_l;&c_ju;&c_n;&c_o;&c_j;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>poslini&tcaron;</trd>,
    <trd>zaslini&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">uts&ouml;ndra <ind>saliv</ind></trd>
  <trd lng="uk">&c_v;&c_i;&c_d;&c_ib;&c_l;&c_ja;&c_t;&c_i; &c_s;&c_l;&c_i;&c_n;&c_u;</trd>
  <trd lng="zh">&#27969;&#21475;&#27700; <pr>li&uacute;k&#466;ushu&#464;</pr></trd>
</drv>

<drv mrk="saliv.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <tld lit="S"/>i kaj lasi <tld/>on liki el bu&scirc;o:
      <ekz>
        vidante <klr>[&#8230;]</klr>
        vaporumitan panon el maizfaruno, mi <tld/>umis kaj pensis,
        kiel feli&ccirc;e, se mi povus &gcirc;ui vaporumitan panon &ccirc;iutage
        <fnt>
          <bib>Monato</bib>
          <aut>Wei Yubin</aut>
          <vrk><url ref="&Monato;/2012/010573.php">La biciklo de mia patro</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        partizanoj kaj gerilanoj jam <tld/>umas pro la ideo, ke la princo de&jcirc;oros en nur
        le&gcirc;ere kirasita skoltad-veturilo
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2007/009240.php"
              >Sango kaj rango</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kracx.0i">kra&ccirc;i</ref>,
      <ref cel="saliv.0i"><tld/>i</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_u;&c_s;&c_k;&c_a;&c_c;&c_mol; <ind>&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_k;&c_ib;</ind></trd>,
    <trd>&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_ib;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">slintat</trd>
  <trdgrp lng="de">
    <trd>speicheln</trd>,
    <trd>sabbern</trd>,
    <trd><ind>Appetit</ind> bekommen</trd>
  </trdgrp>
  <trd lng="en">salivate</trd>
  <trd lng="fr">baver</trd>
  <trd lng="it">sbavare<klr> (salivare)</klr></trd>
  <trd lng="ja">&#12424;&#12384;&#12428;&#12434;&#27969;&#12377;  <pr>&#12424;&#12384;&#12428;&#12434;&#12394;&#12364;&#12377;</pr></trd>
  <trd lng="pl">&sacute;lini&cacute; si&eogonek;</trd>
  <trd lng="pt">babar</trd>
  <trd lng="sk">slinta&tcaron;</trd>
  <trd lng="sv">dregla</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_l;&c_i;&c_n;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_o;&c_b;&c_s;&c_l;&c_i;&c_n;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="zh">&#27969;&#21475;&#27700; <pr>li&uacute;k&#466;ushu&#464;</pr></trd>
</drv>
</art>
