<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>saliv</rad>/o
</kap>
<drv mrk="saliv.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Senkolora flua&jcirc;o, sekreciata de specialaj glandoj, kiu malsekigas la
      bu&scirc;on kaj enhavas fermenton kunefikantan en digestado.
    </dif>
    <refgrp tip="sin">
      <ref cel="kracx.0i">kra&ccirc;i</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Speichel</trd>
  <trd lng="es">saliva</trd>
  <trd lng="nl">speeksel</trd>
  <trd lng="ru">&c_s;&c_l;&c_ju;&c_n;&c_a;</trd>
</drv>

<drv mrk="saliv.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Eligi <tld/>on.
    </dif>
  </snc>
  <trd lng="nl">speeksel afscheiden</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_d;&c_je;&c_l;&c_ja;&c_t;&c_mol; <ind>&c_s;&c_l;&c_ju;&c_n;&c_u;</ind></trd>,
    <trd>&c_i;&c_s;&c_t;&c_je;&c_k;&c_a;&c_t;&c_mol; <ind>&c_s;&c_l;&c_ju;&c_n;&c_o;&c_j;</ind></trd>
  </trdgrp>
</drv>
</art>
