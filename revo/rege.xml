<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rege.xml,v 1.9 2023/05/03 18:28:30 revo Exp $">
<kap>
  <rad>rege</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="rege.0o">
  <kap><tld/>o</kap>
  <snc mrk="rege.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Jamajkdevena muzika stilo el la fino de la 1960aj jaroj:
      <ekz>
        gejunuloj <klr>...</klr> dancis je ritmoj de roko, skao, kaj
        <tld/>o de la bandoj Strika Tango, Esperanto Desperado
        <fnt>
        <aut>F. Martorell</aut>,
        <vrk>Informletero de Vinilkosmo n-ro 12</vrk>,
        <lok>soc.culture.esperanto, 2005-04-30</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_g;&c_ib;</trd>
  <trd lng="cs">reggae</trd>
  <trd lng="de">Reggae</trd>
  <trd lng="el">&rho;&epsilon_ton;&gamma;&gamma;&epsilon;</trd>
  <trd lng="en">reggae</trd>
  <trd lng="fr">reggae</trd>
  <trd lng="pl">reggae</trd>
  <trd lng="ro">reggae</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_g;&c_g;&c_i;</trd>,
    <trd>&c_r;&c_je;&c_g;&c_g;&c_je;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">regge</trd>
  <trd lng="uk">&c_r;&c_je;&c_gu;&c_je;&c_j;</trd>
</drv>
</art>

