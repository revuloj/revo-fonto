<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kompli.xml,v 1.23 2020/07/05 21:11:04 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>komplik</rad>/i
</kap>
<drv mrk="kompli.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Embarase, malfacilige malsimpligi:
      <ekz>
        oni ne devas <tld/>i la gramatikon de internacia lingvo;
      </ekz>
      <ekz>
        jen kio <tld/>as la aferon;
      </ekz>
      <ekz>
        <tld/>itaj le&gcirc;oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_s;&c_k;&c_l;&c_a;&c_d;&c_n;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="cs">komplikovat</trd>
  <trd lng="de">verkomplizieren</trd>
  <trd lng="en">complicate</trd>
  <trd lng="es">complicar</trd>
  <trdgrp lng="fr">
    <trd>complexifier</trd>,
    <trd>compliquer</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>compliceren</trd>,
    <trd><ind>ingewikkeld</ind> maken</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wik&lstroke;a&cacute;</trd>,
    <trd>komplikowa&cacute;</trd>,
    <trd>gmatwa&cacute;</trd>,
    <trd>utrudnia&cacute;</trd>
  </trdgrp> 
  <trd lng="pt">complicar</trd>
  <trdgrp lng="ro">
    <trd>complica</trd>,
    <trd>&icirc;mpiedica</trd>,
    <trd>&icirc;ncurca</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_l;&c_o;&c_zh;&c_n;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_s;&c_l;&c_o;&c_zh;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">komplikova&tcaron;</trd>
</drv>

<drv mrk="kompli.0a">
  <kap><tld/>a</kap>
  <snc mrk="kompli.0a.malsimpliga">
    <dif>
      Rilata al <tld/>ado; <tld/>anta:
      <ekz>
        komplika faktoro
        <fnt><bib>PMEG</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="kompli.0a.EVI">
    <uzo tip="stl">EVI</uzo>
    <dif>
      &Gcirc;ene malsimpla:
      <ekz>
        *komplika rezonado*
        <fnt><bib>PMEG</bib></fnt>;
      </ekz>
      <ekz>
        *komplikaj reguloj*
        <fnt><bib>PMEG</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_a;&c_b;&c_l;&c_y;&c_t;&c_a;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">komplikovan&yacute;</trd>
  <trd lng="de">kompliziert</trd>
  <trdgrp lng="en">
    <trd>complicated</trd>,
    <trd>complex</trd>,
    <trd>intricate</trd>,
    <trd>involved</trd>,
    <trd>knotty</trd>
  </trdgrp>
  <trd lng="es">complicado</trd>
  <trdgrp lng="fr">
    <trd>compliqu&eacute;</trd>,
    <trd>complexe</trd>,
    <trd>confus<klr tip="ind"> (embrouill&eacute;)</klr></trd>,
    <trd>embrouill&eacute;</trd>,
    <trd>d&eacute;routant</trd>
    <trd>obscur<klr tip="ind"> (embrouill&eacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gecompliceerd</trd>,
    <trd>ingewikkeld</trd>
  </trdgrp>
  <trd lng="pl">skomplikowany</trd>
  <trdgrp lng="ro">
    <trd>complex</trd>,
    <trd>sofisticat</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_u;&c_s;&c_l;&c_o;&c_zh;&c_n;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_u;&c_t;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>komplikovan&yacute;</trd>,
    <trd>zlo&zcaron;it&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="kompli.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Cirkonstanco, kiu embarasas, malsimpligas, ka&ubreve;zas zorgon a&ubreve;
      penon:
      <ekz>
        la politikaj <tld/>a&jcirc;oj;
      </ekz>
      <ekz>
        la unuan fojon en &scirc;ia vivo la sociaj <tld/>a&jcirc;oj kaj problemoj
        aperis anta&ubreve; &scirc;iaj okuloj
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Komplikation</trd>
  <trd lng="es">complicaci&oacute;n</trd>
  <trd lng="fr">complication<klr tip="ind"> (situation
  compliqu&eacute;e)</klr></trd>
  <trd lng="nl">verwikkeling</trd>
  <trdgrp lng="pl">
    <trd>powik&lstroke;anie</trd>,
    <trd>komplikacja</trd>,
    <trd>gmatwanina</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>complexitate</trd>,
    <trd>complica&#539;ie</trd>,
    <trd>incurc&abreve;ri</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_o;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_s;&c_l;&c_o;&c_zh;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="kompli.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      I&gcirc;i &gcirc;ene malsimpla:
      <ekz>
        la malsano dan&gcirc;ere <tld/>i&gcirc;as;
      </ekz>
      <ekz>
        la batalo kontra&ubreve; la <ind><tld/>i&gcirc;o</ind>j de la vivo,
        la &scirc;an&gcirc;igoj de la sorto
        <fnt><bib>Marta</bib></fnt>.
        <trd lng="fr">complication<klr tip="ind"> (situation
        compliqu&eacute;e)</klr></trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_s;&c_k;&c_l;&c_a;&c_d;&c_n;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="de">
    <trd>verkomplizieren (sich)</trd>,
    <trd>kompliziert werden</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>complexifier</ind></trd>,
    <trd>conna&icirc;tre des <ind>complications</ind></trd>,
    <trd>se <ind>compliquer</ind></trd>,
    <trd>s'<ind>embrouiller</ind></trd>,
    <trd>s'<ind>obscurcir</ind><klr tip="ind"> (se
    compliquer)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd><ind>gecompliceerd</ind> worden</trd>,
    <trd><ind>ingewikkeld</ind> worden</trd>
  </trdgrp>
  <trd lng="pl">komplikowa&cacute; si&eogonek;</trd>     
  <trd lng="ro">complica</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_l;&c_o;&c_zh;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_o;&c_s;&c_l;&c_o;&c_zh;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
