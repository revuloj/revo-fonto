<kap>
  <ofc>1</ofc>
  <rad>komplik</rad>/i
</kap>
<drv mrk="kompli.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kompli.0i.malsimpligi">
    <dif>
      Embarase, malfacilige malsimpligi:
      <ekz>
        en la lingvoj naturaj ekzistas la plej <tld/>itaj kaj konfuzitaj deklinacioj kaj
        konjugacioj
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        nenio en la mondo estas simpla kaj facila, &ccirc;io estas <tld/>ita kaj &ccirc;irka&ubreve;ita de
        malhelpoj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiel do decidi tiun &ccirc;i <tld/>itan demandon
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Pri la homaranismo</vrk>
        </fnt>?
      </ekz>
      <ekz>
        la demandoj, malgra&ubreve; la &scirc;ajna <tld/>iteco, ne prezentas tamen ian malfacilecon
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ne konsciis precize la tre <tld/>itan fenomenon, kiu regis &scirc;ian sorton
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni povis ricevi kapturnon de tio, kiel la homoj en tiu <tld/>ita amaso da
        stratoj estis pakitaj unuj super la aliaj
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;io, kion ili diris, estis tiel &scirc;ra&ubreve;bita kaj <tld/>ita, tiel terure sa&gcirc;a, ke ili
        tuj ricevis rangojn kaj donacojn
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la provoj estas pli <tld/>aj kaj celmaltrafe preterpafe <klr>[&#x2026;]</klr>
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
          <lok>Vagonrestoracio</lok>
        </fnt>;
      </ekz>
      <ekz>
        eble <klr>[&#x2026;]</klr> en la estonteco la rubpo&scirc;taj programoj kutimos reprovi, sed
        tio serioze <tld/>os ilian taskon
        <fnt>
          <bib>Monato</bib>
          <aut>Franck Arnaud</aut>
          <vrk><url ref="&Monato;/2003/008177.html">Kia solvo por rubpo&scirc;to?</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        pluraj problemoj pli<tld/>as la tradukan procezon, kiel ekzemple mankantaj
        vortoj, tradukado de personaj nomoj, ortografiaj konvencioj
        <fnt>
       <aut>Marc Vanden Bempt</aut>
          <vrk><url ref="&Monato;/2000/006846.php">Perkomputila tradukado</url></vrk>
          <bib>Monato</bib>
          <lok>2000/07, p. 11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_s;&c_k;&c_l;&c_a;&c_d;&c_n;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="cs">komplikovat</trd>
  <trd lng="de">verkomplizieren</trd>
  <trd lng="en">complicate</trd>
  <trd lng="es">complicar</trd>
  <trdgrp lng="fr">
    <trd>complexifier</trd>,
    <trd>compliquer</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8907;&#x96D1;&#x306B;&#x3059;&#x308B;  [<ind>&#x3075;&#x304F;&#x3056;&#x3064;&#x306B;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x8FBC;&#x307F;&#x5165;&#x3089;&#x305B;&#x308B;  [<ind>&#x3053;&#x307F;&#x3044;&#x3089;&#x305B;&#x308B;</ind>]</trd>,
    <trd>&#x3084;&#x3084;&#x3053;&#x3057;&#x304F;&#x3059;&#x308B;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>compliceren</trd>,
    <trd><ind>ingewikkeld</ind> maken</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wik&lstroke;a&cacute;</trd>,
    <trd>komplikowa&cacute;</trd>,
    <trd>gmatwa&cacute;</trd>,
    <trd>utrudnia&cacute;</trd>
  </trdgrp> 
  <trd lng="pt">complicar</trd>
  <trdgrp lng="ro">
    <trd>complica</trd>,
    <trd>&icirc;mpiedica</trd>,
    <trd>&icirc;ncurca</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_l;&c_o;&c_zh;&c_n;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_s;&c_l;&c_o;&c_zh;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">komplikova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&c_s;&#x43A;&#x43B;&c_a;&c_d;&#x43D;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&#x43F;&#x43B;&c_u;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="kompli.0a">
  <kap><tld/>a</kap>
  <snc mrk="kompli.0a.malsimpliga">
    <dif>
      Rilata al <tld/>ado; <tld/>anta:
      <ekz>
        komplika faktoro
        <fnt><bib>PMEG</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="de">komplizierend</trd>
  </snc>
  <snc mrk="kompli.0a.EVI">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <tld lit="K"/>ita, <ref cel="simpl.mal0a">malsimpla</ref>:
      <ekz>
        dudeksistema kalkulado estigas ege longajn kaj <tld/>ajn nombrojn: 77 i&gcirc;as &leftquot;tri
        dudekoj kaj dek sep&rightquot;
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/1993/A931103.php">Nova gaela numerala sistemo</url></vrk>
          <bib>Monato</bib>
          <lok>1993/11, p. 21</lok>
        </fnt>;
      </ekz>
      <ekz>
        dramecaj uzoj de <tld/>aj aparatoj
        <fnt>
          <aut>Marjorie Boulton</aut>
          <vrk><url ref="&Monato;/1994/A940201.php">Dinomanio</url></vrk>
          <bib>Monato</bib>
          <lok>1994/02, p. 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ricevo de apudtelevida sonkanalo ne estas tiom <tld/>a afero
        <fnt>
          <aut>Pejno Simono</aut>
          <vrk><url ref="&Monato;/1995/004358.html">Poloj per pelvo</url></vrk>
          <bib>Monato</bib>
          <lok>1995/04, p. 17</lok>
        </fnt>;
      </ekz>
      <ekz>
        la frazostrukturo jen kaj jen estas tro <tld/>a
        <fnt>
          <aut>Guido kaj Karlijn van Damme</aut>
            <vrk><url ref="&Monato;/2000/007056.php">Edukemaj bestoj</url></vrk>
            <bib>Monato</bib>
            <lok>2000/11, p. 26</lok>
          </fnt>.
      </ekz>
    </dif>  
    <refgrp tip="vid">
       <ref cel="komple.0a.malsimpla">kompleksa<sncref/></ref>,
       <ref cel="konfuz.0a">konfuza</ref>
    </refgrp>
    <rim>
      Tiu &ccirc;i uzo ignoras la transitivecon de la verbo &leftquot;<tld/>i&rightquot;. 
      Simile kelkaj uzas &leftquot;<ref tip="hom" cel="vek.0i">veka</ref>&rightquot; anstata&ubreve; &leftquot;maldorma&rightquot;, &leftquot;vekita&rightquot;
      kaj &leftquot;<ref cel="korekt.0a.EVI">korekta</ref>&rightquot; anstata&ubreve; &leftquot;&gcirc;usta&rightquot;, &leftquot;korektita&rightquot;.
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_a;&c_b;&c_l;&c_y;&c_t;&c_a;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">komplikovan&yacute;</trd>
  
  <trdgrp lng="en">
    <trd>complicated</trd>,
    <trd>complex</trd>,
    <trd>intricate</trd>,
    <trd>involved</trd>,
    <trd>knotty</trd>
  </trdgrp>
  <trd lng="es">complicado</trd>
  <trdgrp lng="fr">
    <trd>compliqu&eacute;</trd>,
    <trd>complexe</trd>,
    <trd>confus<klr tip="ind"> (embrouill&eacute;)</klr></trd>,
    <trd>embrouill&eacute;</trd>,
    <trd>d&eacute;routant</trd>
    <trd>obscur<klr tip="ind"> (embrouill&eacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8907;&#x96D1;&#x306A;  [<ind>&#x3075;&#x304F;&#x3056;&#x3064;&#x306A;</ind>]</trd>,
    <trd>&#x8FBC;&#x307F;&#x5165;&#x3063;&#x305F;  [<ind>&#x3053;&#x307F;&#x3044;&#x3063;&#x305F;</ind>]</trd>,
    <trd>&#x3084;&#x3084;&#x3053;&#x3057;&#x3044;</trd>,
    <trd>&#x7169;&#x308F;&#x3057;&#x3044;  [<ind>&#x308F;&#x305A;&#x3089;&#x308F;&#x3057;&#x3044;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gecompliceerd</trd>,
    <trd>ingewikkeld</trd>
  </trdgrp>
  <trd lng="pl">skomplikowany</trd>
  <trdgrp lng="ro">
    <trd>complex</trd>,
    <trd>sofisticat</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_u;&c_s;&c_l;&c_o;&c_zh;&c_n;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_u;&c_t;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>komplikovan&yacute;</trd>,
    <trd>zlo&zcaron;it&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43A;&#x43B;&c_a;&c_d;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_a;&#x43F;&#x43B;&c_u;&c_t;&c_a;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="kompli.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="kompli.0ajxo.cirkonstanco">
    <dif>
      Cirkonstanco, kiu embarasas, malsimpligas, ka&ubreve;zas zorgon a&ubreve;
      penon:
      <ekz>
        la politikaj <tld/>a&jcirc;oj;
      </ekz>
      <ekz>
        se vi havus de &scirc;i filon, <tld/>a&jcirc;oj povus okazi
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la unuan fojon en &scirc;ia vivo la sociaj <tld/>a&jcirc;oj kaj problemoj
        aperis anta&ubreve; &scirc;iaj okuloj
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_s;&c_k;&c_l;&c_a;&c_d;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">Komplikation</trd>
  <trd lng="es">complicaci&oacute;n</trd>
  <trd lng="fr">complication<klr tip="ind"> (situation
  compliqu&eacute;e)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x8907;&#x96D1;&#x306A;&#x554F;&#x984C;  [<ind>&#x3075;&#x304F;&#x3056;&#x3064;&#x306A;&#x3082;&#x3093;&#x3060;&#x3044;</ind>]</trd>,
    <trd>&#x5384;&#x4ECB;&#x4E8B;  [<ind>&#x3084;&#x3063;&#x304B;&#x3044;&#x3054;&#x3068;</ind>]</trd>,
    <trd>&#x3082;&#x3081;&#x4E8B;  [<ind>&#x3082;&#x3081;&#x3054;&#x3068;</ind>]</trd>,
    <trd>&#x5408;&#x4F75;&#x75C7;  [<ind>&#x304C;&#x3063;&#x307A;&#x3044;&#x3057;&#x3087;&#x3046;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">verwikkeling</trd>
  <trdgrp lng="pl">
    <trd>powik&lstroke;anie</trd>,
    <trd>komplikacja</trd>,
    <trd>gmatwanina</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>complexitate</trd>,
    <trd>complica&#x21B;ie</trd>,
    <trd>incurc&abreve;ri</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_o;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_s;&c_l;&c_o;&c_zh;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_u;&c_s;&#x43A;&#x43B;&c_a;&c_d;&#x43D;&c_je;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43F;&#x43B;&c_u;&c_t;&c_a;&#x43D;&c_i;&#x43D;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="kompli.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      I&gcirc;i &gcirc;ene malsimpla:
      <ekz>
        la malsano dan&gcirc;ere <tld/>i&gcirc;as;
      </ekz>
      <ekz>
        la afero, anstata&ubreve; simpli&gcirc;i, <tld/>i&gcirc;as
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        la batalo kontra&ubreve; la <ind><tld/>i&gcirc;o</ind>j de la vivo,
        la &scirc;an&gcirc;igoj de la sorto
        <fnt><bib>Marta</bib></fnt>.
        <trd lng="fr">complication<klr tip="ind"> (situation
        compliqu&eacute;e)</klr></trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_s;&c_k;&c_l;&c_a;&c_d;&c_n;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="de">
    <trd>sich <ind>verkomplizieren</ind></trd>,
    <trd><ind>kompliziert</ind> werden</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>complexifier</ind></trd>,
    <trd>conna&icirc;tre des <ind>complications</ind></trd>,
    <trd>se <ind>compliquer</ind></trd>,
    <trd>s'<ind>embrouiller</ind></trd>,
    <trd>s'<ind>obscurcir</ind><klr tip="ind"> (se
    compliquer)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8907;&#x96D1;&#x306B;&#x306A;&#x308B;  [<ind>&#x3075;&#x304F;&#x3056;&#x3064;&#x306B;&#x306A;&#x308B;</ind>]</trd>,
    <trd>&#x8FBC;&#x307F;&#x5165;&#x3063;&#x3066;&#x304F;&#x308B;  [<ind>&#x3053;&#x307F;&#x3044;&#x3063;&#x3066;&#x304F;&#x308B;</ind>]</trd>,
    <trd>&#x3084;&#x3084;&#x3053;&#x3057;&#x304F;&#x306A;&#x308B;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd><ind>gecompliceerd</ind> worden</trd>,
    <trd><ind>ingewikkeld</ind> worden</trd>
  </trdgrp>
  <trd lng="pl">komplikowa&cacute; si&eogonek;</trd>     
  <trd lng="ro">complica</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_l;&c_o;&c_zh;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_o;&c_s;&c_l;&c_o;&c_zh;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
