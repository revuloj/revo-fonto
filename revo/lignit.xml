<vortaro>
<art mrk="$Id$">
<kap>
<rad>lignit</rad>/o
</kap>
<drv mrk="lignit.0o">
<kap><tld/>o</kap>
<uzo tip="fak">MIN</uzo>
<snc>
<dif>
Speco de fosilia <ref cel="karb.0o">karbo</ref>, tre ofte prezentanta 
la &scirc;ajnon de duonbrulinta ligno.  
</dif>
<refgrp tip="super">
  <ref cel="brul.0igajxo">bruliga&jcirc;o</ref>.
</refgrp>
</snc>
<trdgrp lng="en">
  <trd>lignite</trd>,
  <trd>brown coal</trd>
</trdgrp>
<trd lng="es">lignito</trd>
<trd lng="fr">lignite</trd>
<trdgrp lng="nl"><trd>ligniet</trd>,
  <trd>bruinkool</trd></trdgrp>
</drv>
</art>
