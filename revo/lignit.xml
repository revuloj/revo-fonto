<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>lignit</rad>/o
</kap>
<drv mrk="lignit.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Speco de fosilia <ref cel="karb.0o">karbo</ref>, tre ofte prezentanta
      la &scirc;ajnon de duonbrulinta ligno.
    </dif>
    <refgrp tip="super">
      <ref cel="brul.0igajxo">bruliga&jcirc;o</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="en">
    <trd>lignite</trd>,
    <trd>brown coal</trd>
  </trdgrp>
  <trd lng="es">lignito</trd>
  <trd lng="fr">lignite</trd>
  <trdgrp lng="nl">
    <trd>ligniet</trd>,
    <trd>bruinkool</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_i;&c_g;&c_n;&c_i;&c_t;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_y;&c_j; &c_u;&c_g;&c_o;&c_l;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
