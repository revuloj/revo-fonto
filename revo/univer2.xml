<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>universitat</rad>/o
</kap>
<drv mrk="univer2.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="univer2.0o.PED">
  <uzo tip="fak">PED</uzo>
    <dif>
      Centro de supera instruado, dividita la&ubreve; fakoj (fakultatoj),
      kies &ccirc;iu disdonas titolojn (licencio, magistro, doktoro):
      <ekz>
        evidenti&gcirc;is, ke 92 % de tiuj, kiuj malbone plenumis la teston,
        malsukcesis anka&ubreve; &ccirc;e la <tld/>o<fnt><aut>Guido Van Damme</aut>:
        <vrk>Studentoj universitaton soifantaj</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 2a, p. 12a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_n;&c_ib;&c_v;&c_e;&c_r;&c_s;&c_ib;&c_t;&c_e;&c_t;</trd>
  <trd lng="br">skol-veur</trd>
  <trd lng="cs">univerzita</trd>
  <trdgrp lng="de">
    <trd>Universit&auml;t</trd>,
    <trd>Hochschule</trd>
  </trdgrp>
  <trd lng="el">&pi;&alfa;&ny;&epsilon;&pi;&jota;&sigma;&tau;&eta_ton;&my;&jota;&omikron;</trd>
  <trd lng="en">university</trd>
  <trd lng="fr">universit&eacute;</trd>
  <trd lng="hu">egyetem</trd>
  <trd lng="it">universit&agrave;</trd>
  <trd lng="nl">universiteit</trd>
  <trd lng="pl">uniwersytet</trd>
  <trd lng="pt">universidade</trd>
  <trd lng="ru">&c_u;&c_n;&c_i;&c_v;&c_je;&c_r;&c_s;&c_i;&c_t;&c_je;&c_t;</trd>
  <trd lng="sv">universitet</trd>
  <trd lng="tr">&uuml;niversite</trd>
  <trd lng="sk">univerzita</trd>
</drv>
</art>
