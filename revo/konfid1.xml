<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>konfidenc</rad>/o
</kap>
<drv mrk="konfid1.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Sciigo, kiun oni konfidas al iu, kaj kiun li ne devas malsekretigi:
      <ekz>
        diri, fari al iu siajn <tld/>ojn;
      </ekz>
      <ekz>
        ricevi, preni, altiri la <tld/>ojn de iu;
      </ekz>
      <ekz>
        <tld/>a letero.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">vertrouwelijke mededeling</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_n;&c_f;&c_i;&c_d;&c_je;&c_n;&c_c;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_je; &c_s;&c_o;&c_o;&c_b;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
<drv mrk="konfid1.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="dif" cel="konfid.0i.3">Konfidi 3</ref>:
      <ekz>
        <tld/>i siajn sentojn, sekretojn al amiko
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="sekret.0i">sekreti</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_v;&c_je;&c_r;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_o;&c_o;&c_b;&c_shch;&c_i;&c_t;&c_mol; &c_p;&c_o; &c_s;&c_je;&c_k;&c_r;&c_je;&c_t;&c_u;</trd>
  </trdgrp>
</drv>
<drv mrk="konfid1.0ema">
  <kap>
    <tld/>ema
  </kap>
  <snc>
    <dif>
      Facile diranta siajn <tld/>ojn; malka&scirc;ema, komuniki&gcirc;ema
    </dif>
    <refgrp tip="sin">
      <ref cel="babil">babilema</ref>,
      <ref cel="anim">vantanima</ref>,
      <ref cel="versx">korelver&scirc;ema</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">mededeelzaam</trd>
</drv>
</art>
