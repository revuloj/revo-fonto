<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: anksi.xml,v 1.12 2022/03/12 10:16:48 revo Exp $">
<kap>
   <rad>anksi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="anksi.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">PSI</uzo>
   <snc>
     <dif>
       Da&ubreve;ra <ref tip="super" cel="tim.antaux0o">anta&ubreve;timo</ref> kaj
       anima perturbo, e&ccirc; sen videbla motivo.
     </dif>
   </snc>
  <trdgrp lng="bg">
    <trd>&c_b;&c_je;&c_z;&c_p;&c_o;&c_k;&c_o;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_v;&c_o;&c_g;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">ansietat</trd>
  <trd lng="cs">ankxie</trd>
  <trd lng="de">Angstst&ouml;rung</trd>
  <trd lng="en">anxiety</trd>
  <trd lng="es">ansiedad</trd>
  <trd lng="fr">anxi&eacute;t&eacute;</trd>
  <trd lng="hu">szorong&aacute;s</trd>
  <trd lng="it">ansiet&agrave;</trd>
  <trd lng="ms">kebimbangan</trd>
  <trd lng="nl">ongerustheid</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_v;&c_o;&c_g;&c_a;</trd>,
    <trd>&c_o;&c_z;&c_a;&c_b;&c_o;&c_ch;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_p;&c_o;&c_k;&c_o;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>obava</trd>,
    <trd>tiese&ncaron;</trd>
  </trdgrp>
</drv>

<drv mrk="anksi.0a">
   <kap><tld/>a</kap>
   <snc>
     <dif>
       Perturbata pro konfuza timo, nervozega.
     </dif>
   </snc>
  <trdgrp lng="bg">
    <trd>&c_r;&c_a;&c_z;&c_t;&c_r;&c_je;&c_v;&c_o;&c_zh;&c_je;&c_n;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_p;&c_o;&c_k;&c_o;&c_je;&c_n;</trd>
  </trdgrp>
  <trd lng="ca">ansi&oacute;s</trd>
  <trdgrp lng="cs">
    <trd>teskliv&yacute;</trd>,
    <trd>t&iacute;sniv&yacute;</trd>,
    <trd>&uacute;zkostliv&yacute;</trd>
  </trdgrp>
  <trd lng="en">anxious</trd>
  <trd lng="es">ansioso</trd>
  <trd lng="fr">anxieux</trd>
  <trd lng="he">&het;&samekh;&resh; &mem;&nun;&vav;&het;&he;</trd>
  <trd lng="hu">szorong&oacute;</trd>
  <trdgrp lng="nl">
    <trd>ongerust</trd>,
    <trd>bezorgd</trd>
  </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_t;&c_o;&c_s;&c_k;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>,
     <trd>&c_t;&c_r;&c_je;&c_v;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>,
     <trd>&c_o;&c_z;&c_a;&c_b;&c_o;&c_ch;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
     <trd>&c_b;&c_je;&c_s;&c_p;&c_o;&c_k;&c_o;&c_j;&c_n;&c_y;&c_j;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>tiesniv&yacute;</trd>,
    <trd>&uacute;zkostliv&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x435;&#x447;&#x430;&#x43B;&#x44C;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x442;&#x440;&#x438;&#x432;&#x43E;&#x436;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x441;&#x442;&#x443;&#x440;&#x431;&#x43E;&#x432;&#x430;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="anksi.0e">
   <kap><tld/>e</kap>
   <snc>
     <dif>
       Maltrankvile pro anta&ubreve;timo:
       <ekz>
         <tld/>e li &gcirc;emadis
         <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 73a, p. 533a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="en">anxiously</trd>
   <trd lng="fr">anxieusement</trd>
   <trd lng="hu">szorongva</trd>
   <trd lng="nl">onrustig</trd>
   <trdgrp lng="ru">
     <trd>&c_t;&c_r;&c_je;&c_v;&c_o;&c_zh;&c_n;&c_o;</trd>,
     <trd>&c_s; &c_m;&c_u;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_m;
     <ind>&c_b;&c_je;&c_s;&c_p;&c_o;&c_k;&c_o;&c_j;&c_s;&c_t;&c_v;&c_o;&c_m;</ind></trd>
   </trdgrp>
</drv>
</art>

