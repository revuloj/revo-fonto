<art mrk="$Id: dioptr1.xml,v 1.17 2023/10/14 11:26:53 revo Exp $">
<kap>
<rad>dioptrik</rad>/o
</kap>
<drv mrk="dioptr1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">FOT</uzo>
  <snc mrk="dioptr1.0o.FIZ">
    <dif>
      Bran&ccirc;o de <ref tip="super" cel="fizik.0o">fiziko</ref>, kiu 
      pritraktas la <ref cel="refrak.0o">refrakton</ref> de lumaj radioj tra
      diversaj medioj: 
      <ekz>
        en sia fama verko de 1611 pritraktanta <tld/>on, <ref cel="kepler.0o">Keplero</ref>
        kreis la fundamenton de la <ref tip="malprt" cel="optik.0o">optiko</ref> kiel scienco.
      </ekz>
    </dif>
    <adm>
      Bran&ccirc;o de fiziko a&ubreve; optiko? <aut>MB</aut>
    </adm>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_jo;&c_p;&c_t;&c_r;&c_y;&c_k;&c_a;</trd>
  <trd lng="bg">&c_d;&c_i;&c_o;&c_p;&c_t;&c_r;&c_i;&c_k;&c_a;</trd>
  <trd lng="cs">dioptrika</trd>
  <trdgrp lng="de">
     <trd>Brechungslehre</trd>,
     <trd>Dioptrik</trd>
  </trdgrp>
  <trd lng="en">dioptrics</trd>
  <trd lng="fr">dioptrique</trd>
  <trdgrp lng="hu">
     <trd>dioptrika</trd>,
     <trd>sug&aacute;rt&ouml;r&eacute;stan</trd>
  </trdgrp>
  <trd lng="ja">&#x5C48;&#x6298;&#x5149;&#x5B66;  <pr>&#x304F;&#x3063;&#x305B;&#x3064;&#x3053;&#x3046;&#x304C;&#x304F;</pr></trd>
  <trd lng="nl">dioptrica</trd>
  <trdgrp lng="pt">
    <trd>di&oacute;ptrica</trd>,
    <trd>anacl&aacute;stica</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_i;&c_o;&c_p;&c_t;&c_r;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">dioptrika</trd>
  <trd lng="sv">dioptrik</trd>
  <trd lng="uk">&#x434;&#x456;&#x43E;&#x43F;&#x442;&#x440;&#x438;&#x43A;&#x430;</trd>
</drv>
</art>
