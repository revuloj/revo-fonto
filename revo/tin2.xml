<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tin</rad> <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tin2.00">
  <kap><tld/>-<tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="tin2.00.SON">
    <dif>
      Bruo de iom akuta <ref tip="vid" cel="tint.0o">tinto</ref>,
      precipe tiu de batata metala&jcirc;o:
      <ekz>
        &leftquot;<tld/>, <tld/>&rightquot; sonoris la sonoriletoj
        <fnt><bib>Fab2</bib>, <lok>ne&gcirc;a re&gcirc;ino, fabelo en sep rakontoj,
        tria rakonto, la flor&gcirc;ardeno de la virino, kiu povosciis
        sor&ccirc;i</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>ding-dong</trd>,
    <trd>dring-dring</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>ding-dong</trd>
  </trdgrp>
</drv>
</art>

