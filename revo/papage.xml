<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: papage.xml,v 1.10 2023/01/09 18:33:42 revo Exp $">
<kap><rad>papaged</rad>/oj</kap>
<drv mrk="papage.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="papag.0oformaj">papagoformaj
      birdoj</ref> (<trd lng="la">Psittacidae</trd>):
      <ekz>
        <tld/>oj a&ubreve; psitakedoj estas familio de birdoj kun dika kaj forta beko, kaj ofte
        multkolora pluma&jcirc;o, oni distingas 332 speciojn en du subfamilioj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16262"
              >Papagedoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_p;&c_u;&c_g;&c_a;&c_je;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  <trd lng="ca">psit&agrave;cids</trd>
  <trd lng="de">Papageien</trd>
  <trd lng="hu">papag&aacute;lyf&eacute;l&eacute;k</trd>
  <trd lng="nl">papegaaien<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pl">papugowate</trd>
  <trd lng="pt">psitac&iacute;deos</trd>
  <trd lng="ro">psittaciformes</trd>
  <trd lng="ru">&c_g;&c_l;&c_a;&c_d;&c_k;&c_o;&c_ja;&c_z;&c_y;&c_ch;&c_n;&c_y;&c_je; <ind>&c_p;&c_o;&c_p;&c_u;&c_g;&c_a;&c_i;</ind> <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
