<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <ofc>*</ofc>
    <rad>datur</rad>/o <fnt><bib>PV</bib></fnt>
  </kap>
  <drv mrk="datur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="datur.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Tre venena vegeta&jcirc;o, <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt"
      cel="solana.0oj">solanacoj</ref>
      (<trd lng="la">Datura</trd>):
      <ekz>
        <uzo tip="stl">FIG</uzo>
        nian sanan spiriton, plenan de azia praforto, ni
        venenis per la e&ubreve;ropa <tld/>-infuza&jcirc;o
        <fnt><aut>J. Baghy</aut>, <vrk>Sur Sanga Tero</vrk>, 
        <lok>1933</lok></fnt>.
      </ekz>  
    </dif>
    </snc>
  <trd lng="be">&c_d;&c_u;&c_r;&c_n;&c_a;&c_p;'&c_ja;&c_n;</trd>
  <trdgrp lng="br">
    <trd>aval-spern</trd>,
    <trd>mezverez<klr tip="ind"> (aval-spern)</klr></trd>
  </trdgrp>
  <trd lng="de">Stechapfel</trd>
  <trdgrp lng="en">
    <trd>thorn apple</trd>,
    <trd>jimson weed</trd>
  </trdgrp>
  <trd lng="fr">datura</trd>
  <trd lng="hu">maszlag</trd>
  <trd lng="pl">bielu&nacute;</trd>
  <trdgrp lng="pt">
    <trd>estram&ocirc;nio</trd>,
    <trd>datura</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_u;&c_r;&c_m;&c_a;&c_n;</trd>
  </drv>
</art>
