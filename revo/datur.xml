<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>datur</rad>/o
</kap>
<drv mrk="datur.0o">
<kap><ofc>*</ofc><tld/>o</kap>
<uzo tip="fak">BOT</uzo>
<snc>
<dif>
Tre venena veg., el fam. solanacoj (datura).  
</dif>
</snc>
<trd lng="be">&c_d;&c_u;&c_r;&c_n;&c_a;&c_p;'&c_ja;&c_n;</trd>
<trdgrp lng="br">
  <trd>aval-spern</trd>,
  <trd>mezverez<klr tip="ind"> (aval-spern)</klr></trd>
</trdgrp>
<trd lng="de">Stechapfel</trd>
<trdgrp lng="en">
  <trd>thorn apple</trd>,
  <trd>jimson weed</trd>
</trdgrp>
<trd lng="fr">datura</trd>
<trd lng="hu">maszlag</trd>
<trd lng="ru">&c_d;&c_u;&c_r;&c_m;&c_a;&c_n;</trd>
</drv>
</art>
