<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>dekad</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="dekad.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="dekad.0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Tagdeko en kelkaj kalendaroj:
      <ekz>
        malfeli&ccirc;a knabino, &scirc;i ne vidis min preska&ubreve; du <tld/>ojn
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 18a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="semajn.0o.KAL">semajno <sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="fr">d&eacute;cade<klr tip="amb"> (dix jours)</klr></trd>
  <trd lng="pl">dekada</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_k;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_d;&c_je;&c_s;&c_ja;&c_t;&c_i;&c_d;&c_n;&c_je;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>

