<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kult</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kult.0o">
  <kap><tld/>o</kap>
  <snc mrk="kult.0o.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Religia ceremoniaro, diservado.
    </dif>
    <trdgrp lng="de">
      <trd>Kultus</trd>,
      <trd>Gottesverehrung</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>cult<klr tip="amb"> (system of religious worship)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kultusz</trd>,
      <trd>istentisztelet</trd>,
      <trd>vall&aacute;sgyakorlat</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>cultus</trd>,
      <trd>eredienst</trd>
    </trdgrp>
  </snc>
  <snc mrk="kult.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Respektego, kvaza&ubreve;religia adorado:
      <ekz>
        ne amindumadon &scirc;i postulis, sed <tld/>on;
      </ekz>
      <ekz>
        senti <tld/>on por ies memoro, por la vereco, por la arto.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Kult</trd>,
      <trd>Verehrung</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>devotion</trd>,
      <trd>homage</trd>,
      <trd>worship</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kultusz</trd>,
      <trd>im&aacute;dat</trd>
    </trdgrp>
    <trd lng="nl">verering</trd>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_l;&c_mol;&c_t;</trd>
  <trd lng="es">culto</trd>
  <trd lng="fr">culte</trd>
  <trd lng="pt">culto</trd>
  <trd lng="ru">&c_k;&c_u;&c_l;&c_mol;&c_t;</trd>
</drv>

<drv mrk="kult.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="kult.0i.REL">
    <dif>
      Honori per <tld/>o:
      <ekz>
        <tld/>i Dion;
      </ekz>
      <ekz>
        <tld/>i al virino, al la prauloj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
      <trd>Kult treiben</trd>,
      <trd>kultisch verehren</trd>,
      <trd>verehren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>idolize</trd>,
    <trd>venerate</trd>,
    <trd>worship</trd>
  </trdgrp>
  <trd lng="es">honrar<klr tip="amb"> (con culto)</klr></trd>
  <trdgrp lng="fr">
    <trd>adorer</trd>,
    <trd>rendre un <ind>culte</ind> &agrave;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kultuszt gyakorol</trd>,
    <trd>kultuszt csin&aacute;l</trd>,
    <trd>im&aacute;d</trd>,
  </trdgrp>
  <trd lng="nl">vereren</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_z;&c_v;&c_je;&c_s;&c_t;&c_i; &c_v; &c_k;&c_u;&c_l;&c_mol;&c_t;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_l;&c_o;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="kult.idol0o">
  <kap>idol<tld/>o</kap>
  <snc mrk="kult.idol0o.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      <tld lit="K"/>o, adoro de
      <ref tip="vid" cel="idol.0o.REL">idolo</ref>;
      <ref tip="sin" cel="idolatri.0o.REL">idolatrio</ref>:
      <ekz>
        iradi en dibo&ccirc;oj, voluptoj, vindrinkado, brufestenoj,
        ebrieco, kaj abomenaj idol<tld/>oj
        <fnt><bib>NT</bib>, <lok>&Pe1; 4:3</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">idolatrie</trd>
</drv>

<drv mrk="kult.person0o">
  <kap>person<tld/>o</kap>
  <snc mrk="kult.person0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Troa idealigo kaj la&ubreve;dado de regnestro,
      grupestro a&ubreve; ajna gvidanto:
      <ekz>
        doma&gcirc;a estas certa person<tld/>o &ccirc;irka&ubreve; Ivo Lapenna
        <fnt>
          <aut>Z. M. Sikosek</aut>,
          <vrk><url
          ref="http://eo.lernu.net/biblioteko/pri_esperanto/eta_historio/rekonoj.php">
          Eta historio de Esperanto</url></vrk>,
          <lok>1997</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">culte de la personnalit&eacute;</trd>
</drv>
</art>
