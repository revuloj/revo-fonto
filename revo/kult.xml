<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kult</rad>/o
</kap>
<drv mrk="kult.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Religia ceremoniaro, diservado.
    </dif>
    <trdgrp lng="de">
      <trd>Kultus</trd>,
      <trd>Gottesverehrung</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>cultus</trd>,
      <trd>eredienst</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Respektego, kvaza&ubreve;religia adorado:
      <ekz>
        ne amindumadon &scirc;i
        postulis, sed <tld/>on;
      </ekz>
      <ekz>
        senti <tld/>on por ies memoro, por la vereco, por la arto.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Kult</trd>,
      <trd>Verehrung</trd>
    </trdgrp>
    <trd lng="nl">verering</trd>
  </snc>
  <trd lng="ru">&c_k;&c_u;&c_l;&c_mol;&c_t;</trd>
</drv>

<drv mrk="kult.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Honori per <tld/>o:
      <ekz>
        <tld/>i Dion;
      </ekz>
      <ekz>
        <tld/>i al virino, al la prauloj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
      <trd>Kult treiben</trd>,
      <trd>kultisch verehren</trd>,
      <trd>verehren</trd>
  </trdgrp>
  <trd lng="nl">vereren</trd>
</drv>
</art>
