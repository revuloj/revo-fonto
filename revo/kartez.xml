<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>Kartezi</rad>/o
</kap>
<drv mrk="kartez.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 46</lok></fnt>
  <snc>
    <dif>
      Franclingve: Ren&eacute; <trd lng="fr">Descartes</trd>, 1596-1650. 
      Franca filozofo, matematikisto kaj fizikisto, multe voja&gcirc;inta en 
      la tiama E&ubreve;ropo.
    </dif>
    <trdgrp lng="de">
      <trd>Descartes</trd>,
      <trd>D&eacute;scartes</trd>,
      <trd>Cartesius</trd>
    </trdgrp>
    <trd lng="en">Descartes</trd>
    <trd lng="la">Renatus <ind>Cartesius</ind></trd>
    <trdgrp lng="nl">
      <trd>Descartes</trd>,
      <trd>Cartesius</trd>
    </trdgrp>
    <trd lng="ru">&c_R;&c_je;&c_n;&c_je; <ind>&c_D;&c_je;&c_k;&c_a;&c_r;&c_t;</ind></trd>
  </snc>
</drv>
<drv mrk="kartez.k0a">
  <kap><tld lit="k"/>a</kap>
  <snc>
    <dif>
      Iel rilatanta kun la filozofio a&ubreve; la matematikaj
      konceptoj de <tld/>o.
    </dif>
    <refgrp tip="vid">
      <ref cel="koordi.kartezia0o"><tld lit="k"/>a koordinato</ref>,
      <ref cel="produt.kartezia0o"><tld lit="k"/>a produto</ref>.
    </refgrp>
  </snc>
  <trd lng="de">kartesisch</trd>
  <trd lng="en">Cartesian</trd>
  <trd lng="fr">cart&eacute;sien</trd>
  <trd lng="nl">cartesiaans</trd>
  <trd lng="ru">&c_d;&c_je;&c_k;&c_a;&c_r;&c_t;&c_o;&c_v;</trd>
</drv>
</art>
