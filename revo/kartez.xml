<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>Kartezi</rad>/o
</kap>
<drv mrk="kartez.0o">
   <kap><tld/>o</kap>
   <fnt>PIV1</fnt>
   <snc>
     <dif>
       Franclingve: Ren&eacute; Descartes, 1596-1650. Franca filozofo,
       matematikisto kaj fizikisto, multe voja&gcirc;inta en la tiama
       E&ubreve;ropo.
     </dif>
     <trdgrp lng="de">
        <trd>Descartes</trd>,
        <trd>D&eacute;scartes</trd>
        <trd>Cartesius</trd></trdgrp>	
   </snc>
</drv>

<drv mrk="kartez.0a">
   <kap><tld lit="k"/>a</kap>
   <snc>
     <dif>
       Iel rilatanta kun la filozofio a&ubreve; la matematikaj
       konceptoj de <tld/>o.
     </dif>
     <refgrp tip="vid">
       <ref cel="koordi.kartezia0o"><tld lit="k"/>a koordinato</ref>,
       <ref cel="produt.kartezia0o"><tld lit="k"/>a produto</ref>.
     </refgrp>
   </snc>
   <trd lng="de">kartesisch</trd>
   <trd lng="fr">cart&eacute;sien</trd>
</drv>
</art>

