<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kartez.xml,v 1.25 2023/12/03 08:00:24 revo Exp $">
<kap>
  <rad>Kartezi</rad>/o
</kap>
<drv mrk="kartez.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 46</lok></fnt>
  <snc>
    <dif>
      Franclingve: Ren&eacute; <trd lng="fr">Descartes</trd>, 1596-1650;
      franca filozofo, <ref tip="lst" lst="voko:historiaj_personoj" cel="matema.0isto">matematikisto</ref>
      kaj fizikisto, multe voja&gcirc;inta en la tiama E&ubreve;ropo:
      <ekz>
        en 6-11 jarcentoj en Hindio, oni regule uzis negativajn nombrojn <klr>[&#x2026;]</klr>
        sed en E&ubreve;ropa scienco &gcirc;i eniris difinitive nur de tempoj de <tld/>o (17 jc),
        kiu donis al negativaj nombroj la signifon de direktitaj eltran&ccirc;oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1032">Entjero</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_D;&c_e;&c_k;&c_a;&c_r;&c_t;</trd>
    <trdgrp lng="de">
      <trd>Descartes</trd>,
      <trd>D&eacute;scartes</trd>,
      <trd>Cartesius</trd>
    </trdgrp>
    <trd lng="bg">&c_R;&c_je;&c_n;&c_je; <ind>&c_D;&c_je;&c_k;&c_a;&c_r;&c_t;</ind></trd>
    <trd lng="br">Descartes</trd>
    <trd lng="en">Descartes</trd>
    <trd lng="es">Descartes</trd>
    <trd lng="hu">Descartes</trd>
    <trd lng="it">Cartesio</trd>
    <trd lng="la">Renatus <ind>Cartesius</ind></trd>
    <trdgrp lng="nl">
      <trd>Descartes</trd>,
      <trd>Cartesius</trd>
    </trdgrp>
    <trd lng="pl">Kartezjusz</trd>
    <trdgrp lng="ru">
      <trd>&c_R;&c_je;&c_n;&c_je; <ind>&c_D;&c_je;&c_k;&c_a;&c_r;&c_t;</ind></trd>,
      <trd>&c_K;&c_a;&c_r;&c_t;&c_je;&c_z;&c_i;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">Descartes Ren&eacute;</trd>
  <trd lng="ja">&#x30C7;&#x30AB;&#x30EB;&#x30C8;</trd>
  <trd lng="sk">Descartes Ren&eacute;</trd>
  <trd lng="uk">&#x414;&#x435;&#x43A;&#x430;&#x440;&#x442;</trd>
  <trd lng="zh">&#x7B1B;&#x5361;&#x513F; <pr>D&#xED;k&#x1CE;&#xE9;r</pr></trd>
</drv>

<drv mrk="kartez.k0a">
  <kap><tld lit="k"/>a</kap>
  <snc>
    <dif>
      Iel rilatanta kun la filozofio a&ubreve; la matematikaj
      laboroj de <tld/>o.
    </dif>
    <refgrp tip="vid">
      <ref cel="koordi.kartezia0o"><tld lit="k"/>a koordinato</ref>,
      <ref cel="produt.kartezia0o"><tld lit="k"/>a produto</ref>,
      <ref cel="foli.kartezia0o"><tld lit="k"/>a folio</ref>,
      <ref cel="oval.kartezia0o"><tld lit="k"/>a ovalo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_D;&c_e;&c_k;&c_a;&c_r;&c_t;&c_a;</trd>,
    <trd>&c_d;&c_e;&c_k;&c_a;&c_r;&c_t;&c_a;&c_w;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_d;&c_je;&c_k;&c_a;&c_r;&c_t;&c_o;&c_v;&c_o;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_t;&c_je;&c_z;&c_i;&c_a;&c_n;&c_s;&c_k;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>-<ind>Descartes</ind></trd>,
    <trd>kartezian</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Descartes'</trd>,
    <trd>Descartesch</trd>,
    <trd>kartesisch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Descartes[']</trd>,
    <trd>Cartesian</trd>
  </trdgrp>
  <trd lng="es">cartesiano</trd>
  <trdgrp lng="fr">
    <trd>de <ind>Descartes</ind></trd>,
    <trd>cart&eacute;sien</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>Descartes-i</trd>,
    <trd>kartezi&aacute;nus</trd>
  </trdgrp>
  <trd lng="it">cartesiano</trd>
  <trdgrp lng="nl">
    <trd>van <ind>Descartes</ind></trd>,
    <trd>cartesiaans</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>Kartezjusza</trd>,
    <trd>kartezja&nacute;ski</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_D;&c_je;&c_k;&c_a;&c_r;&c_t;&c_a;</trd>,
    <trd>&c_d;&c_je;&c_k;&c_a;&c_r;&c_t;&c_o;&c_v;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_t;&c_je;&c_z;&c_i;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x414;&#x435;&#x43A;&#x430;&#x440;&#x442;&#x456;&#x432;</trd>,
    <trd>&#x43A;&#x430;&#x440;&#x442;&#x435;&#x437;&#x456;&#x430;&#x43D;&#x441;&#x44C;&#x43A;&#x438;&#x439;</trd>
  </trdgrp>
</drv>
</art>
