<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: avers.xml,v 1.11 2024/01/19 12:41:38 revo Exp $">
<kap>
  <rad>avers</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="avers.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="avers.0o.KOMUNE">
    <dif>
      &Ccirc;efa flanko,
      <ref tip="super" cel="front.0o.antauxo">fronto<sncref/></ref>
      de plata objekto, kiu estas normale
      elmetata al la vido,
      kontraste al
      <ref tip="ant" cel="revers.0o.KOMUNE">reverso</ref>:
      <ekz>
        en la familia arkivo mi trovis du po&scirc;tkartojn
        kun nigrablankaj reprodukta&jcirc;oj de bildoj <tld/>e
        <fnt>
          <aut>M. Ma&hcirc;na&ccirc;</aut>
          <vrk>Esperanta Saluto el la jaro 1946</vrk>
          <bib>LOdE</bib>
          <lok>1999:6 (56)</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de monero, &scirc;tofo, pa&gcirc;o, kaj similaj
      </ekz>.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_v;&c_e;&c_r;&c_s;</trd>
  <trd lng="ca">anvers</trd>
  <trdgrp lng="cs">
    <trd>averz</trd>,
    <trd>l&iacute;c</trd>,
    <trd>l&iacute;cn&iacute; strana</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bildseite</trd>,
    <trd>Vorderseite</trd>,
    <trd>Avers</trd>
  </trdgrp>
  <trd lng="es">anverso</trd>
  <trdgrp lng="fr">
    <trd>avers</trd>,
    <trd>pile<klr> (c&ocirc;t&eacute; pile)</klr>
  </trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>recto<klr> (di moneta, targa ecc..)</klr></trd>,
    <trd>dritto<klr> (di moneta, targa ecc..)</klr></trd>
  </trdgrp>
  <trd lng="sk">l&iacute;cna strana</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&c_i;&c_c;&#x44C;&#x43E;&c_v;&c_a;&#xA0;&c_s;&c_t;&#x43E;&c_r;&#x43E;&#x43D;&c_a;</trd>,
    <trd>&#x43B;&c_i;&c_c;&c_je;</trd>,
    <trd>&c_a;&c_v;&c_je;&c_r;&c_s;</trd>
  </trdgrp>
</drv>
</art>

