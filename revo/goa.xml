          <aut>M. de Seabra</aut>,
          <vrk>Malamu vin, unu la alian</vrk>,
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_G;&c_o;&c_a;</trd>
  <trd lng="fr">Goa</trd>
  <trd lng="nl">Goa</trd>
  <trd lng="no">Goa</trd>
</drv>
