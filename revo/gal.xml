<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
 
<vortaro>
  <art mrk="$Id$">
    <kap><ofc>*</ofc>
      <rad>gal</rad>/o</kap>
 
    <drv mrk="gal.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <uzo tip="fak">ANA</uzo>
      <snc>
        <dif>
          Maldol&ccirc;a, verde flava sekrecio de la hepato:
          <ekz>
            <uzo tip="stl">FIG</uzo>
            inciti la <tld/>on al iu<fnt>Z</fnt>
            <klr>(koleretigi)</klr>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_zh;&c_o;&c_w;&c_c;&c_mol;</trd>
      <trd lng="de">Galle</trd>
      <trd lng="el">&chi;&omikron;&lambda;&eta_ton;</trd>
      <trdgrp lng="en">
        <trd>gall</trd>,
        <trd>bile</trd>
      </trdgrp>
      <trd lng="es">bilis</trd>
      <trdgrp lng="fr">
        <trd>bile</trd>,
        <trd>fiel<klr tip="ind"> (bile)</klr></trd>
      </trdgrp>
      <trd lng="hu">epe</trd>
      <trd lng="nl">gal</trd>
      <trd lng="pt">b&iacute;lis</trd>
      <trdgrp lng="ru">
        <trd>&c_zh;&c_je;&c_l;&c_ch;&c_mol;</trd>,
        <trd>&c_zh;&c_jo;&c_l;&c_ch;&c_mol;</trd>
      </trdgrp>
      <trd lng="sv">galla</trd>
    </drv>
 
    <drv mrk="gal.0veziko">
      <kap><tld/>veziko</kap>
      <uzo tip="fak">ANA</uzo>
      <snc>
        <dif>
          Parto de la hepato, en kiu kolekti&gcirc;as la <tld/>o.
        </dif>
      </snc>
      <trd lng="be">&c_zh;&c_a;&c_w;&c_c;&c_ja;&c_k;</trd>
      <trd lng="de">Gallenblase</trd>
      <trd lng="es">ves&iacute;cula biliar</trd>
      <trd lng="fr">v&eacute;sicule biliaire</trd>
      <trd lng="hu">epeh&oacute;lyag</trd>
      <trd lng="nl">galblaas</trd>
      <trd lng="pt">ves&iacute;cula biliar</trd>
      <trd lng="ru">&c_zh;&c_jo;&c_l;&c_ch;&c_n;&c_y;&c_j; &c_p;&c_u;&c_z;&c_y;&c_r;&c_mol;</trd>
      <trd lng="sv">gallbl&aring;sa</trd>
    </drv>
 
    <drv mrk="gal.0aveziko">
      <kap><tld/>a veziko</kap>
      <uzo tip="fak">ANA</uzo>
      <ref tip="dif" cel="gal.0veziko"><tld/>veziko</ref>
    </drv>
 
  </art>
