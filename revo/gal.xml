<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gal.xml,v 1.36 2023/11/11 10:00:38 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>gal</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="gal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gal.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Maldol&ccirc;a, verde flava <ref tip="super" cel="sekrec.0ajxo.FIZL">sekrecia&jcirc;o</ref>
      de la hepato:
      <ekz>
        &scirc;i konvulsiis kelkan tempon, fine elvomis iom da <tld/>o
        <fnt>
          <aut>H. Laxness, trad. B. Ragnarsson</aut>,
          <vrk>Sendependaj homoj</vrk>,
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de drakoj estas ilia vino
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 32:33</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili donis al li trinki vinon kun <tld/>o enmiksita
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 27:34</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="gal.0o.PSI">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="koler.0o.PSI">Kolero</ref>
      <klr>(&ccirc;ar antikve <tld/>o estis rigardata tiu el la korpaj flua&jcirc;oj, kiu
      determinas koleran karakteron)</klr>:
      <ekz>
        <klr>[tiu]</klr> vidvino <klr>[&#x2026;]</klr> havis &ccirc;iam cignan kanton en la bu&scirc;o kaj
        strigan <tld/>on en la koro
        <fnt>
          <bib>Fab3</bib>
          <lok>Bona humoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        bolado de la <tld/>o estis a&ubreve;debla en lia trankvila, sed surda vo&ccirc;o
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XXVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; najtingalo ne estas sen <tld/>o
        <klr>(vortludo)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ne povos akcepti tion sen <tld/>eksplodo
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bo">&t_ma;&t_kha;&t_rata;&t_gigu;&t_sa;&t_tsheg;&t_sa;&t_nata;&t_naro;&t_da;&t_tsheg;</trd>
  <trdgrp lng="ja">
    <trd>&#x80C6;&#x6C41;  <pr>&#x305F;&#x3093;&#x3058;&#x3085;&#x3046;</pr></trd>,
    <trd>&#x82E6;&#x3005;&#x3057;&#x3044;&#x611F;&#x60C5;  <pr>&#x306B;&#x304C;&#x306B;&#x304C;&#x3057;&#x3044;&#x304B;&#x3093;&#x3058;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="lat">bilis</trd>
  <trd lng="be">&c_zh;&c_o;&c_w;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_zh;&c_l;&c_malmol;&c_ch;&c_je;&c_n; &c_s;&c_o;&c_k;</trd>
  <trd lng="cs">&zcaron;lu&ccaron; || n&ecaron;co velmi ho&rcaron;k&eacute;ho</trd>
  <trd lng="de">Galle</trd>
  <trd lng="el">&chi;&omikron;&lambda;&eta_ton;</trd>
  <trdgrp lng="en">
      <trd>bile</trd>,
      <trd>gall</trd>
    </trdgrp><trdgrp lng="en">
      <trd>gall</trd>,
      <trd>bile</trd>
    </trdgrp>
  <trd lng="es">bilis</trd>
  <trdgrp lng="fa">
      <trd>&a_S;&a_f;&a_r;&a_A;</trd>,
      <trd>&a_z;&a_r;&a_d;&a_A;&a_b;</trd>
    </trdgrp>
  <trdgrp lng="fr">
      <trd>bile</trd>,
      <trd>fiel<klr tip="ind"> (bile)</klr></trd>
    </trdgrp>
  <trd lng="hu">epe</trd>
  <trd lng="id">empedu</trd>
  <trdgrp lng="it">
      <trd>bile</trd>,
      <trd>fiele</trd>
    </trdgrp>
  <trd lng="la">bilis</trd>
  <trd lng="nl">gal</trd>
  <trd lng="pl">&zdot;&oacute;&lstroke;&cacute;</trd>
  <trd lng="pt">b&iacute;lis</trd>
  <trdgrp lng="ru">
      <trd>&c_zh;&c_je;&c_l;&c_ch;&c_mol;</trd>,
      <trd>&c_zh;&c_jo;&c_l;&c_ch;&c_mol;</trd>
    </trdgrp>
  <trd lng="sk">&zcaron;l&ccaron;</trd>
  <trd lng="sv">galla</trd>
  <trd lng="uk">&c_zh;&c_o;&c_v;&c_ch;</trd>
  <trd lng="zh">&#x82E6;&#x80C6; <pr>k&#x1D4;d&#x1CE;n</pr></trd>
</drv>

<drv mrk="gal.0veziko">
  <kap><tld/>veziko, <var><kap><tld/>a veziko</kap></var></kap>
  <snc mrk="gal.0veziko.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Parto de la <ref tip="malprt" cel="hepat.0o">hepato</ref>, en kiu kolekti&gcirc;as la <tld/>o:
      <ekz>
        sekigitaj <tld/>vezikoj de ursoj vendi&gcirc;as altapreze kiel tradicia medikamento
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="https://www.monato.be/2014/011198.php">Ursoj mortigataj</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        en popola kuracado la pomoj helpas kuraci malsanojn de la hepato kaj <tld/>veziko
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="https://www.monato.be/2014/011362.php">Simbolo de juneco</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_w;&c_c;&c_ja;&c_k;</trd>
  <trd lng="bg">&c_zh;&c_l;&c_malmol;&c_ch;&c_je;&c_n; &c_m;&c_je;&c_h;&c_u;&c_r;</trd>
  <trdgrp lng="cs">
  <trd>&zcaron;lu&ccaron;n&iacute;k</trd>,
  <trd>&zcaron;lu&ccaron;ov&yacute; m&ecaron;ch&yacute;&rcaron;</trd>
  </trdgrp>
  <trd lng="de">Gallenblase</trd>
  <trd lng="es">ves&iacute;cula biliar</trd>
  <trd lng="fa">&f_k;&f_y;&a_s;&a_h;&a_hamza_sure; &a_S;&a_f;&a_r;&a_A;</trd>
  <trd lng="fr">v&eacute;sicule biliaire</trd>
  <trd lng="hu">epeh&oacute;lyag</trd>
  <trd lng="id"><ind>kantung</ind> empedu</trd>
  <trdgrp lng="it">
      <trd>cistifellea</trd>,
      <trd>colecisti</trd>
    </trdgrp>
  <trd lng="ja">&#x80C6;&#x306E;&#x3046;  <pr>&#x305F;&#x3093;&#x306E;&#x3046;</pr></trd>
  <trd lng="nl">galblaas</trd>
  <trd lng="pl">woreczek &zdot;&oacute;&lstroke;ciowy</trd>
  <trd lng="pt">ves&iacute;cula biliar</trd>
  <trd lng="ru">&c_zh;&c_jo;&c_l;&c_ch;&c_n;&c_y;&c_j; &c_p;&c_u;&c_z;&c_y;&c_r;&c_mol;</trd>
  <trd lng="sk">&zcaron;l&ccaron;n&iacute;k</trd>
  <trd lng="sv">gallbl&aring;sa</trd>
  <trd lng="uk">&c_zh;&c_o;&c_v;&c_ch;&c_n;&c_i;&c_j; &c_m;&c_ib;&c_h;&c_u;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#x80C6;&#x56CA; <pr>d&#x1CE;nn&#xE1;ng</pr></trd>,
    <trd>&#x80C6; <pr>d&#x1CE;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="gal.0dukto">
  <kap><tld/>dukto</kap>
  <snc mrk="gal.0dukto.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Ajna el pluraj kanaloj, kondukantaj la
      <ref cel="gal.0o">galon</ref> en
      la <ref cel="duoden.0o">duodenon</ref>,
      kaj precipe la &ccirc;efkanalo en kiun ili
      arbece kuni&gcirc;as:
      <ekz>
        perturbi&gcirc;as la cirkulado de sango <klr>[&#x2026;]</klr> kio
        ka&ubreve;zas <klr>[&#x2026;]</klr> perturbi&gcirc;on de la funkcio de
        <tld/>dukto kaj digesto
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://esperanto.china.org.cn/EL/EL/ElPopolaCxinio/98-3-6.html">
          San-protektado en printempo</url></vrk>,
          <lok>El Popola &Ccirc;inio, 1998:3</lok></fnt>;
      </ekz>
      <ekz>
        la relative malgrandaj ovoj atingas la inteston la&ubreve;longe la
        eksterhepatan grandan <tld/>dukton (<nom>Ductus choledochus</nom>) kaj
        estas eligataj per la feka&jcirc;o
        <fnt>
          <aut>R. Sachs</aut>,
          <vrk><url ref="http://www.freeweb.hu/eventoj/steb/medicino/medicina-parazitologio/medicina-parazitologio.htm">
          Medicina Parazitologio</url>
          </vrk>,
          <lok>Scienca Revuo, 2001, printempo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_o;&c_w;&c_c;&c_je;&c_v;&c_a;&c_ja; <ind>&c_p;&c_r;&c_a;&c_t;&c_o;&c_k;&c_a;</ind></trd>
  <trd lng="bg">&c_zh;&c_l;&c_malmol;&c_ch;&c_je;&c_n; &c_k;&c_a;&c_n;&c_a;&c_l;</trd>
  <trd lng="de">Gallengang</trd>
  <trd lng="en">bile duct</trd>
  <trd lng="fa">&a_m;&a_j;&a_r;&a_A;&f_y; <ind>&a_S;&a_f;&a_r;&a_A;&a_w;&f_y;</ind></trd>
  <trdgrp lng="fr">
    <trd>chol&eacute;doque<klr tip="ind"> (subst.)</klr></trd>,
    <trd>canal <ind>chol&eacute;doque</ind></trd>
  </trdgrp>
  <trd lng="hu">k&ouml;z&ouml;s <ind>epevezet&eacute;k</ind></trd>
  <trdgrp lng="it">
    <trd>dotto<klr> biliare</klr></trd>,
    <trd>coledoco</trd>
  </trdgrp>
  <trd lng="nl">galbuis</trd>
  <trd lng="pl">przew&oacute;d &zdot;&oacute;&lstroke;ciowy</trd>
  <trd lng="pt">col&eacute;doco</trd>
  <trd lng="ru">&c_zh;&c_jo;&c_l;&c_ch;&c_n;&c_y;&c_j; &c_p;&c_r;&c_o;&c_t;&c_o;&c_k;</trd>
</drv>

