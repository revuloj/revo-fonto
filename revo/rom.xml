<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Rom</rad>/o
</kap>
<drv mrk="rom.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      La plej granda urbo kaj &ccirc;efurbo de Italujo; antikve,
      anka&ubreve; de <ref tip="malprt" cel="romi.0o">Romio</ref>.
    </dif>
  </snc>
  <trd lng="de">Rom</trd>
  <trd lng="en">Rome</trd>
  <trd lng="el">&Rho;&omega_ton;&my;&eta;</trd>
  <trd lng="hu">R&oacute;ma</trd>
  <trd lng="ru">&c_R;&c_i;&c_m;</trd>
</drv>

<drv mrk="rom.0a">
  <kap><tld lit="r"/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        la <tld lit="r"/>a senato, imperiestro
        <klr>(kontraste al la konstantinopolaj)</klr>;
      </ekz>
      <ekz>
        la <tld lit="r"/>a episkopo <klr>(la papo de <tld/>o)</klr>;
      </ekz>
      <ekz>
        la <tld/>a eklezio <klr>(kontraste al la Orienta
        kristanismo)</klr>;
      </ekz>
      <ekz>
        la <ind><tld lit="r"/>aj ciferoj</ind> I, V, X, L, C, M
        <klr>(la ciferoj kiujn uzis la antikvaj <tld lit="r"/>anoj,
        dum aliaj romianoj, ekz-e la grekoj kaj judoj,
        havis aliajn)</klr>.
        <trd lng="de">r&ouml;mische <ind>Zahlen</ind></trd>
        <trd lng="hu">r&oacute;mai <ind>sz&aacute;mok</ind></trd>
        <trd lng="ru">&c_r;&c_i;&c_m;&c_s;&c_k;&c_i;&c_je; &c_c;&c_i;&c_f;&c_r;&c_y;</trd>
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="roman.0o">roman&#183;o</ref>
  <trd lng="de">r&ouml;misch</trd>
  <trd lng="ru">&c_r;&c_i;&c_m;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="rom.0ano">
  <kap><tld lit="r"/>ano</kap>
  <snc>
    <dif>
      Lo&gcirc;anto de <tld/>o:
      <ekz>
        epistolo al la <tld lit="r"/>anoj <fnt><bib>NT</bib>,
        <lok>&Rom;</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="roman.0o">roman&#183;o</ref>
  <trd lng="de">R&ouml;mer</trd>
  <trd lng="hu">r&oacute;mai (lakos)</trd>
  <trd lng="ru">&c_r;&c_i;&c_m;&c_l;&c_ja;&c_n;&c_i;&c_n;</trd>
</drv>
</art>
