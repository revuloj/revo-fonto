  <snc mrk="stadi.0o.haltejo">
    <uzo tip="stl">ARK</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Loko, kie militista trupo a&ubreve; voja&gcirc;antoj haltas por pasigi
      nokton.
    </dif>
    <refgrp tip="vid">
      <ref cel="bivak.0o.halto">bivako</ref>,
      <ref cel="etap.0o">etapo</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Rastplatz</trd>,
      <trd>Etappenort</trd>,
      <trd>Etappenziel</trd>,
      <trd>Etappe</trd>
    </trdgrp>
    <trd lng="hu">pihen&odblac;&aacute;llom&aacute;s</trd>
