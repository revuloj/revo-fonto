<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>nuk</rad>/o
</kap>
<drv mrk="nuk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Posta parto de <ref tip="super" cel="kol.0o.korpo">kolo</ref>:
      <ekz>
        fortran&ccirc;i la kapon de birdo &ccirc;e la <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        klini la <tld/>on anta&ubreve; iu
        <klr>(obee submeti&gcirc;i al iu)</klr>.
      </ekz>
    </dif>
  </snc>                       
  <trd lng="be">&c_p;&c_a;&c_t;&c_y;&c_l;&c_ib;&c_c;&c_a;</trd>
  <trd lng="br">kilpenn</trd>
  <trdgrp lng="ca">
    <trd>nuca</trd>,
    <trd>clatell</trd>
  </trdgrp>
  <trd lng="de">Nacken</trd>
  <trdgrp lng="en">
    <trd>nape</trd>,
    <trd>scruff</trd>
  </trdgrp>
  <trd lng="es">nuca</trd>
  <trdgrp lng="fa">
    <trd>&f_p;&a_s; &f_g;&a_r;&a_d;&a_n;</trd>,
    <trd>&f_p;&a_sh;&a_t; &f_g;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">nuque</trd>
  <trdgrp lng="hu">
    <trd>nyakszirt</trd>,
    <trd>tark&oacute;</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>kuduk</trd>,
    <trd>tengkuk</trd>
  </trdgrp>
  <trd lng="it">nuca</trd>
  <trd lng="nl">nek</trd>
  <trd lng="pl">kark</trd>
  <trd lng="pt">nuca</trd>
  <trd lng="ru">&c_z;&c_a;&c_t;&c_y;&c_l;&c_o;&c_k;</trd>
  <trd lng="sv">nacke</trd>
</drv>

<drv mrk="nuk.rigid0a">
  <kap>malmol<tld/>a<fnt>Z</fnt>,
  <var><kap>rigid<tld/>a</kap></var></kap>
  <snc>
    <dif>
      Fiere <ref tip="super" cel="obstin.0a">obstina</ref>, kvaza&ubreve; ne
      klinanta obee la kolon:
      <ekz>
        sed ili kaj niaj patroj fari&gcirc;is fieraj kaj malmol<tld/>aj kaj
        ne
        obeis Viajn ordonojn
        <fnt><bib>MT</bib> <lok>&Neh; 9:16</lok></fnt>.
      </ekz>
    </dif>
  </snc>                     
  <trd lng="be">&c_u;&c_p;&c_a;&c_r;&c_t;&c_y;</trd>
  <trd lng="br">dibleg</trd>
  <trdgrp lng="ca">
    <trd>obstinat</trd>,
    <trd>recalcitrant</trd>,
    <trd>cabut</trd>
  </trdgrp>
  <trd lng="de">hartn&auml;ckig</trd>
  <trdgrp lng="en">
    <trd>obstinate</trd>,
    <trd>intransigent</trd>,
    <trd>stubborn</trd>,
    <trd>mulish</trd>,
    <trd>stiff-necked</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_s;&a_r;&a_s;&a_kh;&a_t;</trd>,
    <trd>&f_y;&f_k;&a_d;&a_n;&a_d;&a_h;</trd>,
    <trd>&a_l;&a_j;&a_w;&a_j;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>intransigeant</trd>,
    <trd>obstin&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hajthatatlan</trd>,
    <trd>kem&eacute;ny</trd>,
    <trd>nyakas</trd>
  </trdgrp>
  <trd lng="id"><ind>keras</ind> kepala</trd>
  <trd lng="it">ostinato<klr> (fiero)</klr></trd>
  <trd lng="nl">hardnekkig</trd>
  <trdgrp lng="pt">
    <trd>teimoso</trd>,
    <trd>obstinado</trd>,
    <trd>cabe&ccedil;a-dura</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_p;&c_r;&c_ja;&c_m;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_r;&c_je;&c_k;&c_l;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_g;&c_i;&c_b;&c_a;&c_je;&c_m;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_r;&c_i;&c_m;&c_i;&c_r;&c_i;&c_m;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">styvnackad</trd>
</drv>
</art>
