<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nuk.xml,v 1.34 2022/03/24 10:41:40 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>nuk</rad>/o
</kap>
<drv mrk="nuk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc mrk="nuk.0o.kolo">
    <dif>
      Malanta&ubreve;a parto de
      <ref tip="malprt" cel="kol.0o.korpo">kolo</ref>:
      <ekz>
        fortran&ccirc;i la kapon de birdo &ccirc;e la <tld/>o<fnt>Z</fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        klini la <tld/>on anta&ubreve; iu
        <klr>(obee submeti&gcirc;i al iu)</klr>.
      </ekz>
    </dif>
    <trd lng="ru">&c_sh;&c_je;&c_ja;</trd>
  </snc>
  <snc mrk="nuk.0o.okcipito">
    <dif>
      Malanta&ubreve;a parto de la kapo,
      <ref tip="sin" cel="okcipi.0o">okcipito</ref>:
      <ekz>
        Antono dubeme gratis sian <tld/>&c_o;n
        <fnt><aut>Hans Weinhengst</aut>:
          <vrk>Tur-Strato 4</vrk>
          <lok>Literatura Mondo, Budapest, 1934</lok></fnt>;
      </ekz><ekz>
        la princo similis al li nur per sia <tld/>o
        <fnt><bib>Fab2</bib>, <lok>Ne&gcirc;a re&gcirc;ino</lok></fnt>;
      </ekz><ekz>
        <uzo tip="stl">FRAZ</uzo>
        Sur lia <tld/>o sidas pli ol peruko
        <fnt><bib>PrV</bib></fnt>
        <klr>(li estas sa&gcirc;a kaj sperta)</klr>.
      </ekz>
    </dif>
    <trd lng="ru">&c_z;&c_a;&c_t;&c_y;&c_l;&c_o;&c_k;</trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_t;&c_y;&c_l;&c_ib;&c_c;&c_a;</trd>
  <trd lng="br">kilpenn</trd>
  <trdgrp lng="ca">
    <trd>nuca</trd>,
    <trd>clatell</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>t&yacute;l</trd>,
    <trd>vaz</trd>,
    <trd>&scaron;&iacute;je</trd>
  </trdgrp>
  <trd lng="de">Nacken</trd>
  <trdgrp lng="en">
    <trd>nape</trd>,
    <trd>scruff</trd>
  </trdgrp>
  <trd lng="es">nuca</trd>
  <trdgrp lng="fa">
    <trd>&f_p;&a_s; &f_g;&a_r;&a_d;&a_n;</trd>,
    <trd>&f_p;&a_sh;&a_t; &f_g;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">nuque</trd>
  <trdgrp lng="hu">
    <trd>nyakszirt</trd>,
    <trd>tark&oacute;</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>kuduk</trd>,
    <trd>tengkuk</trd>
  </trdgrp>
  <trd lng="it">nuca</trd>
  <trd lng="ne">&#x918;&#x93E;&#x901;&#x91F;&#x940;</trd>
  <trd lng="nl">nek</trd>
  <trd lng="pl">kark</trd>
  <trd lng="pt">nuca</trd>
  <trdgrp lng="sk">
    <trd>tylo</trd>,
    <trd>v&auml;z</trd>,
    <trd>z&aacute;tylok</trd>,
    <trd>&scaron;ija</trd>
  </trdgrp>
  <trd lng="sv">nacke</trd>
  <trd lng="uk">&#x43F;&#x43E;&#x442;&#x438;&#x43B;&#x438;&#x446;&#x44F;</trd>
</drv>

<drv mrk="nuk.rigid0a">
  <kap>malmol<tld/>a<fnt>Z</fnt>,
  <var><kap>rigid<tld/>a</kap></var></kap>
  <snc>
    <dif>
      Fiere <ref tip="super" cel="obstin.0a">obstina</ref>, kvaza&ubreve; ne
      klinanta obee la kolon:
      <ekz>
        sed ili kaj niaj patroj fari&gcirc;is fieraj kaj malmol<tld/>aj kaj
        ne obeis Viajn ordonojn
        <fnt><bib>MT</bib> <lok>&Neh; 9:16</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_p;&c_a;&c_r;&c_t;&c_y;</trd>
  <trd lng="br">dibleg</trd>
  <trdgrp lng="ca">
    <trd>obstinat</trd>,
    <trd>recalcitrant</trd>,
    <trd>cabut</trd>
  </trdgrp>
  <trd lng="cs">zatvrzel&yacute;</trd>
  <trd lng="de">hartn&auml;ckig</trd>
  <trdgrp lng="en">
    <trd>obstinate</trd>,
    <trd>intransigent</trd>,
    <trd>stubborn</trd>,
    <trd>mulish</trd>,
    <trd>stiff-necked</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>testarudo</trd>,
    <trd>terco</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_s;&a_r;&a_s;&a_kh;&a_t;</trd>,
    <trd>&f_y;&f_k;&a_d;&a_n;&a_d;&a_h;</trd>,
    <trd>&a_l;&a_j;&a_w;&a_j;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>intransigeant</trd>,
    <trd>obstin&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hajthatatlan</trd>,
    <trd>kem&eacute;ny</trd>,
    <trd>nyakas</trd>
  </trdgrp>
  <trd lng="id"><ind>keras</ind> kepala</trd>
  <trd lng="it">ostinato<klr> (fiero)</klr></trd>
  <trd lng="nl">hardnekkig</trd>
  <trdgrp lng="pt">
    <trd>teimoso</trd>,
    <trd>obstinado</trd>,
    <trd>cabe&ccedil;a-dura</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_je;&c_s;&c_t;&c_o;&c_k;&c_o;&c_v;&c_y;&c_j;&c_n;&c_y;&c_j;<klr> (&c_b;&c_i;&c_b;&c_l;.)</klr></trd>,
    <trd>&c_u;&c_p;&c_r;&c_ja;&c_m;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_r;&c_je;&c_k;&c_l;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_g;&c_i;&c_b;&c_a;&c_je;&c_m;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_r;&c_i;&c_m;&c_i;&c_r;&c_i;&c_m;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>tvrdohlavec</trd>,
    <trd>zatvrdilec</trd>
  </trdgrp>
  <trd lng="sv">styvnackad</trd>
  <trdgrp lng="uk">
    <trd>&#x43D;&#x43E;&#x440;&#x43E;&#x432;&#x438;&#x441;&#x442;&#x438;&#x439;</trd>,
    <trd>&#x43D;&#x435;&#x43F;&#x43E;&#x43A;&#x456;&#x440;&#x43B;&#x438;&#x432;&#x438;&#x439;</trd>,
    <trd>&#x43D;&#x435;&#x43F;&#x43E;&#x43A;&#x456;&#x440;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>
</art>
