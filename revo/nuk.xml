<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>nuk</rad>/o
</kap>
<drv mrk="nuk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Posta parto de kolo:
      <ekz>
        fortran&ccirc;i la kapon de birdo &ccirc;e la <tld/>o<fnt>Z</fnt>;
      </ekz>
      (f)
      <ekz>
        klini la <tld/>on (obee submeti&gcirc;i) anta&ubreve; iu.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Nacken</trd>
  <trd lng="fr">nuque</trd>
  <trd lng="nl">nek</trd>
  <trd lng="ru">&c_z;&c_a;&c_t;&c_y;&c_l;&c_o;&c_k;</trd>
</drv>

<drv mrk="nuk.rigid0a">
  <kap>malmol<tld/>a<fnt>Z</fnt>, rigid<tld/>a</kap>
  <snc>
    <dif>
      Fiere obstina.
    </dif>
  </snc>
  <trd lng="de">hartn&auml;ckig</trd>
  <trdgrp lng="fr">
    <trd>instransigeant</trd>,
    <trd>obstin&eacute;</trd>
  </trdgrp>
  <trd lng="nl">hardnekkig</trd>
</drv>
</art>
