<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: euxfoni.xml,v 1.10 2023/07/14 09:28:48 revo Exp $">
<kap>
  <rad>e&ubreve;foni</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="euxfoni.0o">
  <kap><tld/>o</kap>
  <snc mrk="euxfoni.0o.SON">
    <dif>
     <ref tip="dif" cel="son.bon0eco">Bonsoneco</ref>:
      <ekz>
        en la vivo de lingvo interpu&scirc;i&gcirc;as diversaj principoj:
        analogio, <tld/>o, logiko
        <fnt><bib>Lanti</bib>, <lok>pri stilo</lok></fnt>;
      </ekz>
      <ekz>
        priju&gcirc;i la rilaton inter senco, enhavo, formo kaj <tld/>o de la lingvo &ccirc;e&hcirc;a
        <fnt>
          <vrk><url ref="http://esperantobrno.cz/malnova/1989-05.pdf">Bulteno de Esperantista Klubo
          en Brno</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        montri&gcirc;is ke la deviga akuzativo multe helpas al la <tld/>o de esperanto
        <fnt>
          <vrk><url ref="http://esperantio.net/index.php?id=3496">Neves pri la Ido ...</url></vrk>
          <lok>Esperanta Civito, HeKo 676 8-B, 14 apr 18</lok>
        </fnt>;
      </ekz>
      <ekz>
        el sonora son-girland' kia <tld/>o vasta ver&scirc;as sin kun varma skand'
        <fnt>
          <vrk><url ref="&DonH;Poezio/sonoriloj.html">La sonoriloj (Edgar Allan Poe)</url></vrk>
          <lok>literaturo.org</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
    <rim>
      Ekzistas anka&ubreve; muzikinstrumento <tld/>o:
      <ekz>
        sendube la <tld/>o (tenora <ref cel="tubj.0o">tubjo</ref>) estas mia plej &scirc;atata instrumento
        <fnt>
          <aut>David Gaines</aut>
          <lok><url ref="http://www.davidgainesmusic.com/literatura-foiro-aug-2000-eo/">Literatura
          Foiro, A&ubreve;gusto 2001</url></lok>
        </fnt>.
      </ekz>
    </rim>
    <trdgrp lng="be">
      <trd>&c_e;&c_w;&c_f;&c_a;&c_n;&c_ib;&c_ja;</trd>,
      <trd>&c_m;&c_ib;&c_l;&c_a;&c_g;&c_u;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    </trdgrp>
  <trd lng="de">Euphonie</trd>
  <trd lng="en">euphony</trd>
  <trdgrp lng="nl">
    <trd>welluidendheid</trd>,
    <trd>eufonie</trd>
  </trdgrp>
  <trd lng="pl">eufonia</trd>
  <trd lng="pt">eufonia</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_z;&c_v;&c_u;&c_ch;&c_i;&c_je;</trd>,
    <trd>&c_e;&c_v;&c_f;&c_o;&c_n;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_je;&c_v;&c_f;&#x43E;&#x43D;&c_ib;&#x44F;</trd>,
    <trd>&#x43C;&c_i;&#x43B;&#x43E;&c_z;&c_v;&c_u;&c_ch;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>
  </trdgrp>
</drv>
</art>

