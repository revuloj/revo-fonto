<art mrk="$Id: penc.xml,v 1.25 2024/02/17 06:43:05 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>penc</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="penc.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="unu.monasub0o" lst="voko:monunuoj">Mona
      subunuo</ref>, egala al
      centono de <ref tip="malprt" cel="pund.0o">pundo</ref>;
      tiomvalora <ref tip="super" cel="mon.0ero">monero</ref>:
      <ekz>
        por la voja&gcirc;o li postulis kvar <tld/>ojn<fnt><aut>W. H.
        Simcock</aut>: <vrk>Vizito al la nordaj insuloj</vrk>,
        <bib>Monato</bib><lok>jaro 1999a, numero 9a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="peni.0o.EKON">penio</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_e;&c_n;&c_s;</trd>,
    <trd>&c_p;&c_e;&c_n;&c_ib;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_je;&c_n;&c_i;</trd>
  <trd lng="ca">penic</trd>
  <trdgrp lng="cs">
    <trd>pence</trd>,
    <trd>penny</trd>
  </trdgrp>
  <trd lng="de">Penny</trd>
  <trd lng="en">penny</trd>
  <trd lng="es">penique</trd>
  <trd lng="fr">penny</trd>
  <trd lng="hu">penny</trd>
  <trd lng="id">penny</trd>
  <trdgrp lng="ja">
    <trd>&#x30DA;&#x30CB;&#x30FC;</trd>,
    <trd>&#x30DA;&#x30F3;&#x30B9;</trd>
  </trdgrp>
  <trd lng="nl">penny</trd>
  <trd lng="pl">pens</trd>
  <trd lng="pt">p&ecirc;ni</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_n;&c_s;</trd>,
    <trd>&c_p;&c_je;&c_n;&c_n;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>penca</trd>,
    <trd>penny</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_je;&#x43D;&c_s;</trd>,
    <trd>&#x43F;&c_je;&#x43D;&#x43D;&c_ib; (&c_g;&c_r;&#x43E;&c_sh;&#x43E;&c_v;&c_a; &#x43E;&c_d;&c_i;&#x43D;&c_i;&c_c;&#x44F;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4FBF;&#x58EB; <pr>bi&#xE0;nsh&#xEC;</pr></trd>,
    <trd>&#x672C;&#x58EB; <pr>b&#x11B;nsh&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>
