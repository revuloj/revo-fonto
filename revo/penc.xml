<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>penc</rad>/o
</kap>
<drv mrk="penc.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="unu.monasub0o">Mona subunuo</ref>, egala al
      centono de <ref tip="malprt" cel="pund.0o">pundo</ref>;
      tiomvalora <ref tip="super" cel="mon.0ero">monero</ref>:
      <ekz>
        por la voja&gcirc;o li postulis kvar <tld/>ojn<fnt><aut>W. H.
        Simcock</aut>: <vrk>Vizito al la nordaj insuloj</vrk>,
        <bib>Monato</bib><lok>jaro 1999a, numero 9a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_e;&c_n;&c_s;</trd>,
    <trd>&c_p;&c_e;&c_n;&c_ib;</trd>
  </trdgrp>
  <trd lng="de">Penny</trd>
  <trd lng="en">penny</trd>
  <trd lng="fr">penny</trd>
  <trd lng="hu">penny</trd>
  <trd lng="nl">penny</trd>
  <trd lng="pt">p&ecirc;ni</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_n;&c_s;</trd>,
    <trd>&c_p;&c_je;&c_n;&c_n;&c_i;</trd>
  </trdgrp>
</drv>
</art>
