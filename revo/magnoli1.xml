<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: magnoli1.xml,v 1.11 2020/05/12 12:14:48 revo Exp $">
<kap>
  <rad>magnoliac</rad>/oj
</kap>
<drv mrk="magnoli1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref>
      el <ref tip="malprt" cel="kotile.du0aj">dukotiledonaj
      plantoj</ref> (<trd lng="la">Magnoliaceae</trd>):
      <ekz>
        angelikata acido trov<klr>[i&gcirc;]</klr>as en multaj plantoj de la
        familio apiacoj
        a&ubreve; umbeliferacoj kaj <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=583626"
              >Angelikata acido</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        estragolo estas volatila substanco vaste trovata en plantoj el
        la familio
        lamiacoj, apiacoj, <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=580977"
              >Estragolo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
       <ref cel="ilici.0o">ilicio</ref>,
       <ref cel="magnoli1.0oj">magnoliacoj</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_g;&c_n;&c_o;&c_l;&c_ib;&c_je;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">magnoli&agrave;cies</trd>
  <trd lng="de">Magnoliengew&auml;chse</trd>
  <trd lng="fr">Magnoliac&eacute;es</trd>
  <trdgrp lng="hu">
    <trd>liliomfaf&eacute;l&eacute;k</trd>,
    <trd>magn&oacute;liaf&eacute;l&eacute;k</trd></trdgrp>
  <trd lng="nl">tulpeboomfamilie</trd>
  <trd lng="pl">magnoliowate</trd>
  <trd lng="pt">magnoli&aacute;ceas</trd>
  <trd lng="ro">magnoliaceae</trd>
  <trd lng="ru">&c_m;&c_a;&c_g;&c_n;&c_o;&c_l;&c_i;&c_je;&c_v;&c_y;&c_je;
<klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
