<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kard.xml,v 1.27 2024/01/11 19:20:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kard</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="kard.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kard.0o.KOMUNE">
    <dif>
      Iu el diversaj herboj kun pikilhavaj folioj kaj kapituloj, aparte
      <refgrp tip="sub">
        <ref cel="kardu.0o">karduo</ref>,
        <ref cel="artisx.0o">arti&scirc;oko</ref>,
        <ref cel="centau1.0o">centa&ubreve;reo</ref>,
        <ref cel="cirsi.0o">cirsio</ref>,
        <ref cel="dipsak.0o">dipsako</ref>,
        <ref cel="karlin.0o">karlino</ref>
      </refgrp>:
      <ekz>
        ar&gcirc;ente blankaj <tld/>oj
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        benedikt-<tld/>o <klr>(Centaurea benedicta)</klr>
        <fnt>
          <bib>SrT</bib>
          <lok>Libro IX.</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi dra&scirc;os viajn korpojn per dornoj de dezerto kaj per <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 8:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni grimpis sur la klifojn kaj pluiris la&ubreve; pado plena je dornoj
        kaj <tld/>oj
        <fnt>
          <aut>Lukiano, trad. G. Berveling</aut>,
          <vrk><url ref="http://www.esperanto.gr/lit/els/rakontoj.pdf">veraj
          rakontoj</url>. - 2005</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="kard.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="genr.0o.BIO">Genro</ref> el familio
      <ref tip="malprt" cel="astera.0oj">asteracoj</ref>
      (<trd lng="la">Carduus</trd>):
    </dif>
    <ref tip="sin" cel="kardu.0o">karduo</ref>
    <trd lng="en">thistle</trd>
    <trd lng="fr">chardon</trd>
    <trd lng="nl">distel</trd>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_r;&c_t;&c_a;&c_p;&c_a;&c_l;&c_o;&c_h;</trd>
  <trd lng="br">askolenn</trd>
  <trd lng="ca">card</trd>
  <trd lng="cs">bodl&aacute;k</trd>
  <trd lng="de">Distel</trd>
  <trd lng="hu">bog&aacute;ncs</trd>
  <trd lng="ja">&#12450;&#12470;&#12511;</trd>
  <trd lng="pl">oset</trd>
  <trd lng="ro">carduus</trd>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_t;&c_o;&c_p;&c_o;&c_l;&c_o;&c_h;</trd>
  <trd lng="sk">bodliak</trd>
  <trd lng="uk">&c_b;&c_u;&c_d;&c_ja;&c_k;</trd>
  <trd lng="zh">&#34015; <pr>j&igrave;</pr></trd>
</drv>
</art>
