<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>kard</rad>/o
</kap>
<drv mrk="kard.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="aster.0acoj">asteracoj</ref>
      (<trd lng="la">Carduus</trd>).
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Similaj herboj kun pikilhavaj folioj kaj kapituloj:
      <refgrp tip="vid">
        <ref cel="dipsak.0o">dipsako</ref>,
        <ref cel="artisx.0o">arti&scirc;oko</ref>,
        <ref cel="karlin.0o">karlino</ref>
      </refgrp>
    </dif>
  </snc>
  <trd lng="de">Distel</trd>
</drv>
</art>
