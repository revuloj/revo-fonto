<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fauxk.xml,v 1.26 2025/10/20 22:31:06 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>fa&ubreve;k</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="fauxk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fauxk.0o.ZOO">
    <dif>
      Bu&scirc;kavo de sova&gcirc;aj rabaj bestoj:
      <ekz>
        <tld/>o de tigro;
      </ekz>
      <ekz>
        neniu povis de&scirc;iri la florojn, &ccirc;ar iliaj trunketoj estis abomenindaj,
        venenaj serpentoj, kaj la floroj mem estis fajro, kiu flamis el iliaj <tld/>oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam ili kriis kaj malfermis la bekojn, sopirante man&gcirc;a&jcirc;on, mi blovis al ili en la <tld/>on
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la nigra serpentkapo, kiu en sia <tld/>o tenis la fina&jcirc;on de la blanka serpento
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Akvo de l' Vivo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_sh;&c_ch;&c_a;</trd>,
      <trd>&c_z;&c_ja;&c_p;&c_a;</trd>
    </trdgrp>
    <trd lng="de">Rachen <klr tip="ind">(Raubtier)</klr></trd>
    <trd lng="es">fauces</trd>
    <trd lng="nl">muil</trd>
    <trdgrp lng="pl">
      <trd>paszcza</trd>,
      <trd>paszcz&eogonek;ka</trd>,
      <trd>czelu&sacute;&cacute;</trd>,
      <trd>wylot <ind>lufy</ind></trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_mol; <klr>(&c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_o;&c_g;&c_o;)</klr></trd>
    <trdgrp lng="zh">
    <trd>&#21693;&#22836; <pr>y&amacron;nt&oacute;u</pr></trd>,
    <trd>&#28145;&#28149; <pr>sh&emacron;nyu&amacron;n</pr></trd>,
    <trd>&#21693;&#21897; <pr>y&amacron;nh&oacute;u</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="fauxk.0o.malfermajxo">
    <dif>
      <ref tip="vid" cel="ferm.mal0ajxo.KOMUNE">Malferma&jcirc;o</ref>:
    </dif>
    <subsnc mrk="fauxk.0o.objekta">
      <dif>
        Aperturo de vazo:
        <ekz>
          <tld/>o de glaso, de botelo.
        </ekz>
      </dif>
      <trd lng="be">&c_a;&c_d;&c_t;&c_u;&c_l;&c_ib;&c_n;&c_a;</trd>
      <trd lng="de">&Ouml;ffnung</trd>
      <trd lng="nl">opening</trd>
      <trd lng="ru">&c_o;&c_t;&c_v;&c_je;&c_r;&c_s;&c_t;&c_i;&c_je;</trd>
    </subsnc>
    <subsnc mrk="fauxk.0o.granda">
      <uzo tip="stl">FIG</uzo>
      <dif>
        Lar&gcirc;a malluma a&ubreve; timiga aperturo:
        <ekz>
          <tld/>o de kaverno, minejo, tunelo, fornego, kanono;
        </ekz>
        <ekz>
          lar&gcirc;e malfermis sian <tld/>on la intermonto
          <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 4a</lok></fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="fend.0ego">fendego</ref>.
      </refgrp>
      <trdgrp lng="be">
        <trd>&c_zh;&c_a;&c_r;&c_a;&c_l;&c_o;</trd>,
        <trd>&c_z;&c_ja;&c_p;&c_a;</trd>
      </trdgrp>
      <trd lng="de">&Ouml;ffnung</trd>
      <trd lng="nl">opening</trd>
      <trdgrp lng="ru">
        <trd>&c_z;&c_je;&c_v;</trd>,
        <trd>&c_zh;&c_je;&c_r;&c_l;&c_o;</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="fauxk.0o.grandega">
      <uzo tip="stl">FIG</uzo>
      <dif>
        Abismo, senfunda&jcirc;o:
        <ekz>
          <tld/>o de la morto;
        </ekz>
        <ekz>
          <tld/>o de la ondegoj;
        </ekz>
        <ekz>
          &Scirc;eol lar&gcirc;igis sian internon kaj eksterordinare malfermis sian <tld/>on
          <fnt>
            <bib>MT</bib>
            <lok>Jesaja 5:14</lok>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="be">
        <trd>&c_b;&c_je;&c_z;&c_d;&c_a;&c_n;&c_mol;</trd>,
        <trd>&c_p;&c_r;&c_o;&c_r;&c_v;&c_a;</trd>
      </trdgrp>
      <trd lng="de">Abgrund</trd>
      <trd lng="nl">afgrond</trd>
      <trdgrp lng="ru">
        <trd>&c_z;&c_i;&c_ja;&c_ju;&c_shch;&c_je;&c_je; &c_o;&c_t;&c_v;&c_je;&c_r;&c_s;&c_t;&c_i;&c_je;</trd>
      </trdgrp>
    </subsnc>
  </snc>
  <trdgrp lng="en">
    <trd>maw</trd>,
    <trd>jaws</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gueule<klr tip="ind"> (d'un animal, d'une gouffre)</klr></trd>,
    <trd>goulot</trd>,
    <trd>ouverture<klr tip="ind"> (b&eacute;ante)</klr></trd></trdgrp>
  <trdgrp lng="hu">
    <trd>torok<klr tip="ind"> (&aacute;llat&eacute;, t&aacute;rgy&eacute;)</klr></trd>,
    <trd>garat<klr tip="ind"> (&aacute;llat&eacute;, t&aacute;rgy&eacute;)</klr></trd>,
    <trd>ny&iacute;l&aacute;s<klr tip="ind"> (b&eacute;ante)</klr></trd></trdgrp>
  <trdgrp lng="pt">
    <trd>goela</trd>,
    <trd>fauce</trd>,
    <trd>abertura</trd>,
    <trd>boca <klr tip="amb">(de t&uacute;nel, forno etc.)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_a;&c_shch;&c_a;</trd>,
    <trd>&c_o;&c_t;&c_v;&c_ib;&c_r; (&c_p;&c_l;&c_ja;&c_sh;&c_k;&c_i;)</trd>,
    <trd>&c_d;&c_u;&c_l;&c_o;</trd>,
    <trd>&c_zh;&c_je;&c_r;&c_l;&c_o;</trd>,
    <trd>&c_ch;&c_je;&c_l;&c_ju;&c_s;&c_t;&c_ib; (&c_p;&c_je;&c_ch;&c_ib;)</trd>,
    <trd>&c_v;&c_h;&c_ib;&c_d; (&c_u; &c_p;&c_je;&c_ch;&c_je;&c_r;&c_u;</trd>,
    <trd>&c_t;&c_u;&c_n;&c_je;&c_l;&c_mol; &c_ib; &c_t;. &c_p;.)</trd>
  </trdgrp>
</drv>

<drv mrk="fauxk.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti lar&gcirc;e malfermita:
      <ekz>
        profunda vundo ek<tld/>is, kaj sango elfluis
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Batalo je la Ebura Turo</lok>
        </fnt>;
      </ekz>
      <ekz>
        transverse <klr>[...]</klr> <tld/>is en la tero fendo, kiu &scirc;ajnis lar&gcirc;a je duono de mejlo
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Igramul', la Multulo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      Ne konfuzu kun:
      <ref cel="osced.0i">oscedi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_ja;&c_w;&c_l;&c_ja;&c_c;&c_mol; &c_p;&c_a;&c_sh;&c_ch;&c_u;</trd>,
    <trd>&c_z;&c_je;&c_w;&c_r;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">klaffen</trd>
  <trd lng="en">gape</trd>
  <trd lng="es">estar boquiabierto</trd>
  <trd lng="nl">wijd <ind>openstaan</ind></trd>
  <trdgrp lng="fr">
    <trd>b&acirc;iller<klr tip="ind"> (en p. d'une ouverture)</klr></trd>,
    <trd>b&eacute;er<klr tip="ind"> (en p. d'une
    ouverture)</klr></trd></trdgrp>
  <trd lng="hu">t&aacute;tong</trd>
  <trdgrp lng="pl">
    <trd>rozdziawi&cacute;  <ind>paszcz&eogonek;</ind></trd>,
    <trd>rozwiera&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_je;&c_v;&c_a;&c_t;&c_mol; &c_p;&c_a;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_i;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_z;&c_z;&c_ja;&c_v;&c_l;&c_ja;&c_t;&c_i; &c_p;&c_a;&c_shch;&c_u;</trd>,
    <trd>&c_z;&c_ja;&c_ja;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#35010;&#25104;&#32299; <pr>li&egrave;ch&eacute;ngf&eacute;ng</pr></trd>,
    <trd>&#24352;&#24320; <pr>zh&amacron;ngk&amacron;i</pr></trd>,
    <trd>&#21620; <pr>x&umacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fauxk.al0i">
  <kap>al<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">FIG</uzo>
  <snc>
    <dif>
      Krii al iu per la <tld/>o lar&gcirc;e malfermita:
      <ekz>
        kompreneble la komand&#173;anto al<tld/>is min, ke mi devintus
        revenigi Kurt vivantan
        <fnt>
          <aut>T. Steele</aut>,
          <vrk>Kvaza&ubreve; &ccirc;io dependus de mi</vrk>,
          <lok>p. 332</lok>
        </fnt>;
      </ekz>
      <ekz>
        vendistinoj <klr>[...]</klr> kun fortaj pulmoj kaj Di-donita rajto al<tld/>i
        <fnt>
          <aut>Trevor Steele</aut>
          <vrk><url ref="&LOdE;R-steele.htm">Falantaj muroj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_r;&c_l;&c_a;&c_n;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_r;&c_y;&c_ch;&c_a;&c_c;&c_mol; &c_n;&c_a; &c_w;&c_s;&c_jo; &c_g;&c_o;&c_r;&c_l;&c_a;</trd>
  </trdgrp>
  <trd lng="de">anfauchen</trd>
  <trd lng="en">yell</trd>
  <trd lng="fr">gueuler</trd>
  <trdgrp lng="nl">
    <trd>aanblaffen</trd>,
    <trd>toesnauwen</trd>,
    <trd>wijd <ind>openstaan</ind></trd>
  </trdgrp>
  <trd lng="pl">obszczekiwa&cacute;</trd>
</drv>

<drv mrk="fauxk.fermila0on">
  <kap>fermi la <tld/>on</kap>
  <uzo tip="stl">FRAZ</uzo>
  <uzo tip="stl">VULG</uzo>
  <snc mrk="fauxk.fermila0on.KOMUNE">
    <dif>
      <ref tip="dif" cel="silent.0i.malparoli">Silenti<sncref/></ref>:
      <ekz>
        se ili bonvolos fermi la <tld/>ojn kaj doni al li okazon demandi
        al la intendanto<klr>...</klr>
        <fnt><aut>J. Francis</aut>, <vrk>Misio sen Alveno</vrk>,
        <lok>tk-stafeto, 1982</lok></fnt>;
      </ekz>
      <ekz>
        kial prezidento Chavez ne fermis sian grandan <tld/>on?
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.delbarrio.eu/2007/11/kial-ci-ne-silentas.htm">
          Kial ci ne silentas?</url></vrk>,
          <lok>2007-11-12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_t;&c_k;&c_n;&c_u;&c_c;&c_mol; &c_z;&c_ja;&c_p;&c_u;</trd>
  <trd lng="de">das <ind>Maul</ind> zumachen</trd>
  <trdgrp lng="en">
    <trd>shut one's <ind>mouth</ind></trd>,
    <trd>shut one's cakehole</trd>,
    <trd>shut one's piehole</trd>,
    <trd>shut one's gob <klr>(UK)</klr></trd>
  </trdgrp>
  <trd lng="fr">fermer sa gueule</trd>
  <trd lng="pl">zamkn&aogonek;&cacute;  <ind>paszcz&eogonek;</ind></trd>
</drv>
</art>
