<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: socionik.xml,v 1.5 2023/12/02 21:23:08 revo Exp $">
<kap>
  <rad>socionik</rad>/o
</kap>
<drv mrk="socionik.0o">
  <kap><tld/>o</kap>
  <snc mrk="socionik.0o.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      <ref tip="super" cel="karakterologi.0o.PSI">Karakterologio</ref>,
      kiu klasigas homtipojn la&ubreve; kvar dimensioj kaj analizas
      interhomajn rilatojn la&ubreve; la funkciado de tiuj tipoj:
      <ekz>
        Au&scaron;ra Augustinavi&ccaron;i&#5691;t&#279;, pse&ubreve;donimo: Augusta, <klr>...</klr>
        estis fondinto de la psikologia tipologia teorio <tld lit="S"/>o
        <fnt>
          <bib>Viki</bib>,
          <lok><url
          ref="&Viki;/Au%C5%A1ra_Augustinavi%C4%8Di%C5%ABt%C4%97">
          Au&scaron;ra Augustinavi&ccaron;i&#5691;t&#279;</url>, 2010-07-19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_c;&c_y;&c_jo;&c_n;&c_ib;&c_k;&c_a;</trd>
  <trd lng="de">Sozionik</trd>
  <trd lng="en">socionics</trd>
  <trd lng="fr">socionique</trd>
  <trd lng="pl">socjonika</trd>
</drv>

<drv mrk="socionik.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="socionik.0isto.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      Psi&hcirc;ologo konanta kaj uzanta
      <ref tip="vid" cel="socionik.0o.PSI"><tld/>on</ref>:
      <ekz>
        multaj <tld/>istoj <klr>...</klr> laboras en infana
        kaj lerneja psikologio, profesia kaj familia konsultado
        <fnt>
          <aut>D. Lytov</aut>,
          <vrk><url ref="http://www.ilei.info/ipr/socioniko1.htm">
          Socioniko - Talentoj kaj Rilatoj</url></vrk>,
          <lok>[2010]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_c;&c_y;&c_jo;&c_n;&c_ib;&c_k;</trd>
  <trd lng="de">Sozioniker</trd>
  <trd lng="en">socionist</trd>
  <trd lng="fr">socioniste</trd>
  <trd lng="pl">socjonik</trd>
</drv>
</art>

