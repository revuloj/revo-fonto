<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>fad</rad>/o</kap>
<drv mrk="fad.0o">
  <kap><tld/>o</kap>
  <snc mrk="fad.0o.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Fenomeno konsistanta el sinsekvaj plimalfortigoj a&ubreve; plifortigoj
      de a&ubreve;deblo en radioriceviloj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_g;&c_a;&c_s;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_ib;&c_r;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_f;&c_e;&c_d;&c_y;&c_n;&c_g;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>f&agrave;ding</trd>,
    <trd>fosa</trd>,
    <trd>esva&iuml;ment</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Fading</trd>,
    <trd>Schwankung der Empfangslautst&auml;rke</trd>
  </trdgrp>
  <trd lng="en">fading</trd>
  <trd lng="es">fundido<klr tip="amb"> (imagen o sonido)</klr></trd>
  <trdgrp lng="fr">
    <trd>&eacute;vanouissement<klr tip="ind"> (fading)</klr></trd>,
    <trd>fading</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>elhalkul&aacute;s</trd>,
    <trd>fading</trd>
  </trdgrp>
  <trd lng="nl">fading</trd>
  <trdgrp lng="pt">
    <trd>f&aacute;dingue</trd>,
    <trd>fading</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_t;&c_u;&c_h;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_i;&c_r;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_f;&c_je;&c_d;&c_i;&c_n;&c_g;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>fading</trd>,
    <trd>únik el. magnetických vĺn</trd>
  </trdgrp>
</drv>

<drv mrk="fad.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="fad.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malforti&gcirc;i, <ref tip="vid" 
      cel="aper.mal0i.nevidebligxi">malaperi</ref> iom post iom:
      <ekz>
        montegoj <klr>...</klr> etendi&gcirc;as vico post vico kaj 
        malproksime <tld/>as en blua valo
        <fnt>
          <aut>Vladimir Glazunov</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Noveloj/Cxarmo.html">La
          &ccirc;armo de &Ccirc;ielmontaro</url></vrk>,
          <lok>Fonto, numero 37a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>fondre's</trd>,
    <trd>esvair-se</trd>
  </trdgrp>
  <trd lng="es">fundir<klr tip="amb"> (imagen o sonido)</klr></trd>
  <trdgrp lng="fr">
    <trd>disparaitre<klr> (en s'&eacute;loignant)</klr></trd>,
    <trd>dispara&icirc;tre<klr> (en s'&eacute;loignant)</klr></trd>,
    <trd>s'&eacute;vanouir<klr> (disparaitre)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>slabnúť <klr>(zvuk)</klr></trd>,
    <trd>unikať</trd>
  </trdgrp>
  <trd lng="tr">g&ouml;zden kaybolmak<klr>(uzakla&scedil;arak)</klr></trd>
</drv>
</art>
