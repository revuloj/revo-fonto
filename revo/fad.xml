<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fad.xml,v 1.20 2021/04/14 17:27:48 revo Exp $">
<kap>
  <rad>fad</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fad.0o">
  <kap><tld/>o</kap>
  <snc mrk="fad.0o.RAD">
    <uzo tip="fak">RAD</uzo>
    <dif>
      <ref tip="super" cel="fluktu.0o">Fluktuado</ref> de
      <ref cel="elektr11.0akampo">elektromagneta kampo</ref> 
      pro diversaj fizikaj efikoj kiel <ref cel="interf.0o">interferado</ref>, 
      ombrado, e&hcirc;oj, doplera efiko:
      <ekz>
        li atendas la horon de la elsendoj por fine nur kelkajn disajn frazojn a&ubreve;skulti,
        pro oftaj <tld/>oj kaj malhelpo de familianoj, televidantaj en la sama &ccirc;ambro
        <fnt>
          <bib>Monato</bib>
          <aut>Reinaldo Marcelo Ferreira</aut>
          <vrk><url ref="https://www.monato.be/2006/008937.php">Kronikoj de niaj frustri&gcirc;oj</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>      
    </dif>
  </snc>
  <ref tip="hom" cel="fad1.0o">fado (muzikstilo)</ref>
  <trdgrp lng="be">
    <trd>&c_z;&c_g;&c_a;&c_s;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_ib;&c_r;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_f;&c_e;&c_d;&c_y;&c_n;&c_g;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>f&agrave;ding</trd>,
    <trd>fosa</trd>,
    <trd>esva&iuml;ment</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>fading</trd>,
    <trd>&uacute;nik <klr>(rad.p&rcaron;&iacute;jmu)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Fading</trd>,
    <trd>Schwankung <klr>(Feldst&auml;rke)</klr></trd>
  </trdgrp>
  <trd lng="en">fading</trd>
  <trd lng="es">fundido<klr tip="amb"> (imagen o sonido)</klr></trd>
  <trdgrp lng="fr">
    <trd>&eacute;vanouissement<klr tip="ind"> (fading)</klr></trd>,
    <trd>fading</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>elhalkul&aacute;s</trd>,
    <trd>fading</trd>
  </trdgrp>
  <trd lng="nl">fading</trd>
  <trdgrp lng="pt">
    <trd>f&aacute;dingue</trd>,
    <trd>fading</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_t;&c_u;&c_h;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_i;&c_r;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_f;&c_je;&c_d;&c_i;&c_n;&c_g;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>fading</trd>,
    <trd>&uacute;nik el. magnetick&yacute;ch v&lacute;n</trd>
  </trdgrp>
</drv>

<drv mrk="fad.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="fad.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malforti&gcirc;i, <ref tip="vid" cel="aper.mal0i.nevidebligxi">malaperi</ref> iom post iom:
      <ekz>
        montegoj <klr>[&#8230;]</klr> etendi&gcirc;as vico post vico kaj 
        malproksime <tld/>as en blua valo
        <fnt>
          <aut>Vladimir Glazunov</aut>,
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Noveloj/Cxarmo.html">La
          &ccirc;armo de &Ccirc;ielmontaro</url></vrk>,
          <lok>Fonto, numero 37a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">verblassen</trd>
  </snc>
  <trdgrp lng="ca">
    <trd>fondre's</trd>,
    <trd>esvair-se</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>sl&aacute;bnout <klr>(zvuk)</klr></trd>,
    <trd>unikat</trd>
  </trdgrp>
  <trd lng="es">fundir<klr tip="amb"> (imagen o sonido)</klr></trd>
  <trdgrp lng="fr">
    <trd>disparaitre<klr> (en s'&eacute;loignant)</klr></trd>,
    <trd>dispara&icirc;tre<klr> (en s'&eacute;loignant)</klr></trd>,
    <trd>s'&eacute;vanouir<klr> (disparaitre)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>slabn&uacute;&tcaron; <klr>(zvuk)</klr></trd>,
    <trd>unika&tcaron;</trd>
  </trdgrp>
  <trd lng="tr">g&ouml;zden kaybolmak<klr>(uzakla&scedil;arak)</klr></trd>
</drv>
</art>
