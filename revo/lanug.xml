<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lanug.xml,v 1.21 2023/09/01 19:10:58 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lanug</rad>/o
</kap>
<drv mrk="lanug.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lanug.0o.birdo">
    <dif>
      Tre delikata, maldika kaj malpeza plumaro, kovranta la ventron de
      la
      birdoj sub la pli dikaj plumoj:
      <ekz>
        <tld/>a litkuseno.
      </ekz>
    </dif>
    <trd lng="br">marblu&ntilde;v</trd>
    <trdgrp lng="de">
      <trd>Daune</trd>,
      <trd>Flaumfeder</trd>
    </trdgrp>
    <trd lng="en">down</trd>
    <trd lng="es">plum&oacute;n</trd> 
    <trdgrp lng="hu">
      <trd>pihe</trd>,
      <trd>tollpihe</trd>
    </trdgrp>
    <trd lng="pt">penugem</trd>
    <trd lng="sv">dun</trd>
  </snc>
  <snc mrk="lanug.0o.junulo">
    <dif>
      Tre delikata hararo unuafoje kreskanta sur lipoj, vangoj kaj
      mentono
      de junuloj:
        <ekz>
          inter la nazo kaj la bu&scirc;o de la junulo estas komenco de 
          projekto de <tld/>o
         <fnt><aut>Machado de Assis, trad. Paulo Viana</aut>
         <vrk>La divenistino kaj aliaj rakontoj</vrk><lok>Oportuno, jaro
         2005a, p.
         45a</lok></fnt>.
	</ekz>
    </dif>
    <trd lng="br">marblev</trd>
    <trdgrp lng="de">
      <trd>Flaum</trd>,
      <trd>Milchbart</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>fluff</trd>,
      <trd>nap</trd>
    </trdgrp>
    <trd lng="es">vello</trd> 
    <trd lng="hu">pihesz&odblac;rzet</trd>
    <trd lng="pt">bu&ccedil;o</trd>
    <trd lng="sv">fjun</trd>
  </snc>
  <snc mrk="lanug.0o.simila">
    <dif>
      Io simila al <tld/>o:
      <ekz>
        la <tld/>o de persiko;
      </ekz>
      <ekz>
        <tld/>aj <klr>(kovritaj je <tld/>o)</klr> folioj, florbutonoj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la bran&ccirc;oj <tld/>aj de ne&gcirc;o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="lan.0o">lano</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Flaum</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>pelusa</trd>,
      <trd>pelusilla</trd>
    </trdgrp> 
    <trd lng="hu">pihe</trd>
    <trd lng="pt">cot&atilde;o</trd>
    <trd lng="sv">fjun</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_u;&c_h;</trd>,
    <trd>&c_p;&c_u;&c_sh;&c_o;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>chm&yacute;&rcaron;&iacute;</trd>,
    <trd>prachov&eacute; pe&rcaron;&iacute;</trd>
  </trdgrp>
  <trd lng="fr">duvet</trd>
  <trdgrp lng="ja">
    <trd>&#x7DBF;&#x7FBD;  [<ind>&#x308F;&#x305F;&#x3070;&#x306D;</ind>]</trd>,
    <trd>&#x3046;&#x3076;&#x6BDB;  [<ind>&#x3046;&#x3076;&#x3052;</ind>]</trd>,
    <trd>&#x306B;&#x3053;&#x6BDB;  [<ind>&#x306B;&#x3053;&#x3051;</ind>]</trd>,
    <trd>&#x7DBF;&#x6BDB;  [<ind>&#x308F;&#x305F;&#x3052;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">dons</trd>
  <trdgrp lng="pl">
    <trd>pierze</trd>,
    <trd>puch</trd>
  </trdgrp>
  <trd lng="pt">lanugem</trd>
  <trdgrp lng="ro">
    <trd>pene</trd>,
    <trd>puf</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_u;&c_h;</trd>,
    <trd>&c_p;&c_u;&c_sh;&c_o;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chm&yacute;rie</trd>,
    <trd>jemn&eacute; ch&lacute;pky</trd>,
    <trd>p&aacute;perie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x443;&#x445;</trd>,
    <trd>&#x43F;&#x443;&#x448;&#x43E;&#x43A;</trd>
  </trdgrp>
</drv>
</art>
