<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lanug</rad>/o
</kap>
<drv mrk="lanug.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lanug.0o.birdo">
    <dif>
      Tre delikata, maldika kaj malpeza plumaro, kovranta la ventron de la
      birdoj sub la pli dikaj plumoj:
      <ekz>
        <tld/>a litkuseno.
      </ekz>
    </dif>
    <trd lng="br">marblu&ntilde;v</trd>
    <trdgrp lng="de">
      <trd>Daune</trd>,
      <trd>Flaumfeder</trd>
    </trdgrp>
    <trd lng="en">down</trd>
    <trdgrp lng="hu">
      <trd>pihe</trd>,
      <trd>tollpihe</trd>
    </trdgrp>    
  </snc>
  <snc mrk="lanug.0o.junulo">
    <dif>
      Tre delikata hararo unuafoje kreskanta sur lipoj, vangoj kaj mentono
      de junuloj.
    </dif>
    <trd lng="br">marblev</trd>
    <trdgrp lng="de">
      <trd>Flaum</trd>,
      <trd>Milchbart</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>fluff</trd>,
      <trd>nap</trd>
    </trdgrp>
    <trd lng="hu">pihesz&odblac;rzet</trd>    
  </snc>
  <snc mrk="lanug.0o.simila">
    <dif>
      Io simila al <tld/>o:
      <ekz>
        la <tld/>o de persiko;
      </ekz>
      <ekz>
        <tld/>aj <klr>(kovritaj je <tld/>o)</klr> folioj, florbutonoj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la bran&ccirc;oj <tld/>aj de ne&gcirc;o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="lan.0o">lano</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Flaum</trd>
    </trdgrp>
    <trd lng="hu">pihe</trd>    
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_u;&c_h;</trd>,
    <trd>&c_p;&c_u;&c_sh;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="fr">duvet</trd>
  <trd lng="nl">dons</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_u;&c_h;</trd>,
    <trd>&c_p;&c_u;&c_sh;&c_o;&c_k;</trd>
  </trdgrp>
</drv>
</art>
