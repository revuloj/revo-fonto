<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lanug</rad>/o
</kap>
<drv mrk="lanug.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Tre delikata, maldika kaj malpeza plumaro, kovranta la ventron de la
      birdoj sub la pli dikaj plumoj:
      <ekz>
        <tld/>a litkuseno.
      </ekz>
    </dif>
    <trd lng="en">down</trd>
  </snc>
  <snc>
    <dif>
      Tre delikata hararo unuafoje kreskanta sur lipoj, vangoj kaj mentono
      de junuloj.
    </dif>
    <trdgrp lng="en">
      <trd>fluff</trd>,
      <trd>nap</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Io simila al <tld/>o:
      <ekz>
        la <tld/>o de persiko;
      </ekz>
      <ekz>
        <tld/>aj (kovritaj je <tld/>o) folioj, florbutonoj
        <klr>(f)</klr>;
      </ekz>
      <ekz>
        la bran&ccirc;oj <tld/>aj de ne&gcirc;o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="lan.0o">lano</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">dons</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_u;&c_h;</trd>,
    <trd>&c_p;&c_u;&c_sh;&c_o;&c_k;</trd>
  </trdgrp>
</drv>
</art>
