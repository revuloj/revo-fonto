<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sum.xml,v 1.38 2020/02/16 00:10:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sum</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sum.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sum.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="sum.0o.adicio">
      <fnt><bib>MatTerm</bib><lok>p. 9</lok></fnt>
      <dif>
        Rezulto de <ref cel="adici.0o">adicio</ref>:
        <ekz>
          <frm>5</frm> estas la <tld/>o de <frm>3</frm> 
          kaj <frm>2</frm>;
        </ekz>
        <ekz>
          la <tld/>on de <frm am="a"><k>a</k></frm> kaj <frm
          am="b"><k>b</k></frm> 
          oni signas per <frm am="a+b"><k>a</k>+<k>b</k></frm>,
        </ekz>
        <ekz>
          kaj la <tld/>on de &ccirc;iuj termoj en 
          <ref cel="famili.0o.bildigo">familio</ref>
          <frm am="(a_i)_(i in bb I)"
          >(<k>a</k><sub><k>i</k></sub>)<sub><k>i</k>&#8712;<g>I</g></sub></frm>
          per <frm am="sum_(i in bb I)a_i"
          >&Sigma;<sub><k>i</k>&#8712;<g>I</g></sub>
          <k>a</k><sub><k>i</k></sub></frm>;
        </ekz>
        <ekz>
          <tld/>o de vektoroj, matricoj, polinomoj;
        </ekz>
        <ekz>
          <uzo tip="stl">FIG</uzo>
          jen la <tld/>o de miaj penoj.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>Summe<klr tip="ind"> (von Zahlen)</klr></trd>,
        <trd>Ergebnis</trd>
      </trdgrp>
      <trd lng="en">sum<klr tip="ind"> (of numbers)</klr></trd>
      <trd lng="fr">somme<klr tip="ind"> (de nombres)</klr></trd>
      <trd lng="hu">&ouml;sszeg<klr tip="ind"> (sz&aacute;mok&eacute;)</klr></trd>      
      <trd lng="pl">suma<klr tip="ind"> (liczb)</klr></trd>
      <trd lng="ru">&c_s;&c_u;&c_m;&c_m;&c_a;<klr tip="ind"> (&c_ch;&c_i;&c_s;&c_je;&c_l;)</klr></trd>
    </subsnc>
    <subsnc mrk="sum.0o.serio">
      <dif>
        <klr>(de konver&gcirc;a <ref cel="seri.0o.MAT">serio</ref>) </klr>
        <ref cel="limes.0o">Limeso</ref> de la vico de ties
        <ref cel="sum.parta0o">partaj sumoj</ref>:
        <ekz>
          se <frm am="|q| lt 1">|<k>q</k>| &lt; 1</frm>, 
          la <tld/>o de la serio kun 
          &gcirc;enerala termo <frm><k>q</k><sup><k>n</k></sup></frm> estas
          <frm am="(1-q)^-1">(1-<k>q</k>)<sup>-1</sup></frm>.
        </ekz>
      </dif>
      <rim>
        <tld lit="S"/>o de serio estas konceptebla kiel sumo de senfina
        nombro da termoj. Oni &gcirc;in ofte signas per
       <frm am="u_0+u_1+u_2,..."
       ><k>u</k><sub>0</sub>+<k>u</k><sub>1</sub>+<k>u</k><sub>2</sub>+...</frm>
             
       a&ubreve; <frm am="sum_(n in NN)u_n"
       >&Sigma;<sub><k>n</k>&#8712;<g>N</g></sub><k>u</k><sub><k>n</k></sub></frm>.
      </rim>
      <trd lng="de">Summe<klr tip="ind"> (von einer Reihe)</klr></trd>
      <trd lng="en">sum<klr tip="ind"> (of a series)</klr></trd>
      <trd lng="fr">somme<klr tip="ind"> (d'une s&eacute;rie)</klr></trd>
      <trd lng="hu">&ouml;sszeg<klr tip="ind"> (sorozat&eacute;)</klr></trd>      
      <trd lng="pl">suma<klr tip="ind"> (szeregu)</klr></trd>
      <trd lng="ru">&c_s;&c_u;&c_m;&c_m;&c_a;<klr tip="ind"> (&c_r;&c_ja;&c_d;&c_a;)</klr></trd>
    </subsnc>
    <subsnc mrk="sum.0o.vektorasubspaco">
      <dif>
        <klr>(de <ref cel="spac.vektorasub0o">vektoraj
        subspacoj</ref>)</klr>
        La vektora subspaco, <ref cel="nask.0i.MAT">naskita</ref> de
        ilia
        <ref cel="kun.0ajxo.aro">kuna&jcirc;o</ref>:
        <ekz>
          &ccirc;iu elemento <frm am="x"><k>x</k></frm> de la <tld/>o de
          subspacoj
          <frm am="(bb U_i)">(<g>U</g><sub><k>i</k></sub>)</frm>
          esprimi&gcirc;as kiel <tld/>o de finia nombro de vektoroj
          <frm am="x_i in bb U_i"
          ><k>x</k><sub><k>i</k></sub>&#8712;<g>U</g><sub><k>i</k></sub></frm>;
        </ekz>
        <ekz>
          la <tld/>o de du neegalaj 
          <ref cel="rekt.vektora0o">vektoraj rektoj</ref>
          estas <ref cel="eben.vektora0o">vektora ebeno</ref>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="rekt.0a.sumo">rekta</ref>.
      </refgrp>
      <trd lng="de">Summe<klr tip="ind"> (von Teilr&auml;umen)</klr></trd>
      <trd lng="en">sum<klr tip="ind"> (of vector subspaces)</klr></trd>
      <trd lng="fr">somme<klr tip="ind"> (de sous-espaces
      vectoriels)</klr></trd>
      <trd lng="hu">&ouml;sszeg<klr tip="ind"> (line&aacute;ris alt&eacute;r&eacute;)</klr></trd> 
          
      <trd lng="pl">suma<klr tip="ind"> (podprzestrzeni
      liniowych)</klr></trd>
      <trd lng="ru">&c_s;&c_u;&c_m;&c_m;&c_a;<klr tip="ind"> (&c_v;&c_je;&c_k;&c_t;&c_o;&c_r;&c_n;&c_y;&c_h;
      &c_p;&c_o;&c_d;&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;)</klr></trd>
    </subsnc>
  </snc>
  <snc mrk="sum.0o.mono">
    <dif>
      <ref tip="dif" cel="sum.mon0o.EKON">Monsumo</ref>:
      <ekz>
        devontigi sin al la pagado de grandaj
        <tld/>oj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="hu">p&eacute;nz&ouml;sszeg</trd>    
  </snc>
  <trdgrp lng="cs">
    <trd>obnos</trd>,
    <trd>sou&ccaron;et</trd>,
    <trd>suma</trd>,
    <trd>&uacute;hrn</trd>,
    <trd>&ccaron;&aacute;stka</trd>
  </trdgrp>
  <trd lng="de">Summe</trd>
  <trd lng="en">sum</trd>
  <trd lng="es">suma</trd>
  <trd lng="fr">somme</trd>
  <trd lng="nl">som</trd>
  <trd lng="ru">&c_s;&c_u;&c_m;&c_m;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>suma</trd>,
    <trd>s&uacute;hrn</trd>,
    <trd>s&uacute;&ccaron;et</trd>,
    <trd>&uacute;hrn</trd>
  </trdgrp>
</drv>

<drv mrk="sum.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      &Ccirc;ion adiciante.
    </dif>
    <trd lng="de">insgesamt</trd>
    <trd lng="hu">&ouml;sszesen</trd>    
  </snc>
  <snc>
    <dif>
      &Ccirc;ion konsiderante.
    </dif>
    <refgrp tip="sin">
      <ref cel="tut.en0e">entute</ref>.
    </refgrp>
    <trd lng="de">im <ind>Ganzen</ind> betrachtet</trd>
    <trdgrp lng="es">
      <trd>en <ind>suma</ind></trd>,
      <trd>en <ind>total</ind></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>en <ind>tout</ind></trd>,
      <trd>en <ind>somme</ind></trd>,
      <trd>au <ind>total</ind></trd>
    </trdgrp>
    <trd lng="hu">&ouml;sszess&eacute;g&eacute;ben</trd>    
  </snc>
  <trdgrp lng="cs">
    <trd>celkem</trd>,
    <trd>dohromady</trd>,
    <trd>souhrnn&ecaron;</trd>,
    <trd>vcelku</trd>,
    <trd>&uacute;hrnem</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v; <ind>&c_s;&c_u;&c_m;&c_m;&c_je;</ind></trd>,
    <trd>&c_s;&c_u;&c_m;&c_m;&c_a;&c_r;&c_n;&c_o;</trd>,
    <trd>&c_v; <ind>&c_i;&c_t;&c_o;&c_g;&c_je;</ind></trd>,
    <trd>&c_i;&c_t;&c_o;&c_g;&c_o;</trd>,
    <trd>&c_v; <ind>&c_o;&c_b;&c_shch;&c_je;&c_j; &c_s;&c_l;&c_o;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_i;</ind></trd>,
    <trd>&c_v;&c_s;&c_je;&c_g;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dohromady</trd>,
    <trd>dovedna</trd>,
    <trd>sum&aacute;rne</trd>,
    <trd>vcelku</trd>
  </trdgrp>
</drv>

<drv mrk="sum.0ero">
  <kap><tld/>ero</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 9</lok></fnt>
  <snc>
    <dif>
      <ref tip="dif" cel="term.0o.sumo">Termo</ref> de sumo a&ubreve;
      <ref tip="dif" cel="term.0o.serio">termo</ref> de serio.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>prvek sou&ccaron;tu</trd>,
    <trd>s&ccaron;&iacute;tanec</trd>
  </trdgrp>
  <trd lng="es">sumando</trd>
  <trdgrp lng="hu">
    <trd>&ouml;sszeadand&oacute;</trd>,
    <trd>tag</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>prvok s&uacute;&ccaron;tu</trd>,
    <trd>s&ccaron;&iacute;tanec</trd>
  </trdgrp>
</drv>

<drv mrk="sum.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Adicii.
    </dif>
  </snc>
  <trd lng="cs">nechat se&ccaron;&iacute;st</trd>
  <trd lng="de">summieren</trd>
  <trd lng="es">sumar</trd>
  <trd lng="fr">sommer</trd>
  <trdgrp lng="hu">
    <trd>&ouml;sszead</trd>,
    <trd>&ouml;sszegez</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>optellen</trd>,
    <trd>sommeren</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_u;&c_m;&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>spo&ccaron;&iacute;ta&tcaron;</trd>,
    <trd>zhrn&uacute;&tcaron;</trd>,
    <trd>zr&aacute;ta&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="sum.mon0o">
  <kap>mon<tld/>o</kap>
  <snc mrk="sum.mon0o.EKON">
    <dif>
      Ia, iom granda kvanto da mono;
      <ref cel="sum.0o.mono">sumo<sncref/></ref>:
      <ekz>
        &scirc;i komencis senti, ke malgrandas la donacoj
        kiujn &scirc;i ricevas kompare kun la supoze altaj
        mon<tld/>oj fordonitaj al E&ubreve;lalia
        <fnt>
          <aut>M. Proust, trad. &Jcirc; Va&scirc;e</aut>
          <vrk><url
          ref="http://jxvasxe.free.fr/tradukoj/la-vojo-de-svan-eo.htm">
          Sur la Vojo de Svan</url></vrk>
          <lok>2015-2017</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>montant</trd>,
    <trd>somme d'argent</trd>
  </trdgrp>
</drv>

<drv mrk="sum.re0o">
  <kap>re<tld/>o</kap>
  <snc>
    <dif>
      Mallongigo de io pli longe dirita.
    </dif>
    <refgrp tip="vid">
      <ref cel="skem.0o">skemo</ref>,
      <ref cel="skiz.0o">skizo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>p&rcaron;ehled</trd>,
    <trd>resum&eacute;</trd>,
    <trd>shrnut&iacute;</trd>,
    <trd>souhrn</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Res&uuml;mee</trd>,
    <trd>Zusammenfassung</trd>
  </trdgrp>
  <trd lng="es">resumen</trd>
  <trd lng="fr">r&eacute;sum&eacute;</trd>
  <trdgrp lng="hu">
    <trd>&ouml;sszefoglal&aacute;s</trd>,
    <trd>&ouml;sszegz&eacute;s</trd>,
    <trd>rez&uuml;m&eacute;</trd>
  </trdgrp>
  <trd lng="nl">samenvatting</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_z;&c_ju;&c_m;&c_je;</trd>,
    <trd>&c_i;&c_t;&c_o;&c_g;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>preh&lcaron;ad</trd>,
    <trd>rekapitul&aacute;cia</trd>,
    <trd>zhrnutie</trd>
  </trdgrp>
   <adm>
     &Scirc;ajnas, ke oni nun preferas konsideri "resum",
     kiel memstaran radikon. <aut>MB</aut>
   </adm>
</drv>

<drv mrk="sum.re0i">
  <kap><ofc>*</ofc>re<tld/>i <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url
  ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5210">re'sum'</url></lok></fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Mallongige esprimi per malmultaj vortoj la esencon kaj la precipon
      de tio, kio estis dirita pli longe.
    </dif>
    <refgrp tip="vid">
      <ref cel="redukt.0i">redukti</ref>.
    </refgrp>
  <rim>
   Vidu la rimarkon &ccirc;e <ref cel="re.0rim">re-</ref>.
   </rim>
   </snc>
  <trd lng="cs">konspektovat</trd>
  <trdgrp lng="de">
    <trd>res&uuml;mieren</trd>,
    <trd>zusammenfassen</trd>
  </trdgrp>
  <trd lng="es">resumir</trd>
  <trd lng="fr">r&eacute;sumer</trd>
  <trdgrp lng="hu">
    <trd>&ouml;sszefoglal</trd>,
    <trd>&ouml;sszegez</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>samenvatten</trd>,
    <trd>resumeren</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_z;&c_ju;&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_y;&c_t;&c_o;&c_zh;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zhrn&uacute;&tcaron;</trd>
</drv>

<drv mrk="sum.re0e">
  <kap>re<tld/>e</kap>
  <snc>
    <dif>
      Mallonge, malmultvorte.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>zusammenfassend</trd>,
    <trd>kurz gesagt</trd>
  </trdgrp>
  <trd lng="es">en <ind>resumen</ind></trd>
  <trdgrp lng="fr">
    <trd>en <ind>r&eacute;sum&eacute;</ind></trd>,
    <trd>en <ind>bref</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&ouml;sszegezve</trd>,
    <trd>&ouml;sszefoglal&oacute;an</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>samenvattend</trd>,
    <trd>resumerend</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v; &c_o;&c_b;&c_shch;&c_je;&c_m;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_o;&c_ch;&c_je; &c_g;&c_o;&c_v;&c_o;&c_r;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="sum.parta0o">
  <kap>parta <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>EKV</bib><lok>&para;390</lok></fnt>
  <snc>
    <dif>
      <klr>(de <ref cel="seri.0o.MAT">serio</ref> <frm
      am="u"><k>u</k></frm>,
      je ordo <frm am="n"><k>n</k></frm>)</klr>
      La sumo <frm am="u_0+u_1+...+u_n"
      ><k>u</k><sub>0</sub>+<k>u</k><sub>1</sub>+... 
      +<k>u</k><sub><k>n</k></sub></frm>.
    </dif>
  </snc>
  <trd lng="de">Teilsumme<klr tip="ind"> (von einer Reihe)</klr></trd>
  <trd lng="en">partial <ind>sum</ind><klr tip="ind"> (of a
  series)</klr></trd>
  <trd lng="es">suma parcial <klr>(de una serie)</klr></trd>
  <trd lng="fr">somme partielle<klr tip="ind"> (d'une s&eacute;rie)</klr></trd>
  <trd lng="hu">r&eacute;sz&ouml;sszeg</trd>  
  <trd lng="pl">suma cz&eogonek;&sacute;ciowa<klr tip="ind"> (szeregu)</klr></trd>
  <trd lng="ru">&c_ch;&c_a;&c_s;&c_t;&c_i;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_s;&c_u;&c_m;&c_m;&c_a;</ind><klr tip="ind">
  (&c_r;&c_ja;&c_d;&c_a;)</klr></trd>
</drv>
</art>
