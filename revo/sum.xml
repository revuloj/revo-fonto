<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sum.xml,v 1.124 2023/12/12 20:10:28 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sum</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sum.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sum.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="sum.0o.adicio">
      <fnt><bib>MatTerm</bib><lok>p. 9</lok></fnt>
      <dif>
        Rezulto de <ref cel="adici.0o">adicio</ref>:
        <ekz>
          <frm>5</frm> estas la <tld/>o de <frm>3</frm>
          kaj <frm>2</frm>;
        </ekz>
        <ekz>
          <uzo tip="stl">FIG</uzo>
          <nom>Helga</nom> prezentis al si tage kaj nokte la tutan <tld/>on de &scirc;ia feli&ccirc;o
          <fnt>
            <bib>Fab4</bib>
            <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
          </fnt>;
        </ekz>
        <ekz>
          la <tld/>on de <frm am="a"><k>a</k></frm> kaj <frm am="b"><k>b</k></frm>
          oni signas per <frm am="a+b"><k>a</k>+<k>b</k></frm>,
        </ekz>
        <ekz>
          kaj la <tld/>on de &ccirc;iuj termoj en
          <ref cel="famili.0o.bildigo">familio</ref>
          <frm am="(a_i)_(i in bb          
          I)">(<k>a</k><sub><k>i</k></sub>)<sub><k>i</k>&#8712;<g>I</g></sub></frm>
          per <frm am="sum_(i in bb I)a_i">&Sigma;<sub><k>i</k>&#8712;<g>I</g></sub>
          <k>a</k><sub><k>i</k></sub></frm>;
        </ekz>
        <ekz>
          <tld/>o de vektoroj, matricoj, polinomoj.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>Summe<klr tip="ind"> (von Zahlen)</klr></trd>,
        <trd>Ergebnis</trd>
      </trdgrp>
      <trd lng="en">sum<klr tip="ind"> (of numbers)</klr></trd>
      <trd lng="fr">somme<klr tip="ind"> (de nombres)</klr></trd>
      <trd lng="hu">&ouml;sszeg<klr tip="ind"> (sz&aacute;mok&eacute;)</klr></trd>
      <trd lng="pl">suma<klr tip="ind"> (liczb)</klr></trd>
      <trd lng="ru">&c_s;&c_u;&c_m;&c_m;&c_a;<klr tip="ind"> (&c_ch;&c_i;&c_s;&c_je;&c_l;)</klr></trd>
    </subsnc>
    <subsnc mrk="sum.0o.serio">
      <dif>
        <klr>(de konver&gcirc;a <ref cel="seri.0o.MAT">serio</ref>) </klr>
        <ref cel="limes.0o">Limeso</ref> de la vico de ties
        <ref cel="sum.parta0o">partaj sumoj</ref>:
        <ekz>
          se <frm am="|q| lt 1">|<k>q</k>| &lt; 1</frm>,
          la <tld/>o de la serio kun
          &gcirc;enerala termo <frm am="q^n"><k>q</k><sup><k>n</k></sup></frm> estas
          <frm am="(1-q)^-1">(1-<k>q</k>)<sup>-1</sup></frm>.
        </ekz>
      </dif>
      <rim>
        <tld lit="S"/>o de serio estas konceptebla kiel sumo de senfina
        nombro da termoj. Oni &gcirc;in ofte signas per
       <frm
       am="u_0+u_1+u_2,..."><k>u</k><sub>0</sub>+<k>u</k><sub>1</sub>+<k>u</k><sub>2</sub>+...</frm>
       a&ubreve; <frm am="sum_(n in NN)u_n">&Sigma;<sub><k>n</k>&#8712;<g>N</g></sub><k>u</k><sub><k>n</k></sub></frm>.
      </rim>
      <trd lng="de">Summe<klr tip="ind"> (von einer Reihe)</klr></trd>
      <trd lng="en">sum<klr tip="ind"> (of a series)</klr></trd>
      <trd lng="fr">somme<klr tip="ind"> (d'une s&eacute;rie)</klr></trd>
      <trd lng="hu">&ouml;sszeg<klr tip="ind"> (sorozat&eacute;)</klr></trd>
      <trd lng="pl">suma<klr tip="ind"> (szeregu)</klr></trd>
      <trd lng="ru">&c_s;&c_u;&c_m;&c_m;&c_a;<klr tip="ind"> (&c_r;&c_ja;&c_d;&c_a;)</klr></trd>
    </subsnc>
    <subsnc mrk="sum.0o.vektorasubspaco">
      <dif>
        <klr>(de <ref cel="spac.vektorasub0o">vektoraj
        subspacoj</ref>)</klr>
        La vektora subspaco, <ref cel="nask.0i.MAT">naskita</ref> de
        ilia
        <ref cel="kun.0ajxo.aro">kuna&jcirc;o</ref>:
        <ekz>
          &ccirc;iu elemento <frm am="x"><k>x</k></frm> de la <tld/>o de
          subspacoj
          <frm am="(bb U_i)">(<g>U</g><sub><k>i</k></sub>)</frm>
          esprimi&gcirc;as kiel <tld/>o de finia nombro de vektoroj
          <frm am="x_i in bb U_i"><k>x</k><sub><k>i</k></sub>&#8712;<g>U</g><sub><k>i</k></sub></frm>;
        </ekz>
        <ekz>
          la <tld/>o de du neegalaj
          <ref cel="rekt.vektora0o">vektoraj rektoj</ref>
          estas <ref cel="eben.vektora0o">vektora ebeno</ref>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="rekt.0a.sumo">rekta</ref>.
      </refgrp>
      <trd lng="de">Summe<klr tip="ind"> (von Teilr&auml;umen)</klr></trd>
      <trd lng="en">sum<klr tip="ind"> (of vector subspaces)</klr></trd>
      <trd lng="fr">somme<klr tip="ind"> (de sous-espaces
      vectoriels)</klr></trd>
      <trd lng="hu">&ouml;sszeg<klr tip="ind"> (line&aacute;ris alt&eacute;r&eacute;)</klr></trd>
      <trd lng="pl">suma<klr tip="ind"> (podprzestrzeni
      liniowych)</klr></trd>
      <trd lng="ru">&c_s;&c_u;&c_m;&c_m;&c_a;<klr tip="ind"> (&c_v;&c_je;&c_k;&c_t;&c_o;&c_r;&c_n;&c_y;&c_h;
      &c_p;&c_o;&c_d;&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;)</klr></trd>
    </subsnc>
  </snc>
  <snc mrk="sum.0o.mono">
    <dif>
      <ref tip="dif" cel="sum.mon0o.EKON">Monsumo</ref>:
      <ekz>
        prenu duoblan <tld/>on en viajn manojn
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 43:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        per granda <tld/>o mi akiris tiun civitanecon
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 22:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        devontigi sin <klr>[&#8230;]</klr> al la pagado de grandaj
        <tld/>oj
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">p&eacute;nz&ouml;sszeg</trd>
  </snc>
  <trd lng="af">totale</trd>
  <trd lng="am">&#4896;&#4677;&#x120B;&#x120B;</trd>
  <trd lng="ar">&a_A;&a_l;&a_k;&a_l;</trd>
