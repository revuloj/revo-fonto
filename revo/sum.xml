<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sum</rad>/o
</kap>
<drv mrk="sum.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sum.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="sum.0o.adicio">
      <fnt><bib>MatTerm</bib><lok>p. 9</lok></fnt>
      <dif>
        Rezulto de <ref cel="adici.0o">adicio</ref>:
        <ekz>
          5 estas la <tld/>o de 3 kaj 2;
        </ekz>
        <ekz>
          jen la <tld/>o de miaj penoj.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>Summe<klr tip="ind"> (von Zahlen)</klr></trd>,
        <trd>Ergebnis</trd></trdgrp> 
      <trd lng="en">sum<klr tip="ind"> (of numbers)</klr></trd>
      <trd lng="fr">somme<klr tip="ind"> (de nombres)</klr></trd>
      <trd lng="ru">&c_s;&c_u;&c_m;&c_m;&c_a;<klr tip="ind"> (&c_ch;&c_i;&c_s;&c_je;&c_l;)</klr></trd>
    </subsnc>
    <subsnc mrk="sum.0o.serio">
      <dif>
        <klr>(de konver&gcirc;a <ref cel="seri.0o.MAT">serio</ref>)</klr>
        <ref cel="limes.0o">Limeso</ref> de la vico de ties 
        <ref cel="sum.parta0o">partaj sumoj</ref>:
        <ekz>
          se 0&lt;q&lt;1, la <tld/>o de la serio kun &gcirc;enerala termo
          q<sup>n</sup> estas (1-q)<sup>-1</sup>.
        </ekz>
      </dif>
      <rim>
        <tld lit="S"/>o de serio estas konceptebla kiel sumo de senfina
        nombro da termoj. Oni &gcirc;in ofte signas per
        u<sub>0</sub>+u<sub>1</sub>+u<sub>2</sub>+...
      </rim>
      <trd lng="de">Summe<klr tip="ind"> (von einer Reihe)</klr></trd>
      <trd lng="en">sum<klr tip="ind"> (of a series)</klr></trd>
      <trd lng="fr">somme<klr tip="ind"> (d'une s&eacute;rie)</klr></trd>
      <trd lng="ru">&c_s;&c_u;&c_m;&c_m;&c_a;<klr tip="ind"> (&c_r;&c_ja;&c_d;&c_a;)</klr></trd>
    </subsnc>
    <subsnc mrk="sum.0o.vektorasubspaco">
      <dif>
        <klr>(de <ref cel="spac.vektorasub0o">vektoraj subspacoj</ref>)</klr>
        La vektora subspaco, <ref cel="nask.0i.MAT">naskita</ref> de ilia
        <ref cel="kun.0ajxo.aro">kuna&jcirc;o</ref>:
        <ekz>
          &ccirc;iu elemento x de la <tld/>o de subspacoj (<em>U</em><sub>i</sub>)
          esprimi&gcirc;as kiel sumo de finia nombro da vektoroj
          x<sub>i</sub>&#8712;<em>U</em><sub>i</sub>;
        </ekz>
        <ekz>
          la <tld/>o de du neegalaj <ref cel="rekt.vektora0o">rektoj</ref>
          estas <ref cel="eben.vektora0o">ebeno</ref>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="rekt.0a.sumo">rekta</ref>.
      </refgrp>
      <trd lng="de">Summe<klr tip="ind"> (von Teilr&auml;men)</klr></trd>
      <trd lng="en">sum<klr tip="ind"> (of vector subspaces)</klr></trd>
      <trd lng="fr">somme<klr tip="ind"> (de sous-espaces vectoriels)</klr></trd>
      <trd lng="ru">&c_s;&c_u;&c_m;&c_m;&c_a;<klr tip="ind"> (&c_v;&c_je;&c_k;&c_t;&c_o;&c_r;&c_n;&c_y;&c_h; &c_p;&c_o;&c_d;&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;)</klr></trd>
    </subsnc>
  </snc>
  <snc mrk="sum.0o.mono">
    <dif>
      Difinita kvanto da mono:
      <ekz>
        devontigi sin al la pagado de grandaj
        <tld/>oj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Summe</trd>
  <trd lng="en">sum</trd>
  <trd lng="fr">somme</trd>
  <trd lng="nl">som</trd>
  <trd lng="ru">&c_s;&c_u;&c_m;&c_m;&c_a;</trd>
</drv>

<drv mrk="sum.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      &Ccirc;ion adiciante.
    </dif>
    <trd lng="de">insgesamt</trd>
  </snc>
  <snc>
    <dif>
      &Ccirc;ion konsiderante.
    </dif>
    <refgrp tip="sin">
      <ref cel="tut.en0e">entute</ref>.
    </refgrp>
    <trd lng="de">im <ind>Ganzen</ind> betrachtet</trd>
    <trdgrp lng="fr">
      <trd>en <ind>tout</ind></trd>,
      <trd>en <ind>somme</ind></trd>,
      <trd>au <ind>total</ind></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_v; <ind>&c_s;&c_u;&c_m;&c_m;&c_je;</ind></trd>,
    <trd>&c_s;&c_u;&c_m;&c_m;&c_a;&c_r;&c_n;&c_o;</trd>,
    <trd>&c_v; <ind>&c_i;&c_t;&c_o;&c_g;&c_je;</ind></trd>,
    <trd>&c_i;&c_t;&c_o;&c_g;&c_o;</trd>,
    <trd>&c_v; <ind>&c_o;&c_b;&c_shch;&c_je;&c_j; &c_s;&c_l;&c_o;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_i;</ind></trd>,
    <trd>&c_v;&c_s;&c_je;&c_g;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="sum.0ero">
  <kap><tld/>ero</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 9</lok></fnt>
  <snc>
    <dif>
      <ref tip="dif" cel="term.0o.sumo">Termo de sumo</ref> a&ubreve;
      <ref tip="dif" cel="term.0o.serio">termo de serio</ref>.
    </dif>
  </snc>
</drv>

<drv mrk="sum.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Adicii.
    </dif>
  </snc>
  <trd lng="de">summieren</trd>
  <trd lng="fr">sommer</trd>
  <trdgrp lng="nl">
    <trd>optellen</trd>,
    <trd>sommeren</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_u;&c_m;&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="sum.re0o">
  <kap>re<tld/>o</kap>
  <snc>
    <dif>
      Mallongigo de io pli longe dirita.
    </dif>
    <refgrp tip="vid">
      <ref cel="skem.0o">skemo</ref>,
      <ref cel="skiz.0o">skizo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Res&uuml;mee</trd>,
    <trd>Zusammenfassung</trd>
  </trdgrp>
  <trd lng="fr">r&eacute;sum&eacute;</trd>
  <trd lng="nl">samenvatting</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_z;&c_ju;&c_m;&c_je;</trd>,
    <trd>&c_i;&c_t;&c_o;&c_g;</trd>
  </trdgrp>
  <adm>
    &Scirc;ajnas, ke oni nun preferas konsideri "resum",
    kiel memstaran radikon. <aut>MB</aut>
  </adm>
</drv>

<drv mrk="sum.re0i">
  <kap>re<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Mallongige esprimi per malmultaj vortoj la esencon kaj la precipon
      de tio, kio estis dirita pli longe.
    </dif>
    <refgrp tip="vid">
      <ref cel="redukt.0i">redukti</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>res&uuml;mieren</trd>,
    <trd>zusammenfassen</trd>
  </trdgrp>
  <trd lng="fr">r&eacute;sumer</trd>
  <trdgrp lng="nl">
    <trd>samenvatten</trd>,
    <trd>resumeren</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_z;&c_ju;&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_y;&c_t;&c_o;&c_zh;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="sum.re0e">
  <kap>re<tld/>e</kap>
  <snc>
    <dif>
      Mallonge, malmultvorte.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>zusammenfassend</trd>,
    <trd>kurz gesagt</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>en <ind>r&eacute;sum&eacute;</ind></trd>,
    <trd>en <ind>bref</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>samenvattend</trd>,
    <trd>resumerend</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v; &c_o;&c_b;&c_shch;&c_je;&c_m;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_o;&c_ch;&c_je; &c_g;&c_o;&c_v;&c_o;&c_r;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="sum.parta0o">
  <kap>parta <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(de <ref cel="seri.0o.MAT">serio</ref> u, je ordo n)</klr>
      La sumo u<sub>0</sub>+u<sub>1</sub>+...+u<sub>n</sub>.
    </dif>
  </snc>
  <trd lng="de">Teilsumme<klr tip="ind"> (von einer Reihe)</klr></trd>
  <trd lng="en">partial <ind>sum</ind><klr tip="ind"> (of a series)</klr></trd>
  <trd lng="fr">somme partielle<klr tip="ind"> (d'une s&eacute;rie)</klr></trd>
  <trd lng="ru">&c_ch;&c_a;&c_s;&c_t;&c_i;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_s;&c_u;&c_m;&c_m;&c_a;</ind><klr tip="ind"> (&c_r;&c_ja;&c_d;&c_a;)</klr></trd>
</drv>

</art>
