<drv mrk="sum.mon0o">
  <kap>mon<tld/>o</kap>
  <snc mrk="sum.mon0o.EKON">
    <dif>
      Ia, iom granda kvanto da mono;
      <ref cel="sum.0o.mono">sumo<sncref/></ref>:
      <ekz>
        &scirc;i komencis senti, ke malgrandas la donacoj
        kiujn &scirc;i ricevas kompare kun la supoze altaj
        mon<tld/>oj fordonitaj al E&ubreve;lalia
        <fnt>
          <aut>M. Proust, trad. &Jcirc; Va&scirc;e</aut>
          <vrk><url ref="http://jxvasxe.free.fr/tradukoj/la-vojo-de-svan-eo.htm">
          Sur la Vojo de Svan</url></vrk>
          <lok>2015-2017</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>montant</trd>,
    <trd>somme d'argent</trd>
  </trdgrp>
</drv>

<drv mrk="sum.re0o">
  <kap>re<tld/>o</kap>
  <snc>
    <dif>
      Mallongigo de io pli longe dirita.
    </dif>
    <refgrp tip="vid">
      <ref cel="skem.0o">skemo</ref>,
      <ref cel="skiz.0o">skizo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>p&rcaron;ehled</trd>,
    <trd>resum&eacute;</trd>,
    <trd>shrnut&iacute;</trd>,
    <trd>souhrn</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Res&uuml;mee</trd>,
    <trd>Zusammenfassung</trd>
  </trdgrp>
  <trd lng="es">resumen</trd>
  <trd lng="fr">r&eacute;sum&eacute;</trd>
  <trdgrp lng="hu">
    <trd>&ouml;sszefoglal&aacute;s</trd>,
    <trd>&ouml;sszegz&eacute;s</trd>,
    <trd>rez&uuml;m&eacute;</trd>
  </trdgrp>
  <trd lng="nl">samenvatting</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_z;&c_ju;&c_m;&c_je;</trd>,
    <trd>&c_i;&c_t;&c_o;&c_g;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>preh&lcaron;ad</trd>,
    <trd>rekapitul&aacute;cia</trd>,
    <trd>zhrnutie</trd>
  </trdgrp>
   <adm>
     &Scirc;ajnas, ke oni nun preferas konsideri "resum",
     kiel memstaran radikon. <aut>MB</aut>
   </adm>
</drv>

