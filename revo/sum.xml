<drv mrk="sum.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      &Ccirc;ion adiciante:
      <ekz>
        viro tre altkreska <klr>[&#8230;]</klr> havis sur la manoj kaj sur la piedoj po ses
        fingroj, <tld/>e dudek kvar
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 21:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la da&ubreve;ro de tuta semajno &ccirc;iuvespere <klr>[&#8230;]</klr> li rakontis al li,
        tio estas <tld/>e sep historioj
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e kiom mi &scirc;uldas
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>?
      </ekz>
      <ekz>
        la domo ne estis granda, <tld/>e havis eble du-tri &ccirc;ambrojn
        <fnt>
          <bib>VaK</bib>
          <lok>Dua &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">insgesamt</trd>
    <trd lng="hu">&ouml;sszesen</trd>
  </snc>
  <snc>
    <dif>
      &Ccirc;ion konsiderante:
      <ekz>
        mi estas tre tedita de <klr>[&#8230;]</klr> iliaj koketoj, de iliaj &jcirc;aluzoj, de iliaj
        disputoj, <klr>[&#8230;]</klr> <tld/>e, &ccirc;i tiuj du junulinoj komencas tre tedi min
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e kaj resume, mi eksciis, ke li havas &gcirc;uste la saman kormal&gcirc;uston kiel mi
        <fnt>
          <bib>ChC</bib>
          <lok>18</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel ni povas vivi <tld/>e kaj komune tiel
        <fnt>
          <bib>VaK</bib>
          <lok>Kvina &Ccirc;apitro</lok>
        </fnt>?
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="tut.en0e">entute</ref>
    </refgrp>
    <trd lng="de">im <ind>Ganzen</ind> betrachtet</trd>
    <trdgrp lng="es">
      <trd>en <ind>suma</ind></trd>,
      <trd>en <ind>total</ind></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>en <ind>tout</ind></trd>,
      <trd>en <ind>somme</ind></trd>,
      <trd>au <ind>total</ind></trd>
    </trdgrp>
    <trd lng="hu">&ouml;sszess&eacute;g&eacute;ben</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>celkem</trd>,
    <trd>dohromady</trd>,
    <trd>souhrnn&ecaron;</trd>,
    <trd>vcelku</trd>,
    <trd>&uacute;hrnem</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#21512;&#x8A08;&#12391;  <pr>&#12372;&#12358;&#12369;&#12356;&#12391;</pr></trd>,
    <trd>&#21512;&#x308F;&#x305B;&#12390;  <pr>&#12354;&#x308F;&#x305B;&#12390;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>og&oacute;&lstroke;em</trd>,
    <trd>w  <ind>sumie</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v; <ind>&c_s;&c_u;&c_m;&c_m;&c_je;</ind></trd>,
    <trd>&c_s;&c_u;&c_m;&c_m;&c_a;&c_r;&c_n;&c_o;</trd>,
    <trd>&c_v; <ind>&c_i;&c_t;&c_o;&c_g;&c_je;</ind></trd>,
    <trd>&c_i;&c_t;&c_o;&c_g;&c_o;</trd>,
    <trd>&c_v; <ind>&c_o;&c_b;&c_shch;&c_je;&c_j; &c_s;&c_l;&c_o;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_i;</ind></trd>,
    <trd>&c_v;&c_s;&c_je;&c_g;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dohromady</trd>,
    <trd>dovedna</trd>,
    <trd>sum&aacute;rne</trd>,
    <trd>vcelku</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_u;&c_m;&c_a;&c_r;&c_n;&c_o;</trd>,
    <trd>&c_u; &c_p;&c_ib;&c_d;&c_s;&c_u;&c_m;&c_k;&c_u;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_o;&c_m;</trd>,
    <trd>&c_v;&c_s;&c_mol;&c_o;&c_g;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="sum.0i">
  <kap>
    <tld/>i
  </kap>
  <snc>
    <dif>
      Konsistigi la <tld/>on de io; esti entute:
      <ekz>
        &gcirc;ia jarbud&gcirc;eto kune <klr>[&#8230;]</klr> <tld/>is apena&ubreve; dek milionojn da dolaroj
        <fnt>
          <bib>FSp</bib>
          <lok>Tria Parto: Ligo De Nacioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        nuntempe la CO2-kvanto <tld/>as je 380 ppm kontraste al 270 ppm de la
        anta&ubreve;industriaj tempoj
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2005/008735.php">&Ccirc;u la tero fari&gcirc;os sa&ubreve;no?</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> indi&gcirc;ena komunumo <tld/>as <klr>[&#8230;]</klr> dek mil parolantojn
        <fnt>
          <bib>Monato</bib>
          <aut>Jos&eacute; Leite de Oliveira Junior</aut>
          <vrk><url ref="&Monato;/2006/009106.php">&Ccirc;u lingvoj formortu?</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        dama&gcirc;oj ka&ubreve;zitaj de fungoj al tritiko, rizo kaj maizo <tld/>as jare je 60
        miliardoj da dolaroj
        <fnt>
          <bib>Monato</bib>
          <aut>Joyce Bunting</aut>
          <vrk><url ref="&Monato;/2013/010846.php">Taktiko por noveca tritiko</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="amplek.0i">ampleksi</ref>,
      <ref cel="ating.0i.FIG">atingi<sncref/></ref>,
      <ref cel="ten.en0i">enteni</ref>,
      <ref cel="nombr.0i.enteni">nombri<sncref/></ref>,
      <ref cel="rezult.0i">rezulti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>sich <ind>addieren</ind></trd>,
    <trd>in Summe <ind>ergeben</ind></trd>
   </trdgrp>
   <trdgrp lng="pl">
     <trd>liczy&cacute;</trd>,
     <trd>zlicza&cacute;</trd>
   </trdgrp>
</drv>

<drv mrk="sum.0ero">
  <kap><tld/>ero</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 9</lok></fnt>
  <snc>
    <dif>
