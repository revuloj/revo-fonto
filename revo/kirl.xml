<drv mrk="kirl.0ilo">
  <kap><tld/>ilo <fnt><bib>PIV2</bib></fnt></kap>
  <snc mrk="kirl.0ilo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Aparato por <tld/>i ovojn, kremon ktp:
      <ekz>
        tie mi eksponis ovo<tld/>ilon kaj enspezis sufi&ccirc;e por vivteni min
        <fnt>
          <bib>KrM</bib>
          <lok>34. Granda Parolisto Estas Duba Faristo.</lok>
        </fnt>;
      </ekz>
      <ekz>
        estas uzata elektra <tld/>ilo por teni &gcirc;in malfermita, la &scirc;atantoj de vintra
        banado venas al sia glacitruo &ccirc;iutage
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2009/009629.php">Vintra banado</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kremo devas esti le&gcirc;ere batita per &scirc;a&ubreve;migilo a&ubreve; <tld/>ilo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=462081">Batita kremo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>R&uuml;hrger&auml;t</trd>,
      <trd>Mixer</trd>
    </trdgrp>
    <trd lng="en">whisk</trd>
    <trd lng="es">batidora</trd>
    <trdgrp lng="fr">
      <trd>batteur<klr tip="ind"> (cuisine)</klr></trd>,
      <trd>fouet<klr tip="ind"> (cuisine)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>habver&odblac;</trd>,
      <trd>kever&odblac;(eszk&ouml;z)</trd>
    </trdgrp>
    <trd lng="it">sbattitore<klr tip="ind"> (in cucina)</klr></trd>
    <trdgrp lng="nl">
      <trd>garde</trd>,
      <trd>klopper</trd>
    </trdgrp>
    <trd lng="pl">mikser</trd>
    <trd lng="ru">&c_m;&c_i;&c_k;&c_s;&c_je;&c_r;</trd>
    <trd lng="sv">visp</trd>
  </snc>
  <snc mrk="kirl.0ilo.KOMUNE">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Aparato por <tld/>i diversajn materialojn, ekz. sablon kun cemento
      kaj akvo (por ricevi betonon), grenojn ktp.
    </dif>
    <ref tip="vid" cel="miks.beton0ilo">betonmiksilo</ref>
    <trd lng="en">mixer</trd>
    <trd lng="fr">mixeur</trd>
    <trd lng="pl">mieszalnik</trd>
    <trd lng="zh">&#x6405;&#x62CC;&#x673A; <pr>ji&#x1CE;ob&#xE0;nj&#x12B;</pr></trd>
</snc>
<ref tip="vid" cel="miks.0ilo">miksilo</ref>
  <trdgrp lng="ja">
    <trd>&#x6CE1;&#x7ACB;&#x3066;&#x5668;  <pr>&#x3042;&#x308F;&#x3060;&#x3066;&#x304D;</pr></trd>,
    <trd>&#x652A;&#x62CC;&#x6A5F;  <pr>&#x304B;&#x304F;&#x306F;&#x3093;&#x304D;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
