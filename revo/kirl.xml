<drv mrk="kirl.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="kirl.sen0a.KOMUNE">
    <dif>
      Ne havanta <tld/>ojn:
      <ekz>
        la orgeno ricevas por la ludado de malnova muziko la preferatan
        vivan kaj trankvilan (sen<tld/>an) orgenventon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19603">Orgeno</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">unverwirbelt</trd>
    <trd lng="hu">&ouml;rv&eacute;nymentes</trd>
    <trdgrp lng="pl">
      <trd>bezwirowy</trd>,
      <trd>laminarny</trd>
    </trdgrp>
    <trd lng="sv">virvelfri</trd>
  </snc>
  <snc mrk="kirl.sen0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <klr>(p.p. vektora <ref tip="super" cel="kamp.0o.MAT">kampo</ref>)</klr>
      Kies <ref cel="kirl.0o.MAT"><tld/>o <sncref/></ref> egalas al
      nulo:
      <ekz>
        &ccirc;iu sen<tld/>a vektora kampo povas esti esprimita kiel gradiento
        de iu skalara
        kampo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=270041">Gradienta teoremo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="potenc1.0hava">potencialhava.</ref>
    <trd lng="de">wirbelfrei<klr tip="ind"> (-es Vektorfeld)</klr></trd>
    <trd lng="es">irrotacional</trd>
    <trd lng="en">irrotational<klr tip="ind"> (field)</klr></trd>
    <trd lng="fr"><klr tip="ind">(champ) </klr>irrotationnel</trd>
    <trd lng="hu">rot&aacute;ci&oacute;mentes</trd>
    <trd lng="it">irrotazionale</trd>
    <trd lng="pl"><klr tip="amb">(pole) </klr>bezwirowe</trd>
    <trd lng="ru">&c_b;&c_je;&c_z;&c_v;&c_i;&c_h;&c_r;&c_je;&c_v;&c_o;&c_je;<klr tip="amb"> (&c_p;&c_o;&c_l;&c_je;)</klr></trd>
    <trd lng="sv">virvelfri</trd>
  </snc>
</drv>

<drv mrk="kirl.akvo0o">
  <kap>akvo<tld/>o</kap>
  <snc mrk="kirl.akvo0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Loko de akvejo, kie akvo rapide
      <ref tip="super" cel="vortic.0o">vortice</ref>
      rondiras kun ensu&ccirc;a forto;
      <ref tip="sin" cel="kirl.0o.KOMUNE">akvoturno</ref>:
      <ekz>
        ili sukcesis trairi sendama&gcirc;e la kaptilon <klr>[&#x2026;]</klr> kaj el
        la terura akvo<tld/>o
        <fnt>
          <vrk><url ref="http://www.dantepoliglotta.it/eo/homero-la-sippereo-de-uliso-inter-skilo-kaj-karibdo/">Homero</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        multaj ser&ccirc;antoj faltrafas en la akvo<tld/>i&gcirc;on de spirituala materialismo kaj
        psi&hcirc;ismo, precipe kiam la sola provilo estas ilia propra &leftquot;sento&rightquot;
        <fnt>
          <bib>Monato</bib>
          <aut>Christian Declerck</aut>
          <vrk><url ref="&Monato;/2009/009048.php">Spirituala materialismo</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">vodn&iacute; v&iacute;r</trd>
  <trd lng="de">Strudel</trd>
  <trdgrp lng="en">
    <trd>whirlpool</trd>,
    <trd>eddy</trd>
  </trdgrp>
  <trd lng="es">remolino<klr tip="ind"> (de agua)</klr></trd>
