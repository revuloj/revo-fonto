<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: restrikt.xml,v 1.7 2024/04/13 20:15:53 revo Exp $">
<kap>
  <rad>restrikt</rad>/i<fnt> <bib>PIV1</bib></fnt>
</kap>
<drv mrk="restrikt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="restrikt.0i.KOMUNE">
    <dif>
      <ref tip="dif" cel="lim.0igi.FIG">Limigi<sncref/></ref>:
      <ekz>
        regulo de la Fundamento, <ind><tld/>e</ind> difinita dum la prezido de <nom>William
        Auld</nom>
        <fnt>
          <bib>LOdE</bib>
          <aut>Chimedtseren Enkhee</aut>
          <vrk>Akademio de Esperanto: kompleta organigramo</vrk>
        </fnt>;
        <trd lng="de">restriktiv</trd>
      </ekz>
      <ekz>
        <nom>Fidesz</nom> <tld/>is la gazetaran liberecon
        <fnt>
          <aut>I. Ertl</aut>,
          <vrk>Fidesz: de juna elano al duba etapo</vrk>,
	        <bib>Monato</bib>,
          <lok>2017:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        terkultivado kaj minado estas liberigitaj de naciaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Elson B. Snow</aut>
          <vrk><url ref="&Monato;/2003/008146.html">Sekvoj de la pakto pri libera
          merkato</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> rekomendas limigi ilian agadon per voja&gcirc;malpermesoj kaj
        financaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Serge Zandandu Ntomono Zola</aut>
          <vrk><url ref="&Monato;/2003/007982.html">Disrabado de naturaj ri&ccirc;a&jcirc;oj</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>beschr&auml;nken</trd>,
      <trd>begrenzen</trd>,
      <trd>beschneiden</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_m;&c_ja;&c_zh;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_r;&c_a;&c_z;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_p;&c_je;&c_r;&c_a;&c_n;.)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>borner<klr> (fig.)</klr></trd>,
    <trd>restreindre</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>restrykcjonowa&cacute;</trd>,
    <trd>&sacute;cie&sacute;nia&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>restr&acirc;nge</trd>,
    <trd>comprima</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43A;&#x43E;&c_r;&#x43E;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43E;&c_b;&#x43C;&c_je;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5236;&#x7D04; <pr>zh&#xEC;yu&#x113;</pr></trd>,
    <trd>&#x5236;&#x7EA6; <pr>zh&#xEC;yu&#x113;</pr></trd>,
    <trd>&#x6291;&#x6B62; <pr>y&#xEC;zh&#x1D0;</pr></trd>,
    <trd>&#x9650;&#x5236; <pr>xi&#xE0;nzh&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>

