<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: restrikt.xml,v 1.1 2017/06/19 15:10:16 revo Exp $">
<kap>
  <rad>restrikt</rad>/i<fnt> <bib>PIV1</bib></fnt>
</kap>
<drv mrk="restrikt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="restrikt.0i.KOMUNE">
    <dif>
      <ref tip="dif" cel="vast.mal0igi.KOMUNE">Malvastigi</ref>,
      striktigi, redukti:
      <ekz>
        Fidesz <tld/>is la gazetaran liberecon
        <fnt>
          <aut>I. Ertl</aut>,
          <vrk>Fidesz: de juna elano al duba etapo</vrk>,
	        <bib>Monato</bib>,
          <lok>2017:6</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>borner<klr> (fig.)</klr></trd>,
    <trd>restreindre</trd>
  </trdgrp>
</drv>
</art>

