<art mrk="$Id: fondus.xml,v 1.17 2025/03/29 17:52:08 revo Exp $">
<kap>
  <rad>fondus</rad>/o
<fnt><bib>PVEF</bib></fnt></kap>
<drv mrk="fondus.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Kapitalo destinita por aparta, difinita celo:
      <ekz>
        la abonhelpa <tld/>o de MONATO celas subvencii tiujn personojn,
        kiuj deziras aboni la magazinon sed pro ekonomiaj malfacila&jcirc;oj ne
        povas.
      </ekz>
    </dif>
    <refgrp tip="vid">
      Ne konfuzu kun:
      <ref cel="fond.0ajxo">fonda&jcirc;o</ref>.
    </refgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>fonda&#309;o</ke>.
  </rim>

  <trd lng="be">&c_f;&c_o;&c_n;&c_d; <klr tip="amb">(&c_g;&c_r;&c_a;&c_sh;&c_o;&c_v;&c_y;)</klr></trd>
  <trd lng="cs">fond</trd>
  <trd lng="de">Fonds</trd>
  <trd lng="en">fund</trd>
  <trd lng="es">fondo</trd>
  <trd lng="fr">fonds<klr tip="ind"> (argent d&eacute;di&eacute; &agrave; un usage)</klr></trd>
  <trdgrp lng="hu">
    <trd>alap<klr tip="ind"> (p&eacute;nzalap)</klr></trd>,
    <trd>p&eacute;nzalap</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#22522;&#37329;  <pr>&#12365;&#12365;&#12435;</pr></trd>,
    <trd>&#36001;&#22243;  <pr>&#12374;&#12356;&#12384;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">fonds</trd>
  <trd lng="pt">fundo</trd>
  <trd lng="ru">&c_f;&c_o;&c_n;&c_d; <klr tip="amb">(&c_d;&c_je;&c_n;&c_je;&c_zh;&c_n;&c_y;&c_j;)</klr></trd>
  <trdgrp lng="sk">
    <trd>fond</trd>,
    <trd>z&#225;kladina</trd>
  </trdgrp>
  <trd lng="uk">&#1092;&#1086;&#1085;&#1076;</trd>
  <trd lng="zh">&#36039;&#37329; <pr>z&#299;j&#299;n</pr></trd>
</drv>
</art>
