<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: komik.xml,v 1.23 2022/05/01 08:09:24 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>komik</rad>/a
</kap>
<drv mrk="komik.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <ref tip="sin" cel="rid.0iga">Ridiga</ref>, 
      <ref tip="sin" cel="amuz.0a">amuza</ref> pro 
      <ref tip="super" cel="strang.0a">strangeco</ref>:
      <ekz>
        <tld/>a muziko
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i rakontis kelke da <tld/>aj scenoj el la <ref cel="komedi.0o">komedio</ref>j 
        de <nom>Holberg</nom>
        <fnt>
          <bib>Fab2</bib>
          <lok>Malgranda Tuk</lok>
        </fnt>;
      </ekz>
      <ekz>
        la studento <klr>[&#x2026;]</klr> eltran&ccirc;as la <tld/>ajn amuzajn bildojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        komika birdo <klr>[&#x2026;]</klr> la flugiloj<klr>[n]</klr> &gcirc;i tenas <klr>[&#x2026;]</klr>
        senmove kaj tamen rapide flugas
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        serioze imitante la <tld/>an inventon pri eksplodpafiloj de Ariosto
        <fnt>
          <bib>Kandid</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> rigardis la mastrinon per tiel lar&gcirc;e malfermitaj okuloj, kun
        tiel <tld/>a esprimo de plendo, ofendi&gcirc;o kaj mirego, ke Mario ne povis sin
        deteni de preska&ubreve; la&ubreve;ta rido
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bizar.0a">bizara</ref>,
      <ref cel="grotes.0a.KOMUNE">groteska<sncref/></ref>,
      <ref cel="komedi.0a">komedia</ref>,
      <ref cel="kurioz.0a">kurioza</ref>,
      <ref cel="rid.0inda">ridinda</ref>,
      <ref cel="skuril.0a">skurila</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_m;&c_ib;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_mol;&c_m;&c_je;&c_sh;&c_n;&c_y;</trd>,
    <trd>&c_f;&c_a;&c_c;&c_e;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>komick&yacute;</trd>,
    <trd>legra&ccaron;n&iacute;</trd>,
    <trd>sm&ecaron;&scaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="de">komisch</trd>
  <trdgrp lng="en">
    <trd>comic</trd>,
    <trd>comical</trd>,
    <trd>droll</trd>,
    <trd>facetious</trd>,
    <trd>humorous</trd>,
    <trd>laughable</trd>,
    <trd>ludicrous</trd>,
    <trd>mirth-provoking</trd>,
    <trd>witty</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>comique</trd>,
    <trd>amusant</trd>,
    <trd>cocasse</trd>,
    <trd>dr&ocirc;le<klr tip="ind"> (adj.)</klr></trd>,
    <trd>marrant</trd>,
    <trd>plaisant<klr tip="ind"> (amusant)</klr></trd>,
    <trd>rigolo<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>komikus</trd>,
    <trd>mulats&aacute;gos</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>komisch</trd>,
    <trd>grappig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>komiczny</trd>,
    <trd>zabawny</trd>,
    <trd>&sacute;mieszny</trd>
  </trdgrp>
  <trd lng="pt">c&ocirc;mico</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_k;&c_o;&c_m;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_m;&c_je;&c_sh;&c_n;&c_o;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_b;&c_a;&c_v;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>komick&yacute;</trd>,
    <trd>smie&scaron;ny</trd>
  </trdgrp>
  <trd lng="sv">komisk</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&#x43C;&#x456;&#x447;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x441;&#x43C;&#x456;&#x448;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x437;&#x430;&#x431;&#x430;&#x432;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="komik.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Eco de <tld/>a persono a&ubreve; situacio:
      <ekz>
        li dankis lin pro la tridek, kaj ridis pri la <tld/>eco en almozoredono
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi kunridis, &ccirc;ar nur nun mi ekvidis retrospekte la <tld/>econ de la okazinta&jcirc;o
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>eco rezultas plej ofte el la karakteroj de la protagonistoj
        <fnt>
          <bib>Monato</bib>
          <aut>Luiza Carol</aut>
          <vrk><url ref="&Monato;/2013/010892.php">Spegula mondo</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_m;&c_ib;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;
    </trd>,
    <trd>&c_k;&c_a;&c_m;&c_ib;&c_z;&c_m;</trd>
  </trdgrp>
  <trd lng="ca">c&ograve;mic</trd>
  <trdgrp lng="cs">
    <trd>komi&ccaron;no</trd>,
    <trd>komi&ccaron;nost</trd>
  </trdgrp>
  <trd lng="de">Komik</trd>
  <trdgrp lng="en">
    <trd>comedy</trd>,
    <trd>comicality</trd>,
    <trd>comicalness</trd>,
    <trd>drollery</trd>,
    <trd>drollness</trd>,
    <trd>funniness</trd>,
    <trd>humoursness</trd>,
    <trd>wittiness</trd>
  </trdgrp>
  <trd lng="es">c&oacute;mico</trd>
  <trdgrp lng="fr">
    <trd>comique<klr tip="ind"> (de qc)</klr></trd>,
    <trd>cocasserie</trd>,
    <trd>dr&ocirc;lerie</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>komikuss&aacute;g</trd>,
    <trd>mulats&aacute;goss&aacute;g</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>het <ind>grappig</ind>e</trd>,
    <trd>het <ind>komisch</ind>e</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>komizm</trd>,
    <trd>komiczno&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_i;&c_ch;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_o;&c_m;&c_i;&c_z;&c_m;</trd>,
    <trd>&c_z;&c_a;&c_b;&c_a;&c_v;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>komickos&tcaron;</trd>,
    <trd>smie&scaron;nos&tcaron;</trd>,
    <trd>&zcaron;artovnos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&#x43C;&#x456;&#x437;&#x43C;</trd>,
    <trd>&#x43A;&#x43E;&#x43C;&#x456;&#x447;&#x43D;&#x456;&#x441;&#x442;&#x44C;</trd>,
    <trd>&#x437;&#x430;&#x431;&#x430;&#x432;&#x43D;&#x456;&#x441;&#x442;&#x44C;</trd>
  </trdgrp>
</drv>
</art>
