<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>komik</rad>/a
</kap>
<drv mrk="komik.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Ridiga, amuza:
      <ekz><tld/>a aventuro, gesto, aspekto, situacio;</ekz>
      <ekz>li <tld/>e tiris sian &ccirc;apelon.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="komedi.0a">komedia</ref>,
      <ref cel="rid">ridinda</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="en">
    <trd>comic</trd>,
    <trd>comical</trd>,
    <trd>droll</trd>,
    <trd>facetious</trd>,
    <trd>humorous</trd>,
    <trd>laughable</trd>,
    <trd>ludicrous</trd>,
    <trd>mirth-provoking</trd>,
    <trd>witty</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_k;&c_o;&c_m;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_m;&c_je;&c_sh;&c_n;&c_o;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_b;&c_a;&c_v;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">komisk</trd>
</drv>
<drv mrk="komik.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Eco de <tld/>a persono a&ubreve; situacio:
      <ekz>la <tld/>eco de la aventuro estas, ke...</ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>comedy</trd>,
    <trd>comicality</trd>,
    <trd>comicalness</trd>,
    <trd>drollery</trd>,
    <trd>drollness</trd>,
    <trd>funniness</trd>,
    <trd>humoursness</trd>,
    <trd>wittiness</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_i;&c_ch;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_o;&c_m;&c_i;&c_z;&c_m;</trd>,
    <trd>&c_z;&c_a;&c_b;&c_a;&c_v;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
