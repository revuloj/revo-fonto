<art mrk="$Id: sociol.xml,v 1.25 2024/03/25 06:40:51 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>sociolog</rad>/o
</kap>
<drv mrk="sociol.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="sociol1.0isto">Sociologiisto</ref>:
      <ekz>
        diplomita kaj profesia <tld/>o, profesia esperantisto, kiu havas
        eblon studi la Esperanto-socion de interne
        <fnt><aut>Boris Kolker</aut>: 
        <vrk>Oni ne kulpigu la spegulon (Nikola Rasic, La
        rondo familia)</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero 2a,
        p. 22a</lok></fnt>.
      </ekz>
      <ekz>
        multaj <tld/>oj dubas, ke tio estas nur fascino je iuj fremda&jcirc;oj
       <fnt>
          <aut>Sylvia Brylska</aut>
          <vrk><url ref="&Monato;/1993/A930801.php">&Ccirc;u reiri al naciaj
          radikoj?</url></vrk>
          <bib>Monato</bib>
          <lok>1993/08, p. 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj, psikologoj kaj aliaj fakuloj opinias, ke pli bone estus dialogi kun la
        kontra&ubreve;uloj de la kunsido ol batali kontra&ubreve; ili
        <fnt>
          <aut>jpa</aut>
            <vrk><url ref="&Monato;/2000/007093.php">Internacia kunsido de financistoj en
            Prago</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 9</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_c;&c_y;&c_jo;&c_l;&c_ja;&c_g;</trd>
  <trd lng="bg">&c_s;&c_o;&c_c;&c_i;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="ca">soci&ograve;leg</trd>
  <trd lng="cs">sociolog</trd>
  <trd lng="de">Soziologe</trd>
  <trd lng="en">sociologist</trd>
  <trd lng="es">soci&oacute;logo</trd>
  <trd lng="fa">&a_j;&a_A;&a_m;&a_ayn;&a_h;&#x200C;&a_sh;&a_n;&a_A;&a_s;</trd>
  <trdgrp lng="fr">
    <trd>sociologue</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szociol&oacute;gus</trd>,
    <trd>t&aacute;rsadalomtud&oacute;s</trd>
  </trdgrp>
  <trd lng="ja">&#x793E;&#x4F1A;&#x5B66;&#x8005;  <pr>&#x3057;&#x3083;&#x304B;&#x3044;&#x304C;&#x304F;&#x3057;&#x3083;</pr></trd>
  <trd lng="nl">socioloog</trd>
  <trd lng="pl">socjolog</trd>
  <trd lng="ru">&c_s;&c_o;&c_c;&c_i;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="sk">sociol&oacute;g</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43E;&c_c;&c_ib;&#x43E;&#x43B;&#x43E;&c_g;</trd>,
    <trd>&c_s;&c_u;&c_s;&#x43F;&c_ib;&#x43B;&#x44C;&c_s;&c_t;&c_v;&#x43E;&c_z;&#x43D;&c_a;&c_v;&c_je;&c_c;&#x44C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x793E;&#x4F1A;&#x5B66;&#x5BB6; <pr>sh&#xE8;hu&#xEC;xu&#xE9;ji&#x101;</pr></trd>,
    <trd>&#x793E;&#x6703;&#x5B78;&#x5BB6; <pr>sh&#xE8;hu&#xEC;xu&#xE9;ji&#x101;</pr></trd>
  </trdgrp>
</drv>
