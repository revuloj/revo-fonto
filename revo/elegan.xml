<drv mrk="elegan.0a">
  <kap><tld/>a</kap>
  <snc mrk="elegan.0a.KOMUNE">
    <dif>
      Gustoplene distinginda:
    </dif>
    <subsnc>
      <dif>
        la&ubreve; ekstera formo:
        <ekz>
          li portis <tld/>ajn botojn kaj &ccirc;apelon
          <fnt>
            <bib>Fab2</bib>
            <lok>La ombro</lok>
          </fnt>;
        </ekz>
        <ekz>
          anstata&ubreve; <tld/>a mantelo estos sako, anstata&ubreve; beleco estos brulmakuloj
          <fnt>
            <bib>MT</bib>
            <lok>Jesaja 3:24</lok>
          </fnt>;
        </ekz>
        <ekz>
          &ccirc;iuj aliaj domoj de la strato estis novaj kaj <tld/>aj, kun grandaj fenestraj
          vitroj kaj glataj muroj
          <fnt>
            <bib>Fab2</bib>
            <lok>Malnova domo</lok>
          </fnt>;
        </ekz>
        <ekz>
          <klr>[mi]</klr> nur rigardas la silueton de &ccirc;i tiu palaco kun linioj tiel
          harmoniaj kaj <tld/>aj
          <fnt>
            <bib>IK</bib>
            <lok>&Ccirc;apitro I</lok>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="gust.bon0a.estetika">bongusta<sncref/></ref>,
        <ref cel="luks.0a">luksa</ref>,
        <ref cel="pimp.0a">pimpa</ref>,
        <ref cel="sxik.0a">&scirc;ika</ref>
      </refgrp>
    </subsnc>
    <subsnc>
      <dif>
        la&ubreve; maniero, la&ubreve; konduto, la&ubreve; sintenado:
        <ekz>
          <tld/>a (simpla kaj klara) demonstro;
        </ekz>
        <ekz>
          li povis bone observadi la malgrandan <tld/>an sinjorinon, kiu senlace staris
          sur unu piedo, ne perdante la ekvilibron
          <fnt>
            <bib>Fab1</bib>
            <lok>Persista stana soldato</lok>
          </fnt>;
        </ekz>
        <ekz>
          &gcirc;i denove ekkantis per sia dol&ccirc;a, ensor&ccirc;anta vo&ccirc;o, &ccirc;i tio estas la plej <tld/>a
          maniero, por akiri al si favoron
          <fnt>
            <bib>Fab2</bib>
            <lok>La najtingalo</lok>
          </fnt>;
        </ekz>
        <ekz>
          <klr>[li]</klr> faris anta&ubreve; la generalo de la malgranda armeo bulgaran
          ekzercadon kun tiom da <tld/>a facilo, da rapido, da lerto, da fiero, da
          facilmoveco, ke oni donis al li por komandi infanterian roton
          <fnt>
            <bib>Kandid</bib>
            <lok>&Ccirc;apitro X</lok>
          </fnt>;
        </ekz>
        <ekz>
          virino ankora&ubreve; juna, tre bela, tre bele vestita, <klr>[&#8230;]</klr> kun viza&gcirc;o
          esprimanta grandan inteligentecon, kun parolado rapida, vigla kaj <tld/>a
          <fnt>
            <bib>Marta</bib>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="cxarm.0a">&ccirc;arma</ref>,
        <ref cel="distin.0inda">distinginda</ref>,
        <ref cel="nobl.0a.iu">nobla<sncref/></ref>,
        <ref cel="rafin.0ita">rafinita</ref>,
        <ref cel="pen.sen0e">senpena</ref>,
        <ref cel="sublim.0a">sublima</ref>
      </refgrp>
    </subsnc>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_l;&c_je;&c_g;&c_a;&c_n;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_g;&c_r;&c_a;&c_b;&c_n;&c_y;</trd>,
    <trd>&c_v;&c_y;&c_t;&c_a;&c_n;&c_ch;&c_a;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_je;&c_l;&c_je;&c_g;&c_a;&c_n;&c_t;&c_je;&c_n;</trd>
  <trdgrp lng="ca">
    <trd>refinat</trd>,
    <trd>elegant<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>elegantn&iacute;</trd>,
