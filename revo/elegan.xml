<drv mrk="elegan.0a">
<kap><tld/>a</kap>
<snc>
<dif>
Gustoplene distinginda: <ekz><tld/>a homo, vesto, mantelo stilo, verkisto;
</ekz> <ekz><tld/>a (simpla k klara) demonstracio.
</ekz>
</dif>
</snc>
<trdgrp lng="de">
   <trd>elegant</trd>,
   <trd>vornehm</trd></trdgrp>
<trd lng="es">elegante</trd>
<trd lng="fr">&eacute;l&eacute;gant<klr tip="ind"> (adj.)</klr></trd>
<trdgrp lng="ru">
  <trd>&c_e;&c_l;&c_je;&c_g;&c_a;&c_n;&c_t;&c_n;&c_y;&c_j;</trd>,
  <trd>&c_i;&c_z;&c_ja;&c_shch;&c_n;&c_y;&c_j;</trd>
</trdgrp>
</drv>
<drv mrk="elegan.0ulo">
<kap><tld/>ulo</kap>
<snc>
<dif>
Homo <tld/>a.
</dif>
<refgrp tip="vid">
<ref cel="dand.0o">dando</ref>.
</refgrp>
</snc>
  <trd lng="es">catr&iacute;n</trd>
  <trd lng="fr">&eacute;l&eacute;gant<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="de">
     <trd>eleganter Herr</trd>,
     <trd>elegante Dame</trd>,
     <trd>Person von Geschmack</trd></trdgrp>
</drv>
</art>
