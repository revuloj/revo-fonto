  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_je;&c_l;&c_je;&c_gu;&c_a;&c_n;&c_t;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_i;&c_t;&c_o;&c_n;&c_ch;&c_je;&c_n;&c_i;&c_j;</trd>
