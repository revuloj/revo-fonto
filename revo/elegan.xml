    <trd>&#36259;&#21619;&#12398;&#12424;&#12356;  <pr>&#12375;&#12421;&#12415;&#12398;&#12356;&#12356;</pr></trd>,
    <trd>&#20778;&#32654;&#12394;  <pr>&#12422;&#12358;&#12403;&#12394;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
