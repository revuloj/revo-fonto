<kap>
  <ofc>1</ofc>
  <rad>elegant</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="elegan.0a">
  <kap><tld/>a</kap>
  <snc mrk="elegan.0a.KOMUNE">
    <dif>
      Gustoplene distinginda:
      <ekz>
        <tld/>a homo, vesto, mantelo stilo, verkisto;
      </ekz>
      <ekz>
        <tld/>a (simpla kaj klara) demonstro.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pimp.0a">pimpa</ref>,
      <ref cel="sxik.0a">&scirc;ika</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_l;&c_je;&c_g;&c_a;&c_n;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_g;&c_r;&c_a;&c_b;&c_n;&c_y;</trd>,
    <trd>&c_v;&c_y;&c_t;&c_a;&c_n;&c_ch;&c_a;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_je;&c_l;&c_je;&c_g;&c_a;&c_n;&c_t;&c_je;&c_n;</trd>
  <trdgrp lng="ca">
    <trd>refinat</trd>,
    <trd>elegant<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>elegantn&iacute;</trd>,
    <trd>&uacute;pravn&yacute; <klr>(&ccaron;l.)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>elegant</trd>,
    <trd>vornehm</trd>
  </trdgrp>
  <trd lng="en">elegant</trd>
  <trd lng="es">elegante</trd>
  <trd lng="fr">&eacute;l&eacute;gant<klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>eleg&aacute;ns</trd>,
    <trd>v&aacute;laszt&eacute;kos</trd>,
    <trd>csinos</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4E0A;&#x54C1;&#x306A;  [<ind>&#x3058;&#x3087;&#x3046;&#x3072;&#x3093;&#x306A;</ind>]</trd>,
    <trd>&#x8DA3;&#x5473;&#x306E;&#x3088;&#x3044;  [<ind>&#x3057;&#x3085;&#x307F;&#x306E;&#x3044;&#x3044;</ind>]</trd>,
    <trd>&#x512A;&#x7F8E;&#x306A;  [<ind>&#x3086;&#x3046;&#x3073;&#x306A;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>elegancki</trd>,
    <trd>wytworny</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>elegante</trd>,
    <trd>gracioso</trd>,
    <trd>fino</trd>,
    <trd>gentil</trd>,
    <trd>airoso</trd>,
    <trd>garboso</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_l;&c_je;&c_g;&c_a;&c_n;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_i;&c_z;&c_ja;&c_shch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>elegantn&yacute;</trd>,
    <trd>vkusn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x435;&#x43B;&#x435;&#x491;&#x430;&#x43D;&#x442;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x432;&#x438;&#x442;&#x43E;&#x43D;&#x447;&#x435;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="elegan.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="elegan.0eco.KOMUNE">
    <dif>
      <tld lit="E"/>a aspekto, kiu &ccirc;armas:
      <ekz>
        en matematika pruvo <klr>...</klr> <tld/>eco estas 
        la minimuma kvanto da manipuloj por atingi la 
        solvon kun maksimuma klareco
        <fnt>
          <aut>S. Cobb</aut>
          <vrk>Elegantaj Solvoj</vrk>
          <lok>[2003]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_l;&c_je;&c_g;&c_a;&c_n;&c_t;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_t;&c_a;&c_n;&c_ch;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>elegance</trd>,
    <trd>uhlazenost</trd>
  </trdgrp>
  <trd lng="fr">&eacute;l&eacute;gance</trd>
  <trdgrp lng="ja">
    <trd>&#x6C17;&#x54C1;  [<ind>&#x304D;&#x3072;&#x3093;</ind>]</trd>,
    <trd>&#x4E0A;&#x54C1;&#x3055;  [<ind>&#x3058;&#x3087;&#x3046;&#x3072;&#x3093;&#x3055;</ind>]</trd>,
    <trd>&#x512A;&#x96C5;&#x3055;  [<ind>&#x3086;&#x3046;&#x304C;&#x3055;</ind>]</trd>,
    <trd>&#x512A;&#x7F8E;&#x3055;  [<ind>&#x3086;&#x3046;&#x3073;&#x3055;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>elegancia</trd>,
    <trd>vyberan&yacute; vkus</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x435;&#x43B;&#x435;&#x491;&#x430;&#x43D;&#x442;&#x43D;&#x456;&#x441;&#x442;&#x44C;</trd>,
    <trd>&#x432;&#x438;&#x442;&#x43E;&#x43D;&#x447;&#x435;&#x43D;&#x456;&#x441;&#x442;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="elegan.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="elegan.0igi.KOMUNE">
    <dif>
      Igi <tld/>a, pla&ccirc;a:
      <ekz>
        ili provis <klr>...</klr> sin pli <tld/>igi, 
        razigante siajn vilajn mentonojn
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>
          <vrk>Montara Vila&gcirc;o</vrk>
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; &c_e;&c_l;&c_je;&c_g;&c_a;&c_n;&c_t;&c_n;&c_y;&c_m;</trd>
  <trd lng="fr">rendre &eacute;l&eacute;gant</trd>
</drv>

<drv mrk="elegan.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="elegan.0ulo.MOR">
    <dif>
      Homo <tld/>a:
      <ekz>
        la simplaj manieroj de la <tld/>ulo <nom>Svan</nom> estis eble
        de lia flanko nur pli rafinita formo de vanteco
        <fnt>
          <aut>M. Proust, trad. &Jcirc; Va&scirc;e</aut>
          <vrk><url ref="http://jxvasxe.free.fr/tradukoj/en-ombro-de-florantaj-junulinoj-eo.htm">
          En Ombro de Florantaj Junulinoj</url></vrk>
          <lok>2017-</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dand.0o">dando</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_g;&c_a;&c_n;&c_t;&c_n;&c_y; &c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;</trd>
  <trdgrp lng="ca">
    <trd>dandi</trd>,
    <trd>elegant<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="cs">eleg&aacute;n</trd>
  <trdgrp lng="de">
    <trd>eleganter Herr</trd>,
    <trd>elegante Dame</trd>,
    <trd>Person von Geschmack</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>elegant person</trd>,
  </trdgrp>
  <trd lng="es">catr&iacute;n</trd>
  <trd lng="fr">&eacute;l&eacute;gant<klr> (subst.)</klr></trd>
  <trd lng="he">&#x5D0;&#x5B5;&#x5DC;&#x5B6;&#x5D2;&#x5B7;&#x5E0;&#x5D8;&#x5B4;&#x5D9;</trd>
  <trd lng="hu">eleg&aacute;ns ember</trd>
  <trd lng="ja">&#x4E0A;&#x54C1;&#x306A;&#x4EBA;  [<ind>&#x3058;&#x3087;&#x3046;&#x3072;&#x3093;&#x306A;&#x3058;&#x3093;</ind>]</trd>
  <trdgrp lng="pl">
    <trd>elegant</trd>,
    <trd>fircyk</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_l;&c_je;&c_g;&c_a;&c_n;&c_t;&c_n;&c_y;&c_j; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>
  <trd lng="sk">eleg&aacute;n</trd>
  <trdgrp lng="uk">
    <trd>&#x435;&#x43B;&#x435;&#x491;&#x430;&#x43D;&#x442;</trd>,
    <trd>&#x447;&#x435;&#x43F;&#x443;&#x440;&#x443;&#x43D;</trd>
  </trdgrp>
</drv>

<drv mrk="elegan.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="elegan.mal0a.KOMUNE">
    <dif>
      Ne <tld/>a,
      <ref tip="vid" cel="plump.0a">plumpa</ref>:
      <ekz>
        &scirc;tal&ccirc;apaj botetoj, kiajn portas konstruistoj, estas 
        kutime grandaj, fortikaj, mal<tld/>aj piedvesta&jcirc;oj
        <fnt>
          <aut>I&ccirc;o</aut>,
          <vrk><url ref="http://th.lernu.net/komunikado/forumo/temo.php?t=5236">
          Botetegoj?</url></vrk>,
          <lok>lernu.net, 2009-10-10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_e;&c_l;&c_je;&c_g;&c_a;&c_n;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_z;&c_g;&c_r;&c_a;&c_b;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">nevkusn&yacute;</trd>
  <trd lng="fr">in&eacute;l&eacute;gant</trd>
  <trdgrp lng="ja">
    <trd>&#x4E0B;&#x54C1;&#x306A;  [<ind>&#x3052;&#x3072;&#x3093;&#x306A;</ind>]</trd>,
    <trd>&#x6C17;&#x54C1;&#x306B;&#x6B20;&#x3051;&#x308B;  [<ind>&#x304D;&#x3072;&#x3093;&#x306B;&#x304B;&#x3051;&#x308B;</ind>]</trd>,
    <trd>&#x4E0D;&#x7C8B;&#x306A;  [<ind>&#x3075;&#x3044;&#x304D;&#x306A;</ind>]</trd>,
    <trd>&#x91CE;&#x66AE;&#x306A;  [<ind>&#x3084;&#x307C;&#x306A;</ind>]</trd>,
    <trd>&#x3076;&#x3056;&#x307E;&#x306A;</trd>
  </trdgrp>
  <trd lng="sk">nevkusn&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x43D;&#x435;&#x435;&#x43B;&#x435;&#x491;&#x430;&#x43D;&#x442;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x43F;&#x43E;&#x437;&#x431;&#x430;&#x432;&#x43B;&#x435;&#x43D;&#x438;&#x439; &#x441;&#x43C;&#x430;&#x43A;&#x443;</trd>,
    <trd>&#x431;&#x435;&#x437; &#x441;&#x43C;&#x430;&#x43A;&#x443;</trd>,
    <trd>&#x437;&#x440;&#x43E;&#x431;&#x43B;&#x435;&#x43D;&#x438;&#x439; &#x431;&#x435;&#x437; &#x441;&#x43C;&#x430;&#x43A;&#x443;</trd>
  </trdgrp>
</drv>
</art>
