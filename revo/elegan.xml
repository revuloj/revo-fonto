<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>elegant</rad>/a
</kap>
<drv mrk="elegan.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Gustoplene distinginda:
      <ekz>
        <tld/>a homo, vesto, mantelo stilo, verkisto;
      </ekz>
      <ekz>
        <tld/>a (simpla kaj klara) demonstracio.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pimp.0a">pimpa</ref>,
      <ref cel="sxik.0a">&scirc;ika</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_l;&c_je;&c_g;&c_a;&c_n;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_g;&c_r;&c_a;&c_b;&c_n;&c_y;</trd>,
    <trd>&c_v;&c_y;&c_t;&c_a;&c_n;&c_ch;&c_a;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_je;&c_l;&c_je;&c_g;&c_a;&c_n;&c_t;&c_je;&c_n;</trd>
  <trdgrp lng="de">
    <trd>elegant</trd>,
    <trd>vornehm</trd>
  </trdgrp>
  <trd lng="en">elegant</trd>
  <trd lng="es">elegante</trd>
  <trd lng="fr">&eacute;l&eacute;gant<klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>eleg&aacute;ns</trd>,
    <trd>v&aacute;laszt&eacute;kos</trd>,
    <trd>csinos</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>elegancki</trd>,
    <trd>wytworny</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>elegante</trd>,
    <trd>gracioso</trd>,
    <trd>fino</trd>,
    <trd>gentil</trd>,
    <trd>airoso</trd>,
    <trd>garboso</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_l;&c_je;&c_g;&c_a;&c_n;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_i;&c_z;&c_ja;&c_shch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="elegan.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Homo <tld/>a.
    </dif>
    <refgrp tip="vid">
      <ref cel="dand.0o">dando</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_g;&c_a;&c_n;&c_t;&c_n;&c_y; &c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;</trd>
  <trdgrp lng="de">
    <trd>eleganter Herr</trd>,
    <trd>elegante Dame</trd>,
    <trd>Person von Geschmack</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>elegant person</trd>,
  </trdgrp>
  <trd lng="es">catr&iacute;n</trd>
  <trd lng="fr">&eacute;l&eacute;gant<klr tip="ind">
  (subst.)</klr></trd>
  <trd lng="hu">eleg&aacute;ns ember</trd>
  <trdgrp lng="pl">
    <trd>elegant</trd>,
    <trd>fircyk</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_l;&c_je;&c_g;&c_a;&c_n;&c_t;&c_n;&c_y;&c_j; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>
</drv>
</art>
