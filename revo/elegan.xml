    <trd>&#29808;&#20029; <pr>gu&imacron;l&igrave;</pr></trd>,
    <trd>&#26102;&#26032; <pr>sh&iacute;x&imacron;n</pr></trd>,
    <trd>&#38289;&#38597; <pr>xi&aacute;ny&#462;</pr></trd>,
    <trd>&#32745;&#32745; <pr>pi&amacron;npi&amacron;n</pr></trd>,
    <trd>&#20426;&#38597; <pr>j&ugrave;ny&#462;</pr></trd>,
    <trd>&#39108;&#36920; <pr>pi&amacron;oy&igrave;</pr></trd>,
    <trd>&#20986;&#36523;&#39640;&#36020; <pr>ch&umacron;sh&emacron;ng&amacron;ogu&igrave;</pr></trd>,
    <trd>&#38597;&#33268; <pr>y&#462;zh&igrave;</pr></trd>,
    <trd>&#24069;&#27668; <pr>shu&agrave;iq&igrave;</pr></trd>,
    <trd>&#28418;&#20142;&#22320; <pr>pi&agrave;oliangde</pr></trd>,
    <trd>&#38597; <pr>y&#462;</pr></trd>,
    <trd>&#37057; <pr>y&ugrave;</pr></trd>,
    <trd>&#26000; <pr>f&ecaron;i</pr></trd>,
    <trd>&#20248;&#38597; <pr>y&omacron;uy&#462;</pr></trd>,
    <trd>&#24069; <pr>shu&agrave;i</pr></trd>,
    <trd>&#31168; <pr>xi&ugrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="elegan.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="elegan.0eco.KOMUNE">
    <dif>
      <tld lit="E"/>a aspekto, kiu &ccirc;armas:
      <ekz>
        la saloneto <klr>[&#8230;]</klr> ne estis vasta, tial des pli frapis la okulojn la
        <tld/>eco de &gcirc;ia aran&gcirc;o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        Klaro &ccirc;iam sentis lian superecon en la instruiteco kaj <tld/>eco
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        en matematika pruvo <klr>[&#8230;]</klr> <tld/>eco estas
        la minimuma kvanto da manipuloj por atingi la
        solvon kun maksimuma klareco
        <fnt>
          <aut>S. Cobb</aut>
          <vrk>Elegantaj Solvoj</vrk>
          <lok>[2003]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_l;&c_je;&c_g;&c_a;&c_n;&c_t;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_t;&c_a;&c_n;&c_ch;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>elegance</trd>,
    <trd>uhlazenost</trd>
  </trdgrp>
  <trd lng="de">Eleganz</trd>
  <trd lng="fr">&eacute;l&eacute;gance</trd>
  <trdgrp lng="ja">
    <trd>&#27671;&#21697;  <pr>&#12365;&#12402;&#12435;</pr></trd>,
    <trd>&#19978;&#21697;&#12373;  <pr>&#12376;&#12423;&#12358;&#12402;&#12435;&#12373;</pr></trd>,
    <trd>&#20778;&#38597;&#12373;  <pr>&#12422;&#12358;&#12364;&#12373;</pr></trd>,
    <trd>&#20778;&#32654;&#12373;  <pr>&#12422;&#12358;&#12403;&#12373;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>elegancia</trd>,
    <trd>vyberan&yacute; vkus</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_je;&c_l;&c_je;&c_gu;&c_a;&c_n;&c_t;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_i;&c_t;&c_o;&c_n;&c_ch;&c_je;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="elegan.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="elegan.0igi.KOMUNE">
    <dif>
      Igi <tld/>a, pla&ccirc;a:
      <ekz>
        ili provis <klr>[&#8230;]</klr> sin pli <tld/>igi,
        razigante siajn vilajn mentonojn
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>
          <vrk>Montara Vila&gcirc;o</vrk>
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; &c_e;&c_l;&c_je;&c_g;&c_a;&c_n;&c_t;&c_n;&c_y;&c_m;</trd>
  <trd lng="de"><ind>Eleganz</ind> verleihen</trd>
  <trd lng="fr">rendre &eacute;l&eacute;gant</trd>
</drv>

<drv mrk="elegan.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="elegan.0ulo.MOR">
    <dif>
      Homo <tld/>a:
      <ekz>
        li prezentis al li grupon de junaj <tld/>uloj, parfumitaj, kolorigitaj, en
        vestoj, broditaj per oro
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        la simplaj manieroj de la <tld/>ulo <nom>Svan</nom> estis eble
        de lia flanko nur pli rafinita formo de vanteco
        <fnt>
          <aut>M. Proust, trad. &Jcirc; Va&scirc;e</aut>
          <vrk><url ref="http://jxvasxe.free.fr/tradukoj/en-ombro-de-florantaj-junulinoj-eo.htm">
          En Ombro de Florantaj Junulinoj</url></vrk>
          <lok>2017-</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dand.0o">dando</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_g;&c_a;&c_n;&c_t;&c_n;&c_y; &c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;</trd>
  <trdgrp lng="ca">
    <trd>dandi</trd>,
    <trd>elegant<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="cs">eleg&aacute;n</trd>
  <trdgrp lng="de">
    <trd>eleganter Herr</trd>,
    <trd>elegante Dame</trd>,
    <trd>Person von Geschmack</trd>,
    <trd>vornehmer Mensch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>elegant person</trd>,
  </trdgrp>
  <trd lng="es">catr&iacute;n</trd>
  <trd lng="fr">&eacute;l&eacute;gant<klr> (subst.)</klr></trd>
  <trd lng="he">&alef;&#1461;&lamed;&#1462;&gimel;&#1463;&nun;&tet;&#1460;&yod;</trd>
  <trd lng="hu">eleg&aacute;ns ember</trd>
  <trd lng="ja">&#19978;&#21697;&#12394;&#20154;  <pr>&#12376;&#12423;&#12358;&#12402;&#12435;&#12394;&#12376;&#12435;</pr></trd>
  <trdgrp lng="pl">
    <trd>elegant</trd>,
    <trd>fircyk</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_l;&c_je;&c_g;&c_a;&c_n;&c_t;&c_n;&c_y;&c_j; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>
  <trd lng="sk">eleg&aacute;n</trd>
  <trdgrp lng="uk">
    <trd>&c_je;&c_l;&c_je;&c_gu;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_ch;&c_je;&c_p;&c_u;&c_r;&c_u;&c_n;</trd>
  </trdgrp>
</drv>

<drv mrk="elegan.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="elegan.mal0a.KOMUNE">
    <dif>
      Ne <tld/>a,
      <ref tip="vid" cel="plump.0a">plumpa</ref>:
      <ekz>
        &scirc;tal&ccirc;apaj botetoj, kiajn portas konstruistoj, estas
        kutime grandaj, fortikaj, mal<tld/>aj piedvesta&jcirc;oj
        <fnt>
          <aut>I&ccirc;o</aut>,
          <vrk><url ref="http://th.lernu.net/komunikado/forumo/temo.php?t=5236">
          Botetegoj?</url></vrk>,
          <lok>lernu.net, 2009-10-10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_e;&c_l;&c_je;&c_g;&c_a;&c_n;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_z;&c_g;&c_r;&c_a;&c_b;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">nevkusn&yacute;</trd>
  <trdgrp lng="de">
    <trd>plump</trd>,
    <trd>krude</trd>
  </trdgrp>
  <trd lng="fr">in&eacute;l&eacute;gant</trd>
  <trdgrp lng="ja">
    <trd>&#19979;&#21697;&#12394;  <pr>&#12370;&#12402;&#12435;&#12394;</pr></trd>,
    <trd>&#27671;&#21697;&#12395;&#27424;&#12369;&#12427;  <pr>&#12365;&#12402;&#12435;&#12395;&#12363;&#12369;&#12427;</pr></trd>,
    <trd>&#19981;&#31883;&#12394;  <pr>&#12405;&#12356;&#12365;&#12394;</pr></trd>,
    <trd>&#37326;&#26286;&#12394;  <pr>&#12420;&#12412;&#12394;</pr></trd>,
    <trd>&#12406;&#12374;&#12414;&#12394;</trd>
  </trdgrp>
  <trd lng="sk">nevkusn&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_n;&c_je;&c_je;&c_l;&c_je;&c_gu;&c_a;&c_n;&c_t;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_z;&c_b;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_j; &c_s;&c_m;&c_a;&c_k;&c_u;</trd>,
    <trd>&c_b;&c_je;&c_z; &c_s;&c_m;&c_a;&c_k;&c_u;</trd>,
    <trd>&c_z;&c_r;&c_o;&c_b;&c_l;&c_je;&c_n;&c_i;&c_j; &c_b;&c_je;&c_z; &c_s;&c_m;&c_a;&c_k;&c_u;</trd>
  </trdgrp>
</drv>
