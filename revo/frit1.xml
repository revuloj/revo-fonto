<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: frit1.xml,v 1.4 2022/04/27 17:19:11 afido Exp $">
<kap>
    <rad>frit</rad>/o <fnt><bib>HejmVort</bib></fnt>
</kap>
<drv mrk="frit1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Bastoneto tran&ccirc;ita el terpomo kaj fritita:
      <ekz>
        en Belgio <tld/>oj estas frititaj dufoje anta&ubreve; surtabligo;
      </ekz>
      <ekz>
        li man&gcirc;is kun geamikoj <klr>[&#8230;]</klr> mitulojn kun <tld/>oj, akompane de vino
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2004/008353.html"
              >Plensukcesa oma&gcirc;o al Jacques Brel</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="ter.0pomfingro">terpomfingro</ref>
  </snc>
  <ref tip="hom" cel="frit.0o">frito (ago)</ref>
  <trdgrp lng="be">
    <trd>&c_s;&c_m;&c_a;&c_zh;&c_a;&c_n;&c_y;&c_ja; &c_b;&c_u;&c_l;&c_mol;&c_b;&c_a;</trd>,
    <trd>&c_b;&c_u;&c_l;&c_mol;&c_b;&c_a;-&c_f;&c_r;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_malmol;&c_r;&c_zh;&c_je;&c_n;&c_i; &c_k;&c_a;&c_r;&c_t;&c_o;&c_f;&c_i;</trd>
  <trdgrp lng="de">
    <trd>Pommes frites</trd>,
    <trd>Fritten</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>French <ind>fry</ind><klr> (Usono)</klr></trd>,
    <trd>potato <ind>chip</ind><klr> (Britujo)</klr></trd>
  </trdgrp>
  <trd lng="es"><ind>papas</ind> a la francesa</trd>
  <trd lng="fr">frite<klr tip="ind"> (pommes frites)</klr></trd>
  <trdgrp lng="hu">
    <trd>s&uuml;lt krumpli</trd>,
    <trd>(s&uuml;lt) <ind>has&aacute;bburgonya</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>friet</trd>,
    <trd>frites</trd>,
    <trd>patat<klr tip="ind"> (frites)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fritas</trd>,
    <trd>batatas fritas</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_r;&c_t;&c_o;&c_f;&c_je;&c_l;&c_mol; &c_v;&c_o;
    &c_f;&c_r;&c_i;&c_t;&c_ju;&c_r;&c_je;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_t;&c_o;&c_f;&c_je;&c_l;&c_mol;
    &c_zh;&c_a;&c_r;&c_je;&c_n;&c_y;&c_j;
    &c_s;&c_o;&c_l;&c_o;&c_m;&c_k;&c_o;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">pommes frites</trd>
  <trdgrp lng="th">
    <trd>&#3617;&#3633;&#3609;&#3613;&#3619;&#3633;&#3656;&#3591;&#3607;&#3629;&#3604;</trd>,
    <trd>&#3648;&#3615;&#3619;&#3609;&#3594;&#3660;&#3615;&#3619;&#3634;&#3618;&#3626;&#3660;</trd>
  </trdgrp>
  <trd lng="tr">k&inodot;zarm&inodot;&scedil; patates</trd>
  <trd lng="sk">frita</trd>
  <trd lng="cs">frita</trd>
</drv>
</art>

