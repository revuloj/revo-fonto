<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>loĝi</rad>/o
</kap>
<drv mrk="logxi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="logxi.0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Malgranda aparta ejo, fako por kelkaj personoj en <ref tip="malprt"
      cel="teatr.0ejo">teatrejo</ref>.
    </dif>
    <trd lng="be">ложа <klr tip="amb">(тэатральная)</klr></trd>
    <trd lng="de">Loge</trd>
    <trdgrp lng="en">
      <trd>box <klr tip="amb">(theater)</klr></trd>
    </trdgrp>
    <trd lng="hu">páholy</trd>
    <trd lng="pt">camarote</trd>
    <trd lng="ru">ложа <klr>(театральная)</klr></trd>
  </snc>
  <snc mrk="logxi.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="super" cel="galeri.0o.ARKI">Galerio</ref> larĝe malfermita
      sur ekstero, ordinare ĉe etaĝo de domo:
      <ekz>
        baldaŭ ili atingis la Bambuan <tld lit="L"/>on
        <fnt><bib>RugxDom</bib>, <lok>ĉapitro 74a, p. 559a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">лоджыя</trd>
    <trd lng="de">Loggia</trd>
    <trd lng="en">loggia</trd>
    <trd lng="hu">loggia</trd>
    <trd lng="ru">лоджия</trd>
  </snc>
  <snc mrk="logxi.0o.framasona">
    <dif>
      Grupo da framasonoj, kaj loko en kiu ili kunvenas.
    </dif>
    <trd lng="be">ложа <klr tip="amb">(масонская)</klr></trd>
    <trdgrp lng="de">
      <trd>Loge</trd>,
      <trd>Freimaurerloge</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>lodge <klr tip="amb">(masonic)</klr></trd>,
    </trdgrp>
    <trd lng="hu">szabadkőműves-páholy</trd>
    <trd lng="ru">ложа <klr>(масонская)</klr></trd>
  </snc>
  <trdgrp lng="fr">
    <trd>loge</trd>,
    <trd>loggia</trd>
  </trdgrp>
  <trd lng="nl">loge</trd>
</drv>
</art>
