<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lo&gcirc;i</rad>/o
</kap>
<drv mrk="logxi.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">TEA</uzo>
  <snc mrk="logxi.0o.TEA">
    <dif>
      Malgranda aparta ejo, fako por kelkaj personoj en teatro.
    </dif>
    <trd lng="ru">&c_l;&c_o;&c_zh;&c_a; <klr>(&c_t;&c_je;&c_a;&c_t;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja;)</klr></trd>
  </snc>
  <snc mrk="logxi.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Galerio lar&gcirc;e malfermita sur ekstero, &ccirc;e eta&gcirc;o de domo.
    </dif>
    <trd lng="ru">&c_l;&c_o;&c_d;&c_zh;&c_i;&c_ja;</trd>
  </snc>
  <snc mrk="logxi.0o.framasona">
    <dif>
      Grupo da framasonoj, kaj loko en kiu ili kunvenas.
    </dif>
    <trd lng="ru">&c_l;&c_o;&c_zh;&c_a; <klr>(&c_m;&c_a;&c_s;&c_o;&c_n;&c_s;&c_k;&c_a;&c_ja;)</klr></trd>
  </snc>
  <trdgrp lng="fr">
    <trd>loge</trd>,
    <trd>loggia</trd>
  </trdgrp>
  <trd lng="nl">loge</trd>
</drv>
</art>
