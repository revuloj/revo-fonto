<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nufar.xml,v 1.16 2022/03/24 10:41:40 revo Exp $">
<kap>
  <rad>nufar</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="nufar.0o">
  <kap><tld/>o</kap>
  <snc mrk="nufar.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="dif" cel="lili.akvo0o.BOT">Akvolilio</ref>:
      <ekz>
        plej konata specio estas la Flava <tld/>o, <klr>[la]</klr> folio de la <tld/>o
        estas simbolo por la provinco <ref cel="fris1.F0lando">Frislando</ref>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=201536">Nufaro</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Teichrose</trd>,
    <trd>Mummel</trd>
  </trdgrp>
  <trd lng="fr">n&eacute;nuphar</trd>
  <trdgrp lng="hu">
    <trd>tavir&oacute;zsa</trd>,
    <trd>v&iacute;zit&ouml;k</trd>
  </trdgrp>
  <trd lng="nl">gele <ind>plomp</ind></trd>
  <trd lng="pl">nenufar</trd>
  <trdgrp lng="pt">
    <trd>g&oacute;lf&atilde;o</trd>,
    <trd>aguap&eacute;</trd>,    
    <trd>gigoga</trd>,
    <trd>golfo</trd>,
    <trd>jigoga</trd>,
    <trd>l&iacute;rio-d'&aacute;gua</trd>    
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_b;&c_y;&c_sh;&c_k;&c_a; <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>,
    <trd>&c_zh;&c_jo;&c_l;&c_t;&c_a;&c_ja;
    <ind>&c_k;&c_u;&c_v;&c_sh;&c_i;&c_n;&c_k;&c_a;</ind></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x436;&#x43E;&#x432;&#x442;&#x435; &#x43B;&#x430;&#x442;&#x430;&#x442;&#x442;&#x44F;</trd>,
    <trd>&#x433;&#x43B;&#x435;&#x447;&#x438;&#x43A;&#x438;</trd>
  </trdgrp>
</drv>
</art>
