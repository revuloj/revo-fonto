<vortaro>
<art mrk="$Id$">
<kap>
  <rad>nufar</rad>/o
</kap>
<drv mrk="nufar.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="nimfea.0oj">nimfeacoj</ref>
      (<trd lng="la">Nuphar</trd>), kun flavaj floroj kaj
      verdaj botelformaj fruktoj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Teichrose</trd>,
    <trd>Mummel</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tavir&oacute;zsa</trd>,
    <trd>v&iacute;zit&ouml;k</trd>
  </trdgrp>
  <trd lng="nl">gele <ind>plomp</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_b;&c_y;&c_sh;&c_k;&c_a; <klr
tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>,
    <trd>&c_zh;&c_jo;&c_l;&c_t;&c_a;&c_ja;
<ind>&c_k;&c_u;&c_v;&c_sh;&c_i;&c_n;&c_k;&c_a;</ind></trd>
  </trdgrp>
</drv>
</art>
