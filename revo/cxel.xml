      <trd>white blood cell</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>leucocito</trd>,
      <trd>gl&oacute;bulo blanco</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&f_g;&a_w;&f_y;&f_ch;&a_h;&a_hamza_sure;
      &a_s;&a_f;&f_y;&a_d;</trd>,
