<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&ccirc;el</rad>/o
</kap>
<drv mrk="cxel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      &Ccirc;ambreto, malgranda ejo &ccirc;iuflanke fermita:
      <ekz><tld/>o de malliberejo, de mona&hcirc;o,</ekz>
      <ekz>vaksa <tld/>o de abeloj.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kabine.0o">kabineto</ref>,
      <ref cel="kamer.0o">kamero</ref>,
      <ref cel="karcer.0o">karcero</ref>,
      <ref cel="alveol.0o">alveolo</ref>.
    </refgrp>
    <trd lng="es">celda</trd>
    <trd lng="hu">cella</trd>
    <trd lng="pt">cela</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_a;&c_m;&c_je;&c_r;&c_a;</trd>,
      <trd>&c_k;&c_je;&c_l;&c_mol;&c_ja;</trd>,
      <trd>&c_ch;&c_u;&c_l;&c_a;&c_n;</trd>,
      <trd>&c_k;&c_a;&c_r;&c_c;&c_je;&c_r;</trd>,
      <trd>&c_o;&c_t;&c_s;&c_je;&c_k;</trd>,
      <trd>&c_ja;&c_ch;&c_je;&c_j;&c_k;&c_a; <klr>(&c_v;
&c_s;&c_o;&c_t;&c_a;&c_h;)</klr></trd>
    </trdgrp>
  </snc>
  <snc num="2" mrk="cxel.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      Ero de la organika teksa&jcirc;o, kiu konsistigas vegeta&jcirc;ojn kaj
bestojn.
    </dif>
    <trd lng="es">c&eacute;lula</trd>
    <trd lng="hu">sejt</trd>
    <trd lng="pt">c&eacute;lula</trd>
    <trd lng="ru">&c_k;&c_l;&c_je;&c_t;&c_k;&c_a;</trd>
  </snc>
  <snc num="3">
    <dif>
      Unua &scirc;tupo en kelkaj partiorganizoj.
    </dif>
    <trd lng="es">c&eacute;lula</trd>
    <trd lng="ru">&c_ja;&c_ch;&c_je;&c_j;&c_k;&c_a;</trd>
  </snc>
  <trd lng="en">cell</trd>
  <trd lng="fr">cellule</trd>
  <trd lng="hu">p&aacute;rtsejt</trd>
  <trd lng="nl">cel</trd>
  <trd lng="sk">bunka</trd>
</drv>
<drv mrk="cxel.0aro">
  <kap><tld/>aro</kap>
  <snc>
    <uzo tip="fak">BIO</uzo>
    <dif>
      Organika &ccirc;ela teksa&jcirc;o.
    </dif>
    <ref tip="sin" cel="hist.0o">histo.</ref>
  </snc>
  <trd lng="es">tejido</trd>
  <trd lng="fr">tissu<klr tip="ind"> (cellulaire)</klr></trd>
  <trd lng="hu">sejtsz&ouml;vet</trd>
  <trd lng="nl">weefsel<klr tip="ind"> (celweefsel)</klr></trd>
  <trd lng="pt">tecido</trd>
  <trd lng="ru">&c_t;&c_k;&c_a;&c_n;&c_mol;<klr tip="ind">
(&c_o;&c_r;&c_g;&c_a;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja;)</klr></trd
>
  <trd lng="sk">tkanivo</trd>
</drv>
</art>
