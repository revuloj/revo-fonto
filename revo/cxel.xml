    </dif>
    <trdgrp lng="be">
      <trd>&c_l;&c_je;&c_j;&c_k;&c_a;&c_c;&c_y;&c_t;</trd>,
      <trd>&c_b;&c_je;&c_l;&c_a;&c_ja; &c_k;&c_l;&c_je;&c_t;&c_k;&c_a; &c_k;&c_r;&c_y;&c_v;&c_ib;</trd>
