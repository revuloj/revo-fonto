<drv mrk="embara.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="embara.0igxi.MOV">
    <dif>
      Esti &gcirc;enata de <tld/>o fizika.
    </dif>
     <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_a;&c_p;&c_l;&c_ja;&c_c;&c_mol; &c_u; &c_c;&c_ja;&c_zh;&c_k;&c_a;&c_je; <ind>&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_ib;&c_sh;&c_ch;&c_a;</ind></trd>,
    <trd>&c_s;&c_u;&c_s;&c_t;&c_r;&c_a;&c_k;&c_a;&c_c;&c_mol; <ind>&c_p;&c_je;&c_r;&c_a;&c_sh;&c_k;&c_o;&c_d;&c_y;</ind></trd>
  </trdgrp>
  <trd lng="de"><ind>behindert</ind> werden</trd>
  </snc>
  <snc mrk="embara.0igxi.FIG">
    <dif>
      Ricevi <tld/>on pro malkomfortiga situacio:
      <ekz>
        a&ubreve;skultante lin, li <tld/>i&gcirc;is
        <fnt>
          <bib>NT</bib>
          <lok>Marko 6:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj konfuzi&gcirc;is kaj <tld/>i&gcirc;is, dirante unu al alia: Kion &ccirc;i tio signifas
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 2:12</lok>
        </fnt>?
      </ekz>
      <ekz>
        infanino mia, &scirc;i diris, vi <tld/>i&gcirc;as pro negrava&jcirc;oj
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Deka</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi petegas, al neniu nek unu vorton! &ndash;
        Onjo, kiel do? &ndash; <tld/>i&gcirc;is <nom>Vitalij</nom>
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hont.0i">honti</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_b;&c_ja;&c_n;&c_t;&c_e;&c_zh;&c_y;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_z;&c_a;&c_s;&c_m;&c_u;&c_ch;&c_a;&c_c;&c_c;&c_a;</trd>
    </trdgrp>
  <trdgrp lng="de">
    <trd>sich <ind>genieren</ind></trd>,
    <trd>sich <ind>sorgen</ind></trd>
  </trdgrp>
    <trd lng="zh">&#x96BE;&#x4E3A;&#x60C5; <pr>n&#xE1;nw&#xE9;iq&#xED;ng</pr></trd>
</snc>
  <trd lng="fr">s'<ind>emp&ecirc;trer</ind></trd>
