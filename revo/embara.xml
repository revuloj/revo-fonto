<drv mrk="embara.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="embara.mal0i.MOV">
    <dif>
      <ref tip="super" cel="sen.0igi">Senigi</ref> je &gcirc;ena&jcirc;oj, je baroj:
      <ekz>
        oni tuj mal<tld/>is lin je la malbenindaj vi&scirc;tukoj
        <fnt>
          <bib>MkM</bib>
          <lok>6</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la anta&ubreve;&ccirc;ambro mi mal<tld/>is &scirc;in je la mantelo kaj rapide ni eniris la unuan
        &ccirc;ambron
        <fnt>
          <bib>MkM</bib>
          <lok>13</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="es">
      <trd>liberar</trd>,
      <trd>desobstruir</trd>,
      <trd>descongestionar</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;barrasser</trd>,
      <trd>d&eacute;gager</trd>,
      <trd>d&eacute;sobstruer</trd>
    </trdgrp>
    <trd lng="hu">akad&aacute;lymentes&iacute;t</trd>
  </snc>
  <snc mrk="embara.mal0i.FIG">
    <dif>
      <ref tip="super" cel="el.0igi">Eligi</ref> el malfacila situacio:
      <ekz>
        tia bagateligado de granda poeto estas
        unu maniero mal<tld/>i sin je &gcirc;enanta entrudulo
        <fnt>
          <aut>G. Waringhien</aut>,
          <vrk><url ref="http://www.autodidactproject.org/other/heine-waringhien.html">
          Heine kaj la Amo</url></vrk>,
          <lok>en: Kaj la Ceter' &dash; Nur Literaturo, 1983</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="es">
      <trd>liberar<klr> (de una situaci&oacute;n dif&iacute;cil)</klr></trd>,
      <trd>solucionar<klr> (una complicaci&oacute;n)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;livrer<klr> (d'un embarras)</klr></trd>,
      <trd>tirer d'affaire</trd>
    </trdgrp>
  </snc>
  <refgrp tip="vid">
    <ref cel="liber.0igi">liberigi</ref>,
    <ref cel="pez.mal0igi">malpezigi</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_z;&c_b;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol; <ind>&c_p;&c_je;&c_r;&c_a;&c_sh;&c_k;&c_o;&c_d;</ind></trd>,
    <trd>&c_d;&c_a;&c_p;&c_a;&c_m;&c_a;&c_g;&c_a;&c_c;&c_mol; <klr>(&c_n;&c_a;&c_p;&c_r;. &c_v;&c_y;&c_j;&c_s;&c_mol;&c_c;&c_ib; &c_z;&c_mol; &c_c;&c_ja;&c_zh;&c_k;&c_a;&c_g;&c_a; &c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_ib;&c_sh;&c_ch;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="cs">zbavit rozpak&uring;</trd>
  <trdgrp lng="de">
    <trd>befreien</trd>,
    <trd>heraushelfen</trd>,
    <trd>aus der <ind>Not</ind> helfen</trd>,
    <trd lng="de">aus der <ind>Patsche</ind> helfen</trd>
  </trdgrp>
  <trd lng="ja">&#x53D6;&#x308A;&#x9664;&#x304F;  <pr>&#x3068;&#x308A;&#x306E;&#x305E;&#x304F;</pr></trd>
  <trd lng="sk">zbavi&tcaron; rozpakov</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&c_z;&c_b;&c_a;&c_v;&c_i;&c_t;&c_i; &c_t;&c_r;&c_u;&c_d;&#x43D;&#x43E;&c_shch;&c_ib;&c_v;</trd>,
    <trd>&#x43A;&#x43B;&#x43E;&#x43F;&#x43E;&c_t;&c_ib;&c_v;</trd>,
    <trd>&c_v;&c_i;&c_v;&c_je;&c_s;&c_t;&c_i; &c_z; &c_h;&c_a;&#x43B;&c_je;&#x43F;&c_i;/&c_z;&c_ib; &c_s;&#x43A;&c_r;&c_u;&c_t;&c_i;</trd>
