<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stomak.xml,v 1.29 2023/10/21 07:46:48 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>stomak</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="stomak.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="stomak.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Interna organo, kiu ricevas nutra&jcirc;ojn el la ezofago, kaj en kiu
      komenci&gcirc;as la digestado:
      <ekz>
        <ind>malsata <tld/>o orelon ne havas</ind>
        <trd lng="fr">ventre affam&eacute; n'a as d'oreille</trd>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        vivu <tld/>o <klr>(= ni man&gcirc;u)</klr> la&ubreve; la stato
        de l' sako<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>problemo
        <klr>(demando pri nutrado)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_r;&c_a;&c_w;&c_n;&c_ib;&c_k;</trd>
  <trd lng="bg">&c_s;&c_t;&c_o;&c_m;&c_a;&c_h;</trd>
  <trd lng="bo">&#xF42;&#xFB2;&#xF7C;&#xF51;&#xF0B;&#xF41;&#xF7C;&#xF42;&#xF0B;</trd>
  <trd lng="cs">&zcaron;aludek</trd>
  <trd lng="de">Magen</trd>
  <trd lng="en">stomach</trd>
  <trd lng="es">est&oacute;mago</trd>
  <trd lng="fa">&a_m;&a_ayn;&a_d;&a_h;</trd>
  <trd lng="fr">estomac</trd>
  <trd lng="hr">trbuh</trd>
  <trd lng="hu">gyomor</trd>
  <trd lng="it">stomaco</trd>
  <trdgrp lng="ja">
    <trd>&#x80C3;  <pr>&#12356;</pr></trd>,
    <trd>&#x80C3;&#x888B;  <pr>&#12356;&#12406;&#x304F;&#x308D;</pr></trd>,
    <trd>&#33145;  <pr>&#x306F;&#12425;</pr></trd>
  </trdgrp>
  <trd lng="nl">maag</trd>
  <trd lng="pl">&zdot;o&lstroke;&aogonek;dek</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_u;&c_d;&c_o;&c_k;</trd>
  <trd lng="sk">&#x17E;al&#xFA;dok</trd>
  <trd lng="uk">&c_sh;&#x43B;&c_u;&#x43D;&#x43E;&#x43A;</trd>
</drv>

<drv mrk="stomak.macx0o">
  <kap>ma&ccirc;<tld/>o</kap>
  <snc mrk="stomak.macx0o.ZOO">
    <ref tip="dif" cel="krop.post0o">postkropo</ref>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_u;&c_g;&c_ib; <ind>&c_s;&c_t;&c_r;&c_a;&c_w;&c_n;&c_ib;&c_k;</ind></trd>
  <trd lng="de">Kaumagen</trd>
  <trd lng="fa">&a_s;&a_n;&f_g;&a_d;&a_A;&a_n;</trd>
  <trd lng="fr">g&eacute;sier</trd>
  <trdgrp lng="hu">
    <trd>r&aacute;g&oacute;gyomor</trd>,
    <trd>z&uacute;za</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>ventriglio</trd>,
    <trd><ind>stomaco</ind> trituratore</trd>,
    <trd><ind>stomaco</ind> muscolare</trd>
  </trdgrp>
  <trd lng="nl">spiermaag</trd>
  <trdgrp lng="pl">
    <trd>&zdot;o&lstroke;&aogonek;dek mi&eogonek;&sacute;niowy</trd>,
    <trd>mielec</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_t;&c_o;&c_r;&c_o;&c_j; &c_zh;&c_je;&c_l;&c_u;&c_d;&c_o;&c_k;</trd>
  <trd lng="uk">&c_r;&c_u;&c_b;&c_je;&c_c;&#x44C;</trd>
</drv>
</art>
