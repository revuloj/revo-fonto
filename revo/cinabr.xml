<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>cinabr</rad>/o
</kap>

<drv mrk="cinabr.0o">
<kap><ofc>*</ofc><tld/>o</kap>
<uzo tip="fak">KEM</uzo>
<uzo tip="fak">MIN</uzo>
<snc>
<dif>
HgS. Ru&gcirc;a sulfido de hidrargo, uzata kiel kolorilo.
</dif>
<adm>
  La vorto "sulfido" estas enkondukenda. <aut>MB</aut>
</adm>
</snc>
<trdgrp lng="be">
  <trd>&c_c;&c_y;&c_n;&c_o;&c_b;&c_r;&c_a;</trd>,
  <trd>&c_k;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_r;</trd>
</trdgrp>
<trd lng="de">Zinnober</trd>
<trd lng="fr">cinabre</trd>
<trd lng="hu">cin&oacute;ber</trd>
<trd lng="pt">cinabre</trd>
<trd lng="ru">&c_k;&c_i;&c_n;&c_o;&c_v;&c_a;&c_r;&c_mol;</trd>
</drv>
</art>
