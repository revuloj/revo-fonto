<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cinabr.xml,v 1.25 2024/11/08 18:35:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>cinabr</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="cinabr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <mlg kod="IUPAC">HgS</mlg>
  <snc mrk="cinabr.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <uzo tip="fak">MIN</uzo>
    <dif>
      Ru&gcirc;a sulfido de hidrargo, uzata kiel
      <ref tip="lst" cel="kolor.0ilo" lst="voko:koloroj">kolorilo</ref>:
      <ekz>
        sur &gcirc;ia sojlo ka&ubreve;ris laboristo kaj kirlis brike ru&gcirc;koloran <tld/>on
        <fnt>
          <aut>A. N. Tolstoj, trad. E. Pill</aut>,
          <vrk><url ref="http://akileo.boom.ru/aelita3.htm">
          Aelita</url></vrk>,
          <lok>1928</lok>
        </fnt>.
      </ekz>
    </dif>
    <adm>
      La vorto "sulfido" estas enkondukenda. <aut>MB</aut>
    </adm>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_y;&c_n;&c_o;&c_b;&c_r;&c_a;</trd>,
    <trd>&c_k;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="ca">cinabri</trd>
  <trdgrp lng="cs">
    <trd>cinabarit</trd>,
    <trd>cinabro</trd>,
    <trd>rum&#x11B;lka</trd>
  </trdgrp>
  <trd lng="de">Zinnober</trd>
  <trd lng="en">cinnabar</trd>
  <trd lng="fr">cinabre</trd>
  <trd lng="hu">cin&oacute;ber</trd>
  <trd lng="it">cinabro</trd>
  <trdgrp lng="ja">
    <trd>&#x30B7;&#x30F3;&#30722;  <pr>&#x30B7;&#x30F3;&#12377;&#x306A;</pr></trd>,
    <trd>&#26417;  <pr>&#12375;&#12421;</pr></trd>
  </trdgrp>
  <trd lng="pl">cynober <klr>(minera&lstroke;)</klr></trd>
  <trd lng="pt">cinabre</trd>
  <trd lng="ro">cinabru</trd>
  <trd lng="ru">&c_k;&c_i;&c_n;&c_o;&c_v;&c_a;&c_r;&c_mol;</trd>
  <trd lng="sk">cinabarit</trd>
  <trd lng="uk">&#x43A;&c_ib;&#x43D;&#x43E;&c_v;&c_a;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#x8D64;&#33394;&#x786B;&#21270;&#x6C34;&#37504;
    <pr>ch&#xEC;s&#xE8;li&#xFA;hu&#xE0;shu&#x1D0;y&#xED;n</pr></trd>,
    <trd>&#x4E39;&#x7C89; <pr>d&amacron;nf&#x11B;n</pr></trd>,
    <trd>&#26417;&#30722; <pr>zh&#x16B;sh&amacron;</pr></trd>
  </trdgrp>
</drv>
</art>
