<drv mrk="gxarde.0a">
  <kap><tld/>a</kap>
  <snc mrk="gxarde.0a.HOR">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a palisaro;
      </ekz>
      <ekz>
        <tld/>aj <klr>(nesova&gcirc;aj)</klr> floroj;
      </ekz>
      <ekz>
        <tld/>a kulturo
        <klr>(pli intensa ol tiu de kampo)</klr>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_d;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_a;&c_g;&c_a;&c_r;&c_o;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_g;&c_a;&c_r;&c_o;&c_d;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">Garten-</trd>
  <trd lng="en">garden</trd>
  <trdgrp lng="fr">
    <trd>horticole</trd>,
    <trd>de <ind>jardin</ind></trd>,
    <trd>jardinier<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="hu">kerti</trd>
  <trd lng="ja">&#24237;&#22290;&#12398;  <pr>&#12390;&#12356;&#12360;&#12435;&#12398;</pr></trd>
  <trd lng="nl">tuin-</trd>
