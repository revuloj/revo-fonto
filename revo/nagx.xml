</kap>
<drv mrk="nagx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="nagx.0i.KOMUNE">
    <dif>
      Sin <ref cel="mov.0i">movi</ref> sur a&ubreve; en ia flua&jcirc;o:
    </dif>
    <subsnc>
      <dif>
        Sin peli tra <ref cel="akv.0o">akvo</ref>:  
        <ekz>
          mi &jcirc;etas min en la riveron, mi <tld/>as sub la akvo tiel longe, &gcirc;is
          <klr>[&#8230;]</klr> ili min jam ne vidos
          <fnt>
            <bib>Rabistoj</bib>
            <lok>Akto Dua</lok>
          </fnt>;
        </ekz>
        <ekz>
          vi povus tie lerni <tld/>i
          <fnt>
            <bib>Rabistoj</bib>
            <lok>Akto Dua</lok>
          </fnt>;
        </ekz>
        <ekz>
          karbonigraj cignoj <tld/>is sur la akvo
          <fnt>
            <bib>Fab1</bib>
            <lok>&Gcirc;ardeno de la paradizo</lok>
          </fnt>;
        </ekz>
        <ekz>
          estis tre malfacile <tld/>i kontra&ubreve; la fluado
          <fnt>
            <bib>Fab1</bib>
            <lok>Malgranda Niko kaj granda Niko</lok>
          </fnt>;
        </ekz>
        <ekz>
          fi&scirc;oj kun ar&gcirc;entaj kaj oraj skvamoj <tld/>is post la boato
          <fnt>
            <bib>Fab2</bib>
            <lok>Dio de dormo</lok>
          </fnt>;
        </ekz>
        <ekz>
          instruu viajn <klr>[&#8230;]</klr> infanojn <tld/>i
          <fnt>
            <bib>Fab2</bib>
            <lok>Malbela anasido</lok>
          </fnt>;
        </ekz>
        <ekz>
          <tld/>i alteren, vivsave
          <fnt>
            <bib>Rob</bib>
          </fnt>;
        </ekz>
        <ekz>
          nek trans<tld/>eble, nek transireble
          <fnt><bib>PrV</bib></fnt>;
        </ekz>
        <ekz>
          &ccirc;impanzoj <klr>[&#8230;]</klr> brusto-<tld/>as
          <fnt>
            <bib>Monato</bib>
            <aut>Roberto Pigro</aut>
            <vrk><url ref="&Monato;/2014/011217.php">Nekovrilaj malkovroj</url></vrk>
            <lok>2014</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="bg">&c_p;&c_l;&c_u;&c_v;&c_a;&c_m;</trd>
      <trd lng="el">
      &kappa;&omikron;&lambda;&ypsilon;&my;&pi;&omega_ton;</trd>
      <trd lng="de">schwimmen</trd>
      <trd lng="en">swim</trd>
      <trd lng="id">be<ind>renang</ind></trd>
      <trd lng="nl">zwemmen</trd>
      <trd lng="pl">p&lstroke;ywa&cacute;</trd>
      <trd lng="pt">nadar</trd>
      <trd lng="sv">simma</trd>
    </subsnc>
    <subsnc>
      <dif>
        Esti portata sur akvo, <ref cel="flos.0i">flosi</ref>,
        <ref cel="driv.0i.MAR">drivi</ref>:
        <ekz>
          la tuta supra&jcirc;o de la lago estis kovrita per <tld/>antaj folioj
          <fnt>
            <bib>F</bib>
            <lok>&FE; 35</lok>
          </fnt>;
        </ekz>
        <ekz>
          la arkeo <tld/>is sur la supra&jcirc;o de la akvo
          <fnt>
            <bib>MT</bib>
            <lok>Genezo 7:18</lok>
          </fnt>;
        </ekz>
        <ekz>
          pecoj de la &scirc;iprompita&jcirc;o, &ccirc;iuspecaj lignoj kaj lignetoj al<tld/>is al la bordo
          <fnt>
            <bib>Fab4</bib>
            <lok>Historio el la dunoj</lok>
          </fnt>;
        </ekz>
        <ekz>
          alna trunko <klr>[&#8230;]</klr> <tld/>is sur la supra&jcirc;o de la mar&ccirc;o
          <fnt>
            <bib>Fab4</bib>
            <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
          </fnt>;
        </ekz>
        <ekz>
          la verda folio <tld/>is <klr>[&#8230;]</klr> la&ubreve; la fluo de la rivero
          <fnt>
            <bib>Fab1</bib>
            <lok>Elinjo-fingreto</lok>
          </fnt>;
        </ekz>
        <ekz>
          &ccirc;irka&ubreve;e <tld/>is grandaj montoj de glacio
          <fnt>
            <bib>Fab1</bib>
            <lok>La virineto de maro</lok>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>schwimmen</trd>,
        <trd>treiben</trd>
      </trdgrp>
      <trd lng="nl">drijven</trd>
      <trdgrp lng="pl">
        <trd>p&lstroke;ywa&cacute;</trd>,
        <trd>sp&lstroke;ywa&cacute;</trd>,
        <trd>p&lstroke;yn&aogonek;&cacute;</trd>,
        <trd>unosi&cacute; si&eogonek;</trd>
      </trdgrp>
    </subsnc>
    <subsnc>
      <dif>
        Sin movi en iu flueca a&ubreve; gasa medio:
        <ekz>
          rozeru&gcirc;a nubo <klr>[&#8230;]</klr> <tld/>is en la aero
          <fnt>
            <bib>Fab1</bib>
            <lok>La virineto de maro</lok>
          </fnt>;
        </ekz>
        <ekz>
          en la okuloj de la pli juna fratino <tld/>is larmo
          <fnt>
            <bib>BdV</bib>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>schwimmen</trd>,
        <trd>treiben</trd>
      </trdgrp>
      <trd lng="nl">drijven</trd>
      <trdgrp lng="pl">
        <trd>p&lstroke;ywa&cacute;</trd>,
        <trd>unosi&cacute; si&eogonek;</trd>
      </trdgrp>
    </subsnc>
  </snc>
  <snc mrk="nagx.0i.FIG_sxvebi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Trapasi, iri pp ideoj a&ubreve; sentoj:
      <ekz>
        en liaj okuloj kvaza&ubreve; <tld/>is feli&ccirc;aj rememoroj kaj anta&ubreve;sentoj
        <fnt>
          <bib>RBa</bib>
        </fnt>;
      </ekz>
      <ekz>
        per la fadeno de rememoroj al<tld/>is al &scirc;i la bela viza&gcirc;o de la patrino
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tio]</klr> rememorigadis iajn anta&ubreve;ajn kutimojn, iajn korajn sentojn
        <klr>[&#8230;]</klr>, kiuj for<tld/>is en neatingeblan malproksimon sur tro
        rapidema ondo de senrevena pasinteco
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flug.0i">flugi</ref>,
      <ref cel="sxveb.0i">&scirc;vebi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>flie&szlig;en</trd>,
      <trd>schweifen</trd>
    </trdgrp>
    <trd lng="nl">zweven</trd>
    <trd lng="pl">p&lstroke;yn&aogonek;&cacute;</trd>
  </snc>
  <snc mrk="nagx.0i.FIG_enesti">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Esti kvaza&ubreve; bani&gcirc;anta en abundo de io:
      <ekz>
        Heleno <tld/>is en ravo
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        se vi deziras, ke la faraono <klr>[&#8230;]</klr> <tld/>u en ri&ccirc;a&jcirc;oj, certigu al la
        lando longan pacon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia kapo kvaza&ubreve; <tld/>is pro timo
        <fnt>
          <bib>Rob</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi <tld/>as en plena fantazio
        <fnt>
          <bib>ChL</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>schwimmen</trd>,
      <trd>baden</trd>
    </trdgrp>
    <trd lng="nl">baden</trd>
    <trdgrp lng="pl">
      <trd>p&lstroke;ywa&cacute;</trd>,
      <trd>nurza&cacute; si&eogonek;</trd>
    </trdgrp>
  </snc>
