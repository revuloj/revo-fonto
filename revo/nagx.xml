  <trd lng="lo">&#xEC0;&#xE99;&#xEB7;&#xEC9;&#xEAD;&#xE97;&#xEB5;&#xEC8;&#xEC0;&#xEAB;&#xEB7;&#xEAD;&#xE81;</trd>
  <trd lng="ml">&#xD28;&#xD40;&#xD28;&#xD4D;&#xD24;&#xD41;&#xD15;</trd>
