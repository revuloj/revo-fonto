<drv mrk="nagx.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="nagx.0ejo.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Loko ta&ubreve;ga por <tld/>o, precipe tia
      <ref tip="super" cel="sport.0ejo.SPO">sportejo</ref>
      kun <ref tip="prt" cel="basen.nagx0o.SPO">basenoj</ref>:
      <ekz>
        <klr>[&scirc;i]</klr> <tld/>is belsezone en la propra vilaa <tld/>ejo, dum li tutjare
        vizitadis la hejtatan publikan <tld/>ejon
        <fnt>
          <bib>ChR</bib>
        </fnt>;
      </ekz>
      <ekz>
        negrave &ccirc;u oni plon&gcirc;as en malgrandan <tld/>ejon a&ubreve; la plej grandan maron, oni
        tute same devas na&gcirc;i
        <fnt>
          <bib>Rsp</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ejo sub&ccirc;iela
        <fnt>
          <bib>Monato</bib>
          <aut>John Goodhand</aut>
          <vrk><url ref="&Monato;/2005/008509.php">Nekutima feriejo</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        tegmentitaj <tld/>ejoj
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://ranto.eu/nagxejo/">
          Na&gcirc;ejo en via &gcirc;ardeno</url></vrk>,
          <lok>ranto.eu, 2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="cs">
      <trd>koupali&scaron;t&ecaron;</trd>,
      <trd>plov&aacute;rna</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>plav&aacute;re&ncaron;</trd>,
      <trd>k&uacute;palisko</trd>
    </trdgrp>
  </snc>
  <trd lng="bg">&c_p;&c_l;&c_u;&c_v;&c_je;&c_n;
  &c_b;&c_a;&c_s;&c_je;&c_j;&c_n;</trd>
  <trd lng="bo">&t_cha;&t_shabchu;&t_tsheg;&t_ra;&t_kata;&t_yata;&t_la;&t_tsheg;&t_ra;&t_dzata;&t_gigu;&t_nga;&t_tsheg;&t_ba;&t_shabchu;&t_tsheg;</trd>
  <trd lng="ca">piscina</trd>
  <trd lng="de">Schwimmbad</trd>
  <trd lng="fa">&a_A;&a_s;&a_t;&a_kh;&a_r;</trd>
  <trd lng="fr">piscine</trd>
  <trd lng="id"><ind>kolam</ind> renang</trd>
  <trd lng="pl">p&lstroke;ywalnia</trd>
  <trd lng="th">&#3626;&#3619;&#3632;&#3623;&#3656;&#3634;&#3618;&#3609;&#3657;&#3635;</trd>
  <trd lng="uk">&c_p;&c_l;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j; &c_b;&c_a;&c_s;&c_je;&c_j;&c_n;</trd>
</drv>

<drv mrk="nagx.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="nagx.0ilo.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Membro, per kiu sin movas ekzemple fi&scirc;oj en akvo:
      <ekz>
        <klr>[fi&scirc;oj]</klr> havas <tld/>ilojn kaj skvamojn
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 11:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fi&scirc;oj per siaj <tld/>iloj kaj vostoj &gcirc;in batis
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="cs">ploutev</trd>
    <trd lng="sk">plutva</trd>
  <trd lng="be">&c_p;&c_l;&c_a;&c_w;&c_n;&c_ib;&c_k;</trd>
  <trd lng="bg">&c_p;&c_je;&c_r;&c_k;&c_a;<klr tip="amb"> &c_n;&c_a;
  &c_r;&c_i;&c_b;&c_a;</klr></trd>
  <trd lng="br">angell</trd>
  <trd lng="ca">aleta</trd>
  <trd lng="de">Flosse</trd>
  <trd lng="en">fin</trd>
  <trd lng="es">aleta</trd>
  <trd lng="fa">&a_b;&a_A;&a_l;&a_h;</trd>
  <trd lng="fr">nageoire</trd>
  <trd lng="hu">uszony</trd>
  <trd lng="id">sirip</trd>
  <trd lng="nl">vin</trd>
  <trd lng="pl">p&lstroke;etwa</trd>
  <trd lng="pt">nadadeira</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_v;&c_n;&c_i;&c_k;</trd>
  <trd lng="sv">fena</trd>
  <trd lng="tr">y&uuml;zge&ccedil;</trd>
 </snc>
 <snc mrk="nagx.0ilo.SPO">
     <uzo tip="fak">SPO</uzo>
   <dif>
     Ka&ubreve;&ccirc;ukaj duon&scirc;uoj kun lar&gcirc;a surfaco, por helpi al subakva na&gcirc;ado:
      <ekz>
        <tld/>iloj ebligas <tld/>i sen movi la brakojn, do plon&gcirc;isto
        povas uzi la manojn por porti iun ilon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="&Viki;/Na&gcirc;ilo_(artefarita)">
            Na&gcirc;ilo_(artefarita)</url>
          </lok>
        </fnt>.
      </ekz>
   </dif>
   <trd lng="fa">&f_k;&a_f;&a_sh; &a_gh;&a_w;&a_A;&a_S;&f_y;</trd>
  <trd lng="fr">palme</trd>
  </snc>
  <trdgrp lng="uk">
    <trd>&c_p;&c_l;&c_a;&c_v;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_ib;&c_l;</trd>,
    <trd>&c_l;&c_a;&c_s;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="nagx.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="nagx.cxirkaux0i.KOMUNE">
    <dif>
      <tld lit="N"/>i &ccirc;irka&ubreve;:
      <ekz>
        la grandaj cignoj &ccirc;irka&ubreve;<tld/>is
        <klr>[la cignidon]</klr> kaj karesis &gcirc;in per sia beko
        <fnt><bib>Fab2</bib>, <lok>Malbela anasido</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">nedar al voltant</trd>
  <trd lng="de">umschwimmen</trd>
  <trd lng="fr">contourner en nageant</trd>
  <trd lng="pl">op&lstroke;yn&aogonek;&cacute;</trd>
</drv>

<drv mrk="nagx.el0i">
  <kap>el<tld/>i</kap>
  <snc mrk="nagx.el0i.KOMUNE">
    <dif>
      <tld lit="N"/>ante  <ref tip="super" cel="el.0igxi">eli&gcirc;i</ref>
      el io:
      <ekz>
        el la densejo el<tld/>is tri belaj blankaj cignoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        el la apudbordaj arbeta&jcirc;oj subite el<tld/>is &scirc;ipoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la rano <klr>[&#8230;]</klr> subakvi&gcirc;is kaj post momento el<tld/>is kun la globo en la
        bu&scirc;o
        <fnt>
          <bib>ElektFab</bib>
          <lok>Re&gcirc;o rano a&ubreve; fera Henriko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        penso, kiun li mem ne povas klarigi al si, el<tld/>as en lia brusto
        <fnt>
          <bib>Fab2</bib>
          <lok>Bildo el kastelremparo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        liaj kantoj ankora&ubreve; vivas <klr>[&#8230;]</klr> bildo post bildo el<tld/>as el la
        oriento
        <fnt>
          <bib>Fab3</bib>
          <lok>Dorna vojo de la honoro</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="aper.0i">aperi</ref>,
       <ref cel="emergx.0i">emer&gcirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_p;&c_l;&c_y;&c_c;&c_mol;</trd>
  <trd lng="br">diveuzi&ntilde;</trd>
  <trd lng="ca">emergir</trd>
  <trd lng="de">auftauchen</trd>
  <trd lng="fr">&eacute;merger</trd>
  <trd lng="hu">ki&uacute;szik</trd>
  <trdgrp lng="id">
    <trd><ind>muncul</ind><klr tip="amb"> (dari permukaan
    air)</klr></trd>,
    <trd><ind>timbul</ind><klr tip="amb"> (dari permukaan
    air)</klr></trd>
  </trdgrp>
  <trd lng="nl">bovendrijven</trd>
  <trd lng="pl">sp&lstroke;yn&aogonek;&cacute;</trd>
  <trd lng="ru">&c_v;&c_y;&c_p;&c_l;&c_y;&c_t;&c_mol;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_p;&c_l;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_p;&c_l;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="nagx.sub0i">
  <kap>
    sub<tld/>i
  </kap>
  <snc>
    <dif>
      <tld lit="N"/>i sub ion:
      <ekz>
        subite la &scirc;ipeto sub<tld/>is sub longan stratdefluilan ponton
        <fnt>
          <bib>Fab1</bib>
          <lok>Persista stana soldato</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="nagx.sub0i.subsurface">
    <dif>
      <tld lit="N"/>i sub la akvosurfacon:
      <ekz>
        <klr>[&scirc;i]</klr> sub<tld/>is profunde en la akvon kaj sin levis denove inter la
        altaj ondoj
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="profun.en0igxi">enprofundi&gcirc;i</ref>,
      <ref cel="merg.0igxi">mergi&gcirc;i</ref>,
      <ref cel="plongx.0i.SPO">plon&gcirc;i<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="de">abtauchen</trd>
  <trd lng="en">dive</trd>
  <trd lng="fr">plonger</trd>
  <trd lng="th">&#3604;&#3635;&#3609;&#3657;&#3635;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_r;&c_i;&c_n;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_ib;&c_r;&c_n;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_l;&c_i;&c_s;&c_t;&c_i; &c_p;&c_ib;&c_d; &c_v;&c_o;&c_d;&c_o;&c_ju;</trd>
  </trdgrp>
</drv>





