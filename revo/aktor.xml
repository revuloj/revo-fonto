<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>aktor</rad>/o
</kap>
<drv mrk="aktor.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <dif>
      Teatra ludisto, tragedia a&ubreve; komedia.
    </dif>
    <refgrp tip="sin">
      <ref cel="histri1.0o">histriono</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Schauspieler</trd>,
     <trd>Darsteller</trd>
  </trdgrp>
  <trd lng="en">actor</trd>
  <trd lng="es">actor</trd>
  <trd lng="it">attore</trd>
  <trd lng="fr">acteur</trd>
  <trdgrp lng="nl">
    <trd>acteur</trd>,
    <trd>toneelspeler</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>ator</trd>,
    <trd>artista</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_k;&c_t;&c_jo;&c_r;</trd>,
    <trd>&c_a;&c_r;&c_t;&c_i;&c_s;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">herec</trd>
</drv>
</art>
