<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>aktor</rad>/o
</kap>
<drv mrk="aktor.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <dif>
      Teatra a&ubreve; kina ludisto, tragedia a&ubreve; komedia:
      <ekz>
        kroataj teatra&jcirc;oj kaj <tld/>oj multe kontribuis al la esperanta
        teatro<fnt><aut>Hecimovic Branko</aut>: <vrk>Antologio de kroataj
        unuaktaj dramoj</vrk>, <bib>Monato</bib><lok>jaro 1998a, numero
        3a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Schauspieler</trd>,
    <trd>Darsteller</trd>
  </trdgrp>
  <trd lng="en">actor</trd>
  <trd lng="es">actor</trd>
  <trd lng="it">attore</trd>
  <trd lng="fr">acteur</trd>
  <trdgrp lng="nl">
    <trd>acteur</trd>,
    <trd>toneelspeler</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>ator</trd>,
    <trd>artista</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_k;&c_t;&c_jo;&c_r;</trd>,
    <trd>&c_a;&c_r;&c_t;&c_i;&c_s;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">herec</trd>
</drv>

<drv mrk="aktor.0i">
  <kap><tld/>i</kap>
  <snc>
    <dif>
      Surprenadi rolojn en teatra&jcirc;oj, kina&jcirc;oj:
      <ekz>
        Nikolaj Rytjkov trovis eblojn anka&ubreve; profesie
        <tld/>i<fnt><aut>Boris Kolker</aut>: <vrk>Aktoro malliberigita
        (Sub la signo de Liro kaj Verda Stelo)</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 2a, p. 28a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rol.0i.TEA">roli <sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>jouer<klr> (un r&ocirc;le)</klr></trd>
  </trdgrp>
</drv>
</art>
