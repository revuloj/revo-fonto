<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>aktor</rad>/o
</kap>
<drv mrk="aktor.0o">
  <kap><tld/>o</kap>
  <snc mrk="aktor.0o.TEA">
    <tezrad fak="KIN"/>
    <tezrad fak="TEA"/>
    <uzo tip="fak">KIN</uzo>
    <uzo tip="fak">TEA</uzo>
    <dif>
      Teatra a&ubreve; kina ludisto, plenumanta rolojn:
      <ekz>
        kroataj teatra&jcirc;oj kaj <tld/>oj multe kontribuis al la esperanta
        teatro
        <fnt>
        <aut>Hecimovic Branko</aut>,
        <vrk>Antologio de kroataj unuaktaj dramoj</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 1998a, numero 3a, p. 18a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_t;&c_o;&c_r;</trd>
  <trd lng="bg">&c_a;&c_k;&c_t;&c_mol;&c_o;&c_r;</trd>
  <trd lng="cs">herec</trd>
  <trdgrp lng="de">
    <trd>Schauspieler</trd>,
    <trd>Darsteller</trd>
  </trdgrp>
  <trd lng="en">actor</trd>
  <trd lng="es">actor</trd>
  <trd lng="fr">acteur</trd>
  <trd lng="he">&shin;&het;&qof;&fnun;<klr tip="amb"> (&tav;&yod;&alef;&tet;&resh;&vav;&fnun;,
  &qof;&vav;&lamed;&nun;&vav;&ayin;)</klr></trd>
  <trd lng="hu">sz&iacute;n&eacute;sz</trd>
  <trd lng="it">attore</trd>
  <trdgrp lng="nl">
    <trd>acteur</trd>,
    <trd>toneelspeler</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>ator</trd>,
    <trd>artista</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_k;&c_t;&c_jo;&c_r;</trd>,
    <trd>&c_a;&c_r;&c_t;&c_i;&c_s;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">herec</trd>
</drv>

<drv mrk="aktor.0i">
  <kap><tld/>i</kap>
  <snc mrk="aktor.0i.TEA">
    <uzo tip="fak">KIN</uzo>
    <uzo tip="fak">TEA</uzo>
    <dif>
      Surprenadi rolojn en teatra&jcirc;oj, kina&jcirc;oj:
      <ekz>
        Nikolaj Rytjkov trovis eblojn anka&ubreve; profesie
        <tld/>i
        <fnt>
        <aut>Boris Kolker</aut>,
        <vrk>Aktoro malliberigita (Sub la signo de Liro kaj Verda
        Stelo)</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2001a, numero 2a, p. 28a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la &scirc;ipestro <tld/>is delikate
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>dua libro, &ccirc;apitro
        26a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="komedi.0i.TEA">komedii <sncref/></ref>,
      <ref cel="komedi.0i.FIG">komedii <sncref/></ref>,
      <ref cel="rol.0i.TEA">roli <sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_c;&c_mol; <klr>(&c_r;&c_o;&c_l;&c_ju;)</klr></trd>
  <trd lng="cs">hr&aacute;t</trd>
  <trdgrp lng="de">
    <trd>schauspielern</trd>,
    <trd>im Theater <ind>auftreten</ind></trd>
  </trdgrp>
  <trd lng="es">actuar<klr tip="amb"> (teatro)</klr></trd>
  <trdgrp lng="fr">
    <trd>jouer un r&ocirc;le<klr tip="amb"> (th&eacute;&acirc;tre)</klr></trd>
  </trdgrp>
  <trd lng="he">&lamed;<ind>&shin;&het;&qof;</ind><klr tip="amb">
  (&tav;&pe;&qof;&yod;&dalet;)</klr></trd>
  <trdgrp lng="hu">
    <trd>szerepel<klr> (mint sz&iacute;n&eacute;sz)</klr></trd>,
    <trd>j&aacute;tszik<klr> (mint sz&iacute;n&eacute;sz)</klr></trd>
  </trdgrp>
  <trd lng="it">recitare</trd>
  <trd lng="sk">hra&tcaron;</trd>
  <trd lng="ru">&c_i;&c_g;&c_r;&c_a;&c_t;&c_mol;<klr tip="amb"> &c_r;&c_o;&c_l;&c_mol;</klr></trd>
</drv>

<drv mrk="aktor.film0o">
  <kap>film<tld/>o</kap>
  <snc mrk="aktor.film0o.KIN">
    <uzo tip="fak">KIN</uzo>
    <dif>
      <tld lit="A"/>o, kiu rolas en filmoj:
      <ekz>
        kial ne fari&gcirc;i film<tld/>o? &ndash; Tion mi ne mal&scirc;atus
        <fnt><bib>InfanTorent2</bib>, <lok>dua parto, &ccirc;apitro
        3a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Filmschauspieler</trd>
  <trdgrp lng="en">
    <trd>film actor</trd>,
    <trd>film actress</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>acteur de cin&eacute;ma</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>filmsz&iacute;n&eacute;sz</trd>,
    <trd>filmm&udblac;v&eacute;sz</trd>
  </trdgrp>
  <trd lng="it">attore<klr> cinematografico</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_n;&c_o;&c_a;&c_k;&c_t;&c_jo;&c_r;</trd>,
    <trd>&c_k;&c_i;&c_n;&c_o;&c_a;&c_r;&c_t;&c_i;&c_s;&c_t;</trd>
  </trdgrp>
</drv>
</art>
