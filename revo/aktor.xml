<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aktor.xml,v 1.45 2024/11/02 22:31:04 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>aktor</rad>/o
</kap>
<drv mrk="aktor.0o">
  <kap><tld/>o</kap>
  <snc mrk="aktor.0o.TEA">
    <tezrad fak="KIN"/>
    <tezrad fak="TEA"/>
    <uzo tip="fak">KIN</uzo>
    <uzo tip="fak">TEA</uzo>
    <dif>
      Teatra a&ubreve; kina ludisto, plenumanta
      <ref tip="prt" cel="rol.0o">rolo</ref>jn:
      <ekz>
        eniradis sur la arenon la <tld/>oj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        nenia dramo donis e&ccirc; iajn enspezojn, se la a&ubreve;toro kaj <tld/>oj ne atakadis en &gcirc;i
        siajn kontra&ubreve;ulojn
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kroataj teatra&jcirc;oj kaj <tld/>oj multe kontribuis al la esperanta
        teatro
        <fnt>
          <aut>Hecimovic Branko</aut>,
          <vrk>Antologio de kroataj unuaktaj dramoj</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 1998a, numero 3a, p. 18a</lok>
        </fnt>;
      </ekz>
      <ekz>
        kia sufloro, tia <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="rol.0isto">rolisto</ref>
    <trd lng="zh">&#22823;&#x82B1;&#38754; <pr>d&#xE0;hu&amacron;mi&#xE0;n</pr></trd>
</snc>
  <trd lng="be">&c_a;&c_k;&c_t;&c_o;&c_r;</trd>
  <trd lng="bg">&c_a;&c_k;&c_t;&c_mol;&c_o;&c_r;</trd>
  <trd lng="ja">&#x4FF3;&#x512A;  <pr>&#x306F;&#12356;&#12422;&#12358;</pr></trd>
  <trd lng="ms">pelakon</trd>
  <trdgrp lng="cs">
    <trd>herec</trd>,
    <trd>p&rcaron;edstavitel</trd>,
    <trd>&uacute;&ccaron;inkuj&iacute;c&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schauspieler</trd>,
    <trd>Darsteller</trd>
  </trdgrp>
  <trd lng="en">actor</trd>
  <trd lng="es">actor</trd>
  <trd lng="fr">acteur</trd>
  <trd lng="he">&shin;&het;&qof;&fnun;<klr tip="amb"> (&tav;&yod;&alef;&tet;&resh;&vav;&fnun;,
  &qof;&vav;&lamed;&nun;&vav;&ayin;)</klr></trd>
  <trd lng="hu">sz&iacute;n&eacute;sz</trd>
  <trd lng="io">aktoro</trd>
  <trd lng="it">attore</trd>
  <trd lng="ms">seniwati, pelakon wanita</trd>
  <trdgrp lng="nl">
    <trd>acteur</trd>,
    <trd>toneelspeler</trd>
  </trdgrp>
  <trd lng="pl">aktor</trd>
  <trdgrp lng="pt">
    <trd>ator</trd>,
    <trd>artista</trd>
  </trdgrp>
  <trd lng="ro">actor</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_k;&c_t;&c_jo;&c_r;</trd>,
    <trd>&c_a;&c_r;&c_t;&c_i;&c_s;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>herec</trd>,
    <trd>&uacute;&ccaron;inkuj&uacute;ci</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&c_k;&c_t;&c_o;&c_r;</trd>,
    <trd>&c_a;&c_r;&c_t;&c_i;&c_s;&c_t;</trd>
  </trdgrp>
  <trd lng="vo">dramatan</trd>
</drv>

<drv mrk="aktor.0i">
  <kap><tld/>i</kap>
  <snc mrk="aktor.0i.TEA">
    <uzo tip="fak">KIN</uzo>
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="sin" cel="lud.0i.BELA">Ludi</ref> rolojn en teatra&jcirc;oj, kina&jcirc;oj:
      <ekz>
        li ne &scirc;ercis nek <tld/>is, kaj dum la tuta leciono ne aperis e&ccirc; plej eta rideto
        sur lia viza&gcirc;o
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;e&hcirc;a <tld/>ino <nom>Eva Seemannov&aacute;-Suchardova</nom> <klr>[&#8230;]</klr> <tld/>is en &leftquot;La Verda
        &Ccirc;aro de Julio <nom>Baghy</nom>&rightquot;
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Nikolaj Rytjkov</nom> trovis eblojn anka&ubreve; profesie
        <tld/>i
        <fnt>
          <aut>Boris Kolker</aut>,
          <vrk>Aktoro malliberigita (Sub la signo de Liro kaj Verda
          Stelo)</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2001a, numero 2a, p. 28a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la &scirc;ipestro <tld/>is delikate
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>dua libro, &ccirc;apitro
        26a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="deklam.0i.POE">deklami</ref>,
      <ref cel="interp2.0i.BELA">interpreti<sncref/></ref>,
      <ref cel="komedi.0i">komedii</ref>,
      <ref cel="prezen1.0i.spektaklon">prezenti<sncref/></ref>,
      <ref cel="rol.0i.TEA">roli<sncref/></ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#21809;&#x620F; <pr>ch&#xE0;ngx&#xEC;</pr></trd>,
    <trd>&#21809;&#25138; <pr>ch&#xE0;ngx&#xEC;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_c;&c_mol; <klr>(&c_r;&c_o;&c_l;&c_ju;)</klr></trd>
  <trdgrp lng="cs">
    <trd>hr&aacute;t</trd>,
    <trd>hr&aacute;t divadlo</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schauspielern</trd>,
    <trd>im Theater <ind>auftreten</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>act</trd>,
    <trd>play a role</trd>
  </trdgrp>
  <trd lng="es">actuar<klr tip="amb"> (teatro)</klr></trd>
  <trdgrp lng="fr">
    <trd>jouer un r&ocirc;le<klr tip="amb"> (th&eacute;&acirc;tre)</klr></trd>
  </trdgrp>
  <trd lng="he">&lamed;<ind>&shin;&het;&qof;</ind><klr tip="amb">
  (&tav;&pe;&qof;&yod;&dalet;)</klr></trd>
  <trdgrp lng="hu">
    <trd>szerepel<klr> (mint sz&iacute;n&eacute;sz)</klr></trd>,
    <trd>j&aacute;tszik<klr> (mint sz&iacute;n&eacute;sz)</klr></trd>
  </trdgrp>
  <trd lng="it">recitare</trd>
  <trd lng="ja">&#x6F14;&#25216;&#12377;&#x308B;  <pr>&#12360;&#12435;&#x304E;&#12377;&#x308B;</pr></trd>
  <trdgrp lng="pl">
    <trd>gra&cacute; rol&eogonek;</trd>,
    <trd>odtwarza&cacute; rol&eogonek;</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_g;&c_r;&c_a;&c_t;&c_mol;<klr tip="amb"> &c_r;&c_o;&c_l;&c_mol;</klr></trd>
  <trdgrp lng="sk">
    <trd>hra&tcaron;</trd>,
    <trd>hra&tcaron; divadlo</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_t;&c_i; &c_a;&c_k;&c_t;&c_o;&c_r;&c_o;&c_m;</trd>,
    <trd>&c_v;&c_i;&c_s;&c_t;&c_u;&c_p;&c_a;&c_t;&c_i; &c_u; &c_s;&c_p;&c_je;&c_k;&c_t;&c_a;&c_k;&c_l;&c_ib;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_t;&c_i; &c_r;&c_o;&c_l;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="aktor.0ino">
  <kap><tld/>ino</kap>
  <snc mrk="aktor.0ino.TEA">
    <uzo tip="fak">KIN</uzo>
    <uzo tip="fak">TEA</uzo>
    <dif>
      Ina <ref tip="super" cel="aktor.0o">aktoro</ref>:
      <ekz>
        grandtalenta <tld/>o, kiu faras al <tld/>ino anta&ubreve; entuziasmigita publiko la
        plej pasiajn amkonfesojn
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Oka</lok>
        </fnt>;
      </ekz>
      <ekz>
        fajroj estis ekflamantaj en la arbustoj, timigante nian malfeli&ccirc;an <tld/>inon,
        kiu jam &scirc;an&gcirc;is la tridek kvinan kostumon je ru&gcirc;a atlasa pantaloneto,
        busttenilo kaj rusa ar&gcirc;enta krestsimila &ccirc;apo
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#22899;&#x4F18; <pr>n&#x1DA;y&#x14D;u</pr></trd>,
    <trd>&#22899;&#x512A; <pr>n&#x1DA;y&#x14D;u</pr></trd>,
    <trd>&#22899;&#x6F14;&#21592; <pr>n&#x1DA;y&#x1CE;nyu&#xE1;n</pr></trd>,
    <trd>&#22899;&#x6F14;&#x54E1; <pr>n&#x1DA;y&#x1CE;nyu&#xE1;n</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_k;&c_t;&c_o;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_a;&c_k;&c_t;&c_r;&c_y;&c_s;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schauspielerin</trd>,
    <trd>Darstellerin</trd>
  </trdgrp>
  <trd lng="en">actress</trd>
  <trd lng="es">actriz</trd>
  <trd lng="fr">actrice</trd>
  <trd lng="it">attrice</trd>
  <trd lng="io">aktorino</trd>
  <trd lng="ja">&#22899;&#x512A;  <pr>&#12376;&#12423;&#12422;&#12358;</pr></trd>
  <trdgrp lng="nl">
    <trd>actrice</trd>,
    <trd>toneelspeelster</trd>
  </trdgrp>
  <trd lng="pl">aktorka</trd>
  <trd lng="uk">&c_a;&c_k;&c_t;&c_o;&c_r;&c_k;&c_a;</trd>
  <trd lng="vo">jidramatan</trd>
</drv>

<drv mrk="aktor.film0o">
  <kap>film<tld/>o</kap>
  <snc mrk="aktor.film0o.KIN">
    <uzo tip="fak">KIN</uzo>
    <dif>
      <ref tip="super" cel="aktor.0o">Aktoro</ref>, kiu rolas en filmoj:
      <ekz>
        kial ne fari&gcirc;i film<tld/>o? &ndash; tion mi ne mal&scirc;atus
        <fnt><bib>InfanTorent2</bib>, <lok>dua parto, &ccirc;apitro
        3a</lok></fnt>;
      </ekz>
      <ekz>
        la konata kaj muskola film<tld/>o Rambo
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470; 10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_n;&c_a;&c_a;&c_k;&c_t;&c_o;&c_r;</trd>
  <trd lng="cs">filmov&yacute; herec</trd>
  <trd lng="de">Filmschauspieler</trd>
  <trdgrp lng="en">
    <trd>film actor</trd>,
    <trd>film actress</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>acteur de cin&eacute;ma</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>filmsz&iacute;n&eacute;sz</trd>,
    <trd>filmm&udblac;v&eacute;sz</trd>
  </trdgrp>
  <trd lng="it">attore<klr> cinematografico</klr></trd>
  <trd lng="ja">&#26144;&#x753B;&#x4FF3;&#x512A;  <pr>&#12360;&#12356;&#x304C;&#x306F;&#12356;&#12422;&#12358;</pr></trd>
  <trd lng="pl">aktor filmowy</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_n;&c_o;&c_a;&c_k;&c_t;&c_jo;&c_r;</trd>,
    <trd>&c_k;&c_i;&c_n;&c_o;&c_a;&c_r;&c_t;&c_i;&c_s;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">filmov&yacute; herec</trd>
  <trd lng="uk">&c_k;&c_ib;&c_n;&c_o;&c_a;&c_k;&c_t;&c_o;&c_r;</trd>
</drv>
</art><!--
