<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: raz.xml,v 1.30 2023/10/20 19:20:11 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>raz</rad>/i
</kap>
<drv mrk="raz.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <dif>
    <ref tip="super" cel="trancx.de0i">Fortran&ccirc;i</ref>
    ion tutproksime &ccirc;e ties radiko:
  </dif>
  <snc mrk="raz.0i.harojn">
    <dif>
      <klr>(pp haroj)</klr>:
      <ekz>
        la flankojn de sia barbo ili ne <tld/>u
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 21:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        fre&scirc;e <tld/>ita barbo;
      </ekz>
      <ekz>
        elvenis anka&ubreve; la patro, kun sapita viza&gcirc;o kaj <ind><tld/>aparato</ind> en la mano
        <fnt>
          <bib>ViV</bib>
          <lok>I.</lok>
        </fnt>.
        <trd lng="de">Rasierapparat</trd>
      </ekz>
    </dif>
    <trd lng="es">afeitar</trd>
    <trd lng="hu">borotv&aacute;l</trd>
    <trd lng="nl">scheren</trd>
  </snc>
  <snc mrk="raz.0i.surfacon">
    <dif>
      <klr>(pp tio, kio portas la harojn)</klr>:
      <ekz>
        <tld/>i la kapon, la vangojn;
      </ekz>
      <ekz>
        li sin <tld/>is kaj &scirc;an&gcirc;is siajn vestojn
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 41:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        la frizisto demandis lin plej humile, &ccirc;u li ordonas <tld/>i la kapon
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; operacio flegistino <tld/>as la subventron de paciento
        <fnt>
          <bib>KrB</bib>
          <lok>Mirinda donaco &ccirc;iela</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ita ronda&jcirc;o sur verto de pastro
        (<ref tip="sin" cel="tonsur.0o">tonsuro</ref>);
      </ekz>
      <ekz>
        <tld/>i iun glate.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>borotv&aacute;l</trd>,
      <trd>ny&iacute;r</trd>
    </trdgrp>
    <trd lng="nl">scheren</trd>
  </snc>
  <snc mrk="raz.0i.herbon">
    <dif>
      <klr>(pp herbo)</klr> <ref tip="dif" cel="tond.0i">Tondi</ref>:
      <ekz>
        <tld/>i la herbon de la &gcirc;ardeno
        <fnt><bib>ItEsp</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="hu">ny&iacute;r</trd>
    <trd lng="nl">maaien</trd>
  </snc>
  <trd lng="ja">&#xFF5E;<ind>&#x306E;&#x6BDB;&#12434;&#x305D;&#x308B; 
  </ind><pr>&#x306E;&#12369;&#12434;&#x305D;&#x308B;</pr></trd>
  <trd lng="lat">rado, ere</trd>
  <trd lng="be">&c_g;&c_a;&c_l;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>holit</trd>,
    <trd>oholit</trd>
  </trdgrp>
  <trd lng="de">rasieren</trd>
  <trd lng="en">shave</trd>
  <trd lng="fr">raser</trd>
  <trd lng="pl">goli&cacute;</trd>
  <trd lng="ru">&c_b;&c_r;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>holi&tcaron;</trd>,
    <trd>oholi&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">raka</trd>
  <trd lng="tr">t&inodot;ra&scedil; etmek</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_o;&c_l;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_r;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="raz.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Ilo kun unu a&ubreve; pluraj akretran&ccirc;aj <ref tip="prt" cel="kling.0o">klingo</ref>j,
      destinita por <tld/>i:
      <ekz>
        simpla, elektra <tld/>ilo;
      </ekz>
      <ekz>
        li rapide alportis <tld/>ilon, sapumis la kapon de la kaprino kaj <tld/>is &scirc;in
        glata
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tablo, la orazeno kaj bastono el sako</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ilo ne devas tu&scirc;i lian kapon, &ccirc;ar konsekrita al Dio estos la knabo
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 13:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; li staris spegulo, kaj kun certa digno li movis <tld/>ilon sur siaj vangoj
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[mi]</klr> nur enpo&scirc;igis <tld/>ilon kaj dentobroson
        <fnt>
          <bib>KrM</bib>
          <lok>28. Tute Libera Kiel Birdo Aera.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en tiu tempo la Eternulo <tld/>os per dungita <tld/>ilo, per la transriveranoj,
        per la re&gcirc;o de Asirio, la kapon kaj la harojn de la piedoj, kaj forigos e&ccirc; la
        barbon
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 7:20</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bo">&t_sa;&t_kata;&t_rata;&t_tsheg;&t_kha;&t_rata;&t_gigu;&t_tsheg;</trd>
  <trdgrp lng="ja">
    <trd>&#21059;&#20992;  <pr>&#x304B;&#x307F;&#x305D;&#x308A;</pr></trd>,
    <trd>&#12402;&#12370;&#21059;&#x308A;&#22120;  <pr>&#12402;&#12370;&#x305D;&#x308A;&#12358;&#12388;&#x308F;</pr></trd>
  </trdgrp>
  <trd lng="lat">nouacula, ae</trd>
  <trd lng="be">&c_b;&c_r;&c_y;&c_t;&c_v;&c_a;</trd>
  <trd lng="cs">holic&iacute; strojek</trd>
  <trdgrp lng="de">
    <trd>Rasiermesser</trd>,
    <trd>Rasierer</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>cuchilla de <ind>afeitar</ind></trd>,
    <trd>maquina de <ind>afeitar</ind></trd>,
    <trd>maquinilla de <ind>afeitar</ind></trd>
  </trdgrp>
  <trd lng="fr">rasoir</trd>
  <trd lng="hu">borotva</trd>
  <trdgrp lng="nl">
    <trd>scheermes</trd>,
    <trd>scheerapparaat</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>golarka</trd>,
    <trd>maszynka do  <ind>golenia</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_r;&c_i;&c_t;&c_v;&c_a;</trd>
  <trd lng="sk">holiaci stroj&ccaron;ek</trd>
  <trd lng="sv">rakhyvel</trd>
  <trdgrp lng="tr">
    <trd>ustura</trd>,
    <trd>t&inodot;ra&scedil; b&inodot;&ccedil;a&gbreve;&inodot;</trd>
  </trdgrp>
  <trd lng="uk">&c_je;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_n;&c_a; &c_b;&c_r;&c_i;&c_t;&c_v;&c_a;</trd>
</drv>

<drv mrk="raz.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="barbir.0o">Barbiro</ref>:
      <ekz>
        <tld/>isto <tld/>iston <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_l;&c_ja;&c_r;</trd>,
    <trd>&c_c;&c_y;&c_r;&c_u;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">barber</trd>
  <trdgrp lng="cs">
    <trd>holi&ccaron;</trd>,
    <trd>raz&iacute;r <klr>(zast.)</klr></trd>
  </trdgrp>
  <trd lng="de">Barbier</trd>
  <trd lng="es">barbero</trd>
  <trd lng="fr">barbier</trd>
  <trd lng="hu">borb&eacute;ly</trd>
  <trd lng="nl">barbier</trd>
  <trd lng="pl">fryzjer</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_r;&c_i;&c_k;&c_m;&c_a;&c_h;&c_je;&c_r;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_d;&c_o;&c_b;&c_r;&c_je;&c_j;</trd>,
    <trd>&c_c;&c_i;&c_r;&c_ju;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">holi&ccaron;</trd>
  <trd lng="sv">barberare</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_je;&c_r;&c_u;&c_k;&c_a;&c_r;</trd>,
    <trd>&c_g;&c_o;&c_l;&c_ja;&c_r;</trd>
  </trdgrp>
</drv>
</art>
