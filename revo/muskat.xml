<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>muskat</rad>/o
</kap>
<drv mrk="muskat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Nukso de <ref tip="malprt" cel="muskat.0arbo"><tld/>ujo</ref>
      uzata kiel spica&jcirc;o.
    </dif>
  </snc>
  <trd lng="de">Muskatnu&szlig;</trd>
<trd lng="nl">muskaatnoot</trd>
<trdgrp lng="pl"><trd>ga&lstroke;ka <ind>muszkato&lstroke;owa</ind></trd>,
  <trd>muszkat</trd></trdgrp>
</drv>
<drv mrk="muskat.0arbo">
  <kap><tld/>ujo, <tld/>arbo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="mirist.0o">miristiko</ref>
      (<trd lng="la">Miristica fragrans</trd>),
      &ccirc;iam verda arbo kun persiksimila frukto, kiu entenas
      la <tld/>on.
    </dif>
  </snc>
  <trd lng="de">Muskatnu&szlig;baum</trd>
<trd lng="nl">muskaatboom</trd>
</drv>
<drv mrk="muskat.0vino">
  <kap><tld/>vino</kap>
  <snc>
    <dif>
      Vino farita el apartaj specoj de vinberoj, kun gusto tre agrabla,
      iomete simila al tiu de <tld/>o a&ubreve; de mosko.
    </dif>
  </snc>
  <trd lng="de">Muskateller</trd>
<trd lng="nl">muskaatwijn</trd>
</drv>
</art>
