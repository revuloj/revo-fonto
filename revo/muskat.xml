<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: muskat.xml,v 1.30 2024/01/26 19:05:54 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>muskat</rad>/o
</kap>
<drv mrk="muskat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="muskat.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Fruktokerno de la 
      <ref tip="malprt" cel="muskat.0arbo"><tld/>ujo</ref>
      uzata kiel <ref tip="lst" cel="spic.0o" lst="voko:spicoj">spico</ref>:
      <ekz>
        vino, saturita per <tld/>o kaj bonodoraj herboj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i fine triumfe <klr>[&#x2026;]</klr> ekfrapis per la kribrilo de <tld/>o
        <fnt>
          <bib>BdV</bib>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/8/8b/Nutmeg_p1160003.jpg/320px-Nutmeg_p1160003.jpg" prm="CC-BY-SA-3.0"><tld/>o
        <fnt>
          <aut>David Monniaux, propra verko</aut>
          <lok><url ref="&WCW;/File:Nutmeg_p1160003.jpg">&WCW;/File:Nutmeg_p1160003.jpg</url></lok>
        </fnt>
    </bld>
    <bld lok="&WCU;/thumb/e/e6/Myristica_fragrans-001.jpg/320px-Myristica_fragrans-001.jpg" prm="CC-BY-SA-3.0">fre&scirc;a <tld/>o ene de la frukto 
    
        <fnt>
          <aut>Brocken Inaglory, Amada44</aut>
          <lok><url ref="&WCW;/File:Myristica_fragrans-001.jpg">&WCW;/File:Myristica_fragrans-001.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_s;&c_k;&c_a;&c_t;&c_n;&c_y; &c_a;&c_r;&c_e;&c_h;</trd>
  <trd lng="ca"><ind>nou</ind> moscada</trd>
  <trd lng="cs">mu&scaron;k&aacute;tov&yacute; o&rcaron;&iacute;&scaron;ek</trd>
  <trd lng="de">Muskatnuss</trd>
  <trd lng="en">nutmeg</trd>
  <trd lng="fr">muscade</trd>
  <trd lng="hu">szerecsendi&oacute;</trd>
  <trd lng="id">pala</trd>
  <trdgrp lng="ja">
    <trd>&#x30CA;&#x30C4;&#x30E1;&#x30B0;</trd>,
    <trd>&#x30CB;&#x30AF;&#x30BA;&#x30AF;</trd>
  </trdgrp>
  <trd lng="nl">muskaatnoot</trd>
  <trdgrp lng="pl">
    <trd>ga&lstroke;ka <ind>muszkato&lstroke;owa</ind></trd>,
    <trd>muszkat</trd>
  </trdgrp>
  <trd lng="pt">noz-moscada</trd>
  <trd lng="ru">&c_m;&c_u;&c_s;&c_k;&c_a;&c_t;&c_n;&c_y;&c_j; &c_o;&c_r;&c_je;&c_h;</trd>
  <trd lng="sk">mu&scaron;k&aacute;tov&yacute; orech</trd>
  <trd lng="uk">&#x43C;&#x443;&#x441;&#x43A;&#x430;&#x442;&#x43D;&#x438;&#x439; &#x433;&#x43E;&#x440;&#x456;&#x445;</trd>
  <trd lng="zh">&#x8089;&#x8C46;&#x853B; <pr>r&#xF2;ud&#xF2;uk&#xF2;u</pr></trd>
</drv>

<drv mrk="muskat.0arbo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="muskat.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      &Ccirc;iam verda arbo kun persiksimila kernofrukto, kiu entenas
      la <tld/>on, <ref tip="lst" cel="speci.0o.BIO" lst="voko:botanikaj_specioj">specio</ref> de <ref tip="super" cel="mirist.0o">miristiko</ref>
      (<trd lng="la">Myristica fragrans</trd>):
      <ekz>
        La angloj sukcesis kultivi <tld/>arbojn sur Grenado
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=447542">Maciso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/a/a0/Musskatnuss_-_Myristica_fragrans_(8683133297).jpg/640px-Musskatnuss_-_Myristica_fragrans_(8683133297).jpg" prm="CC BY-SA 2.0">bran&ccirc;oj de <tld/>ujo (Myristica fragrans)
    
        <fnt>
          <aut>gbohne el Berlino, Germanujo</aut>
          <lok><url ref="&WCW;/File:Musskatnuss_-_Myristica_fragrans_(8683133297).jpg">&WCW;/File:Musskatnuss_-_Myristica_fragrans_(8683133297).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_s;&c_k;&c_a;&c_t;&c_n;&c_a;&c_je; &c_d;&c_r;&c_e;&c_v;&c_a;</trd>
  <trd lng="ca">arbre de la <ind>nou</ind> moscada</trd>
  <trd lng="cs">mu&scaron;k&aacute;tovn&iacute;k</trd>
  <trd lng="de">Muskatnussbaum</trd>
  <trd lng="en">nutmeg tree</trd>
  <trd lng="fr">muscadier</trd>
  <trd lng="hu">szerecsendi&oacute;fa</trd>
  <trd lng="ja">&#x30CB;&#x30AF;&#x30BA;&#x30AF;</trd>
  <trd lng="nl">muskaatboom</trd>
  <trd lng="ru">&c_m;&c_u;&c_s;&c_k;&c_a;&c_t;&c_n;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  <trd lng="sk">mu&scaron;k&aacute;tovn&iacute;k</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x443;&#x441;&#x43A;&#x430;&#x442;&#x43D;&#x438;&#x43A;</trd>,
    <trd>&#x43C;&#x443;&#x441;&#x43A;&#x430;&#x442;&#x43D;&#x435; &#x434;&#x435;&#x440;&#x435;&#x432;&#x43E;</trd>
  </trdgrp>
  <trd lng="zh">&#x8089;&#x8C46;&#x853B; <pr>r&#xF2;ud&#xF2;uk&#xF2;u</pr></trd>
</drv>

<drv mrk="muskat.0vino">
  <kap><tld/>vino</kap>
  <snc mrk="muskat.0vino.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="vin.0o">Vino</ref> farita el
      apartaj specoj de vinberoj, kun gusto tre agrabla,
      iomete simila al tiu de <tld/>o a&ubreve; de mosko:
      <ekz>
        fama tre dol&ccirc;a <tld/>vino estas farata sur Samoso
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=93936">Samoso (insulo)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_u;&c_s;&c_k;&c_a;&c_t;</trd>,
    <trd>&c_m;&c_u;&c_s;&c_k;&c_a;&c_t;&c_n;&c_a;&c_je; &c_v;&c_ib;&c_n;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>vi de moscatell</trd>,
    <trd>vi moscat</trd>
  </trdgrp>
  <trd lng="de">Muskateller</trd>
  <trd lng="en">muscat<klr> (wine)</klr></trd>
  <trd lng="fr">muscat<klr> (vin)</klr></trd>
  <trd lng="hu">muskot&aacute;ly</trd>
  <trd lng="ja">&#x30DE;&#x30B9;&#x30AB;&#x30C3;&#x30C8;&#x30EF;&#x30A4;&#x30F3;</trd>
  <trd lng="nl">muskaatwijn</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_u;&c_s;&c_k;&c_a;&c_t;&c_n;&c_o;&c_je; &c_v;&c_i;&c_n;&c_o;</trd>,
    <trd>&c_m;&c_u;&c_s;&c_k;&c_a;&c_t;</trd>
  </trdgrp>
  <trd lng="uk">&#x43C;&#x443;&#x441;&#x43A;&#x430;&#x442;&#x43D;&#x435; &#x432;&#x438;&#x43D;&#x43E;</trd>
  <trd lng="zh">&#x9E9D;&#x9999;&#x8461;&#x8404;&#x9152; <pr>sh&#xE8;xi&#x101;ngp&#xFA;t&#xE1;oji&#x1D4;</pr></trd>
</drv>
</art>
