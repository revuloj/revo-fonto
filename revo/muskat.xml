<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>muskat</rad>/o
</kap>
<drv mrk="muskat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Fruktokerno de la <ref tip="malprt" cel="muskat.0arbo"><tld/>ujo</ref>
      uzata kiel <ref tip="super" cel="spic.0o">spico</ref>.
    </dif>
  </snc>
  <trd lng="de">Muskatnu&szlig;</trd>
  <trd lng="hu">szerecsendi&oacute;</trd>
  <trd lng="nl">muskaatnoot</trd>
  <trdgrp lng="pl">
    <trd>ga&lstroke;ka <ind>muszkato&lstroke;owa</ind></trd>,
    <trd>muszkat</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_u;&c_s;&c_k;&c_a;&c_t;&c_n;&c_y;&c_j;
  &c_o;&c_r;&c_je;&c_h;</trd>
</drv>

<drv mrk="muskat.0arbo">
  <kap><tld/>ujo, <tld/>arbo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="mirist.0o">miristiko</ref>
      (<trd lng="la">Miristica fragrans</trd>),
      &ccirc;iam verda arbo kun persiksimila frukto, kiu entenas
      la <tld/>on.
    </dif>
  </snc>
  <trd lng="de">Muskatnu&szlig;baum</trd>
  <trd lng="hu">szerecsendi&oacute;fa</trd>
  <trd lng="nl">muskaatboom</trd>
  <trd lng="ru">&c_m;&c_u;&c_s;&c_k;&c_a;&c_t;&c_n;&c_o;&c_je;
  &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
</drv>

<drv mrk="muskat.0vino">
  <kap><tld/>vino</kap>
  <snc>
    <dif>
      <ref tip="super" cel="vin.0o">Vino</ref> farita el 
      apartaj specoj de vinberoj, kun gusto tre agrabla,
      iomete simila al tiu de <tld/>o a&ubreve; de mosko.
    </dif>
  </snc>
  <trd lng="de">Muskateller</trd>
  <trd lng="hu">muskot&aacute;ly</trd>
  <trd lng="nl">muskaatwijn</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_u;&c_s;&c_k;&c_a;&c_t;&c_n;&c_o;&c_je;
    &c_v;&c_i;&c_n;&c_o;</trd>,
    <trd>&c_m;&c_u;&c_s;&c_k;&c_a;&c_t;</trd>
  </trdgrp>
</drv>
</art>
