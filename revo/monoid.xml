<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>monoid</rad>/o
</kap>
<drv mrk="monoid.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      <ref cel="asoci.0eca.magmo">Asocieca</ref>
      <ref tip="super" cel="magm.0o.MAT">magmo</ref> kun
      <ref cel="neuxtr.0aelemento">ne&ubreve;tra elemento</ref>:
      <ekz>
        la aro de &ccirc;iuj subaroj de <frm><g>E</g></frm>, konsiderata kune
        kun la operacio <ref cel="komun.0ajxo.aro">komuna&jcirc;o</ref>
        estas <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="super">
      <ref cel="grup.duon0o.MAT">duongrupo</ref>,
      <ref cel="strukt.algebra0o">algebra strukturo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Monoid(e)</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>monoid</trd>
  </trdgrp>
  <trd lng="es">monoide</trd>
  <trdgrp lng="fr">
    <trd>mono&iuml;de</trd>
  </trdgrp>
  <trd lng="pl">monoid</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_n;&c_o;&c_i;&c_d;</trd>
  </trdgrp>
</drv>
</art>
