<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>monoid</rad>/o
</kap>
<drv mrk="monoid.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      <ref cel="asoci.0eca.magmo">Asocieca</ref>
      <ref tip="super" cel="magm.0o.MAT">magmo</ref> kun
      <ref cel="neuxtr.0aelemento">ne&ubreve;tra elemento</ref>:
      <ekz>
        la aro de &ccirc;iuj subaroj de <em>E</em>, konsiderata kune
        kun la operacio <ref cel="komun.0ajxo.aro">komuna&jcirc;o</ref>
        estas <tld/>o.
      </ekz>
    </dif>
    <ref tip="sin" cel="grup.duon0o.MAT">duongrupo;</ref>
    <ref tip="super" cel="strukt.algebra0o">algebra strukturo.</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Monoid(e)</trd>,
    <trd>Halbgruppe</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>monoid</trd>,
    <trd>semigroup</trd>
  </trdgrp>
  <trd lng="es">monoide</trd>
  <trdgrp lng="fr">
    <trd>mono&iuml;de</trd>,
    <trd>demi-groupe</trd>,
    <trd>semi-groupe</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_n;&c_o;&c_i;&c_d;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_u;&c_g;&c_r;&c_u;&c_p;&c_p;&c_a;</trd>
  </trdgrp>
</drv>
</art>
