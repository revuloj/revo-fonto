<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: monoid.xml,v 1.15 2019/07/16 16:10:20 revo Exp $">
<kap>
  <rad>monoid</rad>/o <fnt><bib>MatVort</bib></fnt>,
  <var><kap><rad>monojd</rad>/o <fnt><bib>KompLeks</bib></fnt></kap></var>
</kap>
<drv mrk="monoid.0o">
  <kap><tld/>o,
    <var><kap>monojdo</kap></var>
  </kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <ref cel="asoci.0eca.grupoido">Asocieca</ref>
      <ref tip="super" cel="grupoi.0o">grupoido</ref> kun
      <ref cel="neuxtr.0aelemento">ne&ubreve;tra elemento</ref>:
      <ekz>
        la aro de &ccirc;iuj subaroj de <frm><g>E</g></frm>, konsiderata kune
        kun la operacio <ref cel="komun.0ajxo.aro">komuna&jcirc;o</ref>
        estas <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="super">
      <ref cel="grup.duon0o.MAT">duongrupo</ref>,
      <ref cel="strukt.algebra0o">algebra strukturo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_o;&c_ib;&c_d;</trd>
  <trdgrp lng="ca">
    <trd>monoide</trd>,
    <trd>semigrup</trd>
  </trdgrp>
  <trd lng="cs">monoid</trd>
  <trdgrp lng="de">
    <trd>Monoid[e]</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>monoid</trd>
  </trdgrp>
  <trd lng="es">monoide</trd>
  <trd lng="fa">&a_m;&a_w;&a_n;&a_w;&a_y_hamza;&f_y;&a_d;</trd>
  <trdgrp lng="fr">
    <trd>mono&iuml;de</trd>
  </trdgrp>
  <trd lng="hu">monoid</trd>
  <trd lng="pl">monoid</trd>
  <trd lng="pt">mon&oacute;ide</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_n;&c_o;&c_i;&c_d;</trd>
  </trdgrp>
  <trd lng="sk">monoid</trd>
</drv>
</art>
