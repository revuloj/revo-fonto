<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kanab</rad>/o
</kap>
<drv mrk="kanab.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kanab.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super"
      cel="kanaba.0oj"><tld/>acoj</ref>
      (<trd lng="la">Cannabis</trd>), alta herbo, el kies fadenoj
      oni faras teksa&jcirc;ojn a&ubreve; &scirc;nurojn.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_o;&c_p;&c_l;&c_ib;</trd>   
  <trd lng="br">kanab</trd>
  <trd lng="de">Hanf</trd>
  <trdgrp lng="en">
    <trd>cannabis</trd>,
    <trd>hemp</trd>
  </trdgrp>
  <trd lng="fa">&a_sh;&a_A;&a_h;&a_d;&a_A;&a_n;&a_h;</trd>
  <trd lng="fr">chanvre</trd>
  <trd lng="hu">kender</trd>
  <trdgrp lng="nl">
    <trd>hennep</trd>,
    <trd>cannabis</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_n;&c_o;&c_p;&c_l;&c_ja;</trd>,
    <trd>&c_p;&c_je;&c_n;&c_mol;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sv">hampa</trd>
</drv>
</art>
