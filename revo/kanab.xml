<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>kanab</rad>/o
</kap>
<drv mrk="kanab.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="kanab.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Cannabis</trd>), alta herbo, el kies fadenoj
      oni faras teksa&jcirc;ojn a&ubreve; &scirc;nurojn.
    </dif>
  </snc>
  <trd lng="de">Hanf</trd>
  <trd lng="fr">lin</trd>
</drv>
<drv mrk="kanab.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Cannabaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Hanfgew&auml;chse</trd>
</drv>
</art>
