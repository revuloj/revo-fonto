<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kanab.xml,v 1.33 2023/12/03 08:00:21 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kanab</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kanab.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kanab.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Alta herbo, el kies fibroj oni faras teksa&jcirc;ojn a&ubreve; &scirc;nurojn,
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref> 
      el familio <ref tip="malprt" cel="kanaba.0oj"><tld/>acoj</ref>
      (<trd lng="la">Cannabis</trd>):
      <ekz>
        Sarkas knabino <tld/>on // Apud &gcirc;ardeno sinjora
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi faris lampon, per kaprograso metita en argilujon, kaj peceto da <tld/>o kiel
        me&ccirc;o
        <fnt>
          <bib>Rob</bib>
        </fnt>;
      </ekz>
      <ekz>
        tie kreskas hordeo, milio, maizo, fabo, lento...
        kaj anka&ubreve; <tld/>o, kies elsekigitajn foliojn oni
        fumas en pipetoj
        <fnt>
          <aut>J. Ribillard</aut>,
          <vrk>Vagado sub palmoj</vrk>,
          <lok>Rotterdam, 1956</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="lin.0o">lino</ref>
    </refgrp>
    <bld lok="&WCU;/thumb/1/12/Cannabis_sativa_(K&ouml;hler).jpg/360px-Cannabis_sativa_(K&ouml;hler).jpg" prm="PD">botanika tabulo de <tld/>o (Cannabis sativa)
        <fnt>
          <aut>Hermann Adolph K&ouml;hler (1834 - 1879)</aut>
          <lok><url ref="&WCW;/File:Cannabis_sativa_(K%C3%B6hler).jpg">&WCW;/File:Cannabis_sativa_(K&ouml;hler).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_o;&c_p;&c_l;&c_ib;</trd>
  <trd lng="bg">&c_k;&c_o;&c_n;&c_o;&c_p;</trd>
  <trd lng="br">kanab</trd>
  <trd lng="cs">konop&iacute;</trd>
  <trd lng="de">Hanf</trd>
  <trdgrp lng="en">
    <trd>cannabis</trd>,
    <trd>hemp</trd>
  </trdgrp>
  <trd lng="es">cannabis</trd>
  <trd lng="fa">&a_sh;&a_A;&a_h;&a_d;&a_A;&a_n;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>cannabis</trd>,
    <trd>chanvre</trd>
  </trdgrp>
  <trd lng="hu">kender</trd>
  <trdgrp lng="ja">
    <trd>&#x30A2;&#x30B5;</trd>,
    <trd>&#x30BF;&#x30A4;&#x30DE;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>hennep</trd>,
    <trd>cannabis</trd>
  </trdgrp>
  <trd lng="pl">konopie</trd>
  <trd lng="ro">c&acirc;nep&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_n;&c_o;&c_p;&c_l;&c_ja;</trd>,
    <trd>&c_p;&c_je;&c_n;&c_mol;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">konopa</trd>
  <trd lng="sv">hampa</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&#x43D;&#x43E;&#x43F;&#x43B;&#x456;</trd>,
    <trd>&#x43F;&#x440;&#x44F;&#x434;&#x438;&#x432;&#x43E;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5927;&#x9EBB; <pr>d&#xE0;m&#xE1;</pr></trd>,
    <trd>&#x7E15; <pr>y&#xF9;n</pr></trd>,
    <trd>&#x82F4; <pr>j&#x16B;</pr></trd>
  </trdgrp>
</drv>
</art>
