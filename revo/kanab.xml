<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kanab.xml,v 1.28 2017/08/03 19:10:13 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kanab</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kanab.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kanab.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Alta herbo, el kies fadenoj oni faras teksa&jcirc;ojn a&ubreve; &scirc;nurojn,
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt"
      cel="kanaba.0oj"><tld/>acoj</ref>
      (<trd lng="la">Cannabis</trd>):
      <ekz>
        tie kreskas hordeo, milio, maizo, fabo, lento...
        kaj anka&ubreve; <tld/>o, kies elsekigitajn foliojn oni
        fumas en pipetoj
        <fnt>
          <aut>J. Ribillard</aut>,
          <vrk>Vagado sub palmoj</vrk>,
          <lok>Rotterdam, 1956</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_o;&c_p;&c_l;&c_ib;</trd>
  <trd lng="bg">&c_k;&c_o;&c_n;&c_o;&c_p;</trd>
  <trd lng="br">kanab</trd>
  <trd lng="cs">konop&iacute;</trd>
  <trd lng="de">Hanf</trd>
  <trdgrp lng="en">
    <trd>cannabis</trd>,
    <trd>hemp</trd>
  </trdgrp>
  <trd lng="es">cannabis</trd>
  <trd lng="fa">&a_sh;&a_A;&a_h;&a_d;&a_A;&a_n;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>cannabis</trd>,
    <trd>chanvre</trd>
  </trdgrp>
  <trd lng="hu">kender</trd>
  <trdgrp lng="nl">
    <trd>hennep</trd>,
    <trd>cannabis</trd>
  </trdgrp>
  <trd lng="pl">konopie</trd>
  <trd lng="ro">c&acirc;nep&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_n;&c_o;&c_p;&c_l;&c_ja;</trd>,
    <trd>&c_p;&c_je;&c_n;&c_mol;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">konopa</trd>
  <trd lng="sv">hampa</trd>
</drv>
</art>
