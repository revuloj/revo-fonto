<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kanab</rad>/o
</kap>
<drv mrk="kanab.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="kanaba.0oj"><tld/>acoj</ref>
      (<trd lng="la">Cannabis</trd>), alta herbo, el kies fadenoj
      oni faras teksa&jcirc;ojn a&ubreve; &scirc;nurojn.
    </dif>
  </snc>
  <trd lng="de">Hanf</trd>
  <trd lng="en">hemp</trd>
  <trd lng="fr">chanvre</trd>
  <trd lng="hu">kender</trd>
  <trdgrp lng="nl">
    <trd>hennep</trd>,
    <trd>cannabis</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_n;&c_o;&c_p;&c_l;&c_ja;</trd>,
    <trd>&c_p;&c_je;&c_n;&c_mol;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sv">hampa</trd>
</drv>
</art>
