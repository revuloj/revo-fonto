<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gajl.xml,v 1.23 2022/04/30 08:42:40 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>gajl</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="gajl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gajl.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="tuber.0o.sxvelajxo">Tubero</ref>
      pro nenormala kreskado de vegeta&jcirc;a histo, ka&ubreve;zata de iuj
      parazitoj:
      <ekz>
        tamariko <klr>[&#x2026;]</klr> havas ofte <tld/>ojn pro piko de
        &scirc;ildla&ubreve;so
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=33751">Tamariko</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        el <tld/>oj <klr>(kverkaj)</klr> oni produktas inkon
        <fnt><bib>VdE</bib>, <lok/></fnt>;
      </ekz>
      <ekz>
        <klr>[&ccirc;e]</klr> <nom>Mikiola fagi</nom> <klr>[&#x2026;]</klr> nur unu
        ulo &ccirc;eestas en <tld/>o, se la
        ulo estas ino, la <tld/>o estas dika, se ne, &gcirc;i estas maldika
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=336235">Mikiola fagi</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/4/43/Fagus_sylvatica2.jpg/640px-Fagus_sylvatica2.jpg" prm="CC-BY-SA-3.0"><tld/>oj sur fagofolio
        <fnt>
          <aut>Kurt St&uuml;ber</aut>
          <lok><url ref="&WCW;/File:Fagus_sylvatica2.jpg">&WCW;/File:Fagus_sylvatica2.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="ca">
    <trd>cassanella</trd>,
    <trd>gala</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dub&ecaron;nka</trd>,
    <trd>h&aacute;lka</trd>
  </trdgrp>
  <trdgrp lng="de">
   <trd>Galle</trd>,
   <trd>Gallapfel</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>gall</trd>,
    <trd>oak-apple</trd>,
    <trd>oak apple</trd>
  </trdgrp>
  <trd lng="fa">&a_m;&a_A;&a_z;&a_w;</trd>
  <trdgrp lng="fr">
    <trd>galle</trd>,
    <trd>noix de galle</trd>
  </trdgrp>
  <trd lng="hu">gubacs</trd>
  <trdgrp lng="nl">
    <trd>gal<klr tip="ind"> (galnoot)</klr></trd>,
    <trd>galnoot</trd>,
    <trd>galappel</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>(noz-de-)galha</trd>,
    <trd>bugalho</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_r;&c_n;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_o;&c_r;&c_je;&c_sh;&c_je;&c_k;</trd>,
    <trd>&c_d;&c_u;&c_b;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_o;&c_r;&c_je;&c_sh;&c_je;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">dubienka <klr>(bot)</klr></trd>
  <trd lng="sv">gall&auml;pple</trd>
  <trdgrp lng="uk">
    <trd>&#x433;&#x430;&#x43B;</trd>,
    <trd>&#x447;&#x43E;&#x440;&#x43D;&#x438;&#x43B;&#x44C;&#x43D;&#x438;&#x439; &#x433;&#x43E;&#x440;&#x456;&#x448;&#x43E;&#x43A;</trd>,
    <trd>&#x434;&#x443;&#x431;&#x43E;&#x432;&#x435;&#xA0;&#x44F;&#x431;&#x43B;&#x443;&#x447;&#x43A;&#x43E;</trd>
  </trdgrp>
</drv>
</art>
