<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>gajl</rad>/o
</kap>
<drv mrk="gajl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Tubero kreskanta sur la folioj de kelkaj vegeta&jcirc;oj,
      precipe de kverko k rozujo, k ka&ubreve;zita de piko de insekto.  
    </dif>
  </snc>
  <trdgrp lng="de">
   <trd>Galle</trd>,
   <trd>Gallapfel</trd></trdgrp>
  <trd lng="en">gall</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_r;&c_n;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_o;&c_r;&c_je;&c_sh;&c_je;&c_k;</trd>,
    <trd>&c_d;&c_u;&c_b;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_o;&c_r;&c_je;&c_sh;&c_je;&c_k;</trd>
  </trdgrp>
</drv>
</art>
