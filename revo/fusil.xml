<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>fusil</rad>/o
</kap>
<drv mrk="fusil.0o">
<kap><tld/>o</kap>
<snc>
<dif>
Portebla <ref tip="super" cel="paf.pulvo0ilo">pulvopafilo</ref>, 
konsistanta el ligna <ref tip="sub" cel="afust.0o">afusteto</ref> k 
&scirc;tala <ref tip="sub" cel="tub.0o">tubo</ref>, pli longa ol tiu de 
karabeno: 
<ekz>militista, &ccirc;asa <tld/>o.
</ekz>
</dif>
<refgrp tip="vid">
<ref cel="karabe.0o">karabeno</ref>,
<ref cel="paf.0ilo">pafilo</ref>.
</refgrp>
</snc>
<trd lng="fr">fusil</trd>
</drv>
<drv mrk="fusil.0isto">
<kap><tld/>isto</kap>
<uzo tip="fak">MIL</uzo>
<snc>
<dif>
Soldato, armita per <tld/>o. 
</dif>
<refgrp tip="sin">
<ref cel="fuzili.0o">fuziliero</ref>.
</refgrp>
</snc>
<trd lng="fr">fusillier</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
