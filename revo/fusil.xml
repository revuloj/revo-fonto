<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>fusil</rad>/o
</kap>
<drv mrk="fusil.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Portebla <ref tip="super" cel="paf.pulvo0ilo">pulvopafilo</ref>, 
      konsistanta el ligna <ref tip="sub" cel="afust.0o">afusteto</ref> k 
      &scirc;tala <ref tip="sub" cel="tub.0o">tubo</ref>, pli longa ol tiu de 
      karabeno: 
      <ekz>militista, &ccirc;asa <tld/>o.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="karabe.0o">karabeno</ref>,
      <ref cel="paf.0ilo">pafilo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Flinte</trd>,
     <trd>B&uuml;chse</trd></trdgrp>
  <trd lng="fr">fusil</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_u;&c_zh;&c_mol;&c_jo;</trd>,
    <trd>&c_v;&c_i;&c_n;&c_t;&c_o;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
<drv mrk="fusil.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Soldato, armita per <tld/>o. 
    </dif>
    <refgrp tip="sin">
      <ref cel="fuzili.0o">fuziliero</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Sch&uuml;tze</trd>,
     <trd>F&uuml;silier</trd></trdgrp>
  <trd lng="fr">fusilier</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_je;&c_l;&c_o;&c_k;</trd>
</drv>
</art>

