<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pupil.xml,v 1.19 2019/04/10 20:10:14 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>pupil</rad>/o
</kap>
<drv mrk="pupil.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="pupil.0o.ANA">
     <uzo tip="fak">ANA</uzo>
     <dif>
       Aperturo en mezo de la iriso, tra kiu la lumradioj penetras en
       <ref tip="malprt" cel="okul.0o.ANA">okulon<sncref/></ref>:
       <ekz>
         gardu min kiel la <tld/>on de la okulo
         <fnt>
           <bib>MT</bib>
           <lok>&Psa; 17:8</lok>
         </fnt>.
       </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_z;&c_r;&c_e;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>panenka</trd>,
    <trd>pupila</trd>,
    <trd>zornice</trd>,
    <trd>z&rcaron;&iacute;telnice</trd>
  </trdgrp>
  <trd lng="de">Pupille</trd>
  <trd lng="en">pupil</trd>
  <trdgrp lng="fr">
    <trd>pupille</trd>,
    <trd>prunelle<klr> (pupille)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>szembog&aacute;r</trd>,
     <trd>pupilla</trd>
   </trdgrp>
  <trd lng="id">pupil</trd>
  <trd lng="it">pupilla</trd>
  <trdgrp lng="nl">
     <trd>pupil</trd>
   </trdgrp>
  <trd lng="pl">&zacute;renica</trd>
  <trdgrp lng="pt">
   <trd>pupila</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_r;&c_a;&c_ch;&c_o;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>panenka</trd>,
    <trd>pupila</trd>,
    <trd>zornica</trd>,
    <trd>zrenica</trd>
  </trdgrp>
  <trd lng="sv">pupill</trd>
</drv>
</art>
