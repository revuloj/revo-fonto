<art mrk="$Id: pupil.xml,v 1.23 2024/02/17 12:12:49 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>pupil</rad>/o
</kap>
<drv mrk="pupil.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="pupil.0o.ANA">
     <uzo tip="fak">ANA</uzo>
     <dif>
       Aperturo en mezo de la iriso, tra kiu la lumradioj penetras en
       <ref tip="malprt" cel="okul.0o.ANA">okulon<sncref/></ref>:
       <ekz>
         gardu min kiel la <tld/>on de la okulo
         <fnt>
           <bib>MT</bib>
           <lok>&Psa; 17:8</lok>
         </fnt>.
       </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_z;&c_r;&c_e;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>panenka</trd>,
    <trd>pupila</trd>,
    <trd>zornice</trd>,
    <trd>z&rcaron;&iacute;telnice</trd>
  </trdgrp>
  <trd lng="de">Pupille</trd>
  <trd lng="en">pupil</trd>
  <trdgrp lng="fr">
    <trd>pupille</trd>,
    <trd>prunelle<klr> (pupille)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>szembog&aacute;r</trd>,
     <trd>pupilla</trd>
   </trdgrp>
  <trd lng="id">pupil</trd>
  <trd lng="it">pupilla</trd>
  <trdgrp lng="ja">
    <trd>&#x77B3;&#x5B54;  <pr>&#x3069;&#x3046;&#x3053;&#x3046;</pr></trd>,
    <trd>&#x3072;&#x3068;&#x307F;</trd>,
    <trd>&#x5927;&#x5207;&#x306A;&#x304B;&#x308F;&#x3044;&#x3044;&#x3082;&#x306E;  <pr>&#x305F;&#x3044;&#x305B;&#x3064;&#x306A;&#x304B;&#x308F;&#x3044;&#x3044;&#x3082;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
     <trd>pupil</trd>
   </trdgrp>
  <trd lng="pl">&zacute;renica</trd>
  <trdgrp lng="pt">
   <trd>pupila</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_r;&c_a;&c_ch;&c_o;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>panenka</trd>,
    <trd>pupila</trd>,
    <trd>zornica</trd>,
    <trd>zrenica</trd>
  </trdgrp>
  <trd lng="sv">pupill</trd>
  <trd lng="uk">&#x437;&#x456;&#x43D;&#x438;&#x446;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x77B3; <pr>t&#xF3;ng</pr></trd>,
    <trd>&#x77B3;&#x5B54; <pr>t&#xF3;ngk&#x1D2;ng</pr></trd>,
    <trd>&#x7738; <pr>m&#xF3;u</pr></trd>,
    <trd>&#x7738;&#x5B50; <pr>m&#xF3;uzi</pr></trd>,
    <trd>&#x77B3;&#x4EC1; <pr>t&#xF3;ngr&#xE9;n</pr></trd>,
    <trd>&#x9ED1;&#x773C;&#x73E0; <pr>h&#x113;iy&#x1CE;nzh&#x16B;</pr></trd>,
    <trd>&#x77B3;&#x4EBA; <pr>t&#xF3;ngr&#xE9;n</pr></trd>
  </trdgrp>
</drv>
</art>
