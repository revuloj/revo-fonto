<kap>
  <rad>spros</rad>/o <fnt><bib>Fab4</bib></fnt>
</kap>
<drv mrk="spros.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="spros.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="dif" cel="sxos.0o.BOT">&Scirc;oso</ref>:
      <ekz>
        la pizo povas eligi florantan <tld/>on
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni uzu prefere unujaran <tld/>on, kies &scirc;elo da&ubreve;re estas sufi&ccirc;e
        maldika kaj supla
        <fnt>
          <aut>J. Roux</aut>,
          <vrk><url ref="&STEB;agrikulturo/greftado/greftado.htm">
          Greftado</url></vrk>,
          <lok>La Domo, 2007-01 (13)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>&#285;ermo</ke>, <ke>&#285;ermoplanto</ke>, <ke>plantido</ke> .
  </rim>

  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_s;&c_t;&c_a;&c_k;</trd>
  <trd lng="cs">v&yacute;honek</trd>
  <trdgrp lng="de">
    <trd>Sprosse</trd>,
    <trd>Keimling</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>latoro&sacute;l</trd>,
    <trd>p&eogonek;d<klr>  (odrost)</klr></trd>
  </trdgrp>
  <trd lng="sk">v&yacute;honok</trd>
  <trd lng="uk">&c_t;&c_r;&c_ib;&c_s;&#1083;&c_a; &c_b;&c_r;&c_u;&#1085;&#1100;&#1082;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#26753;&#29366;&#32467;&#26500; <pr>li&#225;ngzhu&#224;ngji&#233;g&#242;u</pr></trd>,
    <trd>&#26753;&#29376;&#32080;&#27083; <pr>li&#225;ngzhu&#224;ngji&#233;g&#242;u</pr></trd>,
    <trd>&#31207;&#33495; <pr>y&amacron;ngmi&#225;o</pr></trd>,
    <trd>&#23273;&#26525; <pr>n&#232;nzh&#299;</pr></trd>,
    <trd>&#23273;&#33469; <pr>n&#232;ny&#225;</pr></trd>
  </trdgrp>
</drv>
