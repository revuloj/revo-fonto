<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: spared.xml,v 1.8 2023/10/05 18:31:47 revo Exp $">
<kap>
    <rad>spared</rad>/oj <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="spared.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="spared.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo de
      <ref tip="malprt" cel="perk.0oformaj">perkoformaj</ref> fi&scirc;oj
      (<trd lng="la">Sparidae</trd>),
      ofte duseksaj a&ubreve; samtempe a&ubreve; sinsekve:
      <ekz>
        <tld/>oj <klr>[...]</klr> trovi&gcirc;as en neprofundaj moderklimataj
        kaj
        tropikaj akvoj kaj estas fundolo&gcirc;antaj karnovoruloj
        <fnt><bib>Viki</bib>, <lok>Sparedoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_s;&c_k;&c_ib;&c_ja; &c_k;&c_a;&c_r;&c_a;&c_s;&c_ib; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  <trd lng="de">Meerbrassen</trd>
  <trdgrp lng="en">
    <trd>Sparidae</trd>,
    <trd>porgies</trd>, <trd>seabreams</trd>
  </trdgrp>
  <trd lng="fr">Sparid&eacute;s</trd>
  <trd lng="pl">pra&zdot;mowate</trd>
  <trd lng="uk">&c_s;&#x43F;&c_a;&c_r;&#x43E;&c_v;&c_ib;</trd>
</drv>
</art>

