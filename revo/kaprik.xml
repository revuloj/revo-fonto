<vortaro>
<art mrk="$Id$">
<kap>
<rad>Kaprikorn</rad>/o
</kap>
<drv mrk="kaprik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Stelaro kaj signo de la <ref tip="malprt" cel="zodiak.0o">zodiako</ref>
      (<trd lng="la">Capricornus</trd>).
    </dif>
  </snc>
  <trd lng="de">Steinbock</trd>
  <trd lng="nl">steenbok</trd>
  <trd lng="pl">kozioro&zdot;ec</trd>
</drv>
</art>
