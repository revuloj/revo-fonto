<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kaprik.xml,v 1.22 2022/09/24 19:28:25 revo Exp $">
<kap>
  <rad>Kaprikorn</rad>/o
</kap>
<drv mrk="kaprik.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">AST</uzo>
  <mlg>Cap</mlg>
  <mlg>&#9809;&#xFE0E;</mlg>
  <snc>
    <dif>
      <ref tip="lst" cel="stel.0figuro" lst="voko:zodiakaj_signoj" val="10">Stelfiguro</ref> kaj
      signo de la <ref tip="malprt" cel="zodiak.0o">zodiako</ref>
      (<trd lng="la">Capricornus</trd>):
      <ekz>
      la <ind>tropiko de la <tld lit="K"/>o</ind>;
      <trd lng="ca">tr&ograve;pic de Capricorn</trd>
      <trd lng="de">Wendekreis des Steinbocks</trd>
      <trdgrp lng="nl">
        <trd>Steenbokskeerkring</trd>,
        <trd>zuiderkeerkring</trd>
      </trdgrp>
      </ekz>
      <ekz>la suno je <tld/>o dum tri semajnoj varmigas Ile-de-France
      per
      siaj vertikalaj radioj
        <fnt>
          <aut>Henri Bernardin de Saint-Pierre, trad. Hector
          Hodler</aut>
          <vrk>Pa&ubreve;lo kaj Virginio</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_K;&c_a;&c_z;&c_ja;&c_r;&c_o;&c_g;</trd>
  <trd lng="bg">&c_k;&c_o;&c_z;&c_i;&c_r;&c_o;&c_g;</trd>
  <trd lng="ca">Capricorn</trd>
  <trd lng="de">Steinbock</trd>
  <trd lng="en">Capricorn</trd>
  <trd lng="es">capricornio</trd>
  <trd lng="fr">Capricorne<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="he">&mem;&zayin;&lamed; &gimel;&dalet;&yod;</trd>
  <trd lng="hu">Bak</trd>
  <trdgrp lng="id">
    <trd>Jadi</trd>,
    <trd>Kaprikornus</trd>,
    <trd>Makara</trd>
  </trdgrp>
  <trd lng="nl">Steenbok<klr tip="ind"> (sterrenbeeld)</klr></trd>
  <trd lng="pl">Kozioro&zdot;ec  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Capricornul  <klr>(constela&#539;ie)</klr></trd>
  <trd lng="ru">&c_K;&c_o;&c_z;&c_je;&c_r;&c_o;&c_g;</trd>
  <trd lng="uk">&#x41A;&#x43E;&c_z;&c_je;&c_r;&#x43E;&c_g;</trd>
</drv>
</art>
