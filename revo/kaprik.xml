<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Kaprikorn</rad>/o
</kap>
<drv mrk="kaprik.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">AST</uzo>
  <mlg>Cap</mlg>
  <mlg>&#9809;</mlg>
  <snc>
    <dif>
      <ref tip="lst" cel="stel.0figuro" lst="voko:zodiakaj_signoj"
      val="10">Stelfiguro</ref> kaj signo de la <ref tip="malprt"
      cel="zodiak.0o">zodiako</ref>
      (<trd lng="la">Capricornus</trd>):
      <ekz>
      la <ind>tropiko de la <tld lit="K"/>o</ind>.
      <trd lng="ca">tr&ograve;pic de Capricorn</trd>
      <trd lng="de">Wendekreis des Steinbocks</trd>
      <trdgrp lng="nl">
        <trd>Steenbokskeerkring</trd>,
        <trd>zuiderkeerkring</trd>
      </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_K;&c_a;&c_z;&c_ja;&c_r;&c_o;&c_g;</trd>
  <trd lng="ca">Capricorn</trd>
  <trd lng="de">Steinbock</trd>
  <trd lng="en">Capricorn</trd>
  <trd lng="es">capricornio</trd>
  <trd lng="fr">Capricorne<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="he">&mem;&zayin;&lamed; &gimel;&dalet;&yod;</trd>
  <trd lng="hu">Bak</trd>
  <trd lng="nl">Steenbok<klr tip="ind"> (sterrenbeeld)</klr></trd>
  <trd lng="pl">kozioro&zdot;ec</trd>
  <trd lng="ru">&c_K;&c_o;&c_z;&c_je;&c_r;&c_o;&c_g;</trd>
</drv>
</art>
