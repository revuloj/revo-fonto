<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kron</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kron2.0o">
  <kap><tld/>o</kap>
  <snc mrk="kron2.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="unu.mon0o">Monunuo</ref> de diversaj 
      e&ubreve;ropaj
      landoj kaj epokoj, ordinare tiel nomata pro figuro de <ref
      tip="hom" cel="kron.0o">krono</ref> sur moneroj:
      <ekz>
        ni povis malaltigi la kotizon &gcirc;is nur 700 <tld/>oj
        <fnt>
        <aut>J. E. Abrahamsen</aut>,
        <vrk>AJN-kotizo malalti&gcirc;is</vrk>,
        <lok>soc.culture.esperanto, 2000-01-31</lok>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="en">crown<klr tip="amb"> (money)</klr></trd>
  <trd lng="fr">couronne<klr tip="amb"> (monnaie)</klr></trd>
  <trd lng="hu">korona<klr tip="amb"> (p&eacute;nznem)</klr></trd>
  <trd lng="nl">kroon<klr tip="ind"> (munt)</klr></trd>
  <trd lng="pl">korona</trd>
  <trd lng="pt">coroa <klr tip="amb">(moeda)</klr></trd>
</drv>
</art>

