<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: megid.xml,v 1.3 2022/05/01 12:05:12 afido Exp $">
    <kap>
      <rad>Megid</rad>/o <fnt><bib>MT</bib></fnt>
    </kap>

<drv mrk="megid.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ARKE</uzo><uzo tip="fak">BIB</uzo>
  <snc mrk="megid.0o.antikva">
    <dif>
      Antikva urbo &ccirc;e la kruci&gcirc;o de du komercaj vojoj, situanta sub <ref tip="super"
      cel="tel.0o">telo</ref> 
      en la nordo de <ref tip="malprt" cel="israel.I0o">Israelo</ref>, 16&#8239;km sudokcidente de
      Nazareto:
      <ekz>
        la lo&gcirc;antoj de <nom>Taana&hcirc;</nom> kaj <klr>[&#8230;]</klr> de <tld/>o kaj de &gcirc;iaj urbetoj,
        kaj la
        triobla alta&jcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Josuo 17:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        li forkuris en <tld/>on kaj mortis tie
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 9:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        la batalo de <tld/>o 609 a.K.
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=293695">Armagedono</url></lok>
        </fnt>
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/4/44/Megiddo_Modell.JPG/640px-Megiddo_Modell.JPG"
      prm="PD"
      >modelo de telo <tld/>o
        <fnt>
          <aut>Drahnier, propra foto</aut>
          <lok><url ref="&WCW;/File:Megiddo_Modell.JPG"
              >&WCW;/File:Megiddo_Modell.JPG</url></lok>
        </fnt>
    </bld>
    <trd lng="ar">&a_t;&a_l; &a_A;&a_l;&a_m;&a_t;&a_s;&a_l;&a_m;</trd>
    <trd lng="de">Megiddo</trd>
    <trd lng="en">Megiddo</trd>
    <trd lng="fr">Megiddo</trd>
    <trd lng="pl">Megiddo</trd>
    <trd lng="ro">Meghido</trd>
  </snc>
</drv>

  </art>

