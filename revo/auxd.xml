<drv mrk="auxd.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="auxd.0i.orele">
    <dif>
      <ref tip="super" cel="sent.0i.sensi">Senti</ref>, percepti per la
      oreloj:
      <ekz>
        orelojn ili havas, sed ne <tld/>as
        <fnt><bib>MT</bib>, <lok>&Psa; 135:17</lok></fnt>;
      </ekz>
      <ekz>
        vian vo&ccirc;on mi <tld/>is en la &gcirc;ardeno
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 3:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>is la &gcirc;emadon
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 6:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        bojas hundido kiel &gcirc;i <tld/>as de hundoj
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        li <tld/>as muzikon
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato1/monato-9401-telefona-labirinto.txt">Will
          Green kondukas vin tra sia</url></vrk>
          <lok>94/01</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ku&scirc;is senmove kelkajn minutojn, provante <tld/>i sian internon
        <fnt>
          <bib>Monato</bib>
          <aut>Geraldo Mattos</aut>
          <vrk><url ref="&Steloj;monato2/006805.html">Kota Akvo: Konstanten
          Krysakov</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="auxsku.0i">a&ubreve;skulti</ref>
    </refgrp>
    <trd lng="it">udire</trd>
  </snc>
  <snc mrk="auxd.0i.sciigxi">
    <dif>
      <ref tip="super" cel="sci.0igxi">Scii&gcirc;i</ref> per a&ubreve;dado:
      <ekz>
        mi <tld/>is multon pri vi;
      </ekz>
      <ekz>
        mi <tld/>is, ke vi rezignas vian postenon;
      </ekz>
      <ekz>
        kie &jcirc;uron vi <tld/>as, malbonon suspektu
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu &ccirc;ion senpripense parolas, <tld/>os tion, kion li ne volas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu <tld/>is unuan, anka&ubreve; <tld/>u la duan
        <klr>(ne ignoru tion, kio ne pla&ccirc;as al vi)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        pri <klr>[&#8230;]</klr> malfeli&ccirc;o vi certe <tld/>as &ccirc;iun tagon
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato1/monato-9312-bosnio.txt">Bosnio</url></vrk>
          <lok>93/12</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni <tld/>as <klr>[&#8230;]</klr> dubeman opinion
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk><url ref="&Steloj;monato2/006813.html">Sa&gcirc;a kapo duonvorton
          komprenas</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kiu ne ridetis, tion <tld/>ante
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="&Steloj;monato2/006823.html">Opinio: Tro Senkosta
          Pentemo</url></vrk>
        </fnt>?
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="sci.ek0i.informon">ekscii</ref>,
     <ref cel="kompre.0i.ion">kompreni</ref>
    </refgrp>
    <trd lng="it">sentire<klr> (udire)</klr></trd>
  </snc>
