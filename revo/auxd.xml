<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>a&ubreve;d</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="auxd.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="auxd.0i.orele">
    <dif>
      <ref tip="super" cel="sent.0i.sensi">Senti</ref>, percepti per la
      oreloj:
      <ekz>
        orelojn ili havas, sed ne <tld/>as
        <fnt><bib>MT</bib>, <lok>&Psa; 135:17</lok></fnt>;
      </ekz>
      <ekz>
        parolanto semas, <tld/>anto rikoltas<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        bojas hundido kiel &gcirc;i <tld/>as de hundoj
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="auxsku.0i">a&ubreve;skulti</ref>
    </refgrp>
  </snc>
  <snc mrk="auxd.0i.sciigxi">
    <dif>
      Scii&gcirc;i per a&ubreve;dado:
      <ekz>
        mi <tld/>is multon pri vi;
      </ekz>
      <ekz>
        mi <tld/>is, ke vi rezignas vian postenon.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_u;&c_c;&c_mol;</trd>
  <trd lng="br">klevet</trd>
  <trd lng="cs">sly&scaron;et</trd>
  <trd lng="de">h&ouml;ren</trd>
  <trd lng="en">hear</trd>
  <trd lng="es">o&iacute;r</trd>
  <trd lng="fr">entendre<klr> (un son)</klr></trd>
  <trd lng="hu">hall</trd>
  <trd lng="ru">&c_s;&c_l;&c_y;&c_sh;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">po&ccaron;u&tcaron;</trd>
  <trd lng="tp">kute</trd>
  <trd lng="vo">lil&ouml;n</trd>
</drv>

<drv mrk="auxd.0o">
  <kap><tld/>o</kap>
  <snc mrk="auxd.0o.sentado">
    <ref tip="dif" cel="auxd.0ado.SEN"><tld/>ado</ref>
    <ekz>
      <ind>unu vido ta&ubreve;gas pli ol dek <tld/>oj</ind>
      <trd lng="fr">voir c'est croire</trd>
      <fnt><bib>PrV</bib></fnt>.
    </ekz>
    <trd lng="de">H&ouml;ren</trd>
  </snc>
  <snc mrk="auxd.0o.povo">
    <ref tip="dif" cel="auxd.0povo.SEN"><tld/>povo</ref>
  </snc>
  <trd lng="be">&c_s;&c_l;&c_y;&c_h;</trd>
  <trdgrp lng="br">
    <trd>klev</trd>,
    <trd>kleved</trd>
  </trdgrp>
  <trd lng="en">hearing</trd>
  <trd lng="hu">hall&aacute;s</trd>
  <trd lng="ru">&c_s;&c_l;&c_u;&c_h;</trd>
</drv>

<drv mrk="auxd.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Koncernanta la <tld/>adon.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_l;&c_y;&c_h;&c_a;&c_v;&c_y;</trd>
  <trd lng="br">-<ind>klev</ind></trd>
  <trd lng="en">auditory</trd>
  <trd lng="fr">auditif</trd>
  <trdgrp lng="hu">
    <trd>hall&oacute;-</trd>,
    <trd>hall&aacute;s-</trd>,
    <trd>audit&iacute;v</trd>,
  </trdgrp>
  <trd lng="ru">&c_s;&c_l;&c_u;&c_h;&c_o;&c_v;&c_o;&c_j;</trd>
  <trd lng="tp">kute</trd>
</drv>

<drv mrk="auxd.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="auxd.0ado.SEN">
    <dif>
      Sentado de sonoj.
    </dif>
  </snc>
  <trd lng="de">H&ouml;ren</trd>
</drv>

<drv mrk="auxd.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>eblan sonon:
      <ekz>
        leonido <tld/>igis sian vo&ccirc;on
        <fnt><bib>MT</bib>, <lok>&Amo; 3:4</lok></fnt>;
      </ekz>
      <ekz>
        la tondro <tld/>igadis brue frapon post frapo
        <fnt><bib>Fab1</bib>, <lok>Sova&gcirc;aj cignoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>anheben</trd>,
    <trd>erheben</trd>,
    <trd>verlauten lassen</trd>,
    <trd>zu <ind>Geh&ouml;r</ind> bringen</trd>
  </trdgrp>
  <trd lng="en">make a <ind>sound</ind></trd>
  <trd lng="fr">faire <ind>entendre</ind></trd>
  <trd lng="hu">hallat</trd>
  <trd lng="ru">&c_i;&c_z;&c_d;&c_a;&c_t;&c_mol;<klr tip="amb"> (&c_z;&c_v;&c_u;&c_k;)</klr></trd>
</drv>

<drv mrk="auxd.0ilo">
  <kap><tld/>ilo <fnt><bib>PIV1</bib></fnt></kap>
  <snc mrk="auxd.0ilo.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Aparato, tenata proksime al la orelo kaj
      transformanta elektran kurenton en sonon:
      <ekz>
        <tld/>ilo de telefono.
      </ekz>
    </dif>
    <ref tip="sin" cel="auxsku.0ilo">a&ubreve;skultilo</ref>
  </snc>
  <trd lng="br">selaouell</trd>
  <trd lng="fr">&eacute;couteur</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_je;&c_l;&c_je;&c_f;&c_o;&c_n;&c_n;&c_a;&c_ja; &c_t;&c_r;&c_u;&c_b;&c_k;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_u;&c_sh;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_g;&c_o;&c_l;&c_o;&c_v;&c_n;&c_o;&c_j; <ind>&c_t;&c_je;&c_l;&c_je;&c_f;&c_o;&c_n;</ind></trd>,
    <trd>&c_sh;&c_l;&c_je;&c_m;&c_o;&c_f;&c_o;&c_n;</trd>
  </trdgrp>
</drv>

<drv mrk="auxd.kap0ilo">
  <kap>kap<tld/>ilo</kap>
  <fnt><bib>PIV2</bib>, <lok>a&ubreve;dilo</lok></fnt>
  <snc mrk="auxd.kap0ilo.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Aparato kun <ref tip="vid" cel="auxd.0ilo.ELE"><tld/>ilo</ref>,
      fiksita sur la kapo a&ubreve; en la oreloj.
    </dif>
  </snc>
  <trd lng="fr">casque<klr tip="ind"> (&agrave; &eacute;couteurs)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_u;&c_sh;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_g;&c_o;&c_l;&c_o;&c_v;&c_n;&c_o;&c_j; <ind>&c_t;&c_je;&c_l;&c_je;&c_f;&c_o;&c_n;</ind></trd>,
    <trd>&c_sh;&c_l;&c_je;&c_m;&c_o;&c_f;&c_o;&c_n;</trd>
  </trdgrp>
</drv>

<drv mrk="auxd.ne0ita">
  <kap>ne<tld/>ita<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Ne jam a&ubreve;dita, nekredebla, senekzempla.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_ja;&c_s;&c_l;&c_y;&c_h;&c_a;&c_n;&c_y;</trd>
  <trd lng="en">inaudible</trd>
  <trd lng="fr">inou&iuml;</trd>
  <trd lng="hu">hallatlan</trd>
  <trd lng="ru">&c_n;&c_je;&c_s;&c_l;&c_y;&c_h;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">nesl&yacute;chan&yacute;</trd>
</drv>

<drv mrk="auxd.0povo">
  <kap><tld/>povo, <var><kap><tld/>kapablo</kap></var></kap>
  <snc mrk="auxd.0povo.SEN">
    <dif>
      Kapablo senti sonojn:
      <ekz>
        mi havas tre bonan <tld/>povon
        <fnt><bib>Lasu</bib>, <lok>6</lok></fnt>;
      </ekz>
      <ekz>
        se mia <tld/>kapablo povus kapti &ccirc;iun bruon en la mondo, mi 
        <tld/>us liajn pa&scirc;ojn
        <fnt>
          <aut>J. L. Borges, trad. M. Giorgini</aut>,
          <vrk><url ref="http://www.yksi.org/tekst/ald/borges_aster-eo.html">
          La domo de Asteriono</url></vrk>,
          <lok>2006-12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Geh&ouml;r</trd>,
    <trd>H&ouml;rverm&ouml;gen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>audition<klr> (sens)</klr></trd>,
    <trd>ou&iuml;e<klr> (sens)</klr></trd>
  </trdgrp>
</drv>
</art>
