<drv mrk="auxd.0o">
  <kap><tld/>o</kap>
  <snc mrk="auxd.0o.sentado">
    <dif>
      <ref tip="dif" cel="auxd.0ado.SEN"><tld lit="A"/>ado</ref>:
      <ekz>
        <ind>unu vido ta&ubreve;gas pli ol dek <tld/>oj</ind>
        <trd lng="de">ein <ind>Bild</ind> sagt mehr als tausend Worte</trd>
        <trd lng="fr">voir c'est croire</trd>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ne la&ubreve; la <tld/>o de siaj oreloj li eldirados verdiktojn
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 11:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia rigardo kaj <tld/>o svagi&gcirc;is
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="&Monato;/2010/009883.php">Trancporto</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        mis<tld/>o
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/007143.html">&Ccirc;u ni mortu miskomprene?
          (2)</url></vrk>
        </fnt>;
      </ekz>
    </dif>
    <trd lng="de">H&ouml;ren</trd>
    <trd lng="it">ascolto</trd>
  </snc>
  <snc mrk="auxd.0o.povo">
    <ref tip="dif" cel="auxd.0povo.SEN"><tld/>povo</ref>
  <trd lng="it">udito</trd>
  </snc>
  <trd lng="be">&c_s;&c_l;&c_y;&c_h;</trd>
  <trdgrp lng="br">
    <trd>klev</trd>,
    <trd>kleved</trd>
  </trdgrp>
  <trd lng="en">hearing</trd>
  <trd lng="hu">hall&aacute;s</trd>
  <trd lng="id">pen<ind>dengar</ind>an</trd>
  <trd lng="pl">s&lstroke;uch</trd>
  <trd lng="ro">auz</trd>
  <trd lng="ru">&c_s;&c_l;&c_u;&c_h;</trd>
</drv>

<drv mrk="auxd.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Koncernanta la <tld/>adon:
      <ekz>
        frekvenco detektebla per la <tld/>a sistemo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3284">Ondo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a subfrekvenco estas 7,74 MHz
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato1/monato-9504-radio.txt">Radio</url></vrk>
          <lok>95/04</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a sistemo
        <fnt>
          <vrk><url ref="&Viki;/A%C5%ADda_sistemo">A&ubreve;da sistemo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_l;&c_y;&c_h;&c_a;&c_v;&c_y;</trd>
  <trd lng="br">-<ind>klev</ind></trd>
  <trd lng="en">auditory</trd>
  <trd lng="fr">auditif</trd>
  <trdgrp lng="hu">
    <trd>hall&oacute;-</trd>,
    <trd>hall&aacute;s-</trd>,
    <trd>audit&iacute;v</trd>,
  </trdgrp>
  <trdgrp lng="it">
    <trd>auditivo</trd>
    <trd>uditivo</trd>
  </trdgrp>
  <trd lng="pl">s&lstroke;uchowy</trd>
  <trd lng="ro">auditiv</trd>
  <trd lng="ru">&c_s;&c_l;&c_u;&c_h;&c_o;&c_v;&c_o;&c_j;</trd>
  <trd lng="tok">kute</trd>
</drv>

<drv mrk="auxd.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="auxd.0ado.SEN">
    <dif>
      Sentado de sonoj:
      <ekz>
        <tld/>ado de instruo
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 28:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne pleni&gcirc;as la orelo de <tld/>ado
        <fnt>
          <bib>MT</bib>
          <lok>Predikanto 1:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        revolucia nova maniero de <tld/>ado
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3601">Renesanco</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        nia a&ubreve;dado alkutimi&gcirc;is <klr>[...]</klr> al la ja tre &leftquot;mal&gcirc;ustaj&rightquot;
        tercioj kaj sekstoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=27349">Piano</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_l;&c_y;&c_h;</trd>
  <trd lng="de">H&ouml;ren</trd>
  <trd lng="he">&shin;&mem;&ayin;</trd>
  <trd lng="it">ascolto</trd>
  <trd lng="kek">ab&#789;iil</trd> 
  <trd lng="nl">gehoor</trd>
  <trd lng="no">h&oslash;rsel</trd>
  <trd lng="pl">s&lstroke;uch</trd>
  <trd lng="ro">auz</trd>
  <trd lng="uk">&c_s;&#x43B;&c_u;&c_h;</trd>
</drv>

<drv mrk="auxd.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>eblan sonon:
      <ekz>
        leonido <tld/>igis sian vo&ccirc;on
        <fnt><bib>MT</bib>, <lok>&Amo; 3:4</lok></fnt>;
      </ekz>
      <ekz>
        la tondro <tld/>igadis brue frapon post frapo
        <fnt><bib>Fab1</bib>, <lok>Sova&gcirc;aj cignoj</lok></fnt>;
      </ekz>
      <ekz>
        sensonan kvakadon &scirc;i de tempo al tempo <tld/>igadis, tute tiel, kiel <tld/>igas
        infano, kiam &gcirc;i &gcirc;emas en son&gcirc;o
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; <ind>&c_g;&c_u;&c_k;</ind></trd>
  <trdgrp lng="de">
    <trd>anheben</trd>,
    <trd>erheben</trd>,
    <trd>verlauten lassen</trd>,
    <trd>zu <ind>Geh&ouml;r</ind> bringen</trd>
  </trdgrp>
  <trd lng="en">make a <ind>sound</ind></trd>
  <trd lng="fr">faire <ind>entendre</ind></trd>
  <trd lng="he">&lamed;&he;&yod;&shin;&mem;&ayin;</trd>
  <trd lng="hu">hallat</trd>
  <trdgrp lng="it">
    <trd>far <ind>udire</ind></trd>,
    <trd>far <ind>sentire</ind><klr> (udire)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rozg&lstroke;asza&cacute;</trd>,
    <trd>rozlega&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>difuzat</trd>,
    <trd>r&abreve;sp&acirc;ndirea</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_z;&c_d;&c_a;&c_t;&c_mol;<klr tip="amb"> 
  (&c_z;&c_v;&c_u;&c_k;)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &c_g;&#x43E;&#x43B;&#x43E;&c_s;</trd>,
    <trd>&c_v;&c_i;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &c_z;&c_v;&c_u;&#x43A;&c_i;</trd>,
    <trd>&c_r;&#x43E;&c_b;&c_i;&c_t;&c_i; &c_ch;&c_u;&c_t;&#x43D;&c_i;&#x43C;</trd>
  </trdgrp>
</drv>

