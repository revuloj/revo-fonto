  <trdgrp lng="uk">
    <trd>&c_r;&c_u;&#x43F;&#x43E;&c_r;</trd>,
    <trd>&c_s;&#x43B;&c_u;&c_h;&c_a;&c_v;&#x43A;&c_a;</trd>,
    <trd>&c_t;&c_je;&#x43B;&c_je;&c_f;&#x43E;&#x43D;&#x43D;&c_a; &c_t;&c_r;&c_u;&c_b;&#x43A;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="auxd.mis0i">
  <kap>
    mis<tld/>i
  </kap>
  <snc>
    <dif>
      <tld lit="A"/>i mal&gcirc;uste; kompreni ion, kio ne estis dirita:
      <ekz>
        &scirc;i mis<tld/>is miajn vortojn mi ne diris pli maldika, sed pli dika
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;vi mis<tld/>is, Jano,&rightquot; &scirc;i diris per tre stranga vo&ccirc;o, <klr>[&#8230;]</klr> la&ubreve;
        maniero incite nekomprenebla
        <fnt>
          <bib>ChM</bib>
        </fnt>;
      </ekz>
      <ekz>
        se mi ne mis<tld/>is, vi bonvolis diri, ke Jesuo neniam ekzistis, &ccirc;u?
        <fnt>
          <bib>MkM</bib>
        </fnt>?
      </ekz>
    </dif>
    <ref tip="vid" cel="kompre.mis0i">miskompreni</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_s;&c_l;&c_u;&c_h;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_n;&c_je;&c_d;&c_a;&c_ch;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">verh&ouml;ren<klr> (missverstehen)</klr></trd>
  <trd lng="en">mishear</trd>
  <trd lng="nl">verkeerd verstaan</trd>
  <trdgrp lng="pl">
    <trd>przes&lstroke;ysze&cacute; si&eogonek;</trd>,
    <trd>&zacute;le us&lstroke;ysze&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>auzit gre&#537;it</trd>,
    <trd>auzi prost</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_s;&c_l;&c_y;&c_sh;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_je; &c_t;&c_je; &#x43F;&#x43E;&c_ch;&c_u;&c_t;&c_i;</trd>,
    <trd>&c_z;&#x43B;&c_je;/&#x43F;&#x43E;&c_g;&c_a;&#x43D;&#x43E; &#x43F;&#x43E;&c_ch;&c_u;&c_t;&c_i;</trd>
  </trdgrp>
</drv>


<drv mrk="auxd.ne0ita">
  <kap>ne<tld/>ita</kap>
  <snc mrk="auxd.ne0ita.ignorata">
    <dif>
      Sen, ke oni <tld/>as ion:
      <ekz>
        &scirc;ia senforti&gcirc;inta vo&ccirc;o timis la stratan bruon, en kiu &gcirc;i devus droni ne<tld/>ita
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">ungeh&ouml;rt</trd>
    <trd lng="pl">nies&lstroke;yszalny</trd>
    <trd lng="ro">inauzibil</trd>
  </snc>
  <snc mrk="auxd.ne0ita.neordinara">
    <dif>
      Ne jam <tld/>ita, nekredebla, senekzempla:
      <ekz>
        tio &ccirc;i estis ne<tld/>ita, nekredebla moko de la sorto
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne<tld/>ita projekto
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        terura, nenatura, ne<tld/>ita
        <fnt>
          <aut>W. Shakespeare, trad. L. L. Zamenhof</aut>
          <vrk><url ref="https://eo.wikisource.org/wiki/Hamleto,_re%C4%9Dido_de_Danujo/Akto_I">
          Hamleto, re&gcirc;ido de Danujo</url></vrk>
          <lok>1894</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_n;&c_ja;&c_s;&c_l;&c_y;&c_h;&c_a;&c_n;&c_y;</trd>
    <trd lng="de">unerh&ouml;rt</trd>
    <trd lng="en">unheard-of</trd>
    <trd lng="fr">inou&iuml;</trd>
    <trd lng="hu">hallatlan</trd>
    <trd lng="it">inaudito</trd>
    <trd lng="nl">ongehoord</trd>
    <trd lng="no">uh&oslash;rt</trd>
    <trd lng="pl">nies&lstroke;ychany</trd>
    <trd lng="ro">neobi&#537;nuit</trd>
    <trd lng="ru">&c_n;&c_je;&c_s;&c_l;&c_y;&c_h;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
    <trd lng="sk">nesl&yacute;chan&yacute;</trd>
  </snc>
  <trd lng="uk">&#x43D;&c_je;&c_ch;&c_u;&c_v;&c_a;&#x43D;&c_i;&c_j;</trd>
</drv>

<drv mrk="auxd.kap0ilo">
  <kap>kap<tld/>ilo</kap>
  <fnt><bib>PIV2</bib>, <lok>a&ubreve;dilo</lok></fnt>
  <snc mrk="auxd.kap0ilo.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Aparato kun <ref tip="vid" cel="auxd.0ilo.ELE"><tld/>ilo</ref>,
      fiksita sur la kapo a&ubreve; en la oreloj:
      <ekz>
        mia kap<tld/>ilo flugis de sur mia kapo
        <fnt>
          <vrk><url ref="http://graynoise.konfuzo.net/esperanto-dumlerna_blogo.php">esperanto
          - graynoise.konfuzo.net</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        hometoj kun kap<tld/>iloj kaj magnetofonoj
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2013/011111.php">Saluton, ser&gcirc;ento!</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_v;&c_u;&c_sh;&c_n;&c_ib;&c_k; <klr>(&c_g;&c_a;&c_l;&c_a;&c_w;&c_n;&c_y;)</klr></trd>
  <trd lng="de">Kopfh&ouml;rer</trd>
  <trd lng="fr">casque<klr tip="ind"> (&agrave; &eacute;couteurs)</klr>
  </trd>
  <trd lng="pl">s&lstroke;uchawki  {pl} <klr>(nag&lstroke;owne)</klr></trd>
  <trd lng="ro">c&abreve;&scedil;ti </trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_u;&c_sh;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_g;&c_o;&c_l;&c_o;&c_v;&c_n;&c_o;&c_j; <ind>
    &c_t;&c_je;&c_l;&c_je;&c_f;&c_o;&c_n;</ind></trd>,
    <trd>&c_sh;&c_l;&c_je;&c_m;&c_o;&c_f;&c_o;&c_n;</trd>
  </trdgrp>
  <trd lng="uk">&c_r;&c_a;&c_d;&c_ib;&#x43E; &#x43D;&c_a;&c_v;&c_u;&c_sh;&#x43D;&c_i;&#x43A;</trd>
</drv>

<drv mrk="auxd.0povo">
  <kap><tld/>povo, <var><kap><tld/>kapablo</kap></var></kap>
  <snc mrk="auxd.0povo.SEN">
    <dif>
      <ref tip="super" cel="sens.0o.FIZL">Senso</ref>,
      per kiu oni perceptas sonojn:
      <ekz>
        mi havas tre bonan <tld/>povon
        <fnt><bib>Lasu</bib>, <lok>6</lok></fnt>;
      </ekz>
      <ekz>
        kiel 16-jarulo <nom>Lorm</nom> perdis sian <tld/>kapablon
        <fnt>
          <bib>Monato</bib>
          <aut>Gert Heintze</aut>
          <vrk><url ref="&Monato;/2003/008104.html">Alfabeto por
          surdblinduloj</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        se mia <tld/>kapablo povus kapti &ccirc;iun bruon en la mondo, 
        mi <tld/>us liajn pa&scirc;ojn
        <fnt>
          <aut>J. L. Borges, trad. M. Giorgini</aut>,
          <vrk>
          <url ref="http://www.yksi.org/tekst/ald/borges_aster-eo.html">
          La domo de Asteriono</url></vrk>,
          <lok>2006-12</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&ccirc;ar multe oni]</klr> skribas, Esperanto <klr>[&#8230;]</klr> estas sendube
        preska&ubreve; tiom utila al la surduloj kiom al <tld/>opovaj
        <fnt>
          <bib>Monato</bib>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url ref="&Steloj;monato2/006880.html">Ne &ccirc;iuj lingvoj estas
          langaj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_l;&c_y;&c_h;</trd>
  <trdgrp lng="de">
    <trd>Geh&ouml;r</trd>,
    <trd>H&ouml;rverm&ouml;gen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>audition<klr> (sens)</klr></trd>,
    <trd>ou&iuml;e<klr> (sens)</klr></trd>
  </trdgrp>
  <trd lng="he">&yod;&kaf;&vav;&lamed;&tav; &shin;&mem;&yod;&ayin;&he;</trd>
  <trd lng="it">udito<klr> (senso)</klr></trd>
  <trd lng="nl">gehoor</trd>
  <trd lng="no">h&oslash;rsel</trd>
  <trd lng="pl">s&lstroke;uch</trd>
  <trd lng="ro">auz</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43B;&c_u;&c_h;</trd>,
    <trd>&c_z;&c_d;&c_a;&c_t;&#x43D;&c_ib;&c_s;&c_t;&#x44C; &c_ch;&c_u;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
