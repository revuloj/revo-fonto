<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>a&ubreve;d</rad>/i
</kap>
<drv mrk="auxd.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="sent.0i">Senti</ref>, percepti per la oreloj:
      <ekz>
        orelojn ili havas, sed ne a&ubreve;das
        <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Psalmaro 135, 17</lok>)</fnt>;
      </ekz>
      <ekz>
        parolanto semas, <tld/>anto rikoltas
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
      </ekz>
      <ekz>
        bojas hundido kiel &gcirc;i <tld/>as de hundoj
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="auxsku.0i">a&ubreve;skulti</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Scii&gcirc;i per a&ubreve;dado:
      <ekz>
        mi <tld/>is multon pri vi;
      </ekz>
      <ekz>
        mi <tld/>is, ke vi rezignas vian postenon.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">sly&scaron;et</trd>
  <trd lng="de">h&ouml;ren</trd>
  <trd lng="en">hear</trd>
  <trd lng="fr">entendre (un son)</trd>
  <trd lng="ru">&#1089;&#1083;&#1099;&#1096;&#1072;&#1090;&#1100;</trd>
</drv>

<drv mrk="auxd.0ado">
  <kap><tld/>(ad)o</kap>
  <snc>
    <dif>
      Perceptado de sonoj:
      <ekz>
        unu vido ta&ubreve;gas pli ol dek <tld/>oj
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Kapablo <tld/>i.
    </dif>
    <trd lng="de">Geh&ouml;r</trd>
    <trdgrp lng="fr">
      <trd>audition<klr> (sens)</klr></trd>,
      <trd>ou&iuml;e<klr> (sens)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="ru">&#1089;&#1083;&#1091;&#1093;</trd>
</drv>

<drv mrk="auxd.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Koncernanta la <tld/>adon.
    </dif>
  </snc>
  <trd lng="fr">auditif</trd>
  <trd lng="ru">&#1089;&#1083;&#1091;&#1093;&#1086;&#1074;&#1086;&#1081;</trd>
</drv>

<drv mrk="auxd.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Produkti <tld/>eblan sonon:
      <ekz>
        leonido <tld/>igis sian vo&ccirc;on
        <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Amos 3, 4</lok>)</fnt>;
      </ekz>
      <ekz>
        la tondro <tld/>igadis brue frapon post frapo
        <fnt>(<aut>Zamenhof</aut>: <vrk>Fabeloj de Andersen,
        Sova&gcirc;aj cignoj</vrk>)</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">faire <ind>entendre</ind></trd>
</drv>

<drv mrk="auxd.ne0ita">
  <kap>ne<tld/>ita<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Nekredebla, senekzempla.
    </dif>
  </snc>
  <trd lng="fr">inou&iuml;</trd>
  <trd
  lng="ru">&#1085;&#1077;&#1089;&#1083;&#1099;&#1093;&#1072;&#1085;&#1085;&#1099;&#1081;</trd>
</drv>
</art>
