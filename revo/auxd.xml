<drv mrk="auxd.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="auxd.0i.orele">
    <dif>
      <ref tip="super" cel="sent.0i.sensi">Senti</ref>, percepti per la
      oreloj:
      <ekz>
        orelojn ili havas, sed ne <tld/>as
        <fnt><bib>MT</bib>, <lok>&Psa; 135:17</lok></fnt>;
      </ekz>
      <ekz>
        vian vo&ccirc;on mi <tld/>is en la &gcirc;ardeno
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 3:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>is la &gcirc;emadon
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 6:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        bojas hundido kiel &gcirc;i <tld/>as de hundoj
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        li <tld/>as muzikon
        <fnt>
          <aut>Will Green</aut>
          <vrk><url ref="&Monato;/1994/A940103.php">Will Green kondukas vin tra sia telefona
          labirinto</url></vrk>
          <bib>Monato</bib>
          <lok>1994/01, p. 13</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ku&scirc;is senmove kelkajn minutojn, provante <tld/>i sian internon
        <fnt>
          <aut>Konstanten Krysakov</aut>
          <vrk><url ref="&Monato;/2000/006805.php">Kota akvo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 28</lok>
          </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="auxsku.0i">a&ubreve;skulti</ref>
    </refgrp>
    <trd lng="it">udire</trd>
  </snc>
  <snc mrk="auxd.0i.sciigxi">
    <dif>
      <ref tip="super" cel="sci.0igxi">Scii&gcirc;i</ref> per a&ubreve;dado:
      <ekz>
        mi <tld/>is multon pri vi;
      </ekz>
      <ekz>
        mi <tld/>is, ke vi rezignas vian postenon;
      </ekz>
      <ekz>
        kie &jcirc;uron vi <tld/>as, malbonon suspektu
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu &ccirc;ion senpripense parolas, <tld/>os tion, kion li ne volas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu <tld/>is unuan, anka&ubreve; <tld/>u la duan
        <klr>(ne ignoru tion, kio ne pla&ccirc;as al vi)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        pri <klr>[&#8230;]</klr> malfeli&ccirc;o vi certe <tld/>as &ccirc;iun tagon
        <fnt>
          <aut>Spomenka &Scaron;timec</aut>
          <vrk><url ref="&Monato;/1993/A931201.php">Saluton el Absurdistano</url></vrk>
          <bib>Monato</bib>
          <lok>1993/12, p. 9</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni <tld/>as <klr>[&#8230;]</klr> dubeman opinion
        <fnt>
	<aut>Pejno Simono</aut>
	<vrk><url ref="&Monato;/2000/006813.php">Plansprache und Phraseologie</url></vrk>
	<bib>Monato</bib>
	<lok>2000/06, p. 33</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu ne ridetis, tion <tld/>ante
        <fnt>
	<aut>Gerrit Berveling</aut>
	<vrk><url ref="&Monato;/2000/006823.html">Tro senkosta pentemo</url></vrk>
	<bib>Monato</bib>
	<lok>2000/08, p. 22</lok>
        </fnt>?
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="sci.ek0i.informon">ekscii</ref>,
     <ref cel="kompre.0i.ion">kompreni</ref>
    </refgrp>
    <trd lng="it">sentire<klr> (udire)</klr></trd>
  </snc>
