<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bastio.xml,v 1.28 2023/10/14 08:39:48 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bastion</rad>/o <fnt><bib>MT</bib></fnt>
</kap>

<drv mrk="bastio.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
    <snc mrk="bastio.0o.siegxturo">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Simpla, ordinare ligna, <ref tip="super" cel="tur.0o.ARKI">turo</ref>
      konstruita por supergrimpi la muron de sie&gcirc;ata urbo:
      <ekz>
        venis <nom>Nebukadnecar</nom>, re&gcirc;o de <nom>Babel</nom>, li kaj lia tuta militistaro,
        kontra&ubreve;
        Jerusalemon, kaj eksie&gcirc;is &gcirc;in, kaj oni konstruis &ccirc;irka&ubreve; &gcirc;i <tld/>ojn
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 25:1</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Belagerungsturm</trd>
  </snc>
  <snc mrk="bastio.0o.fortikajxo">
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="super" cel="fortik.0ajxo">Fortika&jcirc;o</ref>
      el tero a&ubreve; masona&jcirc;o elstaranta el <ref cel="rempar.0o.ARKI">remparo</ref>
      por defendi &gcirc;in:
      <ekz>
        viaj <tld/>oj estas amasoj da argilo
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 13:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        la konstrua&jcirc;o staras sur iama <tld/>o de la viena urbomuro
        <fnt>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2009/009892.php">Kiam akvo trovis la &scirc;ipon</url></vrk>
          <bib>Monato</bib>
          <lok>2009/10, p. 17</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="br">beg-moger</trd>
    <trd lng="ca">basti&oacute;</trd>
    <trdgrp lng="de">
      <trd>Bollwerk</trd>,
      <trd>Bastion</trd>,
      <trd>Bastei</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_b;&c_a;&c_s;&c_t;&c_i;&c_o;&c_n;</trd>,
      <trd>&c_u;&c_k;&c_r;&c_je;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
    <trd lng="hu">b&aacute;stya</trd>
    <trd lng="pl">bastion</trd>
  </snc>
  <snc mrk="bastio.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Defendilo, <ref cel="sxirm.0ilo.KOMUNE">&scirc;irmilo</ref>:
      <ekz>
        la Hungaroj longtempe funkciis kiel <tld/>o
        de la Kristanismo kontra&ubreve; la Turkoj;
      </ekz>
      <ekz>
        Usono agis post la dua mondmilito kiel necesa <tld/>o kontra&ubreve; la sovetia
        hegemonio
        <fnt>
          <aut>Bertil Englund</aut>
          <vrk><url ref="&Monato;/2003/008058.html">Benu modere Usonon!</url></vrk>
           <bib>Monato</bib>
          <lok>2003/03, p. 7</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">baluard</trd>
    <trdgrp lng="de">
      <trd>Bollwerk</trd>,
      <trd>Bastion</trd>,
      <trd>Schutzwall</trd>,
    </trdgrp>
    <trd lng="hu">v&eacute;d&odblac;b&aacute;stya</trd>
    <trdgrp lng="pl">
      <trd>bastion</trd>,
      <trd>ostoja</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_b;&c_a;&c_s;&c_t;&c_i;&c_o;&c_n;</trd>,
      <trd>&c_o;&c_p;&c_l;&c_o;&c_t;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_s;&c_t;&c_y;&c_jo;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>bastion</trd>,
    <trd>ba&scaron;ta</trd>
  </trdgrp>
  <trd lng="en">bastion</trd>
  <trd lng="es">basti&oacute;n</trd>
  <trd lng="fr">bastion</trd>
  <trd lng="he">&#x5DE;&#x5B4;&#x5D1;&#x5E6;&#x5B8;&#x5E8;</trd>
  <trd lng="it">bastione</trd>
  <trdgrp lng="ja">
    <trd>&#x7A1C;&#x5821;  <pr>&#x308A;&#x3087;&#x3046;&#x307B;</pr></trd>,
    <trd>&#x7826;  <pr>&#x3068;&#x308A;&#x3067;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bastion</trd>,
    <trd>bolwerk</trd>
  </trdgrp>
  <trd lng="pt">basti&atilde;o</trd>
  <trdgrp lng="sk">
    <trd>ba&scaron;ta</trd>,
    <trd>ve&zcaron;a <klr>(&scaron;ach.)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&c_s;&c_t;&c_ib;&#x43E;&#x43D;</trd>,
    <trd>&c_u;&#x43A;&c_r;&c_ib;&#x43F;&#x43B;&c_je;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
