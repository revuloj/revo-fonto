<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: semiotik.xml,v 1.13 2025/01/04 10:26:55 revo Exp $">
<kap>
  <rad>semiotik</rad>/o <fnt><bib>NePiv</bib></fnt>,
  <var><kap><rad var="l">semiologi</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>
<drv mrk="semiotik.0o">
  <kap><tld/>o, <var><kap><tld var="l"/>o</kap></var></kap>
  <snc mrk="semiotik.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <uzo tip="fak">LIN</uzo>
    <dif>
      Signoscienco, <ref tip="super" cel="scienc.0o.fako">scienco<sncref/></ref> esploranta la rilatojn
      inter <ref tip="prt" cel="sign.0o.KOMUNE">signoj</ref>, la signata
      kaj la <ref tip="prt" cel="signif.0o.senco">signifo</ref>:
      <ekz>
        docento pri <tld/>o de la teksto
        <fnt>
          <aut>F. Amerio</aut>,
          <vrk>Informitale 9</vrk>,
          <lok>soc.culture.esperanto, 2004-03-04</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiel krei&gcirc;as novaj vortoj kaj novaj signifoj, kaj la lingvo ricevas ekpu&scirc;on al
        enhava kaj forma evoluo (&leftquot;<tld var="l"/>a vivo&rightquot; la&ubreve; <nom>Ferdinand de Saussure</nom>)
        <fnt>
          <bib>EeP</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Umberto Eco</nom> <klr>[&#8230;]</klr>, la mondfama <ind><tld/>isto</ind>
        <fnt>
          <bib>Kon11</bib>
          <lok>Blogo de lingvemulo</lok>
        </fnt>.
        <trd lng="de">Semiotiker</trd>
      </ekz>
    </dif>
  </snc>
  <snc mrk="semiotik.0o.MED">
    <uzo tip="fak">MED</uzo>
    <uzo tip="stl">ARK</uzo>
    <dif>
      Medicina fako studanta la signojn de malsanoj.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_m;&c_ib;&c_jo;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="cs">s&eacute;miotika</trd>
  <trd lng="de">Semiotik</trd>
  <trdgrp lng="es">
    <trd>semiolog&iacute;a<klr tip="amb"> (ling&uuml;&iacute;stica)</klr></trd>,
    <trd>semi&oacute;tica</trd>
  </trdgrp>
  <trd lng="fa">&a_n;&a_sh;&a_A;&a_n;&a_h;&zwnj;&a_sh;&a_n;&a_A;&a_s;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>s&eacute;miologie<klr tip="amb"> (linguistique)</klr></trd>,
    <trd>s&eacute;miotique</trd>
  </trdgrp>
  <trd lng="it">semiotica</trd>
  <trd lng="pl">semiotyka</trd>
  <trd lng="ru">&c_s;&c_je;&c_m;&c_i;&c_o;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">s&eacute;miotika</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_je;&c_m;&c_ib;&c_o;&c_t;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_je;&c_m;&c_ib;&c_o;&c_l;&c_o;&c_g;&c_ib;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#31526;&#21495;&#23398; <pr>f&uacute;h&agrave;oxu&eacute;</pr></trd>,
    <trd>&#31526;&#34399;&#23416; <pr>f&uacute;h&agrave;oxu&eacute;</pr></trd>
  </trdgrp>
</drv>
</art>

