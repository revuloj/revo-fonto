<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>zimbalon</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="zimbalon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Cigan- kaj hungar-devena <ref tip="super"
      cel="instru2.kord0o">kordinstrumento</ref>,
      speco de 35-korda <ref tip="vid" cel="citr.0o.MUZ">citro</ref>
      sonigata per du bastonetoj kun kurba, lede a&ubreve; &scirc;tofe
      garnita ekstrema&jcirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_m;&c_b;&c_a;&c_l;&c_y;</trd>
  <trd lng="de">Zymbel</trd>
  <trdgrp lng="en">
    <trd>dulcimer</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cymbalum</trd>
  </trdgrp>
  <trd lng="hu">cimbalom</trd>
  <trd lng="nl">hakkebord</trd>
  <trd lng="pt">c&iacute;mbalo</trd>
  <trd lng="ru">&c_c;&c_i;&c_m;&c_b;&c_a;&c_l;&c_y;</trd>
  <trd lng="sk">cimbal</trd>
</drv>
</art>

