<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rempar.xml,v 1.27 2024/04/13 20:15:53 revo Exp $">
<kap>
  <ofc>*</ofc> <rad>rempar</rad>/o
</kap>
<drv mrk="rempar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="rempar.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="super" cel="mur.0ego">Murego</ref> a&ubreve; tera alta&jcirc;o
      anta&ubreve;ita de fosa&jcirc;o, ordinare &ccirc;irka&ubreve; urbo a&ubreve; alia grava loko por
      &gcirc;in defendi (a&ubreve; sie&gcirc;i):
      <ekz>
        ili <klr>[&#x2026;]</klr> &scirc;utis &ccirc;irka&ubreve; la urbo remparon, kiu atingis la muron
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 20:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen <tld/>oj alproksimi&gcirc;as al la urbo, por venkopreni &gcirc;in
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 32:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        li kuradas en la re&gcirc;a parko kaj sur la <tld/>oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Malbonkonduta knabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la verda <tld/>o, kiu &ccirc;irka&ubreve;as Kopenhagon
        <fnt>
          <bib>Fab2</bib>
          <lok>El fenestro en Vartou</lok>
        </fnt>;
      </ekz>
      <ekz>
        la potenca kastelo, konsistanta el unu &ccirc;efa argila <tld/>o kaj
        unu anta&ubreve;<tld/>o, ampleksas 6 hektarojn
        <fnt><bib>IntArkeol</bib>, <lok>p. 38a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="dig.0o">digo</ref>,
      <ref cel="bar.0ajxo">bara&jcirc;o</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_m;&c_u;&c_r;</trd>,
      <trd>&c_c;&c_y;&c_t;&c_a;&c_d;&c_e;&c_l;&c_mol;&c_n;&c_y; &c_v;&c_a;&c_l;</trd>
    </trdgrp>
    <trd lng="ca">muralla</trd>
    <trd lng="es">muralla</trd>
    <trd lng="hu">s&aacute;nc</trd>
    <trdgrp lng="it">
      <trd>mura<klr> (difensive)</klr></trd>,
      <trd>ramparo</trd>,
      <trd>baluardo</trd>,
      <trd>vallo</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>obwa&lstroke;owanie</trd>,
      <trd>wa&lstroke; obronny</trd>
    </trdgrp>
  <trdgrp lng="pt">
    <trd>baluarte</trd>,
    <trd>trincheira</trd>,
    <trd>muralha</trd>,
    <trd>anteparo</trd>,
    <trd>reparo</trd>
  </trdgrp>
    <trd lng="sv">mur</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_je;&c_p;&c_o;&c_s;&c_t;&c_n;&c_a;&c_ja; &c_s;&c_t;&c_je;&c_n;&c_a;</trd>,
      <trd>&c_g;&c_o;&c_r;&c_o;&c_d;&c_s;&c_k;&c_a;&c_ja; &c_s;&c_t;&c_je;&c_n;&c_a;</trd>,
      <trd>&c_v;&c_a;&c_l;</trd>,
      <trd>&c_n;&c_a;&c_s;&c_y;&c_p;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="rempar.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Defendilo, &scirc;irmilo.
    </dif>
    <ref tip="sin" cel="bastio.0o.FIG">bastiono<sncref/></ref>
    <trd lng="be">&c_u;&c_m;&c_a;&c_c;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ib;</trd>
    <trd lng="ca">empara</trd>
    <trdgrp lng="es">
      <trd>protecci&oacute;n</trd>,
      <trd>mampara</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>barriera<klr> (riparo)</klr></trd>,
      <trd>riparo</trd>
    </trdgrp>
    <trd lng="pl">os&lstroke;ona</trd>
    <trd lng="ru">&c_u;&c_k;&c_r;&c_je;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <refgrp tip="vid">
    <ref cel="bar.0ilo">barilo</ref>,
    <ref cel="citade.0o">citadelo</ref>,
    <ref cel="dig.0o">digo</ref>,
    <ref cel="mur.0o.FIG">muro <sncref/></ref>
  </refgrp>
  <trdgrp lng="cs">
    <trd>hradba</trd>,
    <trd>n&aacute;sep</trd>,
    <trd>opevn&ecaron;n&iacute;</trd>,
    <trd>val</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Wall</trd>,
    <trd>Bollwerk</trd>,
    <trd>Schutzwall</trd>,
    <trd>Befestigung</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>rampart</trd>,
    <trd>battlement</trd>,
    <trd>bulwark</trd>
  </trdgrp>
  <trd lng="fr">rempart</trd>
  <trd lng="hu">v&eacute;d&odblac;b&aacute;stya</trd>
  <trdgrp lng="ja">
    <trd>&#x57CE;&#x58C1;  <pr>&#x3058;&#x3087;&#x3046;&#x3078;&#x304D;</pr></trd>,
    <trd>&#x5841;&#x58C1;  <pr>&#x308B;&#x3044;&#x3078;&#x304D;</pr></trd>,
    <trd>&#x9632;&#x5FA1;&#x7269;  <pr>&#x307C;&#x3046;&#x304E;&#x3087;&#x3076;&#x3064;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>wal</trd>;
    <trd>bolwerk</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hradba</trd>,
    <trd>opevnenie</trd>,
    <trd>val</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd><klr>(&c_f;&#x43E;&c_r;&c_t;&c_je;&c_ch;&#x43D;&c_i;&c_j;)</klr> &c_v;&c_a;&#x43B;</trd>,
    <trd>&c_f;&#x43E;&c_r;&c_t;&c_je;&c_ch;&#x43D;&c_a; &c_s;&c_t;&c_ib;&#x43D;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x58C1; <pr>b&#xEC;</pr></trd>,
    <trd>&#x7B51;&#x57CE; <pr>zh&#xF9;ch&#xE9;ng</pr></trd>,
    <trd>&#x7BC9;&#x57CE; <pr>zh&#xF9;ch&#xE9;ng</pr></trd>
  </trdgrp>
</drv>
</art>
