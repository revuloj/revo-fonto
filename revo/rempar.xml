<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc> <rad>rempar</rad>/o
</kap>
<drv mrk="rempar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="rempar.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Muro a&ubreve; tera alta&jcirc;o anta&ubreve;ita de fosa&jcirc;o kaj starigita por defendi
      urbon a&ubreve; alian gravan lokon:
      <ekz>la potenca kastelo, konsistanta el unu &ccirc;efa argila <tld/>o kaj unu anta&ubreve;remparo, ampleksas 6
hektarojn<fnt><bib>IntArkeol</bib>, <lok>p. 38a</lok></fnt>.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="mur.0ego">murego</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_m;&c_u;&c_r;</trd>,
      <trd>&c_c;&c_y;&c_t;&c_a;&c_d;&c_e;&c_l;&c_mol;&c_n;&c_y; &c_v;&c_a;&c_l;</trd>
    </trdgrp>
    <trd lng="fr">mur<klr> (d'une ville)</klr></trd>
    <trd lng="hu">s&aacute;nc</trd>
    <trd lng="sv">mur</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_je;&c_p;&c_o;&c_s;&c_t;&c_n;&c_a;&c_ja; &c_s;&c_t;&c_je;&c_n;&c_a;</trd>,
      <trd>&c_g;&c_o;&c_r;&c_o;&c_d;&c_s;&c_k;&c_a;&c_ja; &c_s;&c_t;&c_je;&c_n;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="rempar.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Defendilo, &scirc;irmilo.
    </dif>
    <refgrp tip="vid">
      <ref cel="bar.0ilo">barilo</ref>,
      <ref cel="bastio.0o">bastiono</ref>,
      <ref cel="citade.0o">citadelo</ref>,
      <ref cel="dig.0o">digo</ref>,
      <ref cel="mur.0o.FIG">muro</ref>.
    </refgrp>
    <trd lng="be">&c_u;&c_m;&c_a;&c_c;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ib;</trd>
  </snc>
  <trdgrp lng="de">
    <trd>Wall</trd>;
    <trd>Bollwerk</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>rempart</trd>
  </trdgrp>
  <trd lng="hu">v&eacute;d&odblac;b&aacute;stya</trd>
  <trdgrp lng="nl">
    <trd>wal</trd>;
    <trd>bolwerk</trd>
  </trdgrp>
</drv>
</art>
