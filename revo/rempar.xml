<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc> <rad>rempar</rad>/o
</kap>
<drv mrk="rempar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="rempar.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="super" cel="mur.0ego">Murego</ref> a&ubreve; tera alta&jcirc;o
      anta&ubreve;ita
      de fosa&jcirc;o, ordinare &ccirc;irka&ubreve; urbo a&ubreve; alia grava loko por
      &gcirc;in defendi:
      <ekz>
        la potenca kastelo, konsistanta el unu &ccirc;efa argila <tld/>o kaj
        unu
        anta&ubreve;<tld/>o, ampleksas 6 hektarojn
        <fnt><bib>IntArkeol</bib>, <lok>p. 38a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_m;&c_u;&c_r;</trd>,
      <trd>&c_c;&c_y;&c_t;&c_a;&c_d;&c_e;&c_l;&c_mol;&c_n;&c_y; &c_v;&c_a;&c_l;</trd>
    </trdgrp>
    <trd lng="hu">s&aacute;nc</trd>
    <trdgrp lng="it">
      <trd>mura<klr> (difensive)</klr></trd>,
      <trd>ramparo</trd>,
      <trd>baluardo</trd>,
      <trd>vallo</trd>
    </trdgrp>
  <trdgrp lng="pt">
    <trd>baluarte</trd>,
    <trd>trincheira</trd>,
    <trd>muralha</trd>,
    <trd>anteparo</trd>,
    <trd>reparo</trd>
  </trdgrp>
    <trd lng="sv">mur</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_je;&c_p;&c_o;&c_s;&c_t;&c_n;&c_a;&c_ja; &c_s;&c_t;&c_je;&c_n;&c_a;</trd>,
      <trd>&c_g;&c_o;&c_r;&c_o;&c_d;&c_s;&c_k;&c_a;&c_ja; &c_s;&c_t;&c_je;&c_n;&c_a;</trd>,
      <trd>&c_v;&c_a;&c_l;</trd>,
      <trd>&c_n;&c_a;&c_s;&c_y;&c_p;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="rempar.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Defendilo, &scirc;irmilo.
    </dif>
    <refgrp tip="vid">
      <ref cel="bar.0ilo">barilo</ref>,
      <ref cel="bastio.0o">bastiono</ref>,
      <ref cel="citade.0o">citadelo</ref>,
      <ref cel="dig.0o">digo</ref>,
      <ref cel="mur.0o.FIG">muro <sncref/></ref>
    </refgrp>
    <trd lng="be">&c_u;&c_m;&c_a;&c_c;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ib;</trd>
    <trdgrp lng="it">
      <trd>barriera<klr> (riparo)</klr></trd>,
      <trd>riparo</trd>
    </trdgrp>
    <trd lng="ru">&c_u;&c_k;&c_r;&c_je;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <trdgrp lng="de">
    <trd>Wall</trd>;
    <trd>Bollwerk</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>rampart</trd>,
    <trd>battlement</trd>,
    <trd>bulwark</trd>
  </trdgrp>
  <trd lng="fr">rempart</trd>
  <trd lng="hu">v&eacute;d&odblac;b&aacute;stya</trd>
  <trdgrp lng="nl">
    <trd>wal</trd>;
    <trd>bolwerk</trd>
  </trdgrp>
</drv>
</art>
