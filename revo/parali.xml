<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: parali.xml,v 1.31 2022/06/10 22:29:15 revo Exp $">
<kap>
  <ofc>*</ofc> <rad>paraliz</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="parali.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="parali.0i.MED">
    <dif>
      Perdigi, tute a&ubreve; ne, la movkapablon kaj
      eventuale la sentkapablon.
    </dif>
    <refgrp tip="vid">
      <ref cel="inert">inertigi</ref>,
      <ref cel="rigid">rigidigi</ref>,
      <ref cel="mov.sen0igi">senmovigi</ref>
    </refgrp>
    <trdgrp lng="ca">
      <trd>paralitzar</trd>,
      <trd>immobilitzar</trd>
    </trdgrp>
    <trd lng="pl">by&cacute; <ind>sparali&zdot;owanym</ind></trd>
    <trd lng="ro">fi  <ind>paralizat</ind></trd>
  </snc>
  <snc mrk="parali.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senkapabligi, senefikigi,
      <ref tip="vid" cel="frost.0igi.FIG">frostigi<sncref/></ref>:
      <ekz>
        semi konstantan malpacon kaj <tld/>i &ccirc;ian vivon kaj progreson
        en nia afero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la timo <tld/>is lian penson<fnt>B</fnt>;
      </ekz>
      <ekz>
        tio <tld/>is mian entuziasmon;
      </ekz>
      <ekz>
        nur nenormalaj cirkonstancoj povas <tld/>i tiun naturan
        senton<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la rutino <tld/>as lin;
      </ekz>
      <ekz>
        sen&ccirc;esaj konfiskadoj <tld/>as la liberecon de la gazetaro.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>aturar</trd>,
      <trd>estancar</trd>,
      <trd>obstaculitzar</trd>      
    </trdgrp>
    <trd lng="pl">parali&zdot;owa&cacute;</trd>
    <trd lng="ro">paraliza</trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_l;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="br">seiza&ntilde;</trd>
  <trd lng="ca">paralitzar</trd>
  <trdgrp lng="cs">
    <trd>ochromit</trd>,
    <trd>paralyzovat</trd>
  </trdgrp>
  <trd lng="de">l&auml;hmen</trd>
  <trd lng="en">paralyze</trd>
  <trd lng="es">paralizar</trd>
  <trd lng="fr">paralyser</trd>
  <trdgrp lng="hu">
    <trd>megb&eacute;n&iacute;t</trd>,
    <trd>paraliz&aacute;l</trd>,
    <trd>cselekv&eacute;sk&eacute;ptelenn&eacute; tesz</trd>
  </trdgrp>
  <trd lng="id">lumpuh</trd>
  <trd lng="nl">verlammen</trd>
  <trd lng="pt">paralisar</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_a;&c_l;&c_i;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>ochrn&uacute;&tcaron;</trd>,
    <trd>ochromi&tcaron;</trd>,
    <trd>paralyzova&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&c_a;&c_r;&c_a;&#x43B;&c_ib;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="parali.0o">
  <kap><tld/>o</kap>
  <snc mrk="parali.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Stato de iu a&ubreve; io <tld/>ita.
    </dif>
    <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_a;&c_l;&c_ju;&c_sh;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_a;&c_l;&c_ib;&c_ch;</trd>
  </trdgrp>
  <trd lng="br">seizi</trd>
  <trd lng="ca">par&agrave;lisi</trd>
  <trdgrp lng="cs">
    <trd>ochrnut&iacute;</trd>,
    <trd>paral&yacute;za</trd>
  </trdgrp>
  <trd lng="de">L&auml;hmung</trd>
  <trd lng="en">paralysis</trd>
  <trd lng="es">par&aacute;lisis</trd>
  <trd lng="fr">paralysie</trd>
  <trdgrp lng="hu">
    <trd>b&eacute;nul&aacute;s</trd>,
    <trd>b&eacute;nas&aacute;g</trd>,
    <trd>paral&iacute;zis</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>ke<ind>lumpuh</ind>an</trd>,
    <trd>paralisis</trd>
  </trdgrp>
  <trd lng="nl">verlamming</trd>
  <trd lng="pl">parali&zdot;</trd>
  <trd lng="pt">paralisia</trd>
  <trd lng="ro">paralizie</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_a;&c_l;&c_i;&c_ch;</trd>
  <trdgrp lng="sk">
    <trd>obrna</trd>,
    <trd>ochrnutie</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&c_a;&c_r;&c_a;&#x43B;&c_ib;&c_ch;</trd>
</drv>

<drv mrk="parali.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="parali.0ulo.MED">
    <dif>
      Suferanto pro parta a&ubreve; &gcirc;enerala <tld/>o:
      <ekz>
        kiu volus edzini&gcirc;i al <tld/>ulo?
        <fnt>
          <aut>Tageto</aut>,
          <vrk>Demandoj kaj respondoj</vrk>,
          <lok>en: Vivprotokoloj, 2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">paral&iacute;tic<klr> (subst.)</klr></trd>
  <trdgrp lng="cs">
    <trd>ochrnut&yacute; &ccaron;l.</trd>,
    <trd>paralytik</trd>
  </trdgrp>
  <trd lng="es">paral&iacute;tico<klr> (subst.)</klr></trd>
  <trd lng="fr">paralytique<klr> (subst.)</klr></trd>
  <trd lng="pl">paralityk</trd>
  <trd lng="pt">paral&iacute;tico</trd>
  <trd lng="ro">paralitic</trd>
  <trd lng="sk">ochrnut&yacute; &ccaron;lovek</trd>
  <trd lng="uk">&#x43F;&c_a;&c_r;&c_a;&#x43B;&c_ib;&c_t;&c_i;&#x43A;</trd>
</drv>
</art>
