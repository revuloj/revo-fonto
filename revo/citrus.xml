<art mrk="$Id$">
<kap>
<rad>citrus</rad>/o
</kap>
<drv mrk="citrus.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="rutac.0oj">rutacoj</ref>
      (<trd lng="la">Citrus</trd>).
    </dif>
  </snc>
  <refgrp tip="sub">
    <ref cel="citron.0ujo">citronujo</ref>,
    <ref cel="orangx.0arbo0ujo">oran&gcirc;ujo</ref>
  </refgrp>
  <trd lng="de">Zitrusfrucht</trd>
  <trd lng="fr">citrus</trd>
  <trd lng="hu">citrus</trd>
  <trd lng="ru">&c_c;&c_i;&c_t;&c_r;&c_u;&c_s;&c_o;&c_v;&c_y;&c_je;</trd>
</drv>
</art>
