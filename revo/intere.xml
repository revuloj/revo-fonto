<drv mrk="intere.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="intere.0i.tenadi_atenton">
    <dif>
      Tenadi ies atenton, persistemon,
    </dif>
    <subsnc mrk="intere.0i.per_scivolo">
      <dif>
        vekante scivolemon, deziron pli detale koni:
        <ekz>
          hodia&ubreve; <klr>[la problemo]</klr> <tld/>as la publikon,
          morga&ubreve; &gcirc;i povas esti indiferenta por &gcirc;i
          <fnt><bib>Marta</bib></fnt>;
        </ekz>
        <ekz>
          ili ricevas francan nomon, kiun mi ne povas elparoli kaj kiu min e&ccirc; tute ne
          <tld/>as
          <fnt>
            <bib>Fab2</bib>
            <lok>Najbaraj familioj</lok>
          </fnt>;
        </ekz>
        <ekz>
          sur lia viza&gcirc;o oni ne povis legi, &ccirc;u lin <tld/>as la priskribo, a&ubreve; &ccirc;u indignigas
          lin la mensogo
          <fnt>
            <bib>Far3</bib>
            <lok>&Ccirc;apitro VI</lok>
          </fnt>;
        </ekz>
        <ekz>
          ni anta&ubreve;diras al la homoj la estontecon, &ccirc;ar &gcirc;i <tld/>as ilin
          <fnt>
            <bib>Far3</bib>
            <lok>&Ccirc;apitro IX</lok>
          </fnt>;
        </ekz>
        <ekz>
          nenio min <tld/>as plu kaj neniam mi malfermas &jcirc;urnalon
          <fnt>
            <bib>ChL</bib>
            <lok>&Ccirc;apitro Dekkvina</lok>
          </fnt>;
        </ekz>
        <ekz>
          kion <klr>(per kio)</klr> do povas min <tld/>i tio,
          kio fari&gcirc;os al vi
          <fnt><bib>Marta</bib></fnt>?
        </ekz>
        <ekz>
          la lingvistikaj demandoj <tld/>as multajn esperantistojn
          <fnt><bib>VdE</bib></fnt>;
        </ekz>
        <ekz>
          <nom>Arne</nom> atentis pri la aferoj, kiuj <tld/>is lin
          <fnt>
            <bib>InfanTorent2</bib>
            <lok>Unua Parto</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="de">interessieren</trd>
    </subsnc>
    <subsnc mrk="intere.0i.pro_graveco">
      <dif>
        pro graveco, pro kapablo alporti utilon
        (materian a&ubreve; moralan) a&ubreve; malutilon:
        <ekz>
          oni devas vivi kiel fremdulo en fremda lando, kie oni neniun <tld/>as
          <fnt>
            <bib>Fab3</bib>
            <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
          </fnt>;
        </ekz>
        <ekz>
          mi iros mian propran vojon, kaj tiam la aliaj min plu ne <tld/>os
          <fnt>
            <bib>Fab3</bib>
            <lok>Io</lok>
          </fnt>;
        </ekz>
        <ekz>
          la aklamoj ne <tld/>is lin plu
          <fnt>
            <bib>Far1</bib>
            <lok>&Ccirc;apitro XXI</lok>
          </fnt>;
        </ekz>
        <ekz>
          ili &ccirc;iuj kune ne <tld/>as min tiel, kiel vi sola
          <fnt>
            <bib>Far1</bib>
            <lok>&Ccirc;apitro XIV</lok>
          </fnt>;
        </ekz>
        <ekz>
          mi estas amiko de la princo kaj tial tre <tld/>as min la &gcirc;enerala opinio pri li
          <fnt>
            <bib>IK</bib>
            <lok>&Ccirc;aptiro III</lok>
          </fnt>;
        </ekz>
        <ekz>
          mono lin ne <tld/>as, kiam li povas havi sufi&ccirc;e da &gcirc;ii
          <fnt>
            <bib>Rabistoj</bib>
            <lok>Akto Dua</lok>
          </fnt>;
        </ekz>
        <ekz>
          en la afero, kiu nun vin <tld/>as, mi mem persone
          ne estas kompetenta ju&gcirc;anto
          <fnt><bib>Marta</bib></fnt>;
        </ekz>
        <ekz>
          ni facile forgesas, kio nin ne <tld/>as
          <fnt><bib>PrV</bib></fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="grav.0i">gravi</ref>,
        <ref cel="koncer.0i">koncerni</ref>
      </refgrp>
    </subsnc>
    <subsnc mrk="intere.0i.per_bonfaremo">
      <uzo tip="stl">ARK</uzo>
      <dif>
        vekante bonfareman senton:
        <ekz>
          mi ekkonis anta&ubreve; nelonge iun malri&ccirc;an virinon,
          kiu min tre <tld/>as
          <fnt><bib>Marta</bib></fnt>;
        </ekz>
        <ekz>
          tiu malri&ccirc;a familio <tld/>as multajn personojn
          <fnt><bib>PV</bib></fnt>.
        </ekz>
      </dif>
    <trd lng="de">k&uuml;mmern</trd>
    </subsnc>
    <trd lng="id">ter<ind>tarik</ind></trd>
    <trdgrp lng="pl">
      <trd>interesowa&cacute;</trd>,
      <trd>zaciekawia&cacute;</trd>,
      <trd>zajmowa&cacute;<klr> (przest.)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="intere.0i.per_profito">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Doni profiton en iu afero:
      <ekz>
        <tld/>i iun en komerca, industria societo;
      </ekz>
      <ekz>
        &ccirc;iuj firmoj, kiuj havas <tld/>on en la komercado de <nom>Browning</nom>
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi estas industriisto, devigata gardi miajn <tld/>ojn
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">beteiligen</trd>
    <trd lng="pl">interesowa&cacute;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_ib;&c_k;&c_a;&c_v;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_n;&c_ib;&c_m;&c_a;&c_c;&c_mol; <ind>&c_c;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</ind></trd>,
    <trd>&c_p;&c_r;&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol; <ind>&c_ib;&c_n;&c_t;&c_a;&c_r;&c_e;&c_s;</ind></trd>
  </trdgrp>
  <trd lng="cs">zaj&iacute;mat</trd>
  <trd lng="en">interest</trd>
  <trd lng="es">interesar</trd>
  <trdgrp lng="fr">
    <trd>int&eacute;resser</trd>,
    <trd>brancher<klr tip="ind"> (plaire)</klr></trd>,
    <trd>plaire (&agrave;)</trd>,
    <trd>concerner</trd>,
    <trd>importer (&agrave;)</trd>,
    <trd>s'<ind>appliquer</ind> (&agrave;)</trd>,
    <trd>se <ind>rapporter</ind> (&agrave;)</trd>,
    <trd>relever (de)</trd>,
    <trd>toucher (&agrave;)</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;rdekli</trd>,
    <trd>&eacute;rdekelt <klr tip="amb">(vmiben)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8208;&#x5473;&#x3092;&#x3072;&#x304F;  <pr>&#x304D;&#x3087;&#x3046;&#x307F;&#x3092;&#x3072;&#x304F;</pr></trd>,
    <trd>&#x95A2;&#x5FC3;&#x3092;&#x6301;&#x305F;&#x305B;&#x308B; 
    <pr>&#x304B;&#x3093;&#x3057;&#x3093;&#x3092;&#x3082;&#x305F;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x95A2;&#x4FC2;&#x3055;&#x305B;&#x308B;  <pr>&#x304B;&#x3093;&#x3051;&#x3044;&#x3055;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x5229;&#x5BB3;&#x95A2;&#x4FC2;&#x304C;&#x3042;&#x308B; 
    <pr>&#x308A;&#x304C;&#x3044;&#x304B;&#x3093;&#x3051;&#x3044;&#x304C;&#x3042;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">interesseren</trd>
  <trd lng="pt">interessar</trd>
  <trd lng="ru">
  &c_i;&c_n;&c_t;&c_je;&c_r;&c_je;&c_s;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>p&uacute;ta&tcaron;</trd>,
    <trd>zauj&iacute;ma&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_c;&c_ib;&c_k;&c_a;&c_v;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_ib;&c_n;&c_t;&c_je;&c_r;&c_je;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_k;&c_l;&c_i;&c_k;&c_a;&c_t;&c_i; &c_ib;&c_n;&c_t;&c_je;&c_r;&c_je;&c_s;</trd>,
    <trd>(&c_p;&c_r;&c_i; &c_z;&c_a;&c_p;&c_je;&c_r;&c_je;&c_ch;&c_je;&c_n;&c_n;&c_ib; &c_shch;&c_je;) &c_o;&c_b;&c_h;&c_o;&c_d;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x5174;&#x81F4; <pr>x&#xEC;ngzh&#xEC;</pr></trd>
</drv>

<drv mrk="intere.0o">
  <kap><tld/>o</kap>
  <snc mrk="intere.0o.eco">
    <dif>
      Eco de tio, kio
    </dif>
    <subsnc mrk="intere.0o.scivolo">
      <dif>
        <tld/>as <sncref ref="intere.0i.per_scivolo"/>:
        <ekz>
          tiu libro havas grandan, neniun <tld/>on;
        </ekz>
        <ekz>
          <tld/>on liaj okupi&gcirc;oj certe ne povis prezenti
          <fnt>
            <bib>Fab3</bib>
            <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
          </fnt>;
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="intere.0o.partopreno">
      <dif>
        <tld/>as <sncref ref="intere.0i.pro_graveco"/>:
        <ekz>
          tio estas sen <tld/>o por mia decido;
        </ekz>
        <ekz>
          &scirc;i pensis pri <klr>[&#x2026;]</klr> juna gefian&ccirc;a paro, kiu havis por &scirc;i tre grandan
          <tld/>on, &ccirc;ar &scirc;i mem ja estis duono de tiu paro
          <fnt>
            <bib>Fab3</bib>
            <lok>Kolo de botelo</lok>
          </fnt>;
        </ekz>
        <ekz>
          <klr>[demando]</klr>, kiu povas havi <tld/>on por la homaranoj
          <fnt><bib>Homaranismo</bib><lok/></fnt>;
        </ekz>
        <ekz>
          <klr>[lingvo]</klr>, en kiu povus esti publikigataj tiuj
          verkoj, kiuj havas egalan <tld/>on por &ccirc;iuj popoloj
          <fnt><bib>F</bib><lok>Deklaracio</lok></fnt>;
        </ekz>
        <ekz>
          &ccirc;u plu vivi, a&ubreve; &ccirc;u morti tuj? kio estas
          preferinda, kiam la vivo perdis &ccirc;ian <tld/>on
          <fnt><bib>Lasu</bib>
          <lok>Revenis amo, feli&ccirc;u ni!</lok></fnt>?
        </ekz>
      </dif>
    </subsnc>
    <trdgrp lng="be">
      <trd>&c_c;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_ib;&c_n;&c_t;&c_a;&c_r;&c_e;&c_s;</trd>,
    </trdgrp>
    <trdgrp lng="id">
      <trd>minat</trd>,
      <trd>keter<ind>tarik</ind>an</trd>,
      <trd>ke<ind>penting</ind>an</trd>
    </trdgrp>
  </snc>
  <snc mrk="intere.0o.sento">
    <dif>
      Sento de tiu, kiu estas
    </dif>
    <subsnc mrk="intere.0o.atento">
      <dif>
        <tld/>ata <sncref ref="intere.0i.per_scivolo"/>:
        <ekz>
          la legendo <klr>[&#x2026;]</klr> estis vo&ccirc;legita <klr>[kaj]</klr> &ccirc;iuj a&ubreve;skultis kun
          stre&ccirc;ita atento kaj <tld/>o
          <fnt>
            <bib>Fab3</bib>
            <lok>Juda knabino</lok>
          </fnt>;
        </ekz>
        <ekz>
          la juna virino kun <tld/>o kaj senkonscia kompato rigardis la personon
          <fnt>
            <bib>Marta</bib>
          </fnt>;
        </ekz>
        <ekz>
          vi eble sekvas kun <tld/>o la malfacilan bataladon en la granda multemiliona
          lando
          <fnt>
            <bib>Paroloj</bib>
            <vrk>Parolado anta&ubreve; la Dua Kongreso Esperantista en Gen&egrave;ve en la 28a de a&ubreve;gusto
            1906</vrk>
          </fnt>;
        </ekz>
        <ekz>
          komence &scirc;i legis malrapide, atente, poste kun entuziasmo, kun preska&ubreve; febra
          <tld/>o
          <fnt>
            <bib>Marta</bib>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="intere.0o.intenco">
      <dif>
        <tld/>ata <sncref ref="intere.0i.pro_graveco"/>:
        <ekz>
          la tuta esperantistaro havas grandan <tld/>on pri la oficiala
          enkonduko de &gcirc;ia lingvo en lernejoj;
        </ekz>
        <ekz>
          la persona <tld/>o tro ofte kondukas la homojn
          <fnt><bib>PV</bib></fnt>;
        </ekz>
        <ekz>
          ne ekzistas kareso sen <tld/>o
          <fnt><bib>PrV</bib></fnt>;
        </ekz>
        <ekz>
          al mi pli pla&ccirc;as, ke oni estas kompleza por mi
          el la pura koro kaj ne pro <tld/>o
          <fnt><bib>Revizoro</bib><lok>4</lok></fnt>;
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="intere.0o.bonvolo">
      <dif>
        <tld/>ata <sncref ref="intere.0i.per_bonfaremo"/>:
        <ekz>
          mia konsilo estas nur pro <tld/>o al via sano;
        </ekz>
        <ekz>
          mi havas grandan <tld/>on al, por tiu familio;
        </ekz>
        <ekz>
          tio vekis en mi <tld/>on por la malliberulo
          <fnt><bib>Fab3</bib>
          <lok>Kion rakontis la kvara muso</lok></fnt>;
        </ekz>
      </dif>
    </subsnc>
    <ref tip="sin" cel="intere.0igxo"><tld/>i&gcirc;o</ref>
  <trdgrp lng="be">
    <trd>&c_c;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_ib;&c_n;&c_t;&c_a;&c_r;&c_e;&c_s;</trd>,
    <trd>&c_z;&c_a;&c_c;&c_ib;&c_k;&c_a;&c_w;&c_l;&c_je;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
    <trdgrp lng="pl">
      <trd>zainteresowanie</trd>,
      <trd>zaciekawienie</trd>
    </trdgrp>
  </snc>
  <snc mrk="intere.0o.pro_graveco">
    <dif>
      Tio, kio promesas <ref cel="util.0o">utilo</ref>n, <ref cel="profit.0o">profito</ref>n ktp:
      <ekz>
        tion postulas la <tld/>oj de la afero
        <fnt>
          <bib>DL</bib>
          <lok>Dua Libro de l' Lingvo Internacia</lok>
        </fnt>;
      </ekz>
      <ekz>
        pro konkurantaj <tld/>oj ili trovi&gcirc;adis en konstanta
        malpaco
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi faros &ccirc;ion, kion mi povos, por trovi por vi lecionojn, cetere &gcirc;i koncernas ja
        ne sole vian <tld/>on, sed anka&ubreve; mian propran
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu kutimo devas havi siajn ka&ubreve;zojn, bazitajn sur la <tld/>oj a&ubreve; cirkonstancoj
        de la personoj, kiuj &gcirc;in observas
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiaj proletoj estas &ccirc;iam pretaj perfidi la <tld/>ojn de
        sia klaso
        <fnt><bib>Lanti</bib><lok>Kio estas bur&gcirc;o?</lok></fnt>;
      </ekz>
      <ekz>
        <nom>Volter</nom> opiniis necesa la kredon je dio, <klr>[sed tio]</klr>
        eble estis nur simpla klasa <tld/>o de ri&ccirc;ulo, kiu volas certigi al si la posedon
        <fnt>
          <bib>Kandid</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la intermiksadon de la <tld/>oj de lando kun la <tld/>oj
        de tiu a&ubreve; alia gento, lingvo a&ubreve; religio mi
        rigardas kiel resta&jcirc;on de la tempoj barbaraj
        <fnt><bib>OV</bib><lok>&para; III-6 (Homaranismo)</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="koncer.0o">koncerno</ref>
    <refgrp tip="vid">
      <ref cel="avanta.0o">avanta&gcirc;o</ref>,
      <ref cel="cel.0o">celo</ref>,
      <ref cel="dezir.0o">deziro</ref>,
      <ref cel="intenc.0o">intenco</ref>,
      <ref cel="klopod.0o">klopodo</ref>,
      <ref cel="streb.0o">strebo</ref>,
      <ref cel="vol.0o">volo</ref>,
      <ref cel="zorg.0o.peno">zorgo<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_ib;&c_n;&c_t;&c_a;&c_r;&c_e;&c_s;</trd>,
      <trd>&c_k;&c_a;&c_r;&c_y;&c_s;&c_mol;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="pl">interes</trd>
  </snc>
  <trd lng="af">belangstelling</trd>
  <trd lng="am">&#x134D;&#x120B;&#x130E;&#x1275;</trd>
  <trd lng="ar">&a_m;&a_S;&a_l;&a_H;&a_t_marbuta;</trd>
  <trd lng="az">faiz</trd>
  <trd lng="bn">&#x9B8;&#x9C1;&#x9A6;</trd>
