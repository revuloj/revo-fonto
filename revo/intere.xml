  <trd lng="de">Interesse</trd>
  <trd lng="es">inter&eacute;s</trd>
  <trd lng="et">huvi</trd>
  <trd lng="eu">interes</trd>
  <trdgrp lng="fr">
    <trd>int&eacute;r&ecirc;t<klr tip="ind">
    (qu'on trouve &agrave; qc ou qn)</klr></trd>,
    <trd>utilit&eacute;</trd>,
    <trd>profit<klr tip="ind"> (qu'on trouve dans qc)</klr></trd>,
    <trd>avantage<klr tip="ind"> (qu'on trouve &agrave; qc)</klr></trd>,
    <trd>penchant<klr tip="ind"> (pour qc ou qn)</klr></trd>,
    <trd>inclination<klr tip="ind"> (pour qc ou qn)</klr></trd>,
    <trd>go&ucirc;t<klr tip="ind"> (pour qc ou qn)</klr></trd>,
    <trd>sympathie<klr tip="ind"> (pour qc ou qn)</klr></trd>,
    <trd>sollicitude</trd>,
    <trd>attention<klr tip="ind"> (pour qc ou qn)</klr></trd>.
  </trdgrp>
  <trd lng="fy">rinte</trd>
  <trd lng="ga">&uacute;s</trd>
  <trd lng="gd">&ugrave;idh</trd>
  <trd lng="gl">interese</trd>
  <trd lng="gu">&#xAB0;&#xAB8;</trd>
  <trd lng="ha">amfani</trd>
  <trd lng="hi">&#x92C;&#x94D;&#x92F;&#x93E;&#x91C;</trd>
  <trd lng="hmn">paj</trd>
  <trd lng="hr">interes</trd>
  <trd lng="ht">enter&egrave;</trd>
  <trdgrp lng="hu">
    <trd>&eacute;rdek</trd>,
    <trd>&eacute;rdekel&odblac;d&eacute;s</trd>,
