<drv mrk="intere.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="intere.0i.igi_igxi">
    <dif>
      Igi <tld/>i&gcirc;i:
      <ekz>
        <tld/>igi la komercan &ccirc;ambron pri Esperanto
        <fnt><bib>PV</bib></fnt>;
      </ekz>
      <ekz>
        mi provis paroli al kelkaj ekslernantoj kaj e&ccirc; salutis
        ilin per &leftquot;Bo&ubreve;nan tagon&rightquot; a&ubreve;
        &leftquot;Bo&ubreve;nan vesperon&rightquot; por <tld/>igi ilin,
        sed &scirc;ajne ili jam estas tute kontra&ubreve; Esperanto
        <fnt>
        <bib>Voja&gcirc;o</bib>
        <aut>C. Rossetti</aut>
        <vrk>Son&gcirc;o teruras, son&gcirc;o forkuras</vrk>
        <lok>leciono 7a</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam vi sukcesas <tld/>igi kaj kredigi iun pri la neceseco
        de Esperanto, ne diru, ke vi <ctl>konvinkigis</ctl> lin
        <fnt>
        <aut>H. Seppik</aut>,
        <vrk><url ref="http://www.esperanto.mv.ru/Seppik/lec10.html">
        La tuta Esperanto</url></vrk>,
        <lok>1937</lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      La sendiferenco disde
      "<tld/>i <sncref ref="intere.0i.tenadi_atenton"/>"
      estas apena&ubreve; perceptebla, kio riskas sugesti,
      ke <ctl><tld/>i</ctl> estas netransitiva. Eble tial
      &ccirc;i tiu formo mankas en PIV.
    </rim>
    <trdgrp lng="fr">
      <trd>susciter l'<ind>int&eacute;r&ecirc;t</ind></trd>,
      <trd>int&eacute;resser<klr tip="ind"> (faire s'int&eacute;resser)
      </klr></trd>
      <trd>faire s'<ind>int&eacute;resser</ind></trd>
    </trdgrp>
    <trd lng="hu">felkelti az
    <ind>&eacute;rdekl&odblac;d&eacute;s&eacute;t</ind></trd>
    <trd lng="nl">belangstelling wekken</trd>
    <trdgrp lng="pl">
      <trd>zainteresowa&cacute;<klr> (kogo&sacute;, czym&sacute;)</klr>
      </trd>,
      <trd>zaciekawia&cacute;<klr> (kogo&sacute;, czym&sacute;)</klr>
      </trd>
    </trdgrp>
    <trd lng="ru">&c_z;&c_a;&c_i;&c_n;&c_t;&c_je;&c_r;&c_je;&c_s;&c_o;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_k;&c_o;&c_g;&c_o;-&c_l;.
    &c_ch;&c_je;&c_m;-&c_l;.)</klr>
    </trd>
  </snc>
  <snc mrk="intere.0i.igi_a">
    <fnt><bib>PV</bib></fnt>
    <uzo tip="stl">RAR</uzo>
    <dif>
      Fari ion <tld/>a.
    </dif>
    <adm>
      Mi dubas, cxu ~igi sola havas la montritan signifon.
      <aut>MB</aut>
    </adm>
    <trd lng="hu">&eacute;rdekess&eacute; tesz</trd>
    <trd lng="nl">interessant maken</trd>
    <trd lng="pl">uatrakcyjnia&cacute;</trd>
    <trd lng="ru">&c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; <ind>&c_i;&c_n;&c_t;&c_je;&c_r;&c_je;&c_s;&c_n;&c_y;&c_m;</ind>
    </trd>
  </snc>
</drv>

<drv mrk="intere.0igxisin0i">
  <kap><tld/>i&gcirc;i, <var><kap>sin <tld/>i</kap></var></kap>
  <dif>
    I&gcirc;i <tld/>ata:
  </dif>
  <snc ref="intere.0i.per_scivolo">
    <ekz>
      la germanoj tre interesi&gcirc;as pri verkoj
      el sovetia vivo
      <fnt><bib>Metrop</bib><lok>&ccirc;ap. 2</lok></fnt>;
    </ekz>
    <ekz>
      ni &ccirc;iuj <tld/>i&gcirc;as en tiu demando;
    </ekz>
    <ekz>
      <tld/>i&gcirc;i pri libro, pri oratoro.
    </ekz>
    <ref tip="vid" cel="sekv.0i">sekvi</ref>
    <trd lng="nl">zich <ind>interesseren</ind> voor</trd>
    <trdgrp lng="pl">
      <trd>interesowa&cacute; si&eogonek;</trd>,
      <trd>by&cacute; ciekawym<klr> (czego&sacute;)</klr></trd>,
      <trd>zajmowa&cacute; si&eogonek;<klr> (przest.)</klr></trd>
    </trdgrp>
  </snc>
  <snc ref="intere.0i.per_bonfaremo">
    <ekz>
      mi <tld/>i&gcirc;as pri li<fnt>Z</fnt>.
    </ekz>
    <trd lng="nl">zich <ind>interesseren</ind> voor</trd>
    <trdgrp lng="pl">
      <trd>interesowa&cacute; si&eogonek;</trd>,
      <trd>by&cacute; ciekawym<klr> (czego&sacute;)</klr></trd>,
      <trd>zajmowa&cacute; si&eogonek;<klr> (przest.)</klr></trd>
    </trdgrp>
  </snc>
  <snc ref="intere.0i.per_profito">
    <ekz>
      sin <tld/>i en entrepreno;
    </ekz>
    <ekz>
      <tld/>i&gcirc;i en la profitoj.
    </ekz>
    <trd lng="nl">zich <ind>interesseren</ind> in</trd>
    <trdgrp lng="pl">
      <trd>interesowa&cacute; si&eogonek;</trd>,
      <trd>zajmowa&cacute; si&eogonek;<klr> (przest.)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="cs">zaj&iacute;mat</trd>
  <trd lng="es">interesarse</trd>
  <trdgrp lng="fr">
    <trd>s'<ind>int&eacute;resser</ind></trd>,
    <trd>ressentir de l'<ind>int&eacute;r&ecirc;t</ind></trd>,
    <trd>prendre des <ind>int&eacute;r&ecirc;ts</ind></trd>,
    <trd>aimer<klr tip="ind">
    (avoir de l'int&eacute;r&ecirc;t pour)</klr></trd>
  </trdgrp>
  <trd lng="hu">&eacute;rdekl&odblac;dik</trd>
  <trd lng="pt">interessar-se</trd>
  <trd lng="ru">&c_i;&c_n;&c_t;&c_je;&c_r;&c_je;&c_s;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>p&uacute;ta&tcaron;</trd>,
    <trd>zauj&iacute;ma&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="intere.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc>
    <dif>
      Stato de iu <tld/>ata:
      <ekz>
        li montris plej grandan <tld/>i&gcirc;on pri miaj ideoj
        kaj tre varme min akceptis
        <fnt><bib>MkM</bib><lok>&ccirc;ap. 2</lok></fnt>;
      </ekz>
      <ekz>
        vigla diskutado post la raporto evidentigis profundan
        <tld/>i&gcirc;on
        de la delegitoj koncerne la Esperantajn aferojn.
      </ekz>
    </dif>
    <ref tip="sin" cel="intere.0o.sento"><tld/>o <sncref/></ref>
  </snc>
  <trd lng="de">Interesse</trd>
  <trdgrp lng="fr">
    <trd>int&eacute;r&ecirc;t<klr tip="ind">
    (qu'on trouve &agrave; qc ou qn)</klr></trd>,
    <trd>penchant<klr tip="ind"> (pour qc ou qn)</klr></trd>,
    <trd>inclination<klr tip="ind"> (pour qc ou qn)</klr></trd>,
    <trd>go&ucirc;t<klr tip="ind"> (pour qc ou qn)</klr></trd>,
    <trd>sympathie<klr tip="ind"> (pour qc ou qn)</klr></trd>,
    <trd>sollicitude</trd>,
    <trd>attention<klr tip="ind"> (pour qc ou qn)</klr></trd>.
  </trdgrp>
  <trd lng="hu">&eacute;rdekl&odblac;d&eacute;s</trd>
  <trd lng="nl">interesse</trd>
  <trdgrp lng="pl">
    <trd>zainteresowanie</trd>,
    <trd>ciekawo&sacute;&cacute;</trd>,
    <trd>zaciekawienie</trd>
  </trdgrp>
  <trd lng="pt">interesse</trd>
  <trd lng="ru">&c_z;&c_a;&c_i;&c_n;&c_t;&c_je;&c_r;&c_je;&c_s;&c_o;&c_v;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="intere.ne0one0igxo">
  <kap>ne<tld/>o, <var><kap>ne<tld/>i&gcirc;o</kap></var></kap>
  <snc>
    <dif>
      Manko de <tld/>o:
      <ekz>
        pro la ne<tld/>i&gcirc;o de la ceteraj estraranoj, Renato
        Corsetti mem prenis je si la respondecon plutrakti kun BKC
        <fnt>
        <aut>A. Grigorjevskij</aut>,
        <vrk><url ref="http://www.liberafolio.org/UEA/grigorjevskij">
        Mi estis elektita hazarde</url></vrk>,
        <lok>Libera Folio, 2004-08-19</lok>
        </fnt>;
      </ekz>
      <ekz>
        li a&ubreve;dis vo&ccirc;on, kiu malgra&ubreve; lia ne<tld/>o
        allogis lian atenton
        <fnt>
        <aut>M. C. d'Aulnoy, trad. D. J. Harlow</aut>,
        <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/         Noveloj/blua/flava_gnomo.html">La flava gnomo</url></vrk>,
        <lok>[2000?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Desinteresse</trd>
  <trd lng="es">desinter&eacute;s</trd> 
  <trdgrp lng="fr">
   <trd>d&eacute;sint&eacute;r&ecirc;t</trd>
   <trd>incurie</trd>
  </trdgrp>
  <trd lng="pl">brak <ind>zainteresowanie</ind></trd>
  <trd lng="pt">desinteresse</trd>
  <trd lng="ru">&c_n;&c_je;&c_z;&c_a;&c_i;&c_n;&c_t;&c_je;&c_r;&c_je;&c_s;&c_o;&c_v;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="intere.pli0igi">
  <kap>pli<tld/>igi</kap>
  <snc>
    <dif>
      Igi pli <tld/>a:
      <ekz>
        ni krome ludu almena&ubreve; unu stelon je poento, por
        pli<tld/>igi la ludon
        <fnt>
        <aut>L. Beaucaire</aut>
        <vrk><url ref="http://www.esperanto.mv.ru/Kruko/kruko1.html">
        Kruko kaj Baniko el Bervalo</url></vrk>
        <lok>1974</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>rendre plus <ind>int&eacute;ressant</ind></trd>,
    <trd>int&eacute;resser<klr tip="amb"> (le jeu)</klr></trd>
  </trdgrp>
  <trd lng="pl">uatrakcyjnia&cacute;</trd>
  <trd lng="ru">
  &c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; &c_b;&c_o;&c_l;&c_je;&c_je;
  <ind>&c_i;&c_n;&c_t;&c_je;&c_r;&c_je;&c_s;&c_n;&c_y;&c_m;</ind></trd>
</drv>

<drv mrk="intere.sen0ane0a">
  <kap>sen<tld/>a, <var><kap>ne<tld/>a</kap></var></kap>
  <snc>
    <dif>
      Senigita je <tld/>o:
      <ekz>
        Ken havas malgravan sen<tld/>an oficon en la urba administrejo
        <fnt><bib>Lasu</bib><lok>La vesperfu&scirc;ulo</lok></fnt>;
      </ekz>
      <ekz>
        lia opinio estas tute sen<tld/>a por mi.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="indife.0a">indiferenta</ref>,
      <ref cel="grav.mal0a">malgrava</ref>,
      <ref cel="bagate.0a">bagatela</ref>,
      <ref cel="banal.0a">banala</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="gurd">gurdita</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>bez z&aacute;jmu</trd>,
    <trd>f&aacute;dn&iacute;</trd>
  </trdgrp>
  <trd lng="de">uninteressant</trd>
  <trdgrp lng="es">
    <trd>vulgar<klr> (en el sentido de com&uacute;n)</klr></trd>,
    <trd>insulso</trd>,
    <trd>indiferente</trd> 
  </trdgrp>
  <trdgrp lng="fr">
    <trd>inint&eacute;ressant</trd>,
    <trd>sans <ind>int&eacute;r&ecirc;t</ind></trd>,
    <trd>d&eacute;nu&eacute; d'<ind>int&eacute;r&ecirc;t</ind></trd>,
    <trd>banal</trd>,
    <trd>commun<klr tip="ind"> (inint&eacute;ressant)</klr></trd>,
    <trd>insignifiant<klr tip="ind"> (inint&eacute;ressant)</klr></trd>,
    <trd>quelconque<klr tip="ind"> (inint&eacute;ressant)</klr></trd>
  </trdgrp>
  <trd lng="hu">&eacute;rdektelen</trd>
  <trd lng="nl">onbelangrijk</trd>
  <trdgrp lng="pl">
    <trd>nieinteresuj&aogonek;cy</trd>,
    <trd>nieciekawy</trd>
  </trdgrp>
  <trd lng="pt">sem <ind>interesse</ind><klr tip="ind"> (adj.)</klr>
  </trd>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_y;&c_n;&c_t;&c_je;&c_r;&c_je;&c_s;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">nezainteresovan&yacute;</trd>
</drv>

<drv mrk="intere.sen0igxi">
  <kap>sen<tld/>i&gcirc;i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Perdi <tld/>on, ne havi <tld/>on:
      <ekz>
        la rusa Registaro sen<tld/>i&gcirc;as pri nia propagando,
        e&ccirc; likvidas la poresperantajn organizojn
        <fnt><bib>Lanti</bib><lok>Je la lumo de la sperto</lok></fnt>;
      </ekz>
      <ekz>
        oni do ne miru ke la plej multaj civitanoj de la
        membro&scirc;tatoj
        sen<tld/>i&gcirc;as <klr>[pri la konstitucia traktato]</klr>
        <fnt>
        <aut>A. C. Robert, trad. V. Lutermano</aut>,
        <vrk><url ref="http://eo.mondediplo.com/article884.html">
        Speciala kajero pri E&ubreve;ropo</url></vrk>,
        <lok>Le Monde Diplomatique en Esperanto, 2004-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">desinteressiert sein</trd>
  <trd lng="es">desinteresarse</trd> 
  <trd lng="fr">se <ind>d&eacute;sint&eacute;resser</ind><klr tip="ind">
  (montrer du d&eacute;sint&eacute;r&ecirc;t)</klr></trd>
  <trd lng="pt">desinteressar-se</trd>
  <trd lng="ru">&c_p;&c_o;&c_t;&c_je;&c_r;&c_ja;&c_t;&c_mol; <ind>&c_i;&c_n;&c_t;&c_je;&c_r;&c_je;&c_s;</ind></trd>
</drv>

<drv mrk="intere.sen0igxo">
  <kap>sen<tld/>i&gcirc;o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Stato de iu sen<tld/>inta.
    </dif>
    <ref tip="sin" cel="indife.0eco">indiferenteco</ref>
    <refgrp tip="vid">
      <ref cel="fervor">senfervoreco</ref>,
      <ref cel="flegm.0o">flegmo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Desinteresse</trd>  
  <trdgrp lng="fr">
    <trd>d&eacute;sint&eacute;r&ecirc;t grandissant</trd>,
    <trd>tendance &agrave; l'indiff&eacute;rence</trd>
    <trd>tendance &agrave; la banalisation</trd>
  </trdgrp>
  <trd lng="nl">onverschilligheid</trd>
  <trd lng="pt">desinteresse<klr tip="amb"> (subst.)</klr></trd>
  <trd lng="ru">&c_n;&c_je;&c_z;&c_a;&c_i;&c_n;&c_t;&c_je;&c_r;&c_je;&c_s;&c_o;&c_v;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>
