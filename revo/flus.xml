<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>flus</rad>/o
</kap>
<drv mrk="flus.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Tempo de la tajdo, dum kiu la maro alti&gcirc;as:
      <ekz>
        la maksimuma alti&gcirc;o de la maro dum la <tld/>o nomi&gcirc;as altmaro.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Flut</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_l;&c_i;&c_v;</trd>
</drv>
<drv mrk="flus.mal0o">
  <kap>mal<tld/>o</kap>
  <snc>
    <dif>
      Tempo de la tajdo, dum kiu la maro malalti&gcirc;as:
      <ekz>
        la maksimuma malalti&gcirc;o dum la mal<tld/>o nomi&gcirc;as malaltmaro.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Ebbe</trd>
  <trd lng="ru">&c_o;&c_t;&c_l;&c_i;&c_v;</trd>
</drv>
<drv mrk="flus.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on. 
    </dif>
    <refgrp tip="sin">
      <ref cel="tajd.0o">tajdo</ref>,
      <ref cel="flu.al0o">alfluo</ref>,
      <ref cel="flu.al0o">forfluo</ref>.
    </refgrp>
    <trd lng="ru">&c_p;&c_r;&c_i;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_o; &c_m;&c_o;&c_r;&c_je;)</klr></trd>
</snc>
  <trd lng="de">fluten</trd>
</drv>
</art>
