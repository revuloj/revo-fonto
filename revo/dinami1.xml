<vortaro>
<art mrk="$Id: dinami1.xml,v 1.19 2023/08/31 10:44:00 revo Exp $">
<kap>
  <rad>dinamism</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="dinami1.0o">
  <kap><tld/>o</kap>
  <snc mrk="dinami1.0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Doktrino, kiu samigas materion kun forto.  
    </dif>
    <refgrp tip="vid">
      <ref cel="mehxan1.0o.FIL">me&hcirc;anismo</ref>,
      <ref cel="vital.0ismo">vitalismo</ref>.
    </refgrp>
  </snc>
  <snc mrk="dinami1.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Vigleco, agemo:
      <ekz>
        via <tld/>o <klr>...</klr> alportis al la rumana movado, e&ccirc; dum 
        tia griza periodo, tiom da sukcesoj
        <fnt>
          <aut>M. Butan</aut>,
          <vrk>en: <url ref="&LOdE;Historio/247bocrt.htm">
          Profesoro Bociort respondas</url></vrk>,
          <lok>La Ondo de Esperanto, 2015:4-5 (246-247)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_n;&c_a;&c_m;&c_ib;&c_z;&c_m;</trd>
  <trd lng="ca">dinamisme</trd>
  <trd lng="cs">dynamizmus</trd>
  <trd lng="de">Dynamismus</trd>
  <trd lng="en">dynamism <klr tip="amb">(philosophy)</klr></trd>
  <trd lng="es">dinamismo</trd>
  <trd lng="fr">dynamisme</trd>
  <trd lng="he">&#x5D3;&#x5D9;&#x5DE;&#x5E0;&#x5D9;&#x5D6;&#x5DD;</trd>
  <trd lng="hu">dinamizmus</trd>
  <trdgrp lng="ja">
    <trd>&#x30C0;&#x30A4;&#x30CA;&#x30DF;&#x30BA;&#x30E0;</trd>,
    <trd>&#x529B;&#x52D5;&#x8AAC;  [<ind>&#x3061;&#x304B;&#x3089;&#x3069;&#x3046;&#x305B;&#x3064;</ind>]</trd>,
    <trd>&#x529B;&#x672C;&#x8AAC;  [<ind>&#x308A;&#x304D;&#x307B;&#x3093;&#x305B;&#x3064;</ind>]</trd>
  </trdgrp>
  <trd lng="pl">dynamizm</trd>
  <trd lng="pt">dinamismo</trd>
  <trd lng="ru">&c_d;&c_i;&c_n;&c_a;&c_m;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">dynamizmus</trd>
  <trd lng="uk">&#x434;&#x438;&#x43D;&#x430;&#x43C;&#x456;&#x437;&#x43C;</trd>
</drv>
