<vortaro>
  <art mrk="$Id: ganesh.xml,v 1.5 2025/06/30 20:33:21 afido Exp $">
    <kap>
      <rad>Gane&scirc;</rad>/o
    </kap>

<drv mrk="ganesh.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIN</uzo>
  <snc>
    <dif>
      Hindua <ref tip="super" cel="di.0ajxo">dia&jcirc;o</ref>, kutime bildigita kun elefanta kapo:
      <ekz>
        kulto de <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk>Sutroj kaj sa&ubreve;dadoj</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la nepalanoj ne &scirc;atas katojn, male la ratoj, sanktaj bestoj de dio <tld/>o,
        fojfoje esploreme montras siajn muzeletojn
        <fnt>
          <bib>Monato</bib>
          <aut>Vera Barandovsk&aacute;-Frank</aut>
          <vrk><url ref="&Monato;/2004/008388.html">Nepalo tra e&ubreve;ropaj okuloj</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_G;&c_a;&c_n;&c_e;&c_sh;&c_a;</trd>
    <trd lng="de">Ganesha</trd>
    <trd lng="en">Ganesha</trd>
    <trd lng="fr">Ganesh</trd>
    <trdgrp lng="pl">
      <trd>Gane&sacute;a</trd>,
      <trd>Ganapati</trd>
    </trdgrp>
    <trd lng="sa">&#2327;&#2339;&#2375;&#2358;</trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x30AC;&#x30CD;&#x30FC;&#x30B7;&#x30E3;</trd>,
    <trd>&#x6B53;&#x559C;&#22825;  <pr>&#x304B;&#12435;&#x304E;&#12390;&#12435;</pr></trd>
  </trdgrp>
</drv>
  </art>

