<art mrk="$Id: fajl1.xml,v 1.5 2023/10/14 18:06:40 revo Exp $">
<kap>
  <rad>fajl</rad>/o
</kap>
<drv mrk="fajl1.0o">
  <kap><tld/>o</kap>
  <snc mrk="fajl1.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="dif" cel="dosier.0o.KOMP">Dosiero<sncref/></ref>:
      <ekz>
        <tld/>oj por IBM-kongruaj komputoroj
        <fnt>
          <aut>G. Valente</aut>,
          <vrk><url ref="&STEB;vortaroj/fotografio/fotografia-terminaro-difinoj.pdf">Fotografia
          Fakvortaro</url></vrk>,
          <lok>1997, Triesta Esperanto-Asocio, pa&gcirc;o 6</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam la <tld/>o estas kreita kaj nomita, klaku la piednot-areon
        kaj entajpu la bezonatajn pa&gcirc;numerojn
        <fnt>
          <aut>Stefan MacGill</aut>,
          <vrk><url ref="https://www.ilei.info/dok/Gvidilo_kontribuantoj_al_JA.pdf">Gvidilo
          por rubrikestroj kaj kontribuantoj al Juna amiko</url></vrk>,
          <lok>2013, ILEI</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    Cetere &leftquot;<ref tip="hom" cel="fajl.0i">fajli</ref>&rightquot; estas maniero skrapi, 
    por kio oni uzas <ref cel="fajl.0ilo">fajlilo</ref>n.
  </rim>
  <trdgrp lng="ja">
    <trd>&#x3084;&#x3059;&#x308A;&#x639B;&#x3051;  <pr>&#x30E4;&#x30B9;&#x30EA;&#x304C;&#x3051;</pr></trd>,
    <trd>&#x4E00;&#x524A;&#x308A;  <pr>&#x3072;&#x3068;&#x3051;&#x305A;&#x308A;</pr></trd>
  </trdgrp>
</drv>
</art>

