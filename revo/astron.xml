<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>astronom</rad>/o
</kap>
<drv mrk="astron.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="dif" cel="astron1.0isto">astronomiisto</ref>
      <ekz>
        lastatempe a&ubreve;di&gcirc;as pledoj de <tld/>oj, ke okazu ampleksa
        kalendar-reformo<fnt><aut>Evgeni Georgiev</aut>: <vrk>&Ccirc;u
        kalendar-reformo estos bezonata?</vrk>,
        <bib>Monato</bib><lok>jaro
        1998a, numero 2a, p. 11a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_t;&c_r;&c_a;&c_n;&c_o;&c_m;</trd>
  <trdgrp lng="de">
    <trd>Astronom</trd>,
    <trd>Sternenkundler</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>astronomer</trd>
  </trdgrp>
  <trd lng="es">astr&oacute;nomo</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_kh;&a_t;&a_r;&a_sh;&a_n;&a_A;&a_s;</trd>,
    <trd>&a_m;&a_n;&a_j;&a_m;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>astronome</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csillag&aacute;sz</trd>,
    <trd>asztron&oacute;mus</trd>
  </trdgrp>
  <trd lng="it">astronomo</trd>
  <trd lng="nl">astronoom</trd>
  <trd lng="ru">&c_a;&c_s;&c_t;&c_r;&c_o;&c_n;&c_o;&c_m;</trd>
  <trd lng="sv">astronom</trd>
</drv>
</art>
