<vortaro>
<art mrk="$Id: amazon2.xml,v 1.20 2023/10/28 08:33:05 revo Exp $">
<kap>
  <rad>Amazon</rad>/o
</kap>

<drv mrk="amazon2.A0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <fnt><bib>KrED</bib></fnt>
  <snc mrk="amazon2.A0o.rivero">
    <dif>
      Grandega <ref tip="malprt" cel="amerik.Sud0o">sudamerika</ref>
      <ref tip="lst" cel="river.0o" lst="voko:riveroj">rivero</ref>, plej
      akvori&ccirc;a rivero de la mondo:
      <ekz>
        Amapao estas &scirc;tato de la Federacio de Brazilo, situanta inter la elfluejo de
        rivero <tld/>o kaj la Franca Gviano
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=270">Amapao</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_A;&c_m;&c_a;&c_z;&c_o;&c_n;&c_k;&c_a;</trd>
    <trd lng="es">El Amazonas</trd>
    <trd lng="fa">&a_r;&a_w;&a_d; <ind>&a_A_madda;&a_m;&a_A;&a_z;&a_w;&a_n;</ind></trd>
    <trd lng="fr">Amazone</trd>
    <trd lng="nl">Amazone</trd>
    <trd lng="pl">Amazonka</trd>
    <trd lng="ru">&c_A;&c_m;&c_a;&c_z;&c_o;&c_n;&c_k;&c_a;</trd>
    <trdgrp lng="zh">
    <trd>&#x4E9A;&#x9A6C;&#x5B59;&#x6CB3; <pr>y&#xE0;m&#x1CE;s&#x16B;nh&#xE9;</pr></trd>,
    <trd>&#x4E9E;&#x99AC;&#x5B6B;&#x6CB3; <pr>y&#xE0;m&#x1CE;s&#x16B;nh&#xE9;</pr></trd>,
    <trd>&#x4E9A;&#x9A6C;&#x900A;&#x6CB3; <pr>y&#xE0;m&#x1CE;x&#xF9;nh&#xE9;</pr></trd>,
    <trd>&#x4E9E;&#x99AC;&#x905C;&#x6CB3; <pr>y&#xE0;m&#x1CE;x&#xF9;nh&#xE9;</pr></trd>,
    <trd>&#x4E9A;&#x9A6C;&#x5B59; <pr>y&#xE0;m&#x1CE;s&#x16B;n</pr></trd>,
    <trd>&#x4E9E;&#x99AC;&#x5B6B; <pr>y&#xE0;m&#x1CE;s&#x16B;n</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="amazon2.A0o.subsxtato">
    <dif>
      Nordokcidenta sub&scirc;tato de
      <ref tip="malprt" cel="brazil.B0o">Brazilo</ref>, en la baseno de
      <tld/>o<sncref ref="amazon2.0o.rivero"/>.
    </dif>
    <trd lng="be">&c_A;&c_m;&c_a;&c_z;&c_o;&c_n;&c_a;&c_s;</trd>
    <trd lng="ru">&c_A;&c_m;&c_a;&c_z;&c_o;&c_n;&c_a;&c_s;</trd>
    <trdgrp lng="zh">
    <trd>&#x4E9A;&#x9A6C;&#x900A;&#x76C6;&#x5730; <pr>y&#xE0;m&#x1CE;x&#xF9;np&#xE9;nd&#xEC;</pr></trd>,
    <trd>&#x4E9E;&#x99AC;&#x905C;&#x76C6;&#x5730; <pr>y&#xE0;m&#x1CE;x&#xF9;np&#xE9;nd&#xEC;</pr></trd>
  </trdgrp>
</snc>
  <rim>
    <ref tip="hom" cel="amazon.0o">Amazonoj</ref>
    anka&ubreve; estas antikvaj militistinoj.
  </rim>
  <trd lng="cs">Amazonka</trd>
  <trd lng="de">Amazonas</trd>
  <trd lng="fa">&a_A_madda;&a_m;&a_A;&a_z;&a_w;&a_n;&a_A;&a_s;</trd>
  <trd lng="hu">Amazonas</trd>
  <trd lng="ja">&#x30A2;&#x30DE;&#x30BE;&#x30F3;&#x5DDD; 
  <pr>&#x3042;&#x307E;&#x305E;&#x3093;&#x304C;&#x308F;</pr></trd>
  <trd lng="pl">Amazonia</trd>
  <trd lng="pt">Amazonas</trd>
  <trd lng="sk">Amazonka</trd>
</drv>
