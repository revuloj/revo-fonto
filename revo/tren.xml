<kap>
  <ofc>*</ofc>
  <rad>tren</rad>/i
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="tren.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Pene <ref tip="super" cel="tir.0i.kun_si">tiri</ref> post si:
      <ekz>
        <tld/>i sakojn da greno en la muelejon
        <fnt>
          <bib>ElektFab</bib>
          <lok>La vivolongo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[evitu]</klr> ke li <tld/>u vin al la ju&gcirc;isto
        <fnt>
          <bib>NT</bib>
          <lok>Luko 12:58</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni <tld/>is &scirc;in en malluman, malsekan malliberejon
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        du viroj <klr>[&#x2026;]</klr> iris sur la trotuaro, <tld/>ante inter si trian viron,
        kiu &scirc;ajnis esti malsana
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[meteoro]</klr> falis malsupren kaj trenis post si tra la &ccirc;ielo longan
        strion da fajro
        <fnt>
          <bib>Fab2</bib>
          <lok>Knabineto kun alumetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[tiuj]</klr> esta&jcirc;oj <klr>[&#x2026;]</klr> <tld/>is sian malluman, pezan kaj
        senesperan ekzistadon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        miloj <tld/>e portis plu sian pezan kaj apatian vivon
        <fnt>
          <bib>LGA</bib>
          <lok>I.</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni <tld/>as veturilon kiel bovoj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la urbo sin <tld/>as oficiroj kaj popolamaso
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne volas kokin' al festeno, sed oni &gcirc;in <tld/>as
        perforte
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i sian vivon de mizero al mizero
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="hauxl.0i">ha&ubreve;li</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_ja;&c_g;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_a;&c_l;&c_a;&c_ch;&c_y;</trd>
  </trdgrp>
  <trd lng="bo">&t_tha;&t_drengbu;&t_na;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>t&aacute;hnout <klr>(za sebou)</klr></trd>,
    <trd>vl&aacute;&ccaron;et</trd>,
    <trd>vl&eacute;ci</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schleppen</trd>,
    <trd>schleifen <klr>(ziehen)</klr></trd>,
    <trd>ziehen</trd>
  </trdgrp>
  <trd lng="en">drag</trd>
  <trdgrp lng="es">
    <trd>jalar</trd>,
    <trd>halar</trd>,
    <trd>arrastrar</trd>,
    <trd>remolcar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>remorquer</trd>,
    <trd>trainer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>von</trd>,
    <trd>vonszol</trd>,
    <trd>vontat</trd>,
    <trd>cipel</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5F15;&#x304D;&#x305A;&#x308B;  <pr>&#x3072;&#x304D;&#x305A;&#x308B;</pr></trd>,
    <trd>&#x5F15;&#x304D;&#x305A;&#x3063;&#x3066;&#x884C;&#x304F;  <pr>&#x3072;&#x304D;&#x305A;&#x3063;&#x3066;&#x3044;&#x304F;</pr></trd>,
    <trd>&#x727D;&#x5F15;&#x3059;&#x308B;  <pr>&#x3051;&#x3093;&#x3044;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5F15;&#x3063;&#x3071;&#x308B;  <pr>&#x3072;&#x3063;&#x3071;&#x308B;</pr></trd>,
    <trd>&#x8010;&#x3048;&#x3066;&#x904E;&#x3054;&#x3059;  <pr>&#x305F;&#x3048;&#x3066;&#x3059;&#x3054;&#x3059;</pr></trd>,
    <trd>&#x30C9;&#x30E9;&#x30C3;&#x30B0;&#x3059;&#x308B;  <pr>&#x3069;&#x3089;&#x3063;&#x3050;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>slepen</trd>,
    <trd>voorttrekken</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przeci&aogonek;ga&cacute;</trd>,
    <trd>wlec</trd>,
    <trd>ci&aogonek;gn&aogonek;&cacute;</trd>,
    <trd>w&lstroke;&oacute;czy&cacute;</trd>
  </trdgrp>
  <trd lng="pt">arrastar</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_a;&c_shch;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_o;&c_l;&c_o;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vliec&tcaron;</trd>,
    <trd>&tcaron;aha&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_o;&c_l;&c_o;&c_k;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_o;&c_l;&c_o;&c_ch;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_t;&c_ja;&c_g;&c_t;&c_i; (&c_z;&c_a; &c_s;&c_o;&c_b;&c_o;&c_ju;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6261; <pr>ch&#x1D0;</pr></trd>,
    <trd>&#x62D6;&#x884C; <pr>tu&omacron;x&iacute;ng</pr></trd>,
    <trd>&#x638E; <pr>j&#x1D0;</pr></trd>,
    <trd>&#x62D6; <pr>tu&omacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="tren.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Io <tld/>ita a&ubreve; <tld/>i&gcirc;anta:
      <ekz>
        nebulaj <tld/>a&jcirc;oj;
      </ekz>
      <ekz>
        ni do a&ccirc;etis du tiajn longajn, senmotorajn &scirc;ip<tld/>a&jcirc;ojn
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia praonklo malrapide alproksimi&gcirc;is al la domo, tirante post si la <tld/>a&jcirc;on
        de gastoj
        <fnt>
          <bib>MJA</bib>
          <lok>Parto Dua</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">vontatm&aacute;ny</trd>
    <trd lng="pl">&sacute;lad po przej&sacute;ciu przez atmosfer&eogonek;</trd>
  </snc>
  <snc>
    <dif>
      Longa pendanta parto de vesto etendi&gcirc;anta sur grundo
      a&ubreve; planko:
      <ekz>
        <klr>[la]</klr> re&gcirc;ino <klr>[havis]</klr> longajn <tld/>a&jcirc;ojn &ccirc;e la vestoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;ambelanoj, kiuj devis porti la <tld/>a&jcirc;on de la vesto, eltiris siajn manojn
        al la planko, kvaza&ubreve; ili levas la <tld/>a&jcirc;on
        <fnt>
          <bib>Fab1</bib>
          <lok>La novaj vestoj de la re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la longa <tld/>a&jcirc;o de &scirc;ia vesto susuris sur la pargeto
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bask.0o">basko</ref>,
      <ref cel="vost.0o.fino">vosto<sncref/></ref>
    </refgrp>
    <trd lng="de">Schleppe</trd>
    <trd lng="es">cola</trd>
    <trd lng="hu">ruhausz&aacute;ly</trd>
    <trd lng="nl">sleep</trd>
    <trd lng="pl">tren</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_l;&c_je;&c_j;&c_f;</trd>,
    <trd>&c_h;&c_v;&c_o;&c_s;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3082;&#x3059;&#x305D;</trd>,
    <trd>&#x5F15;&#x304D;&#x3059;&#x305D;  <pr>&#x3072;&#x304D;&#x3059;&#x305D;</pr></trd>,
    <trd>&#x5C3E;  <pr>&#x3073;</pr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_l;&c_je;&c_j;&c_f;</trd>,
    <trd>&c_h;&c_v;&c_o;&c_s;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_sh;&c_l;&c_je;&c_j;&c_f;</trd>,
    <trd>&c_h;&c_v;&c_ib;&c_s;&c_t; (&c_k;&c_o;&c_m;&c_je;&c_t;&c_i; &c_ib; &c_t;.&c_p;.)</trd>
  </trdgrp>
  <trd lng="zh">&#x62D6;&#x88D9; <pr>tu&omacron;q&uacute;n</pr></trd>
</drv>

<drv mrk="tren.0igxi">
  <kap>
    <tld/>i&gcirc;i
  </kap>
  <snc mrk="tren.0igxi.pasive">
    <dif>
      Esti <tld/>ata:
      <ekz>
        ru&gcirc;a &scirc;alo <klr>[&#x2026;]</klr> longe <tld/>i&gcirc;as post &scirc;i
        <fnt>
          <bib>Fab2</bib>
          <lok>El fenestro en Vartou</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi klare sentis, kiel mi <tld/>i&gcirc;as de tago al tago al la rando de la abismo
        <fnt>
          <bib>VaK</bib>
          <lok>Sesa &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="tren.0igxi.refleksive">
    <dif>
      Pene anta&ubreve;en iri; <ref tip="sin" cel="tren.sin0i">sin treni</ref>:
      <ekz>
        <nom>Kandid</nom>, tute frostite, <tld/>i&gcirc;is al proksima urbo
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tempo <tld/>i&gcirc;as, kiel veturiloj en la dezerto
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">sich <ind>schleppen</ind></trd>
  </snc>
</drv>

<drv mrk="tren.kun0i">
  <kap>kun<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="T"/>i kun si:
      <ekz>
        ezoko <klr>[&#x2026;]</klr> baraktas, subakvi&gcirc;as <klr>[&#x2026;]</klr> sed &ccirc;ie la fera&jcirc;on kaj
        &scirc;nuron kun<tld/>as
        <fnt>
          <bib>SrT</bib>
          <lok>Libro V.</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi kun<tld/>is kun vi en tiun mondon ian pekon, kiu baras al vi la eniron en
        la paradizon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>?
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        La &scirc;tona urbo kun<tld/>as la leganton
        <fnt>
          <bib>Monato</bib>
          <aut>Christian Declerck</aut>
          <vrk><url ref="&Monato;/2009/009758.php"
              >Panoramo de la Nerona Romo</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_c;&c_ja;&c_g;&c_n;&c_u;&c_c;&c_mol; <klr>(&c_z;
    &c_s;&c_a;&c_b;&c_o;&c_j;, &c_z;&c_a;
    &c_s;&c_a;&c_b;&c_o;&c_j;)</klr></trd>
      <trdgrp lng="hu">
        <trd>mag&aacute;val <ind>hurcol</ind></trd>,
        <trd>mag&aacute;val <ind>h&uacute;z</ind></trd>
      </trdgrp>
    <trd lng="nl">meeslepen</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Nepre <ref tip="super" cel="sekv.0igi">sekvigi</ref>:
      <ekz>
        amo kun<tld/>as respondecon
        <fnt>
          <bib>Kon11</bib>
          <lok>Intervjuo</lok>
        </fnt>;
      </ekz>
      <ekz>
        uzo de specifa lingvo neeviteble kun<tld/>as la tutan vidpunktaron de la
        indi&gcirc;enaj parolantoj de tiu lingvo
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2006/008843.php"
              >Valora pripensinda&jcirc;aro</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        malfeli&ccirc;o venas, malfeli&ccirc;on kun<tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>.
        <trd lng="de">ein Ungl&uuml;ck kommt selten allein</trd>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_c;&c_ja;&c_g;&c_n;&c_u;&c_c;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="hu">mag&aacute;val <ind>hoz</ind></trd>
    <trd lng="nl">ten <ind>gevolgen</ind> hebben</trd>
  </snc>
  <trdgrp lng="de">
    <trd>mitschleppen</trd>,
    <trd>mitschleifen</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>conllevar</trd>,
    <trd>acarrear</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>entrainer<klr> (tirer avec soi)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5F15;&#x304D;&#x9023;&#x308C;&#x308B;  <pr>&#x3072;&#x304D;&#x3064;&#x308C;&#x308B;</pr></trd>,
    <trd>&#x3082;&#x305F;&#x3089;&#x3059;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&sacute;ci&aogonek;ga&cacute;</trd>,
    <trd>zwlec</trd>,
    <trd>zaci&aogonek;ga&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_shch;&c_i;&c_t;&c_mol; &c_s; &c_s;&c_o;&c_b;&c_o;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>prin&aacute;&scaron;a&tcaron; so sebou</trd>,
    <trd>vl&aacute;&ccaron;i&tcaron; zo sebou</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_ja;&c_g;&c_n;&c_u;&c_t;&c_i;/&c_v;&c_o;&c_l;&c_o;&c_k;&c_t;&c_i; &c_z; &c_s;&c_o;&c_b;&c_o;&c_ju;</trd>,
    <trd>&c_s;&c_p;&c_r;&c_i;&c_ch;&c_i;&c_n;&c_ja;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6261; <pr>ch&#x1D0;</pr></trd>,
    <trd>&#x62FD; <pr>zhu&agrave;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="tren.sin0i">
  <kap>sin <tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
     Pene, malfacile a&ubreve; malrapide anta&ubreve;eniri:
     <ekz>
        li sin <tld/>is tra la ne&gcirc;o
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        malsata kaj tremanta de frosto la kompatinda knabineto
        <tld/>adis sin pluen
        <fnt><bib>Fab2</bib>, <lok>Knabineto kun alumetoj</lok></fnt>;
      </ekz>
      <ekz>
        maljunuloj, <tld/>antaj sin kun helpo de bastonoj
        <fnt>
         <aut>H. Sienkewicz, tr. Lidia Zamenhof</aut>:
         <vrk>Quo vadis</vrk>, <lok>&Ccirc;ap. 20&#xAA;</lok>
        </fnt>;
     </ekz>
     <ekz>
        bela estis la festeno: la viroj kaj virinoj sin <tld/>is sur la planko,
        surver&scirc;ita per vino
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
     <ekz>
        bona famo sin <tld/>as testude, malbona kuras rapide
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="ramp.0i">rampi</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_ja;&c_g;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_a;&c_l;&c_a;&c_ch;&c_y;&c_s;&c_ja;</trd>,
    <trd>&c_t;&c_r;&c_e;&c_s;&c_mol;&c_c;&c_ib;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sich <ind>schleppen</ind></trd>,
    <trd>sich <ind>abm&uuml;hen</ind></trd>
  </trdgrp>
  <trd lng="fr">se tra&icirc;ner</trd>
  <trdgrp lng="pl">
    <trd>d&lstroke;u&zdot;y&cacute; si&eogonek;</trd>,
    <trd>telepa&cacute; si&eogonek;</trd>,
    <trd>wlec si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_a;&c_shch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_l;&c_a;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_o;&c_l;&c_o;&c_ch;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_b;&c_r;&c_je;&c_s;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
</art>
