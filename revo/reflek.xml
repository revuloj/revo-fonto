<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: reflek.xml,v 1.20 2018/08/06 20:10:42 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>reflekt</rad>/i
  <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="reflek.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Re&jcirc;eti, rebrili, resendi, reradii sonojn, lumon, varmon ks:
      <ekz>
        polurita, netrairebla surfaco, kiel ekz. de spegulo, <tld/>as la
        radiojn de la lumo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="refrak.0i">refrakti</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_l;&c_ju;&c_s;&c_t;&c_r;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_b;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">reflectir</trd>
  <trdgrp lng="cs">
    <trd>odrazit</trd>,
    <trd>odr&aacute;&zcaron;et</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>reflektieren</trd>,
    <trd>zur&uuml;ckwerfen</trd>
  </trdgrp>
  <trd lng="en">reflect</trd>
  <trd lng="es">reflejar</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_A;&a_z;&a_t;&a_A;&a_b;&a_A;&a_n;&a_d;&a_n;</trd>,
    <trd>&a_m;&a_n;&a_ayn;&f_k;&a_s; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>r&eacute;fl&eacute;chir<klr tip="ind"> (lumi&egrave;re etc.)</klr></trd>,
    <trd>renvoyer<klr tip="ind"> (lumi&egrave;re etc.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>visszaver</trd>,
    <trd>visszat&uuml;kr&ouml;z</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>weerkaatsen</trd>,
    <trd>reflecteren</trd>
  </trdgrp>
  <trd lng="pl">odbija&cacute; <klr>(promienie, fale)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_r;&c_a;&c_z;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odrazi&tcaron;</trd>,
    <trd>odr&aacute;&zcaron;a&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="reflek.0o">
  <kap><tld/>o</kap>
  <snc mrk="reflek.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      La fenomeno, per kiu sono, lumo, varmo ks estas <tld/>ata.
    </dif>
    <trd lng="ca">reflex</trd>
    <trd lng="de">Reflexion</trd>
    <trd lng="en">reflection</trd>
    <trdgrp lng="fa">
      <trd>&a_b;&a_A;&a_z;&a_t;&a_A;&a_b;</trd>,
      <trd>&a_A;&a_n;&a_ayn;&f_k;&a_A;&a_s;</trd>
    </trdgrp>
    <trd lng="fr">r&eacute;flexion<klr tip="ind"> (phys.)</klr></trd>
  <trdgrp lng="hu">
    <trd>visszaver&odblac;d&eacute;s</trd>,
    <trd>visszat&uuml;kr&ouml;z&eacute;s</trd>,
    <trd>reflexi&oacute;</trd>
  </trdgrp>
  <trd lng="es">reflexi&oacute;n</trd>
  <trd lng="pl">odbicie</trd>
  </snc>
  <snc mrk="reflek.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PIV2</bib></fnt>
    <ref tip="dif" cel="simetr.0o.orta">simetrio.</ref>
    <trd lng="hu">visszat&uuml;kr&ouml;z&eacute;s</trd>    
    <trd lng="pl">symetria</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>odraz <klr>(sv&ecaron;tla)</klr></trd>,
    <trd>reflex</trd>,
    <trd>zrcadlen&iacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_t;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>odraz <klr>(svetla)</klr></trd>,
    <trd>reflex</trd>,
    <trd>zrkadlenie</trd>
  </trdgrp>
</drv>
</art>
