<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>reflekt</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="reflek.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Re&jcirc;eti, rebrili, resendi, reradii sonojn, lumon, varmon:
      <ekz>
        polurita, netrairebla surfaco, kiel ekz. de spegulo, <tld/>as la
        radiojn de la lumo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="refrak.0i">refrakti</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>reflektieren</trd>,
    <trd>zur&uuml;ckwerfen</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>weerkaatsen</trd>,
    <trd>reflecteren</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_r;&c_a;&c_z;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
