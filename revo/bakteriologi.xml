<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bakteriologi.xml,v 1.15 2024/02/24 18:27:26 revo Exp $">
<kap>
  <rad>bakteriologi</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="bakteriologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="bakteriologi.0o.SCI">
    <uzo tip="fak">BAK</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Scienco pri <ref tip="vid" cel="bakter.0o.BAK">bakterioj</ref>, parto de <ref tip="malprt"
      cel="mikrob1.0o.SCI">mikrobiologio</ref>:
      <ekz>
        li fari&gcirc;is fakulo pri <tld lit="B"/>o en Instituto <nom>Pasteur</nom>
        en Parizo
        <fnt>
          <aut>P. S. Viana</aut>,
          <vrk><url
          ref="http://www.kke.org.br/esperanto/prozajxoj/oswaldo_cruz_vivo_kontraux_antauxjugxoj.php">Oswaldo
          Cruz - Vivo kontra&ubreve;
          Anta&ubreve;ju&gcirc;oj</url></vrk>,
          <lok>[vidita je 2006-06-19]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_a;&c_k;&c_t;&c_e;&c_r;&c_y;&c_ja;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
    <trd lng="bg">&c_b;&c_a;&c_k;&c_t;&c_je;&c_r;&c_i;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
    <trd lng="ca">bacteriologia</trd>
    <trd lng="de">Bakteriologie</trd>
    <trd lng="en">bacteriology</trd>
    <trd lng="fa">&a_b;&a_A;&f_k;&a_t;&a_r;&f_y; &a_sh;&a_n;&a_A;&a_s;&f_y;</trd>
    <trd lng="fr">bact&eacute;riologie</trd>
    <trd lng="he">&bet;&qof;&tet;&resh;&yod;&vav;&lamed;&vav;&gimel;&yod;&he;</trd>
    <trd lng="hu">bakterol&oacute;gia</trd>
    <trd lng="id">bakteriologi</trd>
    <trd lng="nl">bacteriologie</trd>
    <trd lng="pl">bakteriologia</trd>
    <trd lng="pt">bacteriologia</trd>
    <trd lng="ru">&c_b;&c_a;&c_k;&c_t;&c_je;&c_r;&c_i;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  </snc>
  <trd lng="ja">&#x7D30;&#x83CC;&#x5B66;  <pr>&#12373;&#12356;&#x304D;&#12435;&#x304C;&#x304F;</pr></trd>
  <trd lng="uk">&c_b;&c_a;&#x43A;&c_t;&c_je;&c_r;&c_ib;&#x43E;&#x43B;&#x43E;&c_g;&c_ib;&#x44F;</trd>
</drv>

<drv mrk="bakteriologi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="bakteriologi.0isto.SCI">
    <dif>
      Fakulo pri <tld/>o:
      <ekz>
        kuracistoj, <tld/>istoj, mikrobiologiistoj
        <fnt>
        <aut>Liga Colombiana de Esperanto</aut>,
        <vrk><url ref="http://us.geocities.com/esperantokolombio/patarroyo.html">Manuel
        Elkin Patarroyo</url></vrk>,
        <lok>[vidita je 2006-06-21]</lok>
        </fnt>
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_a;&c_k;&c_t;&c_e;&c_r;&c_y;&c_jo;&c_l;&c_ja;&c_g;</trd>
    <trd lng="bg">&c_b;&c_a;&c_k;&c_t;&c_je;&c_r;&c_i;&c_o;&c_l;&c_o;&c_g;</trd>
    <trd lng="ca">bacteri&ograve;leg</trd>
    <trd lng="de">Bakteriologe</trd>
    <trd lng="en">bacteriologist</trd>
    <trd lng="fa">&a_m;&f_y;&f_k;&a_r;&a_b; &a_sh;&a_n;&a_A;&a_s;</trd>
    <trd lng="fr">bact&eacute;riologiste</trd>
    <trd lng="he">&bet;&qof;&tet;&resh;&yod;&vav;&lamed;&vav;&gimel;</trd>
    <trd lng="hu">bakterol&oacute;gus</trd>
    <trd lng="id">bakteriolog</trd>
    <trd lng="nl">bacterioloog</trd>
    <trd lng="pl">bakteriolog</trd>
    <trd lng="ru">&c_b;&c_a;&c_k;&c_t;&c_je;&c_r;&c_i;&c_o;&c_l;&c_o;&c_g;</trd>
    <trdgrp lng="pt">
      <trd>bacteriologista</trd>,
      <trd>bacteri&oacute;logo</trd>
    </trdgrp>
    <trd lng="zh">&#x7D30;&#x83CC;&#x5B78;&#x5BB6; <pr>x&#x12B;j&#x16B;nxu&#xE9;ji&amacron;</pr></trd>
</snc>
</drv>
