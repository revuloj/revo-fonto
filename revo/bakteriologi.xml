<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>bakteriologi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="bakteriologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="bakteriologi.0o.SCI">
    <uzo tip="fak">BAK</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Scienco pri <ref tip="vid" 
      cel="bakter.0o.BAK">bakterioj</ref>, parto de <ref tip="malprt" 
      cel="mikrob1.0o.SCI">mikrobiologio</ref>:  
      <ekz>
        li fari&gcirc;is fakulo pri <tld lit="B"/>o en Instituto Pasteur en Parizo
        <fnt>
          <aut>P. S. Viana</aut>,
          <vrk><url ref="http://www.kke.org.br/esperanto/prozajxoj/oswaldo_cruz_vivo_kontraux_antauxjugxoj.php">Oswaldo Cruz - Vivo kontra&ubreve; Anta&ubreve;ju&gcirc;oj</url></vrk>,
          <lok>[vidita je 2006-06-19]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">bact&eacute;riologie</trd>
</drv>

<drv mrk="bakteriologi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="bakteriologi.0isto.SCI">
    <dif>
      Fakulo pri <tld/>o:
      <ekz>
        tie estis formitaj kuracistoj, <tld/>istoj, mikrobiologiistoj
        <fnt>
        <aut>Liga Colombiana de Esperanto</aut>,
        <vrk><url ref="http://us.geocities.com/esperantokolombio/patarroyo.html">Manuel Elkin Patarroyo</url></vrk>,
        <lok>[vidita je 2006-06-21]</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">bact&eacute;riologiste</trd>
</drv>
</art>

