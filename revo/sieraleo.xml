<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>sieraleon</rad>/o
</kap>

<drv mrk="sieraleo.0o">
  <kap>
    <tld lit="S"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="sieraleo.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la okcidenta <ref tip="malprt" 
      cel="afrik.A0o">Afriko</ref> kun &ccirc;efurbo Frita&ubreve;no
      (Liberurbo).
      Teritorio de Respubliko Sieraleono.
      ISO-3166 kodo: SL.
      &Gcirc;i limas kun
      <ref tip="vid" cel="gvine.0o.GEOG">Gvineo</ref>,
      <ref tip="vid" cel="liberi.0o.GEOG">Liberio</ref>
      kaj <ref tip="vid" cel="atlant.0o.GEOG">Atlantiko</ref>.
    </dif>
    <trd lng="bg">&c_S;&c_i;&c_je;&c_r;&c_a; &c_L;&c_je;&c_o;&c_n;&c_je;</trd>
    <trd lng="de">Sierra Leone</trd>
    <trd lng="en">Sierra Leone</trd>    
    <trdgrp lng="pl">
      <trd>Sierra Leone</trd>,
      <trd>Republika <ind>Sierra Leone</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

