<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>higien</rad>/o
</kap>
<drv mrk="higien.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Scienco studanta la rimedojn por konservi la sanon. 
    </dif>
  </snc>
  <trd lng="cs">hygiena</trd>
  <trd lng="de">Hygiene</trd>
  <trd lng="ru">&c_g;&c_i;&c_g;&c_i;&c_je;&c_n;&c_a;</trd>
  <trd lng="sv">hygien</trd>
</drv>
<drv mrk="higien.0a">
  <kap><tld/>a</kap>
  <snc num="1">
    <dif>
      Rilata al <tld/>o: <ekz><tld/>a traktato.</ekz>
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Saniga, konforma al <tld/>o:
      <ekz><tld/>a vivo<fnt>B</fnt>,</ekz>
      <ekz>promenado.</ekz>
    </dif>
  </snc>
  <trd lng="de">hygienisch</trd>
  <trd lng="ru">&c_g;&c_i;&c_g;&c_i;&c_je;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sv">hygienisk</trd>
</drv>
</art>
