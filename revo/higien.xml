<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: higien.xml,v 1.28 2023/11/15 17:08:00 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>higien</rad>/o
</kap>
<drv mrk="higien.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="scienc.0o.fako">Scienco</ref> studanta la rimedojn 
      por konservi la sanon de unuopulo kaj la publiko; 
      la tuto de la tiucelaj reguloj por konservi la sanon kaj eviti malsanojn, infekti&gcirc;ojn ktp:
      <ekz>
        vivmaniero <klr>[&#x2026;]</klr> konforma al la reguloj de la <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de longevivado
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        por ke bacil' ne &gcirc;ermu / Pro malsa&gcirc;a <tld/>', / La fenestrojn vi malfermu / Post
        forigo de l' kurten'
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
          <lok>&Ccirc;astemulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        publika <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="&Monato;/2006/009055.php">Pezaj enlandaj &scirc;ar&gcirc;oj por la registaro</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_g;&c_ib;&c_je;&c_n;&c_a;</trd>
  <trd lng="bg">&c_h;&c_i;&c_g;&c_i;&c_je;&c_n;&c_a;</trd>
  <trd lng="br">yec'hedouriezh</trd>
  <trd lng="ca">higiene</trd>
  <trd lng="cs">hygiena</trd>
  <trd lng="de">Hygiene</trd>
  <trd lng="el">&ypsilon;&gamma;&jota;&epsilon;&jota;&ny;&eta_ton;</trd>
  <trd lng="en">hygiene</trd>
  <trd lng="es">higiene</trd>
  <trd lng="fr">hygi&egrave;ne</trd>
  <trdgrp lng="hu">
    <trd>higi&eacute;nia</trd>,
    <trd>higi&eacute;n&eacute;</trd>,
    <trd>eg&eacute;szs&eacute;gtan</trd>,
    <trd>eg&eacute;szs&eacute;g&uuml;gy</trd>
  </trdgrp>
  <trd lng="it">igiene</trd>
  <trdgrp lng="ja">
    <trd>&#x885B;&#x751F;&#x5B66;  <pr>&#x3048;&#x3044;&#x305B;&#x3044;&#x304C;&#x304F;</pr></trd>,
    <trd>&#x885B;&#x751F;  <pr>&#x3048;&#x3044;&#x305B;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">hygi&euml;ne</trd>
  <trd lng="pl">higiena</trd>
  <trd lng="pt">higiene</trd>
  <trd lng="ru">&c_g;&c_i;&c_g;&c_i;&c_je;&c_n;&c_a;</trd>
  <trd lng="sk">hygiena</trd>
  <trd lng="sv">hygien</trd>
  <trd lng="uk">&#x433;&#x456;&#x433;&#x456;&#x454;&#x43D;&#x430;</trd>
  <trdgrp lng="zh">
    <trd>&#x536B;&#x751F; <pr>w&#xE8;ish&#x113;ng</pr></trd>,
    <trd>&#x885E;&#x751F; <pr>w&#xE8;ish&#x113;ng</pr></trd>,
    <trd>&#x536B;&#x751F;&#x5B66; <pr>w&#xE8;ish&#x113;ngxu&#xE9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="higien.0a">
  <kap><tld/>a</kap>
  <snc mrk="higien.0a.rilata">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        grava afero estas konformi&gcirc;i al la <tld/>aj postuloj: frue ku&scirc;i&gcirc;i por la dormo,
        trinki lakton, man&gcirc;i multe da fruktoj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi legis sufi&ccirc;e da <tld/>aj libroj kaj scias, ke teo kaj kafo estas narkotikaj
        trinka&jcirc;oj sen nutra enhavo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;tualetpapero&rightquot; &dash; &scirc;i ne diras la vulgaran nomon de la <tld/>a papero
        <fnt>
          <bib>KKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu sviso akiru 50 <tld/>ajn maskojn
        <fnt>
          <bib>Monato</bib>
          <aut>Marteno Ecott</aut>
          <vrk><url ref="&Monato;/2007/009259.php">Planoj senmaskigitaj</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="medici.0a">medicina</ref>,
       <ref cel="sanita.0a">sanitara</ref>
    </refgrp>
    <trd lng="de">Hygiene-</trd>
    <trdgrp lng="hu">
      <trd>higi&eacute;niai</trd>,
      <trd>eg&eacute;szs&eacute;g&uuml;gy</trd>
    </trdgrp>
    <trd lng="zh">&#x536B;&#x751F; <pr>w&#xE8;ish&#x113;ng</pr></trd>
</snc>
  <snc mrk="higien.0a.saniga">
    <dif>
      <ref tip="super" cel="san.0iga">Saniga</ref>, konforma al <tld/>o:
      <ekz>
        <tld/>a vivmaniero
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        laboro <klr>[estas]</klr> eble la sola efika <tld/>a rimedo kontra&ubreve; malsanoj de
        la korpo kaj spirito
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        tio ne estas <tld/>a: mi ne povas su&ccirc;i ion, kion multaj fremduloj anta&ubreve;e
        enbu&scirc;igis
        <fnt>
          <bib>KrB</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">hygienisch</trd>
    <trdgrp lng="hu">
      <trd>higi&eacute;nikus</trd>,
      <trd>eg&eacute;szs&eacute;ges</trd>
    </trdgrp>
    <trd lng="zh">&#x536B;&#x751F;&#x7684; <pr>w&#xE8;ish&#x113;ngde</pr></trd>
</snc>
  <trd lng="be">&c_g;&c_ib;&c_g;&c_ib;&c_je;&c_n;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="bg">&c_h;&c_i;&c_g;&c_i;&c_je;&c_n;&c_i;&c_ch;&c_je;&c_n;</trd>
  <trd lng="br">yec'hedus</trd>
  <trd lng="ca">higi&egrave;nic</trd>
  
  <trd lng="el">&ypsilon;&gamma;&jota;&epsilon;&jota;&ny;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">hygienic</trd>
  <trd lng="es">higi&eacute;nico</trd>
  <trdgrp lng="fr">
    <trd>d'<ind>hygi&egrave;ne</ind></trd>,
    <trd>hygi&eacute;nique</trd>
  </trdgrp>
  <trd lng="it">igienico</trd>
  <trdgrp lng="ja">
    <trd>&#x885B;&#x751F;&#x4E0A;&#x306E;  <pr>&#x3048;&#x3044;&#x305B;&#x3044;&#x3058;&#x3087;&#x3046;&#x306E;</pr></trd>,
    <trd>&#x885B;&#x751F;&#x7684;&#x306A;  <pr>&#x3048;&#x3044;&#x305B;&#x3044;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x5065;&#x5EB7;&#x306B;&#x3088;&#x3044;  <pr>&#x3051;&#x3093;&#x3053;&#x3046;&#x306B;&#x3088;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">hygi&euml;nisch</trd>
  <trd lng="pl">higieniczny</trd>
  <trd lng="pt">higi&ecirc;nico</trd>
  <trd lng="ru">&c_g;&c_i;&c_g;&c_i;&c_je;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sv">hygienisk</trd>
  <trdgrp lng="uk">
    <trd>&#x433;&#x456;&#x433;&#x456;&#x454;&#x43D;&#x456;&#x447;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x441;&#x430;&#x43D;&#x456;&#x442;&#x430;&#x440;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>
</art>
