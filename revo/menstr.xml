<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: menstr.xml,v 1.20 2022/10/09 13:00:02 revo Exp $">
<kap>
  <ofc>9</ofc>
  <rad>menstru</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="menstr.0i">
  <kap><ofc>9</ofc><tld/>i</kap>
  <snc mrk="menstr.0i.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Ellasi sangon el la utero dum kelkaj tagoj proksimume unu fojon en
      &ccirc;iu monato: <ekz>&scirc;i regule menstruas</ekz>. 
    </dif>
  </snc>
  <trd
  lng="bo">&#xF5F;&#xFB3;&#xF0B;&#xF58;&#xF5A;&#xF53;&#xF0B;&#xF62;&#xF92;&#xFB1;&#xF42;&#xF0B;</trd>
  <trdgrp lng="de">
    <trd>menstruieren</trd>,
    <trd>die Regelblutung haben</trd>,
    <trd>die Monatsblutung haben</trd>,
    <trd>regelbluten</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>miesi&aogonek;czkowa&cacute;</trd>,
    <trd>mie&cacute; <ind>menstruacj&eogonek;</ind></trd>
  </trdgrp>
  <trd lng="ro">avea menstrua&#x21B;ie</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_je;&#x43D;&c_s;&c_t;&c_r;&c_u;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43C;&c_a;&c_t;&c_i; &#x43C;&c_je;&#x43D;&c_s;&c_t;&c_r;&c_u;&c_a;&c_c;&c_ib;&#x44E;</trd>
  </trdgrp>
</drv>

<drv mrk="menstr.0o">
  <kap><ofc>9</ofc><tld/>o <fnt><bib>PV</bib></fnt></kap>
  <snc mrk="menstr.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Perioda travagina sangelfluo &ccirc;e virino.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="monat.0ajxo.menstruo">monata&jcirc;o</ref>,
    <ref cel="ordina.virina0ajxo.FIZL">virina ordinara&jcirc;o</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_m;&c_je;&c_s;&c_ja;&c_ch;&c_n;&c_y;&c_ja;</trd>,
    <trd>&c_m;&c_e;&c_n;&c_s;&c_t;&c_r;&c_u;&c_a;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
  <trd lng="bo">&#xF5F;&#xFB3;&#xF0B;&#xF58;&#xF5A;&#xF53;&#xF0B;</trd>
  <trdgrp lng="ca">
    <trd>menstruaci&oacute;</trd>,
    <trd>regla</trd>,
    <trd>per&iacute;ode</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Monatsblutung</trd>,
    <trd>Regelblutung</trd>,
    <trd>Menstruation</trd>,
    <trd>Regel</trd>,
    <trd>Periode</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>menstrual flow</trd>,
    <trd>menstruation</trd>
  </trdgrp>
   <trdgrp lng="es">
    <trd>menstruaci&oacute;n</trd>,
    <trd>regla</trd>,
    <trd>per&iacute;odo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>menstrues</trd>,
    <trd>r&egrave;gles<klr> (f&eacute;minines)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>menstr&aacute;ci&oacute;</trd>,
    <trd>havi v&eacute;rz&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>maandstonden</trd>,
    <trd>menstruatie</trd>,
    <trd>regel<klr tip="ind"> (menstruatie)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>miesi&aogonek;czka</trd>,
    <trd>menstruacja</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>menstrua&ccedil;&atilde;o</trd>,
    <trd>m&ecirc;nstruo</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>ciclu</trd>,
    <trd>menstrua&tcedil;ie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_n;&c_s;&c_t;&c_r;&c_u;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_m;&c_je;&c_s;&c_ja;&c_ch;&c_n;&c_y;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_je;&#x43D;&c_s;&c_t;&c_r;&c_u;&c_a;&c_c;&c_ib;&#x44F;</trd>,
    <trd>&#x43C;&c_ib;&c_s;&#x44F;&c_ch;&#x43D;&c_je;</trd>
  </trdgrp>
</drv>
</art>
