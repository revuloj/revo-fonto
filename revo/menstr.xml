<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: menstr.xml,v 1.24 2024/01/25 20:10:36 revo Exp $">
<kap>
  <ofc>9</ofc>
  <rad>menstru</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="menstr.0i">
  <kap><ofc>9</ofc><tld/>i</kap>
  <snc mrk="menstr.0i.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Ellasi sangon el la utero dum kelkaj tagoj proksimume unu fojon en
      &ccirc;iu monato: <ekz>&scirc;i regule menstruas.</ekz> 
    </dif>
  </snc>
  <trd lng="bo">&t_za;&t_lata;&t_tsheg;&t_ma;&t_tsha;&t_na;&t_tsheg;&t_ra;&t_gata;&t_yata;&t_ga;&t_tsheg;</trd>
  <trdgrp lng="de">
    <trd>menstruieren</trd>,
    <trd>die Regelblutung haben</trd>,
    <trd>die Monatsblutung haben</trd>,
    <trd>regelbluten</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6708;&#x7D4C;&#x304C;&#x3042;&#x308B;  <pr>&#x3052;&#x3063;&#x3051;&#x3044;&#x304C;&#x3042;&#x308B;</pr></trd>,
    <trd>&#x751F;&#x7406;&#x304C;&#x3042;&#x308B;  <pr>&#x305B;&#x3044;&#x308A;&#x304C;&#x3042;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>miesi&aogonek;czkowa&cacute;</trd>,
    <trd>mie&cacute; <ind>menstruacj&eogonek;</ind></trd>
  </trdgrp>
  <trd lng="ro">avea menstrua&#x21B;ie</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_je;&c_n;&c_s;&c_t;&c_r;&c_u;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_m;&c_a;&c_t;&c_i; &c_m;&c_je;&c_n;&c_s;&c_t;&c_r;&c_u;&c_a;&c_c;&c_ib;&c_ju;</trd>
  </trdgrp>
</drv>

<drv mrk="menstr.0o">
  <kap><ofc>9</ofc><tld/>o <fnt><bib>PV</bib></fnt></kap>
  <snc mrk="menstr.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Perioda travagina sangelfluo &ccirc;e virino:
      <ekz>
        mia <tld/>o komenci&gcirc;is neatendite dum ni estis sur la &scirc;ipo
        <fnt>
          <bib>StU</bib>
          <lok>Al Romo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi a&ubreve;dis en via oficejo ke vi estas malsana 
        <klr>[&#x2026;]</klr> pro <tld/>o
        <fnt>
          <bib>OIP</bib>
          <lok>Ombro sur interna pejza&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi povas vendi al mi paka&jcirc;on da <tld/>aj tukoj
        <fnt>
          <bib>KrB</bib>
          <lok>La mistero de l&#x2019; sango</lok>
        </fnt>.
      </ekz>
    </dif>
  <refgrp tip="sin">
    <ref cel="monat.0ajxo.menstruo">monata&jcirc;o</ref>,
    <ref cel="ordina.virina0ajxo.FIZL">virina ordinara&jcirc;o</ref>
  </refgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_je;&c_s;&c_ja;&c_ch;&c_n;&c_y;&c_ja;</trd>,
    <trd>&c_m;&c_e;&c_n;&c_s;&c_t;&c_r;&c_u;&c_a;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
  <trd lng="bo">&t_za;&t_lata;&t_tsheg;&t_ma;&t_tsha;&t_na;&t_tsheg;</trd>
  <trdgrp lng="ca">
    <trd>menstruaci&oacute;</trd>,
    <trd>regla</trd>,
    <trd>per&iacute;ode</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Monatsblutung</trd>,
    <trd>Regelblutung</trd>,
    <trd>Menstruation</trd>,
    <trd>Regel</trd>,
    <trd>Periode</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>menstrual flow</trd>,
    <trd>menstruation</trd>
  </trdgrp>
   <trdgrp lng="es">
    <trd>menstruaci&oacute;n</trd>,
    <trd>regla</trd>,
    <trd>per&iacute;odo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>menstrues</trd>,
    <trd>r&egrave;gles<klr> (f&eacute;minines)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>menstr&aacute;ci&oacute;</trd>,
    <trd>havi v&eacute;rz&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6708;&#x7D4C;  <pr>&#x3052;&#x3063;&#x3051;&#x3044;</pr></trd>,
    <trd>&#x751F;&#x7406;  <pr>&#x305B;&#x3044;&#x308A;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>maandstonden</trd>,
    <trd>menstruatie</trd>,
    <trd>regel<klr tip="ind"> (menstruatie)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>miesi&aogonek;czka</trd>,
    <trd>menstruacja</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>menstrua&ccedil;&atilde;o</trd>,
    <trd>m&ecirc;nstruo</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>ciclu</trd>,
    <trd>menstrua&#x21B;ie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_n;&c_s;&c_t;&c_r;&c_u;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_m;&c_je;&c_s;&c_ja;&c_ch;&c_n;&c_y;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_m;&c_je;&c_n;&c_s;&c_t;&c_r;&c_u;&c_a;&c_c;&c_ib;&c_ja;</trd>,
    <trd>&c_m;&c_ib;&c_s;&c_ja;&c_ch;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6708;&#x7D93; <pr>yu&#xE8;j&#x12B;ng</pr></trd>,
    <trd>&#x6708;&#x7ECF; <pr>yu&#xE8;j&#x12B;ng</pr></trd>,
    <trd>&#x7678;&#x6C34; <pr>gu&#x1D0;shu&#x1D0;</pr></trd>,
    <trd>&#x7D93;&#x8840; <pr>j&#x12B;ngxu&#xE8;</pr></trd>,
    <trd>&#x7ECF;&#x8840; <pr>j&#x12B;ngxu&#xE8;</pr></trd>,
    <trd>&#x5929;&#x7678; <pr>ti&#x101;ngu&#x1D0;</pr></trd>,
    <trd>&#x6708;&#x4E8B; <pr>yu&#xE8;sh&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>
