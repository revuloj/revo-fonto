<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>periheli</rad>/o
</kap>
<drv mrk="perihe.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Punkto de la orbito de sun-&ccirc;irka&ubreve;anta objekto, 
      &ccirc;e kiu tiu estas plej proksima al la suno.
    </dif>
    <refgrp tip="vid">
      <ref cel="afeli.0o">afelio</ref>
    </refgrp>
    <refgrp tip="super">
      <ref cel="apsid.0o">apsido</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_r;&c_y;&c_g;&c_je;&c_l;&c_ib;&c_j;</trd>
  <trd lng="ca">periheli</trd>
  <trd lng="cs">p&rcaron;&iacute;slun&iacute;</trd>
  <trd lng="da">perihelion</trd>
  <trd lng="de">Perihel</trd>
  <trd lng="en">perihelion</trd>
  <trd lng="es">perihelio</trd>
  <trd lng="fi">periheli</trd>
  <trd lng="fr">p&eacute;rih&eacute;lie</trd>
  <trdgrp lng="hu">
    <trd>napk&ouml;zelpont</trd>,
    <trd>perih&eacute;lium</trd></trdgrp>
  <trd lng="it">perielio</trd>
  <trd lng="ja">&#36817;&#26085;&#28857; [<ind>&#12365;&#12435;&#12376;&#12388;&#12390;&#12435;</ind>]</trd>
  <trd lng="pl">peryhelium</trd>
  <trd lng="nl">perihelium</trd>
  <trd lng="pt">peri&eacute;lio</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_i;&c_g;&c_je;&c_l;&c_i;&c_j;</trd>
  <trd lng="tr">enberi</trd>
</drv>
</art>
