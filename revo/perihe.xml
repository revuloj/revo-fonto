<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>periheli</rad>/o
</kap>
<drv mrk="perihe.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Punkto de la orbito de planedo, en kiu gi estas plej malproksima de
      la suno.
    </dif>
    <refgrp tip="vid">
      <ref cel="afeli.0o">afelio</ref>,
      <ref cel="apsid.0o">apsido</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Perihel</trd>
  <trd lng="en">perihelion</trd>
  <trd lng="nl">perihelium</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_i;&c_g;&c_je;&c_l;&c_i;&c_j;</trd>
</drv>
</art>
