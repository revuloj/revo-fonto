<vortaro>
  <art mrk="$Id: arhxe.xml,v 1.10 2023/10/28 08:33:08 afido Exp $">
    <kap>
      <rad>ar&hcirc;e</rad>/o,
      <var><kap><rad var="k">arke</rad>/o</kap></var>
    </kap>

<drv mrk="arhxe.0o">
  <kap><tld/>o, <var><kap><tld var="k"/>o</kap></var></kap>
  <uzo tip="fak">BIO</uzo>
  <snc>
    <dif>
      Iu el grupo de <ref tip="super" cel="prokariot.0o">prokarioto</ref>j,
      kiuj distingi&gcirc;as de <ref cel="bakter.0o">bakterio</ref>j en la maniero kiel la 
      DNA estas tradukata al proteinoj:
      <ekz>
        tiuj enzimoj trovi&gcirc;as en <tld/>oj kaj bakterioj
        <fnt>
          <vrk><url ref="&Viki;/Hidrogenazo">Hidrogenazo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la unuaj organismoj estis nuraj unu&ccirc;elaj bakterioj kaj <tld var="k"/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2014/011280.php">De Gabono al A&ubreve;strio por unika
          ekspozicio</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Archaeon</trd>
    <trd lng="en">archaeon</trd>
    <trd lng="fr">archaeon</trd>
  </snc>
  <rim>
    <ref tip="hom" cel="arke.0o">Arkeo</ref> nomi&gcirc;as anka&ubreve; la mita &scirc;ipo, per kiu
    Noa&hcirc;o savis sin de la Diluvo.
  </rim>
  <trd lng="be">&c_a;&c_r;&c_h;&c_je;&c_j; <klr>(&c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_m;)</klr></trd>
  <trd lng="he">&#x5EA;&#x5D9;&#x5D1;&#x5EA; &#x5E0;&#x5D5;&#x5D7;</trd>
  <trd lng="ja">&#x7BB1;&#x8239;  <pr>&#x306F;&#x3053;&#x3076;&#x306D;</pr></trd>
  <trd lng="uk">&#x43A;&#x43E;&c_v;&c_ch;&c_je;&c_g;</trd>
  <trdgrp lng="zh">
    <trd>&#x53E4;&#x7D30;&#x83CC; <pr>g&#x1D4;x&#x12B;j&#x16B;n</pr></trd>,
    <trd>&#x53E4;&#x7EC6;&#x83CC; <pr>g&#x1D4;x&#x12B;j&#x16B;n</pr></trd>
  </trdgrp>
</drv>

