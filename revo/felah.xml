<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: felah.xml,v 1.4 2023/07/14 09:28:45 revo Exp $">
<kap>
  <rad>felah</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="felah.0o">
  <kap><tld/>o</kap>
  <snc mrk="felah.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Malri&ccirc;a sendependa <ref tip="super" cel="kultiv.0isto.AGR">kultivisto</ref>
      en Egiptujo kaj pli vaste en arablingvujo:
      <ekz>
        la <tld/>o larmas sangon, la sangon de sia koro kaj pereas en
        sia doloro
        <fnt>
          <aut>J. Baghy</aut>,
          <vrk><url ref="http://egalite.hu/baghy/migranta/samumo.htm">
          Samumo</url></vrk>,
          <lok>en: Migranta Plumo, 1929</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_e;&c_l;&c_a;&c_h;</trd>
  <trd lng="fr">fellah</trd>
  <trd lng="uk">&c_f;&c_je;&#x43B;&c_a;&c_h; (&c_jeu;&c_g;&c_i;&#x43F;&c_je;&c_t;&c_s;&#x44C;&#x43A;&c_i;&c_j; &c_s;&c_je;&#x43B;&#x44F;&#x43D;&c_i;&#x43D;)</trd>
</drv>
</art>

