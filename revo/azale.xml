<art mrk="$Id: azale.xml,v 1.26 2023/08/30 18:23:38 revo Exp $">
<kap>
  <rad>azale</rad>/o
</kap>
<drv mrk="azale.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio <ref tip="malprt" cel="erikac.0oj">erikacoj</ref>
      (<trd lng="la">Azalea</trd>),
      kun grandaj multnombraj belkoloraj floroj:
      <ekz>
        &ccirc;irka&ubreve;tonditaj <tld/>oj kun ru&gcirc;aj floroj
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Yasuo</aut>
          <vrk><url ref="&Monato;/2007/009114.php">Vizito al Takaha&scirc;i</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la marko, kiu gajnis la kvaran premion, montras du le&gcirc;e protektatajn kreska&jcirc;ojn:
        <klr>[&#x2026;]</klr> saksifragon kaj <klr>[&#x2026;]</klr> alpan <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2014/011197.php">Po&scirc;tmarko premiita</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/a/a5/Azalea.jpg/476px-Azalea.jpg" prm="CC BY-SA 2.0">floroj de <tld/>o, Rio de Janeiro, Brazilo
        <fnt>
          <aut>Eurico Zimbres</aut>
          <lok><url ref="&WCW;/File:Azalea.jpg">&WCW;/File:Azalea.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_a;&c_z;&c_a;&c_l;&c_ib;&c_ja;</trd>
  <trd lng="ca">azalea</trd>
  <trd lng="cs">azalka</trd>
  <trd lng="de">Azalee</trd>
  <trd lng="en">azalea</trd>
  <trd lng="es">azalea</trd>
  <trd lng="fr">azal&eacute;e</trd>
  <trd lng="he">&#x5D0;&#x5D6;&#x5DC;&#x5D9;&#x5D4;</trd>
  <trd lng="hu">az&aacute;lea</trd>
  <trd lng="it">azalea</trd>
  <trdgrp lng="ja">
    <trd>&#x30A2;&#x30B6;&#x30EC;&#x30A2;</trd>,
    <trd>&#x30C4;&#x30C4;&#x30B8;</trd>
  </trdgrp>
  <trd lng="nl">azalea</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_z;&c_a;&c_l;&c_i;&c_ja;</trd>,
    <trd>&c_a;&c_z;&c_a;&c_l;&c_je;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">azalka</trd>
  <trd lng="sv">azalea</trd>
  <trd lng="uk">&#x430;&#x437;&#x430;&#x43B;&#x456;&#x44F;</trd>
</drv>
</art>
