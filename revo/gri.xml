<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>gri</rad>/o</kap>
<drv mrk="gri.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Nomo donita al la grajnoj de greno, kiam ili estas senigitaj je
      sia &scirc;elo kaj maldelikate pistitaj:
      <ekz>
      sed multe pli impone estas, ke en &gcirc;i ni ricevas varman ka&ccirc;on 
      el <tld/>o
      <fnt>
      <aut>Stefan Maul</aut>,
      <vrk>El verva vivo &jcirc;urnalista</vrk>,
      <lok>p. 17</lok>
      </fnt>.
      </ekz>
    </dif>
  </snc>
  <adm>
    Ial PIV1 konas preskauxsinonimon &leftquot;semolo&rightquot;. <aut>MB</aut>
  </adm>
  <trd lng="be">&c_k;&c_r;&c_u;&c_p;&c_y;</trd>
  <trd lng="de">Grie&szlig;</trd>
  <trdgrp lng="en">
    <trd>coarse meal</trd>,
    <trd>grits</trd>,
    <trd>groats</trd>
  </trdgrp>
  <trd lng="fa">&a_b;&a_l;&a_gh;&a_w;&a_r;</trd>
  <trdgrp lng="fr">
    <trd>gruau</trd>,
    <trd>semoule</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>dara</trd>,
    <trd>gr&iacute;z</trd>
  </trdgrp>
  <trd lng="nl">griesmeel</trd>
  <trdgrp lng="pt">
    <trd>s&ecirc;mola</trd>,
    <trd>canjiquinha</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_r;&c_u;&c_p;&c_a;</trd>
  <trd lng="tr">irmik</trd>
</drv>

<drv mrk="gri.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Ka&ccirc;o el <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_sh;&c_a; <klr tip="amb">(&c_z; &c_k;&c_r;&c_u;&c_p;&c_a;&c_w;)</klr></trd>
  <trdgrp lng="de">
    <trd>Grie&szlig;brei</trd>,
    <trd>Gr&uuml;tze</trd>
  </trdgrp>
  <trd lng="en">porridge</trd>
  <trdgrp lng="fa">
    <trd>&a_h;&a_fatha;&a_l;&f_y;&a_m;</trd>,
    <trd>&a_kh;&a_w;&a_r;&a_A;&f_k; &a_b;&a_l;&a_gh;&a_w;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">semoule<klr tip="ind"> (plat)</klr></trd>
  <trd lng="hu">darak&aacute;sa</trd>
  <trd lng="nl">griesmeelpap</trd>
  <trd lng="ru">&c_k;&c_a;&c_sh;&c_a; <klr>(&c_i;&c_z; &c_k;&c_r;&c_u;&c_p;&c_y;)</klr></trd>
</drv>
</art>
