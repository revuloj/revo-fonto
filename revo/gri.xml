<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>gri</rad>/o
</kap>
<drv mrk="gri.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Nomo donita al la grajnoj de greno, kiam ili estas senigitaj je
      ilia &scirc;elo kaj maldelikate pistitaj. 
    </dif>
  </snc>
  <trd lng="ru">&c_k;&c_r;&c_u;&c_p;&c_a;</trd>
</drv>
<drv mrk="gri.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Ka&ccirc;o el <tld/>o.  
    </dif>
  </snc>
  <trd lng="ru">&c_k;&c_a;&c_sh;&c_a; <klr>(&c_i;&c_z; &c_k;&c_r;&c_u;&c_p;&c_y;)</klr></trd>
</drv>
</art>
