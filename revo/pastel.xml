<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pastel</rad>/o
</kap>
<drv mrk="pastel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pastel.0o.KOMUNE">
    <dif>
      Lentoforma bombono.
    </dif>
  </snc>
  <snc mrk="pastel.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Lentoforma solida kuracilo, ofte sukera, kiun oni lasas
      fandi&gcirc;i en la bu&scirc;o.
    </dif>
    <refgrp tip="vid">
      <ref cel="kapsul.0o.MED">kapsulo</ref>,
      <ref cel="oblat.0o.MED">oblato</ref>,
      <ref cel="pilol.0o.MED">pilolo</ref>,
      <ref cel="prem.0ajxo.MED">prema&jcirc;o</ref>,
      <ref cel="tablojd.0o">tablojdo</ref>.
    </refgrp>
  </snc>
  <trd lng="br">pastilhezenn</trd>
  <trd lng="ca">pastilla</trd>
  <trdgrp lng="cs">
    <trd>lentilka</trd>,
    <trd>pastilka</trd>,
    <trd>tabletka</trd>
  </trdgrp>
  <trd lng="de">Pastille</trd>
  <trdgrp lng="en">
    <trd>lozenge</trd>,
    <trd>pastille</trd>
  </trdgrp>
  <trd lng="es">pastilla</trd>
  <trd lng="fr">pastille</trd>
  <trd lng="hu">pasztilla</trd>
  <trd lng="nl">pastille</trd>
  <trd lng="pt">pastilha</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_i;&c_l;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>lentilka</trd>,
    <trd>pastilka</trd>,
    <trd>tabletka</trd>
  </trdgrp>
</drv>
</art>
