<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: itri.xml,v 1.26 2023/11/19 10:22:14 revo Exp $">
<kap>
  <rad>itri</rad>/o
</kap>
<drv mrk="itri.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Y</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="39">elemento</ref>
      kun atomnumero 39, atompezo 88,9059;
      malofta transir<ref tip="super" cel="metal.0o">metalo</ref>:
      <ekz>
        en for&jcirc;eta&jcirc;oj de tiu minejo oni malkovris mineralon enhavantan dek anta&ubreve;e
        nekonatajn kemiajn elementojn, nome rarajn terojn, kaj la loknomo <nom>Ytterby</nom>
        servis kiel fonto por la nomoj de kvar el tiuj, nome <tld/>o,
        <ref cel="terbi.0o">terbio</ref>, <ref cel="erbi.0o">erbio</ref>
        kaj <ref cel="iterbi.0o">iterbio</ref>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=341037">Ytterby</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_t;&c_r;&c_y;&c_j;</trd>
  <trd lng="bg">&c_i;&c_t;&c_r;&c_i;&c_j;</trd>
  <trd lng="br">itriom</trd>
  <trd lng="cs">yttrium</trd>
  <trd lng="de">Yttrium</trd>
  <trd lng="en">yttrium</trd>
  <trd lng="es">itrio</trd>
  <trd lng="fr">yttrium</trd>
  <trd lng="hu">ittrium</trd>
  <trd lng="id">itrium</trd>
  <trd lng="it">ittrio</trd>
  <trd lng="ja">&#x30A4;&#x30C3;&#x30C8;&#x30EA;&#x30A6;&#x30E0;</trd>
  <trd lng="la">Yttrium</trd>
  <trd lng="nl">yttrium</trd>
  <trd lng="pl">itr</trd>
  <trd lng="pt">&iacute;trio<klr tip="ind"> (qu&iacute;mica)</klr></trd>
  <trd lng="ru">&c_i;&c_t;&c_t;&c_r;&c_i;&c_j;</trd>
  <trd lng="sk">ytrium</trd>
  <trd lng="sv">yttrium</trd>
  <trd lng="uk">&#x456;&#x442;&#x440;&#x456;&#x439;</trd>
  <trd lng="zh">&#x91D4; <pr>y&#x1D0;</pr></trd>
</drv>
</art>
