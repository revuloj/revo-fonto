<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>itri</rad>/o
</kap>
<drv mrk="itri.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> 
      kun atomnumero 39, atompezo 88,9059, simbolo Y. 
      Malofta <ref tip="super" cel="metal.0o">metalo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_t;&c_r;&c_y;&c_j;</trd>       
  <trd lng="br">itriom</trd>
  <trd lng="de">Yttrium</trd>
  <trd lng="fr">yttrium</trd>
  <trd lng="hu">ittrium</trd>
  <trd lng="nl">yttrium</trd>
  <trd lng="ru">&c_i;&c_t;&c_t;&c_r;&c_i;&c_j;</trd>
</drv>
</art>
