<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: polip.xml,v 1.22 2023/10/20 16:38:17 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>polip</rad>/o
</kap>
<drv mrk="polip.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Formo de <ref tip="malprt" cel="hidroz.0oj">hidrozooj</ref>
      fiksita al la marfundo:
      <ekz>
        <tld/>oj, duone besto duone kreska&jcirc;o, ili elrigardis kiel centkapaj serpentoj
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili povis etendi iujn mukajn membrojn, kiuj aspektis kiel la kaptiloj de <tld/>oj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La A&hcirc;arajoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="meduz.0o">meduzo</ref>
    <bld lok="&WCU;/thumb/0/0b/Schleiden-meduse.jpg/322px-Schleiden-meduse.jpg" prm="PD">Vivciklo de
    ombrela meduzo: 1&#x202F;-&#x202F;8 &ndash; fiksi&gcirc;o de la larvo kaj 
       transformi&gcirc;o al polipo, 9&#x202F;-&#x202F;11 &ndash; strobili&gcirc;o 12&#x202F;-&#x202F;14 &ndash;
       transformi&gcirc;o al plenkreska meduzo
        <fnt>
          <aut>Matthias Jacob Schleiden: &leftquot;Die Entwicklung der Meduse&rightquot; 
          en &leftquot;Das Meer&rightquot;, eldenojo A. Sacco Nachf., Berlino, 1869</aut>
          <lok><url ref="&WCW;/File:Schleiden-meduse.jpg">&WCW;/File:Schleiden-meduse.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ib;&c_p;</trd>
  <trd lng="ca">p&ograve;lip</trd>
  <trd lng="cs">polyp</trd>
  <trd lng="de">Polyp</trd>
  <trd lng="en">polyp</trd>
  <trd lng="fr">polype</trd>
  <trd lng="hu">polip</trd>
  <trdgrp lng="ja">
    <trd>&#x30DD;&#x30EA;&#x30D7;</trd>,
    <trd>&#x30DD;&#x30EA;&#x30FC;&#x30D7;</trd>,
    <trd>&#x8338;&#x816B;  <pr>&#x304D;&#x306E;&#x3053;&#x816B;</pr></trd>
  </trdgrp>
  <trd lng="nl">poliep</trd>
  <trd lng="pl">polip</trd>
  <trd lng="pt">p&oacute;lipo</trd>
  <trd lng="ro">polip</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_i;&c_p;</trd>
  <trd lng="sk">polyp</trd>
  <trd lng="uk">&#x43F;&#x43E;&#x43B;&c_ib;&#x43F;</trd>
</drv>
</art>
