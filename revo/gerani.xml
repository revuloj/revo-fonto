<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>gerani</rad>/o
</kap>
<drv mrk="gerani.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="gerani.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Geranium</trd>),
      kun belaj brilkoloraj floroj kaj longbeka frukto.
    </dif>
    <ref tip="sin" cel="bek.cikonio0o">cikoniobeko</ref>
  </snc>
  <bld lok="../bld/gerani.jpg">
    <ind>malbonodora <tld/>o</ind>
    <trd lng="la">Geranium robertianum</trd>
  </bld>
  <trd lng="de">Storchschnabel</trd>
</drv>
<drv mrk="gerani.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Geraniaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Storchschnabelgew&auml;chse</trd>
</drv>
</art>
