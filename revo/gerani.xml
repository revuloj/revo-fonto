<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>gerani</rad>/o
</kap>
<drv mrk="gerani.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <tld/>acoj (<trd lng="la">Geranium</trd>),
      kun belaj brilkoloraj floroj kaj longbeka frukto (cikoniobeko).  
    </dif>
  </snc>
  <bld lok="../bld/gerani.jpg">
    malbonodora <tld/>o (<trd lng="la">Geranium robertianum</trd>)
  </bld>
  <trd lng="de">Storchschnabel</trd>
</drv>
<drv mrk="gerani.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Geraniaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Storchschnabelgew&auml;chse</trd>
</drv>
</art>
