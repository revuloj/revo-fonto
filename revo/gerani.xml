<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>gerani</rad>/o
</kap>
<drv mrk="gerani.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="gerani1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Geranium</trd>),
      kun belaj brilkoloraj floroj kaj longbeka frukto.
    </dif>
    <ref tip="sin" cel="bek.cikonio0o">cikoniobeko</ref>
  </snc>
  <bld lok="../bld/gerani.jpg">
    <ind>malbonodora <tld/>o</ind>
    <trd lng="la">Geranium robertianum</trd>
    <trd lng="hu">neh&eacute;zszag&uacute; g&oacute;lyaorr</trd>
    <trd lng="nl">robertskruid</trd>
  </bld>
  <trd lng="de">Storchschnabel</trd>
  <trd lng="es">geranio</trd>
  <trd lng="fr">g&eacute;ranium</trd>
  <trd lng="hu">g&oacute;lyaorr</trd>
  <trdgrp lng="nl">
    <trd>ooievaarsbek</trd>,
    <trd>geranium</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_je;&c_r;&c_a;&c_n;&c_mol;</trd>
</drv>

</art>
