<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gerani.xml,v 1.30 2023/11/11 10:00:40 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>gerani</rad>/o
</kap>
<drv mrk="gerani.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="gerani1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Geranium</trd>),
      kun belaj brilkoloraj floroj kaj longbeka frukto:
      <ekz>
        fenestroj, en kiuj kreskas balzaminoj kaj <tld/>oj
        <fnt>
          <bib>Fab2</bib>
          <lok>El fenestro en Vartou</lok>
        </fnt>;
      </ekz>
      <ekz>
        du elefantoj el argilo sen dorso, sur kies loko el la en&scirc;utita tero
        kreskis en unu el ili <klr>[&#x2026;]</klr> granda floranta <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova strata lanterno</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="bek.cikonio0o">cikoniobeko</ref>
  </snc>
  <bld lok="&WCU;/thumb/7/78/Costigiola-geranio_selvatico-1.jpg/640px-Costigiola-geranio_selvatico-1.jpg" prm="CC BY-SA 3.0"><ind>malbonodora <tld/>o</ind> (Geranium robertianum)
       <fnt>
         <aut>Claudio Gioseffi, propra verko</aut>
         <lok><url ref="&WCW;/File:Costigiola-geranio_selvatico-1.jpg">&WCW;/File:Costigiola-geranio_selvatico-1.jpg</url></lok>
       </fnt>
    <trdgrp lng="de">
      <trd>Ruprechtskraut</trd>,
      <trd>Stinkender Storchschnabel</trd>
    </trdgrp>
    <trd lng="la">Geranium robertianum</trd>
    <trd lng="hu">neh&eacute;zszag&uacute; g&oacute;lyaorr</trd>
    <trd lng="nl">robertskruid</trd>
  </bld>
  <bld lok="&WCU;/thumb/b/be/Geranio_di_Pippi.JPG/640px-Geranio_di_Pippi.JPG" prm="CC BY-SA 4.0">Kulturataj <tld/>oj
       <fnt>
         <aut>Pippicalzelunghe2015, propra verko</aut>
         <lok><url ref="&WCW;/File:Geranio_di_Pippi.JPG">&WCW;/File:Geranio_di_Pippi.JPG</url></lok>
       </fnt>
  </bld>
  <trd lng="be">&c_g;&c_je;&c_r;&c_a;&c_n;&c_mol;</trd>
  <trd lng="bg">&c_z;&c_d;&c_r;&c_a;&c_v;&c_je;&c_c;</trd>
  <trdgrp lng="de">
    <trd>Storchschnabel</trd>,
    <trd>Geranie</trd>
  </trdgrp>
  <trd lng="en">geranium</trd>
  <trd lng="es">geranio</trd>
  <trd lng="fr">g&eacute;ranium</trd>
  <trd lng="hu">g&oacute;lyaorr</trd>
  <trd lng="ja">&#x30D5;&#x30A6;&#x30ED;&#x30BD;&#x30A6;</trd>
  <trdgrp lng="nl">
    <trd>ooievaarsbek</trd>,
    <trd>geranium</trd>
  </trdgrp>
  <trd lng="pl">bodziszek</trd>
  <trdgrp lng="pt">
   <trd>ger&acirc;nio</trd>, 
   <trd>malva-rosa</trd> 
  </trdgrp>
  <trd lng="ru">&c_g;&c_je;&c_r;&c_a;&c_n;&c_mol;</trd>
  <trd lng="tr">sardunya</trd>
  <trd lng="sk">pakost</trd>
  <trdgrp lng="cs">
    <trd>ger&aacute;nie</trd>,
    <trd>ger&aacute;nium</trd>,
    <trd>kakost</trd>,
    <trd>&ccaron;ap&iacute; n&uring;sek</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x433;&#x435;&#x440;&#x430;&#x43D;&#x44C;</trd>,
    <trd>&#x436;&#x443;&#x440;&#x430;&#x432;&#x435;&#x446;&#x44C;</trd>
  </trdgrp>
  <trd lng="zh">&#x8001;&#x9E1B;&#x8349; <pr>l&#x1CE;ogu&#xE0;nc&#x1CE;o</pr></trd>
</drv>

</art>
