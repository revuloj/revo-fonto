<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>sapotac</rad>/oj
</kap>
<drv mrk="sapotac.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="sapotac.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" 
      lst="voko:botanikaj_familioj">Familio</ref>
      de <ref tip="malprt" cel="plant.floro0oj">floroplantoj</ref>
      (<trd lng="la">Sapotaceae</trd>),
      arboj kaj arbedoj enhavantaj laktecan sukon.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Sapotengew&auml;chse</trd>,
    <trd>Breiapfelgew&auml;chse</trd>
  </trdgrp>
</drv>
</art>

