<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: provid.xml,v 1.17 2019/06/30 07:43:29 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>providenc</rad>/o
</kap>
<drv mrk="provid.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Dia sa&gcirc;o, anta&ubreve;zorganta kaj reganta &ccirc;ion:
      <ekz>
        mi scias, kiaj estas miaj agodevoj rilate al la animoj, kiujn la <tld lit="P"/>o
        konfidas al mia prizorgado, mi ne povas rigardi indiferente, se iu el ili
        propravole elmetas sin al dan&gcirc;ero
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne la <tld lit="P"/>o fordonis min al via prizorgado kaj alkondukis min &ccirc;i tien,
        sed mia propra senkapableco
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        trista vo&ccirc;o kiel sonoril' vespera konsilis, ke mi al <tld lit="P"/>o min submetu
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="https://www.monato.be/2004/008265.html"
              >Parakosmo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">provid&egrave;ncia</trd>
  <trdgrp lng="cs">
    <trd>providence</trd>,
    <trd>proz&rcaron;etelnost</trd>,
    <trd>p&rcaron;edv&iacute;davost</trd>
  </trdgrp>
  <trd lng="de">Vorsehung</trd>
  <trd lng="en">providence</trd>
  <trd lng="es">providencia</trd>
  <trd lng="fr">providence</trd>
  <trd lng="hu">gondvisel&eacute;s</trd>
  <trd lng="nl">voorzienigheid</trd>
  <trd lng="pl">opatrzno&sacute;&cacute;</trd>
  <trd lng="pt">provid&ecirc;ncia <klr tip="amb">divina</klr></trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_v;&c_i;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>predv&iacute;davos&tcaron;</trd>,
    <trd>prozrete&lcaron;nos&tcaron;</trd>
  </trdgrp>
</drv>
</art>
