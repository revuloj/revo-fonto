<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: defetism.xml,v 1.10 2021/03/08 14:29:16 revo Exp $">
<kap>
  <rad>defetism</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="defetism.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="defetism.0o.PSI">
    <dif>
      <ref tip="super" cel="rezign.0emo">Rezignemo</ref> pro nefido je
      la venkopovo de si a&ubreve; de sia partio:
      <ekz>
        ni interpaci&gcirc;u kun <nom>Antissa</nom> &ndash; terura <tld/>o
        <fnt><bib>SatirRak</bib>, <lok>la falsa
        a&ubreve;guro</lok></fnt>!
      </ekz>
      <ekz>
        vortoj amaraj, kiuj en la <nom>Lapenna</nom>-epoko estus stampitaj &leftquot;<tld/>aj&rightquot;
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="https://www.monato.be/2003/008022.html">Tridek jaroj da sa&gcirc;o</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="desper.0a">desper</ref>o,
      <ref cel="kuragx.mal0o">malkura&gcirc;o</ref>,
      <ref cel="venk.mal0o">malvenk</ref>emo,
      <ref cel="rezign1.0o">rezignacio</ref>,
      <ref cel="toler.0ema">tolerem</ref>o
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_a;&c_zh;&c_e;&c_n;&c_ch;&c_y;
    &c_n;&c_a;&c_s;&c_t;&c_r;&c_o;&c_j;</trd>,
    <trd>&c_k;&c_a;&c_p;&c_ib;&c_t;&c_u;&c_l;&c_ja;&c_n;&c_c;&c_k;&c_ib;
    &c_n;&c_a;&c_s;&c_t;&c_r;&c_o;&c_j;</trd>,
    <trd>&c_n;&c_ja;&c_v;&c_je;&c_r;'&c_je; &c_w; &c_s;&c_v;&c_a;&c_je;
    &c_s;&c_ib;&c_l;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">derrotisme</trd>
  <trdgrp lng="cs">
    <trd>def&eacute;tizmus</trd>,
    <trd>malodu&scaron;nost</trd>,
    <trd>nev&iacute;ra v &uacute;sp&ecaron;ch</trd>,
    <trd>pora&zcaron;enectv&iacute;</trd>
  </trdgrp>
  <trd lng="de">Def&auml;tismus</trd>
  <trd lng="en">defeatism</trd>
  <trd lng="es">derrotismo</trd>
  <trd lng="fa">&a_sh;&f_k;&a_s;&a_t;&zwnj;&a_b;&a_A;&a_w;&a_r;&f_y;</trd>
  <trd lng="fr">d&eacute;faitisme</trd>
  <trd lng="he">&#x5EA;&#x5B0;&#x5D1;&#x5D5;&#x5BC;&#x5E1;&#x5B0;&#x5EA;&#x5B8;&#x5E0;&#x5D5;&#x5BC;&#x5EA;</trd>
  <trd lng="pl">defetyzm</trd>
  <trd lng="pt">derrotismo</trd>
  <trd lng="sk">neviera v &uacute;spech</trd>
</drv>
</art>

