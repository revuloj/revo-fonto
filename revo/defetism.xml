        ni interpaci&gcirc;u kun Antissa. &ndash; Terura <tld/>o
        <fnt><bib>SatirRak</bib>, <lok>la falsa a&ubreve;guro</lok></fnt>!
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_a;&c_zh;&c_e;&c_n;&c_ch;&c_y; &c_n;&c_a;&c_s;&c_t;&c_r;&c_o;&c_j;</trd>,
