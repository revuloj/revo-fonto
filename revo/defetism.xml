    <trd>&c_n;&c_ja;&c_v;&c_je;&c_r;'&c_je; &c_w; &c_s;&c_v;&c_a;&c_je; &c_s;&c_ib;&c_l;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">derrotisme</trd>
  <trd lng="es">derrotismo</trd>
  <trd lng="fr">d&eacute;faitisme</trd>
</drv>
</art>

