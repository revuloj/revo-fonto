<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>dolar</rad>/o
</kap>
<drv mrk="dolar.0o">
  <kap><tld/>o</kap>
  <snc mrk="dolar.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="unu.mon0o">Monunuo</ref>, interalie de Usono,
      Kanado, A&ubreve;stralio kaj Singapuro:
      <ekz>
        plej ofte oni falsas &ccirc;e ni usonajn <tld/>ojn
        <fnt><aut>Dmitrij Cibulevskij</aut>:
        <vrk>Monfalsado</vrk>, <bib>Monato</bib><lok>jaro 2002a, numero
        7a, p. 16a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="cend.0o">cendo</ref>
  </snc>
  <rim mrk="dolar.0o.RIM_ETIM">
    La nomo venas de la malnova monero
    <ref tip="vid" cel="taler.0o">talero</ref>.
  </rim>
  <trd lng="be">&c_d;&c_o;&c_l;&c_ja;&c_r;</trd>
  <trd lng="de">Dollar</trd>
  <trd lng="en">dollar</trd>
  <trd lng="es">d&oacute;lar</trd>
  <trd lng="fr">dollar</trd>
  <trd lng="hu">doll&aacute;r</trd>
  <trd lng="nl">dollar</trd>
  <trd lng="pl">dolar</trd>
  <trd lng="pt">d&oacute;lar</trd>
  <trd lng="ru">&c_d;&c_o;&c_l;&c_l;&c_a;&c_r;</trd>
  <trd lng="tp">mani</trd>
</drv>
</art>
