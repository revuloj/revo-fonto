<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: boler.xml,v 1.11 2023/07/06 12:55:35 revo Exp $">
<kap>
  <rad>boler</rad>/o
</kap>
<drv mrk="boler.0o">
  <kap><tld/>o</kap>
  <snc mrk="boler.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      &Gcirc;istalia <ref tip="lst" lst="voko:vesta&jcirc;oj" cel="jak.0eto.TEKS">jaketo</ref>
      kun a&ubreve; sen manikoj:
      <ekz>
        la viroj havas pufan pantaloneton, ru&gcirc;an lanzonon kaj
        senmanikan <tld/>on sur helkolora &ccirc;emizo
        <fnt>
          <aut>J. Ribillard</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr36/majorkaj.html">
          Majorkaj azule&hcirc;oj</url></vrk>,
          <lok>La Nica Literatura Revuo, 1958-07 &gcirc;is 08 (3/6),
          p. 211a-217a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o originas de la vestoj de hispana toreadoro
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Bolero_%28vesto%29">
          bolero (vesto)</url>, 2012-02-13</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
    lok="&WCU;/thumb/8/8c/Dress_with_circle_skirt_and_bolero_jacket_-_slight_twirl.jpg/384px-Dress_with_circle_skirt_and_bolero_jacket_-_slight_twirl.jpg"
    prm="CC BY-SA 3.0">virino kun nigra <tld/>o
        <fnt>
          <aut>Tobias ToMar Maier, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Dress_with_circle_skirt_and_bolero_jacket_-_slight_twirl.jpg">&WCW;/File:Dress_with_circle_skirt_and_bolero_jacket_-_slight_twirl.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <rim>
    <ref tip="hom" cel="boler1.0o">Bolero</ref> anka&ubreve; estas danco kaj muzikstilo.
  </rim>
  <trd lng="be">&c_b;&c_a;&c_l;&c_je;&c_r;&c_o; <klr>(&c_v;&c_o;&c_p;&c_r;&c_a;&c_t;&c_k;&c_a;)</klr></trd>
  <trdgrp lng="cs">
    <trd>bolerko</trd>,
    <trd>bolero</trd>
  </trdgrp>
  <trd lng="de">Bolero<klr tip="amb"> (Jacke)</klr></trd>
  <trdgrp lng="en">
    <trd>bolero</trd>,
    <trd>shrug</trd>
  </trdgrp>
  <trd lng="fr">bol&eacute;ro<klr> (veste)</klr></trd>
  <trd lng="he">&#x5E8;&#x5D9;&#x5E7;&#x5D5;&#x5D3; &#x5E1;&#x5E4;&#x5E8;&#x5D3;&#x5D9;</trd>
  <trd lng="nl">bolero</trd>
  <trd lng="sk">bolerko</trd>
  <trd lng="uk">&c_b;&#x43E;&#x43B;&c_je;&c_r;&#x43E; (&c_t;&c_a;&#x43D;&c_je;&c_c;&#x44C; &c_ib;
  &#x43C;&c_je;&#x43B;&#x43E;&c_d;&c_ib;&#x44F;)</trd>
</drv>
</art>

