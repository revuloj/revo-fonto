<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>8</ofc>
<rad>vertic</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="vertic.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="vertic.0o.plurlatero">
    <dif>
      <klr>(de <ref cel="later.plur0o">plurlatero</ref>)</klr>
      &Ccirc;iu <ref tip="super" cel="ekstre.0o.segmento">ekstremo</ref>
      de &gcirc;iaj <ref cel="later.0o.streko">latero</ref>.
    </dif>
  </snc>
  <snc mrk="vertic.0o.pluredro">
    <dif>
      <klr>(de <ref cel="edr.plur0o">pluredro</ref>)</klr>
      &Ccirc;iu 
      <ref tip="super" cel="vertic.0o.plurlatero">vertico 
      <sncref ref="vertic.0o.plurlatero"/></ref> de la plurlateroj, &gcirc;in
      limantaj.
    </dif>
  </snc>
  <trd lng="de">Scheitel</trd>
  <trd lng="fr">sommet<klr tip="ind"> (d'une figure)</klr></trd>
  <trd lng="ru">&c_v;&c_je;&c_r;&c_sh;&c_i;&c_n;&c_a;</trd>
</drv>
</art>
