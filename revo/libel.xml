<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>libel</rad>/o
</kap>
<drv mrk="libel.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Iu el la ordo <ref tip="malprt" cel="odonat.0oj">odonatoj</ref>,
      insekto kun kvar grandaj travideblaj flugiloj kaj longa abdomeno,
      ofte metalkolora.
    </dif>
  </snc>
  <trd lng="de">Libelle</trd>
  <trd lng="en">dragonfly</trd>
  <trd lng="es">lib&eacute;lula</trd>
  <trd lng="nl">libel</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_je;&c_k;&c_o;&c_z;&c_a;</trd>
</drv>
</art>
