<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: koran.xml,v 1.25 2023/09/01 15:46:43 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>Koran</rad>/o
</kap>
<drv mrk="koran.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="koran.0o.ISL">
    <uzo tip="fak">ISL</uzo>
    <dif>
      La <ref tip="super" cel="skrib.SanktaS0o.REL">sankta skribo</ref>
      de <ref tip="vid" cel="islam.0o">islamo</ref>:
      <ekz>
        tiun opinion kontra&ubreve;diras la <tld/>o
        <fnt>
          <aut>C. Lavarenne</aut>
          <vrk>&Ccirc;u islamo ordonas konverti per glavo?</vrk>
          <bib>Monato</bib>
          <lok>2002:2, p. 7a</lok>
         </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_K;&c_a;&c_r;&c_a;&c_n;</trd>
  <trd lng="bg">&c_K;&c_o;&c_r;&c_a;&c_n;</trd>
  <trd lng="ca">Alcor&agrave;</trd>
  <trd lng="cs">kor&aacute;n</trd>
  <trd lng="de">Koran</trd>
  <trd lng="en">Koran</trd>
  <trd lng="fa">&a_q;&a_r;&a_A_madda;&a_n;</trd>
  <trd lng="fr">Coran</trd>
  <trd lng="he">&qof;&vav;&resh;&alef;&fnun;</trd>
  <trd lng="hu">Kor&aacute;n</trd>
  <trdgrp lng="id">
    <trd>Quran</trd>,
    <trd>Alquran</trd>
  </trdgrp>
  <trd lng="ja">&#x30B3;&#x30FC;&#x30E9;&#x30F3;</trd>
  <trd lng="nl">Koran</trd>
  <trd lng="pl">Koran</trd>
  <trdgrp lng="pt">
    <trd>Cor&atilde;o</trd>,
    <trd>Alcor&atilde;o</trd>,    
    <trd>Mo&ccedil;afo</trd>
  </trdgrp>
  <trd lng="ru">&c_K;&c_o;&c_r;&c_a;&c_n;</trd>
  <trd lng="sk">kor&aacute;n</trd>
</drv>
</art>
