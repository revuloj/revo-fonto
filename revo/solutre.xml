<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: solutre.xml,v 1.5 2023/09/27 20:25:31 revo Exp $">
<kap>
  <rad>Solutre</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="solutre.0o">
  <kap><tld/>o</kap>
  <snc mrk="solutre.0o.PRA">
    <uzo tip="fak">ARKE</uzo>
    <uzo tip="fak">PRA</uzo>
    <dif>
      Prahistoria kulturepoko, malfrua parto de e&ubreve;ropa <ref
      tip="malprt" cel="paleolitik.0o.PRA">paleolitiko</ref> (20000 a.K.

      &ndash; 15000 a.K.):
      <ekz>
        oni trovis praepokajn resta&jcirc;ojn <klr>...</klr> de
        kulturoj <tld lit="s"/>a kaj magdalena
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;Baj&oacute;t&rightquot;,
        2008-03-25</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_S;&c_a;&c_l;&c_ju;&c_t;&c_r;&c_e;</trd>,
    <trd>&c_S;&c_a;&c_l;&c_ju;&c_t;&c_r;&c_e;&c_j;&c_s;&c_k;&c_a;&c_ja; &c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">solutri&agrave;</trd>
  <trd lng="fr">Solutr&eacute;en<klr> (subst., &eacute;poque)</klr></trd>
  <trd lng="pl">kultura  <ind>solutrejska</ind></trd>
  <trd lng="pt">solutreano</trd>
</drv>
</art>

