<art mrk="$Id$">
<kap>
  <ofc>*</ofc>-<rad>a</rad>
</kap>
<drv mrk="a.0">
  <kap>-<tld/></kap>
  <snc>
    <dif>
      Fina&jcirc;o (lingvoscience: sufikso) markanta la vorton kiel adjektivon:
    </dif>
    <ekz>
      hom' Mensch - homa menschlich <fnt>F</fnt>.
    </ekz>
    <trdgrp lng="be">
      <trd>-&c_ib;</trd>,
      <trd>-&c_y;</trd>
