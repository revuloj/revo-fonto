      <trd>-<ind>i</ind></trd>,
      <trd>-<ind>s</ind></trd>,
      <trd>-<ind>f&eacute;le</ind></trd>
    </trdgrp>
  <trdgrp lng="pt">
    <trd>-<ind>&aacute;vel</ind></trd>,
    <trd>-<ind>al</ind></trd>
  </trdgrp>
    <trd lng="vo">-ik</trd>
    <trdgrp lng="ru">
      <trd>-&c_i;&c_j;</trd>,
      <trd>-&c_y;&c_j;</trd>,
      <trd>-&c_o;&c_j;</trd>
    </trdgrp>
  </snc>
</drv>
</art>
