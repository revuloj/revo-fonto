<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>-<rad>a</rad>
</kap>
<drv mrk="a.0">
  <kap>-<tld/></kap>
  <snc>
  <uzo tip="fak">GRA</uzo>
    <dif>
      <ref tip="dif" cel="fin.0ajxo.GRA">Fina&jcirc;o</ref>
      (lingvoscience:
      sufikso) markanta la vorton kiel
      <ref tip="dif" cel="adjekt.0o">adjektivon</ref>:
      <ekz>hom' - homa <fnt>F</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>-&c_ib;</trd>,
      <trd>-&c_y;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>-enc</trd>,
      <trd>-&oacute;s</trd>,
      <trd>-aire</trd>,
      <trd>-er</trd>,
      <trd>-ble</trd>,
      <trd>-al</trd>,
      <trd>-&iacute;s... (sufix adjectivador)</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>-ig</trd>,
      <trd>-ich</trd>,
      <trd>-lich</trd>,
      <trd>-isch</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>-al<klr> (suffixe adjectival)</klr></trd>,
      <trd>-eux<klr> (suffixe adjectival)</klr></trd>,
      <trd>-ique<klr> (suffixe adjectival)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>-<ind>i</ind></trd>,
      <trd>-<ind>s</ind></trd>,
      <trd>-<ind>f&eacute;le</ind></trd>
    </trdgrp>
    <trd lng="it"><klr tip="amb">desinenza (suffisso) che trasforma la
    parola in aggettivo (hom-o: uomo - hom-a: umano)</klr></trd>
    <trd lng="es"><klr tip="amb">sufijo adjetival</klr></trd>
    <trd lng="pl"><klr tip="amb">(ko&nacute;c&oacute;wka
    przymiotnika)</klr></trd>
    <trdgrp lng="pt">
      <trd>-<ind>&aacute;vel</ind></trd>,
      <trd>-<ind>al</ind></trd>
    </trdgrp>
    <trd lng="vo">-ik</trd>
    <trdgrp lng="ru">
      <trd>-&c_i;&c_j;</trd>,
      <trd>-&c_y;&c_j;</trd>,
      <trd>-&c_o;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="es">
    <trd>-al/-ar</trd>,
    <trd>-ble</trd>,
    <trd>-ento/-enta</trd>,
    <trd>-il</trd>,
    <trd>-izo/-iza</trd>,
    <trd>-nte</trd>,
    <trd>-osa/-osa</trd>
  </trdgrp>
</drv>
</art>
