<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: a.xml,v 1.30 2023/04/13 18:31:04 revo Exp $">
<kap>
  <ofc>*</ofc>-<rad>a</rad>
</kap>
<drv mrk="a.0">
  <kap>-<tld/></kap>
  <uzo tip="fak">GRA</uzo>
  <dif>
    <ref tip="dif" cel="fin.0ajxo.GRA">Fina&jcirc;o</ref>
    (lingvoscience: sufikso) markanta la vorton kiel
    <ref tip="dif" cel="adjekt.0o">adjektivon</ref>:
  </dif>
  <subdrv>
    <dif>
      <klr>(post a&jcirc;o)</klr> Esprimanta apartenon, materialon, devenon, rolon ks: 
      <ekz>
        hom' &dash; homa
        <fnt><bib>F</bib></fnt>;
      </ekz>
      <ekz>
        mi<tld/>j <klr>(de mi)</klr> filinoj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 31:43</lok>
        </fnt>;
      </ekz>
      <ekz>
        la re&gcirc;<tld/> palaco
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        fremd<klr>(ul)</klr><tld/> teritorio
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Kion Zamenhof ne povis diri en &Gcirc;enevo.</vrk>
        </fnt>;
      </ekz>      
      <ekz>
        laboro &ccirc;iuhom<tld/>
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>(el)</klr>kor<tld/> danko
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en Dresden en la 17a de a&ubreve;gusto
          1908</vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ton<tld/> <klr>(el &scirc;tono)</klr> planko
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>.
      </ekz>     
    </dif>
    <adm>
      Ver&scirc;ajne estus bone distingi la diversajn rilatojn kune kun la ekz-oj per pluraj
      snc-oj. <aut>WD</aut>
    </adm>
  </subdrv>
  <subdrv>
    <dif>
      <klr>(post eco)</klr> Esprimanta similecon, trajton:
      <ekz>
        ru&gcirc;<klr>(est)</klr><tld/>j rond<klr>(form)</klr><tld/>j vangoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Avineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        polv<klr>(hav)</klr><tld/> &ccirc;ambro
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        ra&ubreve;k<klr>(son)</klr><tld/> vo&ccirc;o
        <fnt>
          <bib>Fab4</bib>
          <lok>Anneto</lok>
        </fnt>;
      </ekz>
      <ekz>
        sam<tld/> koloro
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        vento plen<tld/> je odoro <klr>(odorplen<tld/>)</klr>
        <fnt>
          <bib>BdV</bib>
        </fnt>.
      </ekz>
    </dif>
  </subdrv>
  <subdrv>
    <dif>
      <klr>(post ago)</klr> Esprimanta ka&ubreve;zon, esprimon 
      a&ubreve; alian rilaton al tiu ago:
      <ekz>
        mort<klr>(ig)</klr><tld/> bato
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        pardonpet<klr>(esprim)</klr><tld/> rideto
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2006/008760.php"
              >En fremda lando</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        plend<klr>(esprim)</klr><tld/> kriado
        <fnt>
          <bib>Fab4</bib>
          <lok>Anneto</lok>
        </fnt>.
      </ekz>
    </dif>
  </subdrv>
  <subdrv>
    <dif>Post participa sufikso:</dif>
    <snc>
      <dif>
        <klr>(post <ref cel="ant.0">-ant</ref>, <ref cel="int.0">-int</ref>,
        <ref cel="ont.0">-ont</ref>)</klr> Esprimanta rilaton
        inter la ago kaj aganto:
        <ekz>
          bolant<tld/> lakto
          <fnt>
            <bib>PrV</bib>
          </fnt>;
        </ekz>
        <ekz>
          hundo bojnt<tld/>
          <fnt>
            <bib>PrV</bib>
          </fnt>;
        </ekz>
        <ekz>
          pasint<tld/> doloro
          <fnt>
            <bib>PrV</bib>
          </fnt>;
        </ekz>        
        <ekz>
          venont<tld/> jaro
          <fnt>
            <bib>MT</bib>
            <lok>Genezo 17:21</lok>
          </fnt>.
        </ekz>
      </dif>
    </snc>
    <snc>
      <dif>
        <klr>(post <ref cel="at.0">-at</ref>, <ref cel="it.0">-it</ref>, 
        <ref cel="ot.0">-ot</ref>)</klr> Esprimanta rilaton
        inter la ago kaj ties objekto:
        <ekz>
          dezirat<tld/> temo
          <fnt>
            <bib>PrV</bib>
          </fnt>;
        </ekz>
        <ekz>
          &ccirc;evalo donacit<tld/>
          <fnt>
            <bib>PrV</bib>
          </fnt>;
        </ekz>
        <ekz>
          man&gcirc;ot<tld/> fi&scirc;o
          <fnt>
            <bib>PrV</bib>
          </fnt>.
        </ekz>
      </dif>   
    </snc>
  </subdrv>
  <subdrv>
    <dif>
      <klr>(post prepozicio a&ubreve; propozitivo)</klr> Esprimante eston a&ubreve; staton 
      konforman al la prep.:
      <ekz>
        malanta&ubreve;<tld/> flanko
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 26:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        apud<klr>(est)</klr><tld/> &ccirc;ambro
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        surstrat<tld/>j porinfan<tld/>j festoj
        <fnt>
          <aut>Pa&ubreve;lo Gubbins</aut>
          <vrk><url ref="&Monato;/1994/A940601.php">Revivi&gcirc;as la fantomoj -- de 1944</url></vrk>
          <bib>Monato</bib>
          <lok>1994/06, p. 11</lok>
        </fnt>.
      </ekz>
    </dif>
  </subdrv>
  <subdrv>
    <dif>
      <klr>(post nombro)</klr> Esprimanta vicordon:
      <ekz>
        la unu<tld/> &ccirc;ambro
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la XX-<tld/> dinastio
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la 11<tld/> jarcento
        <fnt>
          <aut>Rafaela Urue&ntilde;a</aut>
          <vrk><url ref="&Monato;/2000/006809.php">Vojoj al la sankta urbo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 24</lok>
        </fnt>.
      </ekz>
     </dif>
   </subdrv>
    <refgrp tip="vid">
      <ref cel="ajn.0">ajn</ref>,
      <ref cel="cxia.0">&ccirc;ia</ref>,
      <ref cel="ec.0">-ec</ref>,
      <ref cel="ia.0">ia</ref>,
      <ref cel="kia.0">kia</ref>,
      <ref cel="nenia.0">nenia</ref>,
      <ref cel="tia.0">tia</ref>,
      <ref cel="ul.0">-ul</ref>
    </refgrp>
  <trdgrp lng="be">
    <trd>-&c_ib;</trd>,
    <trd>-&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>-enc</trd>,
    <trd>-&oacute;s</trd>,
    <trd>-aire</trd>,
    <trd>-er</trd>,
    <trd>-ble</trd>,
    <trd>-al</trd>,
    <trd>-&iacute;s... (sufix adjectivador)</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>-ig</trd>,
    <trd>-ich</trd>,
    <trd>-lich</trd>,
    <trd>-isch</trd>
  </trdgrp>
  <trd lng="en">suffix for <ind>adjectives</ind> in Esperanto</trd>
  <trdgrp lng="es">
    <trd><klr tip="amb">sufijo adjetival</klr></trd>
    <trd>-al/-ar</trd>,
    <trd>-ble</trd>,
    <trd>-ento/-enta</trd>,
    <trd>-il</trd>,
    <trd>-izo/-iza</trd>,
    <trd>-nte</trd>,
    <trd>-osa/-osa</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>-al<klr> (suffixe adjectival)</klr></trd>,
    <trd>-eux<klr> (suffixe adjectival)</klr></trd>,
    <trd>-ique<klr> (suffixe adjectival)</klr></trd>
  </trdgrp>
  <trd lng="fy">efterheaksel foar <ind>eigenskipswurden</ind> yn it
  Esperanto</trd>
  <trdgrp lng="hu">
    <trd>-<ind>i</ind></trd>,
    <trd>-<ind>s</ind></trd>,
    <trd>-<ind>f&eacute;le</ind></trd>
  </trdgrp>
  <trd lng="io">-a</trd>
  <trd lng="it"><klr tip="amb">desinenza (suffisso) che trasforma la
  parola in aggettivo (hom-o: uomo - hom-a: umano)</klr></trd>
  <trd lng="nl">achtervoegsel voor <ind>bijvoeglijke naamwoorden</ind>
  in het Esperanto</trd>
  <trd lng="pl"><klr tip="amb">(ko&nacute;c&oacute;wka przymiotnika)</klr></trd>
  <trdgrp lng="pt">
    <trd>-<ind>&aacute;vel</ind></trd>,
    <trd>-<ind>al</ind></trd>
  </trdgrp>
  <trd lng="ro"><klr tip="amb">(finalul adjectivului)</klr></trd>
  <trd lng="th">&#3605;&#3633;&#3623;&#3621;&#3591;&#3607;&#3657;&#3634;&#3618;&#3588;&#3635;&#3588;&#3640;&#3603;&#3624;&#3633;&#3614;&#3607;&#3660;</trd>
  <trd lng="vo">-ik</trd>
  <trdgrp lng="ru">
    <trd>-&c_i;&c_j;</trd>,
    <trd>-&c_y;&c_j;</trd>,
    <trd>-&c_o;&c_j;</trd>
  </trdgrp>
</drv>
</art>
