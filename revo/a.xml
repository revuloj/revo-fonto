<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: a.xml,v 1.23 2021/03/13 13:51:54 revo Exp $">
<kap>
  <ofc>*</ofc>-<rad>a</rad>
</kap>
<drv mrk="a.0">
  <kap>-<tld/></kap>
  <snc mrk="a.0.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      <ref tip="dif" cel="fin.0ajxo.GRA">Fina&jcirc;o</ref>
      (lingvoscience: sufikso) markanta la vorton kiel
      <ref tip="dif" cel="adjekt.0o">adjektivon</ref>:
      <ekz>
        hom' &dash; homa
        <fnt><bib>F</bib></fnt>.
      </ekz>
      <ekz>
        vento plen<tld/> je odoro
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        laboro &ccirc;iuhom<tld/>
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi<tld/> karmemor<tld/> patro
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[el]</klr> mult<tld/>j promesoj <klr>[&#8230;]</klr> 
        grand<tld/> parto estas
        subskribit<tld/>
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi eniros en la unu<tld/>n &ccirc;ambron
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
    <refgrp tip="vid">
      <ref cel="ajn.0">ajn</ref>,
      <ref cel="ec.0">-ec</ref>,
      <ref cel="ia.0">ia</ref>,
      <ref cel="kia.0">kia</ref>,
      <ref cel="nenia.0">nenia</ref>,
      <ref cel="tia.0">tia</ref>
    </refgrp>
  <trdgrp lng="be">
    <trd>-&c_ib;</trd>,
    <trd>-&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>-enc</trd>,
    <trd>-&oacute;s</trd>,
    <trd>-aire</trd>,
    <trd>-er</trd>,
    <trd>-ble</trd>,
    <trd>-al</trd>,
    <trd>-&iacute;s... (sufix adjectivador)</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>-ig</trd>,
    <trd>-ich</trd>,
    <trd>-lich</trd>,
    <trd>-isch</trd>
  </trdgrp>
  <trd lng="en">suffix for <ind>adjectives</ind> in Esperanto</trd>
  <trdgrp lng="es">
    <trd><klr tip="amb">sufijo adjetival</klr></trd>
    <trd>-al/-ar</trd>,
    <trd>-ble</trd>,
    <trd>-ento/-enta</trd>,
    <trd>-il</trd>,
    <trd>-izo/-iza</trd>,
    <trd>-nte</trd>,
    <trd>-osa/-osa</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>-al<klr> (suffixe adjectival)</klr></trd>,
    <trd>-eux<klr> (suffixe adjectival)</klr></trd>,
    <trd>-ique<klr> (suffixe adjectival)</klr></trd>
  </trdgrp>
  <trd lng="fy">efterheaksel foar <ind>eigenskipswurden</ind> yn it
  Esperanto</trd>
  <trdgrp lng="hu">
    <trd>-<ind>i</ind></trd>,
    <trd>-<ind>s</ind></trd>,
    <trd>-<ind>f&eacute;le</ind></trd>
  </trdgrp>
  <trd lng="io">-a</trd>
  <trd lng="it"><klr tip="amb">desinenza (suffisso) che trasforma la
  parola in aggettivo (hom-o: uomo - hom-a: umano)</klr></trd>
  <trd lng="nl">achtervoegsel voor <ind>bijvoeglijke naamwoorden</ind>
  in het Esperanto</trd>
  <trd lng="pl"><klr tip="amb">(ko&nacute;c&oacute;wka
  przymiotnika)</klr></trd>
  <trdgrp lng="pt">
    <trd>-<ind>&aacute;vel</ind></trd>,
    <trd>-<ind>al</ind></trd>
  </trdgrp>
  <trd lng="th">&#3605;&#3633;&#3623;&#3621;&#3591;&#3607;&#3657;&#3634;&#3618;&#3588;&#3635;&#3588;&#3640;&#3603;&#3624;&#3633;&#3614;&#3607;&#3660;</trd>
  <trd lng="vo">-ik</trd>
  <trdgrp lng="ru">
    <trd>-&c_i;&c_j;</trd>,
    <trd>-&c_y;&c_j;</trd>,
    <trd>-&c_o;&c_j;</trd>
  </trdgrp>
</drv>
</art>
