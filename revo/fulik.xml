<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>fulik</rad>/o</kap>
<drv mrk="fulik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="raled.0oj">raledoj</ref>
      (<trd lng="la">Fulica</trd>), kiu vivas apud mar&ccirc;oj:
      <ekz>
        <tld/>oj kaj anasoj sulkis la kanalon fronte sub la
        fenestro<fnt><aut>Istv&aacute;n Ertl</aut>: <vrk>La nekaptebla os'</vrk>,
        <bib>Monato</bib><lok>jaro 1999a, numero 12a, p. 21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="kok.akvo0o">akvokoko</ref>
  </refgrp>
  <trd lng="be">&c_l;&c_y;&c_s;&c_k;&c_a;</trd>
  <trd lng="cs">lyska</trd>
  <trd lng="de">Blessralle</trd>
  <trd lng="en">coot</trd>
  <trd lng="fr">foulque</trd>
  <trd lng="nl">koet</trd>
  <trd lng="hu">sz&aacute;rcsa</trd>
  <trd lng="pl">&lstroke;yska</trd>
  <trdgrp lng="pt">
    <trd>f&#250;lica</trd>,
    <trd>carqueja</trd>,
    <trd>galeir&#227;o</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_y;&c_s;&c_u;&c_h;&c_a;</trd>
</drv>
</art>
