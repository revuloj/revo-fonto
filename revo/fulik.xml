<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fulik.xml,v 1.20 2023/08/31 13:11:32 revo Exp $">
<kap><rad>fulik</rad>/o</kap>
<drv mrk="fulik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="raled.0oj">raledoj</ref>
      (<trd lng="la">Fulica</trd>), kiu vivas apud mar&ccirc;oj:
      <ekz>
        <tld/>oj estas akvaj birdoj sufi&ccirc;e disvasti&gcirc;intaj en E&ubreve;ropo
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2012/010460.php">Subakva spiono</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur Su&hcirc;oj ni kalkulis &ccirc;. 70 cignojn kaj 120 <tld/>ojn
        <fnt>
          <bib>LOdE</bib>
          <aut>Czes&lstroke;aw Baranowski</aut>
          <vrk><url ref="&Tekstaro;?nomo=ondo-de-esperanto">Kalkuli birdojn Kristnaske</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj kaj anasoj sulkis la kanalon fronte sub la fenestro
        <fnt><aut>Istv&aacute;n Ertl</aut>: <vrk>La nekaptebla
        os'</vrk>,
        <bib>Monato</bib><lok>jaro 1999a, numero 12a, p.
        21a</lok></fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/7/74/Eurasian_Coot_on_the_Thames.jpg/320px-Eurasian_Coot_on_the_Thames.jpg" prm="PD"><tld/>o sur Tamiso (Fulica atra)
        <fnt>
          <aut>R H Walpole, Rhw66</aut>
          <lok><url ref="&WCW;/File:Eurasian_Coot_on_the_Thames.jpg">&WCW;/File:Eurasian_Coot_on_the_Thames.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <refgrp tip="super">
    <ref cel="kok.akvo0o">akvokoko</ref>
  </refgrp>
  <ref tip="vid" cel="galinu.0o">galinulo</ref>
  <trd lng="be">&c_l;&c_y;&c_s;&c_k;&c_a;</trd>
  <trd lng="cs">lyska</trd>
  <trdgrp lng="de">
    <trd>Bl&auml;sshuhn</trd>,
    <trd>Blessralle</trd>
  </trdgrp>
  <trd lng="en">coot</trd>
  <trd lng="fr">foulque</trd>
  <trd lng="ja">&#x30AA;&#x30AA;&#x30D0;&#x30F3;</trd>
  <trd lng="nl">koet</trd>
  <trd lng="hu">sz&aacute;rcsa</trd>
  <trd lng="pl">&lstroke;yska</trd>
  <trdgrp lng="pt">
    <trd>f&uacute;lica</trd>,
    <trd>carqueja</trd>,
    <trd>galeir&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_y;&c_s;&c_u;&c_h;&c_a;</trd>
  <trd lng="sk">lyska</trd>
  <trd lng="uk">&#x43B;&#x438;&#x441;&#x43A;&#x430;</trd>
</drv>
</art>
