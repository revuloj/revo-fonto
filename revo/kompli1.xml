<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kompli1.xml,v 1.23 2023/12/03 09:58:54 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>kompliment</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kompli1.0o">
  <kap><tld/>o</kap>
  <snc mrk="kompli1.0o.gratulo">
    <dif>
      Gratulaj flataj vortoj adresataj al iu:
      <ekz>
        fari <tld/>on;
      </ekz>
      <ekz>
        adresi al iu <tld/>on pro lia sukceso;
      </ekz>
      <ekz>
        diri <tld/>ojn al virino;
      </ekz>
      <ekz>
        okaza <tld/>o ne iras al testamento
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Kompliment</trd>,
      <trd>Schmeichelei</trd>
    </trdgrp>
  </snc>
  <snc mrk="kompli1.0o.MOR">
    <dif>
      &Gcirc;entilaj ceremoniaj
      <ref tip="vid" cel="formul.0o.vorta">formuloj</ref>,
      <ref tip="vid" cel="esprim.0o.ajxo">esprimoj</ref>:
      <ekz>
        vi afable tion diras pro <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        manieroj kaj <tld/>oj apartenas al la akcepta
        saluto<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>H&ouml;flichkeitsbezeugung</trd>,
      <trd>Huldigung</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_p;&c_l;&c_ib;&c_m;&c_e;&c_n;&c_t;</trd>
  <trd lng="bg">&c_k;&c_o;&c_m;&c_p;&c_l;&c_i;&c_m;&c_je;&c_n;&c_t;</trd>
  <trd lng="ca">compliment</trd>
  <trdgrp lng="cs">
    <trd>kompliment</trd>,
    <trd>lichotka</trd>,
    <trd>poklona</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kompliment</trd>
  </trdgrp>
  <trd lng="en">compliment</trd>
  <trdgrp lng="fr">
    <trd>compliment<klr tip="ind"> (f&eacute;licitation,
    civilit&eacute;)</klr></trd>,
    <trd>civilit&eacute;<klr tip="ind"> (parole de civilit&eacute;)</klr></trd>,
    <trd>congratulation</trd>,
    <trd>f&eacute;licitation</trd>,
    <trd>louange</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8CDB;&#x8F9E;  <pr>&#x3055;&#x3093;&#x3058;</pr></trd>,
    <trd>&#x304A;&#x4E16;&#x8F9E;  <pr>&#x304A;&#x305B;&#x3058;</pr></trd>,
    <trd>&#x3042;&#x3044;&#x3055;&#x3064;</trd>
  </trdgrp>
  <trd lng="nl">compliment</trd>
  <trdgrp lng="pl">
    <trd>komplement</trd>,
    <trd>pochlebstwo</trd>,
    <trd>duser</trd>
  </trdgrp>   
  <trdgrp lng="pt">
    <trd>cumprimento</trd>,
    <trd>v&ecirc;nia</trd>,
    <trd>lisonja</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>compliment</trd>,
    <trd>flatuire</trd>
  </trdgrp>   
  <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_l;&c_i;&c_m;&c_je;&c_n;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>kompliment</trd>,
    <trd>lich&ocirc;tka</trd>,
    <trd>poklona</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&#x43E;&#x43C;&#x43F;&#x43B;&#x456;&#x43C;&#x435;&#x43D;&#x442;</trd>
</drv>

<drv mrk="kompli1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kompli1.0i.KOMUNE">
    <dif>
      Fari al iu <tld/>on:
      <ekz>
        <tld/>i iun pri lia kura&gcirc;o;
      </ekz>
      <ekz>
        <tld/>u lin komisie de mi<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li estas <tld/>ema kaj flatema.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; &c_k;&c_a;&c_m;&c_p;&c_l;&c_ib;&c_m;&c_e;&c_n;&c_t;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_z;&c_a;&c_c;&c_mol; &c_k;&c_a;&c_m;&c_p;&c_l;&c_ib;&c_m;&c_e;&c_n;&c_t;</trd>
  </trdgrp>
  <trd lng="ca">complimentar</trd>
  <trdgrp lng="cs">
    <trd>dvo&rcaron;it se</trd>,
    <trd>klan&ecaron;t se</trd>,
    <trd>lichotit <klr>(n&ecaron;komu)</klr></trd>,
    <trd>skl&aacute;dat poklony</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Komplimente machen</trd>,
    <trd>schmeicheln</trd>
  </trdgrp>
  <trd lng="en">compliment</trd>
  <trdgrp lng="fr">
    <trd>complimenter</trd>,
    <trd>faire un <ind>compliment</ind> (&agrave;)</trd>,
    <trd>congratuler</trd>,
    <trd>f&eacute;liciter</trd>,
    <trd>louer</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8CDB;&#x8F9E;&#x3092;&#x8FF0;&#x3079;&#x308B;  <pr>&#x3055;&#x3093;&#x3058;&#x3092;&#x306E;&#x3079;&#x308B;</pr></trd>,
    <trd>&#x304A;&#x4E16;&#x8F9E;&#x3092;&#x8A00;&#x3046;  <pr>&#x304A;&#x305B;&#x3058;&#x3092;&#x3044;&#x3046;</pr></trd>,
    <trd>&#x3042;&#x3044;&#x3055;&#x3064;&#x3092;&#x3059;&#x308B;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>complimenteren</trd>,
    <trd>een <ind>compliment</ind> maken</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>komplementowa&cacute;</trd>,
    <trd>pochlebia&cacute;</trd>
  </trdgrp>   
  <trd lng="ro">compliment</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol; &c_k;&c_o;&c_m;&c_p;&c_l;&c_i;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_mol;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>lichoti&tcaron;</trd>,
    <trd>poklonkova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x433;&#x43E;&#x432;&#x43E;&#x440;&#x438;&#x442;&#x438; &#x43A;&#x43E;&#x43C;&#x43F;&#x43B;&#x456;&#x43C;&#x435;&#x43D;&#x442;&#x438;/&#x43B;&#x44E;&#x431;&#x2019;&#x44F;&#x437;&#x43D;&#x43E;&#x441;&#x442;&#x456;</trd>,
    <trd>&#x43B;&#x435;&#x441;&#x442;&#x438;&#x442;&#x438;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x606D;&#x7DAD; <pr>g&#x14D;ngwei</pr></trd>,
    <trd>&#x62DC; <pr>b&#xE0;i</pr></trd>,
    <trd>&#x79F0;&#x8D5E; <pr>ch&#x113;ngz&#xE0;n</pr></trd>,
    <trd>&#x5938;&#x5956; <pr>ku&#x101;ji&#x1CE;ng</pr></trd>,
    <trd>&#x62CD;&#x99AC;&#x5C41; <pr>p&#x101;im&#x1CE;p&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>
