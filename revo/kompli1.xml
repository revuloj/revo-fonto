<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kompli1.xml,v 1.25 2025/09/17 22:27:19 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>kompliment</rad>/o <fnt><bib>JED</bib></fnt>
</kap>

<drv mrk="kompli1.0o">
  <kap><tld/>o</kap>
  <snc mrk="kompli1.0o.MOR">
    <dif>
      <ref tip="malprt" cel="gxenti.0eco.MOR">&Gcirc;entilaj </ref> ceremoniaj
      <ref tip="vid" cel="formul.0o.vorta">formuloj</ref>,
      <ref tip="vid" cel="esprim.0o.ajxo">esprimoj</ref>:
      <ekz>
        vi afable tion diras pro <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        manieroj kaj <tld/>oj apartenas al la akcepta
        saluto<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>H&ouml;flichkeitsbezeugung</trd>,
      <trd>Huldigung</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>compliment<klr tip="ind"> (civilit&eacute;)</klr></trd>,
      <trd>formule de politesse</trd>
    </trdgrp>
  </snc>
  <snc mrk="kompli1.0o.gratulo">
    <dif>
      <ref tip="vid" cel="gratul.0o">Gratulaj</ref> flataj vortoj adresataj al iu:
      <ekz>
        fari <tld/>on;
      </ekz>
      <ekz>
        adresi al iu <tld/>on pro lia sukceso;
      </ekz>
      <ekz>
        diri <tld/>ojn al virino;
      </ekz>
      <ekz>
        okaza <tld/>o ne iras al testamento
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Kompliment</trd>,
      <trd>Schmeichelei</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>congratulation</trd>,
      <trd>f&eacute;licitation</trd>,
      <trd>louange</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_p;&c_l;&c_ib;&c_m;&c_e;&c_n;&c_t;</trd>
  <trd lng="bg">&c_k;&c_o;&c_m;&c_p;&c_l;&c_i;&c_m;&c_je;&c_n;&c_t;</trd>
  <trd lng="ca">compliment</trd>
  <trdgrp lng="cs">
    <trd>kompliment</trd>,
    <trd>lichotka</trd>,
    <trd>poklona</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kompliment</trd>
  </trdgrp>
  <trd lng="en">compliment</trd>
  <trdgrp lng="ja">
    <trd>&#x8CDB;&#x8F9E;  <pr>&#12373;&#12435;&#12376;</pr></trd>,
    <trd>&#x304A;&#x4E16;&#x8F9E;  <pr>&#x304A;&#x305B;&#12376;</pr></trd>,
    <trd>&#12354;&#12356;&#12373;&#12388;</trd>
  </trdgrp>
  <trd lng="nl">compliment</trd>
  <trdgrp lng="pl">
    <trd>komplement</trd>,
    <trd>pochlebstwo</trd>,
    <trd>duser</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>cumprimento</trd>,
    <trd>v&ecirc;nia</trd>,
    <trd>lisonja</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>compliment</trd>,
    <trd>flatuire</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_l;&c_i;&c_m;&c_je;&c_n;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>kompliment</trd>,
    <trd>lich&ocirc;tka</trd>,
    <trd>poklona</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&#x43E;&#x43C;&#x43F;&#x43B;&c_ib;&#x43C;&c_je;&#x43D;&c_t;</trd>
</drv>

<drv mrk="kompli1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kompli1.0i.KOMUNE">
    <dif>
      Fari al iu <tld/>on:
      <ekz>
        <tld/>i iun pri lia kura&gcirc;o;
      </ekz>
      <ekz>
        <tld/>u lin komisie de mi<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li estas <tld/>ema kaj flatema.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; &c_k;&c_a;&c_m;&c_p;&c_l;&c_ib;&c_m;&c_e;&c_n;&c_t;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_z;&c_a;&c_c;&c_mol; &c_k;&c_a;&c_m;&c_p;&c_l;&c_ib;&c_m;&c_e;&c_n;&c_t;</trd>
  </trdgrp>
  <trd lng="ca">complimentar</trd>
  <trdgrp lng="cs">
    <trd>dvo&rcaron;it se</trd>,
    <trd>klan&ecaron;t se</trd>,
    <trd>lichotit <klr>(n&ecaron;komu)</klr></trd>,
    <trd>skl&aacute;dat poklony</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Komplimente machen</trd>,
    <trd>schmeicheln</trd>
  </trdgrp>
  <trd lng="en">compliment</trd>
  <trdgrp lng="fr">
    <trd>complimenter</trd>,
    <trd>faire un <ind>compliment</ind> (&agrave;)</trd>,
    <trd>congratuler</trd>,
    <trd>f&eacute;liciter</trd>,
    <trd>louer</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8CDB;&#x8F9E;&#12434;&#x8FF0;&#12409;&#x308B;  <pr>&#12373;&#12435;&#12376;&#12434;&#x306E;&#12409;&#x308B;</pr></trd>,
    <trd>&#x304A;&#x4E16;&#x8F9E;&#12434;&#x8A00;&#12358;  <pr>&#x304A;&#x305B;&#12376;&#12434;&#12356;&#12358;</pr></trd>,
    <trd>&#12354;&#12356;&#12373;&#12388;&#12434;&#12377;&#x308B;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>complimenteren</trd>,
    <trd>een <ind>compliment</ind> maken</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>komplementowa&cacute;</trd>,
    <trd>pochlebia&cacute;</trd>
  </trdgrp>
  <trd lng="ro">compliment</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol; &c_k;&c_o;&c_m;&c_p;&c_l;&c_i;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_mol;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>lichoti&tcaron;</trd>,
    <trd>poklonkova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&#x43E;&c_v;&#x43E;&c_r;&c_i;&c_t;&c_i;
    &#x43A;&#x43E;&#x43C;&#x43F;&#x43B;&c_ib;&#x43C;&c_je;&#x43D;&c_t;&c_i;/&#x43B;&#x44E;&c_b;&#8217;&#x44F;&c_z;&#x43D;&#x43E;&c_s;&c_t;&c_ib;</trd>,
    <trd>&#x43B;&c_je;&c_s;&c_t;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x606D;&#x7DAD; <pr>g&#x14D;ngwei</pr></trd>,
    <trd>&#x62DC; <pr>b&#xE0;i</pr></trd>,
    <trd>&#x79F0;&#x8D5E; <pr>ch&emacron;ngz&#xE0;n</pr></trd>,
    <trd>&#22840;&#22870; <pr>ku&amacron;ji&#x1CE;ng</pr></trd>,
    <trd>&#x62CD;&#x99AC;&#x5C41; <pr>p&amacron;im&#x1CE;p&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>
