<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kompliment</rad>/o
</kap>
<drv mrk="kompli1.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Gratulaj flataj vortoj adresataj al iu:
      <ekz>fari <tld/>on;</ekz>
      <ekz>adresi al iu <tld/>on pro lia sukceso;</ekz>
      <ekz>diri <tld/>ojn al virino;</ekz>
      <ekz>okaza <tld/>o ne iras al testamento<fnt><bib>PrV</bib></fnt>.</ekz> 
    </dif>
    <trdgrp lng="de">
      <trd>Kompliment</trd>,
      <trd>Schmeichelei</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <dif>
      &Gcirc;entilaj ceremoniaj formuloj:
      <ekz>vi afable tion diras pro <tld/>o<fnt>Z</fnt>;</ekz>
      <ekz>
        manieroj kaj <tld/>oj apartenas al la akcepta saluto<fnt>Z</fnt>.
      </ekz> 
    </dif>
    <trdgrp lng="de">
      <trd>H&ouml;flichkeitsbezeugung</trd>,
      <trd>Huldigung</trd>
    </trdgrp>
  </snc>
  <trd lng="nl">compliment</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_l;&c_i;&c_m;&c_je;&c_n;&c_t;</trd>
</drv>
<drv mrk="kompli1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari al iu <tld/>on 1 kaj 2:
      <ekz><tld/>i iun pri lia kura&gcirc;o;</ekz>
      <ekz><tld/>u lin komisie de mi<fnt>Z</fnt>;</ekz>
      <ekz>li estas <tld/>ema kaj flatema.</ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
      <trd>Komplimente machen</trd>,
      <trd>schmeicheln</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol; &c_k;&c_o;&c_m;&c_p;&c_l;&c_i;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_mol;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
