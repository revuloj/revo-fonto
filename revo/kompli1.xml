<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kompli1.xml,v 1.26 2025/11/20 20:34:23 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>kompliment</rad>/o <fnt><bib>JED</bib></fnt>
</kap>

<drv mrk="kompli1.0o">
  <kap><tld/>o</kap>
  <snc mrk="kompli1.0o.gratulo">
    <dif>
      <ref tip="vid" cel="gratul.0o">Gratulaj</ref>, admiraj, flataj vortoj
      adresataj al iu:
      <ekz>
        fari <tld/>on;
      </ekz>
      <ekz>
        <klr>[ili]</klr> &scirc;utis <tld/>ojn al la a&ubreve;toro
        <fnt>
          <bib>DL</bib>
          <lok>Dua Libro de l' Lingvo Internacia</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;entiluloj renkonti&gcirc;as, <tld/>oj inter&scirc;an&gcirc;i&gcirc;as
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        li diris al &ccirc;iu flatan vorton <klr>[&#x2026;]</klr>, li diris <tld/>ojn al la virinoj
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi diros al vi sen <tld/>oj: la amikeco, kiun mi rimarkas inter vi kaj mia filo,
        sincere &gcirc;ojigas min
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        foje kelkaj <klr>[&#x2026;]</klr> <tld/>oj: la kafaj buletoj lastan mardon estis
        nesupereblaj
        <fnt>
          <bib>OIP</bib>
          <lok>Ombro sur interna pejza&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        disdoni muzikon al viaj amikoj, uzi la verkon en derivaj verkoj <klr>[jen]</klr>
        plej granda <tld/>o por bona artisto
        <fnt>
          <bib>Kon11</bib>
          <lok>Muziko</lok>
        </fnt>;
      </ekz>
      <ekz>
        okaza <tld/>o ne iras al testamento
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Kompliment</trd>,
      <trd>Schmeichelei</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>congratulation</trd>,
      <trd>f&eacute;licitation</trd>,
      <trd>louange</trd>
    </trdgrp>
  </snc>
  <snc mrk="kompli1.0o.MOR">
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="malprt" cel="gxenti.0eco.MOR">&Gcirc;entilaj</ref> ceremoniaj
      <ref tip="vid" cel="formul.0o.vorta">formuloj</ref>,
      <ref tip="vid" cel="esprim.0o.ajxo">esprimoj</ref>:
      <ekz>
        vi afable tion &ccirc;i diras pro <tld/>o
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        manieroj kaj <tld/>oj apartenas al la akcepta saluto
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rivere.0o">riverenco</ref>,
      <ref cel="salut.0o">saluto</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>H&ouml;flichkeitsbezeugung</trd>,
      <trd>Huldigung</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>compliment<klr tip="ind"> (civilit&eacute;)</klr></trd>,
      <trd>formule de politesse</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_p;&c_l;&c_ib;&c_m;&c_e;&c_n;&c_t;</trd>
  <trd lng="bg">&c_k;&c_o;&c_m;&c_p;&c_l;&c_i;&c_m;&c_je;&c_n;&c_t;</trd>
  <trd lng="ca">compliment</trd>
  <trdgrp lng="cs">
    <trd>kompliment</trd>,
    <trd>lichotka</trd>,
    <trd>poklona</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kompliment</trd>
  </trdgrp>
  <trd lng="en">compliment</trd>
  <trdgrp lng="ja">
    <trd>&#x8CDB;&#x8F9E;  <pr>&#x3055;&#x3093;&#x3058;</pr></trd>,
    <trd>&#x304A;&#x4E16;&#x8F9E;  <pr>&#x304A;&#x305B;&#x3058;</pr></trd>,
    <trd>&#x3042;&#x3044;&#x3055;&#x3064;</trd>
  </trdgrp>
  <trd lng="nl">compliment</trd>
  <trdgrp lng="pl">
    <trd>komplement</trd>,
    <trd>pochlebstwo</trd>,
    <trd>duser</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>cumprimento</trd>,
    <trd>v&ecirc;nia</trd>,
    <trd>lisonja</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>compliment</trd>,
    <trd>flatuire</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_p;&c_l;&c_i;&c_m;&c_je;&c_n;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>kompliment</trd>,
    <trd>lich&ocirc;tka</trd>,
    <trd>poklona</trd>
  </trdgrp>
  <trd lng="uk">&c_k;&c_o;&c_m;&c_p;&c_l;&c_ib;&c_m;&c_je;&c_n;&c_t;</trd>
</drv>

<drv mrk="kompli1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kompli1.0i.KOMUNE">
    <dif>
      Fari al iu <tld/>on:
      <ekz>
        &ccirc;iuj homoj <tld/>as min pro mia admirinda filino
        <fnt>
          <bib>StU</bib>
          <lok>Britujo</lok>
        </fnt>;
      </ekz>
      <ekz>
        Grafo <nom>von Schwalbach</nom> <klr>[&#x2026;]</klr> kore <tld/>is Vilfridon pro lia lerta,
        sentplena ludado
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        trovi ekvilibron inter sincera esprimo de dankemo, e&ccirc; por malgranda helpo, kaj
        <tld/>ema sinteno
        <fnt>
          <bib>Kon11</bib>
          <lok>[La Konsiloj de la oficisto]</lok>
        </fnt>;
      </ekz>
      <ekz>
        permesu, ke mi <tld/>u vin pro via ago
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> <tld/>is &scirc;ian prudenton
        <fnt>
          <bib>ViV</bib>
          <lok>II.</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ofte <tld/>as min mem pro mia sa&gcirc;eco
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flat.0i.iu">flati</ref>,
      <ref cel="gratul.0i">gratuli</ref>,
      <ref cel="lauxd.0i">la&ubreve;di</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; &c_k;&c_a;&c_m;&c_p;&c_l;&c_ib;&c_m;&c_e;&c_n;&c_t;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_z;&c_a;&c_c;&c_mol; &c_k;&c_a;&c_m;&c_p;&c_l;&c_ib;&c_m;&c_e;&c_n;&c_t;</trd>
  </trdgrp>
  <trd lng="ca">complimentar</trd>
  <trdgrp lng="cs">
    <trd>dvo&rcaron;it se</trd>,
    <trd>klan&ecaron;t se</trd>,
    <trd>lichotit <klr>(n&ecaron;komu)</klr></trd>,
    <trd>skl&aacute;dat poklony</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Komplimente machen</trd>,
    <trd>schmeicheln</trd>
  </trdgrp>
  <trd lng="en">compliment</trd>
  <trdgrp lng="fr">
    <trd>complimenter</trd>,
    <trd>faire un <ind>compliment</ind> (&agrave;)</trd>,
    <trd>congratuler</trd>,
    <trd>f&eacute;liciter</trd>,
    <trd>louer</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8CDB;&#x8F9E;&#x3092;&#x8FF0;&#x3079;&#x308B;  <pr>&#x3055;&#x3093;&#x3058;&#x3092;&#x306E;&#x3079;&#x308B;</pr></trd>,
    <trd>&#x304A;&#x4E16;&#x8F9E;&#x3092;&#x8A00;&#x3046;  <pr>&#x304A;&#x305B;&#x3058;&#x3092;&#x3044;&#x3046;</pr></trd>,
    <trd>&#x3042;&#x3044;&#x3055;&#x3064;&#x3092;&#x3059;&#x308B;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>complimenteren</trd>,
    <trd>een <ind>compliment</ind> maken</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>komplementowa&cacute;</trd>,
    <trd>pochlebia&cacute;</trd>
  </trdgrp>
  <trd lng="ro">compliment</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol; &c_k;&c_o;&c_m;&c_p;&c_l;&c_i;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_mol;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>lichoti&tcaron;</trd>,
    <trd>poklonkova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_o;&c_v;&c_o;&c_r;&c_i;&c_t;&c_i;
    &c_k;&c_o;&c_m;&c_p;&c_l;&c_ib;&c_m;&c_je;&c_n;&c_t;&c_i;/&c_l;&c_ju;&c_b;&#x2019;&c_ja;&c_z;&c_n;&c_o;&c_s;&c_t;&c_ib;</trd>,
    <trd>&c_l;&c_je;&c_s;&c_t;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x606D;&#x7DAD; <pr>g&omacron;ngwei</pr></trd>,
    <trd>&#x62DC; <pr>b&agrave;i</pr></trd>,
    <trd>&#x79F0;&#x8D5E; <pr>ch&emacron;ngz&agrave;n</pr></trd>,
    <trd>&#x5938;&#x5956; <pr>ku&amacron;ji&#x1CE;ng</pr></trd>,
    <trd>&#x62CD;&#x99AC;&#x5C41; <pr>p&amacron;im&#x1CE;p&igrave;</pr></trd>
  </trdgrp>
</drv>
</art>
