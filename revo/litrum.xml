<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>litrum</rad>/o
</kap>
<drv mrk="litrum.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="litrum.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Lythrum</trd>).
    </dif>
  </snc>
  <bld lok="../bld/litrum.jpg">
    <ind>sanga <tld/>o</ind>
    <trd lng="la">Lythrum salicaria</trd>
  </bld>
  <trd lng="de">Weiderich</trd>
</drv>
<drv mrk="litrum.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Lythraceae</trd>), kreskantaj ofte &ccirc;e akvo.
    </dif>
  </snc>
  <trd lng="de">Weiderichgew&auml;chse</trd>
</drv>
</art>
