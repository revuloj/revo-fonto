<kap>
  <rad>litrum</rad>/o
</kap>
<drv mrk="litrum.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="litrum1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Lythrum</trd>).
    </dif>
  </snc>
  <bld lok="../bld/litrum.jpg">
    <ind>sanga <tld/>o</ind>
    <trd lng="la">Lythrum salicaria</trd>
  </bld>
  <trd lng="de">Weiderich</trd>
  <trd lng="nl">kattestaart<klr tip="ind"> (plant)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_r;&c_b;&c_je;&c_n;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_k;&c_u;&c_n; <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  </trdgrp>
</drv>
