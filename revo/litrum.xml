<drv mrk="litrum.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="litrum1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Lythrum</trd>):
      <ekz>
        <tld/>o estas <klr>[&#x2026;]</klr> herbo, foje kun ligneca bazo, 
        kun purpuraj, rozaj a&ubreve; blankaj floroj, aran&gcirc;itaj en verticiloj,
        kiuj formas &scirc;ajnspikojn, la frukto estas multsema kapsulo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=531105">Litrumo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="&WCU;/thumb/e/e0/Photos_13.jpg/640px-Photos_13.jpg" prm="CC BY-SA 4.0"><ind>sanga <tld/>o</ind> &ccirc;e lago

       <fnt>
         <aut>Havin hp, propra verko</aut>
         <lok><url ref="&WCW;/File:Photos_13.jpg">&WCW;/File:Photos_13.jpg</url></lok>
       </fnt>
    <trd lng="de">Blutweiderich</trd>
    <trd lng="la">Lythrum salicaria</trd>
  </bld>
  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_o;&c_j;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_ch;&c_a;&c_l;&c_mol;&c_ch;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">g&egrave;nere Lythrum</trd>
  <trd lng="de">Weiderich</trd>
  <trd lng="fa">&a_kh;&a_w;&a_n;&zwnj;&a_f;&a_A;&a_m;</trd>
  <trd lng="fr">salicaire</trd>
  <trd lng="hu">f&uuml;z&eacute;ny</trd>
  <trd lng="nl">kattenstaart<klr tip="ind"> (plant)</klr></trd>
  <trd lng="pl">krwawnica  <klr>(ro&sacute;l.)</klr></trd>
  <trd lng="pt">litr&aacute;cea</trd>
  <trd lng="ro">r&abreve;chitan</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_r;&c_b;&c_je;&c_n;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_k;&c_u;&c_n; <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x43B;&#x430;&#x43A;&#x443;&#x43D;</trd>
