<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kvark</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="kvark.0o">
  <kap><tld/>o</kap>
  <snc mrk="kvark.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Hipotezaj fundamentaj partikloj formantaj &ccirc;iujn hadronojn
      (barjonoj konsistas el tri <tld/>oj, mezonoj el <tld/>o kaj
      kontra&ubreve;<tld/>o).
    </dif>
  </snc>
  <ref tip="super" cel="partik2.0o">partiklo</ref>
  <refgrp tip="malprt">
    <ref cel="hadron.0o">hadrono</ref>,
    <ref cel="barjon.0o">barjono</ref>,
    <ref cel="nukleon.0o">nukleono</ref>,
    <ref cel="proton.0o">protono</ref>,
    <ref cel="neuxtr2.0o">ne&ubreve;trono</ref>,
    <ref cel="mezon.0o">mezono</ref>
  </refgrp>
  <trd lng="be">&c_k;&c_v;&c_a;&c_r;&c_k;</trd>
  <trd lng="de">Quark</trd>
  <trd lng="en">quark</trd>
  <trd lng="es">quark</trd>
  <trd lng="fr">quark</trd>
  <trd lng="hu">kvark</trd>
  <trd lng="nl">quark</trd>
  <trd lng="pt">quark</trd>
  <trd lng="ru">&c_k;&c_v;&c_a;&c_r;&c_k;</trd>
</drv>
</art>
