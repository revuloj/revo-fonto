<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bombar.xml,v 1.20 2021/03/05 15:48:37 revo Exp $">
  <kap>
    <ofc>1</ofc>
    <rad>bombard</rad>/i <fnt><bib>PV</bib></fnt>
  </kap>
  <drv mrk="bombar.0i">
    <kap><tld/>i</kap>
    <gra><vspec>tr</vspec></gra>
    <snc mrk="bombar.0i.MIL">
      <uzo tip="fak">MIL</uzo>
      <dif>
        Da&ubreve;re kanoni ion.
      </dif>
      <trd lng="de">beschie&szlig;en</trd>
      <trdgrp lng="it">
        <trd>cannoneggiare</trd>,
        <trd>bersagliare</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>bombardowa&cacute;</trd>,
        <trd>prowadzi&cacute; <ind>ostrza&lstroke;</ind></trd>
      </trdgrp>
    </snc>
    <snc mrk="bombar.0i.FIG">
      <uzo tip="stl">FIG</uzo>
      <dif>
        Ripete senpaciencige direkti postulojn al iu:
        <ekz><tld/>i iun per leteroj;</ekz>
        <ekz>
          sub la konstanta <tld/>ado de tiuj personoj, kiuj postulis
          &scirc;an&gcirc;ojn, mi fine cedis<fnt>Z</fnt>.
        </ekz>
      </dif>
      <trdgrp lng="it">
        <trd>bombardare<klr> (fig.)</klr></trd>,
        <trd>bersagliare</trd>
      </trdgrp>
      <trd lng="pl">bombardowa&cacute;</trd>
    </snc>
  <trd lng="be">&c_b;&c_a;&c_m;&c_b;&c_a;&c_r;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>bombardovat</trd>,
    <trd>ost&#x159;elovat</trd>
  </trdgrp>
  <trd lng="de">bombardieren</trd>
  <trd lng="en">bombard</trd>
  <trd lng="es">bombardear</trd>
  <trd lng="fr">bombarder</trd>
  <trd lng="he">&#x5DC;&#x5B0;&#x5D4;&#x5B7;&#x5E4;&#x5B0;&#x5E6;&#x5B4;&#x5D9;&#x5E5;</trd>
  <trd lng="ru">&c_b;&c_o;&c_m;&c_b;&c_a;&c_r;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>bombardova&tcaron;</trd>,
    <trd>ostre&#x13E;ova&#x165;</trd>
  </trdgrp>
</drv>
</art>
