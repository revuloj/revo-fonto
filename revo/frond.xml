  <trd lng="de">Wedel</trd>
  <trd lng="fr">fronde<klr tip="ind"> (de foug&egrave;re)</klr></trd>
  <trd lng="hu">lomb</trd>
  <trdgrp lng="ja">
    <trd>&#33865;&#29366;&#20307;  <pr>&#12424;&#12358;&#12376;&#12423;&#12358;&#12383;&#12356;</pr></trd>,
    <trd>&#33865;  <pr>&#12424;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="pt">fronde</trd>
  <trdgrp lng="sk">
    <trd>hal&uacute;zka</trd>,
    <trd>vetvi&ccaron;ka</trd>,
    <trd>zelen&aacute; ratoles&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_a;&c_j;&#1103;</trd>,
    <trd>&c_g;&c_ib;&#1083;&#1082;&c_a; (&#1087;&c_a;&#1087;&#1086;&c_r;&#1086;&c_t;&c_ib; &c_a;&c_b;&#1086;
    &#1087;&c_a;&#1083;&#1100;&#1084;&c_i;)</trd>
  </trdgrp>
</drv>
