<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Siri</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="siri.0o">
  <kap><tld/>o</kap>
  <snc mrk="siri.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      &Scirc;tato situanta en
      okcidenta <ref tip="malprt" cel="azi.A0o">Azio</ref>
      kaj limanta okcidente al
      <ref tip="vid" cel="mediterane.0o">Mediteraneo</ref> kaj
      <ref tip="vid" cel="liban.0o">Libano</ref>, norde al
      <ref tip="vid" cel="turk.T0ujo">Turkujo</ref>, oriente
      al <ref tip="vid" cel="irak.0o">Irako</ref>, sude al la
      <ref tip="vid" cel="jordan.0o">Jordanio</ref>,
      sud-okcidente al <ref tip="vid" cel="israel.0o">Israelo</ref>.
      Damasko estas la regurbo kaj plej granda urbo.
    </dif>
  </snc>
  <trd lng="en">Syria</trd>
  <trd lng="fr">Syrie</trd>
  <trd lng="he">&samekh;&vav;&resh;&yod;&he;</trd>
  <trd lng="hu">Sz&iacute;ria</trd>
  <trd lng="nl">Syri&euml;</trd>
  <trd lng="pl">Syria</trd>
  <trd lng="ru">&c_S;&c_i;&c_r;&c_i;&c_ja;</trd>
</drv>

<drv mrk="siri.s0a">
  <kap><tld lit="s"/>a</kap>
  <snc>
    <dif>
      Rilata al <tld lit="s"/>anoj, ilia nacieco a&ubreve; &scirc;tato.
    </dif>
  </snc>
  <trd lng="en">Syrian</trd>
  <trd lng="fr">syrien<klr> (adj.)</klr></trd>
  <trd lng="he">&samekh;&vav;&resh;&yod;</trd>
  <trd lng="hu">sz&iacute;riai</trd>
  <trd lng="nl">Syrisch</trd>
  <trd lng="pl">syryjski</trd>
  <trd lng="ru">&c_s;&c_i;&c_r;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="siri.s0ano">
  <kap><tld lit="s"/>ano</kap>
  <snc>
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="siri.0o"><tld/>o</ref>.
    </dif>
  </snc>
  <trd lng="en">Syrian</trd>
  <trd lng="fr">syrien<klr> (subst.)</klr></trd>
  <trd lng="he">&samekh;&vav;&resh;&yod;</trd>
  <trd lng="hu">sz&iacute;riai <klr>(lakos)</klr></trd>
  <trd lng="nl">Syri&euml;r</trd>
  <trd lng="pl">Syryjczyk</trd>
  <trd lng="ru">&c_s;&c_i;&c_r;&c_i;&c_je;&c_c;</trd>
</drv>

</art>

