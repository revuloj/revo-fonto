<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>karg</rad>/o
</kap>
<drv mrk="karg.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      &Scirc;ar&gcirc;o de varoj sur &scirc;ipo:
      <ekz>
        la <tld/>a prezo estas difinita en &ccirc;arto subskribita de &scirc;ipmastro
        kaj de la <tld/>anto;
      </ekz>
      <ekz>
        la &scirc;ipkapitano estas responda pri la <tld/>o.
      </ekz>
      <refgrp tip="sin">
        <ref cel="frajt.sxip0ajxo">&scirc;ipfrajta&jcirc;o</ref>
      </refgrp>
    </dif>
    <trdgrp lng="de">
      <trd>Ladung</trd>,
      <trd>Schiffsfracht</trd>,
      <trd>Frachtgut</trd>,
      <trd>Transportgut</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>cargo</trd>,
      <trd>freight</trd>,
      <trd>lading</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>cargaison</trd>,
      <trd>fret<klr> (cargaison)</klr></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>vracht</trd>,
      <trd>lading</trd>,
      <trd>goederen</trd>
    </trdgrp>
    <trd lng="ru">&c_g;&c_r;&c_u;&c_z;</trd>
  </snc>
</drv>

<drv mrk="karg.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">KOME</uzo>
  <snc>
    <dif>
      Lui &scirc;ipon por &scirc;ar&gcirc;i &gcirc;in per ekspedotaj
      varoj.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>fr&eacute;ter<klr> (prendre en location)</klr></trd>
  </trdgrp>
  <trd lng="nl">bevrachten</trd>
</drv>

<drv mrk="karg.0anto">
  <kap><tld/>anto</kap>
  <snc>
    <dif>
      Tiu, kiu <tld/>as.
    </dif>
    <trdgrp lng="en">
      <trd>consignor</trd>,
      <trd>shipper</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>affr&eacute;teur</trd>
    </trdgrp>
    <trd lng="nl">cargadoor</trd>
  </snc>
</drv>

<drv mrk="karg.0boato">
  <kap><tld/>boato</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Speciala <ref tip="super" cel="boat.0o">boato</ref> por transporti varojn.
    </dif>
    <trdgrp lng="en">
      <trd>freight boat</trd>,
      <trd>freighter</trd>
    </trdgrp>
    <trd lng="de">Frachtschiff</trd>
    <trd lng="nl">vrachtschip</trd>
  </snc>
</drv>

<drv mrk="karg.sensxargxa0o">
  <kap>sen&scirc;ar&gcirc;a <tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Mono pagita por ne&scirc;ar&gcirc;itaj varoj.
    </dif>
  </snc>
  <trd lng="de">Fracht</trd>
</drv>
</art>
