<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>karg</rad>/o
</kap>
<drv mrk="karg.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">TRA</uzo>
    <dif>
      &Scirc;ar&gcirc;o de varoj en &scirc;ipo, trajno, vagono, ajna transporta
      veturilo:
      <ekz>
        la <tld/>a prezo estas difinita en &ccirc;arto subskribita de
        &scirc;ipmastro
        kaj de la <tld/>anto;
      </ekz>
      <ekz>
        la &scirc;ipkapitano estas responda pri la <tld/>o;
      </ekz>
      <ekz>
        oni arestis 600 protestantojn, kiuj <klr>...</klr> sin katenis
        al
        la fervojaj reloj por haltigi la trajnon kun nukleo-ruba&jcirc;a
        <tld/>o<fnt><aut>Rudolf Fischer</aut>: <vrk>Nia ribelo
        sukcesis</vrk>, <bib>Monato</bib><lok>jaro 1998a, numero 5a, p.
        9a</lok></fnt>.
      </ekz>
      </dif>
      <refgrp tip="sin">
        <ref cel="frajt.0ajxo">frajta&jcirc;o</ref>
      </refgrp>
    <trd lng="be">&c_g;&c_r;&c_u;&c_z;</trd>
    <trd lng="ca">c&agrave;rrega</trd>
    <trdgrp lng="de">
      <trd>Ladung</trd>,
      <trd>Schiffsfracht</trd>,
      <trd>Frachtgut</trd>,
      <trd>Transportgut</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>cargo</trd>,
      <trd>freight</trd>,
      <trd>lading</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>cargaison</trd>,
      <trd>fret<klr tip="ind"> (cargaison)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>rakom&aacute;ny</trd>,
      <trd>karg&oacute;</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>vracht</trd>,
      <trd>lading</trd>,
      <trd>goederen</trd>
    </trdgrp>
    <trd lng="ru">&c_g;&c_r;&c_u;&c_z;</trd>
  </snc>
  <trd lng="sk">lodný náklad</trd>
</drv>

<drv mrk="karg.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
  <uzo tip="fak">TRA</uzo>
    <dif>
      &Scirc;ar&gcirc;i transportujon per <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_r;&c_a;&c_h;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">noliejar</trd>
  <trd lng="hu">sz&aacute;ll&iacute;t&oacute;eszk&ouml;zt <ind>b&eacute;rel</ind></trd>
  <trd lng="nl">bevrachten</trd>
  <trd lng="ru">&c_f;&c_r;&c_a;&c_h;&c_t;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">naložiť tovar na loď</trd>
</drv>

<drv mrk="karg.0anto">
  <kap><tld/>anto</kap>
  <snc>
    <dif>
      Tiu, kiu <tld/>as.
    </dif>
    <trdgrp lng="be">
      <trd>&c_f;&c_r;&c_a;&c_h;&c_t;&c_a;&c_v;&c_a;&c_c;&c_je;&c_l;&c_mol;</trd>,
      <trd>&c_g;&c_r;&c_u;&c_z;&c_a;&c_a;&c_d;&c_p;&c_r;&c_a;&c_w;&c_sh;&c_ch;&c_y;&c_k;</trd>
    </trdgrp>
    <trd lng="ca">noliejador</trd>
    <trdgrp lng="en">
      <trd>consignor</trd>,
      <trd>shipper</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>haj&oacute;b&eacute;rl&odblac;</trd>,
      <trd>sz&aacute;ll&iacute;t&oacute;eszk&ouml;z-b&eacute;rl&odblac;</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>scheepsbevrachter</trd>,
      <trd>cargadoor</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_f;&c_r;&c_a;&c_h;&c_t;&c_o;&c_v;&c_shch;&c_i;&c_k;</trd>,
      <trd>&c_f;&c_r;&c_a;&c_h;&c_t;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
      <trd>&c_g;&c_r;&c_u;&c_z;&c_o;&c_o;&c_t;&c_p;&c_r;&c_a;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="sk">nájomník nákladnej lode</trd>
</drv>

<drv mrk="karg.0boato">
  <kap><tld/>boato</kap>
  <snc>
  <uzo tip="fak">MAR</uzo>
    <dif>
      Speciala <ref tip="super" cel="boat.0o">boato</ref> por transporti
      varojn.
    </dif>
   </snc>
  <trd lng="be">&c_f;&c_r;&c_a;&c_h;&c_t;&c_a;&c_v;&c_a;&c_je; &c_s;&c_u;&c_d;&c_n;&c_a;</trd>
  <trd lng="ca">vaixell de c&agrave;rrega</trd>
  <trd lng="de">Frachtschiff</trd>
  <trdgrp lng="en">
      <trd>freight boat</trd>,
      <trd>freighter</trd>
    </trdgrp>
  <trd lng="fr">cargo<klr tip="ind"> (navire)</klr></trd>
  <trd lng="hu">teherhaj&oacute;</trd>
  <trd lng="nl">vrachtschip</trd>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_z;&c_o;&c_v;&c_o;&c_je; &c_s;&c_u;&c_d;&c_n;&c_o;</trd>
  <trd lng="sk">nákladný čln</trd>
</drv>

<drv mrk="karg.sensxargxa0o">
  <kap>sen&scirc;ar&gcirc;a <tld/>o</kap>
  <snc>
  <uzo tip="fak">EKON</uzo>
    <dif>
      Mono pagita por ne&scirc;ar&gcirc;itaj varoj.
    </dif>
  </snc>
  <trd lng="de">Fracht</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_r;&c_a;&c_h;&c_t;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_t;&c_a; &c_z;&c_a; &c_p;&c_r;&c_o;&c_v;&c_o;&c_z;</trd>
  </trdgrp>
</drv>
</art>
