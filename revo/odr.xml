<kap>
  <rad>Odr</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="odr.O0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" cel="river.0o" lst="voko:riveroj">Rivero</ref>, kun fonto en
      <ref tip="malprt" cel="cxehx.CX0ujo">&Ccirc;e&hcirc;ujo</ref>,
      parte fluanta inter <ref tip="malprt" cel="pol.P0ujo">Polujo</ref>
      kaj <ref tip="malprt" cel="german1.G0ujo">Germanujo</ref>
      kiel landlimo, enfluanta en la
      <ref cel="mar.BaltaM0o">Baltan Maron</ref>:
      <ekz>
        la historie signifa limo <tld/>o-Niso ne plu ekzistas: la germana kanceliero 
        <nom>Angela Merkel</nom> &gcirc;in solene malaperigis
        <fnt>
          <bib>Monato</bib>
          <aut>Karlo Juh&aacute;sz</aut>
          <vrk><url ref="&Monato;/2008/009423.php">Historia momento</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ja">&#x30AA;&#x30FC;&#x30C7;&#x30EB;&#x5DDD;  <pr>&#x30AA;&#x30FC;&#x30C7;&#x30EB;&#x304B;&#x308F;</pr></trd>
  <trd lng="lat">Viadrus</trd>
  <trdgrp lng="be">
    <trd>&c_O;&c_d;&c_r;&c_a;</trd>,
    <trd>&c_O;&c_d;&c_e;&c_r;</trd>
  </trdgrp>
  <trd lng="cs">Odra</trd>
  <trd lng="de">Oder</trd>
  <trd lng="fr">Oder</trd>
  <trd lng="hu">Odera</trd>
  <trd lng="nl">Oder</trd>
  <trd lng="pl">Odra</trd>
  <trdgrp lng="pt">
    <trd>&Oacute;der</trd>,
    <trd>Odra</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_O;&c_d;&c_r;&c_a;</trd>,
    <trd>&c_O;&c_d;&c_je;&c_r;</trd>
  </trdgrp>
  <trd lng="sk">Odra</trd>
  <trdgrp lng="uk">
    <trd>&#x41E;&#x434;&#x440;&#x430;</trd>,
    <trd>&#x41E;&#x434;&#x435;&#x440;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5967;&#x5F97;&#x6CB3; <pr>&#xE0;od&#xE9;h&#xE9;</pr></trd>,
    <trd>&#x5965;&#x5F97;&#x6CB3; <pr>&#xE0;od&#x11B;ih&#xE9;</pr></trd>,
    <trd>&#x5967;&#x5F97;&#x6CB3; <pr>&#xE0;od&#x11B;ih&#xE9;</pr></trd>
  </trdgrp>
</drv>
