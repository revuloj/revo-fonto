<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Odr</rad>/o
</kap>
<drv mrk="odr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Rivero, parte fluanta inter Polujo kaj Germanujo kiel landlimo,
      enfluanta en la Baltan Maron.
    </dif>
  </snc>
  <trd lng="de">Oder</trd>
  <trd lng="lat">Viadrus</trd>
  <trd lng="nl">Oder</trd>
  <trd lng="pl">Odra</trd>
  <trdgrp lng="ru">
    <trd>&c_O;&c_d;&c_r;&c_a;</trd>,
    <trd>&c_O;&c_d;&c_je;&c_r;</trd>
  </trdgrp>
</drv>
</art>
