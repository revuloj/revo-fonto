<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: odr.xml,v 1.16 2019/06/30 18:21:48 revo Exp $">
<kap>
  <rad>Odr</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="odr.O0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" cel="river.0o"
      lst="voko:riveroj">Rivero</ref>, kun fonto en
      <ref tip="malprt" cel="cxehx.CX0ujo">&Ccirc;e&hcirc;ujo</ref>,
      parte fluanta inter <ref tip="malprt" cel="pol.P0ujo">Polujo</ref>
      kaj <ref tip="malprt" cel="german1.G0ujo">Germanujo</ref>
      kiel landlimo, enfluanta en la
      <ref cel="mar.BaltaM0o">Baltan Maron</ref>:
      <ekz>
        la historie signifa limo <tld/>o-Niso ne plu ekzistas: la germana kanceliero 
        <nom>Angela Merkel</nom> &gcirc;in solene malaperigis
        <fnt>
          <bib>Monato</bib>
          <aut>Karlo Juh&aacute;sz</aut>
          <vrk><url ref="&Monato;/2008/009423.php">Historia momento</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="lat">Viadrus</trd>
  <trdgrp lng="be">
    <trd>&c_O;&c_d;&c_r;&c_a;</trd>,
    <trd>&c_O;&c_d;&c_e;&c_r;</trd>
  </trdgrp>
  <trd lng="cs">Odra</trd>
  <trd lng="de">Oder</trd>
  <trd lng="fr">Oder</trd>
  <trd lng="hu">Odera</trd>
  <trd lng="nl">Oder</trd>
  <trd lng="pl">Odra</trd>
  <trdgrp lng="pt">
    <trd>&Oacute;der</trd>,
    <trd>Odra</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_O;&c_d;&c_r;&c_a;</trd>,
    <trd>&c_O;&c_d;&c_je;&c_r;</trd>
  </trdgrp>
  <trd lng="sk">Odra</trd>
</drv>
</art>
