<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Odr</rad>/o
</kap>
<drv mrk="odr.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      <ref tip="super" cel="river.0o">Rivero</ref>, kun fonto en
      <ref tip="malprt" cel="cxehx.CX0ujo">&Ccirc;e&hcirc;ujo</ref>,
      parte fluanta inter <ref tip="malprt" cel="pol.P0ujo">Polujo</ref>
      kaj <ref tip="malprt" cel="german1.G0ujo">Germanujo</ref>
      kiel landlimo, enfluanta en la
      <ref cel="mar.BaltaM0o">Baltan Maron</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_O;&c_d;&c_r;&c_a;</trd>,
    <trd>&c_O;&c_d;&c_e;&c_r;</trd>
  </trdgrp>
  <trd lng="de">Oder</trd>
  <trd lng="fr">Oder</trd>
  <trd lng="hu">Odera</trd>
  <trd lng="lat">Viadrus</trd>
  <trd lng="nl">Oder</trd>
  <trd lng="pl">Odra</trd>
  <trdgrp lng="pt">
    <trd>&Oacute;der</trd>,
    <trd>Odra</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_O;&c_d;&c_r;&c_a;</trd>,
    <trd>&c_O;&c_d;&c_je;&c_r;</trd>
  </trdgrp>
</drv>
</art>
