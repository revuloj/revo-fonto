  <trd lng="ro">meioz&abreve;</trd>
  <trd lng="ru">&c_m;&c_je;&c_j;&c_o;&c_z;</trd>
  <trd lng="sk">mei&oacute;za</trd>
  <trd lng="sl">mejoza</trd>
