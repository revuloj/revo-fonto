  <trd lng="lt">mejoz&#279;</trd>
  <trd lng="mk">&c_m;&c_je;&#1112;&c_o;&c_z;&c_a;</trd>
  <trd lng="ms">meiosis</trd>
  <trd lng="na">meiosis</trd>
  <trd lng="nl">meiose</trd>
  <trd lng="no">meiose</trd>
