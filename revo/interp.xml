<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>interpelaci</rad>/o, interpelac/o
  <fnt>Z</fnt>
</kap>
<drv mrk="interp.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Publika demando farita de deputato en parlamento al ministro por
      ricevi klarigojn pri la agoj de la registaro. 
    </dif>
  </snc>
  <trd lng="de">Anfrage</trd>
  <trd lng="nl">interpellatie</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_p;&c_r;&c_o;&c_s; <klr>(&c_v; &c_p;&c_a;&c_r;&c_l;&c_a;&c_m;&c_je;&c_n;&c_t;&c_je;)</klr></trd>,
    <trd>&c_i;&c_n;&c_t;&c_je;&c_r;&c_p;&c_je;&c_l;&c_l;&c_ja;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>
<drv mrk="interp.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Direkti <tld/>on al ministro.  
    </dif>
  </snc>
  <trd lng="de">anfragen</trd>
  <trd lng="nl">interpelleren</trd>
</drv>
</art>
