<art mrk="$Id: pentagram.xml,v 1.6 2022/03/25 12:53:16 revo Exp $">
<kap>
  <rad>pentagram</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="pentagram.0o">
  <kap><tld/>o</kap>
  <snc mrk="pentagram.0o.MAG">
