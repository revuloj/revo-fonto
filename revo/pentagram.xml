<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pentagram.xml,v 1.8 2024/02/17 06:43:05 revo Exp $">
<kap>
  <rad>pentagram</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="pentagram.0o">
  <kap><tld/>o</kap>
  <snc mrk="pentagram.0o.MAG">
    <dif>
      Figuro de kvinpinta stelo farita per unu sin interkrucanta
      streko, tradicia magia simbolo:
      <ekz>
        li <klr>...</klr> provis per longa flava ungo
        kretan linion de <tld/>o
        <fnt>
          <aut>S. Lukjanenko, trad. S. Andrejsons</aut>,
          <vrk><url ref="http://hemulo.narod.ru/mi_ne_rapidas.html">
          Mi ne rapidas</url></vrk>,
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">pentagrama</trd>
  <trdgrp lng="cs">
    <trd>pentagram</trd>,
    <trd>pravideln&yacute; hv&ecaron;zdicov&yacute; p&ecaron;ti&uacute;heln&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>pentacle</trd>,
    <trd>pentagram</trd>
  </trdgrp>
  <trd lng="fr">pentagramme</trd>
  <trd lng="pl">pentagram</trd>
  <trd lng="ro">pentagram&abreve;</trd>
  <trd lng="sk">pentagram</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_je;&c_n;&c_t;&c_a;&c_g;&c_r;&c_a;&c_m;&c_a;</trd>,
    <trd>&c_p;&#x2019;&c_ja;&c_t;&c_i;&c_k;&c_u;&c_t;&c_n;&c_a; &c_z;&c_ib;&c_r;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#x4E94;&#x89D2;&#x661F; <pr>w&#x1D4;ji&#x1CE;ox&#x12B;ng</pr></trd>
</drv>
</art>

