<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>grob</rad>/a <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="grob.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="grob.0a.MOR">
    <dif>
      <refgrp tip="dif">
        <ref cel="gxenti.mal0a.MOR">Mal&gcirc;entila</ref>,
        <ref tip="dif" cel="krud.0a.MOR">kruda<sncref/></ref>:
      </refgrp>
      <ekz>
        nun mi estos <tld/>a kun vi pro la &scirc;ajno, kiu &jcirc;us venas 
        malanta&ubreve; vi, tiu estas la &ccirc;efrabisto 
        <fnt>
          <aut>J. Rejt&odblac;, trad. L. Bal&aacute;zs</aut>,
          <vrk>La tri korpogardistoj en Afriko</vrk>,
          <lok>[vidita en 2008]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>grob</trd>,
    <trd>krude</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fruste</trd>,
    <trd>goujat</trd>
  </trdgrp>
</drv>

<drv mrk="grob.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="grob.0ajxo.MOR">
    <dif>
      <ref tip="dif" cel="krud.0ajxo.maldelikata">Kruda&jcirc;o</ref>:
      <ekz>
        Marie-Claude <klr>...</klr> estas sanktulo por elporti
        la <tld/>a&jcirc;ojn de tiu frenezulo
        <fnt><aut>E. de Zilah</aut>, <vrk>Vivi sur barko</vrk>, 
        <lok>1994</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Grobheit</trd>
  <trdgrp lng="fr">
    <trd>goujaterie</trd>,
    <trd>incongruit&eacute;</trd>
  </trdgrp>
</drv>
</art>

