<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&ccirc;i&ccirc;eron</rad>/o
</kap>
<drv mrk="cxicxe.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Gvidisto por fremduloj:
      <ekz>
        <tld/>o gvidis nin dum nia vizito al la kastelo kaj &gcirc;ia bela
        intermura &gcirc;ardeno<fnt><aut>W. H. Simcock</aut>: <vrk>Vizito al la
        nordaj insuloj</vrk>, <bib>Monato</bib><lok>jaro 1999a, numero 9a,
        p. 18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_v;&c_a;&c_d;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_g;&c_ib;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Fremdenf&uuml;hrer</trd>,
    <trd>Reisef&uuml;hrer</trd>,
    <trd>Reiseleiter</trd>
  </trdgrp>
  <trd lng="el">&xi;&epsilon;&ny;&alfa;&gamma;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">docent</trd>
  <trd lng="fr">guide <klr tip="ind">(accompagnateur)</klr></trd>
  <trd lng="hu">idegenvezet&odblac;</trd>
  <trd lng="nl">gids</trd>
  <trd lng="pt">cicerone</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_ch;&c_i;&c_ch;&c_je;&c_r;&c_o;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="sv">guide</trd>
</drv>
</art>
