<kap>
  <rad>gerlen</rad>/o
</kap>
<drv mrk="gerlen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="sxnur.0ego">&Scirc;nurego</ref> konsistanta el kelkaj <ref tip="prt"
      cel="hauxser.0o">ha&ubreve;seroj</ref>:
      <ekz>
        ili brue min primokis, svingante per ha&ubreve;ser-peco: <klr>[&#8230;]</klr> tiom da <tld/>o
        ne sufi&ccirc;os &gcirc;is pinto de via longa nazo
        <fnt>
          <vrk><url
          ref="http://corpora.uni-leipzig.de/de/res?corpusId=epo_mixed_2012&amp;word=gerleno">Judaj
          ritoj kaj kredoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_o;&c_s;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_a;&c_t;</trd>
  </trdgrp>
  <trd lng="bg">&c_b;&c_u;&c_k;&c_s;&c_i;&c_r;&c_n;&c_o; &c_v;&c_malmol;&c_zh;&c_je;</trd>
  <trd lng="cs">t&aacute;hlo</trd>
  <trdgrp lng="de">
    <trd>Greling</trd>,
    <trd>Ankerseil</trd>,
    <trd>kleines Kabeltau</trd>
  </trdgrp>
  <trd lng="en">hawser</trd>
  <trd lng="fr">grelin</trd>
  <trdgrp lng="hu">
    <trd>haj&oacute;k&ouml;t&eacute;l</trd>,
    <trd>kik&ouml;t&odblac;k&ouml;t&eacute;l</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12418;&#12420;&#12356;&#x7DB1;  <pr>&#12418;&#12420;&#12356;&#12389;&#x306A;</pr></trd>,
    <trd>&#x592A;&#x7DB1;  <pr>&#12405;&#12392;&#12375;&#12388;&#x306A;</pr></trd>
  </trdgrp>
  <trd lng="nl">tros<klr tip="ind"> (zeewezen)</klr></trd>
  <trdgrp lng="pt">
   <trd>calabre</trd>,
   <trd>amarra</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_o;&c_j;&c_n;&c_o;&c_j; &c_k;&c_a;&c_n;&c_a;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>pr&iacute;vesn&yacute;</trd>,
    <trd>tiahlo</trd>,
    <trd>z&aacute;vesn&yacute; povraz</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x43E;&c_t;&c_r;&c_ib;&c_j;&#x43D;&c_i;&c_j; &#x43A;&c_a;&#x43D;&c_a;&c_t;</trd>
