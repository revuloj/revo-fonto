<art mrk="$Id: gerlen.xml,v 1.14 2022/03/19 16:47:06 revo Exp $">
<kap>
  <rad>gerlen</rad>/o
</kap>
<drv mrk="gerlen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="sxnur.0ego">&Scirc;nurego</ref> konsistanta el kelkaj <ref tip="prt" cel="hauxser.0o">ha&ubreve;seroj</ref>:
      <ekz>
        ili brue min primokis, svingante per ha&ubreve;ser-peco: <klr>[&#x2026;]</klr> tiom da <tld/>o
        ne sufi&ccirc;os &gcirc;is pinto de via longa nazo
        <fnt>
          <vrk><url ref="http://corpora.uni-leipzig.de/de/res?corpusId=epo_mixed_2012&amp;word=gerleno">Judaj ritoj kaj kredoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_b;&c_u;&c_k;&c_s;&c_i;&c_r;&c_n;&c_o; &c_v;&c_malmol;&c_zh;&c_je;</trd>
  <trd lng="cs">t&aacute;hlo</trd>
  <trdgrp lng="de">
    <trd>Greling</trd>,
    <trd>Ankerseil</trd>,
    <trd>kleines Kabeltau</trd>
  </trdgrp>
  <trd lng="en">hawser</trd>
  <trd lng="fr">grelin</trd>
  <trdgrp lng="hu">
    <trd>haj&oacute;k&ouml;t&eacute;l</trd>,
    <trd>kik&ouml;t&odblac;k&ouml;t&eacute;l</trd>
  </trdgrp>
  <trd lng="nl">tros<klr tip="ind"> (zeewezen)</klr></trd>
  <trdgrp lng="pt">
   <trd>calabre</trd>, 
   <trd>amarra</trd> 
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_o;&c_j;&c_n;&c_o;&c_j; &c_k;&c_a;&c_n;&c_a;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>pr&iacute;vesn&yacute;</trd>,
    <trd>tiahlo</trd>,
    <trd>z&aacute;vesn&yacute; povraz</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x43E;&#x442;&#x440;&#x456;&#x439;&#x43D;&#x438;&#x439; &#x43A;&#x430;&#x43D;&#x430;&#x442;</trd>
</drv>
</art>
