<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ergot.xml,v 1.17 2022/03/17 13:47:19 revo Exp $">
<kap>
  <rad>ergot</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ergot.0o">
  <kap><tld/>o</kap>
  <snc mrk="ergot.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      La venena skleroto de <tld/>fungo kiu kreskas sur kelkaj
      gramenacoj
      kaj malsanigas ilin. 
    </dif>
    <trd lng="de">Mutterkorn</trd>
    <trd lng="fr">ergot du seigle</trd>
    <trd lng="hu">anyarozs</trd>
    <trdgrp lng="pt">
      <trd>cravagem</trd>,
      <trd>espor&atilde;o</trd>
    </trdgrp>
    <trd lng="pl">sporysz</trd>
    <trd lng="ru">&c_s;&c_p;&c_o;&c_r;&c_y;&c_n;&c_mol;&c_ja;</trd>
  </snc>
  <snc mrk="ergot.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Pinta ungo kreskanta malanta&ubreve; la piedo &ccirc;e kelkaj
      animaloj:
      <ekz>
        koka <tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Sporn</trd>,
      <trd>Afterklaue</trd>
    </trdgrp>
    <trd lng="fr">ergot</trd>
    <trd lng="hu">sarkanty&uacute;<klr tip="ind"> (pl. kakas&eacute;)</klr></trd>
    <trd lng="pt">espor&atilde;o</trd>
    <trd lng="ru">&c_sh;&c_p;&c_o;&c_r;&c_a; <klr>(&c_n;&c_a;&c_p;&c_r;. &c_u; &c_p;&c_je;&c_t;&c_u;&c_h;&c_a;)</klr></trd>
  </snc>
  <trd lng="cs">pta&#x10D;&#xED; ostruha</trd>
  <trd lng="en">ergot</trd>
  <trd lng="sk">vt&#xE1;&#x10D;ia ostroha</trd>
  <trdgrp lng="uk">
    <trd>&#x440;&#x456;&#x436;&#x43E;&#x43A;</trd>,
    <trd>&#x448;&#x43F;&#x43E;&#x440;&#x430; (&#x443; &#x43F;&#x456;&#x432;&#x43D;&#x44F;)</trd>
  </trdgrp>
</drv>
<drv mrk="ergot.0fungo">
  <kap>
    <tld/>fungo
  </kap>
  <snc mrk="ergot.0fungo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Parazita <ref tip="lst" cel="fung.0o.BIO" lst="voko:fungoj">askofungo</ref>,
      kiu ka&ubreve;zas gravan,
      sklerotan malsanon de pluraj poacoj i.a. sekalo;
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:botanikaj_specioj">specio</ref> 
      <trd lng="la">Claviceps purpurea</trd>.
    </dif>
    <trdgrp lng="de">
      <trd>Roter <ind>Keulenkopf</ind></trd>,
      <trd>Hahnensporn</trd>,
      <trd>Purpurroter <ind>Hahnenpilz</ind></trd>
    </trdgrp>
    <trd lng="la">clavis purpurea</trd>
    <trd lng="pl">bu&lstroke;awinka czerwona</trd>
  </snc>
  <trd lng="uk">&#x440;&#x456;&#x436;&#x43A;&#x438;</trd>
</drv>
</art>
