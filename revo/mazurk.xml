<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>mazurk</rad>/o
</kap>
<drv mrk="mazurk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="mazurk.0o.danco">
    <dif>
      Para <ref tip="super" cel="danc.0o">danco</ref> pol-devena,
      tri-takta kun dekses-mezuraj frazoj,
      populara en E&ubreve;ropo kaj Nord-ameriko precipe
      en la 18a kaj 19a jarcentoj.
    </dif>
  </snc>
  <snc mrk="mazurk.0o.muzikajxo">
    <dif>
      Muzika&jcirc;o ta&ubreve;ga por <tld/>o-danco.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Mazurka</trd>,
    <trd>Masurka</trd>
  </trdgrp>
  <trd lng="en">mazurka</trd>
  <trdgrp lng="fr">
    <trd>mazurka</trd>
  </trdgrp>
  <trd lng="nl">mazurka</trd>
  <trd lng="ru">&c_m;&c_a;&c_z;&c_u;&c_r;&c_k;&c_a;</trd>
  <trdgrp lng="sv">
    <trd>mazurka</trd>,
    <trd>masurka</trd>
  </trdgrp>
</drv>
</art>
