<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: brank.xml,v 1.29 2023/10/29 19:11:03 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>brank</rad>/o <fnt><bib>PV</bib></fnt>
    </kap>

 <drv mrk="brank.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="brank.0o.ZOO">
     <uzo tip="fak">ZOO</uzo>
     <dif>
       Spirorgano de bestoj vivantaj en akvo, ekzemple de
       fi&scirc;oj,
       krustuloj, moluskoj:
       <ekz>
         fi&scirc;o <klr>[&#x2026;]</klr> devas spiri aeron sorbitan en
         akvo, per helpo de plene formitaj <tld/>oj
         <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro
         14a</lok></fnt>;
       </ekz>
      <ekz>
        la homa ontogeneza evoluo <klr>[&#x2026;]</klr> ripetas la pli fruajn evoluajn
        stadiojn, do reproduktas en diversaj etapoj embriajn formojn de iuj pratipaj
        organoj (<tld/>oj, vosteto, aran&gcirc;o de pluraj muskoloj, arterioj, k.t.p.)
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        trenitaj el la profunda&jcirc;oj merlangoj glime fajreras,
        spasme baraktas, ser&ccirc;ante akvon <tld/>e batante, mortas
        <fnt>
          <aut>Garbhan Macaoidh</aut>:
          <vrk>La &Scirc;etlanda</vrk>. <bib>Monato</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_zh;&c_a;&c_b;&c_r;&c_y;</trd>,
      <trd>&c_zh;&c_e;&c_b;&c_r;&c_y;</trd>,
      <trd>&c_z;&c_ja;&c_b;&c_r;&c_y;</trd>,
      <trd>&c_sh;&c_ch;&c_e;&c_l;&c_je;&c_p;&c_y;</trd>
    </trdgrp>
    <trd lng="br">brenk</trd>
    <trd lng="cs">&zcaron;&aacute;bry</trd>
    <trd lng="de">Kiemen</trd>
    <trd lng="es">branquia</trd>
    <trd lng="en">gill</trd>
    <trd lng="fr">branchie</trd>
    <trd lng="he">&zayin;&yod;&fmem;</trd>
    <trd lng="hu">kopolty&uacute;</trd>
    <trd lng="pl">skrzele</trd>
    <trd lng="pt">br&acirc;nquia</trd>
    <trd lng="ru">&c_zh;&c_a;&c_b;&c_r;&c_y;</trd>
    <trdgrp lng="zh">
    <trd>&#x9C13; <pr>s&#x101;i</pr></trd>,
    <trd>&#x816E; <pr>s&#x101;i</pr></trd>
  </trdgrp>
</snc>
  <trd lng="cs">&zcaron;&aacute;bra</trd>
  <trd lng="ja">&#x9C13;  <pr>&#x3042;&#x304E;&#x3068;</pr></trd>
  <trd lng="sk">&zcaron;iabre</trd>
  <trd lng="uk">&c_z;&c_ja;&c_b;&c_r;&c_a;</trd>
</drv>

    <drv mrk="brank.0okovrilo">
      <kap><tld/>okovrilo,
        <var>
          <kap><tld/>oklapo</kap>
        </var>
      </kap>
      <snc mrk="brank.0okovrilo.ZOO">
        <uzo tip="fak">ZOO</uzo>
        <dif>
          Ha&ubreve;tfalto kaj eventuale la ostoj kiuj de ekstere kovras
          la
          <tld/>ofendojn &ccirc;e plimulto da fi&scirc;oj kaj larvoj de
          amfibioj.
        </dif>
        <trd lng="be">&c_sh;&c_ch;&c_e;&c_l;&c_je;&c_p;&c_n;&c_a;&c_ja; &c_k;&c_r;&c_y;&c_sh;&c_k;&c_a;</trd>
      <trd lng="cs">sk&rcaron;ele</trd>
      <trd lng="de">Kiemendeckel</trd>
      <trd lng="la">operculum</trd>
      <trd lng="pl">pokrywa skrzelowa</trd>
      <trd lng="ru">&c_zh;&c_a;&c_b;&c_je;&c_r;&c_n;&c_a;&c_ja;
      &c_k;&c_r;&c_y;&c_sh;&c_k;&c_a;</trd>
      </snc>
    </drv>

  </art>
