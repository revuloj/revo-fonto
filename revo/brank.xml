<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>brank</rad>/o <fnt><bib>PV</bib></fnt>
    </kap>

    <drv mrk="brank.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="brank.0o.ZOO">
        <uzo tip="fak">ZOO</uzo>
        <dif>
          Spirorgano de bestoj vivantaj en akvo, ekzemple de
          fi&scirc;oj,
          krustuloj, moluskoj:
          <ekz>
            fi&scirc;o <klr>...</klr> devas spiri aeron sorbitan en
            akvo, per
            helpo de plene formitaj <tld/>oj
            <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro
            14a</lok></fnt>;
          </ekz><ekz>
            trenitaj el la profunda&jcirc;oj merlangoj glime fajreras,
            spasme baraktas, sercante akvon <tld/>e batante, mortas
            <fnt>
              <aut>Garbhan MACAOIDH</aut>:
              <vrk>La &Scirc;etlanda</vrk>. <bib>Monato</bib>
            </fnt>.
          </ekz>
        </dif>
      <trdgrp lng="be">
        <trd>&c_zh;&c_a;&c_b;&c_r;&c_y;</trd>,
        <trd>&c_zh;&c_e;&c_b;&c_r;&c_y;</trd>,
        <trd>&c_z;&c_ja;&c_b;&c_r;&c_y;</trd>
      </trdgrp>
      <trd lng="br">brenk</trd>
      <trd lng="cs">&zcaron;&aacute;bry</trd>
      <trd lng="de">Kiemen</trd>
      <trd lng="es">branquia</trd>
      <trd lng="fr">branchie</trd>
      <trd lng="he">&zayin;&yod;&fmem;</trd>
      <trd lng="hu">kopolty&uacute;</trd>
      <trd lng="pl">skrzele</trd>
      <trd lng="ru">&c_zh;&c_a;&c_b;&c_r;&c_y;</trd>
      </snc>
    </drv>

    <drv mrk="brank.faden0uloj">
      <kap>faden<tld/>uloj</kap>
      <snc mrk="brank.faden0uloj.ZOO">
        <uzo tip="fak">ZOO</uzo>
        <dif>
          Ordo el la klaso <ref tip="super"
          cel="valv.du0uloj">duvalvuloj</ref>
          (<trd lng="la">Filibranchia</trd>).
        </dif>
      <trd lng="de">Fadenkiemer</trd>
      <trd lng="fr">filibranches</trd>
      <trd lng="hu">fon&aacute;lkopolty&uacute;sok</trd>
      <trd
      lng="ru">&c_n;&c_i;&c_t;&c_je;&c_zh;&c_a;&c_b;&c_je;&c_r;&c_n;&c_y;&c_je;</trd>
      </snc>
    </drv>

    <drv mrk="brank.0okovrilo">
      <kap><tld/>okovrilo,
        <var>
          <kap><tld/>oklapo</kap>
        </var>
      </kap>
      <snc>
        <uzo tip="fak">ZOO</uzo>
        <dif>
          Ha&ubreve;tfalto kaj eventuale la ostoj kiuj de ekstere kovras
          la
          <tld/>ofendojn &ccirc;e plimulto da fi&scirc;oj kaj larvoj de
          amfibioj.
        </dif>
      <trd lng="cs">sk&rcaron;ele</trd>
      <trd lng="la">operculum</trd>
      <trd lng="pl">pokrywa skrzelowa</trd>
      <trd lng="ru">&c_zh;&c_a;&c_b;&c_je;&c_r;&c_n;&c_a;&c_ja;
      &c_k;&c_r;&c_y;&c_sh;&c_k;&c_a;</trd>
      </snc>
    </drv>

  </art>
