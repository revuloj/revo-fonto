<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>brank</rad>/o
</kap>
<drv mrk="brank.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Spirorgano de bestoj vivantaj en akvo, ekzemple de fi&scirc;oj,
      krustuloj, moluskoj.
    </dif>
  </snc>
  <trd lng="de">Kiemen</trd>
  <trd lng="es">branquea</trd>
  <trd lng="fr">branchie</trd>
  <trd lng="he">&zayin;&yod;&fmem;</trd>
  <trd lng="hu">kopolty&uacute;</trd>
  <trd lng="ru">&c_zh;&c_a;&c_b;&c_r;&c_y;</trd>
</drv>

<drv mrk="brank.faden0uloj">
  <kap>
    faden<tld/>uloj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="valv.du0uloj">duvalvuloj</ref>
      (<trd lng="la">Filibranchia</trd>).
    </dif>
  </snc>
  <trd lng="de">Fadenkiemer</trd>
  <trd lng="fr">filibranches</trd>
  <trd lng="hu">fon&aacute;lkopolty&uacute;sok</trd>
</drv>
</art>
