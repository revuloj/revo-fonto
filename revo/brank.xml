<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>brank</rad>/o
</kap>
<drv mrk="brank.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Spirorgano de bestoj vivantaj en akvo, ekz. de fi&scirc;oj,
      krustuloj, moluskoj.
    </dif>
  </snc>
  <trd lng="de">Kiemen</trd>
  <trd lng="ru">&c_zh;&c_a;&c_b;&c_r;&c_y;</trd>
</drv>
<drv mrk="brank.faden0uloj">
  <kap>faden<tld/>uloj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="valv.du0uloj">duvalvuloj</ref>
      (<trd lng="la">Filibranchia</trd>).
    </dif>
  </snc>
  <trd lng="de">Fadenkiemer</trd>
</drv>
</art>
