<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>eminent</rad>/a
</kap>
<drv mrk="eminen1.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Supera, distinginda pro sia rango, merito, talento a&ubreve; scio:
      <ekz>
        <tld/>a oratoro, profesoro, sciencisto;
      </ekz>
      <ekz>
        <tld/>e inteligenta.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rang">unuaranga</ref>,
      <ref cel="fam.0a">fama</ref>,
      <ref cel="glor.0a">glora</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>bedeutend</trd>,
      <trd>hoch</trd>,
      <trd>erhaben</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="klr">(io)</uzo>
    <dif>
      Supera, distinginda pro sia altkvaliteco:
      <ekz>
        <tld/>a servo, afero<fnt>Z</fnt>,
      </ekz>
      <ekz>
        rolo, rango, kapablo;
      </ekz>
      <ekz>
        <tld/>a instruiteco;
      </ekz>
      <ekz>
        <tld/>e ta&ubreve;ga, grava.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="star.el0a">elstara</ref>,
      <ref cel="rimark">rimarkinda</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>hervorragend</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_d;&c_a;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_v;&c_y;&c_b;&c_ib;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_v;&c_u;&c_t;&c_y;</trd>,
    <trd>&c_z;&c_n;&c_a;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_n;&c_a;&c_k;&c_a;&c_m;&c_ib;&c_t;&c_y;</trd>
  </trdgrp>
  <trd lng="en">eminent</trd>
  <trd lng="es">eminente</trd>
  <trd lng="fr">&eacute;minent</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_d;&c_a;&c_ju;&c_shch;&c_i;&c_j;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_z;&c_n;&c_a;&c_m;&c_je;&c_n;&c_i;&c_t;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>eminent</trd>,
    <trd>ausgezeichnet</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wybitny</trd>,
    <trd>znakomity</trd>
  </trdgrp>
  <trd lng="sv">framst&aring;ende</trd>
</drv>

<drv mrk="eminen1.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Persono <tld/>a.
    </dif>
    <refgrp tip="vid">
      <ref cel="lum.0o">lumo 4</ref>,
      <ref cel="korife.0o">korifeo</ref>,
      <ref cel="hero.0o">heroo</ref>.
    </refgrp>
  </snc>
<trdgrp lng="be">
 <trd>&c_z;&c_n;&c_a;&c_k;&c_a;&c_m;&c_ib;&c_t;&c_a;&c_s;&c_mol;&c_c;
 &c_mol;</trd>,
 <trd>&c_s;&c_l;&c_a;&c_v;&c_u;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
 <trd>&c_z;&c_o;&c_r;&c_k;&c_a; <klr tip="amb">(&c_ch;&c_a;&c_l;&c_a;
&c_v;&c_je;&c_k;)</klr></trd>
</trdgrp>
  <trd lng="ca">emin&egrave;ncia<klr> (persona eminent)</klr></trd>
  <trdgrp lng="de">
    <trd>eminente <ind>Pers&ouml;nlichkeit</ind></trd>,
    <trd>hervorragende <ind>Pers&ouml;nlichkeit</ind></trd>,
    <trd>prominente <ind>Pers&ouml;nlichkeit</ind></trd>,
    <trd>wichtige <ind>Pers&ouml;nlichkeit</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>V.I.P.</trd>,
    <trd>star</trd>,
    <trd>celebrity</trd>
  </trdgrp>
  <trd lng="es">eminencia</trd>
  <trdgrp lng="fr">
    <trd>c&eacute;l&eacute;brit&eacute;</trd>,
    <trd>personnalit&eacute;<klr tip="ind"> (personne &eacute;minente)
    </klr></trd>,
    <trd>sommit&eacute;</trd>,
    <trd>vedette</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>osobisto&sacute;&cacute; <ind>wybitna</ind></trd>,
    <trd>znakomito&sacute;&cacute;<klr> (osoba)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_d;&c_a;&c_ju;&c_shch;&c_a;&c_ja;&c_s;&c_ja; &c_l;
&c_i;&c_ch;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_n;&c_a;&c_m;&c_je;&c_n;&c_i;&c_t;&c_o;&c_s;&c_t;&c_mol;
    </trd>
  </trdgrp>
</drv>

<drv mrk="eminen1.0ularo">
  <kap><tld/>ularo</kap>
  <snc>
    <dif>
      Tuto de la <tld/>uloj.
    </dif>
    <refgrp tip="vid">
      <ref cel="elit.0o">elito</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>hochgestellter <ind>Personenkreis</ind></trd>,
    <trd>Personen mit <ind>Rang und Namen</ind></trd>,
    <trd>wichtige <ind>Pers&ouml;nlichkeiten</ind></trd>
  </trdgrp>
  <trd lng="fr">gratin<klr tip="ind"> (les personnalit&eacute;s)</klr>
  </trd>
  <trdgrp lng="pl">
    <trd>elita</trd>,
    <trd>osobisto&sacute;ci</trd>,
    <trd>znakomito&sacute;ci<klr> (osoby)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_i;&c_v;&c_k;&c_i; &c_o;&c_b;&c_shch;&c_je;&c_s;
&c_t;&c_v;&c_a;</trd>,
    <trd>&c_e;&c_l;&c_i;&c_t;&c_a;</trd>
  </trdgrp>
</drv>
</art>
