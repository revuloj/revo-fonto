<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: eminen1.xml,v 1.32 2022/03/22 09:30:56 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>eminent</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="eminen1.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="eminen1.0a.iu">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      <ref tip="super" cel="super.0a">Supera</ref>, <ref tip="super"
      cel="distin.0inda">distinginda</ref>
      pro sia rango, merito, talento a&ubreve; scio:
      <ekz>
        <tld/>a oratoro, profesoro, sciencisto;
      </ekz>
      <ekz>
        la plej <tld/>a en la procesio estis altranga ekleziulo
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Naomi</nom> havis parencon de &scirc;ia edzo, viron <tld/>an
        <fnt>
          <bib>MT</bib>
          <lok>Rut 2:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu el la plej <tld/>aj re&gcirc;aj princoj
        <fnt>
          <bib>MT</bib>
          <lok>Ester 6:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi neniel malsuperas la plej <tld/>ajn apostolojn
        <fnt>
          <bib>NT</bib>
          <lok>II. Korintanoj 11:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sanktulino de lia hejmo kaj de lia infaneco, la nobla dukino de Turingio, la
        alte <tld/>a sinjorino, mem iradis en la plej malri&ccirc;ajn lokojn, kaj
        alportadis al la malsanuloj konsolon
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Kandid</nom> havas multe da legantoj <klr>[&#8230;]</klr>, e&ccirc; plej <tld/>aj literaturistoj
        relegas &gcirc;in pro plezuro kaj distro
        <fnt>
          <bib>Kandid</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj junaj artistoj deklamis poemojn
        <fnt>
          <bib>EeP</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a &scirc;uldanto &dash; malbona paganto
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fam.0a">fama</ref>,
      <ref cel="glor.0a">glora</ref>,
      <ref cel="renom.0a">renoma</ref>,
      <ref cel="rang">unuaranga</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>erhaben</trd>,
      <trd>hoch</trd>
    </trdgrp>
    <trd lng="nl">eminent</trd>
  </snc>
  <snc mrk="eminen1.0a.io">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Supera, distinginda pro sia altkvaliteco,
      <ref tip="vid" cel="star.el0a">elstara</ref>,
      <ref tip="vid" cel="rimark.0inda.KOMUNE">rimarkinda</ref>:
      <ekz>
        <tld/>a pozicio
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a deveno
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        viaj meritoj ne estas ver&scirc;ajne <tld/>aj
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i tiu lingve, stile kaj kritike <tld/>a revuo estis dum longaj jaroj la publika
        debatejo pri literaturaj problemoj
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> bran&ccirc;on de alta cedro <klr>[&#8230;]</klr> Mi plantos &gcirc;in sur monto
        alta kaj <tld/>a
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 17:22</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>hervorragend</trd>
    </trdgrp>
    <trd lng="nl">voortreffelijk</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_d;&c_a;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_v;&c_y;&c_b;&c_ib;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_v;&c_u;&c_t;&c_y;</trd>,
    <trd>&c_z;&c_n;&c_a;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_n;&c_a;&c_k;&c_a;&c_m;&c_ib;&c_t;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dokonal&yacute;</trd>,
    <trd>eminentn&iacute;</trd>,
    <trd>mistrn&yacute;</trd>,
    <trd>ohromn&yacute;</trd>,
    <trd>perfektn&iacute;</trd>,
    <trd>vynikaj&iacute;c&iacute;</trd>,
    <trd>v&yacute;znamn&yacute;</trd>,
    <trd>znamenit&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>eminent</trd>,
    <trd>gehoben</trd>,
    <trd>ausgezeichnet</trd>,
    <trd>bedeutend</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>eminent</trd>,
    <trd>distinguished</trd>,
    <trd>illustrious</trd>,
    <trd>noteworthy</trd>,
    <trd>outstanding</trd>,
    <trd>prominent</trd>
  </trdgrp>
  <trd lng="es">eminente</trd>
  <trd lng="fr">&eacute;minent</trd>
  <trdgrp lng="hu">
    <trd>kiv&aacute;l&oacute;</trd>,
    <trd>kiemelked&odblac;</trd>
    <trd>kit&udblac;n&odblac;</trd>,
    <trd>jeles</trd>
    <trd>eminens</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wybitny</trd>,
    <trd>znakomity</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>eminente</trd>,
    <trd>consp&iacute;cuo</trd>,
    <trd>distinto</trd>,
    <trd>alto</trd>,
    <trd>not&aacute;vel</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_d;&c_a;&c_ju;&c_shch;&c_i;&c_j;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_z;&c_n;&c_a;&c_m;&c_je;&c_n;&c_i;&c_t;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>eminentn&yacute;</trd>,
    <trd>vynikaj&uacute;ci</trd>,
    <trd>v&yacute;born&yacute;</trd>,
    <trd>znamenit&yacute;</trd>
  </trdgrp>
  <trd lng="sv">framst&aring;ende</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_d;&c_a;&c_t;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_i;&c_z;&#x43D;&c_a;&c_ch;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_z;&#x43D;&c_a;&#x43C;&c_je;&#x43D;&c_i;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_z;&#x43D;&c_a;&c_t;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="eminen1.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="eminen1.0ulo.KOMUNE">
    <dif>
      Persono <tld/>a:
      <ekz>
        po unu viro el &ccirc;iu tribo de iliaj patroj sendu, &ccirc;iu estu <tld/>ulo inter ili
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 13:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        esprimis sub mia adreso sian favoron por la afero Esperanto <klr>[&#8230;]</klr> al
        multaj diversaj sciencaj <tld/>uloj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista en Boulogne sur Mer en la 5a de a&ubreve;gusto
          1905</vrk>
        </fnt>;
      </ekz>
      <ekz>
        pri multaj el niaj frujaraj eminentuloj mankas al ni biografiaj informoj
        <fnt>
          <bib>KPr</bib>
          <lok>Anta&ubreve;vorto</lok>
        </fnt>;
      </ekz>
      <ekz>
        iuj personoj estas <tld/>uloj en &ccirc;iuj okazoj, aliaj estas <tld/>uloj nur en
        difinitaj medioj
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la oratoro devas precipe turni sin al la <tld/>uloj en la plej signifaj partoj
        de sia parolado
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        diru tie al &ccirc;iuj diversaj <tld/>uloj, senatanoj kaj admiraloj, ke jen via
        Ekscelenco, a&ubreve; Princa Mo&scirc;to, en tiu kaj tiu urbo lo&gcirc;as <nom>Pjotr Ivanovi&ccirc;
        Bob&ccirc;inskij</nom>
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="eminen.0o">eminenco</ref>,
      <ref cel="grav.0ulo">gravulo</ref>,
      <ref cel="korife.0o">korifeo</ref>,
      <ref cel="mosxt.0o">mo&scirc;to</ref>,
      <ref cel="hero.0o">heroo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
  <trd>&c_z;&c_n;&c_a;&c_k;&c_a;&c_m;&c_ib;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
  <trd>&c_s;&c_l;&c_a;&c_v;&c_u;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
  <trd>&c_z;&c_o;&c_r;&c_k;&c_a; <klr tip="amb">(&c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;)</klr></trd>
 </trdgrp>
  <trd lng="ca">emin&egrave;ncia<klr> (persona eminent)</klr></trd>
  <trdgrp lng="cs">
    <trd>kapacita</trd>,
    <trd>vynikaj&iacute;c&iacute; osobnost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>eminente <ind>Pers&ouml;nlichkeit</ind></trd>,
    <trd><ind>hervorragend</ind>e Pers&ouml;nlichkeit</trd>,
    <trd><ind>prominent</ind>e Pers&ouml;nlichkeit</trd>,
    <trd><ind>wichtig</ind>e Pers&ouml;nlichkeit</trd>
  </trdgrp>
  <trdgrp lng="en">
  <trd>dignitary</trd>, 
    <trd>V.I.P.</trd>,
    <trd>star</trd>,
    <trd>celebrity</trd>
  </trdgrp>
  <trd lng="es">eminencia</trd>
  <trdgrp lng="fr">
    <trd>c&eacute;l&eacute;brit&eacute;</trd>,
    <trd>personnalit&eacute;<klr tip="ind"> (personne &eacute;minente)
    </klr></trd>,
    <trd>sommit&eacute;</trd>,
    <trd>vedette</trd>
  </trdgrp>
  <trd lng="he">&alef;&yod;&shin; &dalet;&gimel;&vav;&lamed;</trd>
  <trdgrp lng="hu">
    <trd>kiv&aacute;l&oacute;s&aacute;g</trd>,
    <trd>kiemelked&odblac; egy&eacute;nis&eacute;g</trd>,
    <trd>h&iacute;ress&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vooraanstaand persoon</trd>,
    <trd>notabele</trd>,
    <trd>VIP</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>osobisto&sacute;&cacute; <ind>wybitna</ind></trd>,
    <trd>znakomito&sacute;&cacute;<klr> (osoba)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>personagem <klr tip="amb">(not&aacute;vel)</klr></trd>,
    <trd>notabilidade</trd>,
    <trd>pessoa not&aacute;vel</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_d;&c_a;&c_ju;&c_shch;&c_a;&c_ja;&c_s;&c_ja; &c_l;&c_i;&c_ch;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_n;&c_a;&c_m;&c_je;&c_n;&c_i;&c_t;&c_o;&c_s;&c_t;&c_mol;
    </trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vynikaj&uacute;ci</trd>,
    <trd>v&yacute;znamn&yacute; &ccaron;lovek</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_d;&c_a;&c_t;&#x43D;&c_a; &#x43E;&c_s;&#x43E;&c_b;&c_a;</trd>,
    <trd>&c_z;&#x43D;&c_a;&#x43C;&c_je;&#x43D;&c_i;&c_t;&c_ib;&c_s;&c_t;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="eminen1.0ularo">
  <kap><tld/>ularo</kap>
  <snc mrk="eminen1.0ularo.KOMUNE">
    <dif>
      Tuto de la <tld/>uloj,
      <ref tip="vid" cel="elit.0o">elito</ref>:
      <ekz>
        jen ni <tld/>ularo de l&#8217; mondo mar&scirc;as per firma, samritma pa&scirc;o 
        <klr>[&#8230;]</klr> al ia-ajna makabra ideo
        <fnt>
          <bib>SkandalJozef</bib>
          <lok>Korpogvardia veterano</lok>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_n;&c_a;&c_k;&c_a;&c_m;&c_ib;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_e;&c_l;&c_ib;&c_t;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>elita</trd>,
    <trd>spole&ccaron;nost vynikaj&iacute;c&iacute;ch osobnost&iacute;</trd>,
    <trd>v&yacute;kv&ecaron;t</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>hochgestellter <ind>Personenkreis</ind></trd>,
    <trd>Personen mit <ind>Rang und Namen</ind></trd>,
    <trd>wichtige <ind>Pers&ouml;nlichkeiten</ind></trd>
  </trdgrp>
  <trd lng="en">elite</trd>
  <trd lng="fr">gratin<klr tip="ind"> (les personnalit&eacute;s)</klr>
  </trd>
  <trd lng="he">&mem;&ayin;&mem;&dalet; &gimel;&bet;&vav;&he;</trd>
  <trdgrp lng="hu">
    <trd>el&odblac;kel&odblac; t&aacute;rsas&aacute;g</trd>,
    <trd>kiv&aacute;l&oacute;s&aacute;gok</trd>,
    <trd>elit</trd>
  </trdgrp>
  <trd lng="nl">elite</trd>
  <trdgrp lng="pl">
    <trd>elita</trd>,
    <trd>osobisto&sacute;ci</trd>,
    <trd>znakomito&sacute;ci<klr> (osoby)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>alta-roda</trd>,
    <trd>fina-flor</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_i;&c_v;&c_k;&c_i; &c_o;&c_b;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_e;&c_l;&c_i;&c_t;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>elita</trd>,
    <trd>zbor vynikaj&uacute;cich</trd>
  </trdgrp>
</drv>

<drv mrk="eminen1.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="eminen1.mal0a.KOMUNE">
    <dif>
      Kiu ne <tld/>as, ne distingi&gcirc;as; ordinara,
      <ref tip="vid" cel="rang.dua0a.OPI">duaranga<sncref/></ref>,
      <ref tip="vid" cel="obskur.0a.nefama">obskura<sncref/></ref>:
      <ekz>
        li iris en la kastelon, sed ne en la festosalonon, &dash; por tio li estis tro
        mal<tld/>a, li povis eniri nur en la servistejon
        <fnt>
          <bib>Fab3</bib>
        </fnt>;
      </ekz>
      <ekz>
        la altkvalita servistaro, kiu paradis en silkaj &scirc;trumpoj, <klr>[&#8230;]</klr> tute
        rigidi&gcirc;is de teruro, ke tia mal<tld/>a persono kura&gcirc;as sidi meze de ili &ccirc;e la
        tablo
        <fnt>
          <bib>Fab3</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi dissendis provan &ccirc;apitron al <tld/>aj (kaj 
        mal<tld/>aj) geesperantistoj
        <fnt>
          <aut>M. Gazzetta</aut>,
          <vrk><url
          ref="https://groups.google.com/forum/#!msg/soc.culture.esperanto/tnY604VWxYM/WuKK8plDQdIJ">
          Helpu min kompreni</url></vrk>,
          <lok>sce, 2014-12-24</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_z;&c_n;&c_a;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_mol;&c_c;&c_ib;&c_p;&c_l;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>unbedeutend <klr>(Pers&ouml;nlichkeit)</klr></trd>,
    <trd>niedrig <klr>(Pers&ouml;nlichkeit)</klr></trd>,
    <trd>von niedrigem <ind>Rang</ind></trd>
  </trdgrp>
  <trd lng="en">undistinguished</trd>
  <trd lng="fr">effac&eacute;</trd>
</drv>
</art>
