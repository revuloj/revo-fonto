  <trd lng="it">crociere</trd>
  <trd lng="ja">&#x30A4;&#x30B9;&#x30AB;</trd>
  <trd lng="nl">kruisbek</trd>
  <trd lng="pl">krzy&zdot;odzi&oacute;b</trd>
