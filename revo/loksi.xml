    <trd>trinca-nozes</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_l;&c_jo;&c_s;&c_t;</trd>
  <trd lng="uk">&#x448;&#x438;&#x448;&#x43A;&#x430;&#x440;</trd>
