      <ekz>
        Krimea montaro: <klr>[&#x2026;]</klr> sur la sudaj deklivoj vivas paruoj, regoloj,
        <tld/>oj, montaraj emberizoj, certioj, kolomboj, apusoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=127782">Krimea montaro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="bek.kruc0ulo">krucbekulo</ref>
    <bld lok="&WCU;/thumb/1/10/Loxia_curvirostra_-Karwendel_mountains%2C_Austria.jpg/640px-Loxia_curvirostra_-Karwendel_mountains%2C_Austria.jpg" prm="CC BY 2.0"><tld/>o (Loxia curvirostra), montaro Karwendel, A&ubreve;strujo
        <fnt>
          <aut>Frank Vassen el Bruselo</aut>
          <lok><url ref="&WCW;/File:Loxia_curvirostra_-Karwendel_mountains,_Austria.jpg">&WCW;/File:Loxia_curvirostra_-Karwendel_mountains,_Austria.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_zh;&c_a;&c_d;&c_z;&c_ju;&c_b;</trd>
  <trd lng="cs">k&rcaron;ivka (pt&aacute;k)</trd>
  <trd lng="de">Kreuzschnabel</trd>
  <trd lng="en">crossbill</trd>
  <trd lng="es">piquituerto</trd>
  <trd lng="fa">&a_s;&a_h;&a_r;&a_h;&a_hamza_sure; &a_n;&a_w;&f_k;&zwnj;&a_q;&f_y;&f_ch;&f_y;</trd>
