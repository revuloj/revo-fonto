<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>muzikologi</rad>/o
</kap>
<drv mrk="muzikologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="muzikologi.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <ref tip="dif" cel="muzik.0oscienco.MUZ">muzikoscienco</ref>
    <ekz>
      fakoj <tld/>o kaj muzikhistorio
      <fnt>
      <aut>-</aut>,
      <vrk><url ref="http://www.ais-sanmarino.org/isd/estrscis2000.html">
      Akademio Internacia de la Sciencoj - Estraro</url></vrk>,
      <lok>2000-01</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="fr">musicologie</trd>
  <trd lng="it">musicologia</trd>
</drv>

<drv mrk="muzikologi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="muzikologi.0isto.MUZ">
    <ref tip="dif" cel="muzik.0osciencisto.MUZ">muzikosciencisto</ref>
    <ekz>
      Otto De Greiff Haeusler estis in&gcirc;eniero, matematikisto,
      <tld/>isto kaj poeto kolombia
      <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;Otto de Greiff Haeusler&rightquot;,
      2006-07</lok></fnt>.
    </ekz>
  </snc>
</drv>
</art>

