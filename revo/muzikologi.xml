<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: muzikologi.xml,v 1.8 2022/03/23 14:02:27 revo Exp $">
<kap>
  <rad>muzikologi</rad>/o
</kap>
<drv mrk="muzikologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="muzikologi.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="dif" cel="muzik.0oscienco.MUZ">Muzikoscienco</ref>:
      <ekz>
        fakoj <tld/>o kaj muzikhistorio
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.ais-sanmarino.org/isd/estrscis2000.html">
          Akademio Internacia de la Sciencoj - Estraro</url></vrk>,
          <lok>2000-01</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[en]</klr> la Sankt-Peterburga <nom>Rimskij-Korsakov</nom> &Scirc;tata Konservatorio
        <klr>[&#x2026;]</klr> funkcias sep fakultatoj: Piana, Orkestra, Popolaj muzikiloj,
        Komponado kaj Direktado, Vokala, Re&gcirc;isora, <tld lit="M"/>a
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=7569">Sankt-Peterburgo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Musikwissenschaft</trd>
  </snc>
  <trd lng="ca">musicologia</trd>
  <trd lng="cs">muzikologie</trd>
  <trd lng="fr">musicologie</trd>
  <trd lng="it">musicologia</trd>
  <trdgrp lng="nl">
    <trd>musicologie</trd>,
    <trd>muziekwetenschap</trd>
  </trdgrp>
  <trd lng="pl">muzykologia</trd>
  <trd lng="ro">muzicologie</trd>
  <trd lng="sk">muzikol&oacute;gia</trd>
  <trd lng="uk">&#x43C;&#x443;&#x437;&#x438;&#x43A;&#x43E;&#x437;&#x43D;&#x430;&#x432;&#x441;&#x442;&#x432;&#x43E;</trd>
</drv>

<drv mrk="muzikologi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="muzikologi.0isto.MUZ">
    <dif>
      <ref tip="dif" cel="muzik.0osciencisto.MUZ">Muzikosciencisto</ref>:
      <ekz>
        <nom>Otto De Greiff Haeusler</nom> estis in&gcirc;eniero, matematikisto,
        <tld/>isto kaj poeto kolombia
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;Otto de Greiff Haeusler&rightquot;,
        2006-07</lok></fnt>;
      </ekz>
      <ekz>
        &gcirc;ia orkestrado estas sufi&ccirc;e simpla kaj grave bazi&gcirc;as je frapinstrumenta sekcio,
        kelkaj <tld/>istoj rigardas &gcirc;in kiel unu el fruaj ekzemploj de minimumisma
        muziko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=322145">Carl Orff</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Musikwissenschaftler</trd>,
      <trd>Musikologe</trd>
    </trdgrp>
  </snc>
  <trd lng="ca">music&ograve;leg</trd>
  <trd lng="nl">musicoloog</trd>
  <trd lng="pl">muzykolog</trd>
  <trd lng="ro">muzicolog</trd>
</drv>
</art>

