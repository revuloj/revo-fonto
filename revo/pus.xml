<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pus</rad>/o
</kap>
<drv mrk="pus.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <uzo tip="fak">FIZL</uzo>
  <snc>
    <dif>
      Densa flaveta flua&jcirc;o, produkti&gcirc;anta en abscesoj.
    </dif>
  </snc>
  <trd lng="de">Eiter</trd>
  <trd lng="nl">etter</trd>
  <trd lng="ru">&c_g;&c_n;&c_o;&c_j;</trd>
</drv>

<drv mrk="pus.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Produkti <tld/>on:
      <ekz>
        la absceso <tld/>as.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">eitern</trd>
  <trd lng="nl">etteren</trd>
  <trd lng="ru">&c_g;&c_n;&c_o;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>
</art>
