<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>pus</rad>/o
</kap>
<drv mrk="pus.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">MED</uzo>
   <uzo tip="fak">FIZL</uzo>
   <snc>
     <dif>
       Densa flaveta flua&jcirc;o, produkti&gcirc;anta en abscesoj.
     </dif>
   </snc>
  <trd lng="be">&c_g;&c_n;&c_o;&c_j;</trd>
  <trd lng="ca">pus</trd>
  <trd lng="cs">hnis</trd>
  <trd lng="de">Eiter</trd>
  <trd lng="en">pus</trd>
  <trd lng="es">pus</trd>
  <trd lng="fr">pus</trd>
  <trd lng="hu">genny</trd>
  <trd lng="nl">etter</trd>
  <trdgrp lng="pt">
   <trd>pus</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_n;&c_o;&c_j;</trd>
  <trd lng="sk">hnis</trd>
</drv>

<drv mrk="pus.0i">
   <kap><tld/>i</kap>
   <gra><vspec>ntr</vspec></gra>
   <snc>
     <dif>
       Produkti <tld/>on:
       <ekz>
         la absceso <tld/>as.
       </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_g;&c_n;&c_a;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trd lng="ca">supurar</trd>
  <trdgrp lng="cs">
    <trd>hnisat</trd>,
    <trd>mokvat</trd>,
    <trd>podbírat se</trd>,
    <trd>podebrat se</trd>
  </trdgrp>
  <trd lng="de">eitern</trd>
  <trdgrp lng="en">
     <trd>suppurate</trd>,
     <trd>discharge</trd>,
     <trd>fester</trd>
   </trdgrp>
  <trd lng="es">supurar</trd>
  <trd lng="fr">suppurer</trd>
  <trd lng="hu">gennyezik</trd>
  <trd lng="nl">etteren</trd>
  <trdgrp lng="pt">
    <trd>supurar</trd>,
    <trd>expelir <ind>pus</ind></trd>,
    <trd>gerar <ind>pus</ind></trd>,
  </trdgrp>
  <trd lng="ru">&c_g;&c_n;&c_o;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>hnisať</trd>,
    <trd>mokvať</trd>
  </trdgrp>
</drv>
</art>
