<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pus.xml,v 1.24 2024/02/17 12:12:49 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>pus</rad>/o
</kap>
<drv mrk="pus.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">MED</uzo>
   <uzo tip="fak">FIZL</uzo>
   <snc>
     <dif>
       Densa flaveta flua&jcirc;o, produkti&gcirc;anta en abscesoj.
     </dif>
   </snc>
  <trd lng="be">&c_g;&c_n;&c_o;&c_j;</trd>
  <trd lng="bg">&c_g;&c_n;&c_o;&c_j;</trd>
  <trd lng="bo">&#xF62;&#xFA3;&#xF42;&#xF0B;</trd>
  <trd lng="ca">pus</trd>
  <trd lng="cs">hnis</trd>
  <trd lng="de">Eiter</trd>
  <trd lng="en">pus</trd>
  <trd lng="es">pus</trd>
  <trd lng="fr">pus</trd>
  <trd lng="hu">genny</trd>
  <trdgrp lng="ja">
    <trd>&#x81BF;  <pr>&#x3046;&#x307F;</pr></trd>,
    <trd>&#x3046;&#x307F;</trd>
  </trdgrp>
  <trd lng="nl">etter</trd>
  <trd lng="pl">ropa  <klr>(medycyna)</klr></trd>
  <trd lng="pt">pus</trd>
  <trd lng="ro">puroi</trd>
  <trd lng="ru">&c_g;&c_n;&c_o;&c_j;</trd>
  <trd lng="sk">hnis</trd>
  <trd lng="uk">&c_g;&#x43D;&c_ib;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x8113; <pr>n&#xF3;ng</pr></trd>,
    <trd>&#x81BF; <pr>n&#xF3;ng</pr></trd>,
    <trd>&#x8113;&#x6C34; <pr>n&#xF3;ngshu&#x1D0;</pr></trd>,
    <trd>&#x81BF;&#x6C34; <pr>n&#xF3;ngshu&#x1D0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pus.0i">
   <kap><tld/>i</kap>
   <gra><vspec>ntr</vspec></gra>
   <snc>
     <dif>
       Produkti <tld/>on:
       <ekz>
         la absceso <tld/>as.
       </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_g;&c_n;&c_a;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trd lng="bg">&c_g;&c_n;&c_o;&c_ja;</trd>
  <trd lng="ca">supurar</trd>
  <trdgrp lng="cs">
    <trd>hnisat</trd>,
    <trd>mokvat</trd>,
    <trd>podb&iacute;rat se</trd>,
    <trd>podebrat se</trd>
  </trdgrp>
  <trd lng="de">eitern</trd>
  <trdgrp lng="en">
     <trd>suppurate</trd>,
     <trd>discharge</trd>,
     <trd>fester</trd>
   </trdgrp>
  <trd lng="es">supurar</trd>
  <trd lng="fr">suppurer</trd>
  <trd lng="hu">gennyezik</trd>
  <trdgrp lng="ja">
    <trd>&#x5316;&#x81BF;&#x3059;&#x308B;  <pr>&#x304B;&#x306E;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x3046;&#x307F;&#x3092;&#x3082;&#x3064;</trd>
  </trdgrp>
  <trd lng="nl">etteren</trd>
  <trd lng="pl">ropie&cacute;</trd>
  <trdgrp lng="pt">
    <trd>supurar</trd>,
    <trd>expelir <ind>pus</ind></trd>,
    <trd>gerar <ind>pus</ind></trd>,
  </trdgrp>
  <trd lng="ro">puroia</trd>
  <trd lng="ru">&c_g;&c_n;&c_o;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>hnisa&tcaron;</trd>,
    <trd>mokva&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&#x43D;&#x43E;&c_ji;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&#x43D;&c_a;&c_g;&#x43D;&#x43E;&#x44E;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&c_z;&c_a;&c_g;&#x43D;&#x43E;&#x44E;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6D41;&#x8113; <pr>li&#xFA;n&#xF3;ng</pr></trd>,
    <trd>&#x6D41;&#x81BF; <pr>li&#xFA;n&#xF3;ng</pr></trd>,
    <trd>&#x6E83;&#x70C2; <pr>ku&#xEC;l&#xE0;n</pr></trd>,
    <trd>&#x6F70;&#x721B; <pr>ku&#xEC;l&#xE0;n</pr></trd>,
    <trd>&#x751F;&#x8113;&#x5305; <pr>sh&#x113;ngn&#xF3;ngb&#x101;o</pr></trd>,
    <trd>&#x751F;&#x81BF;&#x5305; <pr>sh&#x113;ngn&#xF3;ngb&#x101;o</pr></trd>
  </trdgrp>
</drv>
</art>
