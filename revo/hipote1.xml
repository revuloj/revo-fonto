<kap>
  <rad>hipotenuz</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="hipote1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 27</lok></fnt>
  <snc>
    <dif>
      <klr>(de <ref cel="angul.ort0a">ortangula</ref> <ref cel="angul.tri0o">triangulo</ref>)</klr>
      Latero <ref cel="kontraux.0a.triangulo">kontra&ubreve;a</ref> al la
      orta angulo; &gcirc;ia longo:
      <ekz>
        la kvadrato de <tld/>o egalas la sumon de la kvadratoj de la du
        <ref cel="katet.0o">katetoj</ref>
        <klr>(<ref cel="pitago.p0a">pitagora</ref> teoremo)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_p;&c_a;&c_t;&c_e;&c_n;&c_u;&c_z;&c_a;</trd>
  <trd lng="br">hipotenuzenn</trd>
  <trd lng="cs">p&#x159;epona</trd>
  <trd lng="de">Hypotenuse</trd>
  <trd lng="el">&ypsilon;&pi;&omikron;&tau;&epsilon;&jota_ton;&ny;&omikron;&ypsilon;&sigma;&alfa;</trd>
  <trd lng="en">hypotenuse</trd>
  <trd lng="es">hipotenusa</trd>
  <trd lng="fa">&a_w;&a_t;&a_r;</trd>
  <trd lng="fr">hypot&eacute;nuse</trd>
  <trd lng="hu">&aacute;tfog&oacute;</trd>
  <trd lng="it">ipotenusa</trd>
  <trd lng="ja">&#x659C;&#x8FBA;  <pr>&#x3057;&#x3083;&#x3078;&#x3093;</pr></trd>
  <trd lng="nl">hypotenusa</trd>
  <trd lng="pl">przeciwprostok&aogonek;tna</trd>
  <trd lng="pt">hipotenusa</trd>
  <trd lng="ru">&c_g;&c_i;&c_p;&c_o;&c_t;&c_je;&c_n;&c_u;&c_z;&c_a;</trd>
  <trd lng="sk">prepona <klr>(geom)</klr></trd>
  <trd lng="uk">&#x433;&#x456;&#x43F;&#x43E;&#x442;&#x435;&#x43D;&#x443;&#x437;&#x430;</trd>
  <trdgrp lng="zh">
    <trd>&#x7D43; <pr>xi&#xE1;n</pr></trd>,
    <trd>&#x659C;&#x8FB9; <pr>xi&#xE9;bi&#x101;n</pr></trd>
  </trdgrp>
</drv>
