<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hipotenuz</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="hipote1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(de <ref cel="angul.ort0a">ortangula</ref> <ref cel="angul.tri0o">triangulo</ref>)</klr>
      Latero <ref cel="kontraux.0a.triangulo">kontra&ubreve;a</ref> al la
      orta angulo; &gcirc;ia longo:
      <ekz>
        la kvadrato de <tld/>o egalas la sumon de la kvadratoj de la du katetoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">hypot&eacute;nuse</trd>
  <trd lng="ru">&c_g;&c_i;&c_p;&c_o;&c_t;&c_je;&c_n;&c_u;&c_z;&c_a;</trd>
</drv>
</art>

