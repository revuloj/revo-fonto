<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>epidemi</rad>/o
</kap>
<drv mrk="epidem.0o">
  <kap><tld/>o</kap>
  <snc mrk="epidem.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Infekta <ref tip="super" cel="san.mal0o.MED">malsano</ref>
      samtempe atakanta multajn personojn en unu loko:
      <ekz>
        disvasti&gcirc;is inter ili <tld/>o
        <fnt><bib>MT</bib>, <lok>&Psa; 106:29</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_p;&c_ib;&c_d;&c_e;&c_m;&c_ib;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Epidemie</trd>,
    <trd>Seuche</trd>
  </trdgrp>
  <trd lng="es">epidemia</trd>
  <trd lng="fr">&eacute;pid&eacute;mie</trd>
  <trdgrp lng="nl">
    <trd>epidemie</trd>,
    <trd>plaag</trd>
  </trdgrp>
  <trd lng="pt">epidemia</trd>
  <trd lng="ru">&c_e;&c_p;&c_i;&c_d;&c_je;&c_m;&c_i;&c_ja;</trd>
  <trd lng="sv">epidemi</trd>
</drv>

<drv mrk="epidem.0a">
  <kap><tld/>a</kap>
  <snc mrk="epidem.0a.MED">
    <dif>
      Havanta la ecojn de <tld/>o:
      <ekz>
        la pesto estas <tld/>a;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a entuziasmo, idioteco<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_p;&c_ib;&c_d;&c_e;&c_m;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="de">
    <trd>epidemisch</trd>,
    <trd>seuchenhaft</trd>
  </trdgrp>
  <trd lng="es">epid&eacute;mico</trd>
  <trdgrp lng="fr">
    <trd>contagieux</trd>,
    <trd>&eacute;pid&eacute;mique</trd>
  </trdgrp>
  <trd lng="nl">epidemisch</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_p;&c_i;&c_d;&c_je;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
