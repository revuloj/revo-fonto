<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>bac</rad> <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="bac.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="bac.0.SON">
    <dif>
      <ref tip="lst" cel="son.0o" lst="voko:sonoj">Sono</ref>
      de iom mole kunbati&gcirc;antaj a&jcirc;oj;
      <ref tip="vid" cel="bat.0.SON">bat</ref>:
      <ekz>
        la onklo atendas, ektiregas kaj subite <tld/>!
        &mdash; li ku&scirc;as &gcirc;is la genuoj en akvo!
        <fnt>
          <aut>J. Mahen, trad. Moraviaj Esperanto-Pioniroj</aut>,
          <vrk><url
          ref="http://eo.wikisource.org/wiki/La_koboldo_Ondra">
          La koboldo Ondra</url></vrk>,
          <lok>1921</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">pouf !</trd>
</drv>
</art>

