<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>sensor</rad>/o
</kap>
<drv mrk="sensor.0o">
  <kap><tld/>o</kap>
  <snc mrk="sensor.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <ref tip="dif" cel="sent.0ilo.TEK">sentilo<sncref/></ref>
    <ekz>
      la kontakto de la pantografoj kun la katenario
      ne &gcirc;enu la <tld/>ojn almuntitaj<klr>n</klr>
      je la aliaj vagonoj
      <fnt>
        <aut>trad. RoBo</aut>,
        <vrk>Arkimedo la Nova Diagnozika Trajno</vrk>,
        <lok><url ref="http://ifef.free.fr/ifefbultit.pdf">
        Itala Fervojisto, 2003-05</url></lok>
      </fnt>;
    </ekz>                                                      
    <ekz>
      la roboto estas ekipita per <tld/>oj &ndash;
      kameraoj kaj e&hcirc;osondiloj  
      <fnt>
        <aut>F. Maurelli</aut>,
        <vrk><url
        ref="http://www.uea.org/dokumentoj/IKU/2012/Maurelli.html">
        Subakva robotiko (resumo)</url></vrk>,
        <lok>IKU-2012</lok>
      </fnt>.
    </ekz>
  </snc>
</drv>
</art>

<!--
$Log$
-->
</vortaro>