<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sensor.xml,v 1.6 2023/08/05 18:28:09 revo Exp $">
<kap>
  <rad>sensor</rad>/o
</kap>
<drv mrk="sensor.0o">
  <kap><tld/>o</kap>
  <snc mrk="sensor.0o.sentilo">
    <uzo tip="fak">TEK</uzo>
    <uzo tip="fak">BIO</uzo>
    <dif>
      <ref tip="dif" cel="sent.0ilo.TEK">Sentilo<sncref/></ref>:
      <ekz>
         talpo havas sur la nazo kaj la vosto vibrajn <tld/>ojn, pli sentemaj ol tiu plej bona
         <tld/>o fabrikita de homo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=111613">Rekordoj de mamuloj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la roboto estas ekipita per <tld/>oj &ndash;
        kameraoj kaj e&hcirc;osondiloj
        <fnt>
          <aut>F. Maurelli</aut>,
          <vrk><url ref="&UEA;dokumentoj/IKU/2012/Maurelli.html">
          Subakva robotiko (resumo)</url></vrk>,
          <lok>IKU-2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>oblatoj en ciferecaj kameraoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2010/010081.php">Misteroj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la kontakto de la pantografoj kun la katenario
        ne &gcirc;enu la <tld/>ojn almuntitaj<klr>n</klr>
        je la aliaj vagonoj
        <fnt>
          <aut>trad. RoBo</aut>,
          <vrk>Arkimedo la Nova Diagnozika Trajno</vrk>,
          <lok><url ref="http://ifef.free.fr/ifefbultit.pdf">
          Itala Fervojisto, 2003-05</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_e;&c_n;&c_s;&c_a;&c_r;</trd>,
    <trd>&c_d;&c_a;&c_t;&c_ch;&c_y;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Sensor</trd>
  <trdgrp lng="pl">
    <trd>sensor</trd>,
    <trd>czujnik</trd>
  </trdgrp>
</drv>
</art>

