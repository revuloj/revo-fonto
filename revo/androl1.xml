<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: androl1.xml,v 1.4 2024/01/30 06:28:30 revo Exp $">
<kap>
  <rad>andrologi</rad>/o
</kap>
<drv mrk="androl1.0o">
  <kap><tld/>o</kap>
  <snc mrk="androl1.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Medicina <ref tip="super" cel="fak.0o.brancxo">fako</ref>,
      kiu studas la <ref tip="vid"
      cel="fiziol1.0o">fiziologion</ref> kaj la <ref tip="vid"
      cel="patolo.0o">patologion</ref> de viraj seksorganoj.
      <ekz>
        la <tld/>o <klr>[&#8230;]</klr> zorgas pri la studo, analizo, kaj
        scienca esplorado de &ccirc;iu ajn aspekto rilata al la homa seksa funkciado kaj reproduktado vira
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=517553">Andrologio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_d;&c_r;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="de">Andrologie</trd>
  <trd lng="en">andrology</trd>
  <trd lng="es">androlog&iacute;a</trd>
  <trd lng="fi">andrologia</trd>
  <trd lng="fr">andrologie</trd>
  <trd lng="gd">l&egrave;igh-e&ograve;las fhir</trd>
  <trd lng="io">andrologio</trd>
  <trd lng="it">andrologia</trd>
  <trd lng="nl">andrologie</trd>
  <trd lng="pl">andrologia</trd>
  <trd lng="pt">andrologia</trd>
  <trd lng="ru">&c_a;&c_n;&c_d;&c_r;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
</drv>

<drv mrk="androl1.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="androl1.0isto.MED">
    <uzo tip="fak">MED</uzo>
    <ref tip="dif" cel="androl.0o">andrologo</ref>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_d;&c_r;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trdgrp lng="de">
    <trd>Androloge</trd>,
    <trd>M&auml;nnerarzt</trd>
  </trdgrp>
  <trd lng="en">andrologist</trd>
  <trd lng="fi">andrologi</trd>
  <trd lng="fr">andrologue</trd>
  <trd lng="io">andrologo</trd>
  <trdgrp lng="it">
    <trd>andrologo</trd>,
    <trd>androloga</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>androloog</trd>,
    <trd>mannenarts</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>andrologista</trd>,
    <trd>andr&oacute;logo</trd>
  </trdgrp>
  <trd lng="vo">manasanan</trd>
</drv>
</art>

