<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>litograf</rad>/i
</kap>
<drv mrk="litogr.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Reprodukti sur papero per presado skriba&jcirc;on a&ubreve; desegna&jcirc;on anta&ubreve;e
      gravuritan sur la&ubreve;cela &scirc;tono.
    </dif>
  </snc>
  <trd lng="es">litografiar</trd>
  <trd lng="nl">lithograferen</trd>
  <trd lng="ru">&c_l;&c_i;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="litogr.0isto">
  <kap>
    <tld/>isto
  </kap>
  <snc>
    <dif>
      Homo, kies profesio estas <tld/>i.
    </dif>
  </snc>
  <trd lng="es">lit&oacute;grafo</trd>
  <trdgrp lng="nl">
    <trd>lithograaf</trd>,
    <trd>steendrukker</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_i;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
</drv>
</art>
