<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: litogr.xml,v 1.27 2024/03/10 21:20:03 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>litograf</rad>/i
  <fnt><bib>FK</bib></fnt>
</kap>
<drv mrk="litogr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="litogr.0i.TIP">
    <dif>
      <ref cel="produk.re0i.TEK">Reprodukti</ref>
      per presado skriba&jcirc;on a&ubreve; desegna&jcirc;on anta&ubreve;e
      gravuritan sur la&ubreve;cela &scirc;tono:
      <ekz>
        la unua &ccirc;ina gazeto aperis en Parizo, bele <tld/>ita kaj aran&gcirc;ita
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16266">Frua Esperanto-gazetaro</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_t;&c_a;&c_g;&c_r;&c_a;&c_f;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">litografovat</trd>
  <trdgrp lng="de">
    <trd>lithographieren</trd>,
    <trd>lithografieren</trd>,
    <trd>in <ind>Steindruck wiedergeben</ind></trd>
  </trdgrp>
  <trd lng="en">to lithograph</trd>
  <trd lng="el">&lambda;&jota;&theta;&omikron;&gamma;&rho;&alfa;&phi;&omega_ton;</trd>
  <trd lng="es">litografiar</trd>
  <trd lng="fr">lithographier</trd>
  <trdgrp lng="hu">
    <trd>k&odblac;re nyomtat</trd>,
    <trd>k&odblac;re rajzol</trd>,
    <trd>litograf&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x77F3;&#29256;&#21360;&#21047;&#12377;&#x308B;  <pr>&#12356;&#12375;&#x306F;&#12435;&#12356;&#12435;&#12373;&#12388;&#12377;&#x308B;</pr></trd>,
    <trd>&#x77F3;&#29256;&#21047;&#x308A;&#x306B;&#12377;&#x308B;  <pr>&#12356;&#12375;&#x306F;&#12435;&#12377;&#x308A;&#x306B;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">lithograferen</trd>
  <trd lng="pl">litografowa&cacute;</trd>
  <trd lng="pt">litografar</trd>
  <trd lng="ru">&c_l;&c_i;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">litografova&tcaron;</trd>
  <trd lng="uk">&#x43B;&c_ib;&c_t;&#x43E;&c_g;&c_r;&c_a;&c_f;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="litogr.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="litogr.0ajxo.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      <ref cel="verk.0o.propra">Verko</ref>,
      kiun oni prese faris per <ref tip="vid" cel="litogr1.0o.TIP">litografio</ref>:
      <ekz>
        sur la muro estis bone koloritaj <tld/>a&jcirc;oj &ndash; pli belaj, multe
        pli belaj, ol oleaj pentra&jcirc;oj
        <fnt><bib>FK</bib>, <lok>4, rakontoj, la hejmo de la metiisto,
        (de nov-jorka kuracisto, rerakontita de E. Weilsh&auml;user)</lok></fnt>;
      </ekz>
      <ekz>
        ili <klr>[&#8230;]</klr> mem havigis la monrimedojn <klr>[&#8230;]</klr> pere de
        vendo de <klr>[&#8230;]</klr> modeloj de anta&ubreve;e kreitaj arta&jcirc;oj kaj <tld/>a&jcirc;oj
        <fnt>
       <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2000/007013.php">Ekspozicio de famaj artistoj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/07, p. 15</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="litogr1.0o.bildo">litografio<sncref/></ref>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_t;&c_a;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja; <klr>(&c_a;&c_d;&c_b;&c_ib;&c_t;&c_a;&c_k;)</klr></trd>
  <trd lng="en">lithograph</trd>
  <trd lng="fr">lithographie<klr tip="amb"> (&oelig;uvre)</klr></trd>
  <trd lng="pl">litografia</trd>
  <trd lng="uk">&#x43B;&c_ib;&c_t;&#x43E;&c_g;&c_r;&c_a;&c_f;&c_ib;&#x44F; (&c_v;&c_ib;&c_d;&c_b;&c_i;&c_t;&#x43E;&#x43A;)</trd>
  <trdgrp lng="zh">
    <trd>&#x77F3;&#21360; <pr>sh&#xED;y&#xEC;n</pr></trd>,
    <trd>&#x5E73;&#21360; <pr>p&#xED;ngy&#xEC;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="litogr.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="litogr.0isto.TIP">
    <dif>
      Tiu, kiu profesie preparas <tld/>a&jcirc;ojn:
      <ekz>
        la internacie agnoskata <tld/>isto <nom>Adolf Tuma</nom>
        <fnt>
        <aut>Katinjo Fetes-Tosegi</aut>,
        <vrk>E&ubreve;ro-po&scirc;tmarkoj</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2002a, numero 2a, p. 10a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="lst" lst="voko:metioj" cel="meti.0isto">metiisto</ref>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
  <trd lng="cs">litograf</trd>
  <trdgrp lng="de">
    <trd>Lithograph</trd>,
    <trd>Lithograf</trd>,
    <trd>Steindrucker</trd>
  </trdgrp>
  <trd lng="en">lithographer</trd>
  <trd lng="el">&lambda;&jota;&theta;&omikron;&gamma;&rho;&alfa_ton;&phi;&omikron;&sigma_fina;</trd>
  <trd lng="es">lit&oacute;grafo</trd>
  <trd lng="fr">lithographe</trd>
  <trdgrp lng="hu">
    <trd>k&odblac;nyom&oacute;</trd>,
    <trd>litogr&aacute;fus</trd>,
    <trd>k&odblac;rajzol&oacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x77F3;&#29256;&#x5DE5;  <pr>&#12356;&#12375;&#x306F;&#12435;&#12371;&#12358;</pr></trd>,
    <trd>&#x77F3;&#29256;&#x5E2B;  <pr>&#12356;&#12375;&#x306F;&#12435;&#12375;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>lithograaf</trd>,
    <trd>steendrukker</trd>
  </trdgrp>
  <trd lng="pl">litograf</trd>
  <trd lng="pt">lit&oacute;grafo</trd>
  <trd lng="ru">&c_l;&c_i;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
  <trd lng="sk">kame&ncaron;otla&ccaron;iar</trd>
  <trd lng="uk">&#x43B;&c_ib;&c_t;&#x43E;&c_g;&c_r;&c_a;&c_f;</trd>
  <trd lng="zh">&#x5E73;&#21360;&#x5DE5;&#x4EBA; <pr>p&#xED;ngy&#xEC;ng&#x14D;ngr&#xE9;n</pr></trd>
</drv>
</art>
