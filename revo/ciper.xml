<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ciper.xml,v 1.21 2022/03/14 15:25:39 revo Exp $">
<kap>
  <rad>ciperac</rad>/oj
</kap>
<drv mrk="ciper.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="ciper.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref>  de <ref tip="malprt" cel="kotile.unu0aj">unukotiledonaj</ref>
      plantoj (<trd lng="la">Cyperaceae</trd>):
      <ekz>
        vombatoj estas herbovoraj, plejparto de ilia dieto konsistas je herboj el
        familio poacoj kaj <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=8857">Vombato</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
       <ref cel="eriofo.0o">erioforo</ref>,
       <ref cel="kareks.0o">karekso</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_a;&c_k;&c_o;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">ciper&agrave;cies</trd>
  <trd lng="cs">&scaron;&aacute;chorovit&eacute; <klr>(&ccaron;ele&dcaron;)</klr></trd>
  <trdgrp lng="de">
    <trd>Sauergrasgew&auml;chse</trd>,
    <trd>Riedgr&auml;ser</trd>,
    <trd>Zypergr&auml;ser</trd>
  </trdgrp>
  <trd lng="fr">cyp&eacute;rac&eacute;es</trd>
  <trd lng="he">&#x5D1;&#x5E8;&#x5D5;&#x5E9;&#x5D9;&#x5DD;</trd>
  <trdgrp lng="hu">
    <trd>palkaf&eacute;l&eacute;k</trd>,
    <trd>s&aacute;sf&eacute;l&eacute;k</trd>
  </trdgrp>
  <trd lng="it">ciperacee</trd>
  <trd lng="pl">turzycowate</trd>
  <trd lng="pt">ciper&aacute;ceas</trd>
  <trd lng="ru">&c_o;&c_s;&c_o;&c_k;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">&scaron;achorovit&eacute;</trd>
  <trd lng="sv">halvgr&auml;s</trd>
  <trd lng="uk">&#x43E;&#x441;&#x43E;&#x43A;&#x43E;&#x432;&#x456;</trd>
</drv>
</art>
