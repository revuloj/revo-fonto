<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ciperac</rad>/oj
</kap>
<drv mrk="ciper.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="ciper.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Familio de <ref tip="super"
      cel="kotile.unu0aj">unukotiledonaj</ref>
      plantoj (<trd lng="la">Cyperaceae</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_a;&c_k;&c_o;&c_v;&c_y;&c_ja; <klr
  tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trdgrp lng="de">
    <trd>Riedgr&auml;ser</trd>,
    <trd>Zypergr&auml;ser</trd>
  </trdgrp>
  <trd lng="fr">cyp&eacute;rac&eacute;es</trd>
  <trdgrp lng="hu">
    <trd>palkaf&eacute;l&eacute;k</trd>,
    <trd>s&aacute;sf&eacute;l&eacute;k</trd>
  </trdgrp>
  <trd lng="pl">turzycowate</trd>
  <trd lng="pt">ciper&aacute;ceas</trd>
  <trd lng="ru">&c_o;&c_s;&c_o;&c_k;&c_o;&c_v;&c_y;&c_je; <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sv">halvgr&auml;s</trd>
</drv>
</art>
