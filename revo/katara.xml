<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: katara.xml,v 1.21 2022/03/21 17:51:57 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>katarakt</rad>/o
</kap>
<drv mrk="katara.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Maldiafani&gcirc;o de la okullenso, ka&ubreve;zanta kuraceblan 
      <ref tip="lst" lst="voko:malsanoj" cel="blind.0eco">blindecon</ref>:
      <ekz>
        la operacio por kuraci <tld/>on estas nuntempe konsiderata kiel
        tre simpla kaj disvasti&gcirc;inta tekniko
        <fnt>
          <bib>Monato</bib>
          <aut>Pa&ubreve;lo Sergio Viana</aut>
          <vrk><url ref="&Monato;/2013/011162.php">Kataraktotrakto
          revivigas</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        dozoj de radiado <klr>[&#x2026;]</klr> por la infanoj prezentas pli
        altan 
        riskon ol por plenkreskuloj (ekzemple koncerne la efikojn por
        cerbo, <tld/>o kaj tiroidaj tuberoj)
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2014/011193.php">Malsama
          a&gcirc;o, malsamaj riskoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    <ref tip="hom" cel="katarakt.0o">Katarakto</ref>
    oni nomas anka&ubreve; akvofalegon.
  </rim>
  <trd lng="be">&c_k;&c_a;&c_t;&c_a;&c_r;&c_a;&c_k;&c_t;&c_a;</trd>
  <trd lng="ca">cataracta</trd>
  <trdgrp lng="cs">
    <trd>katarakt</trd>,
    <trd>pe&rcaron;eje</trd>,
    <trd>stup&ncaron;ovit&yacute; vodop&aacute;d</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Katarakt</trd>, 
    <trd>Grauer Star</trd>
  </trdgrp>
  <trd lng="en">cataract</trd>
  <trd lng="es">catarata</trd>
  <trd lng="fr">cataracte<klr tip="ind"> (m&eacute;d.)</klr></trd>
  <trd lng="hu">sz&uuml;rkeh&aacute;lyog</trd>
  <trd lng="id">katarak</trd>
  <trdgrp lng="nl">
    <trd>cataract</trd>,
    <trd>grauwe <ind>staar</ind></trd>
  </trdgrp>
  <trd lng="pl">za&cacute;ma  <klr>(katarakta)</klr></trd>     
  <trd lng="ro">cataract&abreve;</trd>
  <trd lng="ru">&c_k;&c_a;&c_t;&c_a;&c_r;&c_a;&c_k;&c_t;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>be&lcaron;mo</trd>,
    <trd>katarakta</trd>,
    <trd>&scaron;ed&yacute; o&ccaron;n&yacute; z&aacute;kal</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x430;&#x442;&#x430;&#x440;&#x430;&#x43A;&#x442;&#x430;</trd>,
    <trd>&#x43F;&#x43E;&#x43C;&#x443;&#x442;&#x43D;&#x456;&#x43D;&#x43D;&#x44F; &#x43A;&#x440;&#x438;&#x448;&#x442;&#x430;&#x43B;&#x438;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>
</art>
