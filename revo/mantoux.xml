<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mantoux.xml,v 1.6 2024/02/09 09:23:35 revo Exp $">
<kap>
  <rad>manto&ubreve;</rad>/o,
  <var><kap><rad var="o">manto</rad>/o</kap></var>
</kap>

<drv mrk="mantoux.0o">
  <kap><tld/>o,
  <var><kap><tld var="o"/>o</kap></var></kap>
  <snc mrk="mantoux.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      &Ccirc;indevena speco de <ref cel="pan.0o.mangxo">pano</ref>
      knedita, fermentigita kaj vaporumita,
      ordinare en formo de malgranda bulko:
      <ekz>
        la viza&gcirc;o sen&ccirc;ese plati&gcirc;adis kaj fari&gcirc;is ridinda
        kiel <tld/>o
        <fnt>
          <aut>Ba Jin, trad. Li Shijun</aut>,
          <vrk>A&ubreve;tuno en la Printempo</vrk>,
          <lok>1980</lok>
        </fnt>;
      </ekz>
      <ekz>
        li komencis man&gcirc;i la <tld var="o"/>ojn
        <fnt>
          <aut>Lao She, trad. Wang Chongfang</aut>,
          <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0026/">
          Kamelo &Scirc;jangzi</url></vrk>,
          <lok>1988</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_mol;&c_t;&c_o;&c_w; <klr>(&c_k;&c_ib;&c_t;&c_a;&c_j;&c_s;&c_k;&c_ib;&c_ja; &c_p;&c_ja;&c_l;&c_mol;&c_m;&c_je;&c_n;&c_ib;)</klr></trd>
  <trdgrp lng="de">
    <trd>Mantou</trd>,
    <trd>Dampfbr&ouml;tchen</trd>,
    <trd>chinesischer <ind>Hefeklos</ind></trd>
  </trdgrp>
  <trd lng="fr">mantou</trd>
  <trd lng="pl">manti</trd>
  <trd lng="ro">manti</trd>
  <trdgrp lng="zh">
    <trd>&#39237;&#x982D; <pr>m&#xE1;ntou</pr></trd>,
    <trd>&#39314;&#22836; <pr>m&#xE1;ntou</pr></trd>
  </trdgrp>
</drv>
</art>

