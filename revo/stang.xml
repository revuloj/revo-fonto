<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>stang</rad>/o
</kap>
<drv mrk="stang.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      &Ccirc;iaspeca, multe pli longa ol dika, peco el ligno a&ubreve; metalo:
      <ekz>
        <tld/>o de telegrafo;
      </ekz>
      <ekz>
        vel<tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Stange</trd>
  <trdgrp lng="fr">
    <trd>barre</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>stang</trd>,
    <trd>paal</trd>
  </trdgrp>
</drv>

<drv mrk="stang.0eto">
  <kap>
    <tld/>eto
  </kap>
  <snc>
    <dif>
      Peco el iu ajn fortika materio, kiu estas multe pli longa ol dika.
    </dif>
    <refgrp tip="sub">
      <ref cel="baston.0o">bastono</ref>,
      <ref cel="verg.0o">vergo</ref>,
      <ref cel="trab.0o">trabo</ref>,
      <ref cel="sxtip.0o">&scirc;tipo</ref>,
      <ref cel="fost.0o">fosto</ref>,
      <ref cel="mast.0o">masto</ref>,
      <ref cel="palis.0o">paliso</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>tige</trd>
  </trdgrp>
  <trd lng="nl">staafje</trd>
</drv>

<drv mrk="stang.vel0o">
  <kap>
    vel<tld/>o
  </kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="jard.0o.MAR">Jardo</ref>.
    </dif>
  </snc>
</drv>


</art>
