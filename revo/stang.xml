<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>stang</rad>/o
</kap>
<drv mrk="stang.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="stang.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      &Ccirc;iaspeca, multe pli longa ol dika, peco el ligno a&ubreve; metalo:
      <ekz>
        <tld/>o de telegrafo;
      </ekz>
      <ekz>
        la giganto <klr>...</klr> prenis grandan feran <tld/>on kaj per
        unu bato disrompis la liton
        <fnt><bib>ElektFab</bib>, <lok>la kura&gcirc;a tajloreto</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="fost.0o">fosto</ref>,
      <ref cel="mast.0o">masto</ref>,
      <ref cel="palis.0o">paliso</ref>,
      <ref cel="sxtip.0o">&scirc;tipo</ref>,
      <ref cel="trab.0o">trabo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_o;&c_s;&c_t;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_l;&c_b;</trd>,
    <trd>&c_zh;&c_e;&c_r;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_t;&c_a;&c_n;&c_g;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Stange</trd>
  <trdgrp lng="fr">
    <trd>barre</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>r&uacute;d</trd>,
    <trd>p&oacute;zna</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>stang</trd>,
    <trd>paal</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_je;&c_s;&c_t;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_l;&c_b;</trd>,
    <trd>&c_zh;&c_je;&c_r;&c_d;&c_mol;</trd>,
    <trd>&c_sh;&c_t;&c_a;&c_n;&c_g;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="stang.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="stang.0eto.TEK">
    <dif>
      Peco el iu ajn fortika materio, kiu multe pli longas ol dikas.
    </dif>
    <refgrp tip="sub">
      <ref cel="baston.0o">bastono</ref>,
      <ref cel="verg.0o">vergo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_u;&c_t;</trd>,
    <trd>&c_zh;&c_e;&c_r;&c_d;&c_a;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="de">St&auml;ngel</trd>
  <trdgrp lng="fr">
    <trd>tige</trd>
  </trdgrp>
  <trd lng="hu">p&aacute;lca</trd>
  <trd lng="nl">staafje</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_u;&c_t;</trd>,
    <trd>&c_zh;&c_jo;&c_r;&c_d;&c_o;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="stang.lev0o">
  <kap>lev<tld/>o</kap>
  <snc mrk="stang.lev0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="stang.0o.TEK"><tld lit="S"/>o</ref>, kiun oni
      &scirc;ovas sub peza&jcirc;on, mezlonge apogas, kaj ekstreme subpremas por levi
      la peza&jcirc;on:
      <ekz>
        kvaza&ubreve; mi povus ka&scirc;i <klr>...</klr> lev<tld/>on sub palpebro
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>unua libro, &ccirc;apitro 3a</lok></fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">levier</trd>
</drv>

<drv mrk="stang.rosto0o">
  <kap>rosto<tld/>o</kap>
  <snc mrk="stang.rosto0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="S"/>o per kiu oni trapikas man&gcirc;a&jcirc;opecojn a&ubreve; tutan beston,
      kaj kiun oni turnas super fajro por kuiro:
      <ekz>
        leporoj kaj kunikloj estis turnataj sur rosto<tld/>o
        <fnt><bib>Fab2</bib>, <lok>ne&gcirc;a re&gcirc;ino, kvina rakonto, malgranda
        rabistinfano</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">broche<klr tip="amb"> (cuisine)</klr></trd>
</drv>

<drv mrk="stang.vel0o">
  <kap>vel<tld/>o</kap>
  <snc mrk="stang.vel0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Ligna stango al kiu estas fiksata
      <ref tip="vid" cel="vel.0o.MAR">velo</ref>:
    </dif>
    <ekz>
      la &scirc;ipo <klr>...</klr> fari&gcirc;is granda, belega, kun silkaj veloj kaj
      oritaj vel<tld/>oj
      <fnt><bib>Fab4</bib>, <lok>historio el la dunoj</lok></fnt>.
    </ekz>
    <refgrp tip="sin">
      <ref cel="jard.0o.MAR">jardo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Mast</trd>,
    <trd>Schiffsmast</trd>,
    <trd>Segelmast</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vitorlafa</trd>,
    <trd>&aacute;rbocfa</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_ja;</trd>
</drv>
</art>
