<vortaro>
<art mrk="$Id: albatr.xml,v 1.26 2023/08/30 18:23:32 revo Exp $">
<kap>
  <rad>albatros</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="albatr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> 
      el la ordo <ref tip="malprt" cel="procel.0oformaj">
      procelarioformaj birdoj</ref>
      (<trd lng="la">Diomedea</trd>), granda 
      mar<ref tip="lst" cel="bird.0o" lst="voko:birdoj">birdo</ref> kun
      longaj flugiloj:
      <ekz>
        sur la rokkornicoj nestas oregreta pingveno kaj raraj marbirdoj kiel migra
        <tld/>o kaj granda procelario
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=266029">Insulo Gough</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/d/db/Diomedea_exulans_in_flight_2_-_SE_Tasmania_straight.jpg/317px-Diomedea_exulans_in_flight_2_-_SE_Tasmania_straight.jpg" prm="CC BY-SA 3.0">fluganta <tld/>o (Diomedea exulans), Tasmana Duoninsulo, A&ubreve;stralio
        <fnt>
          <aut>JJ Harrison, prilaborita de Martin Kraft</aut>
          <lok><url ref="&WCW;/File:Diomedea_exulans_in_flight_2_-_SE_Tasmania_straight.jpg">&WCW;/File:Diomedea_exulans_in_flight_2_-_SE_Tasmania_straight.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_b;&c_a;&c_t;&c_r;&c_o;&c_s;</trd>
  <trd lng="cs">albatros</trd>
  <trd lng="de">Albatros</trd>
  <trd lng="en">albatross</trd>
  <trd lng="es">albatros</trd>
  <trd lng="fr">albatros</trd>
  <trd lng="he">&alef;&lamed;&bet;&tet;&resh;&vav;&samekh;</trd>
  <trd lng="hu">albatrosz</trd>
  <trd lng="id">albatros</trd>
  <trd lng="ja">&#x30A2;&#x30DB;&#x30A6;&#x30C9;&#x30EA;</trd>
  <trd lng="nl">albatros</trd>
  <trd lng="pl">albatros</trd>
  <trd lng="pt">albatroz</trd>
  <trd lng="ru">&c_a;&c_l;&c_mol;&c_b;&c_a;&c_t;&c_r;&c_o;&c_s;</trd>
  <trd lng="sk">albatros</trd>
  <trd lng="uk">&#x430;&#x43B;&#x44C;&#x431;&#x430;&#x442;&#x440;&#x43E;&#x441;</trd>
</drv>
