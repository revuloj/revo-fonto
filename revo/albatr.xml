<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>albatros</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="albatr.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">ZOO</uzo>
    <snc>
      <dif>
        Genro el la familio <ref tip="super" 
cel="albatr.0edoj"><tld/>edoj</ref>
        (<trd lng="la">Diomedea</trd>), granda marbirdo kun longaj flugiloj.
      </dif>
    </snc>
    <trd lng="cs">albatros</trd>
    <trd lng="de">Albatros</trd>
    <trd lng="en">albatross</trd>
    <trd lng="fr">albatros</trd>
    <trd lng="hu">albatrosz</trd>
    <trd lng="pl">albatros</trd>
    <trd
    lng="ru">&#1072;&#1083;&#1100;&#1073;&#1072;&#1090;&#1088;&#1086;&#1089;</trd>
</drv>
<drv mrk="albatr.0edoj">
    <kap><tld/>edoj</kap>
    <uzo tip="fak">ZOO</uzo>
    <snc>
      <dif>
        Familio el la ordo <ref tip="super" cel="procel.0oformaj">
        procelarioformaj birdoj</ref>.
      </dif>
    </snc>
    <trd lng="de">Albatrosse <klr tip="ind">(Familie)</klr></trd>
    <trd lng="fr">diom&eacute;d&eacute;id&eacute;s</trd>
</drv>
</art>
