<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lepor</rad>/o
</kap>
<drv mrk="lepor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="lepore.0oj"><tld/>edoj</ref>
      (<trd lng="la">Lepus</trd>), kun longaj malanta&ubreve;aj kruroj, per
      kiuj ili povas rapide saltkuri:
      <ekz>
        <ind>kiu &ccirc;asas du <tld/>ojn kaptas neniun</ind>;
        <trd lng="ru">&c_z;&c_a; &c_d;&c_v;&c_u;&c_m;&c_ja; &c_z;&c_a;&c_j;&c_c;&c_a;&c_m;&c_i;
        &c_p;&c_o;&c_g;&c_o;&c_n;&c_i;&c_sh;&c_mol;&c_s;&c_ja; &dash;
        &c_n;&c_i; &c_o;&c_d;&c_n;&c_o;&c_g;&c_o; &c_n;&c_je; &c_p;&c_o;&c_j;&c_m;&c_a;&c_je;&c_sh;&c_mol;</trd>
        <fnt><bib>PrV</bib></fnt>
      </ekz>
      <ekz>
        pafi samtempe du <tld/>ojn <klr>(atingi du rezultatojn)</klr>;
      </ekz>
      <ekz>
        fortoj <tld/>aj kaj kapricoj sinjoraj
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_ja;&c_c;</trd>
  <trd lng="br">gad</trd>
  <trd lng="de">Hase</trd>
  <trd lng="el">&lambda;&alfa;&gamma;&omikron_ton;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>hare</trd>,
    <trd>rabbit</trd>
  </trdgrp>
  <trd lng="es">liebre</trd>
  <trd lng="fr">li&egrave;vre</trd>
  <trd lng="hu">mezei <ind>ny&uacute;l</ind></trd>
  <trd lng="nl">haas</trd>
  <trd lng="pt">lebre</trd>
  <trd lng="ru">&c_z;&c_a;&c_ja;&c_c;</trd>
  <trd lng="sv">hare</trd>
</drv>

<drv mrk="lepor.0uloj">
  <kap><tld/>uloj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="mam.0uloj">mamuloj</ref>
      (<trd lng="la">Lagomorpha</trd>), kun longaj malanta&ubreve;aj kruroj,
      mallonga a&ubreve; mankanta vosto kaj du paroj da supraj
      tran&ccirc;dentoj.
    </dif>
    <rim>
      La <tld/>uloj ne apartenas al la
      <ref tip="super" cel="rongx.0uloj">ron&gcirc;uloj</ref>.
    </rim>
  </snc>
  <trd lng="de">Hasentiere <klr tip="ind">(Ordnung)</klr></trd>
  <trd lng="fr">lagomorphes</trd>
  <trd lng="hu">ny&uacute;lalak&uacute;ak</trd>
  <trd lng="ru">&c_z;&c_a;&c_j;&c_c;&c_je;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je; (&c_o;&c_t;&c_r;&c_ja;&c_d;)</trd>
</drv>
</art>
