<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>lepor</rad>/o
</kap>
<drv mrk="lepor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="lepor.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Lepus</trd>), kun longaj malanta&ubreve;aj kruroj, per
      kiuj ili povas rapide saltkuri:
      <ekz>
        kiu &ccirc;asas du <tld/>ojn kaptas neniun<fnt>Z</fnt>;
      </ekz>
      <ekz>
        pafi samtempe du <tld/>ojn <klr>(atingi du rezultatojn)</klr>;
      </ekz>
      <ekz>
        fortoj <tld/>aj kaj kapricoj sinjoraj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Hase</trd>
  <trdgrp lng="en">
    <trd>hare</trd>,
    <trd>rabbit</trd>
  </trdgrp>
  <trd lng="es">liebre</trd>
</drv>
<drv mrk="lepor.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="lepor.0uloj"><tld/>uloj</ref>
      (<trd lng="la">Leporidae</trd>).
    </dif>
    <ref tip="sub" cel="kunikl.0o">kuniklo</ref>
  </snc>
  <trd lng="de">Hasen <klr tip="ind">(Familie)</klr></trd>
</drv>
<drv mrk="lepor.0uloj">
  <kap><tld/>uloj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="mam.0uloj">mamuloj</ref>
      (<trd lng="la">Lagomorpha</trd>), kun longaj malanta&ubreve;aj kruroj,
      mallonga a&ubreve; mankanta vosto kaj du paroj da supraj
      tran&ccirc;dentoj.
    </dif>
    <rim>
      La <tld/>uloj ne apartenas al la
      <ref tip="vid" cel="rod1.0uloj">roduloj</ref>.
    </rim>
  </snc>
  <trd lng="de">Hasentiere <klr tip="ind">(Ordnung)</klr></trd>
</drv>
<drv mrk="lepor.0hundo">
  <kap><tld/>hundo</kap>
  <snc>
    <dif>
      Granda rapidkura gracie maldika hundo, kapabla kurkapti <tld/>on.
    </dif>
  </snc>
</drv>
</art>
