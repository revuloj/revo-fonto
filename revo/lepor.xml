<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lepor.xml,v 1.47 2025/10/21 16:39:01 revo Exp $">
<kap>
 <ofc>*</ofc>
 <rad>lepor</rad>/o
<fnt><bib>UV</bib></fnt></kap>

<drv mrk="lepor.0o">
 <kap><ofc>*</ofc><tld/>o</kap>
 <snc>
 <uzo tip="fak">ZOO</uzo>
   <dif>
     <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref>
     (<trd lng="la">Lepus</trd>)
     el familio <ref tip="malprt" cel="lepore.0oj"><tld/>edoj</ref>,
     kun longaj malanta&ubreve;aj kruroj, per kiuj ili povas rapide
     <ref cel="salt.0i.sin_levi">salt</ref>kuri:
     <ekz>
       <ind>kiu &ccirc;asas du <tld/>ojn kaptas neniun</ind>;
       <trdgrp lng="ca">
         <trd>qui tot ho vol, tot ho perd</trd>,
         <trd>qui molt vol, poc t&eacute;</trd>
       </trdgrp>
       <trd lng="pl">kto chwyta dwie sroki za ogon, &zdot;adnej nie z&lstroke;apie</trd>
       <trd lng="ru">&c_z;&c_a; &c_d;&c_v;&c_u;&c_m;&c_ja; &c_z;&c_a;&c_j;&c_c;&c_a;&c_m;&c_i;
       &c_p;&c_o;&c_g;&c_o;&c_n;&c_i;&c_sh;&c_mol;&c_s;&c_ja; &#8208; &c_n;&c_i;
       &c_o;&c_d;&c_n;&c_o;&c_g;&c_o; &c_n;&c_je; &c_p;&c_o;&c_j;&c_m;&c_a;&c_je;&c_sh;&c_mol;</trd>
       <fnt><bib>PrV</bib></fnt>
     </ekz>
     <ekz>
       <ind>pafi samtempe du <tld/>ojn</ind> <klr>(atingi du
       rezultatojn)</klr>;
       <trd lng="pl">upiec dwie pieczenie na jednym ogniu</trd>
     </ekz>
     <ekz>
       fortoj <tld/>aj kaj kapricoj sinjoraj
       <fnt><bib>PrV</bib></fnt>.
     </ekz>
   </dif>
 </snc>
 <ref tip="lst" cel="lepor.0uloj" lst="voko:leporuloj">leporulo</ref>
 <ref tip="vid" cel="kunikl.0o">kuniklo</ref>
 <ref tip="malprt" cel="pask.0o.KRI">Pasko</ref>
 <trd lng="be">&c_z;&c_a;&c_ja;&c_c;</trd>
 <trd lng="bg">&c_z;&c_a;&c_je;&c_k;</trd>
 <trd lng="bo">&t_ya;&t_naro;&t_sa;&t_tsheg;</trd>
 <trd lng="br">gad</trd>
 <trd lng="ca">llebre</trd>
 <trd lng="cs">zaj&iacute;c</trd>
 <trd lng="cv">&c_m;&c_u;&c_l;&c_k;&c_a;&c_ch;</trd>
 <trd lng="de">Hase</trd>
 <trd lng="el">&lambda;&alfa;&gamma;&omikron_ton;&sigma_fina;</trd>
 <trd lng="en">hare</trd>
 <trd lng="es">liebre</trd>
 <trd lng="fr">li&egrave;vre</trd>
 <trd lng="hu">mezei <ind>ny&uacute;l</ind></trd>
 <trdgrp lng="id">
   <trd>Lepus<klr tip="amb"> (genus)</klr></trd>,
   <trd>terwelu</trd>,
   <trd>kuilu</trd>,
   <trd>tegalan</trd>
 </trdgrp>
 <trd lng="nl">haas</trd>
 <trd lng="pl">zaj&aogonek;c</trd>
 <trd lng="pt">lebre</trd>
 <trd lng="ro">iepure</trd>
 <trd lng="ru">&c_z;&c_a;&c_ja;&c_c;</trd>
 <trd lng="sv">hare</trd>
  <trd lng="sk">zajac</trd>
  <trdgrp lng="zh">
    <trd>&#37326;&#20820; <pr>y&ecaron;t&ugrave;</pr></trd>,
    <trd>&#20820;&#23376; <pr>t&ugrave;zi</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lepor.L0o">
 <kap><tld lit="L"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
  </kap> <mlg>Lep</mlg>
 <snc>
 <uzo tip="fak">AST</uzo>
   <dif>
     Sud-hemisfera <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref>
(<trd lng="la">Lepus</trd>).
   </dif>
 </snc>
 <trd lng="be">&c_Z;&c_a;&c_ja;&c_c; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#8217;&c_je;)</klr></trd>
  <trd lng="bo">&t_ya;&t_naro;&t_sa;&t_tsheg;</trd>
  <trd lng="ca">la <ind>Llebre</ind></trd>
 <trd lng="cs">zaj&iacute;c</trd>
  <trd lng="de">Hase</trd>
  <trd lng="en">Lepus</trd>
  <trd lng="es">Liebre</trd>
  <trd lng="fr">Li&egrave;vre<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="id">Lepus<klr tip="amb"> (konstelasi)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#12494;&#12454;&#12469;&#12462;</trd>,
    <trd>&#12454;&#12469;&#12462;  <pr>&#12358;&#12373;&#12366;</pr></trd>
  </trdgrp>
  <trd lng="nl">Haas<klr tip="ind"> (sterrenbeeld)</klr></trd>
  <trd lng="pl">Zaj&aogonek;c  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Iepurele  <klr>(constela&#539;ie)</klr></trd>
  <trd lng="sk">zajac</trd>
  <trd lng="uk">&c_z;&c_a;&c_jeu;&c_c;&c_mol;</trd>
  <trd lng="zh">&#22825;&#20820;&#24231; <pr>Ti&amacron;nt&ugrave;zu&ograve;</pr></trd>
</drv>

<drv mrk="lepor.0uloj">
 <kap><tld/>uloj</kap>
 <snc>
 <uzo tip="fak">ZOO</uzo>
   <dif>
     <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref>
     de <ref tip="malprt" cel="mam.0uloj">mamuloj</ref>
     (<trd lng="la">Lagomorpha</trd>), kun longaj malanta&ubreve;aj
     kruroj, mallonga a&ubreve; mankanta vosto kaj du paroj da supraj
     tran&ccirc;dentoj.
   </dif>
   <rim>
     La <tld/>uloj ne apartenas al la
     <ref cel="rongx.0uloj">ron&gcirc;uloj</ref>.
   </rim>
 </snc>
 <trd lng="be">&c_z;&c_a;&c_j;&c_c;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja;
 <klr>(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="ca">lagomorfs</trd>
 <trdgrp lng="cs">
    <trd>dvojitozubci</trd>,
    <trd>zaj&iacute;ci</trd>,
    <trd>zaj&iacute;covci</trd>
  </trdgrp>
  <trd lng="de">Hasentiere <klr tip="ind">(Ordnung)</klr></trd>
  <trd lng="fr">lagomorphes</trd>
  <trd lng="hu">ny&uacute;lalak&uacute;ak</trd>
  <trd lng="id">Lagomorpha<klr tip="amb"> (ordo)</klr></trd>
  <trd lng="ja">&#12454;&#12469;&#12462;&#30446;  <pr>&#12454;&#12469;&#12462;&#12418;&#12367;</pr></trd>
  <trd lng="nl">haasachtigen</trd>
  <trd lng="pl">zaj&eogonek;czaki</trd>
  <trd lng="ro">lagomorf&abreve;</trd>
  <trd lng="ru">&c_z;&c_a;&c_j;&c_c;&c_je;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;
  (&c_o;&c_t;&c_r;&c_ja;&c_d;)</trd>
  <trd lng="sk">zajacovit&iacute;</trd>
  <trd lng="uk">&c_z;&c_a;&c_j;&c_c;&c_je;&c_p;&c_o;&c_d;&c_ib;&c_b;&c_n;&c_ib;</trd>
  <trd lng="zh">&#20820;&#24418;&#30446; <pr>t&ugrave;x&iacute;ngm&ugrave;</pr></trd>
</drv>
</art>
