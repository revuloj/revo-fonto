<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>oscil</rad>/i</kap>
<drv mrk="oscil.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      Balanci&gcirc;i alterne en kontra&ubreve;ajn direktojn &ccirc;irka&ubreve; ekvilibra
      punkto:
      <ekz>
        la <tld/>anta nadlo en kompaso.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="balanc1.0i">balanci</ref>,
      <ref cel="lul.0i">luli</ref>,
      <ref cel="sving.0i">svingi</ref>,
      <ref cel="sxance.0i">&scirc;anceli</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_j;&c_d;&c_a;&c_c;&c_c;&c_a;</trd>, 
    <trd>&c_v;&c_ib;&c_b;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>oszillieren</trd>,
    <trd>schwingen</trd>
  </trdgrp>
  <trd lng="fa">&a_n;&a_w;&a_s;&a_A;&a_n; &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="hu">
    <trd>rezeg</trd>,
    <trd>oszcill&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">oscilleren</trd>
  <trd lng="pt">oscilar</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_l;&c_je;&c_b;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_ch;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_o;&c_s;&c_c;&c_i;&c_l;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
