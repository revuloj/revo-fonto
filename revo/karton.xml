<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>karton</rad>/o
</kap>
<drv mrk="karton.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Pli a&ubreve; malpli dika folio, farita el paperpasto pistita, gluita kaj
      sekigita sub premilo:
      <ekz>
        glui fotografa&jcirc;on sur <tld/>o.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Simila pasto, uzata por fabriki aliajn objektojn:
      <ekz>
        <tld/>a pupo, masko.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Karton</trd>,
     <trd>Pappe</trd>
  </trdgrp>
  <trd lng="nl">karton</trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_t;&c_o;&c_n;</trd>
</drv>
</art>
