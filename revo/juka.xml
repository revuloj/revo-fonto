<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: juka.xml,v 1.8 2023/08/31 18:25:29 revo Exp $">
<kap>
  <rad>juka</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="juka.0o">
  <kap><tld/>o</kap>
  <snc mrk="juka.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Malalta vegeta&jcirc;o kun ligna stumpo a&ubreve; trunketo
      kaj disradiaj longaj folioj ekstreme pikaj,
      el norda kaj centra Ameriko kaj el Antiloj;
      genro el familio agavacoj (<trd lng="la">Yucca</trd>):
      <ekz>
        omleto de <tld/>o
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://eo.wikinoticia.com/kulturo-kaj-ciencia/kuirejo-kaj-gastronomio/60258-receptoj-kun-foto-de-agusto-de-2009">
          Receptoj kun foto...</url></vrk>,
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        distilejoj de <tld/>o, malsimile de la distilejoj de kano, 
        havas grandajn bezonojn de energio
        <fnt>
          <aut>-</aut>,
          <vrk>Reser&ccirc;o kaj Disvolvo</vrk>,
          <lok><url ref="http://www.bio-tec.net">biotec</url>,
          [vidita en 2010]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ju;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>juka</trd>,
    <trd>palmov&aacute; lilie</trd>
  </trdgrp>
  <trd lng="es">yuca</trd>
  <trd lng="fr">yucca</trd>
  <trd lng="it">yucca</trd>
  <trdgrp lng="ja">
    <trd>&#x30E6;&#x30C3;&#x30AB;</trd>,
    <trd>&#x30A4;&#x30C8;&#x30E9;&#x30F3;</trd>
  </trdgrp>
  <trd lng="sk">juka</trd>
  <trd lng="uk">&#x44E;&#x43A;&#x430;</trd>
</drv>
</art>

