<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sinonimi.xml,v 1.4 2019/10/19 17:10:19 revo Exp $">
<kap>
  <rad>sinonimi</rad>/o
</kap>
<drv mrk="sinonimi.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="stl">RAR</uzo>
    <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="dif" cel="sinoni.0eco.LIN">Sinonimeco</ref>:
      <ekz>
        nomindikoj en <tld/>a rilato estas nomataj sinonimoj
        <fnt>
          <aut>J. Werner</aut>,
          <vrk><url ref="http://esperanto.net/tec/werner-terminaro_de_terminologia_fako.pdf">
          Esperanta terminaro de la terminologia fako</url></vrk>,
          <lok>[2011?]</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o tre ofte aperas kaj devas aperi en &ccirc;iu lingvo
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Homonimo">
          Homonimo</url>, 2012-01-20</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Synonymie</trd>
  <trd lng="en">synonymy</trd>
</drv>
</art>

