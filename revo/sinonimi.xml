<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>sinonimi</rad>/o
</kap>
<drv mrk="sinonimi.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="stl">RAR</uzo>
  <uzo tip="fak">LIN</uzo>
  <ref tip="dif" cel="sinoni.0eco.LIN">sinonimeco</ref>
  <ekz>
    nomindikoj en <tld/>a rilato estas nomataj sinonimoj
    <fnt>
      <aut>J. Werner</aut>,
      <vrk><url
      ref="http://esperanto.net/tec/werner-terminaro_de_terminologia_fako.pdf">
      Esperanta terminaro de la terminologia fako</url></vrk>,
      <lok>[2011?]</lok>
    </fnt>;
  </ekz>
  <ekz>
    <tld/>o tre ofte aperas kaj devas aperi en &ccirc;iu lingvo
    <fnt>
      <bib>Viki</bib>,
      <lok><url ref="http://eo.wikipedia.org/wiki/Homonimo">
      Homonimo</url>, 2012-01-20</lok>
    </fnt>.
  </ekz>
  </snc>
  <trd lng="en">synonymy</trd>
</drv>
</art>

