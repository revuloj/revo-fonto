<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: oer.xml,v 1.3 2022/03/24 15:12:21 afido Exp $">
<kap>
  <rad>oer</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="oer.0o">
  <kap><tld/>o</kap>
  <snc mrk="oer.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="lst" cel="unu.monasub0o" lst="voko:monunuoj">Mona subunuo</ref>
      de pluraj norde&ubreve;ropaj monunuoj
      (<ref tip="malprt" cel="kron2.0o.EKON">kronoj</ref>):
      <ekz>
        ni &scirc;paras dudekkvin <tld/>ojn ku&scirc;ante en la sama lito
        <fnt>
          <aut>H. Laxness, trad. B. Ragnarsson</aut>
          <vrk>Sendependaj homoj</vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>&oslash;re</trd>,
    <trd>&ouml;re</trd>
  </trdgrp>
  <trd lng="pl">&ouml;re</trd>
  <trd lng="ro">&ouml;re</trd>
  <trd lng="uk">&#x435;&#x440;&#x435; (&#x43C;&#x43E;&#x43D;&#x435;&#x442;&#x430;)</trd>
</drv>
</art>

