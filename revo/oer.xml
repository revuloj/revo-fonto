<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: oer.xml,v 1.4 2023/12/04 09:28:18 afido Exp $">
<kap>
  <rad>oer</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="oer.0o">
  <kap><tld/>o</kap>
  <snc mrk="oer.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="lst" cel="unu.monasub0o" lst="voko:monunuoj">Mona subunuo</ref>
      de pluraj norde&ubreve;ropaj monunuoj
      (<ref tip="malprt" cel="kron2.0o.EKON">kronoj</ref>):
      <ekz>
        ni &scirc;paras dudekkvin <tld/>ojn ku&scirc;ante en la sama lito
        <fnt>
          <aut>H. Laxness, trad. B. Ragnarsson</aut>
          <vrk>Sendependaj homoj</vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>&oslash;re</trd>,
    <trd>&ouml;re</trd>
  </trdgrp>
  <trd lng="be">&c_e;&c_r;&c_e; <klr>(&c_m;&c_a;&c_n;&c_e;&c_t;&c_a;)</klr></trd>
  <trd lng="pl">&ouml;re</trd>
  <trd lng="ro">&ouml;re</trd>
  <trd lng="uk">&c_je;&c_r;&c_je; (&#x43C;&#x43E;&#x43D;&c_je;&c_t;&c_a;)</trd>
</drv>
</art>

