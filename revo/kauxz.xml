  <trd lng="my">&#x1021;&#x1000;&#x103D;&#x1031;&#x102C;&#x1004;&#x103B;&#x1038;</trd>
  <trd lng="ne">&#x915;&#x93E;&#x930;&#x923;</trd>
  <trd lng="ny">chifukwa</trd>
  <trd lng="oc">causa</trd>
  <trd lng="pa">&#xA15;&#xA3E;&#xA30;&#xA28;</trd>
  <trdgrp lng="pl">
    <trd>pow&oacute;d</trd>,
    <trd>przyczyna</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>oorzaak</trd>,
    <trd>reden</trd>
  </trdgrp>
  <trd lng="ps">&#x644;&#x627;&#x645;&#x644;</trd>
  <trd lng="ro">cauza</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_ch;&c_i;&c_n;&c_a;</trd>
  <trd lng="sd">&#x633;&#x628;&#x628;</trd>
  <trd lng="si">&#xDC4;&#xDDA;&#xDAD;&#xDD4;&#xDC0;</trd>
