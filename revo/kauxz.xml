<kap>
  <ofc>*</ofc>
  <rad>ka&ubreve;z</rad>/i
</kap>

<drv mrk="kauxz.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Esti la <tld/>o de; estigi, okazigi:
      <ekz>
        tiu vorto <tld/>is la malpacon;
      </ekz>
      <ekz>
        <tld/>i doloron<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i laboron<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i beda&ubreve;ron al iu;
      </ekz>
      <ekz>
        tiaj komplezaj servoj <tld/>as multajn elspezojn;
      </ekz>
      <ekz>
        akcidento <tld/>ita de a&ubreve;tomobilo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="port.al0i">alporti</ref>,
      <ref cel="port.en0i">enporti</ref>,
      <ref cel="est.0igi">estigi</ref>,
      <ref cel="tren.kun0i">kuntreni</ref>,
      <ref cel="nask.0i">naski</ref>,
      <ref cel="rezult1.0igi">rezultatigi</ref>,
      <ref cel="vek.0i">veki</ref>,
      <ref cel="ven">venigi</ref>,
      <ref cel="vok.0i">voki</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_r;&c_y;&c_ch;&c_y;&c_n;&c_ib;&c_c;&c_c;&c_a;
    </trd>,
    <trd>&c_v;&c_y;&c_z;&c_v;&c_a;&c_c;&c_mol;
    <klr>(&c_sh;&c_t;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>beza&ntilde; <ind>abeg</ind> (da)</trd>,       
    <trd>beza&ntilde; <ind>kaoz</ind> (da)</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>begr&uuml;nden</trd>,
    <trd>verursachen</trd>
  </trdgrp>
  <trd lng="en">cause</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_b;&a_b; &a_sh;&a_d;&a_n;</trd>,
    <trd>&a_b;&a_A;&a_ayn;&a_th; &a_sh;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>causer</trd>,
    <trd>donner <ind>lieu</ind> (&agrave;)</trd>,
    <trd>d&eacute;clencher<klr tip="ind"> (occasionner)</klr></trd>,
    <trd>entra&icirc;ner<klr tip="ind"> (occasionner)</klr></trd>,
    <trd>motiver<klr tip="ind"> (causer)</klr></trd>,
    <trd>occasionner</trd>,
    <trd>produire<klr tip="ind"> (occasionner)</klr></trd>,
    <trd>provoquer<klr tip="ind"> (causer)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>okoz</trd>,
    <trd>el&odblac;id&eacute;z</trd>
  </trdgrp>
  <trd lng="nl">veroorzaken</trd>
  <trdgrp lng="pl">
    <trd>powodowa&cacute;</trd>,
    <trd>przyczynia&cacute; si&eogonek;<klr> do czego&sacute;</klr></trd>,
    <trd>by&cacute; <ind>przyczyn&aogonek;</ind><klr> czego&sacute;</klr></trd>,
    <trd>wyrz&aogonek;dza&cacute;</trd>,
    <trd>sprawia&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;
    <klr>(&c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>,
    <trd>&c_p;&c_r;&c_i;&c_ch;&c_i;&c_n;&c_ja;&c_t;&c_mol;
    <klr>(&c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>,
    <trd>&c_p;&c_o;&c_v;&c_l;&c_je;&c_ch;&c_mol;
    &c_z;&c_a; &c_s;&c_o;&c_b;&c_o;&c_j;
    <klr>(&c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>,
    <trd>&c_s;&c_t;&c_a;&c_t;&c_mol;
    &c_p;&c_r;&c_i;&c_ch;&c_i;&c_n;&c_o;&c_j;
    <klr>(&c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>
  </trdgrp>
  <trd lng="sv">orsaka</trd>
</drv>

<drv mrk="kauxz.0o">
  <kap><ofc>*</ofc><tld/>o <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5057">ka&ubreve;z'o</url></lok></fnt></kap>
  <snc mrk="kauxz.0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Tio, pro kio io estas a&ubreve; i&gcirc;as tia, kiel &gcirc;i
      estas:
      <ekz>
        la unua <tld/>o
        <klr>(anta&ubreve; kiu oni povas koncepti neniun alian)</klr>;
      </ekz>
      <ekz>
        la efika <tld/>o
        <klr>(la fenomeno necese anta&ubreve;a al alia)</klr>;
      </ekz>
      <ekz>
        la cela <tld/>o
        <klr>(la celo, kiun celas inteligenta <tld/>o)</klr>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="kauxz.0o.kialo">
    <dif>
      Tio, pro kio io okazas; kialo:
      <ekz>
        <tld/>o de milito, de malsano;
      </ekz>
      <ekz>
        ne esti sen <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la proceso, al kiu tiu &ccirc;i bela loko donas 
        la <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiam min malbeno ne <ind>mia<tld/>a</ind> 
        <klr>(kies <tld/>o ne estas mi)</klr> subite
        min apartigis de la amatoj<fnt>Z</fnt>.
        <trd lng="fr">de mon <ind>fait</ind></trd>
      </ekz>
    </dif>
  </snc>
  <snc mrk="kauxz.0o.motivo">
    <dif>
      Tio, pro kio oni faras ian agon:
      <ekz>
        mi havis multajn <tld/>ojn, agante tiel;
      </ekz>
      <ekz>
        &scirc;i havis plenan <tld/>on por triumfi
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        mi ekvidas nun la <tld/>on de lia kolero;
      </ekz>
      <ekz>
        doni al iu <tld/>on por plendi<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="font.0o">fonto</ref>,
      <ref cel="motiv.0o">motivo</ref>,
      <ref cel="prav">pravigo</ref>,
      <ref cel="radik.0o.FIG">radiko <sncref/></ref>
    </refgrp>
  </snc>            
  <trd lng="af">oorsaak</trd>
  <trd lng="am">&#x1218;&#x1295;&#x1235;&#x12A4;</trd>
  <trd lng="ar">&#x627;&#x644;&#x633;&#x628;&#x628;</trd>
  <trd lng="az">s&#x259;b&#x259;b</trd>
  <trd lng="bn">&#x995;&#x9BE;&#x9B0;&#x9A3;</trd>
  <trd lng="bo">&#xF62;&#xF92;&#xFB1;&#xF74;&#xF44;&#xF0B;&#xF58;&#xF5A;&#xF53;&#xF0B;</trd>
  <trdgrp lng="br">
    <trd>abeg</trd>,
    <trd>arbenn<klr tip="ind"> (abeg)</klr></trd>,
    <trd>kaoz<klr tip="ind"> (abeg)</klr></trd>,
    <trd>pennabeg</trd>,
    <trd>pennkaoz</trd>
  </trdgrp>
  <trd lng="be">&c_p;&c_r;&c_y;&c_ch;&c_y;&c_n;&c_a;</trd>
  <trd lng="bs">uzrok</trd>
  <trd lng="ca">causa</trd>
  <trd lng="co">a causa</trd>
  <trd lng="da">&#xE5;rsag</trd>
  <trdgrp lng="de">
    <trd>Grund</trd>,
    <trd>Ursache</trd>
  </trdgrp>
  <trd lng="et">p&#xF5;hjus</trd>
  <trd lng="eu">kausa</trd>
  <trdgrp lng="fa">
    <trd>&a_ayn;&a_l;&a_t;</trd>,
    <trd>&a_s;&a_b;&a_b;</trd>,
    <trd>&a_d;&a_l;&f_y;&a_l;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cause</trd>,
    <trd>motif<klr tip="ind"> (cause)</klr></trd>,
    <trd>origine<klr tip="ind"> (cause)</klr></trd>,
    <trd>pourquoi<klr tip="ind"> (subst., cause)</klr></trd>,
    <trd>raison<klr tip="ind"> (cause)</klr></trd>
  </trdgrp>
  <trd lng="fy">oarsaak</trd>
  <trd lng="gl">causa</trd>
  <trd lng="gu">&#xA95;&#xABE;&#xAB0;&#xAA3;</trd>
  <trd lng="ha">sabili</trd>
  <trd lng="hi">&#x915;&#x93E;&#x930;&#x923;</trd>
  <trd lng="hr">uzrok</trd>
  <trd lng="hu">ok</trd>
  <trd lng="hy">&#x57A;&#x561;&#x57F;&#x573;&#x561;&#x57C;&#x568;</trd>
  <trd lng="is">ors&#xF6;k</trd>
  <trd lng="ja">&#x539F;&#x56E0;</trd>
  <trd lng="jw">sabab</trd>
  <trd lng="ka">&#x10DB;&#x10D8;&#x10D6;&#x10D4;&#x10D6;&#x10D8;</trd>
  <trd lng="km">&#x1798;&#x17BC;&#x179B;&#x17A0;&#x17C1;&#x178F;&#x17BB;</trd>
  <trd lng="ko">&#xC6D0;&#xC778;</trd>
  <trd lng="lo">&#xEAA;&#xEB2;&#xEC0;&#xEAB;&#xE94;</trd>
