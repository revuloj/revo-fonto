<drv mrk="kauxz.0e_de">
  <kap><tld/>e de</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="kauxz.0e_de.pro">
    <dif>
      <ref tip="super" cel="pro.0.kial">Pro</ref> senpera efiko de:
      <ekz>
        la fratino <tld/>e de enuo kaj de malboneco malpacas kun la tuta
        familio
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <rim mrk="kauxz.0e_de.RIM.1">
      Oni komprenu &leftquot;ka&ubreve;ze de&rightquot; la&ubreve;
      pasiva senco &leftquot;ka&ubreve;zate de&rightquot;.
      Plej multaj prepozicia&jcirc;oj funkcias aktive, kiel ekzemple la
      samsenca &leftquot;sekve de&rightquot;.
      <aut>&jcirc;v</aut>
    </rim>
  </snc>         
  <trdgrp lng="br">
    <trd>abalamour da</trd>,   
    <trd>en <ind>abeg</ind> da</trd>,
    <trd>en <ind>arbenn</ind> (a, da, eus)</trd>
  </trdgrp>
<trd lng="ca">a <ind>causa</ind> de</trd>
  <trdgrp lng="fr">
    <trd>&agrave; <ind>cause</ind> de</trd>,
    <trd>du <ind>fait</ind> de</trd>,
    <trd>par</trd>
  </trdgrp>
  <trd lng="nl">veroorzaakt door</trd>
  <trd lng="oc">a <ind>causa</ind> de</trd>
  <trdgrp lng="pl">
    <trd>z <ind>powodu</ind></trd>,
    <trd>przez</trd>
  </trdgrp>
</drv>


<drv mrk="kauxz.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="kauxz.0eco.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Rilato inter la <tld/>o kaj &gcirc;ia efiko:
      <ekz>
        <ind>principo pri <tld/>eco</ind>
        <klr>(la&ubreve; kiu &ccirc;io, kio ekzistas, havas
        <tld/>on)</klr>.
        <trd lng="hu">oks&aacute;gi elv</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="br">arbennelezh</trd>
  <trdgrp lng="de">
    <trd>Kausalit&auml;t</trd>,
    <trd>Ursache-Wirkungsbeziehung</trd>
  </trdgrp>
  <trd lng="fa">&a_ayn;&a_l;&f_y;&a_shadda;&a_t;</trd>
  <trd lng="fr">causalit&eacute;</trd>
  <trd lng="hu">oks&aacute;g</trd>
  <trd lng="nl">causaliteit</trd>
  <trdgrp lng="pl">
    <trd>przyczynowo&sacute;&cacute;</trd>,
    <trd>zwi&aogonek;zek <ind>przyczynowo</ind>-skutkowy</trd>
  </trdgrp>
  <trd lng="ru">
  &c_p;&c_r;&c_i;&c_ch;&c_i;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>
