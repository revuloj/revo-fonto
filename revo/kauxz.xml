  <trd lng="st">etsa hore</trd>
  <trd lng="sv">orsak</trd>
  <trd lng="ta">&#xB95;&#xBBE;&#xBB0;&#xBA3;&#xBAE;&#xBCD;</trd>
  <trd lng="te">&#xC15;&#xC3E;&#xC30;&#xC23;&#xC02;</trd>
