<drv mrk="kauxz.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Esti la <tld/>o de; <ref tip="sin" cel="est.0igi">estigi</ref>, 
      <ref cel="okaz.0igi">okazigi</ref>:
      <ekz>
        tiu kompensu, kiu <tld/>is la brulon
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 22:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        malamo <tld/>as malpacon
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 10:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        kolerema homo <tld/>as malpacon
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 15:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        parto de tiuj konsiloj estas ne praktika kaj <tld/>ita de ne sufi&ccirc;a pripensado
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ne povas kompreni tiun &gcirc;ojon, kiun <tld/>as al &scirc;ia bofilo la amaso da
        flavi&gcirc;intaj paperoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        deliro, <tld/>ita de tiu &ccirc;i varmego
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tio]</klr> al via patrino <tld/>is tre grandajn elspezojn
        <fnt>
          <bib>Fab2</bib>
          <lok>El fenestro en Vartou</lok>
        </fnt>;
      </ekz>
      <ekz>
        beda&ubreve;rinde tiu &ccirc;i eraro <tld/>is malkompreni&gcirc;on
        <fnt>
          <bib>LR</bib>
          <lok>2. La verkaro de Zamenhof</lok>
        </fnt>;
      </ekz>
      <ekz>
        pardonu min, sinjorino, ke mi <tld/>is al vi trompi&gcirc;on
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu atakanta vorto <tld/>is al ili doloron
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en Dresden en la 17a de a&ubreve;gusto 1908</vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i kredeble <tld/>as al vi multajn suferojn
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        malgrandaj infanoj <tld/>as laboron, grandaj &dash; doloron
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="port.al0i">alporti</ref>,
      <ref cel="port.en0i">enporti</ref>,
      <ref cel="est.0igi">estigi</ref>,
      <ref cel="tren.kun0i">kuntreni</ref>,
      <ref cel="nask.0i">naski</ref>,
      <ref cel="rezult1.0igi">rezultatigi</ref>,
      <ref cel="sekv.0igi.rezultigi">sekvigi<sncref/></ref>,
      <ref cel="vek.0i">veki</ref>,
      <ref cel="ven">venigi</ref>,
      <ref cel="vok.0i">voki</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_r;&c_y;&c_ch;&c_y;&c_n;&c_ib;&c_c;&c_c;&c_a;
    </trd>,
    <trd>&c_v;&c_y;&c_z;&c_v;&c_a;&c_c;&c_mol;
    <klr>(&c_sh;&c_t;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>beza&ntilde; <ind>abeg</ind> (da)</trd>,       
    <trd>beza&ntilde; <ind>kaoz</ind> (da)</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>begr&uuml;nden</trd>,
    <trd>verursachen</trd>
  </trdgrp>
  <trd lng="en">cause</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_b;&a_b; &a_sh;&a_d;&a_n;</trd>,
    <trd>&a_b;&a_A;&a_ayn;&a_th; &a_sh;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>causer</trd>,
    <trd>donner <ind>lieu</ind> (&agrave;)</trd>,
    <trd>d&eacute;clencher<klr tip="ind"> (occasionner)</klr></trd>,
    <trd>entra&icirc;ner<klr tip="ind"> (occasionner)</klr></trd>,
    <trd>motiver<klr tip="ind"> (causer)</klr></trd>,
    <trd>occasionner</trd>,
    <trd>produire<klr tip="ind"> (occasionner)</klr></trd>,
    <trd>provoquer<klr tip="ind"> (causer)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>okoz</trd>,
    <trd>el&odblac;id&eacute;z</trd>
  </trdgrp>
  <trd lng="nl">veroorzaken</trd>
  <trdgrp lng="pl">
    <trd>powodowa&cacute;</trd>,
    <trd>przyczynia&cacute; si&eogonek;<klr> do czego&sacute;</klr></trd>,
    <trd>by&cacute; <ind>przyczyn&aogonek;</ind><klr> czego&sacute;</klr></trd>,
    <trd>wyrz&aogonek;dza&cacute;</trd>,
    <trd>sprawia&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;
    <klr>(&c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>,
    <trd>&c_p;&c_r;&c_i;&c_ch;&c_i;&c_n;&c_ja;&c_t;&c_mol;
    <klr>(&c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>,
    <trd>&c_p;&c_o;&c_v;&c_l;&c_je;&c_ch;&c_mol;
    &c_z;&c_a; &c_s;&c_o;&c_b;&c_o;&c_j;
    <klr>(&c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>,
    <trd>&c_s;&c_t;&c_a;&c_t;&c_mol;
    &c_p;&c_r;&c_i;&c_ch;&c_i;&c_n;&c_o;&c_j;
    <klr>(&c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>
  </trdgrp>
  <trd lng="sv">orsaka</trd>
</drv>

<drv mrk="kauxz.0o">
  <kap><ofc>*</ofc><tld/>o 
    <fnt><bib>F</bib>, <lok>Universala Vortaro,
    <url ref="&AdE;/fundamento/universala_vortaro.html#av-5057">ka&ubreve;z'o</url></lok>
    </fnt>
  </kap>
  <snc mrk="kauxz.0o.kialo">
    <dif>
      Tio, pro kio io okazas; <ref tip="sin" cel="kial.0o">kialo</ref>:
      <ekz>
        <tld/>o de milito, de malsano;
      </ekz>
      <ekz>
        ne esti sen <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la proceso, al kiu tiu &ccirc;i bela loko donas 
        la <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiam min malbeno ne <ind>mia<tld/>a</ind> 
        <klr>(kies <tld/>o ne estas mi)</klr> subite
        min apartigis de la amatoj<fnt>Z</fnt>;
        <trd lng="fr">de mon <ind>fait</ind></trd>
      </ekz>
      <ekz>
        se ni iam post prepozicio uzas la akuzativon, la akuzativo tie dependas ne de la
        prepozicio, sed de aliaj <tld/>oj
        <fnt>
          <bib>F</bib>
          <lok>&FE; 28</lok>
        </fnt>;
      </ekz>
      <ekz>
        famo ne flugas, se <tld/>o ne estas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ofte de <tld/>o senenhava venas efiko plej grava
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="kauxz.0o.motivo">
    <dif>
      Tio, pro kio oni faras ian agon:
      <ekz>
        mi havis multajn <tld/>ojn, agante tiel;
      </ekz>
      <ekz>
        &scirc;i havis plenan <tld/>on por triumfi
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        mi ekvidas nun la <tld/>on de lia kolero;
      </ekz>
      <ekz>
        doni al iu <tld/>on por plendi<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="font.0o">fonto</ref>,
      <ref cel="motiv.0o">motivo</ref>,
      <ref cel="prav">pravigo</ref>,
      <ref cel="radik.0o.FIG">radiko <sncref/></ref>
    </refgrp>
  </snc>  
  <snc mrk="kauxz.0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Tio, pro kio io estas a&ubreve; i&gcirc;as tia, kiel &gcirc;i
      estas:
      <ekz>
        la unua <tld/>o
        <klr>(anta&ubreve; kiu oni povas koncepti neniun alian)</klr>;
      </ekz>
      <ekz>
        la efika <tld/>o
        <klr>(la fenomeno necese anta&ubreve;a al alia)</klr>;
      </ekz>
      <ekz>
        la cela <tld/>o
        <klr>(la celo, kiun celas inteligenta <tld/>o)</klr>.
      </ekz>
    </dif>
  </snc>          
