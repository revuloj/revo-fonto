<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>odonat</rad>/oj
</kap>
<drv mrk="odonat.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="insekt.0oj">insektoj</ref>
      (<trd lng="la">Odonata</trd>).
    </dif>
    <ref tip="prt" cel="libel.0o">libelo</ref>
  </snc>
  <trd lng="de">Libellen<klr tip="ind"> (Ordnung)</klr></trd>
  <trdgrp lng="nl">
    <trd>libellen</trd>,
    <trd>waternimfen</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_je;&c_k;&c_o;&c_z;&c_y; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
</drv>
</art>
