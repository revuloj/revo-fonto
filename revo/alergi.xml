<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: alergi.xml,v 1.19 2023/10/28 08:33:04 revo Exp $">
<kap>
  <ofc>9</ofc><rad>alergi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="alergi.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="alergi.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">Malsano</ref>,
      ka&ubreve;zita de forta reago de la <ref cel="sistem.imun0o">imunsistemo</ref>
      kontra&ubreve; substancoj, kiuj normale estas sendan&gcirc;eraj, ekzemple 
      man&gcirc;a&jcirc;oj, poleno, polvo ks:
      <ekz>
        oni kalkulas, ke 10% de la monda lo&gcirc;antaro suferas de iu speco
        de <tld/>o
        <fnt>
        <aut>H. Schicke</aut>,
        <vrk>Alergio kaj astmo</vrk>,
        <lok><url ref="http://www.naturkuracado.info">naturkuracado.info</url>,
        2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x654F;&#x611F;&#x75C7; <pr>m&#x1D0;ng&#x1CE;nzh&#xE8;ng</pr></trd>,
    <trd>&#x654F;&#x75C7; <pr>m&#x1D0;nzh&#xE8;ng</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_l;&c_je;&c_r;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_a;&c_l;&c_je;&c_r;&c_g;&c_i;&c_ja;</trd>
  <trd lng="de">Allergie</trd>
  <trd lng="en">allergy</trd>
  <trd lng="es">alergia</trd>
  <trd lng="fr">allergie</trd>
  <trd lng="he">&#x5D0;&#x5B7;&#x5DC;&#x5B6;&#x5E8;&#x5D2;&#x5B4;&#x5D9;&#x5B8;&#x5D4;</trd>
  <trd lng="id">alergi</trd>
  <trd lng="it">allergia</trd>
  <trd lng="ja">&#x30A2;&#x30EC;&#x30EB;&#x30AE;&#x30FC;  <pr>&#x3042;&#x308C;&#x308B;&#x304E;&#x30FC;</pr></trd>
  <trd lng="nl">allergie</trd>
  <trd lng="ru">&c_a;&c_l;&c_l;&c_je;&c_r;&c_g;&c_i;&c_ja;</trd>
  <trd lng="ms">alergi</trd>
  <trd lng="uk">&#x430;&#x43B;&#x435;&#x440;&#x491;&#x456;&#x44F;</trd>
</drv>

<drv mrk="alergi.0a">
  <kap><tld/>a</kap>
  <snc mrk="alergi.0a.FIZL">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a konjunktivito
        <fnt>
        <aut>A. Garc&iacute;a Acosta</aut>,
        <vrk><url ref="http://www.radiosanctispiritus.cu/Version%20Esperanto/Noticias/salud/febrero/19/nueva.htm">Nova
        terapio...</url></vrk>,
        <lok>Radio Sancti Sp&iacute;ritus, 2006-06-05</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_je;&c_r;&c_g;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="bg">&c_a;&c_l;&c_je;&c_r;&c_g;&c_i;&c_ch;&c_je;&c_n;</trd>
  <trd lng="cs">alergick&yacute; <klr>(na)</klr></trd>
  <trd lng="de">allergisch</trd>
  <trd lng="en">allergic</trd>
  <trd lng="es">al&eacute;rgico</trd>
  <trd lng="fr">allergique</trd>
  <trd lng="he">&#x5D0;&#x5B7;&#x5DC;&#x5B6;&#x5E8;&#x5D2;&#x5B4;&#x5D9;&#x5B8;&#x5D4;</trd>
  <trd lng="id">alergis</trd>
  <trd lng="it">allergico</trd>
  <trd lng="nl">allergisch</trd>
  <trd lng="ru">&c_a;&c_l;&c_l;&c_je;&c_r;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">alergick&yacute; <klr>(na)</klr></trd>
  <trd lng="uk">&#x430;&#x43B;&#x435;&#x440;&#x491;&#x456;&#x439;&#x43D;&#x438;&#x439;</trd>
</drv>

<drv mrk="alergi.0iga">
  <kap><tld/>iga</kap>
  <snc mrk="alergi.0iga.FIZL">
    <dif>
      Ka&ubreve;zanta <tld/>ajn reagojn:
      <ekz>
        en la &ccirc;iutaga vivo vi devas bone gardi vin mem kaj ne tu&scirc;i
        la
        facile <tld/>igajn objektojn
        <fnt>
        <aut>&Ccirc;ina Radio Internacia</aut>,
        <vrk><url ref="http://es.chinabroadcast.cn/1/2005/03/18/1@22350.htm">Fruaj
        simptomoj de
        malsanoj</url></vrk>,
        <lok>2005-03-18</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_je;&c_r;&c_g;&c_je;&c_n;&c_n;&c_y;</trd>
  <trd lng="bg">&c_a;&c_l;&c_je;&c_r;&c_g;&c_i;&c_z;&c_i;&c_r;&c_a;&c_shch;</trd>
  <trd lng="de">allergen</trd>
  <trd lng="en">allergenic</trd>
  <trd lng="es">alerg&eacute;nico</trd>
  <trd lng="fr">allerg&egrave;ne<klr> (adj.)</klr></trd>
  <trd lng="it">allergenico</trd>
  <trd lng="nl">allergeen</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_l;&c_l;&c_je;&c_r;&c_g;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_y;&c_z;&c_y;&c_v;&c_a;&c_ju;&c_shch;&c_i;&c_j;
    <ind>&c_a;&c_l;&c_l;&c_je;&c_r;&c_g;&c_i;&c_ju;</ind></trd>
  </trdgrp>
</drv>
</art>
