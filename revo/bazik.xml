        <fnt>
          <vrk><url ref="https://mevamevo.livejournal.com/293236.html"
              >La plej longa vorto...</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">BASIC</trd>
    <trd lng="nl">BASIC</trd>
  </snc>
