<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vant1.xml,v 1.4 2024/04/12 18:25:40 revo Exp $">
<kap>
  <rad>vant</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="vant1.0o">
  <kap><tld/>o</kap>
  <snc mrk="vant1.0o.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      <ref tip="dif" cel="staj.0o.SHI">Stajo</ref>:
      <ekz>
        <nom>Gray</nom> pririgardis la &scirc;ipon, ordonis stre&ccirc;i la <tld/>ojn
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/skarlataj.html">
          Skarlataj veloj</url></vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Want</trd>
    <trdgrp lng="pl">
      <trd>wanta</trd>,
      <trd>lina</trd>
    </trdgrp>
  </snc>
  <rim mrk="vant1.0o.RIM_HOM">
    <ref tip="hom" cel="vant.0o">Vanto</ref>
    fundamente signifas gravigon de bagateloj.
  </rim>
</drv>
</art>

