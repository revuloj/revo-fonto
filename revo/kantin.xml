<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kantin.xml,v 1.23 2023/12/03 08:00:22 revo Exp $">
<kap>
  <rad>kantin</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kantin.0o">
  <kap><tld/>o</kap>
  <snc mrk="kantin.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Komuna, ordinare malmultekosta
      <ref tip="super" cel="mangx.0ejo.KUI">man&gcirc;ejo</ref>
      funkcianta por soldatoj, laboristoj, lernantoj,
      malliberuloj, pensiuloj, lo&gcirc;antaj a&ubreve; laborantaj &ccirc;iuj en sama
      establejo:  
      <ekz>
        envenis servosoldato, al kiu li ordonis a&ccirc;eti du cigaredajn paka&jcirc;etojn en la
        <tld/>o
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o estis preska&ubreve; malplena, supozeble pro la kontra&ubreve;logo 
        de la filmo montrota super &gcirc;i
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la <tld/>o sidis kvinopo laborista
        <fnt>
          <bib>ChB</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="bufed.0ejo">bufedejo</ref>,
       <ref cel="kafeteri.0o">kafeterio</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_l;&c_o;&c_v;&c_a;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_l;&c_o;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_ja;&c_d;&c_a;&c_l;&c_mol;&c_n;&c_ja;</trd>,
    <trd>&c_b;&c_u;&c_f;&c_e;&c_t;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_t;&c_o;&c_l;&c_o;&c_v;&c_a;</trd>
  <trd lng="ca">cantina</trd>
  <trdgrp lng="cs">
    <trd>kant&yacute;na</trd>,
    <trd>z&aacute;vodn&iacute; j&iacute;delna</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kantine</trd>,
    <trd>Mensa</trd>,
    <trd>Betriebsgastst&auml;tte</trd>,
    <trd>Werksgastst&auml;tte</trd>,
    <trd>Casino</trd>
  </trdgrp>
  <trd lng="en">canteen</trd>
  <trd lng="es">cantina</trd>
  <trdgrp lng="fr">
    <trd>cantine</trd>,
    <trd>r&eacute;fectoire</trd>,
    <trd>mess</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>kantin</trd>,
     <trd>&uuml;zemi <ind>&eacute;tkezde</ind></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x69CB;&#x5185;&#x58F2;&#x5E97;  <pr>&#x3053;&#x3046;&#x306A;&#x3044;&#x3070;&#x3044;&#x3066;&#x3093;</pr></trd>,
    <trd>&#x9152;&#x4FDD;  <pr>&#x3057;&#x3085;&#x307B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kantine</trd>,
    <trd>eetlokaal</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kantyna</trd>,
    <trd>sto&lstroke;&oacute;wka</trd>
  </trdgrp>
  <trd lng="ro">cantin&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_u;&c_f;&c_je;&c_t;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_l;&c_o;&c_v;&c_a;&c_ja; <klr>(&c_p;&c_r;&c_i; &c_u;&c_ch;&c_r;&c_je;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_i;,
    &c_z;&c_a;&c_v;&c_o;&c_d;&c_je; &c_i; &c_t;.&c_p;.)</klr></trd>
  </trdgrp>
  <trd lng="sk">z&aacute;vodn&aacute; jed&aacute;le&ncaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x431;&#x443;&#x444;&#x435;&#x442;</trd>,
    <trd>&#x457;&#x434;&#x430;&#x43B;&#x44C;&#x43D;&#x44F;</trd>,
    <trd>&#x43F;&#x435;&#x440;&#x435;&#x441;&#x443;&#x432;&#x43D;&#x430; &#x43F;&#x440;&#x43E;&#x434;&#x43E;&#x432;&#x43E;&#x43B;&#x44C;&#x447;&#x430; &#x43A;&#x440;&#x430;&#x43C;&#x43D;&#x438;&#x446;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9910;&#x5385; <pr>c&#x101;nt&#x12B;ng</pr></trd>,
    <trd>&#x98DF;&#x5802; <pr>sh&#xED;t&#xE1;ng</pr></trd>,
    <trd>&#x98EF;&#x5802; <pr>f&#xE0;nt&#xE1;ng</pr></trd>,
    <trd>&#x4E34;&#x65F6;&#x6D41;&#x52A8;&#x9910;&#x9986; <pr>l&#xED;nsh&#xED;li&#xFA;d&#xF2;ngc&#x101;ngu&#x1CE;n</pr></trd>,
    <trd>&#x519B;&#x4E2D;&#x798F;&#x5229;&#x793E; <pr>j&#x16B;nzh&#x14D;ngf&#xFA;l&#xEC;sh&#xE8;</pr></trd>
  </trdgrp>
</drv>
