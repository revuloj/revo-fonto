<drv mrk="kantin.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Butiko, destinita por speciala aro da personoj, kie oni vendas
      trinka&jcirc;ojn kaj man&gcirc;a&jcirc;ojn al soldatoj, laboristoj, lernantoj,
      malliberuloj, pensiuloj, lo&gcirc;antaj a&ubreve; laborantaj &ccirc;iuj en sama
      establejo.  
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_l;&c_o;&c_v;&c_a;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_l;&c_o;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_ja;&c_d;&c_a;&c_l;&c_mol;&c_n;&c_ja;</trd>,
    <trd>&c_b;&c_u;&c_f;&c_e;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Kantine</trd>,
     <trd>Casino</trd>,
     <trd>Betriebsgastst&auml;tte</trd>,
     <trd>Werksgastst&auml;tte</trd>
  </trdgrp>
  <trd lng="en">canteen</trd>
  <trd lng="es">cantina</trd>
  <trdgrp lng="hu">
     <trd>kantin</trd>,
     <trd>&uuml;zemi <ind>&eacute;tkezde</ind></trd>
  </trdgrp>
  <trd lng="nl">cantine</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_u;&c_f;&c_je;&c_t;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_l;&c_o;&c_v;&c_a;&c_ja; <klr>(&c_p;&c_r;&c_i; &c_u;&c_ch;&c_r;&c_je;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_i;, &c_z;&c_a;&c_v;&c_o;&c_d;&c_je; &c_i; &c_t;.&c_p;.)</klr></trd>
  </trdgrp>
</drv>
