<vortaro>
  <art mrk="$Id: luand.xml,v 1.6 2024/01/15 19:32:08 afido Exp $">
    <kap>
      <rad>Luand</rad>/o
    </kap>

<drv mrk="luand.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc mrk="luand.0o.cxefurbo">    
    <dif>
      <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&Ccirc;efurbo</ref> 
      de <ref tip="malprt" cel="angol.A0o">Angolo</ref>:
      <ekz>
        la portugala grupo <nom>Espirito Santo</nom> ludis pioniran rolon instalante sin en <tld/>o
        tuj post la ekonomia liberaligo de 1993
        <fnt>
          <vrk><url ref="&Mondediplo;article1830.html">Le Monde diplomatique' en Esperanto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        luksaj konstrua&jcirc;oj kreskantaj kiel fungoj en <tld/>o
        <fnt>
          <vrk><url ref="https://eo.globalvoices.org/2020/03/7647/">Kabindo: nigra oro de Angolo &middot; Global Voices en Esperanto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li transportu la sakon kun libroj al <tld/>o
        <fnt>
          <vrk><url ref="https://groups.google.com/d/topic/esperanto-en-angolo/V7MKcqfNt-Y">libroj por angolaj esperantistoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_L;&c_u;&c_a;&c_n;&c_d;&c_a;</trd>
    <trd lng="de">Luanda</trd>
    <trd lng="en">Luanda</trd>
    <trd lng="fr">Luanda</trd>
    <trdgrp lng="zh">
    <trd>&#x7F57;&#x5B89;&#x8FBE; <pr>lu&#xF3;&#x101;nd&#xE1;</pr></trd>,
    <trd>&#x7F85;&#x5B89;&#x9054; <pr>lu&#xF3;&#x101;nd&#xE1;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="ja">&#x30EB;&#x30A2;&#x30F3;&#x30C0;</trd>
  <trd lng="uk">&#x41B;&#x443;&#x430;&#x43D;&#x434;&#x430;</trd>
</drv>

