  <trd lng="cs">v&aacute;&scaron; <klr>(tv&uring;j)</klr></trd>
  <trd lng="da">deres</trd>
  <trd lng="en">your</trd>
  <trdgrp lng="fr">
    <trd>ton</trd>,
    <trd>votre<klr> (sg. et pl.)</klr></trd>
