<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vi</rad> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vi.0">
  <kap><ofc>*</ofc><tld/></kap>
  <gra><vspec>pronomo</vspec></gra>
  <snc mrk="vi.0.KOMUNE">
    <dif>
      La alparolato a&ubreve; alparolatoj (gramatike: dua persono).
    </dif>
    <subsnc mrk="vi.0.unu">
      <dif>
        Ununombre &ndash; la alparolato:
        <ekz>
          sinjoro, <tld/> estas ne&gcirc;entila
          <fnt><bib>F</bib>, <lok>Ekzercaro &FE;16</lok></fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="ci.0">ci</ref>
      </refgrp>
      <trdgrp lng="be">
        <trd>&c_t;&c_y;</trd>,
        <trd>&c_V;&c_y;<klr> (&gcirc;entila)</klr></trd>
      </trdgrp>
      <trdgrp lng="bg">
        <trd>&c_t;&c_i;</trd>,
        <trd>&c_v;&c_i;&c_je;<klr> (&gcirc;entila)</klr></trd>
      </trdgrp>
      <trdgrp lng="br">
        <trd>te</trd>
      </trdgrp>
      <trdgrp lng="ca">
        <trd>tu</trd>,
        <trd>vost&eacute;</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Du</trd>,
        <trd>Sie<klr> (&gcirc;entila)</klr></trd>
      </trdgrp>
      <trdgrp lng="el">
        <trd>(&epsilon;)&sigma;&ypsilon_ton;</trd>,
        <trd>(&epsilon;)&sigma;&epsilon;&jota;&sigma_fina;<klr> (&gcirc;entila)</klr></trd>,
      </trdgrp>
      <trd lng="en">you</trd>
      <trdgrp lng="es">
        <trd>t&uacute;</trd>,
        <trd>usted<klr> (&gcirc;entila)</klr></trd>
      </trdgrp>
      <trdgrp lng="fi">
        <trd>sin&auml;</trd>,
        <trd>te<klr> (&gcirc;entila)</klr></trd>
      </trdgrp>
      <trdgrp lng="he">
        <trd>&alef;&tav;<klr> (fem.)</klr></trd>,
        <trd>&alef;&tav;&he;<klr> (mask.)</klr></trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>te</trd>,
        <trd>ti</trd>,
        <trd>maga</trd>,
        <trd>maguk</trd>,
        <trd>&ouml;n</trd>,
        <trd>&ouml;n&ouml;k</trd>
      </trdgrp>
      <trdgrp lng="id">
        <trd>Anda</trd>,
        <trd>kamu</trd>,
        <trd>kau</trd>,
        <trd>Saudara</trd>
      </trdgrp>
      <trdgrp lng="it">
        <trd>tu</trd>,
        <trd>Lei<klr> (&gcirc;entila)</klr></trd>
      </trdgrp>
      <trd lng="ku">we</trd>
      <trdgrp lng="nl">
        <trd>je</trd>,
        <trd>jij</trd>
        <trd>u<klr> (&gcirc;entila)</klr></trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>ty</trd>,
        <trd>Ty<klr> (&gcirc;entila)</klr></trd>
        <trd>Pan<klr> (&gcirc;entila)</klr></trd>,
        <trd>Pani<klr> (&gcirc;entila)</klr></trd>,
        <trd>Wy<klr> (&gcirc;entila, ar&hcirc;aika)</klr></trd>
      </trdgrp>
      <trdgrp lng="pt">
        <trd>tu</trd>,
        <trd>voc&ecirc;<klr> (bras.)</klr></trd>,
        <trd>o senhor<klr> (&gcirc;entila, mask.)</klr></trd>,
        <trd>a senhora<klr> (&gcirc;entila, fem.)</klr></trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_t;&c_y;</trd>,
        <trd>&c_V;&c_y;<klr> (&gcirc;entila)</klr></trd>
      </trdgrp>
      <trdgrp lng="sk">
        <trd>ty</trd>
        <trd>Vy<klr> (&gcirc;entila)</klr></trd>
      </trdgrp>
      <trdgrp lng="sv">
        <trd>du</trd>,
        <trd>Ni<klr> (&gcirc;entila)</klr></trd>
      </trdgrp>
      <trd lng="tp">sina</trd>
      <trdgrp lng="vo">
        <trd>ol</trd>
      </trdgrp>
      <trdgrp lng="tr">
        <trd>sen</trd>,
        <trd>siz<klr> (&gcirc;entila)</klr></trd>
      </trdgrp>
      <trdgrp lng="zh">
        <trd>&#20320;<klr> [n&#464;]</klr></trd>,
        <trd>&#24744;<klr> [n&iacute;n]</klr><klr> (&gcirc;entila)</klr></trd>,
        <trd>&#20320;&#20204;</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="vi.0.plur">
      <dif>
        Multenombre &ndash; la alparolatoj:
        <ekz>
          <tld/> estas infanoj
          <fnt><bib>F</bib>, <lok>Ekzercaro &FE;16</lok></fnt>.
        </ekz>
      </dif>
      <trdgrp lng="be">
        <trd>&c_v;&c_y;</trd>
      </trdgrp>
      <trdgrp lng="bg">
        <trd>&c_v;&c_i;&c_je;</trd>
      </trdgrp>
      <trdgrp lng="br">
        <trd>c'hwi</trd>
      </trdgrp>
      <trdgrp lng="ca">
        <trd>vosaltres</trd>,
        <trd>vost&eacute;s</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Ihr</trd>,
        <trd>Sie<klr> (&gcirc;entila)</klr></trd>
      </trdgrp>
      <trdgrp lng="el">
        <trd>(&epsilon;)&sigma;&epsilon;&jota;&sigma_fina;</trd>,
      </trdgrp>
      <trd lng="en">you</trd>
      <trdgrp lng="es">
        <trd>vosotros</trd>,
        <trd>ustedes<klr> (&gcirc;entila)</klr></trd>
      </trdgrp>
      <trdgrp lng="fi">
        <trd>sin&auml;</trd>,
        <trd>te</trd>
      </trdgrp>
      <trdgrp lng="he">
        <trd>&alef;&tav;&fnun;<klr> (fem.)</klr></trd>,
        <trd>&alef;&tav;&fmem;<klr> (mask.)</klr></trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>te</trd>,
        <trd>ti</trd>,
        <trd>maga</trd>,
        <trd>maguk</trd>,
        <trd>&ouml;n</trd>,
        <trd>&ouml;n&ouml;k</trd>
      </trdgrp>
      <trd lng="id">kalian</trd>
      <trdgrp lng="it">
        <trd>voi</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>jullie</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>wy</trd>,
        <trd>Wy<klr> (&gcirc;entila)</klr></trd>
      </trdgrp>
      <trdgrp lng="pt">
        <trd>voc&ecirc;s</trd>,
        <trd>os senhores<klr> (&gcirc;entila, mask.)</klr></trd>,
        <trd>as senhoras<klr> (&gcirc;entila, fem.)</klr></trd>,
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_v;&c_y;</trd>
      </trdgrp>
      <trdgrp lng="sk">
        <trd>vy</trd>,
        <trd>Vy<klr> (&gcirc;entila)</klr></trd>
      </trdgrp>
      <trdgrp lng="sv">
        <trd>ni</trd>
      </trdgrp>
      <trd lng="tp">sina</trd>
      <trdgrp lng="vo">
        <trd>ols</trd>
      </trdgrp>
      <trdgrp lng="tr">
        <trd>siz</trd>
      </trdgrp>
      <trdgrp lng="zh">
        <trd>&#20320;&#20204;</trd>,
        <trd><klr>tr. </klr>&#20320;&#20497;<klr> [n&#464;men]</klr></trd>
      </trdgrp>
    </subsnc>
  </snc>
  <trd lng="cs">vy <klr>(ty)</klr></trd>
  <trdgrp lng="fr">
    <trd>tu</trd>,
    <trd>vous<klr> (sg. et pl.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="vi.0a">
  <kap><tld/>a</kap>
  <snc mrk="vi.0a.KOMUNE">
    <dif>
      Posedata de <tld/>, rilata al <tld/>.
    </dif>
    <subsnc mrk="vi.0a.unu">
      <dif>
        Ununombre &ndash; rilata al unu alparolato:
        <ekz>
          <tld/> eraras, sinjoro: <tld/>a pano
          estas malpli fre&scirc;a, ol mia
          <fnt><bib>F</bib>, <lok>Ekzercaro &FE;10</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="ca">teu</trd>
      <trdgrp lng="id">
        <trd><ind>milik</ind>mu</trd>,
        <trd><ind>milik</ind> Anda</trd>
      </trdgrp>
      <trd lng="pl">tw&oacute;j</trd>
      <trdgrp lng="ru">
        <trd>&c_t;&c_v;&c_o;&c_j;</trd>
        <trd>&c_v;&c_a;&c_sh;<klr> (&gcirc;entila)</klr></trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="vi.0a.plur">
      <dif>
        Multenombre &ndash; rilata al pluraj alparolatoj:
        <ekz>
          esperantistoj en Anglujo <klr>...</klr>
          granda kaj grava estas <tld/>a rolo
          <fnt><bib>Paroloj</bib>, <lok>Gramofona parolado de okaze de
          la unua datreveno de la fondo de la Londona Esperanto-Klubo,
          1904</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="ca">vostre</trd>
      <trd lng="id"><ind>milik</ind> kalian</trd>
      <trd lng="pl">wasz</trd>
      <trdgrp lng="ru">
        <trd>&c_t;&c_v;&c_o;&c_i;</trd>
        <trd>&c_v;&c_a;&c_sh;&c_i;<klr> (&gcirc;entila)</klr></trd>
      </trdgrp>
    </subsnc>
  </snc>
  <trd lng="cs">váš <klr>(tvůj)</klr></trd>
  <trdgrp lng="fr">
    <trd>ton</trd>,
    <trd>votre<klr> (sg. et pl.)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>tvoj</trd>,
    <trd>váš</trd>
  </trdgrp>
</drv>

<drv mrk="vi.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vi.0umi.alparole">
    <dif>
      Alparoli per &leftquot;<tld/>&rightquot;:
      <ekz>
        mi havis matematikan fu&scirc;instruiston<klr>...</klr>, 
        kiu unue <tld/>umis &ccirc;iujn lernejanojn e&ccirc; mian tiam 
        10-jaran frateton
        <fnt>
          <aut>darkweasel</aut>,
          <vrk><url
          ref="http://en.lernu.net/komunikado/forumo/temo.php?t=6289">
          Ciumado kaj viumado</url></vrk>,
          <lok>lernu.net, 2010-04-18</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>voussoyer</trd>,
    <trd>vouvoyer</trd>
  </trdgrp>
</drv>
</art>
