<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vi</rad>
</kap>
<drv mrk="vi.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Pronomo de dua persono (unu- a&ubreve; multe-nombra),
      uzata de la parolanto por paroli pri la personoj a&ubreve; personoj,
      al kiuj li parolas.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_y;</trd> (unu-nombra),
    <trd>&c_V;&c_y;</trd> (unu-nombra, &gcirc;entila),
    <trd>&c_v;&c_y;</trd> (multe-nombra)
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_t;&c_i;</trd> (unu-nombra),
    <trd>&c_v;&c_i;&c_je;</trd> (multe-nomrba, &gcirc;entila)
  </trdgrp>
  <trdgrp lng="de">
    <trd>Du</trd> (unu-nombra),
    <trd>Sie</trd> (unu-nombra, &gcirc;entila),
    <trd>Ihr</trd> (multe-nombra)
    <trd>Sie</trd> (multe-nombra, &gcirc;entila),
  </trdgrp>
  <trdgrp lng="el">
    <trd>(&epsilon;)&sigma;&ypsilon_ton;</trd> (unu-nombra),
    <trd>(&epsilon;)&sigma;&epsilon;&jota;&sigma_fina;</trd> (unu-nombra, &gcirc;entila kaj multe-nombra),
  </trdgrp>
  <trd lng="en">you</trd>
  <trdgrp lng="es">
    <trd>t&uacute;</trd> (unu-nombra),
    <trd>usted</trd> (unu-nombra, &gcirc;entila),
    <trd>vosotros</trd> (multe-nombra),
    <trd>ustedes</trd> (multe-nombra, &gcirc;entila)
  </trdgrp>
  <trdgrp lng="fr">
    <trd>tu</trd>,
    <trd>vous<klr> (sing. et pl.)</klr></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&alef;&tav;</trd> (unu-nombra, fem.),
    <trd>&alef;&tav;&he;</trd> (unu-nombra, mask.),
    <trd>&alef;&tav;&fnun;</trd> (multe-nombra, fem.),
    <trd>&alef;&tav;&fmem;</trd> (multe-nombra, mask.)
  </trdgrp>
  <trdgrp lng="it">
    <trd>tu</trd> (unu-nombra),
    <trd>Lei</trd> (unu-nombra, &gcirc;entila),
    <trd>voi</trd> (multe-nombra)
  </trdgrp>
  <trdgrp lng="nl">
    <trd>je</trd> (unu-nombra),
    <trd>jij</trd> (unu-nombra),
    <trd>jullie</trd> (multe-nombra),
    <trd>u</trd> (&gcirc;entila)
  </trdgrp>
  <trdgrp lng="pt">
    <trd>tu</trd> (unu-nombra),
    <trd>voc&ecirc;</trd> (unu-nombra, bras.),
    <trd>o senhor</trd> (unu-nombra, &gcirc;entila, mask.),
    <trd>a senhora</trd> (unu-nombra, &gcirc;entila, fem.),
    <trd>voc&ecirc;s</trd> (multe-nombra),
    <trd>os senhores</trd> (multe-nombra, &gcirc;entila, mask.),
    <trd>as senhoras</trd> (multe-nombra, &gcirc;entila, fem.),
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_y;</trd> (unu-nombra),
    <trd>&c_V;&c_y;</trd> (unu-nombra, &gcirc;entila),
    <trd>&c_v;&c_y;</trd> (multe-nombra)
  </trdgrp>
  <trdgrp lng="tr">
    <trd>sen</trd> (unu-nombra),
    <trd>siz</trd> (multe-nombra a&ubreve; &gcirc;entila)
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ty</trd> (unu-nombra),
    <trd>vy</trd> (multe-nombra),
    <trd>Vy</trd> (&gcirc;entila)
  </trdgrp>
  <trdgrp lng="sv">
    <trd>du</trd> (unu-nombra),
    <trd>Ni</trd> (unu-nombra, &gcirc;entila),
    <trd>ni</trd> (multe-nombra)
  </trdgrp>
  <trdgrp lng="vo">
    <trd>ol</trd> [unu-nombra],
    <trd>ols</trd> [multe-nombra]
  </trdgrp>
</drv>
</art>
