  <trd lng="hy">&#x571;&#x565;&#x580;</trd>
  <trdgrp lng="sk">
    <trd>tvoj</trd>,
    <trd>v&aacute;&scaron;</trd>
  </trdgrp>
  <trd lng="sq">tuaj</trd>
  <trd lng="te">&#xC2E;&#xC40;</trd>
  <trd lng="tl">iyong</trd>
  <trd lng="ur">&#x622;&#x67E;</trd>
  <trd lng="vi">c&#x1EE7;a b&#x1EA1;n</trd>
</drv>

<drv mrk="vi.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vi.0umi.alparole">
    <dif>
      Alparoli per &leftquot;<tld/>&rightquot;:
      <ekz>
        mi havis matematikan fu&scirc;instruiston<klr>...</klr>, 
        kiu unue <tld/>umis &ccirc;iujn lernejanojn e&ccirc; mian tiam 
        10-jaran frateton
        <fnt>
          <aut>darkweasel</aut>,
          <vrk><url ref="http://en.lernu.net/komunikado/forumo/temo.php?t=6289">
          Ciumado kaj viumado</url></vrk>,
          <lok>lernu.net, 2010-04-18</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>voussoyer</trd>,
    <trd>vouvoyer</trd>
  </trdgrp>
  <trd lng="nl">vousvoyeren</trd>
</drv>
