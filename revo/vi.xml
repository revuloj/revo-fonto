<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vi</rad>
</kap>
<drv mrk="vi.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Pronomo de dua persono (unu- a&ubreve; multe-nombra),
      uzata de la parolanto por paroli pri la personoj a&ubreve; personoj,
      al kiuj li parolas.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_y;<klr> (unu-nombra)</klr></trd>,
    <trd>&c_V;&c_y;<klr> (unu-nombra, &gcirc;entila)</klr></trd>,
    <trd>&c_v;&c_y;<klr> (multe-nombra)</klr></trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_t;&c_i;<klr> (unu-nombra)</klr></trd>,
    <trd>&c_v;&c_i;&c_je;<klr> (multe-nombra, &gcirc;entila)</klr></trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>te<klr> (unu-nombra)</klr></trd>,
    <trd>c'hwi<klr> (multe-nombra)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Du<klr> (unu-nombra)</klr></trd>,
    <trd>Sie<klr> (unu-nombra, &gcirc;entila)</klr></trd>,
    <trd>Ihr<klr> (multe-nombra)</klr></trd>,
    <trd>Sie<klr> (multe-nombra, &gcirc;entila)</klr></trd>
  </trdgrp>
  <trdgrp lng="el">
    <trd>(&epsilon;)&sigma;&ypsilon_ton;<klr> (unu-nombra)</klr></trd>,
    <trd>(&epsilon;)&sigma;&epsilon;&jota;&sigma_fina;<klr> (unu-nombra, &gcirc;entila kaj multe-nombra)</klr></trd>,
  </trdgrp>
  <trd lng="en">you</trd>
  <trdgrp lng="es">
    <trd>t&uacute;<klr> (unu-nombra)</klr></trd>,
    <trd>usted<klr> (unu-nombra, &gcirc;entila)</klr></trd>,
    <trd>vosotros<klr> (multe-nombra)</klr></trd>,
    <trd>ustedes<klr> (multe-nombra, &gcirc;entila)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>tu</trd>,
    <trd>vous<klr> (sing. et pl..)</klr></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&alef;&tav;<klr> (unu-nombra, fem.)</klr></trd>,
    <trd>&alef;&tav;&he;<klr> (unu-nombra, mask.)</klr></trd>,
    <trd>&alef;&tav;&fnun;<klr> (multe-nombra, fem.)</klr></trd>,
    <trd>&alef;&tav;&fmem;<klr> (multe-nombra, mask.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>te</trd>,
    <trd>ti</trd>,
    <trd>maga</trd>,
    <trd>maguk</trd>,
    <trd>&ouml;n</trd>,
    <trd>&ouml;n&ouml;k</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>tu<klr> (unu-nombra)</klr></trd>,
    <trd>Lei<klr> (unu-nombra, &gcirc;entila)</klr></trd>,
    <trd>voi<klr> (multe-nombra)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>je<klr> (unu-nombra)</klr></trd>,
    <trd>jij<klr> (unu-nombra)</klr></trd>,
    <trd>jullie<klr> (multe-nombra)</klr></trd>,
    <trd>u<klr> (&gcirc;entila)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>tu<klr> (unu-nombra)</klr></trd>,
    <trd>voc&ecirc;<klr> (unu-nombra, bras.)</klr></trd>,
    <trd>o senhor<klr> (unu-nombra, &gcirc;entila, mask.)</klr></trd>,
    <trd>a senhora<klr> (unu-nombra, &gcirc;entila, fem.)</klr></trd>,
    <trd>voc&ecirc;s<klr> (multe-nombra)</klr></trd>,
    <trd>os senhores<klr> (multe-nombra, &gcirc;entila, mask.)</klr></trd>,
    <trd>as senhoras<klr> (multe-nombra, &gcirc;entila, fem.)</klr></trd>,
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_y;<klr> (unu-nombra)</klr></trd>,
    <trd>&c_V;&c_y;<klr> (unu-nombra, &gcirc;entila)</klr></trd>,
    <trd>&c_v;&c_y;<klr> (multe-nombra)</klr></trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>sen<klr> (unu-nombra)</klr></trd>,
    <trd>siz<klr> (multe-nombra a&ubreve; &gcirc;entila)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ty<klr> (unu-nombra)</klr></trd>,
    <trd>vy<klr> (multe-nombra)</klr></trd>,
    <trd>Vy<klr> (&gcirc;entila)</klr></trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>du<klr> (unu-nombra)</klr></trd>,
    <trd>Ni<klr> (unu-nombra, &gcirc;entila)</klr></trd>,
    <trd>ni<klr> (multe-nombra)</klr></trd>
  </trdgrp>
  <trdgrp lng="vo">
    <trd>ol<klr> (unu-nombra)</klr></trd>,
    <trd>ols<klr> (multe-nombra)</klr></trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20320;<klr> [n&#464;]</klr><klr> (unu-nombra)</klr></trd>,
    <trd>&#24744;<klr> [n&iacute;n]</klr><klr> (unu-nombra, &gcirc;entila)</klr></trd>,
    <trd>&#20320;&#20204;</trd>,
    <trd><klr>tr. </klr>&#20320;&#20497;<klr> [n&#464;men]</klr><klr> (multe-nombra)</klr></trd>
  </trdgrp>
</drv>
</art>
