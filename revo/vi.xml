<drv mrk="vi.0a">
  <kap><ofc>*</ofc><tld/>a <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5250">vi'a</url></lok></fnt></kap>
  <snc mrk="vi.0a.KOMUNE">
    <dif>
      Posedata de <tld/>, rilata al <tld/>.
    </dif>
    <subsnc mrk="vi.0a.unu">
      <dif>
        Ununombre &ndash; rilata al unu alparolato:
        <ekz>
          <tld/> eraras, sinjoro: <tld/>a pano
          estas malpli fre&scirc;a, ol mia
          <fnt><bib>F</bib>, <lok>Ekzercaro &FE;10</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="ca">teu</trd>
      <trdgrp lng="fy">
        <trd>dyn</trd>,
        <trd>jo<klr> (&gcirc;entila)</klr></trd>
      </trdgrp>
      <trdgrp lng="id">
        <trd><ind>milik</ind>mu</trd>,
        <trd><ind>milik</ind> Anda</trd>
      </trdgrp>
      <trdgrp lng="io">
        <trd>tua</trd>,
        <trd>vua<klr> (&gcirc;entila)</klr></trd>
      </trdgrp>
      <trdgrp lng="ja">
        <trd>&#x3042;&#x306A;&#x305F;&#x306E;</trd>,
        <trd>&#x541B;&#x306E; [<ind>&#x304D;&#x307F;&#x306E;</ind>]</trd>,
        <trd>&#x304A;&#x524D;&#x306E; [<ind>&#x304A;&#x307E;&#x3048;&#x306E;</ind>]</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>jouw</trd>,
        <trd>je</trd>,
        <trd>uw<klr> (&gcirc;entila)</klr></trd>
      </trdgrp>
      <trd lng="pl">tw&oacute;j</trd>
      <trdgrp lng="ru">
        <trd>&c_t;&c_v;&c_o;&c_j;</trd>,
        <trd>&c_v;&c_a;&c_sh;<klr> (&gcirc;entila)</klr></trd>
      </trdgrp>
      <trdgrp lng="sv">
        <trd>din</trd>,
        <trd>ditt</trd>,
        <trd>dina</trd>
      </trdgrp>
      <trd lng="tp">sina</trd>
      <trdgrp lng="vo">
        <trd>olik</trd>,
        <trd>ola</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="vi.0a.plur">
      <dif>
        Multenombre &ndash; rilata al pluraj alparolatoj:
        <ekz>
          esperantistoj en Anglujo <klr>...</klr>
          granda kaj grava estas <tld/>a rolo
          <fnt><bib>Paroloj</bib>, <lok>Gramofona parolado de okaze de
          la unua datreveno de la fondo de la Londona Esperanto-Klubo,
          1904</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="ca">vostre</trd>
      <trdgrp lng="fy">
        <trd>jim</trd>,
        <trd>jo<klr> (&gcirc;entila)</klr></trd>
      </trdgrp>
      <trd lng="id"><ind>milik</ind> kalian</trd>
      <trd lng="io">via</trd>
      <trdgrp lng="ja">
        <trd>&#x3042;&#x306A;&#x305F;&#x305F;&#x3061;&#x306E;</trd>,
        <trd>&#x3042;&#x306A;&#x305F;&#x304C;&#x305F;&#x306E;</trd>,
        <trd>&#x541B;&#x305F;&#x3061;&#x306E;
        [<ind>&#x304D;&#x307F;&#x305F;&#x3061;&#x306E;</ind>]</trd>,
        <trd>&#x304A;&#x524D;&#x3089;&#x306E;
        [<ind>&#x304A;&#x307E;&#x3048;&#x3089;&#x306E;</ind>]</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>jullie</trd>,
        <trd>je</trd>,
        <trd>uw<klr> (&gcirc;entila)</klr></trd>
      </trdgrp>
      <trd lng="pl">wasz</trd>
      <trdgrp lng="ru">
        <trd>&c_t;&c_v;&c_o;&c_i;</trd>,
        <trd>&c_v;&c_a;&c_sh;&c_i;<klr> (&gcirc;entila)</klr></trd>
      </trdgrp>
      <trdgrp lng="sv">
        <trd>er</trd>,
        <trd>ert</trd>,
        <trd>era</trd>
      </trdgrp>
      <trdgrp lng="tp">
        <trd>sina<klr> (ajnnombra)</klr></trd>,
        <trd>pi sina tu<klr> (dunombra)</klr></trd>,
        <trd>pi sina mute<klr> (plurnombra)</klr></trd>
      </trdgrp>
      <trdgrp lng="vo">
        <trd>olsik</trd>,
        <trd>olsa</trd>
      </trdgrp>
    </subsnc>
  </snc>
  <trd lng="af">jou</trd>
  <trd lng="am">&#x12EB;&#x1295;&#x1270;</trd>
  <trd lng="ar">&#x643;</trd>
  <trd lng="az">sizin</trd>
