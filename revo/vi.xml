<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>*</ofc>
    <rad>vi</rad>
</kap>
<drv mrk="vi.0">
    <kap><ofc>*</ofc><tld/></kap>
    <snc mrk="vi.0.unu">
      <dif>
        Pronomo de dua persono (ununombra),
        uzata de la parolanto por paroli pri la persono,
        al kiuj li parolas.
      </dif>
    <trdgrp lng="be">
      <trd>&c_t;&c_y;</trd>,
      <trd>&c_V;&c_y;<klr> (&gcirc;entila)</klr></trd>
    </trdgrp>
    <trdgrp lng="bg">
      <trd>&c_t;&c_i;</trd>,
      <trd>&c_v;&c_i;&c_je;<klr> (&gcirc;entila)</klr></trd>
    </trdgrp>
    <trdgrp lng="br">
      <trd>te</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Du</trd>,
      <trd>Sie<klr> (&gcirc;entila)</klr></trd>
    </trdgrp>
    <trdgrp lng="el">
      <trd>(&epsilon;)&sigma;&ypsilon_ton;</trd>,
      <trd>(&epsilon;)&sigma;&epsilon;&jota;&sigma_fina;<klr>
(&gcirc;entila)</klr></trd>,
    </trdgrp>
    <trd lng="en">you</trd>
    <trdgrp lng="es">
      <trd>t&uacute;</trd>,
      <trd>usted<klr> (&gcirc;entila)</klr></trd>
    </trdgrp>
    <trdgrp lng="fi">
      <trd>sin&auml;</trd>,
      <trd>te<klr> (&gcirc;entila)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>tu</trd>,
      <trd>vous<klr> (sing. et pl.)</klr></trd>
    </trdgrp>
    <trdgrp lng="he">
      <trd>&alef;&tav;<klr> (fem.)</klr></trd>,
      <trd>&alef;&tav;&he;<klr> (mask.)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>te</trd>,
      <trd>ti</trd>,
      <trd>maga</trd>,
      <trd>maguk</trd>,
      <trd>&ouml;n</trd>,
      <trd>&ouml;n&ouml;k</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>tu</trd>,
      <trd>Lei<klr> (&gcirc;entila)</klr></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>je</trd>,
      <trd>jij</trd>
      <trd>u<klr> (&gcirc;entila)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>ty</trd>,
      <trd>Ty<klr> (&gcirc;entila)</klr></trd>
      <trd>Pan<klr> (&gcirc;entila)</klr></trd>,
      <trd>Pani<klr> (&gcirc;entila)</klr></trd>,
      <trd>Wy<klr> (&gcirc;entila, ar&hcirc;aika)</klr></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>tu</trd>,
      <trd>voc&ecirc;<klr> (bras.)</klr></trd>,
      <trd>o senhor<klr> (&gcirc;entila, mask.)</klr></trd>,
      <trd>a senhora<klr> (&gcirc;entila, fem.)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_t;&c_y;</trd>,
      <trd>&c_V;&c_y;<klr> (&gcirc;entila)</klr></trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>ty</trd>
      <trd>Vy<klr> (&gcirc;entila)</klr></trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>du</trd>,
      <trd>Ni<klr> (&gcirc;entila)</klr></trd>
    </trdgrp>
    <trd lng="tp">sina</trd>
    <trdgrp lng="vo">
      <trd>ol</trd>
    </trdgrp>
    <trdgrp lng="tr">
      <trd>sen</trd>,
      <trd>siz<klr> (&gcirc;entila)</klr></trd>
    </trdgrp>
    <trdgrp lng="zh">
      <trd>&#20320;<klr> [n&#464;]</klr></trd>,
      <trd>&#24744;<klr> [n&iacute;n]</klr><klr>
(&gcirc;entila)</klr></trd>,
      <trd>&#20320;&#20204;</trd>
    </trdgrp>
  </snc>
  
  <snc mrk="vi.0.plur">
      <dif>
        Pronomo de dua persono (multe-nombra),
        uzata de la parolanto por paroli pri la personoj,
        al kiuj li parolas.
      </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="bg">
      <trd>&c_v;&c_i;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="br">
      <trd>c'hwi</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Ihr</trd>,
      <trd>Sie<klr> (&gcirc;entila)</klr></trd>
    </trdgrp>
    <trdgrp lng="el">
      <trd>(&epsilon;)&sigma;&epsilon;&jota;&sigma_fina;</trd>,
    </trdgrp>
    <trd lng="en">you</trd>
    <trdgrp lng="es">
      <trd>vosotros</trd>,
      <trd>ustedes<klr> (&gcirc;entila)</klr></trd>
    </trdgrp>
    <trdgrp lng="fi">
      <trd>sin&auml;</trd>,
      <trd>te</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>vous</trd>
    </trdgrp>
    <trdgrp lng="he">
      <trd>&alef;&tav;&fnun;<klr> (fem.)</klr></trd>,
      <trd>&alef;&tav;&fmem;<klr> (mask.)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>te</trd>,
      <trd>ti</trd>,
      <trd>maga</trd>,
      <trd>maguk</trd>,
      <trd>&ouml;n</trd>,
      <trd>&ouml;n&ouml;k</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>voi</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>jullie</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>wy</trd>,
      <trd>Wy<klr> (&gcirc;entila)</klr></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>voc&ecirc;s</trd>,
      <trd>os senhores<klr> (&gcirc;entila, mask.)</klr></trd>,
      <trd>as senhoras<klr> (&gcirc;entila, fem.)</klr></trd>,
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>vy</trd>,
      <trd>Vy<klr> (&gcirc;entila)</klr></trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>ni</trd>
    </trdgrp>
    <trd lng="tp">sina</trd>
    <trdgrp lng="vo">
      <trd>ols</trd>
    </trdgrp>
    <trdgrp lng="tr">
      <trd>siz</trd>
    </trdgrp>
    <trdgrp lng="zh">
      <trd>&#20320;&#20204;</trd>,
      <trd><klr>tr. </klr>&#20320;&#20497;<klr> [n&#464;men]</klr></trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="vi.0a">
    <kap><tld/>a</kap>
    <snc mrk="vi.0a.unu">
      <dif>
        Poseda adjektivo a&ubreve; pronomo signifanta:
&leftquot;posedata de vi, rilata al vi&rightquot; 
        (unu-nombra).
      </dif>
    <trd lng="pl">tw&oacute;j</trd>
    </snc>
    
    <snc mrk="vi.0a.plur">
      <dif>
        Poseda adjektivo a&ubreve; pronomo signifanta:
&leftquot;posedata de vi, rilata al vi&rightquot; 
        (multe-nombra).
      </dif>
      <trd lng="pl">wasz</trd>
    </snc>
</drv>
</art>
