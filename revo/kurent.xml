  <snc>
    <dif>
      Fluo de <ref tip="sin" cel="elektr.0o">elektro</ref> tra
      <ref cel="konduk.0ilo.FIZ">kondukilo</ref>:
      <ekz>
        altatensia <tld/>o
        <fnt>
          <bib>CKv</bib>
          <lok>19</lok>
        </fnt>;
      </ekz>
      <ekz>
        elektraj kuiriloj estas sen <tld/>o, &ccirc;ar ili forglitigas la &scirc;altilon
        <fnt>
          <bib>KKr</bib>
          <lok>II</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        iliajn cerbojn konfuzis la stranga <tld/>o, kiun mi nomas timo
        <fnt>
          <bib>VaK</bib>
          <lok>Oka &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam ekmankas la <tld/>o, nur tiam mi konscias pri la nombro de aparatoj kiuj
        &gcirc;in uzas
        <fnt>
          <bib>OIP</bib>
          <lok>Ombro sur interna pejza&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        3300 kilovathoroj da <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2005/008409.php">Rubo neniu dubo!</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        unufaza, trifaza <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2013/011099.php">Ne plu &scirc;ar&gcirc;i sed &scirc;argi</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
