  <trd lng="cs">stejnosm&ecaron;rn&yacute; proud</trd>
</drv>

    <drv mrk="kurent.konstanta0o">
      <kap>konstanta <tld/>o</kap>
      <uzo tip="fak">ELE</uzo>
