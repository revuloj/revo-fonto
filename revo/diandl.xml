<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: diandl.xml,v 1.5 2023/11/04 14:29:01 revo Exp $">
<kap>
    <rad>diandl</rad>/o <fnt><bib>Viki</bib></fnt>,
    <var><kap><rad var="r">dirndl</rad>/o</kap></var>
</kap>
<drv mrk="diandl.0o">
  <kap><tld/>o, <var><kap><tld var="r"/>o</kap></var></kap>
  <snc mrk="diandl.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Tradicia <ref tip="vid" cel="rob.0o">robo</ref> portata
      en suda <ref tip="vid" cel="german1.G0ujo">Germanujo</ref>, aparte en
      Bavarujo, A&ubreve;strujo kaj Sudtirolo, surbaze de la tradicia vesta&jcirc;o de alpaj
      kamparanoj:
      <ekz>
        knabinoj povus turni en siaj buntaj <tld/>oj kiam knaboj montregis siajn
        &leftquot;<nac>Platteln</nac>&rightquot;
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=564225">Schuhplattler</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        hodia&ubreve;, <nac>Lederhosen</nac> <klr>(ledpantalonoj)</klr> kaj <tld var="r"/>o estas kutimaj
        &ccirc;e Oktobrofestoj &ccirc;irka&ubreve; la mondo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=564188">Lederhosen</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/6/63/Dirndl-2011.JPG/319px-Dirndl-2011.JPG" prm="CC BY-SA 3.0">virino en <tld/>o kaj verda anta&ubreve;tuko
        <fnt>
          <aut>Nemoralis, propra verko</aut>
          <lok><url ref="&WCW;/File:Dirndl-2011.JPG">https://commons.wikimedia.org/wiki/File:Dirndl-2011.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="de">
    <trd>Diandl</trd>,
    <trd>Dirndl</trd>
  </trdgrp>
  <trd lng="fr">costume bavarois</trd>
  <trd lng="uk">&c_b;&c_a;&c_v;&c_a;&c_r;&c_s;&#x44C;&#x43A;&c_je; &#x43D;&c_a;&c_c;&c_ib;&#x43E;&#x43D;&c_a;&#x43B;&#x44C;&#x43D;&c_je;
  &#x43B;&c_ib;&c_t;&#x43D;&c_jeu; &#x43F;&#x43B;&c_a;&c_t;&c_t;&#x44F;</trd>
  <trd lng="zh">&#x7D27;&#x8EAB;&#x8FDE;&#x8863;&#x88D9; <pr>j&#x1D0;nsh&#x113;nli&#xE1;ny&#x12B;q&#xFA;n</pr></trd>
</drv>
</art>

