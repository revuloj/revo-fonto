<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>diandl</rad>/o <fnt><bib>Viki</bib></fnt>
</kap>
<drv mrk="diandl.0o">
  <kap><tld/>o, <var><kap>dirndlo</kap></var></kap>
  <snc mrk="diandl.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Speco de tradicia <ref tip="vid" cel="rob.0o">robo</ref> portata
      en <ref tip="vid" cel="german1.G0ujo">Germanujo</ref>, aparte en
      Bavarujo,
      A&ubreve;strujo kaj Sudtirolo, surbaze de la tradicia vesta&jcirc;o de alpaj
      kamparanoj. 
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Diandl</trd>,
    <trd>Dirndl</trd>
  </trdgrp>
  <trd lng="fr">costume bavarois</trd>
</drv>
</art>

