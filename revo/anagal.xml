<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: anagal.xml,v 1.22 2023/08/30 18:23:34 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>anagal</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="anagal.0o">
  <kap><tld/>o</kap>
  <snc mrk="anagal.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="primol1.0oj">primolacoj</ref>
      (<trd lng="la">Anagallis</trd>), herbo
      kun ru&gcirc;aj a&ubreve; bluaj floretoj, venena al birdoj:
      <ekz>
        li trovis la ordinarajn ru&gcirc;an kaj bluan <tld/>ojn
        <klr>[&#x2026;]</klr> absolute malfekundaj inter si
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 8a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="&WCU;/thumb/1/1d/Anagallis_arvensis01.jpg/320px-Anagallis_arvensis01.jpg" prm="CC BY   2.5"><ind>ru&gcirc;a anagalo</ind> (Anagallis arvensis)
       <fnt>
         <aut>Ulf Eliasson, propra verko</aut>
         <lok><url ref="&WCW;/File:Aquilegia_ottonis_amaliae2UME.jpg">&WCW;/File:Aquilegia_ottonis_amaliae2UME.jpg</url></lok>
       </fnt>
    <trdgrp lng="de">
      <trd>Acker-Gauchheil</trd>,
      <trd>Roter Gauchheil</trd>
    </trdgrp>
    <trd lng="en">scarlet pimpernel</trd>
    <trd lng="hu">mezei <ind>tikszem</ind></trd>
    <trd lng="la">Anagallis arvensis</trd>
    <trd lng="sv">r&ouml;dmire</trd>
  </bld>
  <trdgrp lng="be">
    <trd>&c_ch;&c_y;&c_r;&c_v;&c_o;&c_n;&c_k;&c_a; &c_p;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_ja;</trd>,
    <trd>&c_m;&c_a;&c_k;&c_r;&c_y;&c_c;&c_a; &c_ch;&c_y;&c_r;&c_v;&c_o;&c_n;&c_a;&c_ja;</trd>
  </trdgrp>
  <trd lng="bg">&c_o;&c_g;&c_n;&c_i;&c_v;&c_ch;&c_je;</trd>
  <trd lng="de">Gauchheil</trd>
  <trd lng="en">pimpernel</trd>
  <trd lng="fr">mouron</trd>
  <trd lng="he">&#x5DE;&#x5E8;&#x5D2;&#x5E0;&#x5D9;&#x5EA;</trd>
  <trd lng="hu">tikszem</trd>
  <trd lng="ja">&#x30EB;&#x30EA;&#x30CF;&#x30B3;&#x30D9;</trd>
  <trd lng="nl">guichelheil</trd>
  <trd lng="pl">kurzy&sacute;lad</trd>
  <trd lng="ru">&c_o;&c_ch;&c_n;&c_y;&c_j; &c_c;&c_v;&c_je;&c_t; <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="sv">mire</trd>
  <trd lng="uk">&#x43A;&c_u;&c_r;&#x44F;&c_ch;&c_ib;&#xA0;&#x43E;&c_ch;&#x43A;&c_i;</trd>
</drv>
</art>
