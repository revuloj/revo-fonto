<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: epigenetik.xml,v 1.3 2022/08/31 07:49:48 revo Exp $">
<kap>
  <rad>epigenetik</rad>/o
</kap>
<drv mrk="epigenetik.0o">
  <kap><tld/>o</kap>
  <snc mrk="epigenetik.0o.GNT">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Bran&ccirc;o de biologio, kiu studas kiel bio&hcirc;emiaj
      kaj mediaj kondi&ccirc;oj rezultigas el samaj genoj
      diversajn &ccirc;elojn, fenotipojn, karakterojn de vivuloj:
      <ekz>
        genoj povas esti modifitaj <klr>[de]</klr> la medio kaj la 
        vivostilo - tion prizorgas la <tld/>o
        <fnt>
          <aut>[A. Oberndorfer]</aut>,
          <vrk><url
          ref="http://esperantaretradio.blogspot.fr/2013/12/ankau-patroj-heredigas-sian-vivostilon.html">
          Anka&ubreve; patroj heredigas sian vivostilon</url></vrk>,
          <lok>Esperanta Retradio, 2013-12-10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_p;&c_ib;&c_g;&c_je;&c_n;&c_e;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="en">epigenetics</trd>
  <trd lng="fr">&eacute;pig&eacute;n&eacute;tique</trd>
  <trd lng="pl">epigenetyka</trd>
  <trd lng="ro">epigenetic&abreve;</trd>
</drv>
</art>

