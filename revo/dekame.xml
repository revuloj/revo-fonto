<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dekame.xml,v 1.22 2023/11/04 13:16:26 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>dekametr</rad>/o
</kap>
<drv mrk="dekame.0o">
  <kap><tld/>o</kap>
  <mlg kod="SI">dam</mlg>
  <snc>
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de longo, egala
      al dek <ref tip="prt" cel="metr.0o.unuo">metroj</ref>:
      <ekz>
        1 aro = 100 kvadrataj metroj (m<sup>2</sup>) = 1 kvadrata <tld/>o (dam<sup>2</sup>) 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=36855">Aro (mezurunuo)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="malprt">
      <ref cel="kilome.0o">kilometro</ref>,
      <ref cel="hektom.0o">hektometro</ref>,
    </refgrp>
    <refgrp tip="prt">
      <ref cel="metr.0o">metro</ref>,
      <ref cel="decime.0o">decimetro</ref>,
      <ref cel="centim1.0o">centimetro</ref>,
      <ref cel="milime.0o">milimetro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_k;&c_a;&c_m;&c_e;&c_t;&c_a;&c_r;</trd>
  <trd lng="ca">dec&agrave;metre</trd>
  <trdgrp lng="de">
     <trd>Dekameter</trd>,
     <trd>zehn <ind>Meter</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>decameter</trd>,
    <trd>decametre</trd>
  </trdgrp>  <trd lng="fa">&a_d;&f_k;&a_A;&a_m;&a_t;&a_r;</trd>
  <trd lng="fr">d&eacute;cam&egrave;tre</trd>
  <trdgrp lng="hu">
     <trd>t&iacute;z <ind>m&eacute;ter</ind></trd>,
     <trd>dekam&eacute;ter</trd>
  </trdgrp>
  <trd lng="ja">&#x30C7;&#x30AB;&#x30E1;&#x30FC;&#x30C8;&#x30EB;</trd>
  <trd lng="nl">dekameter</trd>
  <trd lng="pl">dekametr</trd>
  <trd lng="pt">dec&acirc;metro</trd>
  <trd lng="ru">&c_d;&c_je;&c_k;&c_a;&c_m;&c_je;&c_t;&c_r;</trd>
  <trd lng="uk">&#x434;&#x435;&#x43A;&#x430;&#x43C;&#x435;&#x442;&#x440;</trd>
  <trd lng="zh">&#x516C;&#x4E08; <pr>g&#x14D;ngzh&#xE0;ng</pr></trd>
</drv>
</art>
