<art mrk="$Id: koleop.xml,v 1.21 2023/12/03 09:58:52 revo Exp $">
<kap>
  <rad>koleopter</rad>/oj <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="koleop.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="koleop.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Bestetoj, kies flugiloj estas kovritaj
      de rigidaj organoj t. n. elitroj, kiuj &scirc;irmas ilin, kiam la
      insekto ne flugas;
      ordo el la klaso <ref tip="super" cel="insekt.0oj">insektoj</ref>
      (<trd lng="la">Coleoptera</trd>):
      <ekz>
        glat&scirc;elaj fruktoj suferas multe pli de [speco de]
        <tld/>o, kurkulio, ol tiuj kun lanugo
        <fnt>
          <bib>OriginoDeSpecioj</bib>,
          <lok>&ccirc;apitro 4a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="skarab.0o.koleoptero">skarabo<sncref/></ref>
  </snc>
  <trd lng="be">&c_c;&c_mol;&c_v;&c_je;&c_r;&c_d;&c_a;&c_k;&c_r;&c_y;&c_l;&c_y;&c_ja;</trd>
  <trd lng="ca">cole&ograve;pters</trd>
  <trd lng="de">K&auml;fer</trd>
  <trd lng="en">beetles</trd>
  <trd lng="fr">col&eacute;opt&egrave;res</trd>
  <trd lng="hu">bogarak</trd>
  <trdgrp lng="ja">
    <trd>&#x9798;&#x7FC5;&#x76EE;  <pr>&#x3057;&#x3087;&#x3046;&#x3057;&#x3081;</pr></trd>,
    <trd>&#x7532;&#x866B;&#x985E;  <pr>&#x3053;&#x3046;&#x3061;&#x3085;&#x3046;&#x308B;&#x3044;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kevers</trd>,
    <trd>schildvleugeligen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>chrz&aogonek;szcze</trd>,
    <trd>t&eogonek;gopokrywowe</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_je;&c_s;&c_t;&c_k;&c_o;&c_k;&c_r;&c_y;&c_l;&c_y;&c_je;</trd>,
    <trd>&c_zh;&c_u;&c_k;&c_i;</trd>
  </trdgrp>
  <trd lng="sk">chrob&#xE1;ky</trd>
  <trd lng="sv">skalbaggar</trd>
  <trdgrp lng="uk">
    <trd>&#x442;&#x432;&#x435;&#x440;&#x434;&#x43E;&#x43A;&#x440;&#x438;&#x43B;&#x456;</trd>,
    <trd>&#x436;&#x443;&#x43A;&#x438;</trd>
  </trdgrp>
  <trd lng="zh">&#x9798;&#x7FC5;&#x76EE; <pr>qi&#xE0;och&#xEC;m&#xF9;</pr></trd>
</drv>
</art>
