<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>koleopter</rad>/oj
</kap>
<drv mrk="koleop.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="insekt.0oj">insektoj</ref>
      (<trd lng="la">Coleoptera</trd>), kies flugiloj estas kovritaj
      de rigidaj organoj t. n. elitroj, kiuj &scirc;irmas ilin, kiam la
      insekto ne flugas.
    </dif>
    <ref tip="sin" cel="skarab.0o.koleoptero">skarabo</ref>
  </snc>
  <trd lng="de">K&auml;fer</trd>
  <trd lng="en">beetles</trd>
  <trdgrp lng="nl">
    <trd>kevers</trd>,
    <trd>schildvleugeligen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_je;&c_s;&c_t;&c_k;&c_o;&c_k;&c_r;&c_y;&c_l;&c_y;&c_je;</trd>,
    <trd>&c_zh;&c_u;&c_k;&c_i;</trd>
  </trdgrp>
</drv>
</art>
