          <aut>H. Heine, trad. L. L. Zamenhof</aut>
          <vrk>La Rabeno de Ba&hcirc;ara&hcirc;</vrk>,
          <lok>1929</lok>
        </fnt>.
       </ekz>
     </dif>
    <refgrp tip="vid">
      <ref cel="dalmat.0o">dalmatiko</ref>,
      <ref cel="sutan.0o.TEKS">sutano</ref>
    </refgrp>     
   </snc>
  <trd lng="be">&c_s;&c_t;&c_y;&c_h;&c_a;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>kom&zcaron;e</trd>,
    <trd>rocheta</trd>
  </trdgrp>
  <trd lng="de">Chorhemd</trd>
  <trd lng="en">surplice</trd>
  <trd lng="fr">surplis</trd>
  <trdgrp lng="hu">
     <trd>karing</trd>,
     <trd>miseing</trd>
   </trdgrp>
  <trd lng="it">cotta<klr> (rel.)</klr></trd>
  <trd lng="ja">&#x30B5;&#x30FC;&#x30D7;&#x30EA;&#x30B9;</trd>
  <trd lng="nl">koorhemd</trd>
  <trd lng="pl">kom&zdot;a</trd>
  <trd lng="ru">&c_s;&c_t;&c_i;&c_h;&c_a;&c_r;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>kam&zcaron;a</trd>,
    <trd>rocheta</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x441;&#x442;&#x438;&#x445;&#x430;&#x440;</trd>,
    <trd>&#x434;&#x430;&#x43B;&#x43C;&#x430;&#x442;&#x438;&#x43A;&#x430;</trd>,
    <trd>&#x43F;&#x456;&#x434;&#x440;&#x438;&#x437;&#x43D;&#x438;&#x43A;</trd>,
    <trd>&#x43F;&#x456;&#x434;&#x441;&#x430;&#x43A;&#x43E;&#x441;&#x43D;&#x438;&#x43A;</trd>
