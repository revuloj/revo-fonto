<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>foton</rad>/o</kap>
<drv mrk="foton.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="kvantum.0o">Kvantumo</ref> de elektromagneta
      radiado, sen&scirc;arga elementa
      <ref tip="super" cel="partik2.0o">partiklo</ref>
      kun nula maso, <ref tip="vid" cel="spin2.0o">spino</ref> 1 kaj
      kun energio egala al la konstanto
      de Planck multiplikita per la frekvenco de la radiado.
    </dif>
    <ref tip="malprt" cel="lum.0o.FIZ">lumo</ref>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_t;&c_o;&c_n;</trd>
  <trdgrp lng="de">
    <trd>Photon</trd>,
    <trd>Lichtteilchen</trd>,
    <trd>Lichtquant</trd>
  </trdgrp>
  <trd lng="en">photon</trd>
  <trd lng="es">fot&oacute;n</trd>
  <trd lng="fr">photon</trd>
  <trd lng="hu">foton</trd>
  <trd lng="nl">foton</trd>
  <trd lng="pl">foton</trd>
  <trdgrp lng="pt">
    <trd>f&oacute;ton</trd>,
    <trd>fot&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_o;&c_t;&c_o;&c_n;</trd>
  <trd lng="sv">foton</trd>
</drv>
</art>
