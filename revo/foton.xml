<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>foton</rad>/o
</kap>
<drv mrk="foton.0o">
<kap><tld/>o</kap>
<uzo tip="fak">FIZ</uzo>
<snc>
<dif>
<ref tip="super" cel="kvantum.0o">Kvantumo</ref> de elektromagneta radiado,
sen&scirc;arga elementa partiklo kun nula maso,
<ref tip="vid" cel="spin2.0o">spino</ref> 1 k kun energio egala al
la konstanto de Planck multiplikita per la frekvenco de la radiado.
</dif>
<refgrp tip="vid">
<ref cel="lum.0o">lumo</ref>,
<ref cel="partik2.0o">partiklo</ref>
</refgrp>
</snc>
<trdgrp lng="de">
   <trd>Photon</trd>,
   <trd>Lichtteilchen</trd>,
   <trd>Lichtquant</trd></trdgrp>
<trd lng="en">photon</trd>
<trd lng="fr">photon</trd>
<trd lng="ru">&c_f;&c_o;&c_t;&c_o;&c_n;</trd>
</drv>
</art>
