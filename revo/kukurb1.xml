<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kukurb1.xml,v 1.12 2018/10/06 20:10:27 revo Exp $">
<kap>
  <rad>kukurbac</rad>/oj
</kap>
<drv mrk="kukurb1.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="kukurb1.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Cucurbitaceae</trd>), al kiuj
      apartenas ekz <ref tip="ekz" cel="kukurb.0o">kukurbo</ref>,
      <ref tip="ekz" cel="kukum.0o">kukumo</ref> kaj
      <ref tip="ekz" cel="melon.0o">melono</ref>:
      <ekz>
        mi tre respekte <klr>[&#8230;]</klr> salutas vin, ho familio de l'
        <tld/>oj!
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="http://steloj.de/esperanto/butiko">La Stranga
          Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en Ekstrema Oriento, al solanacoj kaj <tld/>oj kelkfoje grandan
        malutilon ka&ubreve;zas dudekokmakula kokcinelo
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref="https://eo.wikipedia.org/?curid=9459">Kokcinelo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_t;&c_i;&c_k;&c_v;&c_o;&c_v;&c_i;</trd>
  <trd lng="ca">cucurbit&agrave;cies</trd>
  <trd lng="de">K&uuml;rbisgew&auml;chse</trd>
  <trd lng="en">curcurbits</trd>
  <trd lng="es">cucurbit&aacute;ceas</trd>
  <trd lng="fr">curcurbitac&eacute;e</trd>
  <trd lng="hu">t&ouml;kf&eacute;l&eacute;k</trd>
  <trd lng="nl">kalebasachtigen</trd>
  <trd lng="pl">dyniowate</trd>
  <trd lng="pt">cucurb&aacute;ceas</trd>
  <trd lng="ro">cucurbitaceae</trd>
  <trd lng="ru">&c_t;&c_y;&c_k;&c_v;&c_je;&c_n;&c_n;&c_y;&c_je;</trd>
</drv>
</art>
