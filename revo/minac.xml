  <snc mrk="minac.0o.MOR">
    <dif>
      Paroloj, gestoj per kiuj oni <tld/>as:
      <ekz>
        timi nek <tld/>ojn, nek batojn;
      </ekz>
      <ekz>
        <tld/>oj ne mortigas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="pl">gro&zacute;ba</trd>
  </snc>
  <snc mrk="minac.0o.io">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Balda&ubreve;a dan&gcirc;ero:
      <ekz>
        konstanta <tld/>o de ruini&gcirc;o &scirc;vebas super ili.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>gro&zacute;ba</trd>,
      <trd>zagro&zdot;enie</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_g;&c_r;&c_o;&c_z;&c_a;</trd>
