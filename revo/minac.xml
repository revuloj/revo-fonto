</drv>

<drv mrk="minac.0o">
  <kap><tld/>o</kap>
  <snc mrk="minac.0o.MOR">
    <dif>
      Paroloj, gestoj per kiuj oni <tld/>as:
      <ekz>
        timi nek <tld/>ojn, nek batojn;
      </ekz>

      <ekz>
        <klr>[li]</klr> sentis <tld/>on en la mokaj vortoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        sub <tld/>o de morto respondu al mi
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        post paroloj ili alvenas al <tld/>oj
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        la knaboj parolis tute afable kaj sen ia ajn <tld/>o en la vo&ccirc;o
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj ne mortigas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        kie <tld/>o, tie malpaco
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Drohung</trd>,
      <trd>Androhung</trd>
    </trdgrp>
    <trd lng="pl">gro&zacute;ba</trd>
  </snc>
  <snc mrk="minac.0o.io">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Balda&ubreve;a <ref tip="sin" cel="dangxe.0o">dan&gcirc;ero</ref>:
      <ekz>
        <tld/>o de malsato
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XLIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[super ili]</klr> pendadis per tuta pezo <tld/>o de malfondo
        <fnt>
          <bib>Kon11</bib>
          <lok>Kontakto 50-jara</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;ustatempe preventi <klr>[&#x2026;]</klr> <tld/>ojn por la unueco de UEA
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="risk.0o">risko</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Bedrohung</trd>,
      <trd>Risiko</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>gro&zacute;ba</trd>,
      <trd>zagro&zdot;enie</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_g;&c_r;&c_o;&c_z;&c_a;</trd>
  <trd lng="bg">&c_z;&c_a;&c_p;&c_l;&c_a;&c_h;&c_a;</trd>
  <trd lng="br">gourdrouz<klr tip="ind"> (ak.)</klr></trd>
  <trd lng="ca">amena&ccedil;a</trd>
  <trdgrp lng="cs">
    <trd>hrozba</trd>,
    <trd>pohr&uring;&zcaron;ka</trd>,
    <trd>v&yacute;hr&uring;&zcaron;ka</trd>
  </trdgrp>
  <trd lng="en">threat</trd>
  <trd lng="es">amenaza</trd>
  <trd lng="fa">&a_t;&a_h;&a_d;&f_y;&a_d;</trd>
  <trd lng="fr">menace</trd>
  <trd lng="hu">fenyeget&eacute;s</trd>
  <trdgrp lng="id">
    <trd>ancaman</trd>,
    <trd>gertak</trd>,
    <trd>gertakan</trd>,
    <trd>intimidasi</trd>
