  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_g;&c_r;&c_o;&c_z;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_r;&c_o;&c_z;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
