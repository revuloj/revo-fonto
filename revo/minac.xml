<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: minac.xml,v 1.32 2023/09/08 14:27:05 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>minac</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="minac.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="minac.0i.MOR">
    <dif>
      Montri al iu per paroloj a&ubreve; gestoj, ke oni intencas fari
      al li malbona&jcirc;on:
      <ekz>
        kiu multe <tld/>as, ne estas dan&gcirc;era
        (<refgrp tip="vid">
          <ref cel="hund.0o">hundo</ref>,
          <ref cel="tondr.0i">tondri</ref>
        </refgrp>)
        <fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        <tld/>i al iu;
      </ekz>
      <ekz>
        <tld/>i iun per pugno, bastono, koleraj vortoj, per fingro
        en la po&scirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i iun je bato, morto, puno, proceso.
      </ekz>
      <ekz>
        alia <tld/>is belan regalon al la germanoj, &ccirc;ar tiuj arogis
        &jcirc;eti obuson kiu mortigis lian oficiron
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
      <ref tip="vid" cel="dent.0o">dento</ref>.
    </dif>
    <trdgrp lng="pl">
      <trd>grozi&cacute;</trd>,
      <trd>wygra&zdot;a&cacute;</trd>,
      <trd>odgra&zdot;a&cacute; si&eogonek;</trd>
    </trdgrp>
  </snc>
  <snc mrk="minac.0i.io">
    <uzo tip="stl">FIG</uzo>
    <uzo tip="klr">(pri malbona&jcirc;o, malfeli&ccirc;o)</uzo>
    <dif>
      Sin montri kiel balda&ubreve; okazonta,
      esti ver&scirc;ajne okazonta:
      <ekz>
        malsano <tld/>as lin;
      </ekz>
      <ekz>
        fulmotondro <tld/>as;
      </ekz>
      <ekz>
        dan&gcirc;ero <tld/>as al via honoro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>as ribelo, milito
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pend.0i">pendi</ref>,
      <ref cel="siegx.0i">sie&gcirc;i</ref>,
      <ref cel="sxveb.0i">&scirc;vebi</ref>.
    </refgrp>
    <trdgrp lng="pl">
      <trd>grozi&cacute;</trd>,
      <trd>zagra&zdot;a&cacute;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_g;&c_r;&c_a;&c_zh;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_z;&c_a;&c_p;&c_l;&c_a;&c_sh;&c_v;&c_a;&c_m;</trd>
  <trd lng="br">gourdrouz<klr tip="ind"> (v.)</klr></trd>
  <trd lng="ca">amena&ccedil;ar</trd>
  <trdgrp lng="cs">
    <trd>hrozit</trd>,
    <trd>ohro&zcaron;ovat</trd>,
    <trd>vyhro&zcaron;ovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>drohen</trd>,
    <trd>bedrohen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>threaten</trd>,
    <trd>menace</trd>,
    <trd>intimidate</trd>
  </trdgrp>
  <trd lng="es">amenazar</trd>
  <trd lng="fa">&a_t;&a_h;&a_d;&f_y;&a_d; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">menacer</trd>
  <trd lng="hu">fenyeget</trd>
  <trdgrp lng="ja">
    <trd>&#33029;&#12377;  [<ind>&#x304A;&#12393;&#12377;</ind>]</trd>,
    <trd>&#33029;&#x304B;&#12377;  [<ind>&#x304A;&#12403;&#12420;&#x304B;&#12377;</ind>]</trd>,
    <trd>&#33029;&#x8FEB;&#12377;&#x308B;  [<ind>&#x304D;&#12423;&#12358;&#x306F;&#x304F;&#12377;&#x308B;</ind>]</trd>,
    <trd>&#x8FEB;&#x308B;  [<ind>&#x305B;&#x307E;&#x308B;</ind>]</trd>,
    <trd>&#xFF5E;&#12377;&#x308B;&#24656;&#x308C;&#x304C;&#12354;&#x308B; 
    [&#xFF5E;<ind>&#12377;&#x308B;&#x304A;&#x305D;&#x308C;&#x304C;&#12354;&#x308B;</ind>]</trd>,
    <trd>&#21361;&#12358;&#x304F;&#xFF5E;&#12375;&#x305D;&#12358;&#12391;&#12354;&#x308B; 
    [<ind>&#12354;&#12420;&#12358;&#x304F;&#xFF5E;&#12375;&#x305D;&#12358;&#12391;&#12354;&#x308B;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>dreigen</trd>,
    <trd>bedreigen</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>amea&ccedil;ar</trd>,
   <trd>intimidar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_g;&c_r;&c_o;&c_zh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_g;&c_r;&c_o;&c_z;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hrozi&tcaron;</trd>,
    <trd>ohrozova&tcaron;</trd>,
    <trd>vyhr&aacute;&zcaron;a&tcaron; sa</trd>
  </trdgrp>
  <trd lng="sv">hota</trd>
  <trdgrp lng="th">
    <trd>&#xE02;&#xE48;&#xE21;&#xE02;&#xE39;&#xE48;</trd>,
    <trd>&#xE02;&#xE39;&#xE48;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_r;&#x43E;&c_z;&c_i;&c_t;&c_i;</trd>,
    <trd>&#x43F;&#x43E;&c_g;&c_r;&#x43E;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_r;&#x43E;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="minac.0a">
  <kap><tld/>a</kap>
  <snc mrk="minac.0a.MOR">
    <dif>
      Kiu <tld/>as, kiu &scirc;ajnas dan&gcirc;era:
      <ekz>
        <tld/>a rigardo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_g;&c_r;&c_o;&c_z;&c_mol;&c_l;&c_ib;&c_v;&c_y;</trd>
  <trd lng="en">threatening</trd>
  <trd lng="fr">mena&ccedil;ant</trd>
  <trdgrp lng="ja">
    <trd>&#x5A01;&#22151;&#30340;&#x306A;  [<ind>&#12356;&#x304B;&#x304F;&#12390;&#x304D;&#x306A;</ind>]</trd>,
    <trd>&#33029;&#12375;&#x306E;  [<ind>&#x304A;&#12393;&#12375;&#x306E;</ind>]</trd>,
    <trd>&#x967A;&#x60AA;&#x306A;  [<ind>&#12369;&#12435;&#12354;&#x304F;&#x306A;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&c_g;&c_r;&#x43E;&c_z;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_r;&#x43E;&c_z;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="minac.0o">
  <kap><tld/>o</kap>
  <snc mrk="minac.0o.MOR">
    <dif>
      Paroloj, gestoj per kiuj oni <tld/>as:
      <ekz>
        timi nek <tld/>ojn, nek batojn;
      </ekz>
      <ekz>
        <tld/>oj ne mortigas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="pl">gro&zacute;ba</trd>
  </snc>
  <snc mrk="minac.0o.io">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Balda&ubreve;a dan&gcirc;ero:
      <ekz>
        konstanta <tld/>o de ruini&gcirc;o &scirc;vebas super ili.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>gro&zacute;ba</trd>,
      <trd>zagro&zdot;enie</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_g;&c_r;&c_o;&c_z;&c_a;</trd>
  <trd lng="bg">&c_z;&c_a;&c_p;&c_l;&c_a;&c_h;&c_a;</trd>
  <trd lng="br">gourdrouz<klr tip="ind"> (ak.)</klr></trd>
  <trd lng="ca">amena&ccedil;a</trd>
  <trdgrp lng="cs">
    <trd>hrozba</trd>,
    <trd>pohr&uring;&zcaron;ka</trd>,
    <trd>v&yacute;hr&uring;&zcaron;ka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Drohung</trd>,
    <trd>Bedrohung</trd>
  </trdgrp>
  <trd lng="en">threat</trd>
  <trd lng="es">amenaza</trd>
  <trd lng="fa">&a_t;&a_h;&a_d;&f_y;&a_d;</trd>
  <trd lng="fr">menace</trd>
  <trd lng="hu">fenyeget&eacute;s</trd>
  <trdgrp lng="ja">
    <trd>&#33029;&#12375;  [<ind>&#x304A;&#12393;&#12375;</ind>]</trd>,
    <trd>&#33029;&#x8FEB;  [<ind>&#x304D;&#12423;&#12358;&#x306F;&#x304F;</ind>]</trd>,
    <trd>&#x5A01;&#22151;  [<ind>&#12356;&#x304B;&#x304F;</ind>]</trd>,
    <trd>&#33029;&#x5A01;  [<ind>&#x304D;&#12423;&#12358;&#12356;</ind>]</trd>,
    <trd>&#x8FEB;&#x308A;&#x304F;&#x308B;&#21361;&#x967A; 
    [<ind>&#x305B;&#x307E;&#x308A;&#x304F;&#x308B;&#x304D;&#12369;&#12435;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>dreiging</trd>,
    <trd>bedreiging</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_g;&c_r;&c_o;&c_z;&c_a;</trd>
  <trd lng="sk">hrozba</trd>
  <trd lng="sv">hot</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&c_g;&c_r;&#x43E;&c_z;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_r;&#x43E;&c_z;&c_a;</trd>
  </trdgrp>
</drv>
</art>
