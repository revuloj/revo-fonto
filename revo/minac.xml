<drv mrk="minac.0a">
  <kap><tld/>a</kap>
  <snc mrk="minac.0a.MOR">
    <dif>
      Kiu <tld/>as, kiu &scirc;ajnas dan&gcirc;era:
      <ekz>
        <tld/>a vo&ccirc;o
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 22:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj tentoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i a&ubreve;dis la <tld/>ajn ordonojn de l' princo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        malaperis la <tld/>aj sulkoj de lia viza&gcirc;o, liaj okuloj ekbrilis
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i turnis siajn okulojn al la <tld/>aj, malfeli&ccirc;on anta&ubreve;dirantaj mallumoj
        <fnt>
          <bib>Lasta</bib>
          <lok>La Lasta</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_g;&c_r;&c_o;&c_z;&c_mol;&c_l;&c_ib;&c_v;&c_y;</trd>
  <trdgrp lng="de">
    <trd>bedrohlich</trd>,
    <trd>drohend</trd>
  </trdgrp>
  <trd lng="en">threatening</trd>
  <trd lng="fr">mena&ccedil;ant</trd>
  <trdgrp lng="id">
    <trd><baz>ancam</baz>mengancam<klr tip="amb"> (adj)</klr></trd>,
    <trd><baz>intimidasi</baz>mengintimidasi<klr tip="amb"> (adj)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5A01;&#x5687;&#x7684;&#x306A;  <pr>&#x3044;&#x304B;&#x304F;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x8105;&#x3057;&#x306E;  <pr>&#x304A;&#x3069;&#x3057;&#x306E;</pr></trd>,
    <trd>&#x967A;&#x60AA;&#x306A;  <pr>&#x3051;&#x3093;&#x3042;&#x304F;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
