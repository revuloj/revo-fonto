<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>apla&ubreve;d</rad>/i
</kap>
<drv mrk="aplaux.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Montri aprobon per klakfrapo de la manoj:
      <ekz>
        <tld/>i oratoron, al oratoro, spektaklon;
      </ekz>
      <ekz>
        fari <tld/>on sur la vangon
        <klr>(vangfrapi)</klr><fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_p;&c_l;&c_a;&c_d;&c_z;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_l;&c_ja;&c_s;&c_k;&c_a;&c_c;&c_mol; &c_u; &c_d;&c_a;&c_l;&c_o;&c_n;&c_ib;</trd> 
    </trdgrp>  
    <trdgrp lng="ru">
      <trd>&c_a;&c_p;&c_l;&c_o;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_r;&c_u;&c_k;&c_o;&c_p;&c_l;&c_je;&c_s;&c_k;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Forte aprobi, samopinii:
      <ekz>
        mi <tld/>as vian iniciaton, al via iniciato;
      </ekz>
      <ekz>
        alian ne malla&ubreve;du, vin mem ne <tld/>u<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_u;&c_h;&c_v;&c_a;&c_l;&c_ja;&c_c;&c_mol;</trd>, 
      <trd>&c_v;&c_ib;&c_t;&c_a;&c_c;&c_mol;</trd>, 
      <trd>&c_a;&c_d;&c_o;&c_b;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd> 
    </trdgrp>  
    <trdgrp lng="ru">
      <trd>&c_o;&c_d;&c_o;&c_b;&c_r;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_v;&c_je;&c_t;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="de">
    <trd>applaudieren</trd>,
    <trd>begr&uuml;&szlig;en</trd>
  </trdgrp>
  <trd lng="en">applaud</trd>
  <trd lng="fr">applaudir</trd>
  <trd lng="it">applaudire</trd>
</drv>
</art>
