<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gajak</rad>/o
</kap>
<drv mrk="gajak.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="zigofi.0oj">zigofilacoj</ref>
      (<trd lng="la">Guajacum</trd>), amerika arbo.
    </dif>
  </snc>
  <trd lng="ca">guaiac</trd>
  <trd lng="de">Guajakbaum</trd>
  <trd lng="en">lignum vitae</trd>
  <trd lng="fr">ga&iuml;ac</trd>
  <trd lng="hu">guajak</trd>
  <trd lng="nl">guajak</trd>
  <trd lng="pl">gwajakowiec</trd>
  <trdgrp lng="pt">
    <trd>gu&aacute;iaco</trd>,
    <trd>gaiaco</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_v;&c_a;&c_ja;&c_k;&c_o;&c_v;&c_o;&c_je;
&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>,
    <trd>&c_b;&c_a;&c_k;&c_a;&c_u;&c_t;&c_o;&c_v;&c_o;&c_je;
&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">guajak <klr>(bot)</klr></trd>
</drv>
<drv mrk="gajak.0ligno">
  <kap><tld/>ligno</kap>
  <snc>
    <dif>
      Tre malmola, peza kaj oleeca ligno de <tld/>o,
      uzata por mebloj, en &scirc;ipkonstruado, por rulpremiloj,
      keglogloboj ka.
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>guaiac</trd>,
    <trd>fusta del guaiac</trd>
  </trdgrp>
  <trd lng="de">Pockholz</trd>
  <trdgrp lng="hu">
    <trd>pokkfa</trd>,
    <trd>franciafa</trd></trdgrp>
  <trdgrp lng="nl">
    <trd>guajakhout</trd>,
    <trd>pokhout</trd></trdgrp>
  <trd lng="sk">guajakov√© drevo</trd>
</drv>
  <trdgrp lng="pl">
    <trd>drewno <ind>gwajakowe</ind></trd>,
    <trd>drewno <ind>gwajakowca</ind></trd>
  </trdgrp>
<adm>
PIV1 preferas pli la&ubreve;skriban formon "gvajak" de la radiko.
</adm>
</art>
