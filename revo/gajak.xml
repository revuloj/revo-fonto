<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gajak.xml,v 1.24 2023/10/06 08:29:44 revo Exp $">
<kap>
  <rad>gajak</rad>/o,
  <var><kap><rad var="v">gvajak</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>
<drv mrk="gajak.0o">
  <kap><tld/>o,
    <var><kap><tld var="v"/>o</kap></var>
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="zigofi.0oj">zigofilacoj</ref>
      (<trd lng="la">Guajacum</trd>), amerika arbo:
      <ekz>
        <tld var="v"/>o estas genro de arboj, el kiuj la plej konata estas la sudamerika specio
        <nom>Guajacum officinale</nom> kun pinataj folioj kaj bluaj a&ubreve; ru&gcirc;aj floroj en umbelecaj
        infloreskoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=29637">Gvajako</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
    lok="&WCU;/thumb/6/68/K&ouml;hler's_Medizinal-Pflanzen_in_naturgetreuen_Abbildungen_mit_kurz_erl&auml;uterndem_Texte_(Plate_121)_(8231727103).jpg/352px-K&ouml;hler's_Medizinal-Pflanzen_in_naturgetreuen_Abbildungen_mit_kurz_erl&auml;uterndem_Texte_(Plate_121)_(8231727103).jpg"
    prm="PD">botanika tabulo de <tld/>o (Guajacum officinale)
        <fnt>
          <aut>Brandt, Wilhelm; G&uuml;rke, M.; K&ouml;hler, F. E.; Pabst, G.; Schellenberg, G.; Vogtherr,
          Max.: https://www.flickr.com/photos/biodivlibrary/8231727103</aut>
          <lok><url
          ref="&WCW;/File:K%C3%B6hler's_Medizinal-Pflanzen_in_naturgetreuen_Abbildungen_mit_kurz_erl%C3%A4uterndem_Texte_(Plate_121)_(8231727103).jpg">&WCW;/File:K&ouml;hler's_Medizinal-Pflanzen_in_naturgetreuen_Abbildungen_mit_kurz_erl&auml;uterndem_Texte_(Plate_121)_(8231727103).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_v;&c_a;&c_ja;&c_k;&c_u;&c_m;</trd>,
    <trd>&c_g;&c_v;&c_a;&c_ja;&c_k;&c_a;&c_v;&c_a;&c_je; &c_d;&c_r;&c_e;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">guaiac</trd>
  <trd lng="cs">guajak</trd>
  <trd lng="de">Guajakbaum</trd>
  <trd lng="en">lignum vitae</trd>
  <trd lng="fr">ga&iuml;ac</trd>
  <trd lng="hu">guajak</trd>
  <trdgrp lng="ja">
    <trd>&#x30E6;&#x30BD;&#x30A6;&#x30DC;&#x30AF;</trd>,
    <trd>&#x30B0;&#x30A2;&#x30E4;&#x30AF;</trd>
  </trdgrp>
  <trd lng="nl">guajak</trd>
  <trd lng="pl">gwajakowiec</trd>
  <trdgrp lng="pt">
    <trd>gu&aacute;iaco</trd>,
    <trd>gaiaco</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_v;&c_a;&c_ja;&c_k;&c_o;&c_v;&c_o;&c_je;
&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>,
    <trd>&c_b;&c_a;&c_k;&c_a;&c_u;&c_t;&c_o;&c_v;&c_o;&c_je;
&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">guajak <klr>(bot)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_gu;&c_v;&c_a;&#x44F;&#x43A;&#x43E;&c_v;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&#x43E;</trd>,
    <trd>&c_b;&c_a;&#x43A;&c_a;&c_u;&c_t;&#x43E;&c_v;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&#x43E;</trd>
  </trdgrp>
</drv>
<drv mrk="gajak.0ligno">
  <kap><tld/>ligno</kap>
  <snc>
    <dif>
      Tre malmola, peza kaj oleeca ligno de <tld/>o,
      uzata por mebloj, en &scirc;ipkonstruado, por rulpremiloj,
      keglogloboj k.a.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_v;&c_a;&c_ja;&c_k;&c_a;&c_v;&c_a;&c_ja; &c_d;&c_r;&c_a;&c_w;&c_n;&c_ib;&c_n;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>guaiac</trd>,
    <trd>fusta del guaiac</trd>
  </trdgrp>
  <trd lng="de">Pockholz</trd>
  <trdgrp lng="hu">
    <trd>pokkfa</trd>,
    <trd>franciafa</trd></trdgrp>
  <trdgrp lng="nl">
    <trd>guajakhout</trd>,
    <trd>pokhout</trd></trdgrp>
  <trd lng="sk">guajakov&eacute; drevo</trd>
</drv>

  <trdgrp lng="pl">
    <trd>drewno <ind>gwajakowe</ind></trd>,
    <trd>drewno <ind>gwajakowca</ind></trd>
  </trdgrp>
</art>
