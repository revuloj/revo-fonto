<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>gajak</rad>/o
</kap>
<drv mrk="gajak.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="zigofi.0acoj">zigofilacoj</ref>
      (<trd lng="la">Guajacum</trd>), amerika arbo.
    </dif>
  </snc>
  <trd lng="de">Guajakbaum</trd>
</drv>
<drv mrk="gajak.0ligno">
  <kap><tld/>ligno</kap>
  <snc>
    <dif>
      Tre malmola, peza kaj oleeca ligno de <tld/>o,
      uzata por mebloj, en &scirc;ipkonstruado, por rulpremiloj, 
      keglogloboj k.a.
    </dif>
  </snc>
  <trd>Pockholz</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
