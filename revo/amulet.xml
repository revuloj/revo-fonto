<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>amulet</rad>/o
</kap>
<drv mrk="amulet.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Objekto, kiu, per supozata magia efiko, &scirc;irmas kontra&ubreve;
      dan&gcirc;ero, malsano, sor&ccirc;o, ktp.
      <ekz>
        &ccirc;i tiu ora &scirc;ildo, inkrustita per multekostaj &scirc;tonoj, havis, krom granda mona valoro, forton de l' <tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        tri oraj <tld/>oj kun bildo de la diino <nom>Astarte</nom>
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXV</lok>
        </fnt>;
      </ekz>
      <ekz>
        oficiro, kovrita de <tld/>oj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        li faru al li <tld/>on, ke liaj aferoj sukcesu
        <fnt>
          <bib>Monato</bib>
          <aut>Gbeglo Koffi</aut>
          <vrk><url ref="http://www.monato.net/2010/009910.php">Ventreligio, dua r aldoni&gcirc;u</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;irka&ubreve; lia kolo pendis &ccirc;e &ccirc;eno granda ora <tld/>o, sur kiu estis videblaj 
        du serpentoj, hela kaj malhela, kiuj sin reciproke mordis en la voston kaj formis ovrondon
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Destino de Atrejo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
       <ref cel="talism.0o">talismano</ref>,
       <ref cel="feticx.0o.MAG">feti&ccirc;o<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_u;&c_l;&c_je;&c_t;</trd>
  <trd lng="cs">amulet</trd>
  <trdgrp lng="de">
    <trd>Amulett</trd>,
    <trd>Talisman</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>amulet</trd>,
    <trd>talisman</trd>,
    <trd>mojo</trd>
  </trdgrp>
  <trd lng="es">amuleto</trd>
  <trd lng="fr">amulette</trd>
  <trd lng="hu">amulett</trd>
  <trdgrp lng="nl">
    <trd>amulet</trd>,
    <trd>talisman</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>amulet</trd>,
    <trd>talizman</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_m;&c_u;&c_l;&c_je;&c_t;</trd>,
    <trd>&c_l;&c_a;&c_d;&c_a;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_a;&c_l;&c_i;&c_s;&c_m;&c_a;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>talizman</trd>,
    <trd>amulet</trd>
  </trdgrp>
</drv>
</art>
