  <trdgrp lng="de">
    <trd>Amulett</trd>,
    <trd>Talisman</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>amulet</trd>,
    <trd>talisman</trd>,
    <trd>mojo</trd>
  </trdgrp>
  <trd lng="es">amuleto</trd>
  <trd lng="fr">amulette</trd>
  <trd lng="hu">amulett</trd>
  <trdgrp lng="nl">
    <trd>amulet</trd>,
    <trd>talisman</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_m;&c_u;&c_l;&c_je;&c_t;</trd>,
    <trd>&c_l;&c_a;&c_d;&c_a;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_a;&c_l;&c_i;&c_s;&c_m;&c_a;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>talizman</trd>,
    <trd>amulet</trd>
  </trdgrp>
</drv>
</art>
