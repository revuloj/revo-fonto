<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>majstr</rad>/o
</kap>
<drv mrk="majstr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="majstr.0o.KOMUNE">
    <dif>
      Titolo donata al specialisto alte &scirc;atata pro liaj fakaj kapabloj:
      <ekz>
        scienca, muzika <tld/>o;
      </ekz>
      <ekz>
        koni&gcirc;as <tld/>o la&ubreve; sia verko
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>e &ccirc;izi lignon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ctl>Vivo kaj opinioj de <tld/>o M'saud</ctl>
        <klr>(romano de Jean Ribillard)</klr>
        <fnt><aut>Ribillard Jean</aut>, <vrk>Vivo kaj opinioj de majstro
        M'saud. - La Laguna: J. R&eacute;gulo, 1963</vrk></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_j;&c_s;&c_t;&c_a;&c_r;</trd>
  <trd lng="br">mestr<klr tip="ind"> (mailh)</klr></trd>
  <trd lng="ca">mestre</trd>
  <trd lng="cs">mistr</trd>
  <trd lng="de">Meister</trd>
  <trd lng="en">maestro</trd>
  <trd lng="es">maestro</trd>
  <trd lng="fr">maitre</trd>
  <trdgrp lng="hu">
    <trd>mester</trd>,
    <trd>mesterember</trd>
  </trdgrp>  
  <trd lng="nl">meester</trd>
  <trdgrp lng="pt">
    <trd>mestre</trd>,
    <trd>mag&iacute;ster</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_s;&c_t;&c_je;&c_r;</trd>,
    <trd>&c_m;&c_a;&c_e;&c_s;&c_t;&c_r;&c_o;</trd>
  </trdgrp>
  <trd lng="sv">m&auml;stare</trd>
</drv>

<drv mrk="majstr.0ajxo">
  <kap><tld/>a&jcirc;o, <tld/>overko</kap>
  <snc>
    <dif>
      Verko inda je <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_e;&c_d;&c_e;&c_w;&c_r;</trd>
  <trd lng="br">pennoberenn</trd>
  <trd lng="cs">mistrovsk&eacute; d&iacute;lo</trd>
  <trd lng="de">Meisterwerk</trd>
  <trdgrp lng="en">
    <trd>masterpiece</trd>,
    <trd>masterwork</trd>,
  </trdgrp>
  <trd lng="fr">chef d'&oelig;uvre</trd>
  <trdgrp lng="hu">
    <trd>mesterm&udblac;</trd>,
    <trd>m&udblac;remek</trd>
  </trdgrp>  
  <trd lng="nl">meesterwerk</trd>
  <trd lng="pt">obra de <ind>mestre</ind></trd>
  <trd lng="ru">&c_sh;&c_je;&c_d;&c_je;&c_v;&c_r;</trd>
  <trd lng="sv">m&auml;sterverk</trd>
</drv>
</art>
