<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>majstr</rad>/o
</kap>
<drv mrk="majstr.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Titolo donata al specialisto alte &scirc;atata pro liaj fakaj kapabloj:
      <ekz>
        scienca, muzika <tld/>o;
      </ekz>
      <ekz>koni&gcirc;as <tld/>o la&ubreve; sia verko
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        <tld/>a verko;
      </ekz>
      <ekz>
        <tld/>e &ccirc;izi lignon<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Meister</trd>
  <trd lng="en">maestro</trd>
  <trd lng="nl">meester</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_s;&c_t;&c_je;&c_r;</trd>,
    <trd>&c_m;&c_a;&c_e;&c_s;&c_t;&c_r;&c_o;</trd>
  </trdgrp>
  <trd lng="sv">m&auml;stare</trd>
</drv>

<drv mrk="majstr.0ajxo">
  <kap>
    <tld/>a&jcirc;o, <tld/>overko
  </kap>
  <snc>
    <dif>
      Verko inda je <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Meisterwerk</trd>
  <trdgrp lng="en">
    <trd>masterpiece</trd>,
    <trd>masterwork</trd>,
  </trdgrp>
  <trd lng="nl">meesterwerk</trd>
  <trd lng="ru">&c_sh;&c_je;&c_d;&c_je;&c_v;&c_r;</trd>
  <trd lng="sv">m&auml;sterverk</trd>
</drv>
</art>
