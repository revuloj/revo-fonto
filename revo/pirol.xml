<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pirol.xml,v 1.20 2023/09/03 12:45:08 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pirol</rad>/o
</kap>
<drv mrk="pirol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="pirol.0o.ZOO">
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el la familio
      <ref tip="malprt" cel="fringe1.0oj">fringedoj</ref>
      (<trd lng="la">Pyrrhula</trd>):
      <ekz>
        <tld/>oj <klr>[&#x2026;]</klr> estas fortikaj <klr>[&#x2026;]</klr> birdoj kun diketaj bekoj
        kaj la maskloj havas oran&gcirc;ajn a&ubreve; ru&gcirc;ajn brustojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17685">Pirolo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="oriol.0o">oriolo</ref>
    <bld lok="&WCU;/thumb/a/a7/Pyrrhula_pyrrhula_female_2.jpg/320px-Pyrrhula_pyrrhula_female_2.jpg" prm="CC BY-SA 3.0">ina E&ubreve;razia <tld/>o (Pyrrhula pyrrhula)
        <fnt>
          <aut>Francis C. Franklin, propra verko</aut>
          <lok><url ref="&WCW;/File:Pyrrhula_pyrrhula_female_2.jpg">&WCW;/File:Pyrrhula_pyrrhula_female_2.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_n;&c_ja;&c_g;&c_ib;&c_r;</trd>
  <trd lng="br">beuf</trd>
  <trdgrp lng="ca">
    <trd>g&egrave;nere Pyrrhula</trd>,
    <trd>pins&agrave; borroner</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>h&yacute;l</trd>,
    <trd>hru&scaron;ti&ccaron;ka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gimpel</trd>,
    <trd>Dompfaff</trd>
  </trdgrp>
  <trd lng="en">bullfinch</trd>
  <trd lng="fr">bouvreuil</trd>
  <trd lng="hu">s&uuml;v&ouml;lt&odblac;</trd>
  <trd lng="ja">&#x30A4;&#x30C1;&#x30E4;&#x30AF;&#x30BD;&#x30A6;</trd>
  <trd lng="nl">goudvink</trd>
  <trd lng="pl">gil</trd>
  <trdgrp lng="pt">
    <trd>dom-fafe</trd>,
    <trd>pisco-chilreiro</trd>,    
    <trd>can&ocirc;nico</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_n;&c_je;&c_g;&c_i;&c_r;&c_mol;</trd>
  <trd lng="sk">hru&scaron;ti&ccaron;ka</trd>
  <trd lng="uk">&#x441;&#x43D;&#x456;&#x433;&#x443;&#x440;</trd>
</drv>
</art>
