<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pirol</rad>/o
</kap>
<drv mrk="pirol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="pirol.0o.ZOO">
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el la familio <ref
      tip="malprt"
      cel="fringe1.0oj">fringedoj</ref>
      (<trd lng="la">Pyrrhula</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_n;&c_ja;&c_g;&c_ib;&c_r;</trd>
  <trd lng="br">beuf</trd>
  <trdgrp lng="ca">
    <trd>g&egrave;nere Pyrrhula</trd>,
    <trd>pins&agrave; borroner</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>h&yacute;l</trd>,
    <trd>hruštička</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gimpel</trd>,
    <trd>Dompfaff</trd>
  </trdgrp>
  <trd lng="en">bullfinch</trd>
  <trd lng="fr">bouvreuil</trd>
  <trd lng="hu">s&uuml;v&ouml;lt&odblac;</trd>
  <trd lng="nl">goudvink</trd>
  <trd lng="pl">gil</trd>
  <trdgrp lng="pt">
    <trd>dom-fafe</trd>,
    <trd>pisco-chilreiro</trd>,    
    <trd>can&ocirc;nico</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_n;&c_je;&c_g;&c_i;&c_r;&c_mol;</trd>
  <trd lng="sk">hruštička</trd>
</drv>
</art>
