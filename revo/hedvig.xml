<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: hedvig.xml,v 1.2 2022/03/20 15:11:16 afido Exp $">
    <kap>
      <rad>Hedvig</rad>/o, <var><kap><rad var="J">Jadvig</rad>/o</kap></var>
    </kap>

<drv mrk="hedvig.0o">
  <kap><tld/>o, <var><kap><tld var="J"/>o</kap></var></kap>
  <uzo tip="fak">NOM</uzo>
  <snc>
    <dif>
      Virina <ref tip="lst" lst="voko:personaj_nomoj" cel="nom.persona0o.GEN">nomo</ref>,
      &gcirc;ermandevena; <tld var="J"/>o estas la soraba kaj pola formo:
      <ekz>
        Sankta <tld/>o de Silezio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=476267">Hedvigo de Silezio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        mi finis la hodia&ubreve;an lecionon kun fra&ubreve;lino <tld var="J"/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la unio fini&gcirc;is en 1399 post la morto de <tld var="J"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=197292">Kreva Unio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Hedwig</trd>
    <trd lng="en">Hedwig</trd>
    <trd lng="pl">Jadwiga</trd>
  </snc>
  <trd lng="uk">&#x42F;&#x434;&#x432;&#x456;&#x491;&#x430;</trd>
</drv>

  </art>

