<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>muson</rad>/o
</kap>

<drv mrk="muson.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="vent.0o.MET">Vento</ref>
      kiu trablovas intertropikan Azion, somere de maro
      al tero, vintre de tero al maro.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_s;&c_o;&c_n;</trd>
  <trd lng="de">Monsun</trd>
  <trdgrp lng="fr">
    <trd>mousson</trd>
  </trdgrp>
  <trd lng="hu">monszun</trd>
  <trd lng="nl">moesson</trd>
  <trd lng="pt">mon&ccedil;&atilde;o <klr tip="amb">(meteor.)</klr></trd>
  <trd lng="es">monz&oacute;n</trd>
  <trd lng="ru">&c_m;&c_u;&c_s;&c_s;&c_o;&c_n;</trd>
</drv>
</art>
