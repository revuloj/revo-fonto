<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: memuar.xml,v 1.12 2020/11/02 08:32:15 revo Exp $">
<kap>
   <rad>memuar</rad>/o
</kap>
<drv mrk="memuar.0o">
   <kap>
     <tld/>o
   </kap>
   <snc mrk="memuar.0o.disertacio">
     <dif>
      <ref tip="lst" lst="voko:komunikoj" cel="disert1.0o">Disertacio</ref>
      prezentata al akademia societo:
      <ekz>
        aferoj religiaj <klr>[&#8230;]</klr> ne estas la temo de mia <tld/>o kaj postulas
        priparolon tute specialan
        <fnt>
          <aut>L. L. Zamenhof</aut>
          <vrk><url ref="https://tekstaro.com/t?nomo=gentoj-kaj-lingvo-internacia"
              >Gentoj kaj Lingvo Internacia</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o pri la kontra&ubreve;-armena genocido
        <fnt>
          <vrk><url ref="http://www.planlingvoj.ch/Stakian_memuaro_recenzo.pdf"
              >Atentinda memuaro pri la kontra&ubreve;-armena genocido en Esperanto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        histori-<tld/>o pri la Esperanto-movado
        <fnt>
          <vrk><url ref="https://www.esperantic.org/en/about-us/directors/esther-schor/"
              >Esther Schor - ESF</url></vrk>
        </fnt>.
      </ekz>
     </dif>
     <ref tip="vid" cel="memora.0o">memorando</ref>
     <rim>
       Ne konfuzu kun <ref cel="libr.memor0o">memorlibro</ref>.
     </rim>
   </snc>
   <trdgrp lng="be">
     <trd>&c_m;&c_e;&c_m;&c_u;&c_a;&c_r;</trd>,
     <trd>&c_d;&c_a;&c_k;&c_l;&c_a;&c_d;</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>Denkschrift</trd>,
     <trd>Bericht</trd>
   </trdgrp>
   <trd lng="en">memoir <klr tip="amb">(scholarly paper)</klr></trd>
   <trd lng="fr">m&eacute;moire <klr>(dissertation)</klr></trd>
   <trdgrp lng="hu">
     <trd>eml&eacute;kirat</trd>,
     <trd>memo&aacute;r</trd>
   </trdgrp>
   <trdgrp lng="nl">
     <trd>memoires</trd>,
     <trd>verhandeling</trd>
   </trdgrp>
  <trd lng="pt">mem&oacute;ria <klr tip="amb">(disserta&ccedil;&atilde;o cient&iacute;fica ou
  liter&aacute;ria)</klr></trd>
   <trdgrp lng="ru">
     <trd>&c_d;&c_o;&c_k;&c_l;&c_a;&c_d;</trd>,
     <trd>&c_d;&c_o;&c_k;&c_l;&c_a;&c_d;&c_n;&c_a;&c_ja; 
&c_z;&c_a;&c_p;&c_i;&c_s;&c_k;&c_a;</trd>,
     <trd>&c_m;&c_je;&c_m;&c_u;&c_a;&c_r; 
<klr>(&c_n;&c_a;&c_u;&c_ch;&c_n;&c_y;&c_j;, &c_u;&c_s;&c_t;.)</klr></trd>
   </trdgrp>
</drv>
</art>
