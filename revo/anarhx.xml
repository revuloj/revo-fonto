<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>anar&hcirc;i</rad>/o, anarkio
</kap>
<drv mrk="anarhx.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">POL</uzo>
    <dif>
      Socia kaj politika doktrino, kiu bazi&gcirc;as sur la principo de
      absoluta individua libereco kaj sur forigo de &ccirc;ia &scirc;tata
      regado; senregeco, senestreco.
    </dif>
    <trdgrp lng="fr">
      <trd>anarchie</trd>,
      <trd>anarchisme</trd>
    </trdgrp>
    <trd lng="sv">anarki</trd>
  </snc>
  <snc>
    <dif>
      Senordego.
    </dif>
    <trdgrp lng="de">
      <trd>Unordnung</trd>,
      <trd>Durcheinander</trd>,
      <trd>Chaos</trd>
    </trdgrp>
    <trd lng="fr">anarchie</trd>
    <trd lng="es">anarqu&iacute;a</trd>
    <trdgrp lng="sv">
      <trd>anarki</trd>,
      <trd>lagl&ouml;shet</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="de">
      <trd>Gesetzlosigkeit</trd>,
      <trd>Anarchie</trd>
  </trdgrp>
  <trd lng="en">anarchy</trd>
  <trd lng="nl">anarchie</trd>
  <trd lng="ru">&c_a;&c_n;&c_a;&c_r;&c_h;&c_i;&c_ja;</trd>
  <trd lng="sk">anarchia</trd>
</drv>

<drv mrk="anarhx.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Konforma al la <tld/>a doktrino.
    </dif>
    <trd lng="en">anarchist</trd>
    <trd lng="fr">anarchiste</trd>
    <trd lng="sk">anarchistick&yacute;</trd>
  </snc>
  <snc>
    <dif>
      Senregula, senordege, &hcirc;aosa:
      <ekz>
        <tld/>a vivmaniero.
      </ekz>
    </dif>
    <trd lng="de">chaotisch</trd>
    <trdgrp lng="en">
      <trd>anarchic</trd>,
      <trd>chaotic</trd>
    </trdgrp>
    <trd lng="fr">anarchique</trd>
  </snc>
  <trdgrp lng="de">
      <trd>gesetzlos</trd>,
      <trd>anarchisch</trd>
  </trdgrp>
  <trd lng="es">anarquista</trd>
  <trd lng="nl">anarchistisch</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_a;&c_r;&c_h;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_a;&c_n;&c_a;&c_r;&c_h;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">anarkistisk</trd>
</drv>

<drv mrk="anarhx.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      <tld lit="A"/>o 1.
    </dif>
  </snc>
  <trdgrp lng="de">
      <trd>Gesetzlosigkeit</trd>,
      <trd>Anarchie</trd>
  </trdgrp>
  <trd lng="en">anarchism</trd>
  <trd lng="fr">anarchisme</trd>
  <trd lng="nl">anarchisme</trd>
  <trd lng="ru">&c_a;&c_n;&c_a;&c_r;&c_h;&c_i;&c_z;&c_m;</trd>
  <trd lng="sv">anarkism</trd>
</drv>

<drv mrk="anarhx.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Adepto de <tld/>ismo
    </dif>
    <refgrp tip="sin">
      <ref cel="komun.0isto">komunisto</ref>,
      <ref cel="indivi.0isto">individuisto</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Anarchist</trd>
  <trd lng="en">anarchist</trd>
  <trd lng="fr">anarchiste</trd>
  <trd lng="es">anarquista</trd>
  <trd lng="nl">anarchist</trd>
  <trd lng="ru">&c_a;&c_n;&c_a;&c_r;&c_h;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">anarchista</trd>
  <trd lng="sv">anarkist</trd>
</drv>

<drv mrk="anarhx.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Homo kies konduto kaj vivmaniero estas senregula, senordega.
    </dif>
  </snc>
  <trdgrp lng="de">
      <trd>Anarch</trd>,
      <trd>Chaot</trd>
  </trdgrp>
  <trd lng="en">anarchist</trd>
  <trd lng="es">anarquista</trd>
  <trd lng="fr">anarchiste</trd>
  <trd lng="ru">&c_a;&c_n;&c_a;&c_r;&c_h;&c_i;&c_s;&c_t;</trd>
</drv>
</art>
