<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cxibuk.xml,v 1.8 2024/02/01 09:24:11 revo Exp $">
<kap>
  <rad>&ccirc;ibuk</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="cxibuk.0o">
  <kap><tld/>o</kap>
  <snc mrk="cxibuk.0o.FUM">
    <dif>
      Tre longtuba <ref tip="super" cel="pip.0o.FUM">pipo</ref>,
      por fumi tabakon, populara en Okcidentazio:
      <ekz>
        enspirante tabakfumon el longaj <tld/>oj
        <fnt>
          <aut>N. Gogol, trad. A. Kor&jcirc;enkov</aut>,
          <vrk><url ref="&LOdE;Rn-gogl2.htm">
          Palto</url></vrk>,
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        jam ferminte la okulojn kaj nek viva nek morta li estis preta gustumi &ccirc;erkesan
        <tld/>on
        <fnt>
          <bib>LOdE</bib>
          <aut>Nikolai Gogol, trad. Valentin Melnikov</aut>
          <vrk><url ref="&Tekstaro;?nomo=ondo-de-esperanto">Malvivaj animoj</url></vrk>
          <lok>&ccirc;ap. 4</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_b;&c_u;&c_k; <klr tip="amb">(&c_t;&c_r;&c_u;&c_b;&c_k;&c_a;)</klr></trd>
  <trd lng="de">Tschibuk</trd>
  <trd lng="en">chibouk</trd>
  <trd lng="fa">&f_ch;&f_p;&a_q;</trd>
  <trd lng="fr">chibouque</trd>
  <trd lng="pl">chibouk<klr> (rodzaj fajki)</klr></trd>
  <trd lng="ru">&c_ch;&c_u;&c_b;&c_u;&c_k;</trd>
  <trd lng="tr">&ccedil;ibuk</trd>
  <trdgrp lng="uk">
    <trd>&c_ch;&c_u;&c_b;&c_u;&#x43A;</trd>,
    <trd>&c_c;&c_i;&c_b;&c_u;&c_h;</trd>
  </trdgrp>
</drv>
</art>

