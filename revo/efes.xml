<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: efes.xml,v 1.17 2023/11/07 19:22:22 revo Exp $">
<kap>
  <rad>Efes</rad>/o <fnt><bib>NT</bib></fnt>,
  <var><kap><rad var="z">Efez</rad>/o <fnt><bib>PIV2</bib></fnt></kap></var>
</kap>

<drv mrk="efes.0o">
  <kap>
    <tld/>o,
    <var><kap><tld var="z"/>o</kap></var>
  </kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">BIB</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Antikva <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref> sur la
      okcidenta marbordo de <ref tip="malprt" cel="anatoli.0o">Anatolio</ref>, ne plu
      ekzistanta, fama pro sia templo de Artemiso, letero de
      apostolo Pa&ubreve;lo kaj koncilio:
      <ekz>
        ili alvenis en <tld/>on, kaj li lasis ilin tie
        <fnt><bib>NT</bib>, <lok>&Ago; 18:19.</lok></fnt>;
      </ekz>
      <ekz>
        ne sole en <tld/>o, sed anka&ubreve; tra preska&ubreve; la tuta Azio, &ccirc;i tiu Pa&ubreve;lo jam influis
        kaj forturnis multe da homoj
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 19:26</lok>
        </fnt>;
      </ekz>
      <ekz>
        Pandareo <klr>[&#x2026;]</klr> estas prezentata filo de Meropso kaj
        <nom>Klimena</nom>, aliakaze
        filo de Androklo, fondinto de <tld var="z"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=528792">Pandareo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_E;&c_f;&c_e;&c_s;</trd>
  <trd lng="bg">&c_Je;&c_f;&c_je;&c_s;</trd>
  <trd lng="de">Ephesos</trd>
  <trd lng="es">&Eacute;feso</trd>
  <trd lng="en">Ephesus</trd>
  <trd lng="fa">&a_A;&a_kasra;&a_f;&a_kasra;&a_s;&a_damma;&a_s;</trd>
  <trd lng="fr">Eph&egrave;se</trd>
  <trd lng="id">Efesus</trd>
  <trd lng="ca">Efes</trd>
  <trd lng="ja">&#x30A8;&#x30D5;&#x30A7;&#x30BD;&#x30B9;</trd>
  <trd lng="pt">&Eacute;feso</trd>
  <trdgrp lng="hu">
    <trd>Epheszosz</trd>,
    <trd>Efezus</trd>
  </trdgrp>
  <trd lng="pl">Efez</trd>
  <trd lng="ru">&c_E;&c_f;&c_je;&c_s;</trd>
  <trd lng="zh">&#x4EE5;&#x5F17;&#x6240; <pr>y&#x1D0;f&#xFA;su&#x1D2;</pr></trd>
</drv>

<drv mrk="efes.0ano">
  <kap><tld lit="e"/>ano</kap>
  <snc>
  <uzo tip="fak">BIB</uzo>
  <uzo tip="fak">HIS</uzo>
	<dif>
	  Lo&gcirc;anto de <tld/>o:
	  <ekz>
	    ho <tld lit="e"/>anoj, &ccirc;u do ekzistas homo nescianta, ke
	    la urbo de la <tld lit="e"/>anoj estas templogardanto de
	    Artemis la granda kaj de la el&ccirc;iela falinta&jcirc;o
	    <fnt><bib>NT</bib>, <lok>&Ago; 19:35.</lok></fnt>?
	  </ekz>
	</dif>
    <trd lng="be">&c_e;&c_f;&c_e;&c_s;&c_ja;&c_n;&c_ib;&c_n;</trd>
    <trd lng="de">Ephesier</trd>
    <trd lng="es">efesio</trd>
    <trd lng="fr">Eph&egrave;sien<klr> (subst.)</klr></trd>
    <trd lng="ca">efesi</trd>
    <trd lng="pt">ef&eacute;sio</trd>
    <trdgrp lng="hu">
      <trd>epheszoszi<klr tip="amb"> (lakos)</klr></trd>,
      <trd>efezusi<klr tip="amb"> (lakos)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
	  <trd>&c_e;&c_f;&c_je;&c_s;&c_ja;&c_n;&c_i;&c_n;</trd>,
	  <trd>&c_je;&c_f;&c_je;&c_s;&c_ja;&c_n;&c_i;&c_n;<klr tip="amb">
	  (&c_u;&c_s;&c_t;.)</klr></trd>
    </trdgrp>
  </snc>
</drv>

</art>

