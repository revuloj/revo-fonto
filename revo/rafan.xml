<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rafan</rad>/o
</kap>
<drv mrk="rafan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="brasik1.0oj">brasikacoj</ref>
      (<trd lng="la">Raphanus</trd>), kies dika tubera
      diverskolora radiko estas man&gcirc;ebla.
    </dif>
  </snc>
  <trd lng="de">Rettich</trd>
  <trd lng="en">radish</trd>
  <trd lng="nl">ramenas</trd>
  <trd lng="ru">&c_r;&c_je;&c_d;&c_mol;&c_k;&c_a;</trd>
</drv>

<drv mrk="rafan.0eto">
  <kap><tld/>eto</kap>
  <snc>
    <dif>
      Varia&jcirc;o de <tld/>o kun malgranda ekstere ru&gcirc;a radiko.
    </dif>
  </snc>
  <trd lng="de">Radieschen</trd>
  <trd lng="fr">radis</trd>
  <trd lng="nl">radijs</trd>
  <trd lng="ru">&c_r;&c_je;&c_d;&c_i;&c_s;</trd>
</drv>
</art>
