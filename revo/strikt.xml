<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: strikt.xml,v 1.18 2021/04/04 07:50:49 revo Exp $">
<kap>
  <rad>strikt</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="strikt.0a">
  <kap><tld/>a</kap>
  <snc mrk="strikt.0a.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Al&gcirc;ustigita, mal<ref tip="ant" cel="puf.0a">pufa</ref>:
      <ekz>
        <nom>Joab</nom> havis sur si <tld/>an veston
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 20:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj kolegoj nur strabas al &scirc;ia <tld/>a pulovero
        <fnt>
          <bib>KrB</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="vast.mal0a.space">malvasta<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_a;&c_b;&c_l;&c_ja;&c_g;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>, 
      <trd>&c_sh;&c_ch;&c_y;&c_l;&c_mol;&c_n;&c_y;</trd>, 
      <trd>&c_sh;&c_ch;&c_y;&c_l;&c_mol;&c_n;&c_a;
      <ind>&c_p;&c_r;&c_y;&c_l;&c_ja;&c_g;&c_a;&c_ju;&c_ch;&c_y;</ind></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>straff</trd>,
      <trd>enganliegend</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>nauwsluitend</trd>,
      <trd>strak</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>szoros</trd>,
      <trd>sz&udblac;k</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>obcis&lstroke;y</trd>,
      <trd>opi&eogonek;ty</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_t;&c_ja;&c_g;&c_i;&c_v;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>,
      <trd>&c_p;&c_l;&c_o;&c_t;&c_n;&c_o;
      &c_p;&c_r;&c_i;&c_l;&c_je;&c_g;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>,
      <trd>&c_o;&c_b;&c_l;&c_je;&c_g;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="strikt.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref cel="sever.0a">Severe</ref> preciza, <ref cel="rigor.0a">rigora</ref>, 
      ne permesanta deflanki&gcirc;on:
      <ekz>
        la <tld/>a senco de la vorto
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        argumentoj de la plej <tld/>a logiko
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a observado de tiel difinita principo de ne&ubreve;traleco
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        temas nur pri lar&gcirc;aj principoj kaj ne pri le&gcirc;aj reguloj a&ubreve; <tld/>aj ordonoj
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiel vi &ccirc;i-foje ne a&ubreve;digas vian <tld/>an malpermeson NE FARU
        <fnt>
          <bib>SkandalJozef</bib>
        </fnt>?!
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="discip.0a">disciplina</ref>,
      <ref cel="ekzakt.0a">ekzakta</ref>,
      <ref cel="rigid.0a.necedeme">rigida<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_d;&c_a;&c_k;&c_l;&c_a;&c_d;&c_n;&c_y;</trd>, 
      <trd>&c_s;&c_t;&c_r;&c_o;&c_g;&c_ib;</trd>, 
      <trd>&c_n;&c_ja;&c_w;&c_h;&c_ib;&c_l;&c_mol;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>strikt</trd>,
      <trd>streng</trd>,
      <trd>genau</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>szoros</trd>,
      <trd>szigor&uacute;</trd>
    </trdgrp>
    <trd lng="nl">streng</trd>
    <trdgrp lng="pl">
      <trd>&sacute;cis&lstroke;y</trd>,
      <trd>co do <ind>joty</ind></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_t;&c_r;&c_o;&c_g;&c_i;&c_j;</trd>,
      <trd>&c_t;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_u;&c_k;&c_o;&c_s;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>p&rcaron;esn&yacute;</trd>,
    <trd>p&rcaron;il&eacute;hav&yacute;</trd>,
    <trd>p&rcaron;&iacute;sn&yacute;</trd>,
    <trd>striktn&iacute;</trd>,
    <trd>stroh&yacute;</trd>,
    <trd>t&ecaron;sn&yacute;</trd>,
    <trd>&uacute;zk&yacute;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>serr&eacute;</trd>,
    <trd>strict</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>presn&yacute;</trd>,
    <trd>priliehav&yacute;</trd>,
    <trd>pr&iacute;sny</trd>,
    <trd>striktn&yacute;</trd>,
    <trd>stroh&yacute;</trd>,
    <trd>tesn&yacute;</trd>,
    <trd>&uacute;zky</trd>
  </trdgrp>
</drv>

<drv mrk="strikt.0e">
  <kap>
    <tld/>e
  </kap>
  <dif>
    En <tld/>a maniero:
  </dif>
  <snc ref="strikt.0a.TEKS">
    <ekz>
      la ru&gcirc;a kamizolo sidis sur mi <tld/>e kaj stre&ccirc;ite
      <fnt>
        <bib>Fab1</bib>
        <lok>Ligo de amikeco</lok>
      </fnt>;
    </ekz>
    <ekz>
      li surhavis baletan pantalonon kaj blankan torsoveston, kiuj tiel <tld/>e
      glui&gcirc;is al la korpo, ke &ccirc;iuj detaloj de ties reliefo tute klare videblis
      <fnt>
        <bib>ChC</bib>
      </fnt>.
    </ekz>
  </snc>
  <snc ref="strikt.0a.FIG">
    <ekz>
      li ne faras ion pli ol li <tld/>e devas
      <fnt>
        <bib>ChC</bib>
      </fnt>;
    </ekz>
    <ekz>
      &ccirc;io estos <tld/>e konfidenca
      <fnt>
        <bib>ChB</bib>
      </fnt>;
    </ekz>
    <ekz>
      la amplekso de nia reala stomako estas <tld/>e difinita
      <fnt>
        <bib>VaK</bib>
      </fnt>;
    </ekz>
    <ekz>
      originalaj poemoj <tld/>e ligitaj al la stilo de la unuatempa skolo
      <fnt>
        <bib>EeP</bib>
      </fnt>.
    </ekz>
  </snc>
</drv>


<drv mrk="strikt.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="strikt.0igi.KOMUNE">
    <dif>
      Igi <tld/>a, mallar&gcirc;igi, malvastigi:
      <ekz>
        <tld/>igi la regulojn
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.liberafolio.org/2007/sentero">
          Esperanta retbutiko sekvas rusiajn le&gcirc;ojn</url></vrk>,
          <lok>libera folio, 2007-04-04</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;tato <klr>[&#8230;]</klr> <tld/>igis la subvenciojn destinitajn al la kampara
        disvolvado
        <fnt>
          <bib>MD02</bib>
          <vrk>La dan&gcirc;eroj de la internacia helpismo</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>versch&auml;rfen</trd>,
    <trd>enger <ind>auslegen</ind></trd>,
    <trd>k&uuml;rzen <klr>(Mittel)</klr></trd>
  </trdgrp>
  <trd lng="fr">serrer</trd>
</drv>

<drv mrk="strikt.mal0a">
  <kap>mal<tld/>a</kap>
  <dif>
    Ne <tld/>a, ne prema, sed libera;
    <ref tip="sin" cel="loz.0a.KOMUNE">loza</ref>:
  </dif>
  <snc mrk="strikt.mal0a.KOMUNE" ref="strikt.0a.TEKS">
    <ekz>
      &scirc;ia mallar&gcirc;a malgranda staturo estis volvita en mal<tld/>an silkan veston
      <fnt>
        <aut>Michael Ende, trad. Wolfram Diestel</aut>
        <vrk>La Sen&ccirc;esa Rakonto</vrk>
        <lok>La Flugo al la Ebura Turo</lok>
      </fnt>.
    </ekz>
  </snc>
  <snc ref="strikt.0a.FIG">
    <ekz>
      mal<tld/>a konfederacio
      <fnt>
        <aut>J. S. Larsen</aut>,
        <vrk><url ref="http://www.ipernity.com/blog/jens_s_larsen/242584">
        Amikejo: la Esperantolando kiu preska&ubreve; ekzistis</url></vrk>,
        <lok>2011-10-24</lok>
      </fnt>;
    </ekz>
    <ekz>
      &ccirc;u do la normo de EU estas tro mal<tld/>a
      <fnt>
        <bib>Monato</bib>
        <aut>Pa&ubreve;l Peeraerts</aut>
        <vrk><url ref="https://www.monato.be/2008/009656.php">Belgio pintas</url></vrk>
        <lok>2008</lok>
      </fnt>?
    </ekz>
  </snc>
  <trdgrp lng="cs">
    <trd>nenapjat&yacute;</trd>,
    <trd>voln&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>locker</trd>,
    <trd>lose</trd>,
    <trd>weit <klr>(Kleid)</klr></trd>
  </trdgrp>
  <trd lng="fr">rel&acirc;ch&eacute;</trd>
  <trdgrp lng="sk">
    <trd>nenap&auml;t&yacute;</trd>,
    <trd>vo&lcaron;n&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="strikt.mal0igi">
  <kap>mal<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <dif>
      Igi mal<tld/>a, ne prema, iom vasta:
  </dif>
  <snc mrk="strikt.mal0igi.KOMUNE" ref="strikt.0a.TEKS">    
    <ekz>
      la junuloj mal<tld/>igadis siajn zonojn kaj bate 
      forigis la kampopolvon desur siaj kiteloj
      <fnt>
        <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
        <vrk>Montara Vila&gcirc;o</vrk>,
        <lok>1984</lok>
      </fnt>.
    </ekz>
  </snc>
  <snc ref="strikt.0a.FIG">
    <ekz>
      la germanoj <klr>[&#8230;]</klr> forte pledas por mal<tld/>igi tiun pakton
      <fnt>
        <bib>Monato</bib>
        <aut>Stefan Maul</aut>
        <vrk><url ref="https://www.monato.be/2005/008595.php">Pretekstoj kontra&ubreve; pakto</url></vrk>
        <lok>2005</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="de">lockern</trd>
  <trdgrp lng="fr">
    <trd>desserrer</trd>,
    <trd>rel&acirc;cher</trd>
  </trdgrp>
</drv>
</art>
