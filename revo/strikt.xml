    <trd>&#x677E;&#x5F1B; <pr>s&#x14D;ngch&#xED;</pr></trd>,
    <trd>&#x9B06;&#x5F1B; <pr>s&#x14D;ngch&#xED;</pr></trd>,
    <trd>&#x677E;&#x6563; <pr>s&#x14D;ngsan</pr></trd>,
    <trd>&#x9B06;&#x6563; <pr>s&#x14D;ngsan</pr></trd>,
    <trd>&#x61C8; <pr>xi&#xE8;</pr></trd>,
    <trd>&#x677E; <pr>s&#x14D;ng</pr></trd>
