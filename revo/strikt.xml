<drv mrk="strikt.0a">
  <kap><tld/>a</kap>
  <snc mrk="strikt.0a.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Al&gcirc;ustigita, mal<ref tip="ant" cel="puf.0a">pufa</ref>:
      <ekz>
        <nom>Joab</nom> havis sur si <tld/>an veston
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 20:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj kolegoj nur strabas al &scirc;ia <tld/>a pulovero
        <fnt>
          <bib>KrB</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="vast.mal0a.space">malvasta<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_a;&c_b;&c_l;&c_ja;&c_g;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
      <trd>&c_sh;&c_ch;&c_y;&c_l;&c_mol;&c_n;&c_y;</trd>,
      <trd>&c_sh;&c_ch;&c_y;&c_l;&c_mol;&c_n;&c_a;
      <ind>&c_p;&c_r;&c_y;&c_l;&c_ja;&c_g;&c_a;&c_ju;&c_ch;&c_y;</ind></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>straff</trd>,
      <trd>enganliegend</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>close-fitting</trd>,
      <trd>tight</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>nauwsluitend</trd>,
      <trd>strak</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>szoros</trd>,
      <trd>sz&udblac;k</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>obcis&lstroke;y</trd>,
      <trd>opi&eogonek;ty</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_t;&c_ja;&c_g;&c_i;&c_v;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>,
      <trd>&c_p;&c_l;&c_o;&c_t;&c_n;&c_o;
      &c_p;&c_r;&c_i;&c_l;&c_je;&c_g;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>,
      <trd>&c_o;&c_b;&c_l;&c_je;&c_g;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="strikt.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref cel="sever.0a">Severe</ref> preciza, <ref cel="rigor.0a">rigora</ref>,
      ne permesanta deflanki&gcirc;on:
      <ekz>
        la <tld/>a senco de la vorto
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        argumentoj de la plej <tld/>a logiko
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a observado de tiel difinita principo de ne&ubreve;traleco
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        temas nur pri lar&gcirc;aj principoj kaj ne pri le&gcirc;aj reguloj a&ubreve; <tld/>aj ordonoj
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiel vi &ccirc;i-foje ne a&ubreve;digas vian <tld/>an malpermeson NE FARU
        <fnt>
          <bib>SkandalJozef</bib>
        </fnt>?!
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="discip.0a">disciplina</ref>,
      <ref cel="ekzakt.0a">ekzakta</ref>,
      <ref cel="rigid.0a.necedeme">rigida<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_d;&c_a;&c_k;&c_l;&c_a;&c_d;&c_n;&c_y;</trd>,
      <trd>&c_s;&c_t;&c_r;&c_o;&c_g;&c_ib;</trd>,
      <trd>&c_n;&c_ja;&c_w;&c_h;&c_ib;&c_l;&c_mol;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>strikt</trd>,
      <trd>streng</trd>,
      <trd>genau</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>strict</trd>,
      <trd>stringent</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>szoros</trd>,
      <trd>szigor&uacute;</trd>
    </trdgrp>
    <trd lng="nl">streng</trd>
    <trdgrp lng="pl">
      <trd>&sacute;cis&lstroke;y</trd>,
      <trd>co do <ind>joty</ind></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_t;&c_r;&c_o;&c_g;&c_i;&c_j;</trd>,
      <trd>&c_t;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_u;&c_k;&c_o;&c_s;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>p&rcaron;esn&yacute;</trd>,
    <trd>p&rcaron;il&eacute;hav&yacute;</trd>,
    <trd>p&rcaron;&iacute;sn&yacute;</trd>,
    <trd>striktn&iacute;</trd>,
    <trd>stroh&yacute;</trd>,
    <trd>t&ecaron;sn&yacute;</trd>,
    <trd>&uacute;zk&yacute;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>serr&eacute;</trd>,
    <trd>strict</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x53B3;&#x683C;&#x306A;  <pr>&#x3052;&#x3093;&#x304B;&#x304F;&#x306A;</pr></trd>,
    <trd>&#x7AAE;&#x5C48;&#x306A;  <pr>&#x304D;&#x3085;&#x3046;&#x304F;&#x3064;&#x306A;</pr></trd>,
    <trd>&#x3074;&#x3063;&#x305F;&#x308A;&#x5408;&#x3063;&#x305F;  <pr>&#x3074;&#x3063;&#x305F;&#x308A;&#x3042;&#x3063;&#x305F;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>presn&yacute;</trd>,
    <trd>priliehav&yacute;</trd>,
    <trd>pr&iacute;sny</trd>,
    <trd>striktn&yacute;</trd>,
    <trd>stroh&yacute;</trd>,
    <trd>tesn&yacute;</trd>,
    <trd>&uacute;zky</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&#x43E;&c_ch;&#x43D;&#x43E; &c_v;&c_i;&c_z;&#x43D;&c_a;&c_ch;&c_je;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_t;&#x43E;&c_ch;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_t;&c_r;&#x43E;&c_g;&c_i;&c_j;</trd>,
    <trd>&#x43D;&c_je;&c_u;&c_h;&c_i;&#x43B;&#x44C;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43E;&c_b;&c_t;&c_i;&c_s;&#x43B;&c_i;&c_j;</trd>,
