<drv mrk="strikt.mal0igi">
  <kap>mal<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <dif>
      Igi mal<tld/>a, ne prema, iom vasta:
  </dif>
  <snc mrk="strikt.mal0igi.KOMUNE" ref="strikt.0a.TEKS">
    <ekz>
      la junuloj mal<tld/>igadis siajn zonojn kaj bate
      forigis la kampopolvon desur siaj kiteloj
      <fnt>
        <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
        <vrk>Montara Vila&gcirc;o</vrk>,
        <lok>1984</lok>
      </fnt>.
    </ekz>
  </snc>
  <snc ref="strikt.0a.FIG">
    <ekz>
      la germanoj <klr>[&#x2026;]</klr> forte pledas por mal<tld/>igi tiun pakton
      <fnt>
        <bib>Monato</bib>
        <aut>Stefan Maul</aut>
        <vrk><url ref="&Monato;/2005/008595.php">Pretekstoj kontra&ubreve; pakto</url></vrk>
        <lok>2005</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_n;&c_ja;&c_v;&c_o;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_s;&c_l;&c_a;&c_b;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">lockern</trd>
  <trdgrp lng="en">
    <trd>loosen</trd>,
    <trd>make lax</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>desserrer</trd>,
    <trd>rel&acirc;cher</trd>
  </trdgrp>
  <trd lng="pl">luzowa&cacute;</trd>
  <trdgrp lng="zh">
    <trd>&#x95D3; <pr>k&#x1CE;i</pr></trd>,
    <trd>&#x95FF; <pr>k&#x1CE;i</pr></trd>,
