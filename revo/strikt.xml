<drv mrk="strikt.mal0a">
  <kap>mal<tld/>a</kap>
  <dif>
    Ne <tld/>a, ne prema, sed libera;
    <ref tip="sin" cel="loz.0a.KOMUNE">loza</ref>:
  </dif>
  <snc mrk="strikt.mal0a.KOMUNE" ref="strikt.0a.TEKS">
    <ekz>
      &scirc;ia mallar&gcirc;a malgranda staturo estis volvita en mal<tld/>an silkan veston
      <fnt>
        <aut>Michael Ende, trad. Wolfram Diestel</aut>
        <vrk>La Sen&ccirc;esa Rakonto</vrk>
        <lok>La Flugo al la Ebura Turo</lok>
      </fnt>.
    </ekz>
  </snc>
  <snc ref="strikt.0a.FIG">
    <ekz>
      mal<tld/>a konfederacio
      <fnt>
        <aut>J. S. Larsen</aut>,
        <vrk><url ref="http://www.ipernity.com/blog/jens_s_larsen/242584">
        Amikejo: la Esperantolando kiu preska&ubreve; ekzistis</url></vrk>,
        <lok>2011-10-24</lok>
      </fnt>;
    </ekz>
    <ekz>
      &ccirc;u do la normo de EU estas tro mal<tld/>a
      <fnt>
        <bib>Monato</bib>
        <aut>Pa&ubreve;l Peeraerts</aut>
        <vrk><url ref="&Monato;/2008/009656.php">Belgio pintas</url></vrk>
        <lok>2008</lok>
      </fnt>?
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_s;&c_t;&c_r;&c_o;&c_g;&c_ib;</trd>,
    <trd>&c_n;&c_ja;&c_sh;&c_ch;&c_y;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nenapjat&yacute;</trd>,
    <trd>voln&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>locker</trd>,
    <trd>lose</trd>,
    <trd>weit <klr>(Kleid)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>lax</trd>,
    <trd>baggy</trd>,
    <trd>loose</trd>
  </trdgrp>
  <trd lng="fr">rel&acirc;ch&eacute;</trd>
  <trdgrp lng="ja">
    <trd>&#23515;&#22823;&#12394;  <pr>&#12363;&#12435;&#12384;&#12356;&#12394;</pr></trd>,
