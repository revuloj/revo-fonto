    <trd>&#x4E25;&#x683C; <pr>y&#xE1;ng&#xE9;</pr></trd>,
    <trd>&#x56B4;&#x683C; <pr>y&#xE1;ng&#xE9;</pr></trd>,
    <trd>&#x4ECE;&#x4E25; <pr>c&#xF3;ngy&#xE1;n</pr></trd>,
    <trd>&#x5F9E;&#x56B4; <pr>c&#xF3;ngy&#xE1;n</pr></trd>,
    <trd>&#x68EE;&#x4E25; <pr>s&#x113;ny&#xE1;n</pr></trd>,
    <trd>&#x68EE;&#x56B4; <pr>s&#x113;ny&#xE1;n</pr></trd>,
    <trd>&#x7D27;&#x7EF7;&#x7EF7; <pr>j&#x1D0;nb&#x113;ngb&#x113;ng</pr></trd>,
