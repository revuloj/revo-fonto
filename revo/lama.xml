<art mrk="$Id: lama.xml,v 1.28 2024/01/15 16:14:08 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>lama</rad>/o
</kap>
<drv mrk="lama.0o">
  <kap><tld/>o</kap>
  <snc mrk="lama.0o.BUD">
    <uzo tip="fak">BUD</uzo>
    <dif>
      <ref tip="super" cel="majstr.0o.rabeno">Majstro</ref> en tibeta
      kaj mongola budaismo, kaj preska&ubreve; &ccirc;iam anka&ubreve;
      <ref tip="super" cel="monahx.0o">mona&hcirc;o</ref>:
      <ekz>
        Alexandra sukcesis trapasi la tibetan limon vestinte sin kiel
        almozulino kune kun fidela <tld/>o Yongden
        <fnt>
          <aut>R. Roze</aut>,
          <vrk>Verkistino - orientalistino - esploristino</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 1999a, numero 1a, p. 17a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_m;&c_a; <klr>(&c_b;&c_u;&c_d;&c_y;&c_j;&c_s;&c_k;&c_ib; &c_m;&c_a;&c_n;&c_a;&c_h;)</klr></trd>
  <trd lng="bg">&c_l;&c_a;&c_m;&c_a;<klr tip="amb"> (&c_b;&c_u;&c_d;&c_i;&c_z;&c_malmol;&c_m;)</klr></trd>
  <trd lng="bo">&#xF56;&#xFB3;&#xF0B;&#xF58;&#xF0B;</trd>
  <trd lng="br">lama<klr tip="ind"> (manac'h)</klr></trd>
  <trd lng="ca">lama</trd>
  <trd lng="cs">l&aacute;ma</trd>
  <trd lng="de">Lama<klr tip="amb"> (Buddhismus)</klr></trd>
  <trd lng="en">lama</trd>
  <trd lng="es">lama</trd>
  <trd lng="fr">lama<klr> (bouddhisme)</klr></trd>
  <trd lng="hu">l&aacute;ma <klr tip="ind">(szerzetes)</klr></trd>
  <trd lng="ja">&#x30E9;&#x30DE;&#x50E7;  <pr>&#x3089;&#x307E;&#x305D;&#x3046;</pr></trd>
  <trd lng="nl">lama</trd>
  <trd lng="no">lama</trd>
  <trd lng="pl">lama <klr>(religioznawstwo)</klr></trd>
  <trd lng="pt">lama <klr tip="amb">(sacerdote)</klr></trd>
  <trd lng="ro">lama <klr>(budism)</klr></trd>
  <trd lng="ru">&c_l;&c_a;&c_m;&c_a; <klr>(&c_b;&c_u;&c_d;&c_d;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j; &c_m;&c_o;&c_n;&c_a;&c_h;)</klr></trd>
  <trd lng="sk">l&aacute;ma</trd>
  <trd lng="uk">&#x43B;&#x430;&#x43C;&#x430; (&#x431;&#x443;&#x434;&#x434;&#x456;&#x439;&#x441;&#x44C;&#x43A;&#x438;&#x439; &#x43C;&#x43E;&#x43D;&#x430;&#x445;)</trd>
  <trd lng="zh">&#x5587;&#x561B; <pr>l&#x1CE;ma</pr></trd>
</drv>
