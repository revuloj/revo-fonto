<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>lama</rad>/o
</kap>
<drv mrk="lama.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BUD</uzo>
  <snc>
    <dif>
      Budaisma <ref tip="super" cel="monahx.0o">mona&hcirc;o</ref> en tibeta kaj
      mongola tradicio:
      <ekz>
        Alexandra sukcesis trapasi la tibetan limon vestinte sin kiel
        almozulino kune kun fidela <tld/>o Yongden<fnt><aut>Robert
        Roze</aut>: <vrk>Verkistino - orientalistino - esploristino</vrk>,
        <bib>Monato</bib><lok>jaro 1999a, numero 1a, p. 17a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Lama</trd>
  <trd lng="en">lama</trd>
  <trd lng="es">lama</trd>
  <trdgrp lng="fr">
    <trd>lama<klr> (bouddhisme)</klr></trd>
  </trdgrp>
  <trd lng="nl">lama</trd>
  <trd lng="pt">lama</trd>
  <trd lng="ru">&c_l;&c_a;&c_m;&c_a; <klr>(&c_b;&c_u;&c_d;&c_d;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j; &c_m;&c_o;&c_n;&c_a;&c_h;)</klr></trd>
</drv>
</art>
