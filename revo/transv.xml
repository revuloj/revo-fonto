<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: transv.xml,v 1.16 2022/03/28 14:44:12 revo Exp $">
<kap>
  <rad>transvers</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="transv.0a">
  <kap><tld/>a</kap>
  <snc>
    <uzo tip="klr">(pp linio)</uzo>
    <dif>
      Tratran&ccirc;anta perpendikulare a&ubreve; oblikve tra la &ccirc;efa direkto de
      ia
      linio a&ubreve; objekto:
      <ekz>
        <tld/>a trabo de fervojo<fnt>B</fnt>;
      </ekz>
      <ekz>
        la <tld/>aj strioj de la tigra felo;
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="largx">la&ubreve;lar&gcirc;a</ref>,
      <ref cel="diagon">diagonala</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_p;&c_ja;&c_r;&c_o;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>p&#x159;&#xED;&#x10D;n&#xFD;</trd>,
    <trd>transverz&#xE1;ln&#xED;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>transversal</trd>,
    <trd>quer verlaufend</trd>
  </trdgrp>
  <trd lng="en">transverse</trd>
  <trd lng="fr">transversal</trd>
  <trdgrp lng="hu">
    <trd>&aacute;tl&oacute;s</trd>,
    <trd>har&aacute;nt-</trd>,
    <trd>r&eacute;zsutos</trd>
  </trdgrp>
  <trd lng="nl">dwars</trd>
  <trd lng="pt">transversal</trd>
  <trd lng="ru">&c_p;&c_o;&c_p;&c_je;&c_r;&c_je;&c_ch;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>prie&#x10D;ny</trd>,
    <trd>transverz&#xE1;lny</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x43E;&#x43F;&#x435;&#x440;&#x435;&#x447;&#x43D;&#x438;&#x439;</trd>
</drv>

<drv mrk="transv.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      En <tld/>a direkto.
      <ekz>
        <tld/>e de io<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_p;&c_ja;&c_r;&c_o;&c_k;</trd>,
    <trd>&c_u;&c_p;&c_o;&c_p;&c_je;&c_r;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nap&#x159;&#xED;&#x10D;</trd>,
    <trd>p&#x159;es</trd>,
    <trd>p&#x159;&#xED;&#x10D;n&#x11B;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>quer</trd>,
    <trd>schr&auml;g</trd>
  </trdgrp>
  <trd lng="en">transversely</trd>
  <trdgrp lng="hu">
    <trd>&aacute;tl&oacute;san</trd>,
    <trd>har&aacute;nt</trd>,
    <trd>r&eacute;zsutosan</trd>
  </trdgrp>
  <trd lng="nl">dwars</trd>
  <trd lng="ru">&c_p;&c_o;&c_p;&c_je;&c_r;&c_jo;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>naprie&#x10D;</trd>,
    <trd>prie&#x10D;ne</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x43E;&#x43F;&#x435;&#x440;&#x435;&#x43A;</trd>
</drv>

<drv mrk="transv.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="T"/>a linio.
      <ekz>
        la&ubreve; la <tld/>o<fnt>B</fnt>;
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_p;&c_ja;&c_r;&c_o;&c_ch;&c_y;&c_n;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_p;&c_ja;&c_r;&c_o;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_v;&c_e;&c_r;&c_s;&c_a;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>p&#x159;&#xED;&#x10D;ka</trd>,
    <trd>transverz&#xE1;la</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Transversale</trd>,
    <trd>Schnittgerade</trd>
  </trdgrp>
  <trd lng="en">transversal</trd>
  <trd lng="fr">transversale</trd>
  <trdgrp lng="hu">
    <trd>&aacute;tl&oacute;</trd>,
    <trd>har&aacute;ntvonal</trd>,
  </trdgrp>
  <trd lng="nl">dwarslijn</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_p;&c_je;&c_r;&c_je;&c_ch;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_v;&c_je;&c_r;&c_s;&c_a;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>prie&#x10D;ka</trd>,
    <trd>prie&#x10D;na &#x10D;iara</trd>,
    <trd>transverz&#xE1;la</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43F;&#x435;&#x440;&#x435;&#x447;&#x43D;&#x430; &#x43B;&#x456;&#x43D;&#x456;&#x44F;</trd>,
    <trd>&#x43F;&#x43E;&#x43F;&#x435;&#x440;&#x435;&#x447;&#x43D;&#x438;&#x439; &#x43C;&#x2019;&#x44F;&#x437;</trd>
  </trdgrp>
</drv>
</art>
