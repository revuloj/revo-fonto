<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ziter</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="ziter.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="instru2.kord0o">Kordinstrumento</ref>, eta
      <ref tip="vid" cel="liut.0o">liuto</ref> populara en E&ubreve;ropo
      inter la 16 kaj la 18a jarcentoj, kun pir-silueta, plata, ligna kesto,
      longa kolo, 30 &gcirc;is 45 metalaj, pin&ccirc;e sonigataj kordoj.
    </dif>
  </snc>
  <rim>
  Konfuzo inter <ref tip="vid" cel="citr.0o">citro</ref>
  kaj zitero oftas en diverslingvaj referencverkoj.
  <aut>&jcirc;v</aut>
  </rim>
  <trdgrp lng="en">
    <trd>cittern</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cistre</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>cetera</trd>
  </trdgrp>
</drv>

<drv mrk="ziter.hispana.0o">
  <kap>hispana <tld/>o</kap> <fnt><bib>MuzTerm</bib></fnt>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Hispana <ref tip="super" cel="instru2.kord0o">kordinstrumento</ref>
      uzata en urbaj popolaj bandoj,
      kun mallonga kolo kaj ses duoblaj kordoj.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="banduri.0o">bandurio</ref>
  </refgrp>
  <trdgrp lng="es">
    <trd>bandurria</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bandurria</trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
-->
</vortaro>