<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hopi</rad>/o
</kap>
<drv mrk="hopi.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="indian.0o">Indiano</ref>, ano de popolo
      vivanta en Arizono.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>Hopi<klr> (subst.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="hopi.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al la <tld/>oj:
      <ekz>
        mia iama studento <klr>...</klr> decidis, ke Zamenhof, pro
        stulteco, ne sufi&ccirc;e atentis la indonezian kaj <tld/>an lingvojn
        <fnt>
        <aut>D. Harlow</aut>,
        <vrk>Re: Kaj ri kaj &scirc;li</vrk>,
        <lok>soc.culture.esperanto, 1994-01-20</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>hopi<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>
</art>

