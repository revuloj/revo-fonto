<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kanabe.xml,v 1.16 2023/08/31 19:24:09 revo Exp $">
<kap>
<ofc>2</ofc>
<rad>kanaben</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kanabe.0o">
  <kap><tld/>o</kap>
  <snc mrk="kanabe.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_specioj" cel="speci.0o.BIO">Specio</ref> de 
      <ref tip="super" cel="kardel.0o.genro">kardelo</ref>
      kun ru&gcirc;a brusto (<trd lng="la">Carduelis cannabina</trd>):
      <ekz>
        &scirc;i &jcirc;us ornamis per herbo la ka&gcirc;on, en kiu malgranda <tld/>o saltetadis de stango
        sur stangon kaj gaje kantis
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>;
      </ekz>
      <ekz>
        staras la pomujo plena je floroj,
        la <tld/>o kantas en la grosarbeta&jcirc;o
        <fnt>
          <aut>S. S. Blicher, trad. H. J. Bulthuis</aut>,
          <vrk>Taglibro de Vila&gcirc;-pedelo</vrk>,
          <lok>1922</lok>
        </fnt>.
      </ekz>      
    </dif>
    <bld lok="&WCU;/thumb/0/07/Linotte_m&eacute;lodieuse.jpg/640px-Linotte_m&eacute;lodieuse.jpg" prm="CC BY-SA 3.0"><tld/>o &ccirc;e Maz&egrave;res, Ari&egrave;ge, Francujo
        <fnt>
          <aut>Pierre Dalous, propra verko</aut>
          <lok><url ref="&WCW;/File:Linotte_m%C3%A9lodieuse.jpg">&WCW;/File:Linotte_m&eacute;lodieuse.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_a;&c_p;&c_l;&c_ja;&c_n;&c_k;&c_a;</trd>
  <trd lng="cs">konopka obecn&aacute;</trd>
  <trd lng="de">Bluth&auml;nfling</trd>
  <trd lng="en">linnet</trd>
  <trd lng="fr">linotte m&eacute;lodieuse</trd>
  <trd lng="hu">kenderike</trd>
  <trd lng="ja">&#x30E0;&#x30CD;&#x30A2;&#x30AB;&#x30D2;&#x30EF;</trd>
  <trdgrp lng="nl">
    <trd>kneu</trd>,
    <trd>robijntje</trd>,
    <trd>vlasvink</trd>,
    <trd>hennepvink</trd>,
    <trd>kneuter</trd>
  </trdgrp>
  <trd lng="pl">makol&aogonek;gwa</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_o;&c_p;&c_l;&c_ja;&c_n;&c_k;&c_a;</trd>
  <trd lng="sk">stehl&iacute;k kon&ocirc;pka</trd>
  <trd lng="uk">&#x43A;&#x43E;&#x43D;&#x43E;&#x43F;&#x43B;&#x44F;&#x43D;&#x43A;&#x430;</trd>
</drv>
</art>
