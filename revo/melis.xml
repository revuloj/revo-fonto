<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>melis</rad>/o
</kap>
<drv mrk="melis.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="lamiac.0oj">lamiacoj</ref>
      (<trd lng="la">Melissa</trd>).
    </dif>
  </snc>
  <trd lng="de">Melisse</trd>
  <trd lng="nl">melisse</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_l;&c_i;&c_s;&c_s;&c_a;</trd>,
    <trd>&c_m;&c_je;&c_d;&c_o;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="melis.citron0o">
  <kap>
    citron<tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="melis.0o"><tld/>o</ref>
      (<trd lng="la">Melissa officinalis</trd>), havanta
      citronan odoron kaj guston kaj uzata en tizanoj kaj farmacio.
    </dif>
  </snc>
  <trd lng="de">Zitronenmelisse</trd>
  <trd lng="nl">citroenmelisse</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_l;&c_i;&c_s;&c_s;&c_a; &c_l;&c_je;&c_k;&c_a;&c_r;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_a;&c_ja;</trd>,
    <trd>&c_l;&c_i;&c_m;&c_o;&c_n;&c_n;&c_a;&c_ja; &c_m;&c_ja;&c_t;&c_a;</trd>
  </trdgrp>
</drv>
</art>
