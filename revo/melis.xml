<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>melis</rad>/o
</kap>
<drv mrk="melis.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="lami.0acoj">lamiacoj</ref>
      (<trd lng="la">Melissa</trd>).
    </dif>
  </snc>
  <trd lng="de">Melisse</trd>
</drv>
<drv mrk="melis.citron0o">
  <kap>citron<tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="melis.0o"><tld/>o</ref>
      (<trd lng="la">Melissa officinalis</trd>), havanta
      citronan odoron kaj guston kaj uzata en tizanoj kaj farmacio.
    </dif>
  </snc>
  <trd lng="de">Zitronenmelisse</trd>
</drv>
</art>
