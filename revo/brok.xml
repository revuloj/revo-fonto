<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: brok.xml,v 1.22 2025/11/04 06:46:05 revo Exp $">
<kap>
  <rad>brok</rad>/i
</kap>
<drv mrk="brok.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ornami teksa&jcirc;on enteksante metalfadenojn:
      <ekz>
        or<tld/>ita
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#8470;5</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pastro malgra&ubreve; la malhelpo de la surmetita <tld/>ita sako, egalrapide levis
        amba&ubreve; brakojn supren
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 11</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="brod.0teksi">brodteksi</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_k;&c_a;&c_c;&c_mol; <klr>(&c_u;&c_z;&c_o;&c_r;&c_a;&c_m;, &c_z;&c_o;&c_l;&c_a;&c_t;&c_a;&c_m;)</klr></trd>,
    <trd>&c_v;&c_y;&c_t;&c_y;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_t;&c_y;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">fer un brocat</trd>
  <trdgrp lng="cs">
    <trd>st&rcaron;&iacute;brem nebo zlatem</trd>,
    <trd>vetk&aacute;vat obrazce hedv&aacute;b&iacute;m</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>brochieren</trd>,
    <trd>einweben <klr>(Muster)</klr></trd>
  </trdgrp><trd lng="en">brocade</trd>
  <trdgrp lng="hu">
    <trd>sz&odblac; <klr tip="ind">(brok&aacute;tot)</klr></trd>,
    <trd>&aacute;tsz&odblac; <klr>(selyemmel)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#32011;&#32340;&#12426;&#12395;&#12377;&#12427;  <pr>&#12418;&#12435;&#12362;&#12426;&#12395;&#12377;&#12427;</pr></trd>,
    <trd>&#37670;&#32340;&#12426;&#12395;&#12377;&#12427;  <pr>&#12395;&#12375;&#12365;&#12362;&#12426;&#12395;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>tka&cacute;</trd>,
    <trd>przetyka&cacute;</trd>
  </trdgrp>
  <trd lng="pt">tecer brocado</trd>
  <trd lng="ru">&c_t;&c_k;&c_a;&c_t;&c_mol; <klr>(&c_z;&c_o;&c_l;&c_o;&c_t;&c_o;&c_m;, &c_s;&c_je;&c_r;&c_je;&c_b;&c_r;&c_o;&c_m; &c_i;&c_l;&c_i;
  &c_sh;&c_jo;&c_l;&c_k;&c_o;&c_m;)</klr></trd>
  <trd lng="sk">pretka&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_k;&c_a;&c_t;&c_i; &c_z;&c_o;&c_l;&c_o;&c_t;&c_o;&c_m; (&c_a;&c_b;&c_o;&nbsp;&c_s;&c_r;&c_ib;&c_b;&c_l;&c_o;&c_m;)</trd>,
    <trd>&c_sh;&c_o;&c_v;&c_k;&c_o;&c_m;</trd>
  </trdgrp>
</drv>

<drv mrk="brok.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
  <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="teks.0ajxo.TEKS">Teksa&jcirc;o</ref>
      <tld/>ita per (ar&gcirc;entaj a&ubreve; oraj) fadenoj:
      <ekz>
        <nac>shiromuku</nac> estas tradicia japana edzini&gcirc;kimono, kutime
        el silka <tld/>a&jcirc;o kun
        enteksitaj bon&scirc;ancanoncaj motivoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=206268">Shiromuku</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_ch;&c_a;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_k;&c_a;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Brokatstoff</trd>,
    <trd>Brokat</trd>
  </trdgrp>
  <trd lng="ca">brocat</trd>
  <trd lng="en">brocade</trd>
  <trd lng="fr">brocart</trd>
  <trd lng="hu">brok&aacute;t</trd>
  <trdgrp lng="ja">
    <trd>&#12502;&#12525;&#12465;&#12540;&#12489;</trd>,
    <trd>&#32011;&#32340;&#12426;  <pr>&#12418;&#12435;&#12362;&#12426;</pr></trd>,
    <trd>&#37670;  <pr>&#12395;&#12375;&#12365;</pr></trd>,
    <trd>&#37329;&#35188;  <pr>&#12365;&#12435;&#12425;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="pl">brokat</trd>
  <trd lng="pt">brocado</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_ch;&c_a;</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_r;&c_o;&c_k;&c_a;&c_t;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_ch;&c_a;</trd>
  </trdgrp>
</drv>
</art>
