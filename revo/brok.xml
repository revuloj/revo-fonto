<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: brok.xml,v 1.20 2023/10/14 08:39:48 revo Exp $">
<kap>
  <rad>brok</rad>/i
</kap>
<drv mrk="brok.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ornami teksa&jcirc;on enteksante metalfadenojn:
      <ekz>
        or<tld/>ita
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#x2116;5</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pastro malgra&ubreve; la malhelpo de la surmetita <tld/>ita sako, egalrapide levis
        amba&ubreve; brakojn supren
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 11</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="brod.0teksi">brodteksi</ref>
  </snc>
  <trd lng="ca">fer un brocat</trd>
  <trdgrp lng="cs">
    <trd>st&rcaron;&iacute;brem nebo zlatem</trd>,
    <trd>vetk&aacute;vat obrazce hedv&aacute;b&iacute;m</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>brochieren</trd>,
    <trd>einweben <klr>(Muster)</klr></trd>
  </trdgrp><trdgrp lng="hu">
    <trd>sz&odblac; <klr tip="ind">(brok&aacute;tot)</klr></trd>,
    <trd>&aacute;tsz&odblac; <klr>(selyemmel)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7D0B;&#x7E54;&#x308A;&#x306B;&#x3059;&#x308B;  <pr>&#x3082;&#x3093;&#x304A;&#x308A;&#x306B;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x9326;&#x7E54;&#x308A;&#x306B;&#x3059;&#x308B;  <pr>&#x306B;&#x3057;&#x304D;&#x304A;&#x308A;&#x306B;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>tka&cacute;</trd>,
    <trd>przetyka&cacute;</trd>
  </trdgrp>
  <trd lng="pt">tecer brocado</trd>  
  <trd lng="ru">&c_t;&c_k;&c_a;&c_t;&c_mol; <klr>(&c_z;&c_o;&c_l;&c_o;&c_t;&c_o;&c_m;, &c_s;&c_je;&c_r;&c_je;&c_b;&c_r;&c_o;&c_m; &c_i;&c_l;&c_i;
  &c_sh;&c_jo;&c_l;&c_k;&c_o;&c_m;)</klr></trd>
  <trd lng="sk">pretka&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x442;&#x43A;&#x430;&#x442;&#x438; &#x437;&#x43E;&#x43B;&#x43E;&#x442;&#x43E;&#x43C; (&#x430;&#x431;&#x43E;&#xA0;&#x441;&#x440;&#x456;&#x431;&#x43B;&#x43E;&#x43C;)</trd>,
    <trd>&#x448;&#x43E;&#x432;&#x43A;&#x43E;&#x43C;</trd>
  </trdgrp>
</drv>

<drv mrk="brok.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
  <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="teks.0ajxo.TEKS">Teksa&jcirc;o</ref>
      <tld/>ita per (ar&gcirc;entaj a&ubreve; oraj) fadenoj:
      <ekz>
        <nac>shiromuku</nac> estas tradicia japana edzini&gcirc;kimono, kutime
        el silka <tld/>a&jcirc;o kun
        enteksitaj bon&scirc;ancanoncaj motivoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=206268">Shiromuku</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Brokatstoff</trd>,
    <trd>Brokat</trd>
  </trdgrp>
  <trd lng="ca">brocat</trd>
  <trd lng="fr">brocart</trd>
  <trd lng="hu">brok&aacute;t</trd>
  <trdgrp lng="ja">
    <trd>&#x30D6;&#x30ED;&#x30B1;&#x30FC;&#x30C9;</trd>,
    <trd>&#x7D0B;&#x7E54;&#x308A;  <pr>&#x3082;&#x3093;&#x304A;&#x308A;</pr></trd>,
    <trd>&#x9326;  <pr>&#x306B;&#x3057;&#x304D;</pr></trd>,
    <trd>&#x91D1;&#x8974;  <pr>&#x304D;&#x3093;&#x3089;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="pl">brokat</trd>
  <trd lng="pt">brocado</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_ch;&c_a;</trd>
  <trdgrp lng="uk">
    <trd>&#x431;&#x440;&#x43E;&#x43A;&#x430;&#x442;</trd>,
    <trd>&#x43F;&#x430;&#x440;&#x447;&#x430;</trd>
  </trdgrp>
</drv>
</art>
