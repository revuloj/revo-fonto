<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pastinak</rad>/o
</kap>
<drv mrk="pastin.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">Pastinaca</trd>),
      kun man&gcirc;ebla radiko.
    </dif>
  </snc>
  <trd lng="de">Pastinak</trd>
  <trdgrp lng="nl">
    <trd>pastinaak</trd>,
    <trd>witte <ind>peen</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_t;&c_je;&c_r;&c_n;&c_a;&c_k;</trd>
</drv>
</art>
