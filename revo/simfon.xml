<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: simfon.xml,v 1.23 2019/06/30 18:21:49 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>simfoni</rad>/o
</kap>
<drv mrk="simfon.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="kompon.0ajxo">Kompona&jcirc;o</ref>
      por orkestro, kiu amplekse prezentas kelkajn motivojn
      la&ubreve; unu &ccirc;efa tonalo dum kvar partoj alterne rapid- kaj malrapid-ritmaj:
      <ekz>
        <tld/>o de Betoveno
        <fnt>
          <bib>Monato</bib>
          <aut>Alberto Garc&iacute;a Fumero</aut>
          <vrk><url ref="&Monato;/2014/011301.php"
              >Laste (&ccirc;u?) pri scienco kaj religio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>on n-ro&#8239;75 D-ma&jcirc;oran <nom>Joseph Haydn</nom> komponis en la jaro 1780
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=283634"
              >75-a simfonio (Haydn)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sonat.0o">sonato</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_m;&c_f;&c_o;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="ca">simfonia</trd>
  <trd lng="cs">symfonie</trd>
  <trdgrp lng="de">
    <trd>Symphonie</trd>,
    <trd>Sinfonie</trd>
  </trdgrp>
  <trd lng="en">symphony</trd>
  <trd lng="es">sinfon&iacute;a</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_m;&a_f;&a_w;&a_n;&f_y;</trd>
    <trd>&a_s;&a_n;&a_f;&a_w;&a_n;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">symphonie</trd>
  <trd lng="hu">szimf&oacute;nia</trd>
  <trd lng="it">sinfonia</trd>
  <trd lng="nl">symfonie</trd>
  <trd lng="pl">symfonia</trd>
  <trd lng="ru">&c_s;&c_i;&c_m;&c_f;&c_o;&c_n;&c_i;&c_ja;</trd>
  <trd lng="sk">symf&oacute;nia</trd>
</drv>
</art>
