<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>simfoni</rad>/o
</kap>
<drv mrk="simfon.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzika &gcirc;enro de e&ubreve;ropa klasiko,
      en kiu orkestro amplekse esploras kelkajn motivojn
      la&ubreve; unu &ccirc;efa ton-skalo
      dum kvar partoj alterne rapid- kaj malrapid-taktaj.
    </dif>
    <refgrp tip="vid">
      <ref cel="sonat.0o">sonato</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Symphonie</trd>,
    <trd>Sinfonie</trd>
  </trdgrp>
  <trd lng="en">symphony</trd>
  <trd lng="fr">symphonie</trd>
  <trd lng="nl">symfonie</trd>
  <trd lng="ru">&c_s;&c_i;&c_m;&c_f;&c_o;&c_n;&c_i;&c_ja;</trd>
</drv>
</art>
