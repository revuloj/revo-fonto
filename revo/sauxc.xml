<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sauxc.xml,v 1.29 2024/11/26 08:36:20 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sa&ubreve;c</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="sauxc.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sauxc.0o.KUI">
  <uzo tip="fak">KUI</uzo>
    <dif>
      Flueca spica&jcirc;o aldonata al man&gcirc;a&jcirc;o:
      <ekz>
        kaj e&ccirc; nenia <tld/>o a&ubreve; kuka&jcirc;o! senta&ubreve;guloj
        <fnt><bib>Revizoro</bib>, <lok>akto 2a, sceno 6a</lok></fnt>!
      </ekz>
      <ekz>
        la <tld/>oj de tiuj festenoj restas ankora&ubreve; sur viaj vestoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi denove vidis en la spirito la karpojn kun bruna vinbera <tld/>o
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni ricevis bova&jcirc;on kun mustarda <tld/>o
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 27</lok>
        </fnt>;
      </ekz>
      <ekz>
        fazeoloj kun lakto<tld/>o
        <fnt>
          <aut>Eduardo Novembro</aut>
          <vrk><url ref="http://www.karavelo.net/html/pdf/LK_023_RV.pdf"
              >Legendo pri Martim de Freitas</url></vrk>
          <lok>La Karavelo, n-ro 23, oktobro 2010, p. 18</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kaserolo <klr>[&#8230;]</klr> entenis kolbasetojn en tomata <tld/>o
        <fnt>
          <bib>MkM</bib>
          <lok>7</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;okolada kremo kun poma <tld/>o
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 3</lok>
        </fnt>;
      </ekz>
      <ekz>
        prepari <tld/>on per sojolikva&jcirc;o, vino, fekulo
        <fnt>
          <bib>ChC</bib>
          <lok>11</lok>
        </fnt>;
      </ekz>
      <ekz>
        tro da ostra <tld/>o
        <fnt>
          <bib>ChC</bib>
          <lok>14</lok>
        </fnt>;
      </ekz>
      <ekz>
        forta pimenta <tld/>o
        <fnt>
          <bib>ChC</bib>
          <lok>Glosaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        vianda&jcirc;o kun paprika <tld/>o
        <fnt>
          <bib>Kon11</bib>
          <lok>15 faktoj kiujn vi certe ne sciis pri Hungario...</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi komentis pri la abomeninda gusto de la fi&scirc;a <tld/>o
        <fnt>
          <bib>StU</bib>
          <lok>Al Romo</lok>
        </fnt>;
      </ekz>
      <ekz>
        soja <tld/>o
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 4 (66)</lok>
        </fnt>;
      </ekz>
      <ekz>
        menta <tld/>o
        <fnt>
        <aut>OH</aut>,
        <vrk>Molekula arkeologio por frandemuloj</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2001a, numero 3a, p. 22a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_o;&c_w;&c_s;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_l;&c_ib;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_o;&c_s;</trd>
  <trd lng="ca">salsa</trd>
  <trdgrp lng="de">
    <trd>So&szlig;e</trd>,
    <trd>Sauce</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>sauce</trd>,
    <trd>gravy</trd>
  </trdgrp>
  <trd lng="es">salsa</trd>
  <trd lng="fa">&a_s;&a_damma;&a_s;</trd>
  <trd lng="fr">sauce</trd>
  <trd lng="he">&resh;&vav;&tet;&bet;</trd>
  <trdgrp lng="hu">
    <trd>sz&oacute;sz</trd>,
    <trd>m&aacute;rt&aacute;s</trd>
  </trdgrp>
  <trd lng="it">salsa</trd>
  <trd lng="nl">saus</trd>
  <trd lng="pl">sos</trd>
  <trd lng="ro">sos</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_u;&c_s;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_l;&c_i;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="tok">telo</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_ib;&c_d;&c_l;&c_i;&c_v;&c_a;</trd>,
    <trd>&c_s;&c_o;&c_u;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#37233;&#26009; <pr>ji&agrave;ngli&agrave;o</pr></trd>,
    <trd>&#37292;&#26009; <pr>ji&agrave;ngli&agrave;o</pr></trd>,
    <trd>&#37233; <pr>ji&agrave;ng</pr></trd>,
    <trd>&#37292; <pr>ji&agrave;ng</pr></trd>,
    <trd>&#27801;&#21496; <pr>sh&amacron;s&imacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sauxc.buter0o">
  <kap>
    buter<tld/>o, <var><kap>nederlanda <tld/>o</kap></var>
  </kap>
  <snc>
    <dif>
      <tld lit="S"/>o francdevena, el butero, ovoflavo, salo kaj pipro, kiun oni man&gcirc;as kun fi&scirc;o a&ubreve;
      legomoj,
      ekz-e asparago:
      <ekz>
        truto kuirita kun buter<tld/>o
        <fnt>
          <aut>J. Tenot</aut>
          <vrk><url ref="https://arkivo.esperanto-france.org/divers/tenot/mangho-k.htm"
              >Man&gcirc;okarto </url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la blankradika cikorio estas delikata, sed iom amara, plej bone gustis kun milda
        buter<tld/>o
        <fnt>
          <aut>Eduardo Novembro</aut>
          <vrk><url ref="http://www.karavelo.net/html/pdf/LK_023_RV.pdf"
              >Legendo pri Martim de Freitas, La tagman&gcirc;o</url></vrk>
          <lok>La Karavelo, n-ro 23, oktobro 2010, p. 18</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni kirlis nederlandan sa&ubreve;con en iu blua ujo, sed &gcirc;i neniel volis sukcesi
        <fnt>
          <bib>OIP</bib>
          <lok>Ombro sur interna pejza&gcirc;o</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_l;&c_ja;&c_n;&c_d;&c_z;&c_k;&c_ib; <ind>&c_s;&c_o;&c_w;&c_s;</ind></trd>,
    <trd>&c_g;&c_a;&c_l;&c_ja;&c_n;&c_d;&c_e;&c_z;</trd>
  </trdgrp>
  <trd lng="de">Holl&auml;ndische So&szlig;e</trd>
  <trd lng="en">Hollandaise sauce</trd>
  <trd lng="fr">sauce hollandaise</trd>
  <trd lng="ru">&c_g;&c_o;&c_l;&c_l;&c_a;&c_n;&c_d;&c_s;&c_k;&c_i;&c_j; &c_s;&c_o;&c_u;&c_s;</trd>
</drv>

</art>
