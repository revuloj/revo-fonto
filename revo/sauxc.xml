        <bib>Monato</bib><lok>jaro 2001a, numero 3a, p. 22a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_o;&c_w;&c_s;</trd>, 
    <trd>&c_p;&c_a;&c_d;&c_l;&c_ib;&c_w;&c_k;&c_a;</trd> 
