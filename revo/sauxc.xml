  <trd lng="de">So&szlig;e</trd>
  <trd lng="en">sauce</trd>
  <trd lng="fr">sauce</trd>
  <trd lng="he">&resh;&vav;&tet;&bet;</trd>
  <trdgrp lng="hu">
    <trd>sz&oacute;sz</trd>,
    <trd>m&aacute;rt&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">saus</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_u;&c_s;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_l;&c_i;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
