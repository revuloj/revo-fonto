</drv>

<drv mrk="bild.idento0igo">
  <kap>idento-<tld/>igo</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(en aro <frm><g>E</g></frm>)</klr>
      <ref tip="super" cel="bild.0igo.MAT"><tld lit="B"/>igo</ref>, kiu
      &jcirc;etas &ccirc;iun elementon al &gcirc;i mem.
    </dif>
    <refgrp tip="sin">
      <ref cel="ident.0o.bildigo">idento</ref>,
      <ref cel="rilat.idento0o">idento-rilato</ref>.
    </refgrp>
    <rim>
      Troveblas <ctl>identa funkcio</ctl> en
      <fnt><bib>EKV</bib><lok>&para;166</lok></fnt>.
    </rim>
  </snc>
  <adm>
    Fonto trovenda. <aut>MB</aut>
  </adm>
  <trd lng="en">identity <ind>mapping</ind></trd>
  <trd lng="es">funci&oacute;n identidad</trd>
  <trd lng="de">identische <ind>Abbildung</ind></trd>
  <trdgrp lng="fr">
    <trd>identit&eacute;<klr tip="ind"> (application)</klr></trd>,
    <trd>application identique</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>funkcja identyczno&sacute;ciowa</trd>,
    <trd>przekszta&lstroke;cenie to&zdot;samo&sacute;ciowe</trd>,
    <trd>identyczno&sacute;&cacute;</trd>
  </trdgrp> 
  <trd lng="ru">&c_t;&c_o;&c_zh;&c_d;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;&c_je; <ind>&c_o;&c_t;&c_o;&c_b;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</ind></trd>
</drv>

<drv mrk="bild.sub0igo">
  <kap>sub<tld/>igo</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(de bildigo)</klr>
      <ref tip="sin" cel="vast.mal0igajxo.bildigo">Malvastiga&jcirc;o</ref>
      de la bildigo la&ubreve; iu subaro de &gcirc;ia fonto-aro.
    </dif>
  </snc>
  <adm>
    Fonto trovenda. <aut>MB</aut>
  </adm>
  <trd lng="be">&c_z;&c_v;&c_u;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="de">Einschr&auml;nkung<klr tip="ind"> (einer Abbildung)</klr></trd>
  <trdgrp lng="en">
    <trd>restriction<klr tip="ind"> (of a mapping)</klr></trd>,
    <trd>partial <ind>mapping</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>restriction<klr tip="ind"> (d'une application)</klr></trd>,
    <trd>sous-application</trd>,
    <trd>trace<klr tip="ind"> (d'une application)</klr></trd>
  </trdgrp>
  <trd lng="pl">funkcja cz&eogonek;&sacute;ciowa</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_zh;&c_je;&c_n;&c_i;&c_je;<klr tip="ind"> (&c_o;&c_t;&c_o;&c_b;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>,
    <trd>&c_o;&c_g;&c_r;&c_a;&c_n;&c_i;&c_ch;&c_je;&c_n;&c_i;&c_je;<klr tip="ind"> (&c_o;&c_t;&c_o;&c_b;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="bild.inversa0igo">
  <kap>inversa <tld/>igo</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatStokTerm</bib><lok>p. 23,
  &leftquot;inversa funkcio&rightquot;</lok></fnt>
  <snc>
    <dif>
      <klr>(de <ref cel="bild.0igo.MAT"><tld/>igo</ref>
      <frm><k>f</k></frm>)</klr>
      <ref cel="rilat.inversa0o">Inversa rilato</ref> de <frm><k>f</k></frm>,
      se anka&ubreve; &gcirc;i estas <tld/>igo.
    </dif>
    <refgrp tip="vid">
      Oni diras anka&ubreve;:
      <ref cel="invers.0o.variaj">inverso</ref> de bildigo.
    </refgrp>
    <rim>
      Bricard <fnt><bib>MatTerm</bib><lok>p. 19</lok></fnt> &ccirc;i-sence uzas
      <ctl>reciproka funkcio</ctl>, kiel faras pluraj
      naciaj lingvoj, sed tiu uzo &scirc;ajne ne disvasti&gcirc;is kaj
      Werner ignoras &gcirc;in.
    </rim>
  </snc>
  <trd lng="de">inverse <ind>Abbildung</ind></trd>
  <trd lng="en">inverse mapping</trd>
  <trd lng="es">funci&oacute;n inversa</trd>
  <trd lng="fr">application r&eacute;ciproque</trd>
  <trd lng="hu">inverz <ind>lek&eacute;pez&eacute;s</ind></trd>
  <trd lng="pl">funkcja odwrotna<klr tip="ind"> (do danej funkcji)</klr></trd>
  <trd lng="ru">&c_o;&c_b;&c_r;&c_a;&c_t;&c_n;&c_o;&c_je; <ind>&c_o;&c_t;&c_o;&c_b;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</ind></trd>
</drv>

<drv mrk="bild.inversa0o">
  <kap>inversa <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="bild.inversa0o.MAT">
    <subsnc mrk="bild.inversa0o.subaro">
      <dif>
        <klr>(de subaro <frm><g>B</g>&#x2282;<g>F</g></frm> per
        <ref cel="rilat.0o.MAT">rilato</ref> 
        <frm><g>R</g>&#x2282;<g>E</g>&#xd7;<g>F</g></frm>)</klr>
        Tia subaro de <frm><g>E</g></frm>, ke por &ccirc;iu &gcirc;ia 
        elemento ekzistas almena&ubreve; unu paro en <frm><g>R</g></frm>, 
        kies unua termo &gcirc;i estas kaj kies dua termo apartenas al 
        <frm><g>B</g></frm>:
        <ekz>
          la inversan <tld/>on de <frm><g>B</g></frm> per <frm><g>R</g></frm>
          oni signas per <frm><g>R</g><sup>-1</sup>(<g>B</g>)</frm>.
        </ekz>
      </dif>
      <trd lng="pl">przeciwobraz<klr tip="ind"> (podzbioru przez funkcj&eogonek;)</klr></trd>
    </subsnc>
    <subsnc mrk="bild.inversa0o.elemento">
      <dif>
        <klr>(de elemento <frm><k>b</k>&#x2208;<g>F</g></frm> per
        <ref cel="bild.0igo.MAT"><tld/>igo</ref> <frm><k>f</k></frm> de 
        <frm><g>E</g></frm> al <frm><g>F</g></frm>)</klr>
        La inversa <tld/>o <sncref ref="bild.inversa0o.subaro"/> de subaro
        <frm>{<k>b</k>}</frm> per <frm><k>f</k></frm>.
      </dif>
    </subsnc>
    <subsnc mrk="bild.inversa0o.per_bijekcio">
      <dif>
        <klr>(de elemento <frm><k>b</k>&#x2208;<g>F</g></frm> per
        <ref cel="bijekc.0a">bijekcia</ref>
        <ref cel="bild.0igo.MAT"><tld/>igo</ref> <frm><k>f</k></frm> 
        de <frm><g>E</g></frm> al <frm><g>F</g></frm>)</klr>
        La <tld/>o <sncref ref="bild.0o.elemento"/> de <frm><k>b</k></frm> 
        per la <ref cel="bild.inversa0igo">inversa <tld/>igo</ref> 
        de <frm><k>f</k></frm>.
      </dif>
    </subsnc>
    <ref tip="sin" cel="bild.mal0o.MAT">mal<tld/>o.</ref>
    <rim>
      Ni ne trovis fonton por &ccirc;i tiu termino, sed konsideras &gcirc;in
      sufi&ccirc;e internacia kaj logika sekvo de <ctl>inversa <tld/>igo</ctl>.
    </rim>
    <trdgrp lng="de">
      <trd>Urbild</trd>,
      <trd>inverses <ind>Bild</ind></trd>
    </trdgrp>
    <trd lng="en">inverse <ind>image</ind></trd>
    <trd lng="fr">image r&eacute;ciproque<klr tip="ind"> (math.)</klr></trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;</trd>
  </snc>
</drv>

<drv mrk="bild.rekta0o">
  <kap>rekta <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <ref cel="bild.0o.MAT"><tld lit="B"/>o</ref> <klr>(kontraste al
      <ref cel="bild.inversa0o.MAT">inversa <tld/>o</ref>)</klr>.
    </dif>
  </snc>
</drv>
