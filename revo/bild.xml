<drv mrk="bild.sub0igo">
  <kap>sub<tld/>igo</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="bild.sub0igo.MAT">
    <dif>
      <klr>(de <ref tip="super" cel="bild.0igo.MAT"><tld/>igo</ref>)
      </klr>
      <ref tip="sin" cel="vast.mal0igajxo.bildigo">Malvastiga&jcirc;o
      </ref>
      de la <tld/>igo la&ubreve; iu subaro de &gcirc;ia fonto-aro.
    </dif>
  </snc>
  <adm>
    Fonto trovenda. <aut>MB</aut>
  </adm>
  <trd lng="be">&c_z;&c_v;&c_u;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="ca">restricci&oacute; <klr>(del domini d'una
  funci&oacute;)</klr></trd>
  <trd lng="de">Einschr&auml;nkung<klr tip="ind"> (einer Abbildung)
  </klr></trd>
  <trdgrp lng="en">
    <trd>restriction<klr tip="ind"> (of a mapping)</klr></trd>,
