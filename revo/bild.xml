    </dif>
    <refgrp tip="sin">
      <ref cel="ident.0o.bildigo">idento</ref>,
      <ref cel="rilat.idento0o">idento-rilato</ref>.
    </refgrp>
    <rim>
      Troveblas <ctl>identa funkcio</ctl> en
