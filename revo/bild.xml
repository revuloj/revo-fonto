<drv mrk="bild.spegul0o">
  <kap>spegul<tld/>o</kap>
  <snc mrk="bild.spegul0o.KOMUNE">
    <dif>
      <tld lit="B"/>o precize prezentanta la videblan aspekton de io,
      eventuale renverse kiel en spegulo:  
      <ekz>
        &gcirc;i havas mian formon, miajn trajtojn <klr>[&#8230;]</klr>
        &gcirc;i estas mia spegul<tld/>o
        <fnt>
          <aut>C. Piron</aut>,
          <vrk><url
          ref="http://esperanto.org.uk/eldonoj/piron/tekstoj/Tien.html">
          Tien</url></vrk>,
          <lok>1997</lok>
        </fnt>;
      </ekz>
      <ekz>
        paruo vidas sian spegul<tld/>on en a&ubreve;ta retrospegulo kaj
        senhezite atakas
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="https://www.monato.be/2012/010680.php">Brutalaj
          birdoj</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Spiegelbild</trd>
  <trd lng="es">reflejo</trd>
  <trd lng="fr">reflet</trd>
</drv>

