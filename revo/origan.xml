<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>origan</rad>/o</kap>
<drv mrk="origan.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="lamiac.0oj">lamiacoj</ref>
      (<trd lng="la">Origanum</trd>),
      <ref tip="lst" cel="spic.0o" lst="voko:spicoj">spic</ref>herbo.
    </dif>
  </snc>
  <ref tip="sub" cel="majoran.0o">majorano</ref>
  <trd lng="be">&c_m;&c_a;&c_ja;&c_r;&c_a;&c_n;</trd>
  <trd lng="bg">&c_r;&c_i;&c_g;&c_a;&c_n;</trd>
  <trdgrp lng="ca">
    <trd>orenga</trd>,
    <trd>marduix</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Oregano</trd>,
     <trd>Dost</trd>,
     <trd>wilder Majoran</trd>
  </trdgrp>
  <trd lng="el">&rho;&jota_ton;&gamma;&alfa;&ny;&eta;</trd>
  <trdgrp lng="en">
    <trd>origanum</trd>,
    <trd>oregano</trd>,
    <trd>marjoram</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_m;&a_r;&a_z;&a_n;&f_g;&a_w;&a_sh;</trd>,
    <trd>&a_m;&a_r;&a_z;&a_n;&a_j;&a_w;&a_sh;</trd>
  </trdgrp>
  <trd lng="fr">origan</trd>
  <trdgrp lng="hu">
    <trd>szurokf&udblac;</trd>,
    <trd>vadmajor&aacute;nna</trd>
  </trdgrp>
  <trd lng="nl">marjolein</trd>
  <trdgrp lng="pt">
    <trd>or&eacute;gano</trd>,
    <trd>or&eacute;g&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_u;&c_sh;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_j;&c_o;&c_r;&c_a;&c_n;</trd>
  </trdgrp>
</drv>
</art>
