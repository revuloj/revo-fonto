<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: origan.xml,v 1.30 2024/01/30 16:09:26 revo Exp $">
<kap><rad>origan</rad>/o</kap>
<drv mrk="origan.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="lamiac.0oj">lamiacoj</ref>
      (<trd lng="la">Origanum</trd>);
      <ref tip="lst" cel="stauxd.0o" lst="voko:lamioparencoj">sta&ubreve;do</ref> a&ubreve; 
      duonarbusto kun blankaj, roz- a&ubreve; violkoloraj floretoj, sidantaj
      en spikoj de panikloj aksele de volbitaj brakteoj;
      kelkaj specioj estas uzataj kiel
      <ref tip="lst" cel="spic.0o" lst="voko:spicoj">spic</ref>herbo:
      <ekz>
        pluraj <klr>[specioj estas]</klr> kultivataj por medicino,
        spicado kaj ornamo, inter aliaj diktamno-<tld/>o, majorano, kaj
        la ordinara <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3286">Origano</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="majoran.0o">majorano</ref>
    <bld lok="&WCU;/thumb/1/14/Origanum_vulgare_-_harilik_pune.jpg/512px-Origanum_vulgare_-_harilik_pune.jpg" prm="CC BY-SA 3.0">floraro de Ordinara <tld/>o
         <fnt>
           <aut>Ivar Leidus, propra verko</aut>
           <lok><url ref="&WCW;/File:Origanum_vulgare_-_harilik_pune.jpg">&WCW;/File:Origanum_vulgare_-_harilik_pune.jpg</url></lok>
         </fnt>
    </bld>
    <bld lok="&WCU;/thumb/5/5d/Origanum_dictamnus_hh.jpg/600px-Origanum_dictamnus_hh.jpg" prm="CC BY-SA 3.0">diktamno-<tld/>o (Origanum dictamnus)
         <fnt>
           <aut>HelenaH, propra verko</aut>
           <lok><url ref="&WCW;/File:Origanum_dictamnus_hh.jpg">&WCW;/File:Origanum_dictamnus_hh.jpg</url></lok>
         </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_ja;&c_r;&c_a;&c_n;</trd>
  <trd lng="bg">&c_r;&c_i;&c_g;&c_a;&c_n;</trd>
  <trdgrp lng="ca">
    <trd>orenga</trd>,
    <trd>marduix</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Oregano</trd>,
     <trd>Dost</trd>,
     <trd>wilder Majoran</trd>
  </trdgrp>
  <trd lng="el">&rho;&jota_ton;&gamma;&alfa;&ny;&eta;</trd>
  <trdgrp lng="en">
    <trd>origanum</trd>,
    <trd>oregano</trd>,
    <trd>marjoram</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_m;&a_r;&a_z;&a_n;&f_g;&a_w;&a_sh;</trd>,
    <trd>&a_m;&a_r;&a_z;&a_n;&a_j;&a_w;&a_sh;</trd>
  </trdgrp>
  <trd lng="fr">origan</trd>
  <trdgrp lng="hu">
    <trd>szurokf&udblac;</trd>,
    <trd>vadmajor&aacute;nna</trd>
  </trdgrp>
  <trd lng="id">oregano</trd>
  <trdgrp lng="ja">
    <trd>&#x30CF;&#x30CA;&#x30CF;&#x30C3;&#x30AB;</trd>,
    <trd>&#x30AA;&#x30EC;&#x30AC;&#x30CE;</trd>
  </trdgrp>
  <trd lng="nl">marjolein</trd>
  <trd lng="pl">lebiodka</trd>
  <trdgrp lng="pt">
    <trd>or&eacute;gano</trd>,
    <trd>or&eacute;g&atilde;o</trd>
  </trdgrp>
  <trd lng="ro">origanum</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_u;&c_sh;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_j;&c_o;&c_r;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="sk">pamajor&aacute;n oby&ccaron;ajn&yacute;</trd>
  <trd lng="cs">dobromysl obecn&aacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x430;&#x442;&#x435;&#x440;&#x438;&#x43D;&#x43A;&#x430;</trd>,
    <trd>&#x43C;&#x430;&#x439;&#x43E;&#x440;&#x430;&#x43D;</trd>
  </trdgrp>
  <trd lng="zh">&#x725B;&#x81F3; <pr>ni&#xFA;zh&#xEC;</pr></trd>
</drv>
</art>
