<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>anonim</rad>/a
</kap>
<drv mrk="anonim.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Sennoma:
      <ekz>
        <tld/>a a&ubreve;toro, letero;
      </ekz>
      <ekz>
        komerca <tld/>a societo
        <klr>(kies asocianoj ne publikigas siajn nomojn kaj riskas nur
        siajn enmetitajn kapitalojn)</klr>;
      </ekz>
      <ekz>
        sin ka&scirc;i sub <tld/>eco.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">anonym</trd>
  <trd lng="en">anonymous</trd>
  <trd lng="es">an&oacute;nimo</trd>
  <trd lng="fr">anonyme</trd>
  <trdgrp lng="nl">
    <trd>anoniem</trd>,
    <trd>naamloos</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_o;&c_n;&c_i;&c_m;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_y;&c_m;&c_ja;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">anonymn&yacute;</trd>
</drv>
</art>
