<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>turb</rad>/o
</kap>
<drv mrk="turb.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ludilo konsistanta el konusforma peco kun centra pinto, kiun oni
      diversmaniere rapide turnsvingas sur la grundo.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_w;&c_ch;&c_o;&c_k; <klr>(&c_c;&c_a;&c_c;&c_k;&c_a;)</klr></trd>
  <trd lng="de">Kreisel</trd>
  <trd lng="nl">tol<klr tip="ind"> (speelgoed)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_l;&c_ch;&c_o;&c_k;</trd>,
    <trd>&c_ju;&c_l;&c_a;</trd>
  </trdgrp>
</drv>
</art>
