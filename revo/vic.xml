<drv mrk="vic.finia0o">
  <kap>finia <tld/>o, finilonga <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="vic.0o.MAT"><tld lit="V"/>o</ref>, kies
      <ref cel="ar.fonto0o">fonto-aro</ref> estas finia.
    </dif>
    <rim>
      Finilongaj <tld/>oj estas prezenteblaj per
      <ref cel="op.0o.vico">opoj</ref> kaj oni ofte
      konfuzas amba&ubreve; nociojn.
    </rim>
  </snc>
  <trd lng="de">endliche <ind>Reihe</ind></trd>
  <trd lng="en">finite <ind>sequence</ind></trd>
  <trd lng="fr">suite finie</trd>
  <trd lng="hu">v&eacute;ges <ind>sor</ind></trd>  
  <trd lng="nl">eindige <ind>reeks</ind></trd>
  <trd lng="pl">ci&aogonek;g sko&nacute;czony</trd>
  <trd lng="pt">seq&uuml;&ecirc;ncia finita</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_n;&c_je;&c_ch;&c_n;&c_a;&c_ja;
&c_p;&c_o;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_t;&c_je;&c_zh;</trd>
  </trdgrp>
</drv>

<drv mrk="vic.geometria0o">
  <kap>geometria <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <ref tip="dif" cel="progre1.geometria0o">geometria progresio.</ref>
