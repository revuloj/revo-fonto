<drv mrk="vic.sub0o">
  <kap>sub<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(de <ref tip="super" cel="vic.0o.MAT"><tld/>o</ref> u)</klr>
      Tia <tld/>o v, ke &gcirc;ia <ref cel="ar.fonto0o">fonto-aro</ref> estas
      subaro de la fonto-aro de u, kaj ke &ccirc;iu termo de v estas termo
      anka&ubreve; de u por la sama indico:
      <ekz>
        &ccirc;iu ajn nefinia sub<tld/>o de ko&scirc;ia <tld/>o estas
        ko&scirc;ia.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Teilfolge</trd>
  <trd lng="en">subsequence</trd>
  <trd lng="fr">sous-suite</trd>
  <trd lng="pl">podci&aogonek;g</trd>
  <trd lng="pt">sub-seq&uuml;&ecirc;ncia</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_p;&c_o;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="vic.0o.de_funkcioj">
  <kap><tld/>o de funkcioj<fnt><bib>MatStokTerm</bib><lok>p. 28</lok></fnt>, funkci<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="vic.0o.MAT"><tld lit="V"/>o</ref>, kies
      termoj estas <ref cel="funkci.0o.MAT">funkcioj</ref>.
    </dif>
    <refgrp tip="vid">
      <ref cel="konver.simple0a">simple konver&gcirc;a</ref>,
      <ref cel="konver.unuforme0a">unuforme konver&gcirc;a</ref>.
    </refgrp>
    <rim>
      En <fnt><bib>MatVort</bib></fnt> troveblas <ctl>funkciala <tld/>o</ctl>,
      kun la ekzota adjektiva formo responda al <ctl>funkcio</ctl>.
    </rim>
  </snc>
  <trd lng="de">Funktionfolge</trd>
  <trdgrp lng="fr">
    <trd>function <ind>sequence</ind></trd>,
    <trd>sequence of functions</trd>
  </trdgrp>
  <trd lng="fr">suite de fonctions</trd>
  <trd lng="pl">ci&aogonek;g funkcijny</trd>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol; &c_f;&c_u;&c_n;&c_k;&c_c;&c_i;&c_j;</trd>
</drv>
