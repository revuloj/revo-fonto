      <ref cel="kosxi.0o">Ko&scirc;io</ref>.
    </refgrp>
  </snc>
  <trd lng="ca">successi&oacute; de Cauchy</trd>
  <trdgrp lng="de">
    <trd>Cauchy-Folge</trd>,
    <trd>Fundamentalfolge</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Cauchy['s] <ind>sequence</ind></trd>,
    <trd>fundamental <ind>sequence</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>suite de Cauchy</trd>,
    <trd>suite fondamentale</trd>
  </trdgrp>
  <trd lng="hu">Cauchy-sorozat</trd> 
  <trdgrp lng="it">
    <trd><ind>successione</ind> di Cauchy</trd>,
    <trd><ind>successione</ind> fondamentale</trd>
  </trdgrp> 
  <trd lng="pl">ci&aogonek;g Cauchy'ego</trd>
  <trd lng="pt">seq&uuml;&ecirc;ncia de Cauchy</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_h;&c_o;&c_d;&c_ja;&c_shch;&c_a;&c_ja;&c_s;&c_ja;
    &c_v; &c_s;&c_je;&c_b;&c_je;
    &c_p;&c_o;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_f;&c_u;&c_n;&c_d;&c_a;&c_m;&c_je;&c_n;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja;
    &c_p;&c_o;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;
