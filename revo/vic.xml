    <ind>&c_p;&c_r;&c_o;&c_g;&c_r;&c_je;&c_s;&c_s;&c_i;&c_ja;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="vic.harmona0o">
  <kap>harmona <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <ref tip="dif" cel="progre1.harmona0o">harmona progresio.</ref>
  </snc>
  <trdgrp lng="de">
    <trd>harmonische <ind>Folge</ind></trd>,
    <trd>harmonische <ind>Progression</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>harmonic <ind>sequence</ind></trd>,
    <trd>harmonic <ind>progression</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>suite harmonique</trd>,
    <trd>progression harmonique</trd>
  </trdgrp>
  <trd lng="hu">harmonikus <ind>sorozat</ind></trd>  
  <trd lng="pl">ci&aogonek;g harmoniczny</trd>
  <trd lng="pt">progress&atilde;o harm&ocirc;nica</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_r;&c_m;&c_o;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja;
    <ind>&c_p;&c_r;&c_o;&c_g;&c_r;&c_je;&c_s;&c_s;&c_i;&c_ja;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="vic.kosxia0o">
  <kap>ko&scirc;ia <tld/>o<fnt><bib>PIV2</bib></fnt>,
  Ko&scirc;i-<tld/>o<fnt><bib>EKV</bib><lok>&para;236</lok></fnt></kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      Tia <ref tip="super" cel="vic.0o.MAT"><tld/>o</ref> en
      <ref cel="spac.metrika0o">metrika spaco</ref>,
      ke la distanco inter du &gcirc;iaj termoj povas
      i&gcirc;i arbitre malgranda, se la indicoj estas sufi&ccirc;e
      grandaj:
      <ekz>
        konver&gcirc;anta <tld/>o estas ko&scirc;ia;
      </ekz>
      <ekz>
        en la metrika spaco de reeloj &ccirc;iu ko&scirc;ia <tld/>o
        konver&gcirc;as.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="komple2.0a.spaco">kompleta</ref>,
      <ref cel="kosxi.0o">Ko&scirc;io</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Cauchy-Folge</trd>,
    <trd>Fundamentalfolge</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Cauchy['s] <ind>sequence</ind></trd>,
    <trd>fundamental <ind>sequence</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>suite de Cauchy</trd>,
    <trd>suite fondamentale</trd>
  </trdgrp>
  <trd lng="&scirc;u">Cauchy-sorozat</trd>  
  <trd lng="pl">ci&aogonek;g Cauchy'ego</trd>
  <trd lng="pt">seq&uuml;&ecirc;ncia de Cauchy</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_h;&c_o;&c_d;&c_ja;&c_shch;&c_a;&c_ja;&c_s;&c_ja;
    &c_v; &c_s;&c_je;&c_b;&c_je;
    &c_p;&c_o;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_f;&c_u;&c_n;&c_d;&c_a;&c_m;&c_je;&c_n;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja;
    &c_p;&c_o;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;
    &c_K;&c_o;&c_sh;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="vic.sub0o">
  <kap>sub<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(de <ref tip="super" cel="vic.0o.MAT"><tld/>o</ref>
      <frm><k>u</k></frm>)</klr>
      Tia <tld/>o <frm><k>v</k></frm>, ke &gcirc;ia
      <ref cel="ar.fonto0o">fonto-aro</ref> estas subaro de la fonto-aro
      de <frm><k>u</k></frm>, kaj ke &ccirc;iu termo de <frm><k>v</k></frm>
      estas termo anka&ubreve; de <frm><k>u</k></frm> por la sama indico:
      <ekz>
        &ccirc;iu ajn nefinia sub<tld/>o de ko&scirc;ia <tld/>o estas
        ko&scirc;ia.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Teilfolge</trd>
  <trd lng="en">subsequence</trd>
  <trd lng="fr">sous-suite</trd>
  <trd lng="hu">r&eacute;szsorozat</trd>  
  <trdgrp lng="pl">
    <trd>podci&aogonek;g</trd>,
    <trd>ci&aogonek;g cz&eogonek;&sacute;ciowy</trd>
  </trdgrp>
  <trd lng="pt">sub-seq&uuml;&ecirc;ncia</trd>
  <trd
  lng="ru">&c_p;&c_o;&c_d;&c_p;&c_o;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="vic.0o.de_funkcioj">
  <kap><tld/>o de funkcioj<fnt><bib>MatStokTerm</bib><lok>p.
  28</lok></fnt>, funkci<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="vic.0o.MAT"><tld lit="V"/>o</ref>, kies
      termoj estas <ref cel="funkci.0o.MAT">funkcioj</ref>.
    </dif>
    <refgrp tip="vid">
      <ref cel="konver.simple0a">simple konver&gcirc;a</ref>,
      <ref cel="konver.unuforme0a">unuforme konver&gcirc;a</ref>,
      <ref cel="konver.0a.en_mezuro">konver&gcirc;a en mezuro</ref>.
    </refgrp>
    <rim>
      En <fnt><bib>MatVort</bib></fnt> troveblas <ctl>funkciala
      <tld/>o</ctl>, kun la ekzota adjektiva formo responda al
      <ctl>funkcio</ctl>.
    </rim>
  </snc>
  <trdgrp lng="de">
    <trd>Funktionsfolge</trd>,
    <trd>Funktionenfolge</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>function <ind>sequence</ind></trd>,
    <trd>sequence of functions</trd>
  </trdgrp>
  <trd lng="fr">suite de fonctions</trd>
  <trd lng="hu">f&uuml;ggv&eacute;nysorozat</trd>  
  <trd lng="pl">ci&aogonek;g funkcyjny</trd>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;
  &c_f;&c_u;&c_n;&c_k;&c_c;&c_i;&c_j;</trd>
</drv>
<rim>
  La radiko <em><tld/></em> estas anka&ubreve; prefikse uzata por montri
  personon, kiu helpas kaj la&ubreve;bezone anstata&ubreve;as alian:
  <ekz>
    <ind><tld/>prezidanto</ind>;
    <trd lng="hu">aleln&ouml;k</trd>
  </ekz>
  <ekz>
    <ind><tld/><tld/>re&gcirc;o</ind>;
    <trd lng="hu">alkir&aacute;ly</trd>
  </ekz>  
  <ekz>
    <tld/>lui, <tld/>luigi.
  </ekz>
</rim>
