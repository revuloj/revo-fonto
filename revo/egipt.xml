<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>egipt</rad>/o</kap>
<drv mrk="egipt.0o">
  <kap><tld/>o</kap>
  <snc mrk="egipt.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Ano de antikva popolo, kiu kreis grandan civilizon en la
      malsupra fluo de Nilo, en Afriko:
      <ekz>
        similan teorion li prezentas pri <tld lit="E"/>io, atentigante,
        ke antikvaj skriba&jcirc;oj mencias iujn misterajn marpopolojn kaj tiel
        nomatajn <ctl>re&gcirc;ojn-pa&scirc;tistojn</ctl>, de kiuj la
        <tld/>oj supozeble heredis sian sperton pri navigado
        <fnt>
        <aut>Garbhan Macaoidh</aut>: <vrk>&Ccirc;u Esperanto de la Andoj?</vrk>
        <bib>Monato</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i havis sklavinon, <tld/>inon, kies nomo estis Hagar
        <fnt>
        <bib>MT</bib>, <lok>&Gen; 16:1</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">&Auml;gypter</trd>
    <trd lng="en">Egyptian</trd>
    <trd lng="fr">&eacute;gyptien</trd>
    <trd lng="es">egipcio</trd>
    <trd lng="ru">&c_je;&c_g;&c_i;&c_p;&c_t;&c_ja;&c_n;&c_i;&c_n;</trd>
  </snc>
  <snc mrk="egipt.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="egipt.0ujano"><tld lit="E"/>ujano</ref>:
      <ekz>
        la unuan fojon &scirc;i a&ubreve;dis la legendon, kiun la
        <tld/>oj rakontas al si pri la struto
        <fnt><bib>Fab4</bib>, <lok>Filino de la mar&ccirc;a re&gcirc;o</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>&Auml;gypter</trd>,
      <trd>&auml;gyptischer <ind>Staatsb&uuml;rger</ind></trd>
    </trdgrp>
    <trd lng="en">Egyptian</trd>
    <trd lng="es">egipcio</trd>
    <trd lng="fr">&eacute;gyptien</trd>
    <trd lng="ru">&c_je;&c_g;&c_i;&c_p;&c_t;&c_ja;&c_n;&c_i;&c_n;</trd>
  </snc>
  <snc mrk="egipt.0o.Kabe">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <klr>(<tld lit="E"/>o)</klr>
      <ref tip="dif" cel="egipt.E0ujo"><tld lit="E"/>ujo</ref>:
      <ekz>
        li estis fama en la tuta <tld lit="E"/>o
        <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 2</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">&Auml;gypten</trd>
    <trd lng="es">Egipto</trd>
    <trd lng="fr">&Eacute;gypte</trd>
  </snc>
  <trd lng="nl">Egyptenaar</trd>
</drv>

<drv mrk="egipt.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al la <tld/>oj.
    </dif>
  </snc>
  <snc>
    <dif>
      <tld lit="E"/>uja, rilata al la <tld lit="E"/>ujo.
    </dif>
  </snc>
  <trd lng="de">&auml;gyptisch</trd>
  <trd lng="es">egipcio</trd>
  <trd lng="fr">&eacute;gyptien</trd>
  <trd lng="nl">Egyptisch</trd>
  <trd lng="ru">&c_je;&c_g;&c_i;&c_p;&c_je;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="egipt.E0ujo">
  <kap><tld lit="E"/>ujo</kap>
  <fnt><bib>ZR</bib></fnt>
  <snc>
    <dif>
      Lando en la nordorienta Afriko, &ccirc;irka&ubreve; la malsupra parto de
      Nilo, kie iam esti&gcirc;is la civilizo de la
      <ref cel="egipt.0o.HIS"><tld/>oj<sncref/></ref> kaj nun
      estas araba &scirc;tato.
    </dif>
  </snc>
  <trd lng="de">&Auml;gypten</trd>
  <trd lng="es">Egipto</trd>
  <trd lng="fr">&Eacute;gypte</trd>
  <trd lng="ru">&c_Je;&c_g;&c_i;&c_p;&c_je;&c_t;</trd>
  <trd lng="nl">Egypte</trd>
  <trd lng="tr">M&inodot;s&inodot;r</trd>
</drv>

<drv mrk="egipt.0ujano">
  <kap><tld/>ujano</kap>
  <snc>
    <dif>
      Lo&gcirc;anto de <tld lit="E"/>ujo:
      <ekz>
        la modernaj <tld/>ujanoj estas, plejparte, arboj kaj koptoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">&Auml;gypter</trd>
  <trd lng="en">Egyptian</trd>
  <trd lng="es">egipcio</trd>
  <trd lng="fr">&eacute;gyptien</trd>
  <trd lng="nl">Egyptenaar</trd>
  <trd lng="ru">&c_je;&c_g;&c_i;&c_p;&c_t;&c_ja;&c_n;&c_i;&c_n;</trd>
</drv>
</art>
