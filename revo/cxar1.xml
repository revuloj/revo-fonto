<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>&ccirc;ar</rad>/o
</kap>
<drv mrk="cxar1.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Antikva durada veturilo por kurludoj, bataloj, en kiu oni ne sidis, sed
      &ccirc;iam staris.
    </dif>
    <trd lng="ru">&c_k;&c_o;&c_l;&c_je;&c_s;&c_n;&c_i;&c_c;&c_a;</trd>
  </snc>
  <snc num="2">
    <dif>
      &Scirc;ar&gcirc;veturilo:
      <ekz>
        jungi sian <tld/>on
        <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Eliro 14, 6</lok>)</fnt>.
      </ekz> 
      <trdgrp lng="ru">
        <trd>&c_p;&c_o;&c_v;&c_o;&c_z;&c_k;&c_a;</trd>,
        <trd>&c_v;&c_o;&c_z;</trd>,
        <trd>&c_t;&c_je;&c_l;&c_je;&c_g;&c_a;</trd>
      </trdgrp>
    </dif>
  </snc>
  <snc num="3">
    <dif>
      Solena radveturilo:
      <ekz>
        funebra <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de"><trd>Wagen</trd>, <trd>Karren</trd></trdgrp>
  <trd lng="en">car</trd>
</drv>
</art>
