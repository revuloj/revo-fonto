<art mrk="$Id: cxar1.xml,v 1.46 2022/11/23 08:35:11 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>&ccirc;ar</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="cxar1.0o">
  <kap><tld/>o</kap>
  <snc mrk="cxar1.0o.KOMUNE">
    <dif>
      Rada <ref tip="super" cel="vetur.0ilo">veturilo</ref> sen motoro.
    </dif>
    <subsnc mrk="cxar1.0o.sxargxoveturilo">
      <dif>
        &Scirc;ar&gcirc;veturilo:
        <ekz>
          <ind>jungi sian <tld/>on</ind>
          <fnt><bib>MT</bib>, <lok>&Eli; 14:6</lok></fnt>;
          <trd lng="de">den Wagen <ind>anspannen</ind></trd>
          <trd lng="en">hitch one's cart</trd>
        </ekz>
        <ekz>
          li &scirc;ar&gcirc;is <tld/>on per ligno, jungis du bovojn kaj veturis en la urbon
          <fnt>
            <bib>ElektFab</bib>
            <lok>Doktoro &Ccirc;ioscia</lok>
          </fnt>;
        </ekz>
        <ekz>
          la reto ja apena&ubreve; estis &jcirc;etita en la akvon, oni jam tiradis plenajn <tld/>ojn da
          haringoj sur la teron
          <fnt>
            <bib>Fab4</bib>
            <lok>Historio el la dunoj</lok>
          </fnt>;
        </ekz>
        <ekz>
          malnova <tld/>o, kiun mi eltrovis anta&ubreve; kelkaj tagoj en kale&scirc;ejo
          <fnt>
            <bib>KPr</bib>
          </fnt>;
        </ekz>
        <ekz>
          dum mi gapadas, a&ubreve;toj, <tld/>oj, &scirc;ar&gcirc;veturiloj, bicikloj kaj zorgo&scirc;ar&gcirc;itaj homoj
          kuras preter mi
          <fnt>
            <bib>LGA</bib>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="fr">chariot<klr tip="ind"> (v&eacute;hicule)</klr></trd>
      <trd lng="hu">szek&eacute;r</trd>
      <trdgrp lng="nl">
        <trd>kar</trd>,
        <trd>wagen</trd>
      </trdgrp>
      <trd lng="pl">w&oacute;z</trd>
      <trdgrp lng="ru">
        <trd>&c_p;&c_o;&c_v;&c_o;&c_z;&c_k;&c_a;</trd>,
        <trd>&c_v;&c_o;&c_z;</trd>,
        <trd>&c_t;&c_je;&c_l;&c_je;&c_g;&c_a;</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="cxar1.0o.solena">
      <dif>
        Solena radveturilo:
        <ekz>
          <ind>funebra <tld/>o</ind>;
          <trd lng="hu">halottaskocsi</trd>
          <trd lng="pl">karawan</trd>
          <trd lng="ru">&c_k;&c_a;&c_t;&c_a;&c_f;&c_a;&c_l;&c_k;</trd>
        </ekz>
        <ekz>
          post la kale&scirc;oj de invititoj aperis la grandfesta <tld/>o de Prelongoj
          <fnt>
            <bib>KPr</bib>
          </fnt>;
        </ekz>
        <ekz>
          mi havas ordonon peti vin veni en la re&gcirc;an palacon, re&gcirc;a <tld/>o atendas nin
          apud la pordego
          <fnt>
            <bib>I&scirc;tar</bib>
          </fnt>;
        </ekz>
        <ekz>
          la fajra <tld/>o de <nom>Apollon</nom> ripoziras vespere
          ne al la akvo de la <nac>Okeanos</nac> sed al la vinkeloj
          <fnt>
            <bib>SatirRak</bib>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="fr">char<klr tip="ind"> (v&eacute;hicule de
      c&eacute;r&eacute;monie)</klr></trd>
      <trd lng="hu">kocsi</trd>
      <trd lng="nl">ceremoniewagen</trd>
      <trd lng="pl">pow&oacute;z</trd>
      <trd lng="ru">&c_p;&c_o;&c_v;&c_o;&c_z;&c_k;&c_a;</trd>
    </subsnc>
    <subsnc mrk="cxar1.0o.antikva">
      <dif>
        <ref tip="dif" cel="cxar1.batal0o.MIL">Batal<tld/>o</ref>:
        <ekz>
          la &ccirc;evaloj de Faraono kun liaj <tld/>oj kaj rajdantoj eni&gcirc;is en la maron
          <fnt>
            <bib>MT</bib>
            <lok>Eliro 15:19</lok>
          </fnt>;
        </ekz>
        <ekz>
          li <klr>[&#8230;]</klr> ekmirus, se iu ajn malamiko ne forkurus, vidante liajn
          galopantajn &ccirc;evalojn, sur la <tld/>o de l&#8217; faraono staras la dioj mem por
          &scirc;irmi lin per &scirc;ildo kaj trapiki la malamikojn
          <fnt>
            <bib>Far2</bib>
            <lok>&ccirc;apitro 5a</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="en">chariot</trd>
      <trd lng="fr">char <klr tip="ind">(antique)</klr></trd>
      <trd lng="hu">kocsi</trd>
      <trd lng="pl">rydwan</trd>
      <trd lng="ru">&c_k;&c_o;&c_l;&c_je;&c_s;&c_n;&c_i;&c_c;&c_a;</trd>
      <trdgrp lng="nl">
        <trd>kar</trd>,
        <trd>wagen</trd>
      </trdgrp>
    </subsnc>
    <ekz>
      homo enirinte en la piramidon en handikapula <tld/>o foriras proprapiede
      <fnt>
        <bib>Monato</bib>
        <aut>Laimius Stra&zcaron;nickas</aut>
        <vrk><url ref="&Monato;/2004/008225.html">Mirakloj en arbarprofundo</url></vrk>
        <lok>2004</lok>
      </fnt>.
    </ekz>
    <refgrp tip="sub">
      <ref cel="kalesx.0o">kale&scirc;o</ref>,
      <ref cel="vetur.post0ilo">postveturilo</ref>,
      <ref cel="vagon.0o">vagono</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_o;&c_z;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_jo;&c_s;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_o;&c_z;&c_k;&c_a;</trd>,
    <trd>&c_b;&c_r;&c_y;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_ja;&c_s;&c_mol;&c_n;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">carro</trd>
  <trdgrp lng="de">
   <trd>Wagen</trd>,
   <trd>Karren</trd>
  </trdgrp>
  <trd lng="en">car</trd>
  <trdgrp lng="es">
    <trd>carro</trd>,
    <trd>carruaje</trd>
  </trdgrp>
  <trd lng="he">&ayin;&#1458;&gimel;&#1464;&lamed;&#1464;&he;</trd>
  <trd lng="it">carro</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_o;&c_l;&c_ib;&c_s;&c_n;&c_i;&c_c;&c_ja;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_ja;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_je;&c_t;&c_a;</trd>,
    <trd>&c_v;&c_ib;&c_z;&c_o;&c_k;</trd>,
    <trd>&c_v;&c_ib;&c_z;</trd>,
    <trd>&c_d;&c_v;&c_o;&c_k;&c_o;&c_l;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="cxar1.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="cxar1.0eto.durada">
    <dif>
      Malgranda <tld/>o, kiun homo povas pu&scirc;i a&ubreve; tiri:
      <ekz>
        <tld/>eto, kies rado rompi&gcirc;is
        <fnt>
          <bib>VRA</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>eto anta&ubreve;eniris en la &ccirc;ambron, 
        alportante &ccirc;iujn elementojn de bongusta vesperman&gcirc;o
        <klr>(vd <ref tip="sub" cel="cxar1.servo0eto">servo&ccirc;areto</ref>)</klr>
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[en]</klr> la superbazaro, kie multaj homoj &ndash; &ccirc;efe virinoj &ndash; pu&scirc;adis sian
        <tld/>eton de sekcio al sekcio 
        <klr>(vd <ref tip="sub" cel="cxar1.acxet0eto">a&ccirc;et&ccirc;areto</ref>)</klr>
        <fnt>
          <bib>ChV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_t;&c_a;&c_ch;&c_k;&c_a;</trd>,
      <trd>&c_k;&c_a;&c_l;&c_ja;&c_s;&c_k;&c_a;</trd>,
      <trd>&c_d;&c_r;&c_a;&c_b;&c_ib;&c_n;&c_k;&c_ib;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>carret</trd>,
      <trd>carret&oacute;</trd>
    </trdgrp>
    <trd lng="de">W&auml;gelchen</trd>
    <trd lng="en">pushcart</trd>
    <trdgrp lng="fr">
      <trd>charriot</trd>,
      <trd>chariot</trd>,
      <trd>charrette</trd>
    </trdgrp>
    <trd lng="hu">taliga</trd>
    <trdgrp lng="it">
      <trd>carretto</trd>,
      <trd>carrello</trd>
    </trdgrp>
    <trd lng="nl">handkar</trd>
    <trdgrp lng="pl">
      <trd>taczka</trd>,
      <trd>kara<klr> (region.)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_t;&c_je;&c_l;&c_je;&c_zh;&c_k;&c_a;</trd>,
      <trd>&c_t;&c_a;&c_ch;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="cxar1.0eto.skribmasxino">
    <uzo tip="fak">TEK</uzo>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Movebla parto de skribma&scirc;ino, konsistanta &ccirc;efe el
      ka&ubreve;&ccirc;uka cilindro,
      &ccirc;irka&ubreve; kiu volvi&gcirc;as la papero, kaj el la
      &gcirc;in portanta &ccirc;asio.
    </dif>
    <trd lng="be">&c_k;&c_a;&c_r;&c_e;&c_t;&c_k;&c_a;</trd>
    <trd lng="ca">carro <klr>(m&agrave;quina d'escriure)</klr></trd>
    <trd lng="en">carriage</trd>
    <trd lng="fr">chariot<klr tip="ind"> (d'une machine &agrave;
    &eacute;crire)</klr></trd>
    <trd lng="hu">kocsi<klr tip="ind">
    (&iacute;r&oacute;g&eacute;pen)</klr></trd>
    <trd lng="it">carrello<klr> (comp., stamp. e sim.)</klr></trd>
    <trd lng="nl">wagen<klr tip="ind"> (v.schrijfmachine)</klr></trd>
    <trdgrp lng="pl">
      <trd>w&oacute;zek</trd>,
      <trd>karetka</trd>,
      <trd>g&lstroke;owica<klr> drukuj&aogonek;ca</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_a;&c_r;&c_je;&c_t;&c_k;&c_a;</trd>
  </snc>
  <trd lng="he">&ayin;&gimel;&lamed;&tav; &yod;&dalet;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_z;&c_o;&c_k;</trd>,
    <trd>&c_t;&c_a;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_je;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="cxar1.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="cxar1.0isto.TRA">
    <dif>
      <ref tip="super" cel="vetur.0igisto">Veturigisto</ref>
      de <tld/>o:
      <ekz>
        la <tld/>o, portanta la <tld/>iston, la oficiston kaj Omaron,
        da&ubreve;rigis la vojiron
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 15a</lok></fnt>;
      </ekz>
      <ekz>
        la kuzo de mia patrino estis la batal<tld/>isto de <nom>Katumandos</nom>
        <fnt>
          <aut>A. L&ouml;wenstein</aut>,
          <vrk>La &scirc;tona urbo</vrk>
          <lok>1999</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_o;&c_z;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_k;&c_u;&c_ch;&c_a;&c_r;</trd>,
    <trd>&c_f;&c_u;&c_r;&c_m;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="ca">carreter</trd>
  <trdgrp lng="de">
    <trd>Fuhrmann</trd>,
    <trd>Kutscher</trd>
  </trdgrp>
  <trd lng="fr">charretier</trd>
</drv>

<drv mrk="cxar1.CX0isto">
  <kap><tld lit="&Ccirc;"/>isto <fnt>
    <aut>G.-H. Clopeau, C.  Bertin</aut>,
    <vrk>Esperanta-Franca Fakvortaro pri Astronomio</vrk></fnt></kap>
  <mlg>Aur</mlg>
  <snc mrk="cxar1.CX0isto.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Nord-hemisfera <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref>
      (<trd lng="la">Auriga</trd>).
    </dif><ref tip="sin" cel="kocxer.K0o">Ko&ccirc;ero</ref>
  </snc>
  <trd lng="be">&c_V;&c_o;&c_z;&c_mol;&c_n;&c_ib;&c_k; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#8217;&c_je;)</klr></trd>
  <trd lng="ca">el Cotxer</trd>
  <trd lng="de">Fuhrmann <klr>(Sternbild)</klr></trd>
  <trd lng="en">Auriga</trd>
  <trd lng="es">Cochero</trd>
  <trd lng="fr">Cocher<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="it">Cocchiere</trd>
  <trd lng="nl">Voerman<klr tip="ind"> (sterrenbeeld)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_z;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_v;&c_ib;&c_z;&c_n;&c_i;&c_c;&c_ja;</trd>,
    <trd>&c_ji;&c_z;&c_d;&c_o;&c_v;&c_i;&c_j; (&c_ib;&c_m;.)</trd>
  </trdgrp>
</drv>


<drv mrk="cxar1.acxet0eto">
  <kap>a&ccirc;et<tld/>eto</kap>
  <snc mrk="cxar1.acxet0eto.HEJM">
    <dif>
      <tld lit="&Ccirc;"/>eto disponigata en grandaj vendejoj
      por koleko de a&ccirc;eta&jcirc;oj:
      <ekz>
        kiujn kvin a&jcirc;ojn vi metus en vian a&ccirc;et<tld/>eton
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://koro.lavilleheleuc.fr/babiladeto-1.pdf">
          Babiladeto 1</url></vrk>,
          <lok>[2010?]</lok>
        </fnt>?
      </ekz>
      <ekz>
        li ekiras, pu&scirc;ante a&ccirc;et<tld/>eton la&ubreve;longe de la stratoj, li plenigas &gcirc;in per
        boteloj por reciklado
        <fnt>
          <bib>MD14</bib>
          <vrk>La reveno de uzuro en socio plagata de en&scirc;uldi&gcirc;o</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="korb.0o">rulkorbo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_z;&c_o;&c_k; <klr>(&c_u; &c_k;&c_r;&c_a;&c_m;&c_je;)</klr></trd>
  <trd lng="de">Einkaufswagen</trd>
  <trdgrp lng="fr">
    <trd>caddie</trd>,
    <trd>charriot<klr> (caddie)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="cxar1.batal0o">
  <kap>batal<tld/>o</kap>
  <snc mrk="cxar1.batal0o.MIL">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Antikva durada veturilo por kurludoj, bataloj, en kiu oni &ccirc;iam staris; 
      <ref tip="sin" cel="cxar1.0o.antikva">&ccirc;aro<sncref/></ref>:
      <ekz>
        la militistoj de nia popolo estis famaj pro sia lerteco per la batal<tld/>o, kaj
        ili scipovis plenumi tiajn manovrojn, kiel mar&scirc;i la&ubreve; la timono de la &ccirc;aro dum
        la &ccirc;evaloj galopis je plena impeto
        <fnt>
          <aut>Anna L&ouml;wenstein</aut>
          <vrk>La &scirc;tona urbo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la militin&gcirc;eniero de nia batal<tld/>a brigado
        <fnt>
          <bib>SatirRak</bib>
        </fnt>;
      </ekz>
      <ekz>
        al mi trupojn kaj batal<tld/>istojn nur
        malmultanombre li donis 
        <fnt>
          <aut>trad. C. Brosch</aut>,
          <vrk><url ref="http://www.cyrilbrosch.net/wb/pages/esperanto/tradukoj/prozo.php">
          La Apologio de &Hcirc;atusilio III</url></vrk>,
          <lok>[vidita en 2015]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Streitwagen</trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_ja;&c_s;&c_mol;&c_n;i&c_c;&c_a; <klr>(&c_v;&c_a;&c_j;&c_s;&c_k;&c_o;&c_v;&c_a;&c_ja;)</klr></trd>
  <trd lng="fr">char<klr> (de bataille)</klr></trd>
</drv>

<drv mrk="cxar1.bebo0eto">
  <kap>bebo<tld/>eto</kap>
  <snc mrk="cxar1.bebo0eto.HEJM">
    <dif>
      <tld lit="&Ccirc;"/>eto en kiu povas ku&scirc;i
      bebo, ordinare sub &scirc;irmo de refaldebla tegmento:
      <ekz>
        <nom>Muzeta</nom> <klr>[&#8230;]</klr> pu&scirc;as anta&ubreve;en duoblan
        bebo<tld/>eton kun dormantaj &gcirc;emelaj beboj
        <fnt>
          <aut>L. Carol</aut>,
          <vrk>La veki&gcirc;o</vrk>,
          <lok>La Karavelo, 2010-10 (23)</lok>
        </fnt>;
      </ekz>
      <ekz>
        aperis en tri kvartalo-oficejoj patrinoj kun bebo<tld/>etoj
        <fnt>
          <bib>Monato</bib>
          <aut>Kawai Yuka</aut>
          <vrk><url ref="&Monato;/2013/011028.php">Mankas vartejoj</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Kinderwagen</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_l;&c_ja;&c_s;&c_k;&c_a; <klr>(&c_d;&c_z;&c_ib;&c_c;&c_ja;&c_ch;&c_a;&c_ja;)</klr></trd>,
    <trd>&c_v;&c_a;&c_z;&c_o;&c_k;</trd>,
    <trd>&c_l;&c_ju;&c_l;&c_mol;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="fr">landau</trd>
</drv>

<drv mrk="cxar1.cxeval0o">
  <kap>
    &ccirc;eval<tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="&Ccirc;"/>o tiarata de unu a&ubreve; pluraj &ccirc;evaloj:
      <ekz>
        ekskurso per &ccirc;eval<tld/>o
        <fnt>
          <bib>LOdE</bib>
          <aut>Lu Wunsch-Rolshoven</aut>
          <vrk>Esperanto kun multa plezuro</vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i transportas siajn verkojn &gcirc;is la a&ubreve;tobus-stacio per &ccirc;eval<tld/>o
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/007061.html">Litovio: Ceramika&jcirc;oj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="cxar1.batal0o">batal&ccirc;aro</ref>,
      <ref cel="kalesx.0o">kale&scirc;o</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_l;&c_jo;&c_s;&c_y;</trd>,
    <trd>&c_d;&c_r;&c_a;&c_b;&c_ib;&c_n;&c_y;</trd>,
    <trd>&c_f;&c_u;&c_r;&c_m;&c_a;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_o;&c_z;</trd>
  </trdgrp>
  <trd lng="de">Pferdefuhrwerk</trd>
</drv>


<drv mrk="cxar1.infan0eto">
  <kap>infan<tld/>eto</kap>
  <snc mrk="cxar1.infan0eto.HEJM">
    <dif>
      <tld lit="&Ccirc;"/>eto en kiu povas ku&scirc;i a&ubreve; sidi
      infano, kiun oni tiurimede transportas:
      <ekz>
        &scirc;i promenas, pu&scirc;ante sian infan<tld/>eton sur la trotuaro
        <fnt><aut>L. Beaucaire</aut>, <vrk>Kruko kaj Baniko el 
        Bervalo</vrk>, <lok>1974</lok></fnt>;
      </ekz>
      <ekz>
        Lotinjo &ccirc;iam estis kun infan<tld/>eto, la bebo nomi&gcirc;is Felikso
        <klr>(do <ref tip="sin" cel="cxar1.bebo0eto">bebo&ccirc;areto</ref>)</klr>
        <fnt>
          <bib>LOdE</bib>
          <aut>G&uuml;nter Grass, trad. Wolfgang Kirschstein</aut>
          <vrk>Mia Jarcento</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Sportwagen <klr>(Kind)</klr></trd>,
      <trd>Kinderwagen</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_l;&c_ja;&c_s;&c_k;&c_a; <klr>(&c_d;&c_z;&c_ib;&c_c;&c_ja;&c_ch;&c_a;&c_ja;)</klr></trd>,
    <trd>&c_v;&c_a;&c_z;&c_o;&c_k;</trd>,
    <trd>&c_l;&c_ju;&c_l;&c_mol;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="fr">poussette</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_i;&c_t;&c_ja;&c_ch;&c_i;&c_j; &c_v;&c_ib;&c_z;&c_o;&c_k;</trd>,
    <trd>&c_d;&c_i;&c_t;&c_ja;&c_ch;&c_a; &c_k;&c_o;&c_l;&c_ja;&c_s;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="cxar1.krom0o">
  <kap>krom<tld/>o</kap>
  <snc mrk="cxar1.krom0o.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      Malalta <tld/>eto alkro&ccirc;ita &ccirc;e la flanko de
      motorciklo por ensido de unu homo:
      <ekz>
        <nom>Helmut</nom> veturis la unua, en krom<tld/>o
        de motociklo
        <fnt><aut>I. Nemere</aut>, <vrk>Sur Kampo Granita</vrk>, 
        <lok>1983</lok></fnt>;
      </ekz>
      <ekz>
        al motorcikla krom<tld/>o oni fiksas ladan keston kun grandaj fenestroj, multe
        pli ampleksan ol la kutima se&gcirc;o por iel ka&ubreve;ranta krompasa&gcirc;ero
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2012/010723.php">Lastatikle motorcikle</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ju;&c_l;&c_mol;&c_k;&c_a; <klr>(&c_m;&c_a;&c_t;&c_a;&c_c;&c_y;&c_k;&c_l;&c_a;)</klr></trd>
  <trd lng="ca">sidecar</trd>
  <trdgrp lng="de">
    <trd>Beiwagen<klr> (Motorradgespann)</klr></trd>
  </trdgrp>
  <trd lng="en">sidecar</trd>
  <trd lng="fr">side-car</trd>
  <trd lng="hu">oldalkocsi</trd>
  <trd lng="it">sidecar</trd>
  <trd lng="nl">zijspan</trd>
  <trdgrp lng="pl">
    <trd>przyczepka<klr> boczna (motocykla)</klr></trd>,
    <trd>gondola<klr> (motocykla)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_ja;&c_s;&c_k;&c_a;
  <klr>(&c_m;&c_o;&c_t;&c_o;&c_c;&c_i;&c_k;&c_l;&c_a;)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_o;&c_l;&c_ja;&c_s;&c_k;&c_a; (&c_d;&c_o; &c_m;&c_o;&c_t;&c_o;&c_c;&c_i;&c_k;&c_l;&c_a; &c_ib; &c_t;.&c_p;.)</trd>,
    <trd>&c_p;&c_r;&c_i;&c_ch;&c_ib;&c_p;</trd>
  </trdgrp>
</drv>

<drv mrk="cxar1.lev0o">
  <kap>
    lev<tld/>o
  </kap>
  <snc mrk="cxar1.lev0o.MAS">
    <uzo tip="fak">MAS</uzo>
    <dif>
      Motora sursidebla <tld/>eto havanta horizontalan dudentan forkon, per kiu oni
      levas kaj mallevas peza&jcirc;ojn:
      <ekz>
        oni kontrolu <nom>YouTube</nom>-on nur iomete por trovi multajn filmetojn de teruraj
        akcidentoj kun lev<tld/>oj
        <fnt>
          <aut>-</aut>
          <vrk><url ref="https://priioajn.wordpress.com/2019/06/28/levcaro/"
              >Lev&ccirc;aro</url></vrk>
          <lok>Pri io ajn, 2019-06-28</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ju;&c_l;&c_mol;&c_k;&c_a; <klr>(&c_p;&c_a;&c_d;&#8217;&c_jo;&c_m;&c_n;&c_a;&c_ja;)</klr></trd>
  <trdgrp lng="de">
    <trd>Gabelstapler</trd>,
    <trd>Hubstapler</trd>
  </trdgrp>
  <trd lng="fr">charriot &eacute;l&eacute;vateur</trd>
</drv>

<drv mrk="cxar1.man0eto">
  <kap>
    man<tld/>eto
  </kap>
  <snc>
    <dif>
      <tld lit="&Ccirc;"/>eto kiun oni mane tiras per timoneto:
      <ekz>
        mi ekvidis man-<tld/>eton, &scirc;ovatan de viro kaj sur kiu ku&scirc;is vira kadavro
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        ni transportis niajn varojn kaj rekvizitojn al stacidomo per <klr>[&#8230;]</klr>
        pruntita man<tld/>eto
        <fnt>
          <bib>KrM</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_z;&c_o;&c_k; <klr>(&c_r;&c_u;&c_ch;&c_n;&c_y;)</klr></trd>,
    <trd>&c_d;&c_r;&c_a;&c_b;&c_ib;&c_n;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="de">Handwagen</trd>
  <trd lng="en">toy wagon</trd>
</drv>


<drv mrk="cxar1.pakajxo0eto">
  <kap>paka&jcirc;o<tld/>eto</kap>
  <snc mrk="cxar1.pakajxo0eto.HEJM">
    <dif>
      <tld lit="&Ccirc;"/>eto por portado de paka&jcirc;oj, valizoj a&ubreve; aliaj
      peza&jcirc;oj, precipe uzata en stacio, haveno...:
      <ekz>
        &scirc;i estas transportata <klr>[&#8230;]</klr>
        per malalta paka&jcirc;o<tld/>eto
        <fnt>
          <aut>-</aut>,
          <vrk>&Ccirc;inio Perdis siajn Lastajn E&ubreve;ropajn Mona&hcirc;inojn</vrk>,
          <lok><url ref="http://www.esperokatolika.org/ek19661970/ek1966_10.htm">
          Espero Katolika, 1966:10</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_g;&c_a;&c_zh;&c_n;&c_y; <ind>&c_v;&c_a;&c_z;&c_o;&c_k;</ind></trd>  
  <trd lng="de">Gep&auml;ckwagen</trd>
  <trd lng="fr">charriot &agrave; bagages</trd>
</drv>


<drv mrk="cxar1.0umo">
  <kap>pu&scirc;<tld/>o, <var><kap><tld/>umo</kap></var></kap>
  <snc mrk="cxar1.0umo.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      Malgranda <tld/>eto, kiun oni pu&scirc;as a&ubreve; tiras per 
      du timonetoj, kaj kiu ruli&gcirc;as sur unu a&ubreve; du radoj:
      <ekz>
        la vila&gcirc;ano e&ccirc; donacis al li ankora&ubreve; grandan pu&scirc;<tld/>on, por ke li povu
        forveturigi la monon kaj la keston
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        knabineto <klr>[&#8230;]</klr> alveturigis al la benko pu&scirc;<tld/>eton kaj komencis
        plenigi &gcirc;in per la sablo
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> pavimbatiloj <klr>[&#8230;]</klr> staris meze de &scirc;oveliloj, mezuriloj kaj
        pu&scirc;<tld/>oj
        <fnt>
          <bib>Fab3</bib>
          <lok>Du pavimbatiloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        li tre rapide pu&scirc;is tra la strato <tld/>umon, en kiu li portis grandan sakon
        <fnt>
          <bib>MMa</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_ch;&c_k;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>carret&oacute;</trd>,
    <trd>bolquet</trd>
  </trdgrp>
  <trd lng="de">Schubkarre</trd>
  <trd lng="en">wheelbarrow</trd>
  <trd lng="fr">brouette</trd>
  <trd lng="he">&mem;&resh;&yod;&tsadi;&he;</trd>
  <trd lng="hu">talicska</trd>
  <trd lng="it">carriola</trd>
  <trd lng="nl">kruiwagen</trd>
  <trdgrp lng="pl">
    <trd>taczka</trd>,
    <trd>kara<klr> (region.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_ch;&c_k;&c_a;</trd>
  <trd lng="uk">&c_t;&c_a;&c_ch;&c_k;&c_a;</trd>
</drv>

<drv mrk="cxar1.servo0eto">
  <kap>servo<tld/>eto</kap>
  <snc mrk="cxar1.servo0eto.HEJM">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="&Ccirc;"/>eto kun pletoj, per kiu oni
      proksimigas man&gcirc;a&jcirc;ojn al la man&gcirc;antoj:
      <ekz>
        &ccirc;i tie trovis spacon klaplito por sunumi, ventumil-forma se&gcirc;o, 
        servo<tld/>eto
        <fnt>
          <aut>H. Zs&oacute;ri</aut>,
          <vrk><url ref="http://www.zsorihajnalka.hu/teraszok_erkelyek3_esp.htm">
          Balkonoj</url></vrk>,
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_r;&c_v;&c_ib;&c_r;&c_o;&c_v;&c_a;&c_ch;&c_n;&c_y; <ind>&c_s;&c_t;&c_o;&c_l;&c_ib;&c_k;</ind> <klr>(&c_n;&c_a; &c_k;&c_o;&c_l;&c_c;&c_a;&c_h;)</klr></trd>
  <trdgrp lng="de">
      <trd>Servierwagen</trd>,
      <trd>Teewagen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>desserte roulante</trd>,
    <trd>table roulante</trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_ib;&c_z;&c_o;&c_k; (&c_n;&c_a; &c_k;&c_o;&c_l;&c_ib;&c_shch;&c_a;&c_t;&c_a;&c_h;)</trd>
</drv>
