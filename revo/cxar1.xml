<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>&ccirc;ar</rad>/o
</kap>
<drv mrk="cxar1.0o">
  <kap><tld/>o</kap>
  <snc mrk="cxar1.0o.antikva">
    <dif>
      Antikva durada veturilo por kurludoj, bataloj, en kiu oni ne sidis, sed
      &ccirc;iam staris.
    </dif>
    <trd lng="en">chariot</trd>
    <trd lng="fr">char <klr tip="ind">(antique)</klr></trd>
    <trd lng="ru">&c_k;&c_o;&c_l;&c_je;&c_s;&c_n;&c_i;&c_c;&c_a;</trd>
  <trdgrp lng="nl">
    <trd>kar</trd>,
    <trd>wagen</trd>
  </trdgrp>
  </snc>
  <snc mrk="cxar1.0o.sxargxoveturilo">
    <dif>
      &Scirc;ar&gcirc;veturilo:
      <ekz>
        jungi sian <tld/>on
        <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Eliro 14, 6</lok>)</fnt>.
        <trd lng="en">hitch one's cart</trd>
      </ekz> 
    </dif>
      <trd lng="fr">chariot<klr tip="ind"> (v&eacute;hicule)</klr></trd>
      <trdgrp lng="nl">
    <trd>kar</trd>,
    <trd>wagen</trd>
  </trdgrp>
  <trdgrp lng="ru">
        <trd>&c_p;&c_o;&c_v;&c_o;&c_z;&c_k;&c_a;</trd>,
        <trd>&c_v;&c_o;&c_z;</trd>,
        <trd>&c_t;&c_je;&c_l;&c_je;&c_g;&c_a;</trd>
      </trdgrp>
  </snc>
  <snc mrk="cxar1.0o.solena">
    <dif>
      Solena radveturilo:
      <ekz>
        funebra <tld/>o.
      </ekz>
    </dif>
    <trd lng="fr">char<klr tip="ind"> (v&eacute;hicule de c&eacute;r&eacute;monie)</klr></trd>
  <trd lng="nl">ceremoniewagen</trd>
</snc>
  <trdgrp lng="de"><trd>Wagen</trd>, <trd>Karren</trd></trdgrp>
  <trd lng="en">car</trd>
</drv>
<drv mrk="cxar1.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="cxar1.0eto.durada">
    <dif>
      Durada malgranda <tld/>o, kiun oni pu&scirc;as a&ubreve; tiras per du timonetoj.
    </dif>
    <trd lng="en">pushcart</trd>
    <trd lng="fr">charrette<klr tip="ind"> (v&eacute;hicule)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_je;&c_l;&c_je;&c_zh;&c_k;&c_a;</trd>,
      <trd>&c_t;&c_a;&c_ch;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="cxar1.0eto.skribmasxino">
    <uzo tip="fak">TEK</uzo>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Movebla parto de skribma&scirc;ino, konsistanta &ccirc;efe el ka&ubreve;&ccirc;uka cilindro,
      &ccirc;irka&ubreve; kiu volvi&gcirc;as la papero, k el la &gcirc;in portanta &ccirc;asio.
    </dif>
   <trd lng="en">carriage</trd>
   <trd lng="fr">chariot <klr tip="ind">(d'une machine &agrave; &eacute;crire)</klr></trd>
   <trd lng="nl">wagen<klr tip="ind"> (v.schrijfmachine)</klr></trd>
   <trd lng="ru">&c_k;&c_a;&c_r;&c_je;&c_t;&c_k;&c_a;</trd>
  </snc>
</drv>
<drv mrk="cxar1.0umo">
  <kap><tld/>umo</kap>
  <snc>
    <dif>
      Malgranda <tld/>eto, kiun oni pu&scirc;as a&ubreve; tiras per du timonetoj,
      kaj kiu ruli&gcirc;as sur unu sola rado, posta a&ubreve; meza.
    </dif>
  </snc>
  <trd lng="en">wheelbarrow</trd>
  <trd lng="fr">brouette</trd>
  <trd lng="nl">kruiwagen</trd>
  <trd lng="ru">&c_t;&c_a;&c_ch;&c_k;&c_a;</trd>
</drv>
<drv mrk="cxar1.krom0o">
  <kap>krom<tld/>o</kap>
  <snc>
    <dif>
      Gondolforma <tld/>eto alkro&ccirc;ita &ccirc;e la flanko de motorciklo.
    </dif>
  </snc>
  <trd lng="en">sidecar</trd>
  <trd lng="fr">side-car</trd>
  <trd lng="nl">zijspan</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_ja;&c_s;&c_k;&c_a; <klr>(&c_m;&c_o;&c_t;&c_o;&c_c;&c_i;&c_k;&c_l;&c_a;)</klr></trd>
</drv>
</art>
