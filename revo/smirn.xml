<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: smirn.xml,v 1.5 2023/09/29 21:21:52 afido Exp $">
    <kap>
      <rad>Smirn</rad>/o
    </kap>

<drv mrk="smirn.0o">
  <kap><tld/>o</kap>
<uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Malnova nomo (&gcirc;is 1930) de <ref tip="sin" cel="izmir.0o">Izmiro</ref>:
      <ekz>
        el Tripoliso mi estis revendata en Aleksandrio,
        el Aleksandrio denove mi estis
        vendata en Smirno
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
     <ekz>
        sankta Polikarpo el <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=132931">Apostolaj patroj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    </snc>
    <trd lng="be">&c_S;&c_mol;&c_m;&c_ib;&c_r;&c_n;&c_a;</trd>
    <trd lng="de">Smyrna</trd>
    <trd lng="en">Smyrna</trd>
    <trd lng="fr">Smyrne</trd>
    <trd lng="grc">&Sigma;&my;&ypsilon_ton;&rho;&ny;&alfa;</trd>
    <trd lng="ja">&#x30B9;&#x30DF;&#x30EB;&#x30CA;</trd>
    <trd lng="nl">Smyrna</trd>
    <trd lng="pl">Izmir</trd>
</drv>
  </art>

