<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: esoter.xml,v 1.24 2023/11/07 20:34:12 revo Exp $">
<kap>
  <rad>esoter</rad>/a
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="esoter.0a">
  <kap><tld/>a</kap>
  <snc>
  <uzo tip="fak">FIL</uzo>
    <dif>
      Intime, aparte instruata al la adeptoj:
      <ekz>
        la <tld/>a doktrino de Aristotelo;
      </ekz>
      <ekz>
        enigmo <tld/>a
        <fnt>
          <bib>LSB</bib>
          <lok>Lamentado</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;is la fino de la 19-a jarcento Esperanto restis <tld/>a lingvo de apena&ubreve;
        rimarkebla grupeto de poeziemaj idealistoj
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        senbaza kredo al ia <tld/>a vero
        <fnt>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2003/007929.html">Priskribo tre bone farita</url></vrk>
          <bib>Monato</bib>
          <lok>2003/02, p. 24</lok>
        </fnt>;
      </ekz>
      <ekz>
        la poezio de Antonio <nom>Machado</nom>, verkisto en tiu apena&ubreve; konata kaj iom <tld/>a
        kelta idiomo
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2004/008467.html">Kampoj de Soria Antonio Machado, universala
          poeto</url></vrk>
              <bib>Monato</bib>
          <lok>2004/12, p. 23</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi neniam sukcesis kompreni <tld/>a&jcirc;ojn kiel kalkulon diferencialan kaj
        integralan
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2003/007921.html">Ho, mistera pi!</url></vrk>
          <bib>Monato</bib>
          <lok>2003/06, p. 22</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="metafi.0o">metafizika</ref>,
      <ref cel="mistik.0a">mistika</ref>,
      <ref cel="okult.0a">okulta</ref>,
      <ref cel="obskur.0a.nefama">obskura<sncref/></ref>,
      <ref cel="sekret.0a">sekreta</ref>,
      <ref cel="spiritual.0a">spirituala</ref>
    </refgrp>
    <ref tip="vid" cel="inic.0i">inici</ref>
  </snc>
  <trd lng="be">&c_e;&c_z;&c_a;&c_t;&c_e;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>ezoterick&yacute;</trd>,
    <trd>ezotern&iacute;</trd>,
    <trd>tajen&yacute;</trd>,
    <trd>tajn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>esoterisch</trd>,
    <trd>geheim</trd>
  </trdgrp>
  <trd lng="en">esoteric</trd>
  <trd lng="fr">&eacute;sot&eacute;rique</trd>
  <trdgrp lng="hu">
    <trd>rejtett &eacute;rtelm&udblac;</trd>,
    <trd>titkos &eacute;rtelm&udblac;</trd>,
    <trd>ezoterikus</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x79D8;&#x6559;&#x7684;&#x306A;  <pr>&#x3072;&#x304D;&#x3087;&#x3046;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x79D8;&#x4F1D;&#x306E;  <pr>&#x3072;&#x3067;&#x3093;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="pl">ezoteryczny</trd>
  <trd lng="pt">esot&eacute;rico</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_z;&c_o;&c_t;&c_je;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_t;&c_a;&c_j;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_y;&c_t;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ezoterick&yacute;</trd>,
    <trd>tajn&yacute;</trd>,
    <trd>tajomn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_je;&c_z;&c_o;&c_t;&c_je;&c_r;&c_i;&c_ch;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_t;&c_a;&c_jeu;&c_m;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_h;&c_o;&c_v;&c_a;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_d;&c_o;&c_s;&c_t;&c_u;&c_p;&c_n;&c_i;&c_j; &c_d;&c_l;&c_ja; &c_z;&c_a;&c_g;&c_a;&c_l;&c_u;</trd>
  </trdgrp>
  <trd lng="zh">&#x79D8;&#x4F20; <pr>m&#xEC;chu&#xE1;n</pr></trd>
</drv>

<drv mrk="esoter.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Publike al &ccirc;iuj instruata.
    </dif>
    <refgrp tip="vid">
      <ref cel="profan.0a">profana</ref>,
      <ref cel="sekular.0a">sekulara</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_z;&c_a;&c_t;&c_e;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="de">exoterisch</trd>
  <trd lng="en">exoteric</trd>
  <trd lng="fr">exot&eacute;rique</trd>
  <trdgrp lng="hu">
    <trd>k&ouml;z&eacute;rthet&odblac;</trd>,
    <trd>exoterikus</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x516C;&#x6559;&#x7684;&#x306A;  <pr>&#x304A;&#x304A;&#x3084;&#x3051;&#x304D;&#x3087;&#x3046;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x516C;&#x958B;&#x306E;  <pr>&#x3053;&#x3046;&#x304B;&#x3044;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_k;&c_z;&c_o;&c_t;&c_je;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="esoter.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      Scio instruata nur al la adeptoj:
      <ekz>
        la verkoj de <nom>A&iuml;vanhov</nom> havas multajn komunajn punktojn kun <tld/>ismo kaj
        <ref cel="mistik.0ismo">mistikismo</ref>
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 10 (108)</lok>
        </fnt>;
      </ekz>
      <ekz>
        la a&ubreve;toro aludas pri certa <klr>[&#x2026;]</klr> <tld/>ismo: promesoj allogaj &gcirc;enerale,
        beda&ubreve;rinde ne por la recenzonto
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 5 (103)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_z;&c_a;&c_t;&c_e;&c_r;&c_y;&c_z;&c_m;</trd>,
    <trd>&c_e;&c_z;&c_a;&c_t;&c_e;&c_r;&c_y;&c_ch;&c_n;&c_y;&c_ja; &c_v;&c_je;&c_d;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Esoterik</trd>,
    <trd>Geheimlehre</trd>
  </trdgrp>
  <trd lng="en">esotericism</trd>
  <trd lng="fr">&eacute;sot&eacute;risme</trd>
  <trd lng="he">&alef;&zayin;&vav;&tet;&resh;&yod;&vav;&tav;</trd>
  <trdgrp lng="hu">
    <trd>titkos tan</trd>,
    <trd>ezoterizmus</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x79D8;&#x6559;  <pr>&#x3072;&#x304D;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x5BC6;&#x6559;  <pr>&#x307F;&#x3063;&#x304D;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="pl">ezoteryka</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_z;&c_o;&c_t;&c_je;&c_r;&c_i;&c_z;&c_m;</trd>,
    <trd>&c_t;&c_a;&c_j;&c_n;&c_o;&c_je;
    &c_u;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="zh">&#x5965;&#x79D8;&#x8BFB;&#x7269; <pr>&#xE0;om&#xEC;d&#xFA;w&#xF9;</pr></trd>
</drv>
</art>
