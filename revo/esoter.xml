<vortaro>
<art mrk="$Id: esoter.xml,v 1.18 2022/03/17 13:47:19 revo Exp $">
<kap>
  <rad>esoter</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="esoter.0a">
  <kap><tld/>a</kap>
  <snc>
  <uzo tip="fak">FIL</uzo>
    <dif>
      Intime, aparte instruata al la adeptoj:
      <ekz>
        la <tld/>a doktrino de Aristotelo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_z;&c_a;&c_t;&c_e;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>ezoterick&#xFD;</trd>,
    <trd>ezotern&#xED;</trd>,
    <trd>tajen&#xFD;</trd>,
    <trd>tajn&#xFD;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>geheim</trd>,
    <trd>esoterisch</trd>
  </trdgrp>
  <trd lng="en">esoteric</trd>
  <trd lng="fr">&eacute;sot&eacute;rique</trd>
  <trdgrp lng="hu">
    <trd>rejtett &eacute;rtelm&udblac;</trd>,
    <trd>titkos &eacute;rtelm&udblac;</trd>,
    <trd>ezoterikus</trd>
  </trdgrp>
  <trd lng="pl">ezoteryczny</trd>
  <trd lng="pt">esot&eacute;rico</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_z;&c_o;&c_t;&c_je;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_t;&c_a;&c_j;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_y;&c_t;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ezoterick&#xFD;</trd>,
    <trd>tajn&#xFD;</trd>,
    <trd>tajomn&#xFD;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x435;&#x437;&#x43E;&#x442;&#x435;&#x440;&#x438;&#x447;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x442;&#x430;&#x454;&#x43C;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x43F;&#x440;&#x438;&#x445;&#x43E;&#x432;&#x430;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x43D;&#x435;&#x434;&#x43E;&#x441;&#x442;&#x443;&#x43F;&#x43D;&#x438;&#x439; &#x434;&#x43B;&#x44F; &#x437;&#x430;&#x433;&#x430;&#x43B;&#x443;</trd>
  </trdgrp>
</drv>

<drv mrk="esoter.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Publike al &ccirc;iuj instruata.
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_z;&c_a;&c_t;&c_e;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="en">exoteric</trd>
  <trd lng="fr">exot&eacute;rique</trd>
  <trdgrp lng="hu">
    <trd>k&ouml;z&eacute;rthet&odblac;</trd>,
    <trd>exoterikus</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_k;&c_z;&c_o;&c_t;&c_je;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="esoter.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      Scio instruata nur al la adeptoj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_z;&c_a;&c_t;&c_e;&c_r;&c_y;&c_z;&c_m;</trd>,
    <trd>&c_e;&c_z;&c_a;&c_t;&c_e;&c_r;&c_y;&c_ch;&c_n;&c_y;&c_ja; &c_v;&c_je;&c_d;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Geheimlehre</trd>,
    <trd>Esoterismus</trd>
  </trdgrp>
  <trd lng="en">esotericism</trd>
  <trd lng="fr">&eacute;sot&eacute;risme</trd>
  <trd lng="he">&#x5D0;&#x5D6;&#x5D5;&#x5D8;&#x5E8;&#x5D9;&#x5D5;&#x5EA;</trd>
  <trdgrp lng="hu">
    <trd>titkos tan</trd>,
    <trd>ezoterizmus</trd>
  </trdgrp>
  <trd lng="pl">ezoteryka</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_z;&c_o;&c_t;&c_je;&c_r;&c_i;&c_z;&c_m;</trd>,
    <trd>&c_t;&c_a;&c_j;&c_n;&c_o;&c_je;
    &c_u;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
