<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>esoter</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="esoter.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">FIL</uzo>
  <snc>
    <dif>
      Intime, aparte instruata al la adeptoj:
      <ekz>
        la <tld/>a doktrino de Aristotelo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_z;&c_a;&c_t;&c_e;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="de">
    <trd>geheim</trd>,
    <trd>esoterisch</trd>
  </trdgrp>
  <trd lng="fr">&eacute;sot&eacute;rique</trd>
  <trdgrp lng="hu">
    <trd>rejtett &eacute;rtelm&udblac;</trd>,
    <trd>titkos &eacute;rtelm&udblac;</trd>,
    <trd>ezoterikus</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_z;&c_o;&c_t;&c_je;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_t;&c_a;&c_j;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_y;&c_t;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="esoter.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Publike al &ccirc;iuj instruata.
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_z;&c_a;&c_t;&c_e;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="fr">exot&eacute;rique</trd>
  <trdgrp lng="hu">
    <trd>k&ouml;z&eacute;rthet&odblac;</trd>,
    <trd>exoterikus</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_k;&c_z;&c_o;&c_t;&c_je;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="esoter.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      Scio instruata nur al la adeptoj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Geheimlehre</trd>,
    <trd>Esoterismus</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>titkos tan</trd>,
    <trd>ezoterizmus</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_z;&c_o;&c_t;&c_je;&c_r;&c_i;&c_z;&c_m;</trd>,
    <trd>&c_t;&c_a;&c_j;&c_n;&c_o;&c_je; &c_u;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
