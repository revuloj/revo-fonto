<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vespert</rad>/oj
</kap>

<drv mrk="vesper1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Iu el <ref tip="malprt" cel="vesper1.0oj"><tld/>oj</ref>.
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd>murci&eacute;lago</trd>,
    <trd>quir&oacute;ptero</trd>
  </trdgrp>
</drv>

<drv mrk="vesper1.0oj">
  <kap><ofc>*</ofc><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="malprt" cel="mam.0uloj">mamuloj</ref>
      (<trd lng="la">Chiroptera</trd>), povantaj flugi:
      <ekz>
        sub la plafono flugis vespertoj
        <fnt><bib>Fab1</bib>, <lok>Galo&scirc;oj de feli&ccirc;o</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;iumomente traflugis super li noktaj birdoj
        kaj grandaj vespertoj
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 20</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_u;&c_k;&c_a;&c_k;&c_r;&c_y;&c_l;&c_y;&c_ja; <klr tip="amb">(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trdgrp lng="ca">
    <trd>ratpenat</trd>,
    <trd>ratapinyada</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Flattertiere</trd>,
    <trd>Flederm&auml;use</trd>
  </trdgrp>
  <trd lng="en">bats</trd>
  <trd lng="es">murci&eacute;lagos<klr tip="amb"> (Orden de los
  quir&oacute;pteros)</klr></trd>
  <trdgrp lng="fr">
    <trd>chauve-souris</trd>
  </trdgrp>
  <trd lng="hu">denev&eacute;rek<klr tip="ind"> (rend)</klr></trd>
  <trd lng="nl">vleermuizen<klr tip="ind"> (orde)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_je;&c_t;&c_u;&c_ch;&c_i;&c_je; &c_m;&c_y;&c_sh;&c_i;<klr tip="ind"> (&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>,
    <trd>&c_r;&c_u;&c_k;&c_o;&c_k;&c_r;&c_y;&c_l;&c_y;&c_je; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  </trdgrp>
</drv>
</art>
