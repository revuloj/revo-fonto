<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vespert</rad>/oj
</kap>
<drv mrk="vesper1.0oj">
  <kap><ofc>*</ofc><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="mam.0uloj">mamuloj</ref>
      (<trd lng="la">Chiroptera</trd>), povantaj flugi:
      <ekz>
        sub la plafono flugis vespertoj
        <fnt>(<aut>Andersen, trad. Zamenhof</aut>: <vrk>Fabeloj,
        Galo&scirc;oj de feli&ccirc;o</vrk>)</fnt>;
      </ekz>
      <ekz>
        &ccirc;iumomente traflugis super li noktaj birdoj
        kaj grandaj vespertoj
        <fnt>(<aut>Prus, trad. Kabe</aut>: <vrk>La Faraono, 1a vol.</vrk>,
        <lok>&ccirc;ap. 20</lok>)</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Flattertiere</trd>,
    <trd>Flederm&auml;use</trd>
  </trdgrp>
  <trd lng="en">bats</trd>
  <trdgrp lng="fr">
    <trd>chauve-souris</trd>
  </trdgrp>
  <trd lng="nl">vleermuizen<klr tip="ind"> (orde)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_je;&c_t;&c_u;&c_ch;&c_i;&c_je; &c_m;&c_y;&c_sh;&c_i;<klr tip="ind"> (&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>,
    <trd>&c_r;&c_u;&c_k;&c_o;&c_k;&c_r;&c_y;&c_l;&c_y;&c_je; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  </trdgrp>
</drv>
</art>
