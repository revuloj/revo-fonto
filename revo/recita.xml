<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>recitativ</rad>/o
</kap>
<drv mrk="recita.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Deklamita duonkanto sen fiksa takto, sekvanta nur la naturan ritmon
      de la parola frazo.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Rezitativ</trd>,
    <trd>Sprechgesang</trd>
  </trdgrp>
  <trd lng="nl">recitatief</trd>
  <trd lng="ru">&c_r;&c_je;&c_ch;&c_i;&c_t;&c_a;&c_t;&c_i;&c_v;</trd>
</drv>
</art>
