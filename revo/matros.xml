<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: matros.xml,v 1.11 2024/01/25 20:10:35 revo Exp $">
<kap>
  <rad>matros</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="matros.0o">
  <kap><tld/>o</kap>
  <snc mrk="matros.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      <ref tip="super" cel="mar.0isto">Maristo</ref>, ferdeka &scirc;iplaboristo de komerca &scirc;ipo:
      <ekz>
        <tld/>o du metrojn alta manipuladis la alteri&gcirc;an ponton
        <fnt>
          <aut>P. Bog&aacute;ti, trad. J. S&aacute;rosi</aut>,
          <vrk><url ref="http://puritane.uw.hu/galanto/galanto.html">
          La vicstiristo de Galanto raportas</url></vrk>,
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum la &scirc;ipestro man&gcirc;is kaj drinkis, la <tld/>o &jcirc;etadis
        oblikvajn rigardojn al li
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/skarlataj.html">
          Skarlataj veloj</url></vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_t;&c_r;&c_o;&c_s;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">mariner</trd>
  <trdgrp lng="cs">
    <trd>matr&oacute;z</trd>,
    <trd>&rcaron;adov&yacute; n&aacute;mo&rcaron;n&iacute;k</trd>
  </trdgrp>
  <trd lng="de">Matrose</trd>
  <trd lng="en">seaman</trd>
  <trd lng="fa">&a_m;&a_l;&a_w;&a_A;&a_n;</trd>
  <trd lng="fr">matelot</trd>
  <trd lng="nl">matroos</trd>
  <trdgrp lng="pl">
    <trd>matros</trd>,
    <trd>marynarz</trd>
  </trdgrp>
  <trd lng="ro">matroz</trd>
  <trdgrp lng="sk">
    <trd>matr&oacute;z</trd>,
    <trd>radov&yacute; n&aacute;morn&iacute;k</trd>
  </trdgrp>
  <trd lng="uk">&c_m;&c_a;&c_t;&c_r;&c_o;&c_s;</trd>
  <trd lng="zh">&#x6C34;&#x624B; <pr>shu&#x1D0;sh&#x1D2;u</pr></trd>
</drv>
</art>

