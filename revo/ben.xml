<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ben.xml,v 1.40 2023/12/12 19:23:56 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ben</rad>/i
  <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="ben.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ben.0i.alvoki">
    <dif>
      <ref tip="super" cel="vok.al0i">Alvoki</ref> dian favoron sur iun a&ubreve; ion:
      <ekz>
        la avo ne volis <tld/>i sian nepon, sed li anka&ubreve; lin ne mal<tld/>is
        <fnt>
          <bib>DL</bib>
          <lok>Dua Libro de l' Lingvo Internacia</lok>
        </fnt>;
      </ekz>
      <ekz>
        la brako kvaza&ubreve; konscie &ccirc;irka&ubreve;prenis &scirc;in <klr>[komprenigante]</klr>: Dio vin
        <tld/>u!
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        Mi pre&gcirc;as // Dio vin savu! // Dio <tld/>u vin, sinjoro!
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni devas <tld/>i la junan paron
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XXVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i &ccirc;irka&ubreve;prenis lin kaj ka&scirc;e, post lia dorso, trifoje <tld/>is lin per la signo
        de la kruco
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        surprizita pro la malavara donaco <klr>[li]</klr> plenigis lian manon per kisoj
        kaj petis Dion <tld/>i la bonan donanton kaj &ccirc;iujn liajn nepojn
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
          <lok>&Ccirc;e en</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="konsek.0i">konsekri</ref>
    </refgrp>
    <ref tip="vid" cel="sankt.0igi">sanktigi</ref>
    <trd lng="zh">&#x4F51; <pr>y&#xF2;u</pr></trd>
</snc>
  <snc mrk="ben.0i.doni">
    <dif>
      <klr>(Dio, destino, naturo kc)</klr> Doni sian favoron:
      <ekz>
        Dio <tld/>is la sepan tagon kaj sanktigis &gcirc;in
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 2:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        Dio <tld/>is <nom>Noa</nom>n kaj liajn filojn, kaj diris al ili: <klr>[&#8230;]</klr> multi&gcirc;u,
        kaj plenigu la teron
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 9:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>ata tempo, kiam la suno staras plej alte
        <fnt>
          <bib>ViV</bib>
        </fnt>;
      </ekz>
      <ekz>
        liaj kolegoj trovi&gcirc;is en tiu <tld/>ata stato, kiu ne estas kompleta dormo, sed
        en kiu oni pla&ccirc;e drivas de unu revo al alia
        <fnt>
          <bib>CKv</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu sian langon katenas, Dio lin <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        Dio donis kaj <tld/>is, sed diablo forprenis
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="favor.0i">favori</ref>
  </snc>
  <snc mrk="ben.0i.danki">
    <dif>
      Respekte <ref tip="super" cel="dank.0i">dank</ref>senti pro io:
      <ekz>
        la posteuloj <tld/>os nian memoron
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        La nepoj vin <tld/>os, // Se vi pacience eltenos
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i obstine sie&gcirc;is mian pordon: kaj mi <tld/>as &scirc;in pro &scirc;ia persisteco
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is la ventegon, kiu, difektante lian &scirc;ipon <klr>[&#8230;]</klr> venkis lian
        voladon
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        vin <tld/>as la bonfaro, ne la gasto
        <fnt>
          <bib>Ifigenio</bib>
          <lok>Ifigenio en Ta&ubreve;rido</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_l;&c_a;&c_s;&c_l;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_zh;&c_a;&c_g;&c_n;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_b;&c_l;&c_a;&c_g;&c_o;&c_s;&c_l;&c_a;&c_v;&c_ja;&c_m;</trd>
  <trd lng="br">benniga&ntilde;</trd>
  <trd lng="cs">&zcaron;ehnat</trd>
  <trd lng="de">segnen</trd>
  <trd lng="en">bless</trd>
  <trd lng="es">bendecir</trd>
  <trd lng="fr">b&eacute;nir</trd>
  <trd lng="he">&lamed;&bet;&resh;&fkaf;</trd>
  <trdgrp lng="hu">
    <trd>&aacute;ld</trd>,
    <trd>meg&aacute;ld</trd>
  </trdgrp>
  <trd lng="io">benedikar</trd>
  <trd lng="ja">&#x795D;&#x798F;&#12377;&#x308B; <pr>&#12375;&#12421;&#x304F;&#12405;&#x304F;&#12377;&#x308B;</pr></trd>
  <trd lng="nl">zegenen</trd>
  <trd lng="pl">b&lstroke;ogos&lstroke;awi&cacute;</trd>
  <trd lng="pt">aben&ccedil;oar</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_s;&c_l;&c_o;&c_v;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_s;&c_l;&c_o;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>&zcaron;ehna&tcaron;</trd>,
    <trd>po&zcaron;ehna&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_b;&#x43B;&c_a;&c_g;&#x43E;&c_s;&#x43B;&#x43E;&c_v;&#x43B;&#x44F;&c_t;&c_i;</trd>
</drv>

<drv mrk="ben.0o">
  <kap><tld/>o</kap>
  <snc mrk="ben.0o.ago">
    <dif>
      Ago a&ubreve; vortoj por <ref tip="sin" cel="ben.0i.alvoki"><tld/>i<sncref/></ref>:
      <ekz>
        via frato venis kun ruzo kaj prenis vian <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 27:35</lok>
        </fnt>;
      </ekz>
      <ekz>
        la gefian&ccirc;oj donis unu al la alia la manon kaj ricevis la <tld/>on de la episkopo
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i a&ubreve;dis la nomojn de siaj karaj <klr>[&#8230;]</klr> en akompano de tiu <tld/>o, kiun
        oni uzas por mortintoj
        <fnt>
          <bib>RBa</bib>
        </fnt>;
      </ekz>
      <ekz>
        okulo de mastro pli ol <tld/>o de pastro
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_b;&c_l;&c_a;&c_g;&c_o;&c_s;&c_l;&c_o;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="ben.0o.favoro">
    <dif>
      <ref tip="super" cel="favor.0o">Favoro</ref> de Dio, destino kc:
      <ekz>
        kie estas harmonio, estas <tld/>o de Dio
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        Kaj <tld/>u min, se vi ankora&ubreve; mem // Deziras havi <tld/>on de l&#8217; &ccirc;ielo
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i elrigardis kiel an&gcirc;elo, kiu <tld/>on portante vizitis la teron
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ilia amo estis akceptita per la oficiala <tld/>o
        <klr>(<refgrp tip="vid"><ref cel="aprob.0o">aprobo</ref>,
        <ref cel="ten.sub0o">subteno</ref></refgrp>)</klr>
        de la banko
        <fnt>
          <bib>KKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi sentas la deziron <klr>[&#8230;]</klr> turni min al iu plej alta forto kaj alvoki
        &gcirc;ian helpon kaj <tld/>on
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista en Boulogne sur Mer en la 5a de a&ubreve;gusto
          1905</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x60E0; <pr>hu&#xEC;</pr></trd>
</snc>
  <snc mrk="ben.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Io, kio efikas favora, prospera:
      <ekz>
        estus efektiva <tld/>o por mia popolo <klr>[&#8230;]</klr>, se mi lin elektus por esti
        mia edzo!
        <fnt>
          <bib>DL</bib>
          <lok>Dua Libro de l' Lingvo Internacia</lok>
        </fnt>;
      </ekz>
      <ekz>
        fruktoportado estas <tld/>o
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        Sara vartadis, flegadis, laboradis, milda kaj pia, <tld/>o en la malri&ccirc;a domo
        <fnt>
          <bib>Fab3</bib>
          <lok>Juda knabino</lok>
        </fnt>;
      </ekz>
      <ekz>
        en tiuj larmoj estis ia mirinda <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Koboldo &ccirc;e la butikisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        pluvu sur vin &ccirc;iuj <tld/>oj: ri&ccirc;a&jcirc;oj, potenco kaj sa&gcirc;o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>!
      </ekz>
      <ekz>
        ordo estas <tld/>o por &ccirc;iu entrepreno
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="favor.0o">favoro</ref>,
      <ref cel="prospe1.0o">prospero</ref>
    </refgrp>
    <trd lng="ru">&c_b;&c_l;&c_a;&c_g;&c_o;</trd>
    <trdgrp lng="zh">
    <trd>&#x793D; <pr>r&#xE9;ng</pr></trd>,
    <trd>&#x798F;&#20998; <pr>f&#xFA;f&#275;n</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_b;&c_l;&c_a;&c_s;&c_l;&c_a;&c_v;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="bg">&c_b;&c_l;&c_a;&c_g;&c_o;&c_s;&c_l;&c_o;&c_v;&c_i;&c_ja;</trd>
  <trd lng="bo">&t_ba;&t_yata;&t_gigu;&t_na;&t_tsheg;&t_ba;&t_ra;&t_lata;&t_ba;&t_sa;&t_tsheg;</trd>
  <trd lng="br">bennozh</trd>
  <trdgrp lng="cs">
    <trd>bene</trd>,
    <trd>ud&ecaron;lat si bene</trd>
  </trdgrp>
  <trd lng="de">Segen</trd>
  <trd lng="en">blessing</trd>
  <trd lng="es">bendici&oacute;n</trd>
  <trd lng="fr">b&eacute;n&eacute;diction</trd>
  <trd lng="he">&bet;&resh;&kaf;&he;</trd>
  <trd lng="hu">&aacute;ld&aacute;s</trd>
  <trd lng="io">benediko</trd>
  <trdgrp lng="ja">
    <trd>&#x795D;&#x798F; <pr>&#12375;&#12421;&#x304F;&#12405;&#x304F;</pr></trd>,
    <trd>&#x795E;&#x306E;&#24693;&#x307F; <pr>&#x304B;&#x307F;&#x306E;&#12417;&#12368;&#x307F;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zegen</trd>,
    <trd>zegening</trd>
  </trdgrp>
  <trd lng="pl">b&lstroke;ogos&lstroke;awie&nacute;stwo</trd>
  <trd lng="sk">po&zcaron;ehnanie</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&#x43B;&c_a;&c_g;&#x43E;&c_s;&#x43B;&#x43E;&c_v;&c_je;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_b;&#x43B;&c_a;&c_g;&#x43E;</trd>
  </trdgrp>
</drv>

<drv mrk="ben.for0i">
  <kap>for<tld/>i</kap>
  <snc>
    <dif>
      Forigi per <tld/>o:
      <ekz>
        voku do la konfesprenanton, ke li for<tld/>u al mi miajn pekojn
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="absolv.0i">absolvi</ref>
  <trd lng="be">&c_d;&c_a;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_g;&c_r;&c_a;&c_h;&c_ib;)</klr></trd>
  <trd lng="cs">za&zcaron;ehnat</trd>
  <trdgrp lng="de">
    <trd>lossprechen</trd>,
    <trd><ind>vergeben</ind> machen</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>absolver</trd>,
    <trd>perdonar</trd>
  </trdgrp>
  <trd lng="fr">absoudre</trd>
  <trd lng="nl">vergeven</trd>
  <trd lng="ru">&c_o;&c_t;&c_p;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol;<klr tip="amb">
  (&c_g;&c_r;&c_je;&c_h;&c_i;)</klr></trd>
  <trd lng="sk">za&zcaron;ehna&tcaron;</trd>
  <trd lng="uk">&c_v;&c_ib;&c_d;&c_p;&c_u;&c_s;&c_k;&c_a;&c_t;&c_i; &c_g;&c_r;&c_ib;&c_h;&c_i;
  &c_b;&c_l;&c_a;&c_g;&c_o;&c_s;&c_l;&c_o;&c_v;&c_je;&c_n;&c_n;&c_ja;&c_m;</trd>
</drv>
<drv mrk="ben.mal0i">
  <kap>mal<tld/>i</kap>
  <snc mrk="ben.mal0i.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      <ref tip="super" cel="vok.al0i">Alvoki</ref> la dian malfavoron sur:
      <ekz>
        li sentis sin tiel malfeli&ccirc;a, ke li mal<tld/>is la tagon, en kiu li estis naskita
        <fnt>
          <bib>F</bib>
          <lok>&FE; 33</lok>
        </fnt>;
      </ekz>
      <ekz>
        per &ccirc;iuj fortoj de mia animo mi mal<tld/>as tiun &ccirc;i teruran momenton!
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li bruas kaj mal<tld/>as <klr>(<ref tip="sin" cel="damn.0i">damnas</ref>)</klr>
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista en Washington en la 15a de a&ubreve;gusto
          1910</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la malbona virino la&ubreve;te mal<tld/>is kaj tiel ektimis, ke &scirc;i ne povis trankvili&gcirc;i
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi &jcirc;uras <klr>[&#8230;]</klr>, ke vi &ccirc;iam min amos, ke neniam vi min malbenos
        <fnt>
          <bib>KPr</bib>
        </fnt>?
      </ekz>
      <ekz>
        <klr>[li]</klr> mal<tld/>as sian patron
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 21:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne mal<tld/>u surdulon
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 19:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu langon ne tenas, mem sin mal<tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="anatem.0i">anatemi</ref>,
      <ref cel="ekskom.0i">ekskomuniki</ref>,
      <ref cel="sakr1.0i">sakri</ref>
    </refgrp>
    <trd lng="zh">&#x8A5B; <pr>z&#x1D4;</pr></trd>
</snc>
  <snc mrk="ben.mal0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Mal<ref tip="ant" cel="favor.0i">favori</ref>:
      <ekz>
        &Scirc;i malalti&gcirc;is al pekulo, kiun // Per &ccirc;io la naturo ja mal<tld/>is!
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tiu]</klr> pekulo estos mal<tld/>ata per a&gcirc;o centjara
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 65:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        alproksimi&gcirc;is la <tld/>ata-mal<tld/>ata tempo, kiam, kiel &ccirc;iam, el ferio fari&gcirc;is
        foriro
        <fnt>
          <bib>LOdE</bib>
          <lok>2003,  &#8470; 8-9 (106-107)</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x8A48; <pr>l&#xEC;</pr></trd>,
    <trd>&#x8A5B;&#21650; <pr>z&#x1D4;zh&#xF2;u</pr></trd>,
    <trd>&#x88AB;&#x8A5B;&#21650; <pr>b&#xE8;iz&#x1D4;zh&#xF2;u</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_l;&c_ja;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_k;&c_l;&c_ib;&c_n;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_k;&c_l;&c_ja;&c_s;&c_mol;&c_c;&c_ib;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_r;&c_o;&c_k;&c_l;&c_i;&c_n;&c_a;&c_m;</trd>
  <trdgrp lng="br">
    <trd>milliga&ntilde;</trd>,
    <trd>mallozhi&ntilde;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kl&iacute;t</trd>,
    <trd>prokl&iacute;nat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verdammen</trd>,
    <trd>verfluchen</trd>
  </trdgrp>
  <trd lng="en">curse<klr tip="ind"> (verb)</klr></trd>
  <trd lng="es">maldecir</trd>
  <trd lng="fr">maudire</trd>
  <trd lng="hu">meg&aacute;tkoz</trd>
  <trdgrp lng="io">
    <trd>maledikar</trd>,
    <trd>imprekar</trd>
  </trdgrp>
  <trd lng="ja">&#x546A;&#12358; <pr>&#x306E;&#x308D;&#12358;</pr></trd>
  <trd lng="nl">vervloeken</trd>
  <trd lng="pl">przeklina&cacute;<klr tip="amb"> (rzuca&cacute; kl&aogonek;tw&eogonek;)</klr></trd>
  <trdgrp lng="ru">
