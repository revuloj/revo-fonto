<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: makada.xml,v 1.25 2024/01/18 09:22:51 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>makadam</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="makada.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="makada.0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      Strat<ref tip="super" cel="teg.0ajxo.tego">tega&jcirc;o</ref> el pluraj tavoloj de diversgrandaj kunpremitaj &scirc;tonrompa&jcirc;oj
      la&ubreve; invento de skoto <nom>John Loudon McAdam</nom>:
      <ekz>
        sur la vojo <klr>[&#8230;]</klr> ankora&ubreve; &ccirc;iam ku&scirc;as la sama griza <tld/>o, sur kiu
        a&ubreve;tunon post a&ubreve;tuno veturadis la farmejaj veturiloj
        <fnt>
          <bib>IdO</bib>
          <lok>Kvina Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li saltas trans la foson sur la <tld/>an vojon
        <fnt>
          <bib>IdO</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="asfalt.0o">asfalto</ref>,
      <ref cel="balast.0o.FER">balasto</ref>,
      <ref cel="bitum.0o">bitumo</ref>,
      <ref cel="gruz.0o">gruzo</ref>,
      <ref cel="pavim.0o">pavimo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_k;&c_a;&c_d;&c_a;&c_m;</trd>
  <trd lng="ca">macadam</trd>
  <trdgrp lng="de">
    <trd>Makadampflaster</trd>,
    <trd>Kieselschlag</trd>,
    <trd>Schotter <klr>(Makadam)</klr></trd>
  </trdgrp>
  <trd lng="en">macadam</trd>
  <trd lng="fa">&a_m;&a_A;&f_k;&a_A;&a_d;&a_A;&a_m;</trd>
  <trd lng="fr">macadam</trd>
  <trd lng="hu">makad&aacute;m</trd>
  <trd lng="ja">&#12510;&#12459;&#12480;&#12512;</trd>
  <trd lng="nl">macadam</trd>
  <trd lng="pl">makadam</trd>
  <trd lng="pt">macadame</trd>
  <trd lng="ro">macadam</trd>
  <trd lng="ru">&c_m;&c_a;&c_k;&c_a;&c_d;&c_a;&c_m;</trd>
  <trd lng="sv">makadam</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_a;&c_k;&c_a;&c_d;&c_a;&c_m;</trd>,
    <trd>&c_shch;&c_je;&c_b;&c_je;&c_n;&c_je;&c_v;&c_je; &c_p;&c_o;&c_k;&c_r;&c_i;&c_t;&c_t;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="makada.0i">
  <kap><tld/>i</kap>
  <snc mrk="makada.0i.TRA">
    <dif>
      Tegi per <tld/>o:
      <ekz>
        poezio ne povas savi la mondon, sed eble <tld/>i ies vojon al
        pli
        bona ekzisto
        <fnt>
        <aut>Poul Thorsen</aut>,
        <vrk>Babile kun mi mem. - Beletra: 1996</vrk>,
        <lok><url ref="http://www.poulthorsen.dk/finbabile.htm">Finbabile?</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_k;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol; <ind>&c_m;&c_a;&c_k;&c_a;&c_d;&c_a;&c_m;&c_a;&c_m;</ind></trd>
  <trd lng="ca">macadamitzar</trd>
  <trd lng="cs">pokr&yacute;t makadamem</trd>
  <trdgrp lng="de">
    <trd>makadamisieren</trd>,
    <trd>schottern <klr>(makadamisieren)</klr></trd>
  </trdgrp>
  <trd lng="fa">&a_m;&a_A;&f_k;&a_A;&a_d;&a_A;&a_m; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">macadamiser</trd>
  <trd lng="ja">&#12510;&#12459;&#12480;&#12512;&#12391;&#33303;&#35013;&#12377;&#12427;  <pr>&#12510;&#12459;&#12480;&#12512;&#12391;&#12411;&#12381;&#12358;&#12377;&#12427;</pr></trd>
  <trd lng="nl">macadamiseren</trd>
  <trd lng="pl">pokry&cacute; <ind>makadamem</ind></trd>
  <trd lng="sk">pokry&tcaron; makadamom</trd>
</drv>
</art>
