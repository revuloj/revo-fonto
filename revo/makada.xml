<art mrk="$Id: makada.xml,v 1.21 2022/10/06 06:49:24 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>makadam</rad>/o
</kap>
<drv mrk="makada.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="makada.0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      Tavolo el kunpremitaj &scirc;tonrompa&jcirc;oj, per kiu oni kovras la vojojn
    </dif>
    <refgrp tip="vid">
      <ref cel="asfalt.0o">asfalto</ref>,
      <ref cel="bitum.0o">bitumo</ref>,
      <ref cel="pavim.0o">pavimo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_k;&c_a;&c_d;&c_a;&c_m;</trd>
  <trd lng="ca">macadam</trd>
  <trd lng="de">Makadam</trd>
  <trd lng="en">macadam</trd>
  <trd lng="fa">&a_m;&a_A;&f_k;&a_A;&a_d;&a_A;&a_m;</trd>
  <trd lng="fr">macadam</trd>
  <trd lng="hu">makad&aacute;m</trd>
  <trd lng="nl">macadam</trd>
  <trd lng="pl">makadam</trd>
  <trd lng="pt">macadame</trd>
  <trd lng="ro">macadam</trd>
  <trd lng="ru">&c_m;&c_a;&c_k;&c_a;&c_d;&c_a;&c_m;</trd>
  <trd lng="sv">makadam</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_a;&#x43A;&c_a;&c_d;&c_a;&#x43C;</trd>,
    <trd>&c_shch;&c_je;&c_b;&c_je;&#x43D;&c_je;&c_v;&c_je; &#x43F;&#x43E;&#x43A;&c_r;&c_i;&c_t;&c_t;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="makada.0i">
  <kap><tld/>i</kap>
  <snc mrk="makada.0i.TRA">
    <dif>
      Kovri per <tld/>o:
      <ekz>
        poezio ne povas savi la mondon, sed eble <tld/>i ies vojon al
        pli
        bona ekzisto
        <fnt>
        <aut>Poul Thorsen</aut>,
        <vrk>Babile kun mi mem. - Beletra: 1996</vrk>,
        <lok><url ref="http://www.poulthorsen.dk/finbabile.htm">Finbabile?</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_k;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol; <ind>&c_m;&c_a;&c_k;&c_a;&c_d;&c_a;&c_m;&c_a;&c_m;</ind></trd>
  <trd lng="ca">macadamitzar</trd>
  <trd lng="cs">pokr&yacute;t makadamem</trd>
  <trd lng="fa">&a_m;&a_A;&f_k;&a_A;&a_d;&a_A;&a_m; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">macadamiser</trd>
  <trd lng="nl">macadamiseren</trd>
  <trd lng="pl">pokry&cacute; <ind>makadamem</ind></trd>
  <trd lng="sk">pokry&tcaron; makadamom</trd>
</drv>
</art>
