<drv mrk="profun.en0igxi">
  <kap>en<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="profun.en0igxi.KOMUNE">
    <dif>
      Iri <tld/>e en ion:
      <ekz>
        viaj piedoj en<tld/>i&gcirc;is en koton
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 38:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ne kura&gcirc;is pli en<tld/>i&gcirc;i en la straton, nek peti de iu klarigon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        liaj okuloj kvaza&ubreve; kovris sin per &scirc;irma vualo kaj en<tld/>i&gcirc;is en siaj kavernoj
        <fnt>
          <bib>HsT</bib>
          <lok>Heredanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la virino en<tld/>i&gcirc;is en tiajn demandojn, esplorante &ccirc;iun eblan motivon
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dron.0i">droni</ref>,
      <ref cel="kav.en0igxi">enkavi&gcirc;i</ref>
    </refgrp>
  </snc>
  <snc mrk="profun.en0igxi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="vid" cel="absorb.0igxi.PSI">Absorbi&gcirc;i</ref>:
      <ekz>
        <ind>en<tld/>i&gcirc;i en pensojn</ind>
        <fnt>
          <bib>IdO</bib>
          <lok>Dua Parto</lok>
        </fnt>;
        <trd lng="de">in Gedanken versinken</trd>
        <trd lng="hu">gondolataiba m&eacute;lyed</trd>
      </ekz>
      <ekz>
        &scirc;i klinis malalte la kapon, <klr>[&#x2026;]</klr> kaj &scirc;i en<tld/>i&gcirc;is en sian laboron
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi legis la libron <klr>[&#x2026;]</klr> kaj tiel en<tld/>i&gcirc;is en &gcirc;in,
        ke mi tute forgesis mian ordinaran plezuron de la Novjara nokto
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pens.en0igxi">enpensi&gcirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_g;&c_l;&c_y;&c_b;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
