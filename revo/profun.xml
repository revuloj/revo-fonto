  </trdgrp>
  <trd lng="pl">zag&lstroke;&eogonek;bia&cacute; si&eogonek;</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_g;&c_l;&c_u;&c_b;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_u;&c_g;&c_l;&c_u;&c_b;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_g;&c_l;&c_i;&c_b;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_l;&c_i;&c_b;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
