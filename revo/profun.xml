    <trd>&#22887;  [<ind>&#x304A;&#x304F;</ind>]</trd>,
    <trd>&#22885;&#x5E95;  [<ind>&#x304A;&#x304F;&#x305D;&#12371;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">diepte</trd>
  <trdgrp lng="pl">
    <trd>g&lstroke;&eogonek;boko&sacute;&cacute;</trd>,
    <trd>drugi plan</trd>
  </trdgrp>
