<drv mrk="profun.en0igxi">
  <kap>en<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="profun.en0igxi.KOMUNE">
    <dif>
      Iri <tld/>e en ion:
      <ekz>
        mi <tld/>i&gcirc;is en <tld/>an mar&ccirc;on, kaj mi ne havas, sur kio stari
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 69:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        viaj piedoj en<tld/>i&gcirc;is en koton
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 38:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        li &jcirc;etis en la akvon la fi&scirc;on, kiu <tld/>i&gcirc;is &gcirc;is la fundo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fi&scirc;kaptisto kaj lia edzino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>en<tld/>i&gcirc;intaj okuloj</ind>
        <trd lng="hu">beesett szemek</trd>
      </ekz>
      <ekz>
        ne estas tie &ccirc;i la loko por en<tld/>i&gcirc;i en detalojn teknikajn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dron.0i">droni</ref>,
      <ref cel="kav.en0igxi">enkavi&gcirc;i</ref>
    </refgrp>
  </snc>
  <snc mrk="profun.en0igxi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="vid" cel="absorb.0igxi.PSI">Absorbi&gcirc;i</ref>:
      <ekz>
        en<tld/>i&gcirc;inta en la rigardado;
      </ekz>
      <ekz>
        mi legis la libron <klr>[&#8230;]</klr> kaj tiel en<tld/>i&gcirc;is en &gcirc;in,
       ke mi tute forgesis mian ordinaran plezuron de la Novjara nokto
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
      <ekz>
        li staris en<tld/>i&gcirc;inte en dol&ccirc;aj pensoj
        <fnt>
          <bib>Fab4</bib>
          <lok>Ne&gcirc;ulo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pens.en0igxi">enpensi&gcirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_g;&c_l;&c_y;&c_b;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
