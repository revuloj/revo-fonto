<drv mrk="profun.0o">
  <kap><tld/>o</kap>
  <snc mrk="profun.0o.dimensio">
    <dif>
      <ref tip="super" cel="dimens.0o.ampleksa">Dimensio</ref>
      de objekto, mezurata la&ubreve; la malproksimeco for de
      la supra&jcirc;o a&ubreve; aperturo, ekzemple de la pli anta&ubreve;a
      parto al la pli malanta&ubreve;a, a&ubreve; de la pli alta
      parto al la pli malalta (rigardate de supre):
      <ekz>
        &gcirc;ia <tld/>o <klr>[de tiu valo]</klr> estas kelkcent
        metroj, longo 130 mejloj, meza lar&gcirc;o apena&ubreve; unu mejlo
        <fnt><bib>Far1</bib>, <lok>anta&ubreve;parolo</lok></fnt>;
      </ekz>
      <ekz>
        je profundo de 30-40 kilometroj sub la suda poluso de Encelado trovi&gcirc;as maso da likva akvo
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011292.php">Oceano, 
          kaj eble vivo, sur saturna satelito</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
         la tubo estis ka&scirc;ita je <tld/>o de &ccirc;. 10 cm
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2005/008524.php">Jen la tub-ero</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">profondeur<klr tip="ind"> (dimension)</klr></trd>
  </snc>
  <snc mrk="profun.0o.profundajxo">
    <dif>
      <tld lit="P"/>a loko; parto plej malproksima de la supra&jcirc;o a&ubreve;
      aperturo:
      <ekz>
        en la <tld/>o de la scenejo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        pordo kondukanta en la <tld/>on de la domo
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        la <tld/>o de la &ccirc;ambro estis preska&ubreve; tute malluma
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        granda &scirc;ipo bezonos <tld/>on
        <klr>(samsignifa kiel: fi&scirc;o granda na&gcirc;as <tld/>e)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;i ekparolis vortojn, venantajn el la <tld/>o de la koro
        <fnt><bib>Fab4</bib>, <lok>infano en la tombo</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>fond</trd>,
      <trd>profondeur<klr tip="ind"> (abime)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_g;&c_l;&c_y;&c_b;&c_ib;&c_n;&c_ja;</trd>
  <trd lng="bo">&#xF42;&#xFB2;&#xF72;&#xF44;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>hlubina</trd>,
    <trd>t&uring;&ncaron;</trd>
  </trdgrp>
  <trd lng="de">Tiefe</trd>
  <trd lng="hu">m&eacute;lys&eacute;g</trd>
  <trd lng="nl">diepte</trd>
  <trd lng="pt">pofundeza</trd>
  <trd lng="ru">&c_g;&c_l;&c_u;&c_b;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">hlbina</trd>
  <trd lng="sv">djup</trd>
</drv>

<drv mrk="profun.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Eco de tio, kio estas <tld/>a:
      <ekz>
        mi ne kapablas penetri la <tld/>econ de liaj pensoj;
      </ekz>
      <ekz>
        maljunuloj kaj maljunulinoj <klr>[&#x2026;]</klr> &ccirc;iu kun bastono en la mano 
        pro <tld/>eco de la a&gcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Ze&hcirc;arja 8:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        grandaj humidaj okuloj fiksrigardis min kaj nesenteble mi dronis en ilia <tld/>eco
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007140.html">de Julian
          Modest</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        grumblado de tia <tld/>eco, ke oni &gcirc;in sentis pli forte ol a&ubreve;dis
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Goabo, la dezerto de la Koloroj</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Tiefe</trd>
    <trd lng="fr">profondeur<klr tip="ind"> (fait d'&ecirc;tre
    profond)</klr></trd>
    <trd lng="sv">djup</trd>
  </snc>
  <snc>
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="dif" cel="profun.0o.dimensio"><tld lit="P"/>o
      <sncref ref="profun.0o.dimensio"/></ref>:
      <ekz>
        kompreni, kio estas la lar&gcirc;eco kaj longeco kaj alteco kaj <tld/>eco
        <fnt>
          <bib>NT</bib>
          <lok>Efesanoj 3:18</lok>
        </fnt>.
      </ekz>
      <ekz>
        ne konante la <tld/>econ ne iru en la riveron
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        el la <tld/>eco de la maro
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 68:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en la <tld/>eco de la koro
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 64:6</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_l;&c_y;&c_b;&c_ib;&c_n;&c_ja;</trd>
  <trd lng="cs">hloubka</trd>
  <trd lng="de">Tiefe</trd>
  <trd lng="hu">m&eacute;lys&eacute;g <klr tip="ind">(tulajdons&aacute;g)</klr></trd>
  <trd lng="nl">diepte</trd>
  <trd lng="ru">&c_g;&c_l;&c_u;&c_b;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">h&lacute;bka</trd>
</drv>

<drv mrk="profun.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="profun.0ego.KOMUNE">
    <dif>
      Tre granda <ref tip="super" cel="kav.0o">kavo</ref>, tre <tld/>a
      loko:
      <ekz>
        vi &jcirc;etis min en la <tld/>egon, en la internon de la maro
        <fnt>
          <bib>MT</bib>
          <lok>Jona 2:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        la akvo &scirc;a&ubreve;me batadis, turni&gcirc;ante kiel bruantaj radoj muelaj,
        kaj &ccirc;ion, kion &gcirc;i kaptadis, tiregadis kun si en la <tld/>egon
        <fnt><bib>Fab1</bib>, <lok>la virineto de maro</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_je;&c_z;&c_d;&c_a;&c_n;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_r;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>abime</trd>,
    <trd>ab&icirc;me</trd>,
    <trd>gouffre</trd>
  </trdgrp>
  <trd lng="hu">szakad&eacute;k</trd>
  <trd lng="nl">afgrond</trd>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_d;&c_n;&c_a;</trd>
  <trd lng="sv">avgrund</trd>
</drv>

<drv mrk="profun.0egajxo">
  <kap><tld/>ega&jcirc;o</kap>
  <snc mrk="profun.0egajxo.KOMUNE">
    <dif>
      Io tre <tld/>a, <ref tip="dif" cel="profun.0ego"><tld/>ego</ref>:
      <ekz>
        li sidi&gcirc;is <klr>[&#x2026;]</klr> kaj sentime deglitis en la <tld/>ega&jcirc;on
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 20a</lok></fnt>;
      </ekz>
      <ekz>
        li havis impreson, ke li estas en <tld/>ega&jcirc;o kaj ke Labirinto premegas lin per sia pezo
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_je;&c_z;&c_d;&c_a;&c_n;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_r;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Abgrund</trd>
  <trdgrp lng="fr">
    <trd>abime</trd>,
    <trd>ab&icirc;me</trd>,
    <trd>gouffre</trd>
  </trdgrp>
  <trd lng="hu">szakad&eacute;k</trd>
  <trd lng="nl">afgrond</trd>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_d;&c_n;&c_a;</trd>
  <trd lng="sv">avgrund</trd>
</drv>

<drv mrk="profun.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="profun.0igi.KOMUNE">
    <dif>
      Igi <tld/>a, vasta &gcirc;is la fundo, radikala:
      <ekz>
        li fosis kavon kaj <tld/>igis &gcirc;in
        <fnt><bib>MT</bib>, <lok>&Psa; 7:15</lok></fnt>;
      </ekz>
      <ekz>
        per premado ili <tld/>igis la pekojn
        <fnt><bib>MT</bib>, <lok>&Hos; 5:2</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li sidis <klr>[&#x2026;]</klr> <tld/>igita en malgajan meditadon
        <fnt>
          <bib>BdV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fos.0i">fosi</ref>
    </refgrp>
  </snc>
  <trd lng="cs">prohloubit</trd>
  <trd lng="de">vertiefen</trd>
  <trd lng="fr">approfondir</trd>
  <trd lng="ru">&c_u;&c_g;&c_l;&c_u;&c_b;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">vyh&lacute;bi&tcaron;</trd>
</drv>

<drv mrk="profun.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="profun.0igxi.KOMUNE">
    <dif>
      I&gcirc;i pli <tld/>a, atingi pli <tld/>an lokon:
      <ekz>
        la kadavro <tld/>i&gcirc;is iom post iom en la akvon.
      </ekz>
    </dif>
  </snc>
  <snc mrk="profun.0igxi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Atingi pli bazan, pli fundamentan nivelon:
      <ekz>
        <ind><tld/>i&gcirc;i en siaj pensoj</ind><fnt>Z</fnt>
        <trd lng="hu">gondolataiba m&eacute;lyed</trd>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_g;&c_l;&c_y;&c_b;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
  <trd lng="de">sich vertiefen</trd>
  <trd lng="fr">s'<ind>enfoncer</ind></trd>
  <trdgrp lng="hu">
    <trd>elm&eacute;ly&uuml;l</trd>,
    <trd>elmer&uuml;l</trd>,
    <trd>belemer&uuml;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>wegzinken</trd>,
    <trd>verzinken</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_g;&c_l;&c_u;&c_b;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_u;&c_g;&c_l;&c_u;&c_b;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="profun.en0igxi">
  <kap>en<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="profun.en0igxi.KOMUNE">
    <dif>
      Iri <tld/>e en ion:
      <ekz>
        mi <tld/>i&gcirc;is en <tld/>an mar&ccirc;on, kaj mi ne havas, sur kio stari
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 69:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        viaj piedoj en<tld/>i&gcirc;is en koton
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 38:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        li &jcirc;etis en la akvon la fi&scirc;on, kiu <tld/>i&gcirc;is &gcirc;is la fundo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fi&scirc;kaptisto kaj lia edzino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>en<tld/>i&gcirc;intaj okuloj</ind>
        <trd lng="hu">beesett szemek</trd>
      </ekz>
      <ekz>
        ne estas tie &ccirc;i la loko por en<tld/>i&gcirc;i en detalojn teknikajn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dron.0i">droni</ref>,
      <ref cel="kav.en0igxi">enkavi&gcirc;i</ref>
    </refgrp>
  </snc>
  <snc mrk="profun.en0igxi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="vid" cel="absorb.0igxi.PSI">Absorbi&gcirc;i</ref>:
      <ekz>
        en<tld/>i&gcirc;inta en la rigardado;
      </ekz>
      <ekz>
        mi legis la libron <klr>[&#x2026;]</klr> kaj tiel en<tld/>i&gcirc;is en &gcirc;in,
       ke mi tute forgesis mian ordinaran plezuron de la Novjara nokto
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
      <ekz>
        li staris en<tld/>i&gcirc;inte en dol&ccirc;aj pensoj
        <fnt>
          <bib>Fab4</bib>
          <lok>Ne&gcirc;ulo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pens.en0igxi">enpensi&gcirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_g;&c_l;&c_y;&c_b;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
  <trd lng="de">sich <ind>vertiefen</ind></trd>
  <trdgrp lng="fr">
    <trd>s'<ind>engouffrer</ind></trd>,
    <trd>s'<ind>enfoncer</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>elm&eacute;ly&uuml;l</trd>,
    <trd>elmer&uuml;l</trd>,
    <trd>belemer&uuml;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>wegzinken</trd>,
    <trd>verzinken</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_g;&c_l;&c_u;&c_b;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="profun.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="profun.mal0a.KOMUNE">
    <dif>
      Havanta malgrandan <tld/>econ:
      <ekz>
        la fluejo de la rivero estas mal<tld/>a
        <fnt>
          <bib>Fab4</bib>
          <lok>Peco da perlovico</lok>
        </fnt>;
      </ekz>
      <ekz>
        apude estis ronda, mal<tld/>a golfeto
        <fnt><bib>InfanTorent2</bib>, <lok>unua parto, &ccirc;apitro
        1a</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        eble <klr>[miaj vortoj]</klr> &scirc;ajnis al vi tro mal<tld/>aj
        <fnt><bib>Fab2</bib>, <lok>dio de dormo, merkredo</lok></fnt>?
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frivol.0a">frivola</ref>,
      <ref cel="plat.0a">plata</ref>,
      <ref cel="supr.0ajxa">supra&jcirc;a</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_je;&c_l;&c_k;&c_ib;</trd>,
    <trd>&c_n;&c_je;&c_g;&c_l;&c_y;&c_b;&c_o;&c_k;&c_ib;</trd>,
    <trd>&c_p;&c_l;&c_y;&c_t;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="cs">m&ecaron;lk&yacute;</trd>
  <trdgrp lng="de">
    <trd>flach</trd>,
    <trd>seicht</trd>,
    <trd>oberfl&auml;chlich</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>peu <ind>profond</ind></trd>,
    <trd>superficiel</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sek&eacute;ly</trd>,
    <trd>felsz&iacute;nes</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ondiep</trd>,
    <trd>oppervlakkig</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_l;&c_k;&c_i;&c_j;<klr tip="ind"> (&c_n;&c_je;&c_g;&c_l;&c_u;&c_b;&c_o;&c_k;&c_i;&c_j;)</klr></trd>,
    <trd>&c_n;&c_je;&c_g;&c_l;&c_u;&c_b;&c_o;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_v;&c_je;&c_r;&c_h;&c_n;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">plytk&yacute;</trd>
  <trd lng="sv">grund</trd>
</drv>

<drv mrk="profun.mal0ajxo">
  <kap>mal<tld/>a&jcirc;o</kap>
  <snc mrk="profun.mal0ajxo.loko">
    <dif>
      Loko mal<tld/>a:
      <ekz>
        <uzo tip="fak">MAR</uzo>
        ili <klr>[&#x2026;]</klr> remis al la mezo de la lago, kie trovi&gcirc;is
        mal<tld/>a&jcirc;o
        <fnt><bib>InfanTorent2</bib>, <lok>dua parto, &ccirc;apitro
        1a</lok></fnt>;
      </ekz>
      <ekz>
        bufo <klr>[&#x2026;]</klr> post estado en seka loko, sidas en mal<tld/>a&jcirc;o por reakvi&gcirc;i
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=42664">Anuroj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_je;&c_l;&c_mol;</trd>
    <trd lng="de">Untiefe</trd>
    <trd lng="fr">haut-fond</trd>
    <trd lng="ru">&c_m;&c_je;&c_l;&c_mol;</trd>
  </snc>
</drv>
