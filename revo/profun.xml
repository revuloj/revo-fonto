<drv mrk="profun.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="profun.0igxi.KOMUNE">
    <dif>
      I&gcirc;i pli <tld/>a, atingi pli <tld/>an lokon:
      <ekz>
        la remiloj <tld/>i&gcirc;is, la &scirc;ipo turni&gcirc;is sur la loko
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fi&scirc;hoko <tld/>i&gcirc;is &gcirc;is la fundo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fi&scirc;kaptisto kaj lia edzino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sulko inter liaj brovoj <tld/>i&gcirc;is, la viza&gcirc;o &scirc;ajnis laca kaj dolora
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>i&gcirc;is en <tld/>an mar&ccirc;on, kaj mi ne havas, sur kio stari
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 69:2</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="profun.0igxi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Atingi pli bazan, pli fundamentan nivelon:
      <ekz>
        <tld/>i&gcirc;i en revado
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la dia Amon <klr>[&#x2026;]</klr> pli kaj pli <tld/>i&gcirc;is en la konsiderado de l' propra
        dieco
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj opinias, ke li forkuris kun amatino, aliaj kontra&ubreve;diras tion
        <klr>[&#x2026;]</klr> kaj ili <tld/>i&gcirc;as en &ccirc;iujn supozojn
        <fnt>
          <bib>IdO</bib>
          <lok>Kvina Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        konati&gcirc;u kun la konstruo de tiu &ccirc;i lingvo, <tld/>i&gcirc;u iom en &gcirc;ian jam tre ri&ccirc;an
        kaj diversaspecan literaturon
        <fnt>
          <bib>EE</bib>
          <lok>V</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Vinokurov</nom> <tld/>i&gcirc;is en la legadon
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_g;&c_l;&c_y;&c_b;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
  <trd lng="de">sich vertiefen</trd>
  <trd lng="fr">s'<ind>enfoncer</ind></trd>
  <trdgrp lng="hu">
    <trd>elm&eacute;ly&uuml;l</trd>,
    <trd>elmer&uuml;l</trd>,
    <trd>belemer&uuml;l</trd>
  </trdgrp>
  <trd lng="id"><baz>dalam</baz>mendalam</trd>
  <trdgrp lng="ja">
    <trd>&#x6DF1;&#x304F;&#x306A;&#x308B;  <pr>&#x3075;&#x304B;&#x304F;&#x306A;&#x308B;</pr></trd>,
    <trd>&#x6DF1;&#x307E;&#x308B;  <pr>&#x3075;&#x304B;&#x307E;&#x308B;</pr></trd>,
    <trd>&#x6DF1;&#x304F;&#x6C88;&#x3080;  <pr>&#x3075;&#x304B;&#x304F;&#x3057;&#x305A;&#x3080;</pr></trd>,
    <trd>&#x6C88;&#x3080;  <pr>&#x3057;&#x305A;&#x3080;</pr></trd>,
    <trd>&#x3075;&#x3051;&#x308B;</trd>,
    <trd>&#x6CA1;&#x982D;&#x3059;&#x308B;  <pr>&#x307C;&#x3063;&#x3068;&#x3046;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>wegzinken</trd>,
    <trd>verzinken</trd>
  </trdgrp>
  <trd lng="pl">zag&lstroke;&eogonek;bia&cacute; si&eogonek;</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_g;&c_l;&c_u;&c_b;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_u;&c_g;&c_l;&c_u;&c_b;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_g;&c_l;&c_i;&c_b;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_l;&c_i;&c_b;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_n;&c_u;&c_r;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_d;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9677;&#x5165; <pr>xi&#xE0;nr&#xF9;</pr></trd>,
    <trd>&#x9677; <pr>xi&#xE0;n</pr></trd>
