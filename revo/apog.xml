<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>apog</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="apog.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="apog.0i.KOMUNE">
    <dif>
      Subteni kaj firmigi objekton pere de alia:
      <ekz>
        <tld/>i trabon &ccirc;e muron;
      </ekz>
      <ekz>
        <tld/>i muron per traboj;
      </ekz>
      <ekz>
        plafono <tld/>ita sur kolonoj
        <fnt><bib>Far3</bib>, <lok>&ccirc;ap. 14</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i la kapon sur la mano
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 6</lok></fnt>;
      </ekz>
      <ekz>
        <ind>sin <tld/>i</ind> al kolono
        <fnt><bib>MT</bib>, <lok>&Jug; 16:29</lok></fnt>;
        <trd lng="be">
        &c_a;&c_b;&c_a;&c_p;&c_ib;&c_r;&c_a;&c_c;&c_c;&c_a;</trd>
        <trd lng="ru">
        &c_o;&c_p;&c_i;&c_r;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
      </ekz>
      <ekz>
        sin <tld/>adi sur ies brako
        <fnt><bib>MT</bib>, <lok>&Re2; 7:17</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>st&uuml;tzen</trd>, <trd>lehnen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>appuyer</trd>,
      <trd>&eacute;tayer</trd>
    </trdgrp>
    <trd lng="pl">podpiera&cacute;</trd>
   <trdgrp lng="it">
      <trd>appoggiare</trd>,
      <trd>addossare</trd>
    </trdgrp>
  </snc>
  <snc mrk="apog.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plia&ubreve;toritatigi:
      <ekz>
        <tld/>i proponon;
      </ekz>
      <ekz>
        peti <tld/>on de iu;
      </ekz>
      <ekz>
        unu kapo facile trovas <tld/>on<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        al via talento mankas la <tld/>o de lernado<fnt>Z</fnt>;
      </ekz>
      <ekz>
        por vendisto mensogo estas necesa
        <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        li <tld/>as siajn postulojn sur la kutimo;
      </ekz>
      <ekz>
        sin <tld/>i sur ies opinio.
      </ekz>
    </dif>
    <trd lng="de">unterst&uuml;tzen</trd>
    <trdgrp lng="fr">
      <trd>appuyer</trd>,
      <trd>soutenir</trd>
    </trdgrp>
    <trd lng="pl">popiera&cacute;</trd>
   <trdgrp lng="it">
      <trd>appoggiare</trd>,
      <trd>supportare</trd>,
      <trd>fiancheggiare</trd>,
      <trd>sostenere</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_a;&c_p;&c_ib;&c_r;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_t;&c_r;&c_y;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">skora&ntilde;</trd>
  <trdgrp lng="en">
    <trd>support</trd>, <trd>uphold</trd>
  </trdgrp>
  <trd lng="es">apoyar</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_p;&c_i;&c_r;&c_a;&c_t;&c_mol;</trd>,
    <trd>
    &c_p;&c_o;&c_d;&c_d;&c_je;&c_r;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;
    </trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>oprieť</trd>,
    <trd>podoprie&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">st&ouml;dja</trd>
</drv>

<drv mrk="apog.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="apog.0igxi.KOMUNE">
    <dif>
      Sin <tld/>i sur iu a&ubreve; sur io:
      <ekz>
        evidentas, ke la internacia lingvo devas <tld/>i&gcirc;i sur
        lingvaj faktoj, t.e. sur internacieco.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>s'<ind>appuyer</ind> sur</trd>,
    <trd>prendre appui sur </trd>
    </trdgrp>
  <trd lng="it">appoggiarsi</trd>
</drv>

<drv mrk="apog.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="apog.0ilo.KOMUNE">
    <dif>
      Tio, kio servas por apogi ion a&ubreve; por sin apogi:
      <ekz>
        <tld/>ilo de kaduka muro.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">skor</trd>
  <trdgrp lng="de">
    <trd>St&uuml;tze</trd>, <trd>Lehne</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>pillar</trd>,
    <trd>buttress</trd>
  </trdgrp>
  <trd lng="es">apoyo</trd>
  <trdgrp lng="fr">
    <trd>appui</trd>,
    <trd>&eacute;tai</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>appoggio</trd>,
    <trd>sostegno</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>oparcie</trd>,
    <trd>podparcie</trd>,
    <trd>podpora</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_p;&c_o;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_p;&c_i;&c_n;&c_k;&c_a;
    <klr>(&c_s;&c_t;&c_u;&c_l;&c_a;,
    &c_k;&c_r;&c_je;&c_s;&c_l;&c_a;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>operadlo</trd>,
    <trd>podpera</trd>
  </trdgrp>
  <trd lng="sv">st&ouml;d</trd>
</drv>

<drv mrk="apog.brak0ilo">
  <kap>brak<tld/>ilo</kap>
  <snc mrk="apog.brak0ilo.MEBL">
    <dif>
      Flanka peco de
      <ref tip="malprt" cel="sid.0ilo.MEBL">sidilo</ref>,
      sur kiu la sidanto povas ku&scirc;igi anta&ubreve;brakon:
      <ekz>
        la fremdulo sidi&gcirc;is sur la fotelo kun mano apoganta 
        la mentonon kaj kubuto sur la brak<tld/>ilo
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>accotoir</trd>,
    <trd>accoudoir</trd>
  </trdgrp>
  <trd lng="it">bracciolo</trd>
  <trd lng="sk">operadlo rúk</trd>
</drv>

<drv mrk="apog.dors0ilo">
  <kap>dors<tld/>ilo</kap>
  <snc mrk="apog.dors0ilo.MEBL">
    <dif>
      Malanta&ubreve;a peco de
      <ref tip="malprt" cel="sid.0ilo.MEBL">sidilo</ref>,
      &ccirc;e kiu la sidanto povas ripozigi la dorson:
      <ekz>
        dors<tld/>ilo de benko.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">dossier<klr> (de si&egrave;ge)</klr></trd>
  <trd lng="sk">zadné operadlo</trd>
</drv>

<drv mrk="apog.mal0i">
  <kap>mal<tld/>i</kap>
  <snc mrk="apog.mal0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ne <ref tip="ant" cel="ten.sub0i.FIG">subteni</ref>,
      <ref tip="vid" cel="aprob.mal0i.KOMUNE">malaprobi</ref>:
      <ekz>
        kiam <klr>[esplorado]</klr> ka&ubreve;zas suferadon de bestoj,
        &gcirc;i des pli mal<tld/>indas
        <fnt>
          <aut>K. Miner</aut>,
          <vrk><url
          ref="http://www.airbits.com/~miner/TAGLIBRO_package/Auxgusto_2008.html">
          Taglibro</url></vrk>,
          <lok>2008-08-26</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref
      cel="kondam.0i.deklari_malprava_riprocxinda">kondamni<sncref/></ref>,
      <ref cel="distan.sin0igi.FIG">sin distancigi</ref>
    </refgrp>
  </snc>
  <trd lng="fr">r&eacute;prouver</trd>
</drv>
</art>
