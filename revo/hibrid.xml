<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hibrid</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="hibrid.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Planto a&ubreve; animalo, devenanta de du malsamaj specoj a&ubreve; rasoj:
      <ekz>
        <tld/>oj estas nur malofte fekundaj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Hybride</trd>,
    <trd>Bastard</trd>
  </trdgrp>
  <trd lng="fr">hybride<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="nl">
    <trd>kruisingsproduct</trd>,
    <trd>bastaard</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_i;&c_b;&c_r;&c_i;&c_d;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_je;&c_s;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="hibrid.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Devenanta de du malsamaj specoj, de du rasoj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>hybrid</trd>,
    <trd>zwitterhaft</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>hybride<klr tip="ind"> (adj.)</klr></trd>,
    <trd>crois&eacute;<klr tip="ind"> (hybride)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_i;&c_b;&c_r;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
