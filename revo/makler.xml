<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>makler</rad>/i
</kap>
<drv mrk="makler.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">KOME</uzo>
  <snc>
    <dif>
      Profesie servi kiel profitanta peranto inter vendantoj kaj a&ccirc;etantoj:
      <ekz>
        <tld/>i pri teksa&jcirc;oj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>makeln</trd>,
    <trd>als <ind>Makler</ind> t&auml;tig sein</trd>
  </trdgrp>
  <trd lng="en">broker</trd>
  <trdgrp lng="fr">
    <trd>faire l'<ind>interm&eacute;diaire</ind></trd>,
    <trd>repr&eacute;senter<klr> (commercialement)</klr></trd>
  </trdgrp>
  <trd lng="nl">makelen</trd>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_r;&c_je;&c_d;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="makler.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Homo kiu profesie makleradas.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>broker</trd>,
    <trd>courtier</trd>,
    <trd>repr&eacute;sentant</trd>
  </trdgrp>
</drv>
</art>
