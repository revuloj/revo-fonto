    <trd>B&ouml;rsenmakler</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>agent de borsa</trd>,
    <trd>broker</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd><ind>corredor</ind> de bolsa</trd>,
    <trd><ind>agente</ind> de bolsa</trd>
