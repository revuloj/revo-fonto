<drv mrk="makler.kambi0isto">
  <kap>kambia <tld/>isto</kap>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>IKEV</bib></fnt>
  <snc>
    <dif>
      Makleristo pri kambioj. Kambi-makleristo.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_e;&c_k;&c_s;&c_a;&c_l;&c_mol;&c_n;&c_y; <ind>&c_b;&c_r;&c_o;&c_k;&c_je;&c_r;</ind></trd>,
    <trd>&c_v;&c_e;&c_k;&c_s;&c_a;&c_l;&c_mol;&c_n;&c_y; <ind>&c_m;&c_a;&c_k;&c_l;&c_je;&c_r;</ind></trd>
  </trdgrp>
  <trd lng="ca">agent de canvi</trd>
  <trd lng="cs">sm&ecaron;ne&ccaron;n&yacute; makl&eacute;&rcaron;</trd>
  <trdgrp lng="de">
    <trd>Wechselmakler</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bill broker</trd>
  </trdgrp>
  <trd lng="es"><ind>agente</ind> de cambio</trd>
  <trdgrp lng="hu">
    <trd>v&aacute;lt&oacute;alkusz</trd>,
    <trd>lesz&aacute;m&iacute;tol&aacute;si &uuml;gyn&ouml;k</trd>,
    <trd>lesz&aacute;m&iacute;tol&aacute;si alkusz</trd>
  </trdgrp>
  <trd lng="nl">wisselmakelaar</trd>
  <trd lng="pl">broker walutowy</trd>
  <trd lng="ro">broker valutar</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_k;&c_s;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; <ind>&c_b;&c_r;&c_o;&c_k;&c_je;&c_r;</ind></trd>,
    <trd>&c_v;&c_je;&c_k;&c_s;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; <ind>&c_m;&c_a;&c_k;&c_l;&c_je;&c_r;</ind></trd>
  </trdgrp>
  <trd lng="sk">zmenkov&yacute; makl&eacute;r</trd>
</drv>

