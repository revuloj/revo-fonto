    <trd>agent de burs&abreve;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_i;&c_r;&c_zh;&c_je;&c_v;&c_o;&c_j; <ind>&c_b;&c_r;&c_o;&c_k;&c_je;&c_r;</ind></trd>,
    <trd>&c_b;&c_i;&c_r;&c_zh;&c_je;&c_v;&c_o;&c_j; <ind>&c_m;&c_a;&c_k;&c_l;&c_je;&c_r;</ind></trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4EA4;&#x6613;&#x6240;&#x7684;&#x6295;&#x673A;&#x8005; <pr>ji&#x101;oy&#xEC;su&#x1D2;det&#xF3;uj&#x12B;zh&#x11B;</pr></trd>,
    <trd>&#x4EA4;&#x6613;&#x6240;&#x7684;&#x6295;&#x6A5F;&#x8005; <pr>ji&#x101;oy&#xEC;su&#x1D2;det&#xF3;uj&#x12B;zh&#x11B;</pr></trd>
  </trdgrp>
