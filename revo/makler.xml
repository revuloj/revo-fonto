<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>makler</rad>/i
</kap>
<drv mrk="makler.0i">
  <kap>
    <ofc>*</ofc><tld/>i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Profesie servi kiel profitanta peranto inter vendantoj kaj a&ccirc;etantoj:
      <ekz>
        <tld/>i pri teksa&jcirc;oj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
      <trd>makeln</trd>,
      <trd>als <ind>Makler</ind> t&auml;tig sein</trd>
  </trdgrp>
  <trd lng="en">broker</trd>
  <trd lng="nl">makelen</trd>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_r;&c_je;&c_d;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
