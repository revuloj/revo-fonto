<art mrk="$Id: hepati.xml,v 1.20 2023/08/31 15:46:16 revo Exp $">
<kap>
  <rad>hepatik</rad>/o
</kap>
<drv mrk="hepati.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio 
      <ref tip="malprt" cel="ranunk1.0oj">ranunkolacoj</ref>
      (<trd lng="la">Hepatica</trd>), plurjara herbo kun plej ofte bluaj
      floroj kaj trilobaj folioj:
      <ekz>
        la klimato de Antarkto ne permesas la formadon de etenda vegeta&jcirc;aro, &ccirc;irka&ubreve; 100
        specioj de muskoj kaj 25 specioj de <tld/>oj, sed nur tri specioj de
        angiospermaj plantoj, &ccirc;iuj el kiuj trovi&gcirc;as en la Antarkta Duoninsulo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=47">Antarkto</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/6/62/Hepatica_transsylvanica.jpg/360px-Hepatica_transsylvanica.jpg" prm="PD">transsilvania <tld/>o (Hepatica transsilvanica), Botanika &Gcirc;ardeno V&aacute;cr&aacute;t&oacute;t, Hungarujo
        <fnt>
          <aut>Pipi, propra verko</aut>
          <lok><url ref="&WCW;/File:Hepatica_transsylvanica.jpg">&WCW;/File:Hepatica_transsylvanica.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_l;&c_je;&c_s;&c_k;&c_a;</trd>
  <trd lng="de">Leberbl&uuml;mchen</trd>
  <trd lng="fr">h&eacute;patique<klr tip="ind"> (subst., bot.)</klr></trd>
  <trdgrp lng="hu">
    <trd>m&aacute;jvir&aacute;g</trd>,
    <trd>m&aacute;jf&udblac;</trd>
  </trdgrp>
  <trd lng="ja">&#x30B9;&#x30CF;&#x30DE;&#x30BD;&#x30A6;</trd>
  <trd lng="nl">leverbloempje</trd>
  <trd lng="no">bl&aring;veis</trd>
  <trd lng="pl">przylaszczka</trd>
  <trd lng="pt">hep&aacute;tica</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_ch;&c_jo;&c_n;&c_o;&c_ch;&c_n;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_l;&c_je;&c_s;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x435;&#x447;&#x456;&#x43D;&#x43E;&#x447;&#x43D;&#x438;&#x446;&#x44F;</trd>,
    <trd>&#x43F;&#x435;&#x447;&#x456;&#x43D;&#x43A;&#x43E;&#x432;&#x438;&#x439; &#x43C;&#x43E;&#x445;</trd>
  </trdgrp>
</drv>
</art>
