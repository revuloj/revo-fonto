<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hipoglos.xml,v 1.7 2023/11/15 17:08:01 revo Exp $">
<kap>
  <rad>hipoglos</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="hipoglos.0o">
  <kap><tld/>o</kap>
  <snc mrk="hipoglos.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda, &gcirc;is 3&nbsp;m, plata <ref tip="lst" cel="fisx.mar0o" lst="voko:fi&scirc;oj">marfi&scirc;o</ref>
       kun triangula vosto kaj amba&ubreve; okuloj sur la dekstra flanko;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref>
       el familio <ref tip="malprt" cel="pleuxr3.0oj">ple&ubreve;ronektedoj</ref>
      (<trd lng="la">Hippoglossus</trd>):
      <ekz>
        ofte ili revenis sen kapta&jcirc;o &ndash; a&ubreve; nur kun malgrasa
        <tld/>o sur la fundo de la boateto
        <fnt>
          <aut>G. Gunnarsson, trad. P. Y. Gunnlaugsson</aut>,
          <vrk><url ref="http://www.ismennt.is/vefir/esperant/la_tradukisto/esperanto/hejmen_01.htm">
          Patro kaj filo</url></vrk>,
          <lok>la Tradukisto, 2002-11 (42)</lok>
        </fnt>;
      </ekz>
      <ekz>
      <tld/>o stufita en aluminia folio
      <fnt>
      <aut>Maria Becker-Meisberger</aut>
      <vrk><url ref="&Monato;/1985/monato198506.pdf#page=21">Kvarfoje hipogloso</url></vrk>
      <bib>Monato</bib>
      <lok>1985/06, p. 21</lok>
      </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_t;&c_u;&c_s;</trd>
  <trd lng="de">Heilbutt</trd>
  <trd lng="en">halibut</trd>
  <trd lng="fr">fl&eacute;tan</trd>
  <trd lng="nl">heilbot</trd>
  <trdgrp lng="pl">
   <trd>halibut</trd>,
   <trd>kulbak</trd>
 </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6BD4;&#x76EE;&#x9B5A; <pr>b&#x1D0;m&#xF9;y&#xFA;</pr></trd>,
    <trd>&#x5927;&#x6BD4;&#x76EE;&#x9B5A; <pr>d&#xE0;b&#x1D0;m&#xF9;y&#xFA;</pr></trd>
  </trdgrp>
</drv>
</art>

