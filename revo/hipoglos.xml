<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hipoglos</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="hipoglos.0o">
  <kap><tld/>o</kap>
  <snc mrk="hipoglos.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda plata marfi&scirc;o kun amba&ubreve; okuloj sur sama flanko;
      genro el familio <ref tip="super" 
      cel="pleuxr3.0oj.ZOO">ple&ubreve;ronektedoj</ref>
      (<trd lng="la">Hippoglossus</trd>):
      <ekz>
        ofte ili revenis sen kapta&jcirc;o &ndash; a&ubreve; nur kun malgrasa
        <tld/>o sur la fundo de la boateto
        <fnt>
          <aut>G. Gunnarsson, trad. P. Y. Gunnlaugsson</aut>,
          <vrk><url
          ref="http://www.ismennt.is/vefir/esperant/la_tradukisto/esperanto/hejmen_01.htm">
          Patro kaj filo</url></vrk>,
          <lok>la Tradukisto, 2002-11 (42)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">halibut</trd>
  <trd lng="fr">fl&eacute;tan</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>