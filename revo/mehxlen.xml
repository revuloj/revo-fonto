  <trd lng="de">Mecheln</trd>
  <trdgrp lng="en">
    <trd>Mechlin</trd>,
    <trd>Malines</trd>
  </trdgrp>
  <trd lng="fr">Malines</trd>
  <trd lng="lat">Mechlinia</trd>
  <trd lng="nl">Mechelen</trd>
  <trd lng="ru">&c_M;exe&c_l;e&c_n;</trd>
 
