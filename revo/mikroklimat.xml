<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mikroklimat.xml,v 1.5 2023/09/02 06:04:12 revo Exp $">
<kap>
  <rad>mikroklimat</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="mikroklimat.0o">
  <kap><tld/>o</kap>
  <snc mrk="mikroklimat.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      <ref tip="vid" cel="klimat.0o.MET">Klimato</ref>
      de tre malvasta regiono, alia ol la &ccirc;irka&ubreve;a,
      ordinare estigata de loka reliefo:
      <ekz>
        dolino <klr>...</klr> estas medio tute
        speciala, kun propra <tld/>o,
        aparte favora por kultivado
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://ifef.free.fr/Karsto.pdf">
          Karsto</url></vrk>,
          <lok>[vidita en 2012]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_k;&c_r;&c_a;&c_k;&c_l;&c_ib;&c_m;&c_a;&c_t;</trd>
  <trd lng="bg">&c_m;&c_i;&c_k;&c_r;&c_o;&c_k;&c_l;&c_i;&c_m;&c_a;&c_t;</trd>
  <trd lng="cs">mikroklima</trd>
  <trd lng="da">mikroklima</trd>
  <trd lng="de">Mikroklima</trd>
  <trd lng="en">microclimate</trd>
  <trd lng="es">microclima</trd>
  <trd lng="fr">microclimat</trd>
  <trd lng="it">microclima</trd>
  <trdgrp lng="ja">
    <trd>&#x5FAE;&#x6C17;&#x5019;  [<ind>&#x3073;&#x304D;&#x3053;&#x3046;</ind>]</trd>,
    <trd>&#x5C0F;&#x6C17;&#x5019;  [<ind>&#x3057;&#x3087;&#x3046;&#x304D;&#x3053;&#x3046;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">microklimaat</trd>
  <trd lng="pl">mikroklimat</trd>
  <trd lng="ru">&c_m;&c_i;&c_k;&c_r;&c_o;&c_k;&c_l;&c_i;&c_m;&c_a;&c_t;</trd>
  <trd lng="sk">mikrokl&iacute;ma</trd>
</drv>
</art>

