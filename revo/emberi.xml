<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: emberi.xml,v 1.21 2023/08/31 11:49:28 revo Exp $">
<kap>
  <rad>emberiz</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="emberi.0o">
  <kap><tld/>o</kap>
  <snc mrk="emberi.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref>
      el familio <ref tip="malprt" cel="fringe1.0oj">fringedoj</ref>
      (<trd lng="la">Emberiza</trd>):
      <ekz>
        la Flava <tld/>o estas fortika 15.5&#x202F;&ndash;&#x202F;17&#x202F;cm longa birdo, kun griza dika
        beko de
        semoman&gcirc;antoj, la masklo havas brilflavan kapon, flavajn subajn partojn kaj
        tre striecan brunan dorson <klr>[&#x2026;]</klr>, la ino estas multe malpli kolora
        kaj pli strieca sube, sed &scirc;i facile identigeblas pro la kombino de helbruna
        senmarka pugo kaj nigreca vosto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18260">Flava emberizo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/d/d1/Red-headed_bunting_(Emberiza_bruniceps)_male_non-breeding_plumage.jpg/480px-Red-headed_bunting_(Emberiza_bruniceps)_male_non-breeding_plumage.jpg" prm="CC BY-SA 4.0">masklo de Brunkapa <tld/>o (Emberiza bruniceps), Salai, Barato
        <fnt>
          <aut>Charles J Sharp, propra verko</aut>
          <lok><url ref="&WCW;/File:Red-headed_bunting_(Emberiza_bruniceps)_male_non-breeding_plumage.jpg">&WCW;/File:Red-headed_bunting_(Emberiza_bruniceps)_male_non-breeding_plumage.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_a;&c_w;&c_t;&c_a;&c_b;&c_r;&c_u;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_a;&c_w;&c_s;&c_ja;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_s;&c_ja;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>sit</trd>,
    <trd>g&egrave;nere Emberiza</trd>
  </trdgrp>
  <trd lng="cs">strnad</trd>
  <trd lng="de">Ammer</trd>
  <trd lng="fr">bruant</trd>
  <trd lng="hu">s&aacute;rm&aacute;ny</trd>
  <trd lng="ja">&#x30DB;&#x30AA;&#x30B8;&#x30ED;</trd>
  <trd lng="pl">trznadel</trd>
  <trd lng="pt">emberiza</trd>
  <trd lng="ru">&c_o;&c_v;&c_s;&c_ja;&c_n;&c_k;&c_a; <klr>(&c_p;&c_t;&c_i;&c_c;&c_a;)</klr></trd>
  <trd lng="sk">strn&aacute;dka</trd>
  <trd lng="uk">&c_v;&c_ib;&c_v;&c_s;&#x44F;&#x43D;&#x43A;&c_a;</trd>
</drv>
</art>
