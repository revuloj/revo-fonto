<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>miriapod</rad>/oj
</kap>
<drv mrk="miriap.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="miriap.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Klaso el la filumo <ref tip="super"
      cel="artik.0pieduloj">artikpieduloj</ref>
      (<trd lng="la">Myriapoda</trd>), kiuj havas
      tre multajn krurojn.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_n;&c_a;&c_g;&c_a;&c_n;&c_o;&c_zh;&c_k;&c_ib; <klr tip="amb">(&c_k;&c_l;&c_ja;&c_s;)</klr></trd>
  <trd lng="ca">miri&agrave;podes</trd>
  <trd lng="de">Tausendf&uuml;&szlig;er</trd>
  <trd lng="fr">myriapodes</trd>
  <trd lng="hu">sokl&aacute;b&uacute;ak</trd>
  <trd lng="nl">duizendpoten</trd>
  <trd lng="ru">&c_m;&c_n;&c_o;&c_g;&c_o;&c_n;&c_o;&c_zh;&c_k;&c_i; <klr tip="amb">(&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>
</drv>
</art>
