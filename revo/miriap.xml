<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: miriap.xml,v 1.22 2024/01/26 17:40:06 revo Exp $">
<kap>
  <rad>miriapod</rad>/oj <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="miriap.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="miriap.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Bestoj, kiuj havas
      tre multajn krurojn;
      <ref tip="lst" cel="grup.0o.BIO" lst="voko:zoologiaj_grupoj">grupo</ref>
       el la filumo
      <ref tip="malprt" cel="artik.0uloj">artikuloj</ref>
      (<trd lng="la">Myriapoda</trd>):
      <ekz>
        milpieduloj estas <tld/>oj kiuj havas longajn korpojn dividitajn en segmentoj,
        kun du paroj da kruroj po &ccirc;iu segmento
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=215086">Milpieduloj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_n;&c_a;&c_g;&c_a;&c_n;&c_o;&c_zh;&c_k;&c_ib; <klr tip="amb">(&c_k;&c_l;&c_ja;&c_s;)</klr></trd>
  <trd lng="ca">miri&agrave;podes</trd>
  <trd lng="cs">stono&zcaron;ky</trd>
  <trd lng="de">Tausendf&uuml;&szlig;er</trd>
  <trd lng="en">myriapods</trd>
  <trdgrp lng="es">
    <trd>miri&aacute;podos</trd>,
    <trd>miri&oacute;podos</trd>,
    <trd>milpi&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>mille-pattes</trd>,
    <trd>myriapodes</trd>
  </trdgrp>
  <trd lng="hu">sokl&aacute;b&uacute;ak</trd>
  <trd lng="ja">&#x591A;&#x8DB3;&#x985E;  <pr>&#x305F;&#x305D;&#x304F;&#x308B;&#x3044;</pr></trd>
  <trd lng="nl">duizendpoten</trd>
  <trd lng="pl">wije</trd>
  <trd lng="ru">&c_m;&c_n;&c_o;&c_g;&c_o;&c_n;&c_o;&c_zh;&c_k;&c_i; <klr tip="amb">(&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>
  <trd lng="sk">stono&zcaron;ky</trd>
  <trd lng="uk">&#x431;&#x430;&#x433;&#x430;&#x442;&#x43E;&#x43D;&#x456;&#x436;&#x43A;&#x438;</trd>
  <trdgrp lng="zh">
    <trd>&#x8708; <pr>w&#xFA;</pr></trd>,
    <trd>&#x86A3; <pr>g&#x14D;ng</pr></trd>,
    <trd>&#x8708;&#x86A3; <pr>w&#xFA;g&#x14D;ng</pr></trd>
  </trdgrp>
</drv>
</art>
