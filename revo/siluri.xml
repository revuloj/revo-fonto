<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: siluri.xml,v 1.19 2024/03/24 20:21:57 revo Exp $">
<kap>
  <rad>siluri</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="siluri.0o">
  <kap><tld/>o</kap>
  <snc mrk="siluri.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      La tria <ref tip="lst" val="-444000000" lst="voko:geologiaj_periodoj" cel="period.0o.dauxro">periodo</ref>
      de la <ref tip="malprt" cel="paleozoik.0o">paleozoiko</ref>:
      <ekz>
        <tld/>o estas terhistoria <klr>[periodo]</klr> de &ccirc;irka&ubreve; 444 &gcirc;is 417 milionoj da jaroj, dum
        <klr>[kiu]</klr> aperis la makzelaj fi&scirc;oj, konodontoj, krinoideoj,
        pteridofitoj, unuaj kontinentaj plantoj, bestoj (insektoj) <klr>[kaj]</klr>
        abundis prakoraloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=138848">Silurio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_y;&c_l;&c_ju;&c_r;</trd>,
    <trd>&c_s;&c_y;&c_l;&c_ju;&c_r;&c_y;&c_j;&c_s;&c_k;&c_ib; &c_p;&c_e;&c_r;&c_y;&c_ja;&c_d;</trd>
  </trdgrp>
  <trd lng="cs">silur</trd>
  <trd lng="de">Silur</trd>
  <trd lng="es">sil&uacute;rico</trd>
  <trd lng="fr">silurien</trd>
  <trd lng="hu">szilur</trd>
  <trdgrp lng="ja">
    <trd>&#x30B7;&#x30EB;&#x30EB;&#x7D00;  <pr>&#x30B7;&#x30EB;&#x30EB;&#x304D;</pr></trd>,
    <trd>&#x30B7;&#x30EB;&#x30EB;&#x7CFB;  <pr>&#x30B7;&#x30EB;&#x30EB;&#x3051;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">sylurium</trd>
  <trd lng="pl">sylur</trd>
  <trd lng="ru">&c_s;&c_i;&c_l;&c_u;&c_r;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j; &c_p;&c_je;&c_r;&c_i;&c_o;&c_d;</trd>
  <trd lng="sk">sil&uacute;r</trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x438;&#x43B;&#x443;&#x440;</trd>,
    <trd>&#x441;&#x438;&#x43B;&#x443;&#x440;&#x456;&#x439;&#x441;&#x44C;&#x43A;&#x438;&#x439; &#x43F;&#x435;&#x440;&#x456;&#x43E;&#x434;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5FD7;&#x7559;&#x7D00; <pr>zh&#xEC;li&#xFA;j&#xEC;</pr></trd>,
    <trd>&#x5FD7;&#x7559;&#x7EAA; <pr>zh&#xEC;li&#xFA;j&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>
