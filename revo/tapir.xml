<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>9</ofc><rad>tapir</rad>/o 
</kap>
<drv mrk="tapir.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="tapir.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
     <dif>
       Genro (<trd lng="la">Tapirus</trd>) de mamuloj
       el la ordo de <ref tip="super"
       cel="huf.nepar0uloj">neparhufuloj</ref>, kun mallonga movebla
       rostro, vivantaj en Suda Azio kaj Suda Ameriko.
    </dif>
  </snc>
  <trd lng="da">tapir</trd>
  <trd lng="de">Tapir</trd>
  <trd lng="fr">tapir</trd>
  <trd lng="it">tapiro</trd>
  <trd lng="nl">tapir</trd>
</drv>
</art>

