<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: uzin.xml,v 1.13 2022/08/05 14:31:07 revo Exp $">
<kap>
   <rad>uzin</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="uzin.0o">
   <kap><tld/>o</kap>
   <snc mrk="uzin.0o.EKON">
     <uzo tip="fak">EKON</uzo>
     <dif>
       <ref tip="super" cel="fabrik.0o.EKON">Fabriko</ref>, precipe por
       metalproduktado kaj metalprilaborado, en kiu la plej grava laboro
       estas farata per ma&scirc;inoj:
       <ekz>
         la <tld/>o prosperis dum la pasinta duonjaro
         <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 18a, p. 106a</lok></fnt>;
       </ekz>
      <ekz>
        la feraj uzinoj havis altkonjunkturon jam komence de la milito
        <fnt>
          <bib>HsT</bib>
          <lok>Disaj vojoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj <tld/>oj kaj altfornoj funkciis, estis fumnubo apud fumnubo en &ccirc;iu valo
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        nun la <tld/>o fabrikas nur centojn da volframaj fadenoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de ba&ubreve;ksito
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="https://www.monato.be/2015/011584.php"
              >Ru&gcirc;aj &scirc;limoj en Mediteraneo</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
   <trdgrp lng="be">
     <trd>&c_z;&c_a;&c_v;&c_o;&c_d;</trd>,
     <trd>&c_f;&c_a;&c_b;&c_r;&c_y;&c_k;&c_a; <klr>(&c_m;&c_e;&c_t;&c_a;&c_l;&c_ju;&c_r;&c_g;&c_ib;&c_ch;&c_n;&c_a;&c_ja;)</klr></trd>
   </trdgrp>
  <trd lng="ca">foneria</trd>
  <trdgrp lng="cs">
    <trd>tov&aacute;rna</trd>,
    <trd>z&aacute;vod</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Werk <klr>(Industrie)</klr></trd>,
    <trd>Produktionsst&auml;tte</trd>,
    <trd>Fabrik <klr>(Metallurgie...)</klr></trd>
  </trdgrp>
  <trdgrp lng="fi">
     <trd>tehdas <klr tip="amb"> (suuri metalliteollisuuslaitos)</klr></trd>,
   </trdgrp>
  <trdgrp lng="fr">
     <trd>fonderie</trd>,
     <trd>usine m&eacute;tallurgique</trd>
   </trdgrp>
  <trd lng="it">fonderia</trd>
  <trdgrp lng="no">
     <trd>jernverk</trd>,
     <trd>fabrikkanlegg</trd>
   </trdgrp>
  <trdgrp lng="pt">
    <trd>usina</trd>
   </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_v;&c_o;&c_d;</trd>
  <trd lng="sk">z&aacute;vod</trd>
  <trd lng="uk">&c_z;&c_a;&c_v;&c_o;&c_d;</trd>
</drv>
</art>
