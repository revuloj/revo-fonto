<vortaro>
<art mrk="$Id: vinaj.xml,v 1.4 2023/10/18 06:31:30 revo Exp $">
<kap>
  <rad>Vinaj</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="vinaj.0o">
  <kap><tld/>o</kap>
  <snc mrk="vinaj.0o.BUD">
    <uzo tip="fak">BUD</uzo>
    <dif>
      Mona&hcirc;a vivregularo,
      kolektata en la unua korbo de la
      <ref tip="malprt" cel="tripitak.0o.BUD">tripitako</ref>:
      <ekz>
        200 jarojn post la morto de la Budao, la Re&gcirc;o A&scirc;oko
        kunvenigis la Samganojn por surpaperigi la
        Instruojn, kiuj i&gcirc;is la <tld/>o kaj la komenco de la Sutroj

        <fnt>
          <aut>S. Schwichow</aut>,
          <vrk><url
          ref="http://stechjo.blogspot.com/2005/12/esperanto.html">a
          musing journey</url></vrk>,
          <lok>2005-12-01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_V;&c_ib;&c_n;&c_a;&c_ja;</trd>
  <trd lng="ca">Vinaya Pitaka</trd>
  <trd lng="fr">vinaya</trd>
  <trd lng="hu">Vinaya</trd>
</drv>
</art>

