<kap>
  <ofc>3</ofc>
  <rad>melankoli</rad>/o
</kap>

<drv mrk="melank.0o">
  <kap><tld/>o</kap>
  <snc mrk="melank.0o.malgajo">
    <dif>
      Da&ubreve;ra reva, ordinare milda
      <ref tip="super" cel="gaj.mal0o">malgajo</ref>:
      <ekz>
        li rememoris la marbordon <klr>[&#x2026;]</klr> kaj <tld/>o sed ne doloro, plenigis
        lian bruston
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la re&gcirc;o de la lando malsani&gcirc;is je tre profunda <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Folio el la &ccirc;ielo</lok>
        </fnt>;
      </ekz>
      <ekz>
        post momento &scirc;i trankvili&gcirc;is, kaj &scirc;iaj veluraj okuloj ree rigardis kun la kutima
        dol&ccirc;a <tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ensorbis la vortojn, plenajn de maldol&ccirc;a <tld/>o
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la anasido rekonis la belajn bestojn, kaj stranga <tld/>o &gcirc;in atakis
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        la franca vorto <klr>[&#x2026;]</klr> elvokas ian dol&ccirc;an
        <tld/>on, mankon de forto
        <fnt><bib>BonaLingvo</bib>, <lok>p.
        104a</lok></fnt><klr>[&#x2026;]</klr>
      </ekz>.
    </dif>
    <ref tip="vid" cel="splen.0o">spleno</ref>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_l;&c_ja;&c_n;&c_h;&c_o;&c_l;&c_ib;&c_ja;</trd>
  <trdgrp lng="bg">
    <trd>&c_m;&c_je;&c_l;&c_a;&c_n;&c_h;&c_o;&c_l;&c_i;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_t;&c_i;&c_s;&c_n;&c_a;&c_t;&c_o;&c_s;&c_t;</trd>
  </trdgrp>
  <trd lng="br">melkoni</trd>
  <trdgrp lng="ca">
    <trd>malenconia</trd>,
    <trd>melangia</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Melancholie</trd>,
    <trd>Schwermut</trd>,
    <trd>Tr&uuml;bsinn</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>melancholy</trd>,
    <trd>melancholia</trd>,
    <trd>gloom</trd>,
    <trd>depression</trd>,
    <trd>dejection</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>mal de vivre</trd>,
    <trd>m&eacute;lancolie</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>melank&oacute;lia</trd>,
    <trd>m&eacute;lab&uacute;</trd>,
    <trd>b&uacute;skomors&aacute;g</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6182;&#x9B31;  <pr>&#x3086;&#x3046;&#x3046;&#x3064;</pr></trd>,
    <trd>&#x54C0;&#x6101;  <pr>&#x3042;&#x3044;&#x3057;&#x3085;&#x3046;</pr></trd>,
    <trd>&#x30E1;&#x30E9;&#x30F3;&#x30B3;&#x30EA;&#x30FC;</trd>,
    <trd>&#x3082;&#x306E;&#x60B2;&#x3057;&#x304F;&#x3075;&#x3055;&#x304E;&#x8FBC;&#x3093;&#x3060;&#x6C17;&#x5206;  <pr>&#x3082;&#x306E;&#x304C;&#x306A;&#x3057;&#x304F;&#x3075;&#x3055;&#x304E;&#x3053;&#x3093;&#x3060;&#x304D;&#x3076;&#x3093;</pr></trd>,
    <trd>&#x9B31;&#x75C5;  <pr>&#x3046;&#x3064;&#x3073;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>melancholie</trd>,
    <trd>zwaarmoedigheid</trd>
  </trdgrp>
  <trd lng="pt">melancolia</trd>
  <trd lng="pl">melancholia</trd>
  <trd lng="ro">melancolie</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_u;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_n;&c_y;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_h;&c_a;&c_n;&c_d;&c_r;&c_a;</trd>,
    <trd>&c_m;&c_je;&c_l;&c_a;&c_n;&c_h;&c_o;&c_l;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="it">melanconia</trd>
  <trd lng="sk">melanch&oacute;lia</trd>
  <trdgrp lng="cs">
    <trd>melancholie</trd>,
    <trd>stesk</trd>
  </trdgrp>
  <trd lng="uk">&#x43C;&#x435;&#x43B;&#x430;&#x43D;&#x445;&#x43E;&#x43B;&#x456;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x60C6;&#x6005; <pr>ch&#xF3;uch&#xE0;ng</pr></trd>,
    <trd>&#x60C6;&#x60B5; <pr>ch&#xF3;uch&#xE0;ng</pr></trd>,
    <trd>&#x610A; <pr>b&#xEC;</pr></trd>,
    <trd>&#x60B2;&#x6101; <pr>b&#x113;ich&#xF3;u</pr></trd>,
    <trd>&#x61D1; <pr>m&#xE8;n</pr></trd>,
    <trd>&#x61E3; <pr>m&#xE8;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="melank.0a">
  <kap><tld/>a</kap>
  <snc mrk="melank.0a.melankolisenta">
    <dif>
      Malgajeta, sentanta a&ubreve; sentiganta <tld/>on:
      <ekz>
        ili eka&ubreve;dis virinan vo&ccirc;on, kiu <klr>[&#x2026;]</klr> kantis per <tld/>a tono
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        rigidaj statuoj de Oziriso kun <tld/>a rideto sur la lipoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        fluto kaj ludis <tld/>an melodion
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        la viro sidis <tld/>a dum longa tempo
        <fnt>
          <aut>Will Green</aut>,
          <vrk>Gantoj</vrk>,
          <bib>Monato</bib>
          <lok>jaro 1997a, numero 5a, p. 24a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>malenconi&oacute;s</trd>,
      <trd>melangi&oacute;s</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>melankolisch</trd>,
      <trd>schwerm&uuml;tig</trd>,
      <trd>tr&uuml;bsinnig</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>melankolikus</trd>,
      <trd>m&eacute;lab&uacute;s</trd>,
      <trd>b&uacute;skomor</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_g;&c_r;&c_u;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_m;&c_je;&c_l;&c_a;&c_n;&c_h;&c_o;&c_l;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="melank.0a.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      Propra al <tld/>ulo:
      <ekz>
        la <tld/>a <ref tip="super" cel="temper.0o">temperamento</ref>.
      </ekz>
    </dif>
    <trd lng="ru">&c_m;&c_je;&c_l;&c_a;&c_n;&c_h;&c_o;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
  <snc mrk="melank.0a.melankoliiga">
    <dif>
      <ref tip="dif" cel="morn.0a">Morna</ref>:
      <ekz>
        jen lia vida&jcirc;o de tiu &leftquot;<tld/>a montetaro&rightquot;
        <fnt>
          <aut>A. Giridhar Rao</aut>,
          <vrk>La triumfo de rezono (Arthur Conan Doyle, La 
          &Ccirc;ashundo de la Baskerviloj)</vrk>,
          <bib>Monato</bib>
          <lok>jaro 2000a, numero 10a, p. 30a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="hu">komor</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_je;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_u;&c_n;&c_y;&c_l;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <rim mrk="melank.0a.RIM.1">
    <ctl>Melankolia</ctl> celas precipe la internan senton de homo,
    <ctl>moroza</ctl> prefere lian eksteran aspekton,
    <ctl>morna</ctl> prefere la etoson de loko a&ubreve; cirkonstancoj.
    Praktike, &ccirc;ar etoso, sento kaj aspekto sin influas, &ccirc;iu el 
    la vortoj povas surpreni alterne &ccirc;iun nuancon.
    <aut>&jcirc;v</aut>
  </rim>
  <trdgrp lng="be">
    <trd>&c_m;&c_e;&c_l;&c_ja;&c_n;&c_h;&c_a;&c_l;&c_ib;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_u;&c_m;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_m;&c_r;&c_o;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_t;&c_malmol;&c_zh;&c_je;&c_n;</trd>,
    <trd>&c_m;&c_r;&c_a;&c_ch;&c_je;&c_n;</trd>,
    <trd>&c_p;&c_o;&c_t;&c_i;&c_s;&c_n;&c_a;&c_t;</trd>
  </trdgrp>
  <trd lng="br">melkonius</trd>
  <trdgrp lng="en">
    <trd>melancholic</trd>,
    <trd>melancholy</trd>,
    <trd>gloomy</trd>,
    <trd>depressed</trd>,
    <trd>dejected</trd>
  </trdgrp>
  <trd lng="fr">m&eacute;lancolique</trd>
  <trdgrp lng="ja">
    <trd>&#x6182;&#x9B31;&#x306A;  <pr>&#x3086;&#x3046;&#x3046;&#x3064;&#x306A;</pr></trd>,
    <trd>&#x3075;&#x3055;&#x304E;&#x8FBC;&#x3093;&#x3060;  <pr>&#x3075;&#x3055;&#x304E;&#x3053;&#x3093;&#x3060;</pr></trd>,
    <trd>&#x3082;&#x306E;&#x60B2;&#x3057;&#x3044;  <pr>&#x3082;&#x306E;&#x304C;&#x306A;&#x3057;&#x3044;</pr></trd>,
    <trd>&#x9B31;&#x75C5;&#x306E;  <pr>&#x3046;&#x3064;&#x3073;&#x3087;&#x3046;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>melancholisch</trd>,
    <trd>treurig</trd>
  </trdgrp>
  <trd lng="pl">melancholiczny</trd>
  <trd lng="pt">melanc&oacute;lico</trd>
  <trd lng="it">melanconico</trd>
  <trdgrp lng="sk">
    <trd>melancholick&yacute;</trd>,
    <trd>trudnomyse&lcaron;n&yacute;</trd>,
    <trd>&tcaron;a&zcaron;komyse&lcaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="cs">melancholicky</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x435;&#x43B;&#x430;&#x43D;&#x445;&#x43E;&#x43B;&#x456;&#x439;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x441;&#x443;&#x43C;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x437;&#x430;&#x43C;&#x438;&#x441;&#x43B;&#x435;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x60B6; <pr>m&#xE8;n</pr></trd>,
    <trd>&#x95F7; <pr>m&#xE8;n</pr></trd>,
    <trd>&#x610A; <pr>b&#xEC;</pr></trd>,
    <trd>&#x843D;&#x843D;&#x5BE1;&#x6B22; <pr>lu&#xF2;lu&#xF2;gu&#x1CE;hu&#x101;n</pr></trd>,
    <trd>&#x843D;&#x843D;&#x5BE1;&#x6B61; <pr>lu&#xF2;lu&#xF2;gu&#x1CE;hu&#x101;n</pr></trd>,
    <trd>&#x60C6;&#x6005; <pr>ch&#xF3;uch&#xE0;ng</pr></trd>,
    <trd>&#x60C6;&#x60B5; <pr>ch&#xF3;uch&#xE0;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="melank.0e">
  <kap>
    <tld/>e
  </kap>
  <snc mrk="melank.0e.malgaje">
    <dif>
      La&ubreve; <tld/>a maniero:
      <ekz>
        multe da pensoj atakis lin, lia koro estis agordita melankolie, nevole li
        de&scirc;iris unu el la plej proksimaj floroj
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        bildoj <klr>[&#x2026;]</klr> <tld/>e &gcirc;ojaj
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Alpa korno sonis profunde <tld/>e
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_l;&c_ja;&c_n;&c_h;&c_a;&c_l;&c_ib;&c_ch;&c_n;&c_a;</trd>
</drv>

<drv mrk="melank.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="melank.0ulo.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      Homo kies <ref tip="super" cel="temper.0o">temperamenton</ref>
      karakterizas malforta ekscitemo, emocioj profundaj kaj 
      da&ubreve;remaj:
      <ekz>
        la sinjorinoj nomis lin &ccirc;arma <tld/>ulo,
        sed la virinoj ne ekzistis plu por li
        <fnt>
          <aut>I. Turgenjev, tr. Kabe</aut>:
          <vrk>Patroj kaj filoj</vrk>.
          <lok>Inko Tyreso 2001. &#x2010; &Ccirc;ap. 7a.</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_l;&c_ja;&c_n;&c_h;&c_o;&c_l;&c_ib;&c_k;</trd>
  <trd lng="ca">malenconi&oacute;s <klr>(individu amb temperament
      malenconi&oacute;s)</klr></trd>
  <trd lng="cs">melancholik</trd>
  <trd lng="de">Melankoliker</trd>
  <trd lng="en">melancholic</trd>
  <trd lng="it">melanconico</trd>
  <trd lng="ja">&#x9B31;&#x75C5;&#x60A3;&#x8005;  <pr>&#x3046;&#x3064;&#x3073;&#x3087;&#x3046;&#x304B;&#x3093;&#x3058;&#x3083;</pr></trd>
  <trd lng="pl">melancholik</trd>
  <trd lng="ro">melancolic</trd>
  <trd lng="ru">&c_m;&c_je;&c_l;&c_a;&c_n;&c_h;&c_o;&c_l;&c_i;&c_k;</trd>
  <trd lng="sk">melancholik</trd>
  <trd lng="uk">&#x43C;&#x435;&#x43B;&#x430;&#x43D;&#x445;&#x43E;&#x43B;&#x456;&#x43A;</trd>
</drv>

