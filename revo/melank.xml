<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>melankoli</rad>/o
</kap>
<drv mrk="melank.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Stato de konstanta sopira malgajeco kaj malvigleco
    </dif>
    <refgrp tip="sin">
      <ref cel="splen.0o">spleno</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="nigr.0a">nigra</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Melancholie</trd>,
    <trd>Tr&uuml;bsinn</trd>,
    <trd>Schwermut</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>melancholie</trd>,
    <trd>zwaarmoedigheid</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_je;&c_l;&c_a;&c_n;&c_h;&c_o;&c_l;&c_i;&c_ja;</trd>
</drv>
</art>
