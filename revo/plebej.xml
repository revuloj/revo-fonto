<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: plebej.xml,v 1.1 2025/02/25 18:35:36 afido Exp $">
    <kap>
      <rad>plebej</rad>/o
      <fnt><bib>JED</bib></fnt>
    </kap>

<drv mrk="plebej.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="pleb.0ano">Plebano</ref>:
      <ekz>
        liberulo kaj sklavo, patricio kaj <tld/>o, bienulo kaj servutulo, gildmajstro
        kaj metiisto, unuvorte, subpremanto kaj subpremato, staris konstante unu
        kontra&ubreve; la alia
        <fnt>
          <aut>Karlo Markso kaj Frederiko Engelso, trad. Arturo Baker</aut>
          <vrk><url ref="https://www.marxists.org/esperanto/marx-engels/1848/manifesto/manifesto-2011-01-07.pdf"
              >Manifesto De La Komunista Partio</url></vrk>
          <lok>La Kapitalistoj kaj la Proletarioj, p. 169</lok>
        </fnt>;
      </ekz>
      <ekz>
        la liberon amas plej &parsl; la <tld/>&#8217;
        <fnt>
          <vrk><url ref="https://satesperanto.org/spip.php?article1766"
              >Plebejo</url></vrk>
          <lok>Poemo de l&#8217; Plugisto, eld. SAT</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_l;&c_je;&c_b;&c_e;&c_j;</trd>
    <trdgrp lng="cs">
      <trd>plebejec</trd>,
      <trd>p&rcaron;&iacute;slu&scaron;n&iacute;k &rcaron;&iacute;m. chudiny</trd>
    </trdgrp>
    <trd lng="de">Plebejer<klr tip="amb"> (Person)</klr></trd>
    <trdgrp lng="en">
      <trd>plebian</trd>,
      <trd>commoner</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>pl&eacute;b&eacute;ien<klr> (subst.)</klr></trd>
    </trdgrp>
    <trd lng="ja">&#x5E73;&#x6C11;  <pr>&#12408;&#12356;&#x307F;&#12435;</pr></trd>
    <trd lng="nl">plebejer</trd>
    <trd lng="pl">plebejczyk</trd>
    <trd lng="pt">plebeu</trd>
    <trd lng="sk">plebejec</trd>
    <trdgrp lng="uk">
      <trd>&#x43F;&#x43B;&c_je;&c_b;&c_je;&c_j;</trd>,
      <trd>&c_shch;&c_je; &c_s;&#x43C;&c_je;&c_r;&c_d;</trd>
    </trdgrp>
  </snc>
</drv>

  </art>

