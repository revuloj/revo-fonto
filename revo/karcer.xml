<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>karcer</rad>/o
</kap>
<drv mrk="karcer.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Mallar&gcirc;a kaj malluma &ccirc;elo de malliberejo, kelkfoje e&ccirc; subtera kamero
      por punitoj a&ubreve; kondamnitoj:
      <ekz>
        meti iun en <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        en<tld/>igi ribelulon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Karzer</trd>,
    <trd>Kerker</trd>,
    <trd>Verlies</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cell</trd>,
    <trd>jail</trd>,
    <trd>lock up</trd>,
    <trd>prison</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kerker</trd>,
    <trd>cel<klr tip="ind"> (gevangenis)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_r;&c_c;&c_je;&c_r;</trd>,
    <trd>&c_t;&c_je;&c_m;&c_n;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
</drv>
</art>
