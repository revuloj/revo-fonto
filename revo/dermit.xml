<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>dermit</rad>/o
</kap>
<drv mrk="dermit.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="inflam.0o" lst="voko:malsanoj">Inflamo</ref>
      de la dermo (= ha&ubreve;tinflamo).
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_r;&c_m;&c_a;&c_t;&c_y;&c_t;</trd>
  <trd lng="ca">dermatitis</trd>
  <trdgrp lng="de">
    <trd>Hautentz&uuml;ndung</trd>,
    <trd>Dermitis</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>dermatitis</trd>
    <trd>eczema</trd>
  </trdgrp>
  <trd lng="es">dermatitis</trd>
  <trdgrp lng="fr">
    <trd>dermite</trd>,
    <trd>dermatite</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>dermatitis</trd>,
    <trd>b&odblac;rgyullad&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>dermatitis</trd>,
    <trd>eksem</trd>,
    <trd><ind>radang</ind> kulit</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>dermatitis</trd>,
    <trd>huidontsteking</trd>
  </trdgrp>
  <trd lng="no">dermatitis</trd>
  <trd lng="pl">zapalenie <ind>sk&oacute;ry</ind></trd>
  <trd lng="ru">&c_d;&c_je;&c_r;&c_m;&c_a;&c_t;&c_i;&c_t;</trd>
</drv>
</art>
