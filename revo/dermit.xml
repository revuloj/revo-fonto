<art mrk="$Id: dermit.xml,v 1.21 2023/08/20 20:22:24 revo Exp $">
<kap>
  <rad>dermit</rad>/o
  <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="dermit.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Nekonta&gcirc;a <ref tip="lst" cel="inflam.0o" lst="voko:malsanoj">inflamo</ref>
      de la ha&ubreve;to:
      <ekz>
        salakvaj lagoj <klr>[kun]</klr> sane favoraj efikoj <klr>[&#8230;]</klr> ekzemple cele
        al la mildigo de la ha&ubreve;taj malsanoj <ind>atopia <tld/>o</ind> a&ubreve; psoriazo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=274597">Salakva lago</url></lok>
        </fnt>.
        <trdgrp lng="de">
          <trd lng="">atopisches Ekzem</trd>,
          <trd>Neurodermitis</trd>
        </trdgrp>
      </ekz>
    </dif>
    <ref tip="sin" cel="ekzem.0o">ekzemo</ref>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_r;&c_m;&c_a;&c_t;&c_y;&c_t;</trd>
  <trd lng="bg">&c_d;&c_je;&c_r;&c_m;&c_a;&c_t;&c_i;&c_t;</trd>
  <trd lng="ca">dermatitis</trd>
  <trd lng="cs">z&aacute;pal poko&zcaron;ky</trd>
  <trdgrp lng="de">
    <trd>Hautentz&uuml;ndung</trd>,
    <trd>Dermitis</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>dermatitis</trd>
    <trd>eczema</trd>
  </trdgrp>
  <trd lng="es">dermatitis</trd>
  <trdgrp lng="fr">
    <trd>dermite</trd>,
    <trd>dermatite</trd>
  </trdgrp>
  <trd lng="he">&dalet;&#1463;&lamed;&#1462;&qof;&#1462;&tav; &he;&#1464;&ayin;&vav;&#1465;&resh;</trd>
  <trdgrp lng="hu">
    <trd>dermatitis</trd>,
    <trd>b&odblac;rgyullad&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>dermatitis</trd>,
    <trd>eksem</trd>,
    <trd><ind>radang</ind> kulit</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>dermatitis</trd>,
    <trd>huidontsteking</trd>
  </trdgrp>
  <trd lng="no">dermatitis</trd>
  <trd lng="pl">zapalenie <ind>sk&oacute;ry</ind></trd>
  <trd lng="ru">&c_d;&c_je;&c_r;&c_m;&c_a;&c_t;&c_i;&c_t;</trd>
  <trd lng="sk">ko&zcaron;n&yacute; z&aacute;pal</trd>
</drv>
</art>
