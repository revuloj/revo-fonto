<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pejzag.xml,v 1.22 2022/05/01 17:51:19 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pejza&gcirc;</rad>/o
</kap>
<drv mrk="pejzag.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pejzag.0o.vidajxo">
    <dif>
      Pli malpli pitoreska landvida&jcirc;o:
      <ekz>
        la hirundo flugis &ccirc;iam ankora&ubreve; pluen, kaj la <tld/>oj fari&gcirc;adis &ccirc;iam pli belaj
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la unutoneco de la <tld/>o, la incitanta flaveco de la sablo, la varmego
        <klr>[&#x2026;]</klr> jen estas la &ccirc;efaj ecoj de la libia dezerto
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        specifa formo de pentrado de urbaj <tld/>oj evoluis en Venecio dum la 18a
        jarcento
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2007/009224.php">Canaletto: Imponaj pentra&jcirc;oj de
          e&ubreve;ropaj urbvida&jcirc;oj</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a parko
        <fnt>
          <bib>Monato</bib>
          <aut>Anna kaj Mati Pentus</aut>
          <vrk><url ref="&Monato;/2004/008505.html">Universitata festojaro</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        estas facile &scirc;ati tiujn simplajn, &ccirc;armajn rakontojn, kiuj
        prezentas homojn, bestojn, <tld/>on de suda Francio
        <fnt>
          <aut>S. Johansson</aut>
          <vrk>Per sia ronda okulo</vrk>
          <bib>Monato</bib>,
          <lok>1998:6, p. 22a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="panora.0o.vidajxo">panoramo</ref>
    </refgrp>
    <trdgrp lng="hu">
      <trd>t&aacute;j</trd>,
      <trd>vid&eacute;k</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_l;&c_a;&c_n;&c_d;&c_sh;&c_a;&c_f;&c_t;</trd>,
      <trd>&c_v;&c_i;&c_d;<klr> (&c_m;&c_je;&c_s;&c_t;&c_n;&c_o;&c_s;&c_t;&c_i;)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="pejzag.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      <ref tip="super" cel="pentr.0ajxo">Pentra&jcirc;o</ref> prezentanta
      landvida&jcirc;on:
      <ekz>
        la muroj estis kovritaj per pentra&jcirc;oj &mdash; portretoj, historiaj skizoj kaj <tld/>oj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la fortepianon oni malfermis, la interna flanko de la kovrilo estis ornamita per
        <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la son&gcirc;oj pentris <tld/>on per koloriloj el smeraldoj, ar&gcirc;ento, rubenoj, perloj
        kaj topazoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">KOMP</uzo>
        turno de la ekrano je 90 gradoj, de &leftquot;pejza&gcirc;a&rightquot; <klr>(la&ubreve;lar&gcirc;a)</klr> al &leftquot;portreta&rightquot; bildo
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2008/009467.php">Pli komforte labori per
          komputilo</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">t&aacute;jk&eacute;p</trd> 
    <trd lng="ru">&c_p;&c_je;&c_j;&c_z;&c_a;&c_zh;</trd>
  </snc>
  <snc mrk="pejzag.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Momenta tuto de la kondi&ccirc;oj kaj agantoj en iu socia
      medio, agadkampo, sektoro:
      <ekz>
        la nacia inform<tld/>o
        <fnt>
          <aut>C. Dada, trad. J. Rezende Piton</aut>
          <vrk>Lumturo danke al La Reto</vrk>
          <lok><url ref="http://uea.org/pdf/Unesko-Kuriero_2-2017.pdf">
          Unesko-kuriero, 2017-07 &gcirc;is 09 (2)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la politika <tld/>o &scirc;an&gcirc;i&gcirc;is post la lasta balotado
        <fnt>
          <bib>Monato</bib>
          <aut>D. E. Rogers</aut>
          <vrk><url ref="&Monato;/2008/009557.php">Jam komenci&gcirc;is elektoj</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_j;&c_z;&c_a;&c_zh;</trd> 
  <trd lng="bo">&#xF66;&#xF0B;&#xF42;&#xF51;&#xF0B;&#xF54;&#xF0B;</trd>
  <trdgrp lng="br">
    <trd>dremmvro</trd>,
    <trd>gweledva</trd>
  </trdgrp>
  <trd lng="de">Landschaft</trd>
  <trd lng="en">landscape</trd>
  <trd lng="es">paisaje</trd>
  <trd lng="fr">paysage</trd>
  <trd lng="nl">landschap</trd>
  <trdgrp lng="pl">
    <trd>krajobraz</trd>,
    <trd>pejza&zdot;</trd>
  </trdgrp>
  <trd lng="pt">paisagem</trd>
  <trd lng="ro">peisaj</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x440;&#x430;&#x454;&#x432;&#x438;&#x434;</trd>,
    <trd>&#x43F;&#x435;&#x439;&#x437;&#x430;&#x436;</trd>
  </trdgrp>
</drv>

<drv mrk="pejzag.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="pejzag.0isto.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      <ref tip="super" cel="pentr.0isto">Pentristo</ref>, kiu pentras precipe <tld/>ojn:
      <ekz>
        <nom>Karel Liebscher</nom> estis &ccirc;e&hcirc;a pentristo &dash; <tld/>isto kaj ilustristo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=663269">Karel Liebscher</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_e;&c_j;&c_z;&c_a;&c_zh;&c_y;&c_s;&c_t;</trd>
    <trd lng="br">livour maezio&ugrave;</trd>
    <trd lng="de">Landschaftsmaler</trd>
    <trd lng="en">landscape painter</trd>
    <trd lng="es">paisajista</trd>
    <trd lng="fr">paysagiste</trd>
    <trd lng="hu">t&aacute;jk&eacute;pfest&odblac;</trd>   
    <trd lng="nl">landschapschilder</trd>
    <trd lng="pl">pejza&zdot;ysta</trd>
    <trd lng="ro">peisagist</trd>
    <trd lng="ru">&c_p;&c_je;&c_j;&c_z;&c_a;&c_zh;&c_i;&c_s;&c_t;</trd>
  </snc>
  <snc mrk="pejzag.0isto.gxardeniso">
    <uzo tip="fak">HOR</uzo>
    <dif>
      &Gcirc;ardenisto, speciali&gcirc;inta pri la aran&gcirc;o de naturaj areoj, parkoj ks:
      <ekz>
        <nom>Gilles Cl&eacute;ment</nom> estas &gcirc;ardenisto, <tld/>isto, botanikisto, entomologo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=206022">Gilles Cl&eacute;ment</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Landschaftsg&auml;rtner</trd>
  </snc>                  
  <trd lng="uk">&#x43F;&#x435;&#x439;&#x437;&#x430;&#x436;&#x438;&#x441;&#x442;</trd>
</drv>
</art>
