<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sakr1.xml,v 1.30 2024/03/24 19:59:59 revo Exp $">
<kap>
  <rad>sakr</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sakr1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sakr1.0i.KOMUNE">
    <dif>
      Esprimi <ref cel="indign.0o">indigno</ref>n a&ubreve; koleron 
      per blasfemaj a&ubreve; nedecaj vortoj:
      <ekz>
        mi atendis pacience, dum ili kriis kaj <tld/>adis
        <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;apitro 1a</lok></fnt>;
      </ekz>
      <ekz>
        mi <tld/>is, bone, plensente kaj suke
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;iesulino <tld/>as pri lia avareco kaj foriras
        <fnt>
          <bib>KrB</bib>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;korpo de porko!&rightquot; <tld/>is unu, &leftquot;mi faligis &gcirc;in&rightquot;
        <fnt>
          <bib>ChC</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="blasfe.0i">blasfemi</ref>,
      <ref cel="damn.0i">damni</ref>,
      <ref cel="ben.mal0i">malbeni</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_ja;&c_c;&c_mol;</trd>, 
    <trd>&c_k;&c_l;&c_ja;&c_s;&c_mol;&c_c;&c_ib;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_s;&c_u;&c_v;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>kl&iacute;t</trd>,
    <trd>l&aacute;te&rcaron;it</trd>,
    <trd>nad&aacute;vat</trd>,
    <trd>sakrovat</trd>
  </trdgrp>
  <trd lng="de">fluchen</trd>
  <trd lng="en">swear</trd>
  <trd lng="es">maldecir</trd>
  <trdgrp lng="fr">
    <trd>dire des gros mots</trd>,
    <trd>jurer<klr tip="amb"> (dire des gros mots)</klr></trd>,
    <trd>sacrer<klr tip="amb"> (dire des gros mots)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&aacute;tkoz&oacute;dik</trd>,
    <trd>szents&eacute;gel</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>imprecare</trd>,
    <trd>bestemmiare</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x306E;&#x306E;&#x3057;&#x308B;</trd>,
    <trd>&#x60AA;&#x614B;&#x3092;&#x3064;&#x304F;  <pr>&#x3042;&#x304F;&#x305F;&#x3044;&#x3092;&#x3064;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">vloeken</trd>
  <trdgrp lng="pl">
    <trd>kl&aogonek;&cacute;</trd>,
    <trd>przeklina&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>praguejar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_u;&c_g;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_k;&c_v;&c_je;&c_r;&c_n;&c_o;&c_s;&c_l;&c_o;&c_v;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_t;&c_je;&c_r;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>klia&tcaron;</trd>,
    <trd>nad&aacute;va&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">sv&auml;ra</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43B;&#x44F;&c_s;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_r;&#x43E;&#x43A;&#x43B;&c_i;&#x43D;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5492;&#x7F75; <pr>zh&#xF2;um&#xE0;</pr></trd>,
    <trd>&#x5492;&#x9A82; <pr>zh&#xF2;um&#xE0;</pr></trd>,
    <trd>&#x7F75; <pr>m&#xE0;</pr></trd>,
    <trd>&#x9A82; <pr>m&#xE0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sakr1.0o">
  <kap><tld/>o, <var><kap><tld/>a&jcirc;o</kap></var></kap>
  <snc>
    <dif>
      Nedeca <ref tip="super" cel="esprim.0o">esprimo</ref> de kolero:
      <ekz>
        oni povas uzi eksklamojn, <tld/>ojn, k.a., kompreneblajn, 
        &ccirc;ar akceptitajn en lingva normo
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Zajcev</nom> eligis ruslingvan <tld/>a&jcirc;on
        <fnt>
          <bib>CKv</bib>
        </fnt>;
      </ekz>
      <ekz>
        post momenta pripenso &scirc;i finis sian diron per longa nepresebla <tld/>a&jcirc;o
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi &gcirc;ojas, ke tiuj milionoj da <tld/>oj, 
        kiuj &scirc;ar&gcirc;as la vivon de tiu viro, ne
        tondras sur min
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 4</lok>
        </fnt>;
      </ekz>
      <ekz>
        kafo &ndash; dol&ccirc;a kiel knabino en la unua vespero 
        <klr>[&#x2026;]</klr> kaj nigra kiel la
        <tld/>oj de la patrino, kiam &scirc;i ekscias tion
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="sakr.0o">sakro (pelvosto)</ref>
  <trd lng="be">&c_p;&c_r;&c_a;&c_k;&c_l;&c_jo;&c_n;</trd>
  <trd lng="de">Fluch</trd>
  <trdgrp lng="en">
    <trd>expletive</trd>,
    <trd>curse</trd>,
    <trd>bad language</trd>,
    <trd>profanity</trd>,
    <trd>obscenity</trd>
  </trdgrp>
  <trd lng="es">maldici&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>juron</trd>,
    <trd>grossi&egrave;ret&eacute;</trd>
  </trdgrp>
  <trd lng="ja">&#x4ED9;&#x9AA8;  <pr>&#x305B;&#x3093;&#x3053;&#x3064;</pr></trd>
  <trd lng="pl">przekle&nacute;stwo</trd>
  <trd lng="uk">&#x43F;&c_r;&#x43E;&#x43A;&#x43B;&c_ib;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x8A5B;&#x5492; <pr>z&#x1D4;zh&#xF2;u</pr></trd>,
    <trd>&#x8BC5;&#x5492; <pr>z&#x1D4;zh&#xF2;u</pr></trd>,
    <trd>&#x7C97;&#x53E3; <pr>c&#x16B;k&#x1D2;u</pr></trd>,
    <trd>&#x810F;&#x8BDD; <pr>z&#x101;nghu&#xE0;</pr></trd>,
    <trd>&#x9AD2;&#x8A71; <pr>z&#x101;nghu&#xE0;</pr></trd>
  </trdgrp>
</drv>
</art>
