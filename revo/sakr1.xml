<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sakr1.xml,v 1.24 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>sakr</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sakr1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sakr1.0i.KOMUNE">
    <dif>
      Esprimi indignon a&ubreve; koleron per blasfemaj a&ubreve; nedecaj
      vortoj:
      <ekz>
        mi atendis pacience, dum ili kriis kaj <tld/>adis
        <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;apitro 1a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_ja;&c_c;&c_mol;</trd>, 
    <trd>&c_k;&c_l;&c_ja;&c_s;&c_mol;&c_c;&c_ib;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_s;&c_u;&c_v;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>klít</trd>,
    <trd>láteřit</trd>,
    <trd>nadávat</trd>,
    <trd>sakrovat</trd>
  </trdgrp>
  <trd lng="de">fluchen</trd>
  <trd lng="en">swear</trd>
  <trd lng="es">maldecir</trd>
  <trdgrp lng="fr">
    <trd>dire des gros mots</trd>,
    <trd>jurer<klr tip="amb"> (dire des gros mots)</klr></trd>,
    <trd>sacrer<klr tip="amb"> (dire des gros mots)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&aacute;tkoz&oacute;dik</trd>,
    <trd>szents&eacute;gel</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>imprecare</trd>,
    <trd>bestemmiare</trd>
  </trdgrp>
  <trd lng="nl">vloeken</trd>
  <trdgrp lng="pl">
    <trd>kl&aogonek;&cacute;</trd>,
    <trd>przeklina&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>praguejar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_u;&c_g;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_k;&c_v;&c_je;&c_r;&c_n;&c_o;&c_s;&c_l;&c_o;&c_v;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_t;&c_je;&c_r;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kliať</trd>,
    <trd>nadávať</trd>
  </trdgrp>
  <trd lng="sv">sv&auml;ra</trd>
</drv>

<drv mrk="sakr1.0o">
  <kap><tld/>o, <var><kap><tld/>a&jcirc;o</kap></var></kap>
  <snc>
    <dif>
      Nedeca esprimo de kolero.
    </dif>
    <ref tip="hom" cel="sakr.0o">sakro</ref>
  </snc>
  <trdgrp lng="en">
    <trd>expletive</trd>,
    <trd>curse</trd>,
    <trd>bad language</trd>,
    <trd>profanity</trd>,
    <trd>obscenity</trd>
  </trdgrp>
  <trd lng="es">maldici&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>juron</trd>,
    <trd>grossi&egrave;ret&eacute;</trd>
  </trdgrp>
  <trd lng="pl">przekle&nacute;stwo</trd>
</drv>
</art>
