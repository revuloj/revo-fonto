<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: oriole.xml,v 1.11 2022/03/24 15:32:04 revo Exp $">
<kap><rad>orioled</rad>/oj</kap>
<drv mrk="oriole.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="paser.0oformaj">paseroformaj
      birdoj</ref> (<trd lng="la">Oriolidae</trd>):
      <ekz>
        la oriolo <klr>[&#x2026;]</klr> estas la ununura membro de la familio de <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=223836">Oriolo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_v;&c_a;&c_l;&c_g;&c_a;&c_v;&c_y;&c_ja;</trd>,
    <trd>&c_v;&c_ib;&c_sh;&c_n;&c_je;&c_w;&c_k;&c_ib; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  </trdgrp>
  <trd lng="ca">ori&ograve;lids</trd>
  <trd lng="de">Pirole<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="en">orioles<klr tip="ind"> (family)</klr></trd>
  <trd lng="fr">loriots</trd>
  <trdgrp lng="hu">
    <trd>s&aacute;rgarig&oacute;f&eacute;l&eacute;k</trd>,
    <trd>m&aacute;link&oacute;f&eacute;l&eacute;k</trd>
  </trdgrp>
  <trd lng="nl">wielewalen<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pl">wilgi {pl}</trd>
  <trd lng="pt">oriol&iacute;deos</trd>
  <trd lng="ro">orioli</trd>
  <trd lng="ru">&c_i;&c_v;&c_o;&c_l;&c_g;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&#x432;&#x438;&#x432;&#x456;&#x43B;&#x44C;&#x433;&#x43E;&#x432;&#x456;</trd>
</drv>
</art>
