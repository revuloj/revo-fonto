<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: kombajn.xml,v 1.4 2022/10/25 07:54:17 afido Exp $">
    <kap>
      <rad>kombajn</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="kombajn.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="masxin.0o.TEK">Ma&scirc;ino</ref>, kiu rikoltas, dra&scirc;as
      kaj purigas grenon:
      <ekz>
        apud la pajlostrioj de la <tld/>o staras la malnova lokomobilo kun la radoj en
        fosita kava&jcirc;o
        <fnt>
          <aut>Mats Traat</aut>
          <vrk><url ref="http://miresperanto.com/biblioteko/lokomobilo/09.htm">Danco &ccirc;irka&ubreve;
          lokomobilo</url></vrk>
          <lok>Kvina Danco</lok>
        </fnt>;
      </ekz>
      <ekz>
        funkciigo de la ma&scirc;inluaj stacioj <klr>[por]</klr> traktoroj, <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=34811">Kol&hcirc;ozo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/6/66/Lely-M&auml;hdrescher.jpg/640px-Lely-M&auml;hdrescher.jpg"
    prm="CC-BY-SA-3.0"><tld/>o LELY, 1970
        <fnt>
          <aut>Markus Hagenlocher, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Lely-M%C3%A4hdrescher.jpg">&WCW;/File:Lely-M&auml;hdrescher.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="be">&c_k;&c_a;&c_m;&c_b;&c_a;&c_j;&c_n; <klr tip="amb">(&c_s;&c_je;&c_l;&c_mol;&c_s;&c_k;&c_a;&c_g;&c_a;&c_s;&c_p;&c_a;&c_d;&c_a;&c_r;&c_ch;&c_y;)</klr></trd>
    <trd lng="de">M&auml;hdrescher</trd>
    <trd lng="en">combine harvester</trd>
    <trd lng="fr">moissonneuse-batteuse</trd>
  </snc>
  <trd lng="uk">&#x43A;&#x43E;&#x43C;&c_b;&c_a;&c_j;&#x43D;</trd>
</drv>

  </art>

