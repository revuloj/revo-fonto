<art mrk="$Id: pumik.xml,v 1.20 2023/11/02 09:27:11 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pumik</rad>/o
</kap>
<drv mrk="pumik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Porori&ccirc;a malpeza vulkan&scirc;tono:
      <ekz>
        la <tld/>oj &jcirc;etitaj en alton de pli ol 50 km falis en la sekvaj
        dek
        tagoj ene de radiuso de pli ol 5000 km<fnt><aut>Evgeni
        Georgiev</aut>: <vrk>Vulkanoj</vrk>, <bib>Monato</bib><lok>jaro
        2001a, numero 2a, p. 22a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_m;&c_z;&c_a;</trd>
  <trd lng="bg">&c_p;&c_je;&c_m;&c_z;&c_a;</trd>
  <trd lng="cs">pemza</trd>
  <trd lng="de">Bimsstein</trd>
  <trd lng="en">pumice</trd>
  <trdgrp lng="fr">
    <trd>ponce<klr> (min&eacute;ral)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>habk&odblac;</trd>,
    <trd>tajt&eacute;kk&odblac;</trd>,
    <trd>horzsak&odblac;</trd>
  </trdgrp>
  <trd lng="ja">&#x8EFD;&#x77F3;  <pr>&#x304B;&#x308B;&#12356;&#12375;</pr></trd>
  <trd lng="nl">puimsteen</trd>
  <trd lng="pl">pumeks</trd>
  <trdgrp lng="pt">
    <trd>p&uacute;mice</trd>,
    <trd>pedra-pomes</trd>
  </trdgrp>
  <trd lng="ro">piatr&abreve; ponce</trd>
  <trd lng="ru">&c_p;&c_je;&c_m;&c_z;&c_a;</trd>
  <trd lng="sk">pemza</trd>
  <trd lng="uk">&#x43F;&c_je;&#x43C;&c_z;&c_a;</trd>
</drv>
</art>
