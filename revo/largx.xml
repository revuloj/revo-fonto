<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lar&gcirc;</rad>/a
</kap>
<drv mrk="largx.0a">
  <kap>
    <ofc>*</ofc><tld/>a
  </kap>
  <snc>
    <dif>
      Granda la&ubreve; la dimensio <tld/>o;
      havanta difinitan grandon la&ubreve; tiu dimensio:
      <ekz>
        <tld/>a rivero, strato, lito, mano, pordo, brusto, fasado de domo;
      </ekz>
      <ekz>
        &ccirc;apelo kun <tld/>aj randoj;
      </ekz>
      <ekz>
        rubando <tld/>a je du centimetroj;
      </ekz>
      <ekz>
        vojeto du pa&scirc;ojn <tld/>a;
      </ekz>
      <ekz>
        tiu &scirc;tofo havas unu metron da <tld/>o.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Vasta, multampleksa:
      <ekz>
        <tld/>a <klr>(ne al&gcirc;ustigita)</klr> vesto;
      </ekz>
      <ekz>
        <tld/>a <klr>(vaste komprenanta; tolerema)</klr> spirito;
      </ekz>
      <ekz>
        <tld/>a <klr>(senskrupula)</klr> konscienco<fnt>Z</fnt>;
      </ekz>
      <ekz>
        vorto uzata <ind><tld/>(a)sence</ind>;
        <trd lng="be">&c_u; &c_sh;&c_y;&c_r;&c_o;&c_k;&c_ib;&c_m; 
<ind>&c_s;&c_e;&c_n;&c_mol;&c_s;&c_je;</ind></trd>
        <trd lng="fr">au <ind>sens</ind> large</trd>
        <trd lng="hu">t&aacute;g <ind>&eacute;rtelemben</ind></trd>       
      </ekz>
      <ekz>el fremda ledo oni tran&ccirc;as <tld/>e
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        <ind>pli<tld/>igi</ind> robon;
        <trd lng="be">&c_p;&c_a;&c_sh;&c_y;&c_r;&c_a;&c_c;&c_mol;</trd>
        <trd lng="br">ledanaat</trd>
        <trd lng="fr">&eacute;largir</trd>
        <trd lng="hu">b&odblac;v&iacute;t</trd>       
      </ekz>
      <ekz>
        <ind><tld/>igi</ind> la debaton;
        <trd lng="be">&c_p;&c_a;&c_sh;&c_y;&c_r;&c_a;&c_c;&c_mol;</trd>
        <trd lng="fr">&eacute;largir</trd>
        <trd lng="hu">kiterjeszt</trd>       
      </ekz>
      <ekz>
        tiam la ebloj de teknika progreso <tld/>i&gcirc;os.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_r;&c_o;&c_k;&c_ib;</trd>
  <trd lng="br">ledan</trd>
  <trd lng="cs">&scaron;irok&yacute;</trd>
  <trd lng="de">breit</trd>
  <trdgrp lng="el">
    <trd>&pi;&lambda;&alfa;&tau;&ypsilon_ton;&sigma_fina;</trd>,
    <trd>&phi;&alfa;&rho;&delta;&ypsilon_ton;&sigma_fina;</trd>
  </trdgrp>
  <trd lng="en">wide</trd>
  <trd lng="es">ancho</trd>
  <trdgrp lng="fr">
    <trd>ample</trd>,
    <trd>coulant<klr tip="ind"> (&agrave; l'esprit large)</klr></trd>,
    <trd>g&eacute;n&eacute;reux</trd>,
    <trd>large</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&eacute;les</trd>,
    <trd>b&odblac;</trd>,
    <trd>t&aacute;g</trd>,
    <trd>t&aacute;gas</trd>,
    <trd>nagyar&aacute;ny&uacute;</trd>
  </trdgrp> 
  <trd lng="it">largo</trd>
  <trd lng="nl">breed</trd>
  <trd lng="pt">largo</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_i;&c_r;&c_o;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_o;&c_b;&c_sh;&c_i;&c_r;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="tr">geni&scedil;</trd>
</drv>

<drv mrk="largx.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="largx.0o.dimensio">
    <dif>
      <ref tip="super" cel="dimens.0o.ampleksa">Dimensio</ref>
      mezurata la&ubreve; direkto orta al
      <ref cel="long.0o.dimensio">longo</ref> a&ubreve;
      <ref cel="alt.0o.dimensio">alto</ref>:
      <ekz>
        tricent ulnoj estu la longo de la arkeo, kvindek ulnoj &gcirc;ia
        <tld/>o, kaj tridek ulnoj &gcirc;ia alto
        <fnt><bib>M</bib>, <lok>&Gen; 6:15</lok></fnt>;
      </ekz>
      <ekz>
        li ekmigris pluen al unu el la plej mal<tld/>aj stratoj, kies
        <tld/>o apena&ubreve; sufi&ccirc;is por bone &scirc;ar&gcirc;ita 
azeno
        <fnt><bib>Fab1</bib>, <lok>Kupra porko</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">ARK</uzo>
        grado de <tld/>o
        <klr>
        (distanco de iu loko al la terekvatoro, mezurata la&ubreve;longe 
de la
        meridiano de tiu loko.
        <ref tip="vid" cel="latitu.0o">latitudo</ref>)
        </klr>.
      </ekz>
    </dif>
    <trd lng="fr">largeur<klr tip="ind"> (dimension)</klr></trd>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_r;&c_y;&c_n;&c_ja;</trd>
  <trdgrp lng="br">
    <trd>led</trd>,
    <trd>ledander</trd>
  </trdgrp>
  <trd lng="de">Breite</trd>
  <trd lng="en">width</trd>
  <trd lng="es">anchura</trd>
  <trd lng="hu">sz&eacute;less&eacute;g <klr 
tip="ind">(m&eacute;ret)</klr></trd> 
  <trd lng="ru">&c_sh;&c_i;&c_r;&c_i;&c_n;&c_a;</trd>
</drv>

<drv mrk="largx.0eco">
  <kap>
    <tld/>eco
  </kap>
  <snc>
    <dif>
      Eco de tio, kio estas <tld/>a:
      <ekz>
        <tld/>eco de &scirc;ultroj;
      </ekz>
      <ekz>
        la <tld/>eco de la foso malhelpis la transsalton<fnt>
        <bib>PIV1</bib></fnt>;
      </ekz>
      <ekz>
        la <tld/>eco de la ideoj.
      </ekz>
    </dif>
    <trd lng="de">Breitsein</trd>
    <trd lng="en">wideness</trd>
    <trdgrp lng="fr">
      <trd>largesse</trd>,
      <trd>largeur<klr tip="ind"> (fait d'&ecirc;tre large)</klr></trd>
    </trdgrp>   
  </snc>
  <snc>
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="dif" cel="largx.0o.dimensio"><tld lit="L"/>o
      <sncref ref="largx.0o.dimensio"/></ref>:
      <ekz>
        na&ubreve; ulnoj estas &gcirc;ia longeco kaj kvar ulnoj estas
        &gcirc;ia <tld/>eco
        <fnt><bib>M</bib>, <lok>&Rea; 3:11</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Breite</trd>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_r;&c_y;&c_n;&c_ja;</trd>
  <trd lng="br">ledander</trd>
  <trdgrp lng="el">
    <trd>&pi;&lambda;&alfa_ton;&tau;&omikron;&sigma_fina;</trd>,
    <trd>&phi;&alfa_ton;&rho;&delta;&omikron;&sigma_fina;</trd>
  </trdgrp>
  <trd lng="hu">sz&eacute;less&eacute;g</trd> 
  <trd lng="pt">largura</trd>
  <trd lng="ru">&c_sh;&c_i;&c_r;&c_i;&c_n;&c_a;</trd>
</drv>

<drv mrk="largx.mal0a">
  <kap>
    mal<tld/>a
  </kap>
  <snc>
    <dif>
      Malgranda la&ubreve; &gcirc;ia lar&gcirc;o; havanta &gcirc;enajn
      limojn:
      <ekz>
        mal<tld/>a rubando, vojeto, pordo;
      </ekz>
      <ekz>
        mal<tld/>a vesto, cerbo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_u;&c_z;&c_k;&c_ib;</trd>,
    <trd>&c_a;&c_b;&c_m;&c_je;&c_zh;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>enk</trd>,
    <trd>strizh</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>eng</trd>,
     <trd>schmal</trd>
  </trdgrp>
  <trd lng="el">&sigma;&tau;&epsilon;&ny;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">narrow</trd>
  <trdgrp lng="es">
    <trd>angosto</trd>,
    <trd>estrecho</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;troit</trd>,
    <trd>mince<klr tip="ind"> (&eacute;troit)</klr></trd>,
    <trd>limit&eacute;<klr tip="ind"> (&eacute;troit)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>keskeny</trd>,
    <trd>sz&udblac;k</trd>
  </trdgrp> 
  <trd lng="pt">estreito</trd>
  <trd lng="ru">&c_u;&c_z;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
