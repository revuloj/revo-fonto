<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: largx.xml,v 1.33 2020/01/15 10:10:25 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lar&gcirc;</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="largx.0a">
  <kap>
    <ofc>*</ofc><tld/>a <fnt><bib>F</bib>, <lok>Universala Vortaro, <url
    ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-2487">lar&gcirc;'</url></lok></fnt>
  </kap>
  <snc>
    <dif>
      Granda la&ubreve; la dimensio <tld/>o;
      havanta difinitan grandon la&ubreve; tiu dimensio:
      <ekz>
        <tld/>a strato, lito, mano, pordo, brusto, fasado de
        domo;
      </ekz>
      <ekz>
        <tld/>a kvieta rivero
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a &ccirc;apelo
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;apelo kun <tld/>aj randoj;
      </ekz>
      <ekz>
        rubando <tld/>a je du centimetroj;
      </ekz>
      <ekz>
        vojeto du pa&scirc;ojn <tld/>a;
      </ekz>
      <ekz>
        akvolilioj kun siaj <tld/>aj verdaj folioj
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        faru &ccirc;irka&ubreve; &gcirc;i man<tld/>an listelon
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 25:25</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">ample</trd>
    <trdgrp lng="fa">
      <trd>&f_p;&a_h;&a_n;</trd>
      <trd>&a_ayn;&a_r;&f_y;&a_D;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Vasta, multampleksa:
      <ekz>
        promeso estas <tld/>a, plenumo estas &scirc;ar&gcirc;a
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a <klr>(ne al&gcirc;ustigita)</klr> vesto;
      </ekz>
      <ekz>
        <tld/>a <klr>(vaste komprenanta; tolerema)</klr> spirito;
      </ekz>
      <ekz>
        havi <tld/>an <klr>(senskrupulan)</klr> konsciencon
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>vivi <tld/>e kaj lukse</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
        <trd lng="de">auf gro&szlig;em <ind>Fu&szlig;</ind> leben</trd>
      </ekz>
      <ekz>
        vorto uzata <ind><tld/>(a)sence</ind>;
        <trd lng="be">&c_u; &c_sh;&c_y;&c_r;&c_o;&c_k;&c_ib;&c_m; 
        <ind>&c_s;&c_e;&c_n;&c_mol;&c_s;&c_je;</ind></trd>
        <trdgrp lng="fa">
          <trd>&a_d;&a_r; &a_m;&a_f;&a_h;&a_w;&a_m;
          <ind>&a_w;&a_s;&f_y;&a_ayn;</ind></trd>
          <trd>&a_d;&a_r; &a_m;&a_ayn;&a_n;&a_A;&f_y;
          <ind>&a_w;&a_s;&f_y;&a_ayn;</ind></trd>
        </trdgrp>
        <trd lng="fr">au <ind>sens</ind> large</trd>
        <trd lng="hu">t&aacute;g <ind>&eacute;rtelemben</ind></trd>     
         
      </ekz>
      <ekz>
        <ind>pli<tld/>igi</ind> robon;
        <trd lng="be">&c_p;&c_a;&c_sh;&c_y;&c_r;&c_a;&c_c;&c_mol;</trd>
        <trd lng="br">ledanaat</trd>
        <trd lng="fa">&f_g;&a_sh;&a_A;&a_d;
        &f_k;&a_r;&a_d;&a_n;</trd>
        <trd lng="fr">&eacute;largir</trd>
        <trd lng="hu">b&odblac;v&iacute;t</trd>       
      </ekz>
      <ekz>
        <ind><tld/>igi</ind> la debaton;
        <trd lng="be">&c_p;&c_a;&c_sh;&c_y;&c_r;&c_a;&c_c;&c_mol;</trd>
        <trd lng="fa">&a_w;&a_s;&a_ayn;&a_t;
        &a_d;&a_A;&a_d;&a_n;</trd>
        <trd lng="fr">&eacute;largir</trd>
        <trd lng="hu">kiterjeszt</trd>       
      </ekz>
      <ekz>
        tiam la ebloj de teknika progreso <tld/>i&gcirc;os.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>vast</trd>,
      <trd>extens</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&f_p;&a_h;&a_n;&a_A;&a_w;&a_r;</trd>
      <trd>&a_w;&a_s;&f_y;&a_ayn;</trd>
      <trd>&f_g;&a_sh;&a_A;&a_d;</trd>
      <trd>&f_g;&a_sh;&a_A;&a_d;&a_h;&zwnj;&a_d;&a_s;&a_t;</trd>
      <trd>&f_g;&a_sh;&a_A;&a_d;&a_h;&zwnj;&a_r;&a_w;&a_A;&a_n;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_r;&c_o;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_sh;&c_i;&c_r;&c_o;&c_k;</trd>
  <trd lng="br">ledan</trd>
  <trd lng="ca">ample</trd>
  <trd lng="cs">&scaron;irok&yacute;</trd>
  <trd lng="de">breit</trd>
  <trdgrp lng="el">
    <trd>&pi;&lambda;&alfa;&tau;&ypsilon_ton;&sigma_fina;</trd>,
    <trd>&phi;&alfa;&rho;&delta;&ypsilon_ton;&sigma_fina;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>broad</trd>,
    <trd>wide</trd>
  </trdgrp>
  <trd lng="es">ancho</trd>
  <trdgrp lng="fr">
    <trd>ample</trd>,
    <trd>coulant<klr tip="ind"> (&agrave; l'esprit large)</klr></trd>,
    <trd>g&eacute;n&eacute;reux</trd>,
    <trd>large</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&eacute;les</trd>,
    <trd>b&odblac;</trd>,
    <trd>t&aacute;g</trd>,
    <trd>t&aacute;gas</trd>,
    <trd>nagyar&aacute;ny&uacute;</trd>
  </trdgrp> 
  <trd lng="it">largo</trd>
  <trd lng="nl">breed</trd>
  <trd lng="pt">largo</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_i;&c_r;&c_o;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_o;&c_b;&c_sh;&c_i;&c_r;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="tr">geni&scedil;</trd>
</drv>

<drv mrk="largx.0e">
  <kap>
    <tld/>e
  </kap>
  <snc>
    <dif>
      Abunde, grandkvante, tiel ke &gcirc;i estas <tld/>a:
      <ekz>
        el fremda ledo oni tran&ccirc;as <tld/>e
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e malfermi&gcirc;is mia bu&scirc;o kontra&ubreve; miaj malamikoj
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 2:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e malfermante la okulojn &ccirc;e la vido de la nova amaso da
        mono
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fenestro <tld/>e malfermi&gcirc;is
        <fnt>
          <bib>Fab1</bib>
          <lok>Persista stana soldato</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la arbo]</klr> etendis <tld/>e siajn bran&ccirc;ojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Abio</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">weit <klr>(breit)</klr></trd>
</drv>


<drv mrk="largx.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="largx.0o.dimensio">
    <dif>
      <ref tip="super" cel="dimens.0o.ampleksa">Dimensio</ref>
      mezurata la&ubreve; direkto orta al
      <ref cel="long.0o.dimensio">longo</ref> a&ubreve;
      <ref cel="alt.0o.dimensio">alto</ref>:
      <ekz>
        tricent ulnoj estu la longo de la arkeo, kvindek ulnoj &gcirc;ia
        <tld/>o, kaj tridek ulnoj &gcirc;ia alto
        <fnt><bib>MT</bib>, <lok>&Gen; 6:15</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>o de &ccirc;iu tapi&scirc;o estu kvar ulnoj
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 26:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        manstre&ccirc;o estu &gcirc;ia longo, kaj manstre&ccirc;o &gcirc;ia <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 28:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ekmigris pluen al unu el la plej mal<tld/>aj stratoj, kies
        <tld/>o apena&ubreve; sufi&ccirc;is por bone &scirc;ar&gcirc;ita azeno
        <fnt><bib>Fab1</bib>, <lok>Kupra porko</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">ARK</uzo>
        grado de <tld/>o
        <klr>
        (distanco de iu loko al la terekvatoro, mezurata la&ubreve;longe
        de la meridiano de tiu loko
        <ref tip="vid" cel="latitu.0o">latitudo</ref>)
        </klr>.
      </ekz>
    </dif>
    <trd lng="fa">&f_p;&a_h;&a_n;&a_A;</trd>
    <trd lng="fr">largeur<klr tip="ind"> (dimension)</klr></trd>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_r;&c_y;&c_n;&c_ja;</trd>
  <trd lng="bg">&c_sh;&c_i;&c_r;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="br">
    <trd>led</trd>,
    <trd>ledander</trd>
  </trdgrp>
  <trd lng="ca">ampl&agrave;ria</trd>
  <trd lng="de">Breite</trd>
  <trd lng="en">width</trd>
  <trd lng="es">anchura</trd>
  <trd lng="hu">sz&eacute;less&eacute;g <klr tip="ind">(m&eacute;ret)</klr></trd> 
  <trd lng="ru">&c_sh;&c_i;&c_r;&c_i;&c_n;&c_a;</trd>
</drv>

<drv mrk="largx.0eco">
  <kap>
    <tld/>eco
  </kap>
  <snc>
    <dif>
      Eco de tio, kio estas <tld/>a:
      <ekz>
        <tld/>eco de &scirc;ultroj;
      </ekz>
      <ekz>
        la <tld/>eco de la foso malhelpis la transsalton<fnt>
        <bib>PIV1</bib></fnt>;
      </ekz>
      <ekz>
        Mi ne donos al vi el ilia lando e&ccirc; <tld/>econ de plando
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 2:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>eco de la ideoj.
      </ekz>
    </dif>
    <trd lng="de">Breitsein</trd>
    <trd lng="en">wideness</trd>
    <trdgrp lng="fa">
      <trd>&f_p;&a_h;&a_n;&f_y;</trd>
      <trd>&f_p;&a_h;&a_n;&a_A;&a_w;&a_r;&f_y;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>largesse</trd>,
      <trd>largeur<klr tip="ind"> (fait d'&ecirc;tre large)</klr></trd>
    </trdgrp>   
  </snc>
  <snc>
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="dif" cel="largx.0o.dimensio"><tld lit="L"/>o
      <sncref ref="largx.0o.dimensio"/></ref>:
      <ekz>
        na&ubreve; ulnoj estas &gcirc;ia longeco kaj kvar ulnoj estas
        &gcirc;ia <tld/>eco
        <fnt><bib>MT</bib>, <lok>&Rea; 3:11</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Breite</trd>
    <trd lng="fa">&f_p;&a_h;&a_n;&a_A;</trd>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_r;&c_y;&c_n;&c_ja;</trd>
  <trdgrp lng="bg">
    <trd>&c_sh;&c_i;&c_r;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_sh;&c_i;&c_r;&c_o;&c_ch;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="br">ledander</trd>
  <trd lng="ca">amplitud</trd>
  <trd lng="de">Breite</trd>
  <trdgrp lng="el">
    <trd>&pi;&lambda;&alfa_ton;&tau;&omikron;&sigma_fina;</trd>,
    <trd>&phi;&alfa_ton;&rho;&delta;&omikron;&sigma_fina;</trd>
  </trdgrp>
  <trd lng="es">amplitud</trd>
  <trd lng="hu">sz&eacute;less&eacute;g</trd> 
  <trd lng="pt">largura</trd>
  <trd lng="ru">&c_sh;&c_i;&c_r;&c_i;&c_n;&c_a;</trd>
</drv>

<drv mrk="largx.mal0a">
  <kap>
    <ofc>*</ofc>mal<tld/>a
  </kap>
  <snc>
    <dif>
      Malgranda la&ubreve; &gcirc;ia lar&gcirc;o; havanta &gcirc;enajn
      limojn:
      <ekz>
        mal<tld/>a rubando, strato;
      </ekz>
      <ekz>
        mal<tld/>a kanalo
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>a strio
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>a vesto, cerbo;
      </ekz>
      <ekz>
        li estas tiel dika, ke li ne povas trairi tra nia mal<tld/>a
        pordo
        <fnt><bib>F</bib>, <lok>&FE; 33</lok></fnt>;
      </ekz>
      <ekz>
        mal<tld/>a vojeto kondukas tra tiu &ccirc;i kampo al nia domo
        <fnt><bib>F</bib>, <lok>&FE; 38</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_u;&c_z;&c_k;&c_ib;</trd>,
    <trd>&c_a;&c_b;&c_m;&c_je;&c_zh;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_t;&c_je;&c_s;&c_je;&c_n;</trd>
  <trdgrp lng="br">
    <trd>enk</trd>,
    <trd>strizh</trd>
  </trdgrp>
  <trd lng="ca">estret</trd>
  <trdgrp lng="de">
     <trd>eng</trd>,
     <trd>schmal</trd>
  </trdgrp>
  <trd lng="el">&sigma;&tau;&epsilon;&ny;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">narrow</trd>
  <trdgrp lng="es">
    <trd>angosto</trd>,
    <trd>estrecho</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_b;&a_A;&a_r;&f_y;&f_k;</trd>
    <trd>&a_t;&a_n;&f_g;</trd>
    <trd>&a_t;&a_n;&f_g;&zwnj;&a_n;&a_Z;&a_r;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;troit</trd>,
    <trd>mince<klr tip="ind"> (&eacute;troit)</klr></trd>,
    <trd>limit&eacute;<klr tip="ind"> (&eacute;troit)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>keskeny</trd>,
    <trd>sz&udblac;k</trd>
  </trdgrp> 
  <trd lng="pt">estreito</trd>
  <trd lng="ru">&c_u;&c_z;&c_k;&c_i;&c_j;</trd>
</drv>


<drv mrk="largx.laux0e">
  <kap>
    <ofc>*</ofc> la&ubreve;<tld/>e <fnt><bib>F</bib>, <lok>Universala Vortaro,
    <url
    ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5085">la&ubreve;'lar&gcirc;'</url></lok></fnt>
  </kap>
  <snc>
    <dif>
      Transverse:
      <ekz>
        la&ubreve;<tld/>e de la korto, sur la flanko okcidenta, estu kurtenoj
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 27:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        trairu la landon la&ubreve;longe kaj la&ubreve;<tld/>e
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 13:17</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">quer</trd>
  <trdgrp lng="en">
    <trd>across</trd>,
    <trd>in width</trd>,
  </trdgrp>
  <trd lng="es">de ancho</trd>
  <trdgrp lng="fr">
    <trd>&agrave; travers</trd>,
    <trd>en large</trd>,
  </trdgrp>
 </drv>
</art>
