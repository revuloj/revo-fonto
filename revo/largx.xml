  <trd lng="lo">&#xEA2;&#xEC8;&#xEB2;&#xE87;&#xE81;&#xEA7;&#xEC9;&#xEB2;&#xE87;&#xE82;&#xEA7;&#xEB2;&#xE87;</trd>
  <trd lng="ml">&#xD35;&#xD3F;&#xD36;&#xD3E;&#xD32;&#xD2E;&#xD3E;&#xD2F;</trd>
  <trd lng="mr">&#x92C;&#x94D;&#x930;&#x949;&#x921;</trd>
  <trd lng="my">&#x1000;&#x103B;&#x101A;&#x103A;&#x1015;&#x103C;&#x1014;&#x1037;&#x103A;</trd>
