      <trd>&f_g;&a_sh;&a_A;&a_d;&a_h;&#x200C;&a_r;&a_w;&a_A;&a_n;</trd>
    </trdgrp>
  </snc>
  <trd lng="af">bre&#xEB;</trd>
  <trd lng="am">&#4656;&#x134A;</trd>
  <trd lng="ar">&a_w;&a_A;&a_s;&a_ayn;</trd>
