  <ofc>*</ofc>
  <rad>lar&gcirc;</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="largx.0a">
  <kap>
    <ofc>*</ofc><tld/>a <fnt><bib>F</bib>, <lok>Universala Vortaro, <url
    ref="&AdE;/fundamento/universala_vortaro.html#av-2487">lar&gcirc;'</url></lok></fnt>
  </kap>
  <snc>
    <dif>
      Granda la&ubreve; la dimensio <tld/>o;
      havanta difinitan grandon la&ubreve; tiu dimensio:
      <ekz>
        <tld/>a strato, lito, mano, pordo, brusto, fasado de
        domo;
      </ekz>
      <ekz>
        <tld/>a kvieta rivero
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a &ccirc;apelo
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;apelo kun <tld/>aj randoj;
      </ekz>
      <ekz>
        rubando <tld/>a je du centimetroj;
      </ekz>
      <ekz>
        vojeto du pa&scirc;ojn <tld/>a;
      </ekz>
      <ekz>
        akvolilioj kun siaj <tld/>aj verdaj folioj
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        faru &ccirc;irka&ubreve; &gcirc;i man<tld/>an listelon
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 25:25</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">ample</trd>
    <trdgrp lng="fa">
      <trd>&f_p;&a_h;&a_n;</trd>
      <trd>&a_ayn;&a_r;&f_y;&a_D;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Vasta, multampleksa:
      <ekz>
        promeso estas <tld/>a, plenumo estas &scirc;ar&gcirc;a
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a <klr>(ne al&gcirc;ustigita)</klr> vesto;
      </ekz>
      <ekz>
        <tld/>a <klr>(vaste komprenanta; tolerema)</klr> spirito;
      </ekz>
      <ekz>
        havi <tld/>an <klr>(senskrupulan)</klr> konsciencon
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>vivi <tld/>e kaj lukse</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
        <trd lng="de">auf gro&szlig;em <ind>Fu&szlig;</ind> leben</trd>
      </ekz>
      <ekz>
        vorto uzata <ind><tld/>(a)sence</ind>;
        <trd lng="be">&c_u; &c_sh;&c_y;&c_r;&c_o;&c_k;&c_ib;&c_m;
        <ind>&c_s;&c_e;&c_n;&c_mol;&c_s;&c_je;</ind></trd>
        <trdgrp lng="fa">
          <trd>&a_d;&a_r; &a_m;&a_f;&a_h;&a_w;&a_m;
          <ind>&a_w;&a_s;&f_y;&a_ayn;</ind></trd>
          <trd>&a_d;&a_r; &a_m;&a_ayn;&a_n;&a_A;&f_y;
          <ind>&a_w;&a_s;&f_y;&a_ayn;</ind></trd>
        </trdgrp>
        <trd lng="fr">au <ind>sens</ind> large</trd>
        <trd lng="hu">t&aacute;g <ind>&eacute;rtelemben</ind></trd>

      </ekz>
      <ekz>
        <ind>pli<tld/>igi</ind> robon;
        <trd lng="be">&c_p;&c_a;&c_sh;&c_y;&c_r;&c_a;&c_c;&c_mol;</trd>
        <trd lng="br">ledanaat</trd>
        <trd lng="fa">&f_g;&a_sh;&a_A;&a_d;
        &f_k;&a_r;&a_d;&a_n;</trd>
        <trd lng="fr">&eacute;largir</trd>
        <trd lng="hu">b&odblac;v&iacute;t</trd>
        <trd lng="pl">poszerzy&cacute;</trd>
      </ekz>
      <ekz>
        <ind><tld/>igi</ind> la debaton;
        <trd lng="be">&c_p;&c_a;&c_sh;&c_y;&c_r;&c_a;&c_c;&c_mol;</trd>
        <trd lng="fa">&a_w;&a_s;&a_ayn;&a_t;
        &a_d;&a_A;&a_d;&a_n;</trd>
        <trd lng="fr">&eacute;largir</trd>
        <trd lng="hu">kiterjeszt</trd>
        <trd lng="pl">poszerzy&cacute;</trd>
      </ekz>
      <ekz>
        tiam la ebloj de teknika progreso <tld/>i&gcirc;os.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>vast</trd>,
      <trd>extens</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&f_p;&a_h;&a_n;&a_A;&a_w;&a_r;</trd>
      <trd>&a_w;&a_s;&f_y;&a_ayn;</trd>
      <trd>&f_g;&a_sh;&a_A;&a_d;</trd>
