<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ajl.xml,v 1.41 2022/10/16 16:51:57 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>ajl</rad>/o
</kap>

<drv mrk="ajl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="ajl.0o.genro">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="liliac.0oj">liliacoj</ref>
      (<trd lng="la">Allium</trd>).
    </dif>
    <trdgrp lng="be">
      <trd>&c_c;&c_y;&c_b;&c_u;&c_l;&c_ja;</trd>,
      <trd>&c_ch;&c_a;&c_s;&c_n;&c_o;&c_k;</trd>
    </trdgrp>
    <trd lng="br">kignen-gouez</trd>
    <trd lng="de">Lauch</trd>
    <trd lng="hu">hagyma</trd>
    <trd lng="nl">look</trd>
    <trd lng="pl">czosnek</trd>
  </snc>
  <snc mrk="ajl.0o.specio">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:botanikaj_specioj">Specio</ref> 
      de <ref tip="super" cel="ajl.0o.genro"><tld/>o
      <sncref ref="ajl.0o.genro"/></ref>
      (<trd lng="la">Allium sativum</trd>),
      uzata kiel spica&jcirc;o kaj kuracilo:
      <ekz>
        sur &ccirc;iu nove konstruita domo ili pendigas marioneton, <tld/>o-bulbaron a&ubreve; huf-fera&jcirc;on
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2006/008930.php">Pagana Tirano</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_ch;&c_a;&c_s;&c_n;&c_o;&c_k;</trd>
    <trd lng="bg">&c_ch;&c_je;&c_s;&c_malmol;&c_n;</trd>
    <trd lng="br">kignen</trd>
    <trd lng="ca">all</trd>
    <trd lng="de">Knoblauch</trd>
    <trd lng="en">garlic</trd>
    <trd lng="es">ajo</trd>
    <trd lng="fr">ail</trd>
    <trd lng="gl">allo</trd>
    <trd lng="hu">fokhagyma</trd>
    <trd lng="id">bawang putih</trd>
    <trd lng="it">aglio</trd>
    <trd lng="nl">knoflook</trd>
    <trd lng="pl">czosnek</trd>
    <trd lng="tr">sar&inodot;msak</trd>
    <trd lng="ru">&c_ch;&c_je;&c_s;&c_n;&c_o;&c_k;</trd>
    <trd lng="sk">cesnak</trd>
    <trd lng="sv">vitl&ouml;k</trd>
  </snc>
  <snc mrk="ajl.0o.bulbo">
    <dif>
      La bulbo, bulbero de tiu vegeta&jcirc;o, uzata kiel <ref tip="lst" cel="spic.0ajxo"
      lst="voko:spicoj">spica&jcirc;o</ref>:
      <ekz>
        ni man&gcirc;is <klr>[&#8230;]</klr> la poreojn kaj la bulbojn kaj la <tld/>ojn  
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 11:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        li forman&gcirc;is sekan kukon frotitan per <tld/>o kaj eltrinkis acidan bieron
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni povas konsili al li man&gcirc;i bulbon, a&ubreve; <tld/>on a&ubreve; ion similan
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        trafis la nazon forta odoro de <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2010/009961.php">Kurac-ajlo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd><klr>[ajla bulbo] </klr>Knoblauchzwiebel</trd>,
      <trd><klr>[ajla bulbero] </klr>Knoblauchzehe</trd>
    </trdgrp>
  </snc>
  <trd lng="bo">&t_sa;&t_gata;&t_naro;&t_ga;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trd lng="cs">&ccaron;esnek</trd>
  <trd lng="he">&shin;&vav;&fmem;</trd>
  <trd lng="pt">alho</trd>
  <trd lng="uk">&c_ch;&c_a;&c_s;&c_n;&c_i;&c_k;</trd>
</drv>

<drv mrk="ajl.urs0o">
  <kap>
    urs<tld/>o,
    <var><kap>ursa <tld/>o</kap></var>
  </kap>
  <snc>
    <dif>
      Sova&gcirc;a specio de <tld/>o (<trd lng="la">Allium ursinum</trd>), kutima en arbaroj de E&ubreve;ropo kaj
      kelkaj aziaj regionoj:
      <ekz>
        jen kuko kun rafanfolietoj kaj urs<tld/>o
        <fnt>
          <vrk><url ref="http://esperanto-gironde.fr/wp-content/uploads/2019/05/LEGI-158_Ekr.pdf"
              >Kunveno en Cenon Palmer, la 21an de Marto 2019</url></vrk>
          <lok>La Esperanto-Gironde Informilo, apr 2019, n-ro 158</lok>
        </fnt>;
      </ekz>
      <ekz>
        Ursa <tld/>o havas florarojn el po kvin &gcirc;is dudek unuopaj floroj, la floroj
        havas po ses blankajn petalojn, kiuj aran&gcirc;i&gcirc;as stelforme
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=282915"
              >Ursa ajlo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/c/c6/Allium_ursinum_RF.jpg/551px-Allium_ursinum_RF.jpg"
      prm="CC BY 4.0"
      >urs<tld/>o &ccirc;e M&ouml;nchbruch, Germanujo
        <fnt>
          <aut>Robert Flogaus-Faust, propra verko</aut>
          <lok><url ref="&WCW;/File:Allium_ursinum_RF.jpg"
              >&WCW;/File:Allium_ursinum_RF.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="de">B&auml;rlauch</trd>
  <trdgrp lng="en">
    <trd>wild garlic</trd>,
    <trd>ramson</trd>,
    <trd>bear leek</trd>
  </trdgrp>
  <trd lng="fr">l'ail des ours</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_r;&c_je;&c_m;&c_sh;&c_a;&#769;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_b;&c_a;&#769;</trd>,
    <trd>&c_l;&c_u;&c_k; &c_m;&c_je;&c_d;&c_v;&c_je;&#769;&c_zh;&c_i;&c_j;</trd>,
    <trd>&c_d;&c_i;&#769;&c_k;&c_i;&c_j; &c_ch;&c_je;&c_s;&c_n;&c_o;&#769;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ch;&c_a;&c_s;&c_n;&c_i;&c_k; &c_v;&c_je;&c_d;&c_m;&c_je;&c_zh;&c_i;&c_j;</trd>,
    <trd>&c_c;&c_i;&c_b;&c_u;&#769;&c_l;&c_ja; &c_v;&c_je;&c_d;&c_m;&c_je;&#769;&c_zh;&c_a;</trd>
  </trdgrp>
</drv>




</art>
