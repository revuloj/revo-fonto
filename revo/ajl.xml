<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>ajl</rad>/o
</kap>
<drv mrk="ajl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="ajl.0o.1" num="1">
    <dif>
      Genro el familio <ref tip="super" cel="lili.0acoj">liliacoj</ref>
      (<trd lng="la">Allium</trd>).
    </dif>
    <trd lng="de">Lauch</trd>
    <trd lng="nl">look</trd>
  </snc>
  <snc mrk="ajl.0o.2" num="2">
    <dif>
      Specio de <ref tip="super" cel="ajl.0o.1"><tld/>o 1</ref> 
      (<trd lng="la">Allium sativum</trd>),
      uzata kiel spica&jcirc;o kaj kuracilo.  
    </dif>
    <trd lng="de">Knoblauch</trd>
    <trd lng="fr">ail</trd>
    <trd lng="nl">knoflook</trd>
    <trd lng="tr">sar&inodot;msak</trd>
  </snc>
</drv>
</art>
