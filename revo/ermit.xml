<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>ermit</rad>/o
    </kap>
    <drv mrk="ermit.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc>
        <uzo tip="fak">REL</uzo>
        <dif>
          Piulo vivanta en soleco. 
        </dif>
        <trdgrp lng="de">
          <trd>Eremit</trd>,
          <trd>Klausner</trd>,
          <trd>Einsiedler</trd></trdgrp>
        <trd lng="ru">&c_o;&c_t;&c_sh;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
      </snc>
      <snc>
        <uzo tip="stl">FIG</uzo><dif>
          Homo, kiu sisteme evitas la aliajn, kaj izole vivas. 
        </dif>
        <trdgrp lng="de">
          <trd>Einsiedler</trd>,
          <trd>Einzelg&auml;nger</trd></trdgrp>
      </snc>
      <trdgrp lng="fr">
        <trd>ermite</trd>,
        <trd>solitaire<klr tip="ind"> (subst.)</klr></trd></trdgrp> 
    </drv>
    <drv mrk="ermit.0ejo">
      <kap><tld/>ejo</kap>
      <snc>
        <dif>
          Loko, kie vivas unu a&ubreve; pluraj <tld/>oj.  
        </dif>
        <trdgrp lng="de">
          <trd>Eremitage</trd>,
          <trd>Klause</trd>,
          <trd>Einsiedelei</trd></trdgrp>
        <trd lng="fr">ermitage</trd>
        <trdgrp lng="ru">
          <trd>&c_m;&c_je;&c_s;&c_t;&c_o; &c_u;&c_je;&c_d;&c_i;&c_n;&c_je;&c_n;&c_i;&c_ja;</trd>;
          <trd>&c_s;&c_k;&c_i;&c_t;</trd>,
          <trd>&c_p;&c_u;&c_s;&c_t;&c_y;&c_n;&c_mol;</trd></trdgrp>
      </snc>
    </drv>
    <drv mrk="ermit.0ludo">
      <kap><tld/>ludo<fnt><bib>PIV1</bib></fnt></kap>
      <uzo tip="fak">SPO</uzo>
      <snc>
        <dif>
          Ludo, kiun oni ludas sola.
        </dif>
        <trd lng="en">solitaire</trd>
        <trd lng="fr">solitaire<klr tip="ind"> (jeu)</klr></trd>
        <trd lng="ru">&c_p;&c_a;&c_s;&c_mol;&c_ja;&c_n;&c_s;</trd>
      </snc>
    </drv>
  </art>
