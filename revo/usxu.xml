<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: usxu.xml,v 1.12 2022/04/03 19:25:10 revo Exp $">
<kap>
   <rad>u&scirc;u</rad>/o
</kap>
<drv mrk="usxu.0o">
   <kap><tld/>o</kap>
   <snc mrk="usxu.0o.SPO">
     <uzo tip="fak">SPO</uzo>
     <dif>
       &Ccirc;indevena batalarto kun a&ubreve; sen armilo:
       <ekz>
         multaj alilandaj amatoroj venis al &Ccirc;inio por lerni <tld/>on
         <fnt>
         <aut>&Ccirc;ina Radio Internacia</aut>,
         <vrk><url
 
ref="http://es.chinabroadcast.cn/1/2006/08/01/1@42680.htm">&Ccirc;inio
         klopodas...</url></vrk>,
         <lok>2006-08-01</lok>
         </fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_u;&c_sh;&c_u;</trd>
   <trdgrp lng="ca">
     <trd>kung-fu</trd>,
     <trd>wushu</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>Kung Fu</trd>,
     <trd>Wushu<klr> (Kampfkunst)</klr></trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>kung fu</trd>,
     <trd>wushu</trd>
   </trdgrp>
   <trd lng="fa">&a_w;&a_w;&a_sh;&a_w;</trd>
   <trdgrp lng="fi">
     <trd>wushu</trd>,
     <trd>kungfu<klr tip="amb"> (kamppailulaji)</klr></trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>kungfu</trd>,
     <trd>wushu<klr> (kungfu)</klr></trd>
   </trdgrp>
   <trd lng="hu">kungfu</trd>
   <trdgrp lng="it">
     <trd>kungfu</trd>, 
     <trd>kung-fu</trd>, 
     <trd>kung fu</trd>,
     <trd>wushu</trd>
   </trdgrp>
   <trd lng="nl">wushu</trd>
   <trdgrp lng="zh">
     <trd>&#27494;&#26415;<klr> [w&#468;sh&ugrave;]</klr></trd>
     <trd><klr>tr. </klr>&#27494;&#34899;</trd>
   </trdgrp>
   <trd lng="ru">&c_u;&c_sh;&c_u;</trd>
</drv>

<drv mrk="usxu.0isto">
   <kap><tld/>isto</kap>
   <snc mrk="usxu.0isto.SPO">
     <dif>
       Praktikanto de <tld/>o:
       <ekz>
         li ja vere estis eminentulo de la <tld/>istoj en nia lando
         <fnt>
           <aut>Xu Dongping, trad. Miaohui</aut>,
           <vrk><url ref="http://www.ebudhano.cn/blogoj/">E-budhana
Retejo</url></vrk>,
           <lok>2005-10-02</lok>
         </fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_u;&c_sh;&c_u;&c_ib;&c_s;&c_t;</trd>
   <trd lng="fa">&a_w;&a_w;&a_sh;&a_w;&f_k;&a_A;&a_r;</trd>
   <trd lng="fi">wushu-urheilija</trd>
   <trd lng="fr">kungfuiste</trd>
   <trd lng="hu">kungfuista</trd>
   <trd lng="nl">wushu-beoefenaar</trd>
   <trd lng="ru">&c_u;&c_sh;&c_u;&c_i;&c_s;&c_t;</trd>
</drv>
</art>

