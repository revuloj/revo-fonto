<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>otorinolaringologi</rad>/o
</kap>
<drv mrk="otorinolaringologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="otorinolaringologi.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Medicino-kirurgia fako, kiu studas la <ref tip="vid"
      cel="fiziol1.0o">fiziologion</ref> kaj la <ref tip="vid"
      cel="patolo.0o">patologion</ref> de la supraj aervojoj (oreloj,
      nazo, sinusaj kavoj, faringo-laringo kaj apudaj regionoj).
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>otorhinolaryngology</trd>,
    <trd>otolaryngology</trd>,
    <trd>ENT<klr tip="amb"> (Ear, Nose and Throat)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>oto-rhino-laryngologie</trd>,
    <trd>ORL</trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
