<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: otorinolaringologi.xml,v 1.5 2022/03/24 15:32:04 revo Exp $">
<kap>
    <rad>otorinolaringologi</rad>/o
</kap>
<drv mrk="otorinolaringologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="otorinolaringologi.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Medicino-kirurgia <ref tip="lst" lst="voko:sciencoj" cel="fak.0o.brancxo">fako</ref>,
      kiu studas la <ref tip="vid" cel="fiziol1.0o">fiziologion</ref> kaj la
      <ref tip="vid" cel="patolo.0o">patologion</ref> de la supraj aervojoj (oreloj,
      nazo, sinusaj kavoj, faringo-laringo kaj apudaj regionoj):
      <ekz>
        la Papo estas asistata de sia persona kuracisto, <klr>[&#x2026;]</klr> de du kuracistoj
        fakuloj pri reanimado, de fakulo pri <tld/>o kaj de du flegistoj
        <fnt>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="otorinolaringolog.0o">otorinolaringologo</ref>
    <trdgrp lng="de">
      <trd>Hals-Nasen-Ohren-Heilkunde</trd>,
      <trd>Oto-Rhino-Laryngologie</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="en">
    <trd>otorhinolaryngology</trd>,
    <trd>otolaryngology</trd>,
    <trd>ENT<klr tip="amb"> (Ear, Nose and Throat)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>oto-rhino-laryngologie</trd>,
    <trd>ORL</trd>
  </trdgrp>
  <trd lng="pl">otorynolaryngologia</trd>
  <trd lng="uk">&#x43E;&#x442;&#x43E;&#x440;&#x438;&#x43D;&#x43E;&#x43B;&#x430;&#x440;&#x438;&#x43D;&#x433;&#x43E;&#x43B;&#x43E;&#x433;&#x456;&#x44F;</trd>
</drv>
</art>

