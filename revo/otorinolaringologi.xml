<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: otorinolaringologi.xml,v 1.10 2024/02/24 18:27:25 revo Exp $">
<kap>
    <rad>otorinolaringologi</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="otorinolaringologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="otorinolaringologi.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Medicina-&hcirc;irurgia <ref tip="lst" lst="voko:sciencoj" cel="fak.0o.brancxo">fako</ref>,
      kiu studas la <ref tip="vid" cel="fiziol1.0o">fiziologion</ref> kaj la
      <ref tip="vid" cel="patolo.0o">patologion</ref> de la supraj aervojoj: oreloj,
      nazo, sinusaj kavoj, faringo-laringo kaj apudaj organaroj:
      <ekz>
        la Papo estas asistata de sia persona kuracisto, <klr>[&#8230;]</klr> de du kuracistoj
        fakuloj pri reanimado, de fakulo pri <tld/>o kaj de du flegistoj.
      </ekz>
    </dif>
    <ref tip="vid" cel="otorinolaringolog.0o">otorinolaringologo</ref>
    <trdgrp lng="de">
      <trd>Hals-Nasen-Ohren-Heilkunde</trd>,
      <trd>Oto-Rhino-Laryngologie</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_o;&c_t;&c_a;&c_r;&c_y;&c_n;&c_a;&c_l;&c_ja;&c_r;&c_y;&c_n;&c_g;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trdgrp lng="en">
    <trd>otorhinolaryngology</trd>,
    <trd>otolaryngology</trd>,
    <trd>ENT<klr tip="amb"> (Ear, Nose and Throat)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>oto-rhino-laryngologie</trd>,
    <trd>ORL</trd>
  </trdgrp>
  <trd lng="ja">&#32819;&#x9F3B;&#x54BD;&#21897;&#x79D1;  <pr>&#12376;&#12403;&#12356;&#12435;&#12371;&#12358;&#x304B;</pr></trd>
  <trd lng="pl">otorynolaryngologia</trd>
  <trd
  lng="uk">&#x43E;&c_t;&#x43E;&c_r;&c_i;&#x43D;&#x43E;&#x43B;&c_a;&c_r;&c_i;&#x43D;&c_g;&#x43E;&#x43B;&#x43E;&c_g;&c_ib;&#x44F;</trd>
  <trd lng="zh">&#32819;&#x9F3B;&#x54BD;&#21897; <pr>&#x11B;rb&#xED;y&amacron;nh&#xF3;u</pr></trd>
</drv>
</art>

