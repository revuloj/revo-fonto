<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: saturnali.xml,v 1.6 2023/07/28 20:26:11 revo Exp $">
<kap>
  <rad>saturnali</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="saturnali.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <snc mrk="saturnali.0o.saturnofesto">
    <dif>
      <ref tip="dif" cel="fest.saturno0o">Saturnofesto</ref>:
      <ekz>
        <tld lit="S"/>o kutime okazis en Romo en la dua duono de decembro,
        fine de la rikolta sezono
        <fnt>
          <aut>M. Lamkin, trad. M. Leon</aut>,
          <vrk><url
          ref="http://towerofbabel.com/sections/ourmaninhavana/ourwomaninitaly/marialamkin/carnevale/esperantoversion/">
          &leftquot;Carnevale&rightquot;</url></vrk>,
          <lok>2000-03</lok>
        </fnt>;
      </ekz>
      <ekz>
        estis la <tld/>o kaj la mitraisma festo, kun kiu ili devis konkurenci
        <fnt>
          <vrk><url ref="https://neciklopedio.org/wiki/Kristnasko">Kristnasko -
          Neciklopedio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        lin incitis lia sento de na&ubreve;zo kaj de memkompato en la arida
        vakuo de la <tld/>o
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>
    </dif>
    <rim mrk="saturnali.0o.RIM_NOMBRO">
      PIV prezentas la vorton plurnombre e&ccirc;
      pri unuopa festo imite al la latina etimo.
    </rim>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_t;&c_u;&c_r;&c_n;&c_a;&c_l;&c_ib;&c_ib;</trd>
  <trd lng="de">Saturnalien</trd>
  <trd lng="it">saturnali</trd>
  <trd lng="pl">Saturnalia</trd>
  <trd lng="uk">&c_s;&c_a;&c_t;&c_u;&c_r;&#x43D;&c_a;&#x43B;&c_ib;&c_ji;</trd>
</drv>
</art>

