<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fokstrot.xml,v 1.13 2023/11/09 20:48:33 revo Exp $">
<kap>
  <rad>fokstrot</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="fokstrot.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="fokstrot.0o.danco">
    <dif>
      Para <ref tip="lst" cel="danc.0o" lst="voko:dancoj">danco</ref>
      uson-devena, kvar-takta, vigla, populara meze de la 20a jarcento:
      <ekz>
        ili prenis unu la alian je la manoj, ekdancis iun varianton de
        &rightquot;<tld/>o&#x201D; kaj forkuris
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  <trdgrp lng="pt">
    <trd>foxtrote</trd>,
    <trd>fox</trd>
  </trdgrp>
  </snc>
  <snc mrk="fokstrot.0o.muzikajxo">
    <dif>
      Muzika&jcirc;o ta&ubreve;ga por <tld/>-danco.
    </dif>
  <trdgrp lng="pt">
    <trd>foxtrote</trd>,
    <trd>fox</trd>
  </trdgrp>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_k;&c_s;&c_t;&c_r;&c_o;&c_t;</trd>
  <trd lng="cs">foxtrot</trd>
  <trd lng="de">Foxtrott</trd>
  <trd lng="en">foxtrot</trd>
  <trd lng="es">fox-trot</trd>
  <trd lng="fr">fox-trot</trd>
  <trd lng="hu">foxtrott</trd>
  <trd lng="it">fox-trot</trd>
  <trdgrp lng="pl">
    <trd>fokstrot</trd>,
    <trd>foxtrot</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_o;&c_k;&c_s;&c_t;&c_r;&c_o;&c_t;</trd>
  <trd lng="sk">foxtrot</trd>
  <trd lng="uk">&c_f;&c_o;&c_k;&c_s;&c_t;&c_r;&c_o;&c_t;</trd>
  <trd lng="zh">&#x72D0;&#x6B65; <pr>h&#xFA;b&#xF9;</pr></trd>
</drv>
</art>

