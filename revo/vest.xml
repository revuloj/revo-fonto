<drv mrk="vest.sen0igxi">
  <kap>sen<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="vest.sen0igxi.KOMUNE">
    <dif>
      Seni&gcirc;i je <tld/>oj:
      <ekz>
        la dormeman servistinon <klr>[&#8230;]</klr> &scirc;i rapide
        forpermesis, dirinte ke &scirc;i mem sen<tld/>i&gcirc;os
        <fnt>
          <aut>A. Pu&scirc;kin, trad. A. Kor&jcirc;enkov</aut>,
          <vrk><url ref="&LOdE;Libroj/V_bibl01.pdf">
          La pika damo</url></vrk>,
          <lok>2003-2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i sen<tld/>i&gcirc;is kaj <klr>[&#8230;]</klr> tuj malaperis
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>sich <ind>ausziehen</ind></trd>,
      <trd>die <ind>Kleidung</ind> ablegen</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_p;&c_r;&c_a;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_a;&c_g;&c_a;&c_l;&c_ib;&c_c;&c_c;&c_a; <klr tip="amb">(&c_s;&c_k;&c_ib;&c_n;&c_u;&c_c;&c_mol; &c_a;&c_d;&c_z;&c_je;&c_n;&c_mol;&c_n;&c_je;)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>undress</trd>,
    <trd>disrobe</trd>
  </trdgrp>
  <trd lng="fr">se <ind>d&eacute;shabiller</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x670D;&#12434;&#33073;&#12368;  <pr>&#12405;&#x304F;&#12434;&#x306C;&#12368;</pr></trd>,
    <trd>&#x88F8;&#x306B;&#x306A;&#x308B;
