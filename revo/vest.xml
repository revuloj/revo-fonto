<drv mrk="vest.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="vest.0ajxo.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      &Ccirc;iu el la pecoj de plurpeca <ref tip="malprt" cel="vest.0o.TEKS"><tld/>o</ref>:
      <ekz>
        en blanka vesta&jcirc;o el lino, silente sursidas palega fra&ubreve;lino
        <fnt>
          <bib>FK</bib>
        </fnt>;
