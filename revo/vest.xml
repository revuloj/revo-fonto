<drv mrk="vest.0o">
  <kap><ofc>*</ofc><tld/>o <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5249">vest'o</url></lok></fnt></kap>
  <snc mrk="vest.0o.TEKS">
    <tezrad fak="TEKS"/>	
    <uzo tip="fak">TEKS</uzo>
    <dif>
      &Ccirc;io, per kio oni kovras la korpon:
      <ekz>
        surmeti, demeti <tld/>on;
      </ekz>
      <ekz>
        malnova, eluzita <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        li faris al li mikskoloran <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 37:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        li dis&scirc;iris siajn <tld/>ojn
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 37:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i kaptis lin je lia vesto, dirante: ku&scirc;i&gcirc;u kun mi, sed li lasis sian <tld/>on
        en &scirc;iaj manoj kaj forkuris
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 39:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        kia drapo, tia <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiun <tld/>o ornamas, tiun homoj ekamas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        valoras ne la <tld/>o <klr>(ekstera &scirc;ajno)</klr> valoras la
        enesto
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind><tld/>o homon prezentas</ind>
        (<ref tip="vid" cel="monahx.0o">mona&hcirc;o</ref>)
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">ruha teszi az embert</trd>
      </ekz>
      <ekz>
        kontra&ubreve; <tld/>o malbona konspiras &ccirc;iu &scirc;tono
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        lia nigra <tld/>o estis el la plej kara &scirc;tofo
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        civilaj <tld/>oj
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        funebraj <tld/>oj
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        lanaj kaj katunaj tali<tld/>oj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la tuta <tld/>aro
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la arbaro montris sian multkoloran <tld/>on
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[<nom>Zamenhof</nom>]</klr> rigardis la diversajn homajn religiojn kvaza&ubreve; malsamajn
        <tld/>ojn de unu sama vero
        <fnt>
          <bib>VivZam</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kostum.0o">kostumo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_z;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_a;&c_p;&c_r;&c_a;&c_n;&c_a;&c_h;&c_a;</trd>,
    <trd>&c_u;&c_b;&c_o;&c_r;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_o;&c_ib;</trd>,
    <trd>&c_v;&c_o;&c_p;&c_r;&c_a;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_a;&c_d;&c_z;&c_jo;&c_zh;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_o;&c_b;&c_l;&c_je;&c_k;&c_l;&c_o;</trd>,
    <trd>&c_d;&c_r;&c_je;&c_h;&c_i;</trd>
  </trdgrp>
  <trd lng="bo">&#xF51;&#xF74;&#xF42;&#xF0B;&#xF66;&#xFB3;&#xF7C;&#xF42;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>&scaron;aty</trd>,
    <trd>oblek</trd>,
    <trd>&ccaron;&aacute;st od&ecaron;vu <klr>(kalhoty, svetr, ko&scaron;ile apod.)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kleidung</trd>
  </trdgrp>
  <trd lng="el">&rho;&omikron;&ypsilon_ton;&chi;&alfa;</trd>
  <trd lng="es">vestido</trd>
  <trdgrp lng="fr">
    <trd>habit</trd>,
    <trd>v&ecirc;tement</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ruha</trd>,
    <trd>&ouml;lt&ouml;zet</trd>,
    <trd>&ouml;lt&ouml;z&eacute;k</trd>
  </trdgrp>
  <trd lng="ne">&#x932;&#x941;&#x917;&#x93E; &#x932;&#x917;&#x93E;&#x909;&#x928;&#x941;</trd>
  <trdgrp lng="nl">
    <trd>kleding</trd>,
    <trd>kleren</trd>,
    <trd>kledij</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>roupa</trd>,
    <trd>vestimenta</trd>,
    <trd>vestido</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_d;&c_je;&c_zh;&c_d;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>&ccaron;as&tcaron; odevu</trd>,
    <trd>&scaron;aty</trd>
  </trdgrp>
  <trd lng="tp">len</trd>
  <trdgrp lng="tr">
    <trd>elbise</trd>,
    <trd>giysi</trd>
  </trdgrp>
</drv>

<drv mrk="vest.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="vest.0ajxo.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      &Ccirc;iu el la pecoj de plurpeca <ref tip="malprt" cel="vest.0o.TEKS"><tld/>o</ref>:
      <ekz>
        en blanka vesta&jcirc;o el lino, silente sursidas palega fra&ubreve;lino
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        sinjoro en <klr>[&#x2026;]</klr> <tld/>a&jcirc;o de rajdo
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i metis siajn <tld/>a&jcirc;ojn tiel orde, dum la aliaj
        &jcirc;etis la siajn pelmele
        <fnt>
          <bib>Kiso</bib>,
          <lok>la bovinoj de la paro&hcirc;estro</lok></fnt>.
      </ekz>
    </dif>
    <lstref lst="voko:vestajxoj"><tld/>a&jcirc;oj</lstref>
  </snc>  
  <trd lng="bg">&c_d;&c_r;&c_je;&c_h;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Kleidungsst&uuml;ck</trd>
  </trdgrp>
  <trd lng="es">prenda<klr> de vestir</klr></trd>
  <trd lng="fr">pi&egrave;ce de v&ecirc;tement</trd>
  <trd lng="hu">ruhadarab</trd>
  <trd lng="nl">kledingstuk</trd>
  <trd lng="ru">&c_o;&c_d;&c_je;&c_zh;&c_d;&c_a;<klr> (&c_ch;&c_a;&c_s;&c_t;&c_mol;)</klr></trd>
  <trd lng="tp">len</trd>
</drv>

<drv mrk="vest.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="vest.0ejo.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Loko, kie oni konservas a&ubreve; lasas la <tld/>ojn:
      <ekz>
        <tld/>ejo de teatro;
      </ekz>
      <ekz>
        li diris al la estro de la <tld/>ejo: elportu <tld/>ojn por
        &ccirc;iuj servantoj de <nom>Baal</nom>
        <fnt><bib>MT</bib>, <lok>&Re2; 10:22</lok></fnt>.
      </ekz>
      <ekz>
        &scirc;i iras el la kabineto en la <tld/>ejon
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la re&gcirc;o estas en la <tld/>ejo
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_d;&c_e;&c_r;&c_o;&c_b;</trd>
  <trdgrp lng="cs">
    <trd>garderoba</trd>,
    <trd>&scaron;atna</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Garderobe</trd>,
    <trd>Kleiderkammer</trd>,
    <trd>Ankleidezimmer</trd>
  </trdgrp>
  <trd lng="es">vestidor</trd>
  <trd lng="fr">vestiaire</trd>
  <trd lng="hu">ruhat&aacute;r</trd>
  <trd lng="nl">vestiaire</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_r;&c_d;&c_je;&c_r;&c_o;&c_b;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_d;&c_je;&c_v;&c_a;&c_l;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>garder&oacute;ba</trd>,
    <trd>&scaron;at&ncaron;a</trd>
  </trdgrp>
  <trd lng="tr">vestiyer</trd>
</drv>

<drv mrk="vest.ali0i">
  <kap>ali<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vest.ali0i.TEKS">
    <dif>
      <tld lit="V"/>i iun per <tld/>o diferenca je
      tiu de ties sekso a&ubreve; pozicio:
      <ekz>
        mi ali<tld/>os min kaj iros en la batalon
        <fnt>
          <bib>MT</bib>
          <lok>I. re&gcirc;oj 22:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        la re&gcirc;ino ali<tld/>is sin kiel kamparanino
        <fnt><bib>ElektFab</bib>, <lok>Ne&gcirc;ulino</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> apena&ubreve; havis tempon sin lavi kaj ali<tld/>i post la voja&gcirc;o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        pastrino, ali<tld/>ita kiel la diino Izido
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">umziehen <klr>(Kleidung)</klr></trd>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_a;&c_p;&c_r;&c_a;&c_n;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">transvestir</trd>
  <trd lng="es">travestir</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;guiser</trd>,
    <trd>travestir</trd>
  </trdgrp>
  <trd lng="hu">(vminek) <ind>&ouml;lt&ouml;ztet</ind></trd>
  <trd lng="nl">verkleden</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_o;&c_d;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_o;&c_d;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="tr">k&inodot;l&inodot;k de&gbreve;i&scedil;tirmek</trd>
</drv>

<drv mrk="vest.mal0i">
  <kap>mal<tld/>i</kap>
  <snc mrk="vest.mal0i.KOMUNE">
    <dif>
      <ref tip="dif" cel="vest.sen0igi.KOMUNE">Sen<tld/>igi</ref>:
      <ekz>
        ni ne havis ion, per kio sin <tld/>i a&ubreve; mal<tld/>i
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>dua libro, &ccirc;apitro 
        24a</lok></fnt>;
      </ekz>
      <ekz>
        li ne plu havis ion por mal<tld/>i sin, nur la propran ha&ubreve;ton
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.net/2010/010189.php">Du mil dek</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        junulino mal<tld/>is sin ekster la prezidantejo kaj staris nuda anta&ubreve; la pordego
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="https://www.monato.net/2008/009474.php">&Scirc;oforo-angoro</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_p;&c_r;&c_a;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_d;&c_z;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>obna&zcaron;it</trd>,
    <trd>svl&eacute;ci</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ausziehen</trd>,
    <trd>entkleiden</trd>
  </trdgrp>
  <trd lng="el">&gamma;&delta;&ypsilon_ton;&ny;&omega;</trd>
  <trd lng="es">desvestir</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;shabiller</trd>,
    <trd>d&eacute;v&ecirc;tir</trd>
  </trdgrp>
  <trd lng="hu">levet<klr tip="ind"> (ruh&aacute;t)</klr></trd>
  <trd lng="nl">uitkleden</trd>
  <trd lng="pt">despir</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_d;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_d;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">vyzliec&tcaron; sa</trd>
  <trd lng="tr">soymak</trd>
</drv>

<drv mrk="vest.kap0o">
  <kap>kap<tld/>o</kap>
  <snc mrk="vest.kap0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="vest.0o.TEKS"><tld lit="V"/>o</ref> per kiu
      oni 
      kovras la kapon, precipe kiam ne temas pri la kutimaj &ccirc;apelo
      a&ubreve; &ccirc;apo:
      <ekz>
        la basketoj de lia blanka kap<tld/>o ankora&ubreve; ka&scirc;is
        la supran parton de lia viza&gcirc;o
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 5a</lok></fnt>;
      </ekz>
      <ekz>
        kap<tld/>ita per sia nemalhavebla konusa &ccirc;apelo
        <fnt>
          <bib>Monato</bib>
          <aut>S. Maul</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006831.html">La&ubreve; ritmo de la rizejoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_l;&c_a;&c_w;&c_n;&c_y; &c_w;&c_b;&c_o;&c_r;</trd>
  <trdgrp lng="de">
    <trd>Kopfbedeckung</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>coiffure<klr tip="amb"> (v&ecirc;tement)</klr></trd>,
    <trd>couvre-chef</trd>
  </trdgrp>
  <trd lng="hu">fejfed&odblac;</trd>
  <trd lng="nl">hoofddeksel</trd>
  <trd lng="ru">&c_g;&c_o;&c_l;&c_o;&c_v;&c_n;&c_o;&c_j; &c_u;&c_b;&c_o;&c_r;</trd>
</drv>

<drv mrk="vest.labor0o">
  <kap>labor<tld/>o</kap>
  <snc mrk="vest.labor0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="vest.0o.TEKS"><tld lit="V"/>o</ref>
      surportata dum laboro kiel &scirc;irmilo a&ubreve; kiel signo de funkcio...:
      <ekz>
        la humila klientaro de l' hebrea kvartalo, en siaj labor<tld/>oj
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i surhavas normalan hejman labor<tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Julia Sigmond kaj Sen Rodin</aut>
          <vrk><url ref="https://www.monato.net/2010/009988.php">La mal&jcirc;aluzo de Klorinda</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i klake trotas en la kuirejon por lavi lian labor<tld/>on
        <fnt>
          <aut>Zhu Yuqi</aut>,
          <vrk><url ref="http://esperanto.cri.cn/1/2005/09/30/1@29700.htm">
          Matena Melodio</url></vrk>,
          <lok>2005-09</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Arbeitskleidung</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>mont&eacute;rky</trd>,
    <trd>pracovn&iacute; oblek</trd>,
    <trd>pracovn&iacute; od&ecaron;v</trd>
  </trdgrp>
  <trd lng="fr">v&ecirc;tement de travail</trd>
  <trd lng="sk">pracovn&eacute; &scaron;aty</trd>
</drv>


<drv mrk="vest.maten0o">
  <kap>
    maten<tld/>o
  </kap>
  <snc>
    <dif>
      Vesto por porti matene endome, ordinare varma kaj mola:
      <ekz>
        mi por la honorario tuj a&ccirc;etos al mi plu&scirc;an maten<tld/>on kun pelta &ccirc;irka&ubreve;kudro
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Morgenkleid</trd>,
    <trd>Morgenmantel</trd>
  </trdgrp>
</drv>


<drv mrk="vest.nokto0o">
  <kap>nokto<tld/>o</kap>
  <snc mrk="vest.nokto0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="vest.0o.TEKS"><tld lit="V"/>o</ref>, kiun oni surmetas por nokta tempo
      a&ubreve; por dormo:
      <ekz>
        &scirc;i trovis &scirc;in jam en nokto<tld/>o, kaj evidente preta por enliti&gcirc;i
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="https://www.monato.net/2010/010023.php">Varmbotelo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <klr>[&#x2026;]</klr> subite staris apud li en sia maldikega 
        nokto<tld/>o
        <fnt>
          <aut>J. H. Rosbach</aut>,
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/np/np5703/felicxo.html">
          La feli&ccirc;o</url></vrk>,
          <lok>Norda Prismo, 1957:3, p. 163a-170a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Nachthemd</trd>,
      <trd>Schlafzeug</trd>,
      <trd>Schlafanzug</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">v&ecirc;tement de nuit</trd>
</drv>


<drv mrk="vest.pied0o">
  <kap>pied<tld/>o</kap>
  <snc mrk="vest.pied0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="vest.0o.TEKS"><tld lit="V"/>o</ref> per kiu
      oni kovras la piedojn:
      <ekz>
        lia pied<tld/>o ne estis pli bona &ndash; unu fliko sur alia
        <fnt><bib>FK</bib>, <lok>4, la forgesita pipo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_u;&c_t;&c_a;&c_k;</trd>
  <trd lng="ca">cal&ccedil;at</trd>
  <trd lng="cs">obuv</trd>
  <trdgrp lng="de">
    <trd>Schuhe</trd>,
    <trd>Fu&szlig;bekleidung</trd>
  </trdgrp>
  <trd lng="fr">chaussure</trd>
  <trd lng="hu">l&aacute;bbeli</trd>
  <trd lng="nl">schoeisel</trd>
  <trd lng="oc">cau&ccedil;adura</trd>
  <trd lng="ru">&c_o;&c_b;&c_u;&c_v;&c_mol;</trd>
  <trd lng="sk">top&aacute;nky</trd>
</drv>

<drv mrk="vest.sen0igi">
  <kap>sen<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vest.sen0igi.KOMUNE">
    <dif>
      Demeti ies <tld/>ojn:
      <ekz>
        kaj ili sen<tld/>igis lin, kaj surmetis al li skarlatan mantelon
        <fnt><bib>NT</bib>, <lok>&Mat; 27:28</lok></fnt>;
      </ekz>
      <ekz>
        ferminte la pordon, li komencis sin sen<tld/>igi
        <fnt>
          <bib>DL</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="vest.mal0i">mal<tld/>i</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_p;&c_r;&c_a;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_d;&c_z;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>svl&eacute;knout</trd>,
    <trd>zbavit od&ecaron;vu</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ausziehen</trd>,
    <trd>entkleiden</trd>
  </trdgrp>
  <trd lng="el">&gamma;&delta;&ypsilon_ton;&ny;&omega;</trd>
  <trd lng="en">undress</trd>
  <trd lng="es">desvestir</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;shabiller</trd>,
    <trd>d&eacute;v&ecirc;tir</trd>
  </trdgrp>
  <trd lng="hu">levetk&odblac;ztet</trd>
  <trd lng="nl">uitkleden</trd>
  <trd lng="pt">despir</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_d;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_d;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">vyzliec&tcaron; sa</trd>
</drv>

<drv mrk="vest.sen0igxi">
  <kap>sen<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="vest.sen0igxi.KOMUNE">
    <dif>
      Seni&gcirc;i je <tld/>oj:
      <ekz>
        la dormeman servistinon <klr>[&#x2026;]</klr> &scirc;i rapide
        forpermesis, dirinte ke &scirc;i mem sen<tld/>i&gcirc;os
        <fnt>
          <aut>A. Pu&scirc;kin, trad. A. Kor&jcirc;enkov</aut>,
          <vrk><url ref="http://www.esperanto-ondo.ru/Libroj/V_bibl01.pdf">
          La pika damo</url></vrk>,
          <lok>2003-2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i sen<tld/>i&gcirc;is kaj <klr>[&#x2026;]</klr> tuj malaperis
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>sich <ind>ausziehen</ind></trd>,
      <trd>die <ind>Kleidung</ind> ablegen</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">se <ind>d&eacute;shabiller</ind></trd>
</drv>

<drv mrk="vest.sub0o">
  <kap>sub<tld/>o</kap>
  <snc mrk="vest.sub0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Nemontrata <ref tip="super" cel="vest.0o.TEKS"><tld/>o</ref>,
      kiun oni portas sub la videblaj:
      <ekz>
        delikataj sub<tld/>oj de virino;
      </ekz>
      <ekz>
        <nom>Elsa</nom> eliris el la lito kaj komencis surmeti la sub<tld/>ojn
        <fnt><bib>InfanTorent2</bib>, <lok>unua parto, &ccirc;apitro
        1a</lok></fnt>;
      </ekz>
      <ekz>
        la reklamanta firmao pretendas, ke tiu mirinda sub<tld/>o efike maldikigos
        elstaran ventron
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="https://www.monato.net/2006/008827.php">Pri kutimoj kaj kalsonoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_o;&c_d;&c_n;&c_ja;&c_je;</trd>
  <trd lng="cs">spodn&iacute; pr&aacute;dlo</trd>
  <trdgrp lng="de">
    <trd>Unterw&auml;sche</trd>,
    <trd>Unterbekleidung</trd>
  </trdgrp>
  <trd lng="es">ropa interior</trd>
  <trd lng="fr">sous-v&ecirc;tement</trd>
  <trd lng="hu">feh&eacute;rnem&udblac;</trd>
  <trdgrp lng="nl">
    <trd>ondergoed</trd>,
    <trd>onderkleding</trd>,
    <trd>onderkleren</trd>
  </trdgrp>
  <trd lng="pt">roupa de baixo</trd>
  <trd lng="ru">&c_n;&c_i;&c_zh;&c_n;&c_je;&c_je; &c_b;&c_je;&c_l;&c_mol;&c_jo;</trd>
  <trd lng="sk">spodn&aacute; bielize&ncaron;</trd>
  <trd lng="tr">i&ccedil; &ccedil;ama&scedil;&inodot;r&inodot;</trd>
</drv>

<drv mrk="vest.sur0o">
  <kap>sur<tld/>o</kap>
  <snc mrk="vest.sur0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="dif" cel="vest.super0o.TEKS">Super<tld/>o</ref>:
      <ekz>
        aperis nealta homo en ru&gcirc;a sur<tld/>o, brodita per bizare 
        sinuantaj oraj serpentoj
        <fnt>
          <aut>I. Efremov, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Efremov/horo.html">
          Horo de Bovo</url></vrk>,
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Umhang</trd>
  </snc>
  <trd lng="cs">od&ecaron;v</trd>
  <trd lng="sk">odev</trd>
</drv>

<drv mrk="vest.super0o">
  <kap><ofc>*</ofc> super<tld/>o</kap>
  <snc mrk="vest.super0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="vest.0o.TEKS"><tld lit="V"/>o</ref> kiun oni
      surmetas sur aliajn vestojn, precipe tiaj kiujn oni portas
      eksterdome por sin protekti kontra&ubreve; malvarmo a&ubreve; pluvo:
      <ekz>
        li portas rozokoloran super<tld/>on kaj teleroforman
        &ccirc;apelon
        <fnt><bib>F</bib>, <lok>&FE; 31</lok></fnt>;
      </ekz>
      <ekz>
        li detiris siajn &scirc;amajn gantojn kaj disbutonumas la
        super<tld/>on
        <fnt>
        <aut>I. Lojko</aut>, <vrk>kulereto</vrk>,
        <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        li prenis sian super<tld/>on kaj forlasis la balon
        anta&ubreve; kelkaj minutoj, diris <nom>Olsen</nom>
        <fnt><bib>FK</bib>, <lok>4, , la porcio da 
        glacia&jcirc;o</lok></fnt>;
      </ekz>
      <ekz>
        ni enrigardu iom en la anta&ubreve;&ccirc;ambron, kie
        trovis rifu&gcirc;on la super<tld/>oj, bastonoj,
        pluvombreloj kaj galo&scirc;oj
        <fnt><bib>Fab1</bib>, <lok>galo&scirc;oj de
        feli&ccirc;o</lok></fnt>;
      </ekz>
      <ekz>
        oni povus diri, ke &scirc;in kaptis forta malvarmo,
        &ccirc;ar &scirc;i iom tremis sub la pelta super<tld/>o, kaj
        ke &scirc;i ion profunde hontis, &ccirc;ar &scirc;i ne povis
        forlevi la okulojn de la &scirc;tonoj de la trotuaro
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        du sinjorinoj en veluraj super<tld/>oj, apogante sin
        reciproke unu al la alia, iris rapide kaj interparolis gaje
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">svrchn&iacute;k</trd>
  <trdgrp lng="de">
    <trd>&Uuml;berbekleidung</trd>,
    <trd>Oberbekleidung</trd>,
    <trd>Umhang</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>overcoat</trd>,
    <trd>outer clothing</trd>
  </trdgrp>
  <trd lng="fr">v&ecirc;tement d'ext&eacute;rieur</trd>
  <trdgrp lng="hu">
    <trd>fel&ouml;lt&odblac;</trd>,
    <trd>fels&odblac;kab&aacute;t</trd>,
    <trd>k&ouml;peny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_r;&c_h;&c_n;&c_ja;&c_ja; &c_o;&c_d;&c_je;&c_zh;&c_d;&c_a;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_h;&c_n;&c_je;&c_je; &c_p;&c_l;&c_a;&c_t;&c_mol;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">pl&aacute;&scaron;&tcaron;</trd>
</drv>
</art>
