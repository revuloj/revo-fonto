<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vest</rad>/i
</kap>
<drv mrk="vest.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">TEKS</uzo>
  <snc mrk="vest.0i.sxtofo">
    <dif>
      Kovri per &scirc;tofoj a&ubreve; peltoj pli malpli al&gcirc;ustigitaj al la korpo por
      &scirc;irmi kontra&ubreve; malvarmo a&ubreve; ka&scirc;i nudecon:
      <ekz>
        simple, sed pure <tld/>ita;
      </ekz>
      <ekz>
        belaj sklavinoj <tld/>itaj en <klr>(per)</klr>
        silko kaj oro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i tiujn, kiuj estas nudaj.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_p;&c_r;&c_a;&c_n;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_d;&c_z;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>einkleiden</trd>,
      <trd>ausstaffieren</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_d;&c_je;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_d;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="vest.0i.kovri">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kovri:
      <ekz>
        la printempo verde <tld/>is la teron<fnt>B</fnt>;
      </ekz>
      <ekz>
        arbaro <tld/>as la monton;
      </ekz>
      <ekz>
        la kampoj <tld/>i&gcirc;as per greno<fnt>Z</fnt>;
      </ekz>
      <ekz>
        sennombraj insuletoj <tld/>itaj de bela verda&jcirc;o;
      </ekz>
      <ekz>
        ladoj el bronzo <tld/>as la murojn;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ili <tld/>i&gcirc;u per honto kaj malhonoro<fnt>Z</fnt>(<ref tip="sin"
        cel="zon.0i">zoni</ref>);
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        plej pura stilo <tld/>anta viglan kaj profundan penson.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="teg.0i">tegi</ref>,
      <ref cel="mask.0i">maski</ref>,
      <ref cel="sxirm.0i">&scirc;irmi</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_k;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_k;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_n;&c_a;&c_k;&c_r;&c_y;&c_t;&c_mol;</trd>,
      <trd>&c_n;&c_a;&c_k;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_o;&c_k;&c_r;&c_y;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_o;&c_k;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_l;&c_je;&c_ch;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_l;&c_je;&c_k;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">obl&eacute;ci</trd>
  <trd lng="de">kleiden</trd>
  <trd lng="el">&ny;&tau;&ypsilon_ton;&ny;&omega;</trd>
  <trd lng="en">clothe</trd>
  <trd lng="es">vestir</trd>
  <trdgrp lng="fr">
    <trd>habiller</trd>,
    <trd>v&ecirc;tir</trd>
  </trdgrp>
  <trd lng="nl">aankleden</trd>
  <trd lng="pt">vestir</trd>
</drv>

<drv mrk="vest.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      &Ccirc;io, per kio oni kovras la korpon:
      <ekz>
        surmeti, demeti <tld/>on;
      </ekz>
      <ekz>valoras ne la <tld/>o (ekstera &scirc;ajno) valoras la
        enesto
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz><tld/>o homon prezentas(<ref tip="vid"
        cel="monahx.0o">mona&hcirc;o</ref>)
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la arbaro montris sian multkoloran <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Z rigardis la diversajn religiojn kvaza&ubreve; malsamajn <tld/>ojn de
        unu sama vero.
      </ekz>
    </dif>
    <ref tip="vid" cel="kostum.0o">kostumo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_z;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_a;&c_p;&c_r;&c_a;&c_n;&c_a;&c_h;&c_a;</trd>,
    <trd>&c_u;&c_b;&c_o;&c_r;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_o;&c_ib;</trd>,
    <trd>&c_v;&c_o;&c_p;&c_r;&c_a;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_a;&c_d;&c_z;&c_jo;&c_zh;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>oblek</trd>,
    <trd>&scaron;aty</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Kleidung</trd>
  </trdgrp>
  <trd lng="el">&rho;&omikron;&ypsilon_ton;&chi;&alfa;</trd>
  <trd lng="es">vestido</trd>
  <trd lng="fr">v&ecirc;tement</trd>
  <trdgrp lng="nl">
    <trd>kleding</trd>,
    <trd>kleren</trd>,
    <trd>kledij</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>roupa</trd>,
    <trd>vestimenta</trd>,
    <trd>vestido</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_d;&c_je;&c_zh;&c_d;&c_a;</trd>
</drv>

<drv mrk="vest.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      &Ccirc;iu el la pecoj de plurpeca <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="de">
      <trd>Kleidungsst&uuml;ck</trd>
  </trdgrp>
  <trd lng="es">prenda<klr> de vestir</klr></trd>
  <trd lng="nl">kledingstuk</trd>
  <trd lng="ru">&c_o;&c_d;&c_je;&c_zh;&c_d;&c_a; <klr>(&c_ch;&c_a;&c_s;&c_t;&c_mol;)</klr></trd>
</drv>

<drv mrk="vest.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Loko, kie oni konservas a&ubreve; lasas la <tld/>ojn:
      <ekz>
        <tld/>ejo de teatro.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">vestiaire</trd>
  <trd lng="be">&c_g;&c_a;&c_r;&c_d;&c_e;&c_r;&c_o;&c_b;</trd>
  <trd lng="de">Garderobe</trd>
  <trd lng="es">vestidor</trd>
  <trd lng="fr">vestiaire</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_r;&c_d;&c_je;&c_r;&c_o;&c_b;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_d;&c_je;&c_v;&c_a;&c_l;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="vest.ali0i">
  <kap>ali<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="V"/>i iun per <tld/>o diferenca je
      tiu de ties sekso a&ubreve; pozicio.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_a;&c_p;&c_r;&c_a;&c_n;&c_a;&c_c;&c_mol;</trd>
  <trd lng="es">trasvestir</trd>
  <trd lng="fr">travestir</trd>
  <trd lng="nl">verkleden</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_o;&c_d;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_o;&c_d;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="vest.sen0igi">
  <kap>sen<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Demeti ies <tld/>ojn.
    </dif>
    <ref tip="sin" cel="vest.mal0i">mal<tld/>i</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_p;&c_r;&c_a;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_d;&c_z;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>ausziehen</trd>,
      <trd>entkleiden</trd>
  </trdgrp>
  <trd lng="el">&gamma;&delta;&ypsilon_ton;&ny;&omega;</trd>
  <trd lng="es">desvestir</trd>
  <trd lng="fr">d&eacute;shabiller</trd>
  <trd lng="nl">uitkleden</trd>
  <trd lng="pt">despir</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_d;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_d;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="vest.mal0i">
  <kap>mal<tld/>i</kap>
  <ref tip="dif" cel="vest.sen0igi">sen<tld/>igi</ref>
  <trdgrp lng="de">
      <trd>ausziehen</trd>,
      <trd>entkleiden</trd>
  </trdgrp>
  <trd lng="es">desvestir</trd>
</drv>

<drv mrk="vest.kap0o">
  <kap>kap<tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="vest.0o"><tld lit="V"/>o</ref> per kiu oni
      kovras la kapon, precipe
      kiam ne temas pri la kutimaj &ccirc;apelo a&ubreve; &ccirc;apo.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_l;&c_a;&c_w;&c_n;&c_y; &c_w;&c_b;&c_o;&c_r;</trd>
  <trdgrp lng="de">
      <trd>Kopfbedeckung</trd>
  </trdgrp>
  <trd lng="fr">coiffure<klr tip="amb"> (v&ecirc;tement)</klr></trd>
  <trd lng="nl">hoofddeksel</trd>
  <trd lng="ru">&c_g;&c_o;&c_l;&c_o;&c_v;&c_n;&c_o;&c_j; &c_u;&c_b;&c_o;&c_r;</trd>
</drv>

<drv mrk="vest.pied0o">
  <kap>pied<tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="vest.0o"><tld lit="V"/>o</ref> per kiu oni
      kovras la piedojn.
    </dif>
  </snc>
  <trdgrp lng="de">
      <trd>Beinkleid</trd>
  </trdgrp>
  <trd lng="nl">schoeisel</trd>
  <trd lng="be">&c_a;&c_b;&c_u;&c_t;&c_a;&c_k;</trd>
  <trd lng="ru">&c_o;&c_b;&c_u;&c_v;&c_mol;</trd>
</drv>

<drv mrk="vest.sub0o">
  <kap>sub<tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Nemontrata <ref tip="super" cel="vest.0o"><tld/>o</ref>, kiun oni
      portas sub la videblaj:
      <ekz>
        delikataj sub<tld/>oj de virino.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
      <trd>Unterw&auml;sche</trd>,
      <trd>Unterbekleidung</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ondergoed</trd>,
    <trd>onderkleding</trd>,
    <trd>onderkleren</trd>
  </trdgrp>
  <trd lng="be">&c_s;&c_p;&c_o;&c_d;&c_n;&c_ja;&c_je;</trd>
  <trd lng="es">ropa interior</trd>
  <trd lng="fr">sous-v&ecirc;tement</trd>
  <trd lng="pt">roupa de baixo</trd>
  <trd lng="ru">&c_n;&c_i;&c_zh;&c_n;&c_je;&c_je; &c_b;&c_je;&c_l;&c_mol;&c_jo;</trd>
</drv>

<drv mrk="vest.sur0o">
  <kap>sur<tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Lar&gcirc;a <ref tip="super" cel="vest.0o"><tld/>o</ref>, kiun oni portas
      sur la aliaj, por ilin &scirc;irmi.
    </dif>
    <ref tip="sin" cel="vest.super0o">super<tld/>o</ref>
  </snc>
  <trdgrp lng="de">
      <trd>&Uuml;berbekleidung</trd>,
      <trd>Oberbekleidung</trd>
  </trdgrp>
  <trd lng="nl">overjas</trd>
  <trd lng="be">&c_n;&c_a;&c_k;&c_ib;&c_d;&c_k;&c_a;</trd>
  <trdgrp lng="fr">
    <trd>sarrau</trd>,
    <trd>blouse <klr tip="ind">(de protection)</klr></trd>,
    <trd>v&ecirc;tement de protection</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_k;&c_i;&c_d;&c_k;&c_a;</trd>
</drv>

<drv mrk="vest.super0o">
  <kap>super<tld/>o</kap>
  <ref tip="dif" cel="vest.sur0o">sur<tld/>o</ref>
  <trdgrp lng="de">
      <trd>&Uuml;berbekleidung</trd>,
      <trd>Oberbekleidung</trd>
  </trdgrp>
</drv>
</art>
