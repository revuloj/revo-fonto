<drv mrk="vest.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vest.0i.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="kovr.0i.surmeti">Kovri</ref> per &scirc;tofoj a&ubreve; peltoj pli malpli
      al&gcirc;ustigitaj al la korpo
      por &scirc;irmi kontra&ubreve; malvarmo, suno a&ubreve; ka&scirc;i nudecon:
      <ekz>
        simple, sed pure <tld/>ita;
      </ekz>
      <ekz>
        belaj sklavinoj <tld/>itaj en silko kaj oro
        <fnt><bib>Fab1</bib>, <lok>la virineto de maro</lok></fnt>;
      </ekz>
      <ekz>
        mi estis <klr>[&#8230;]</klr> nuda, kaj vi <tld/>is min
        <fnt><bib>NT</bib>, <lok>&Mat; 25:35-36</lok></fnt>.
      </ekz>
      <ekz>
        la homo estis tre ri&ccirc;e <tld/>ita, li estis videble grava persono
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        funebre <tld/>ita virino
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        virinoj, <tld/>itaj per atlaso kaj zibelo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>os vin kiel por edzi&gcirc;o
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        en silk' brodita en<tld/>itaj
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_p;&c_r;&c_a;&c_n;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_d;&c_z;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>einkleiden</trd>,
      <trd>ausstaffieren</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd><baz>baju</baz>membajui</trd>,
      <trd><baz>kena</baz>mengenakan (pakaian)</trd>,
      <trd><baz>pakai</baz>memakai (pakaian)</trd>,
      <trd><baz>pakai</baz>berpakaian</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_d;&c_je;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_d;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="vest.0i.kovri">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kovri:
      <ekz>
        la printempo verde <tld/>is la teron<fnt>B</fnt>;
      </ekz>
      <ekz>
        arbaro <tld/>as la monton;
      </ekz>
      <ekz>
        la kampoj <tld/>i&gcirc;as per greno
        <fnt><bib>MT</bib>, <lok>&Psa; 65:13</lok></fnt>;
      </ekz>
      <ekz>
        sennombraj insuletoj <tld/>itaj de bela verda&jcirc;o;
      </ekz>
      <ekz>
        ladoj el bronzo <tld/>as la murojn;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i&gcirc;u per honto kaj malhonoro tiuj, kiuj fanfaronas
        kontra&ubreve;
        mi
        <fnt><bib>MT</bib>, <lok>&Psa; 35:26</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        plej pura stilo <tld/>anta viglan kaj profundan penson.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mask.0i">maski</ref>,
      <ref cel="sxirm.0i">&scirc;irmi</ref>,
      <ref cel="teg.0i">tegi</ref>
