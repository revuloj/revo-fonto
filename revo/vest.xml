  <snc mrk="vest.0i.kovri">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="kovr.0i.surmeti">Kovri</ref> kvaza&ubreve; vesto:
      <ekz>
