<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rukt</rad>/i
</kap>
<drv mrk="rukt.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Eligi tra la bu&scirc;o stomakajn gasojn kun pla&ubreve;da bruo
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_y;&c_g;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">r&uuml;lpsen</trd>
  <trdgrp lng="en">
    <trd>belch</trd>,
    <trd>burp</trd>
  </trdgrp>
  <trd lng="fr">roter</trd>
  <trd lng="es">eructar</trd>
  <trdgrp lng="hu">
    <trd>b&ouml;f&ouml;g</trd>,
    <trd>b&uuml;f&ouml;g</trd>
  </trdgrp>
  <trd lng="nl">boeren</trd>
  <trd lng="pt">arrotar</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_y;&c_g;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_y;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
