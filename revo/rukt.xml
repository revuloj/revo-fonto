<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rukt</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="rukt.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="rukt.0i.SON">
    <dif>
      Eligi tra la bu&scirc;o stomakajn gasojn kun pla&ubreve;da bruo.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_y;&c_g;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">říhat</trd>
  <trd lng="de">r&uuml;lpsen</trd>
  <trdgrp lng="en">
    <trd>belch</trd>,
    <trd>burp</trd>
  </trdgrp>
  <trd lng="es">eructar</trd>
  <trdgrp lng="fr">
    <trd>roter</trd>,
    <trd>&eacute;ructer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>b&ouml;f&ouml;g</trd>,
    <trd>b&uuml;f&ouml;g</trd>
  </trdgrp>
  <trd lng="nl">boeren</trd>
  <trd lng="pt">arrotar</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_y;&c_g;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_y;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>grgať</trd>,
    <trd>odgrgnúť si</trd>
  </trdgrp>
  <trd lng="sv">rapa</trd>
</drv>

<drv mrk="rukt.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="rukt.0.SON">
    <dif>
      <ref tip="lst" cel="son.0o" lst="voko:sonoj">Sono</ref>
      de <tld/>ado:
      <ekz>
        de la dua horo &ndash; <tld/>! &ndash; &gcirc;is nun mi ankora&ubreve; nenion man&gcirc;is kaj 
        trinkis
        <fnt>
          <aut>Lao She, trad. Wang Chongfang</aut>,
          <vrk><url
          ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0026/">
          Kamelo &Scirc;jangzi</url></vrk>,
          <lok>1988</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">burp !</trd>
</drv>


<drv mrk="rukt.0o">
  <kap><tld/>o</kap>
  <snc mrk="rukt.0o.SNC">
    <dif>
      Ago de tiu, kiu ruktas:
      <ekz>
        li ellasis rukton
        <fnt>
          <aut>George Orwell, trad. Donald Broadribb</aut>,
          <vrk><url
          ref="http://i-espero.info/files/elibroj/eo%20-%20orwell,%20george%20-%201984.pdf">Mil
          Na&ubreve;cent Okdek Kvar</url></vrk>,
          <lok>p. 12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">říhání</trd>
  <trdgrp lng="fr">
    <trd>rot</trd>,
    <trd>&eacute;ructation</trd>,
    <trd>renvoi</trd>
  </trdgrp>
  <trd lng="sk">grganie</trd>
</drv>
</art>
