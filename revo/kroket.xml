  <trd lng="de">Krocket</trd>
  <trd lng="en">croquet</trd>
  <trd lng="hu">krokett</trd>
  <trd lng="ru">&c_k;&c_r;&c_o;&c_k;&c_je;&c_t;</trd>
</drv>
</art>
