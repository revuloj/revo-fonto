<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>legi</rad>/o
</kap>
<drv mrk="legi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Militista <ref tip="super" cel="korpus.0o.MIL">korpuso</ref> en <ref tip="vid" cel="romi.0o">Romio</ref>,
      enhavanta &ccirc;irka&ubreve;e 5000 soldatojn.
    </dif>
    <ref tip="sub" cel="kohort.0o">kohorto</ref>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <ref tip="dif" cel="mult.0o">Multo</ref>
  </snc>
  <trd lng="en">legion</trd>
  <trd lng="es">legi&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>l&eacute;gion</trd>
  </trdgrp>
  <trd lng="nl">legioen</trd>
  <trd lng="ru">&c_l;&c_je;&c_g;&c_i;&c_o;&c_n;</trd>
</drv>

<drv mrk="legi.honora0o">
  <kap>honora <tld/>o</kap>
  <snc>
    <dif>
      Plej alta franca <ref tip="super" cel="ordeno.0o.honora">ordeno</ref> por civila a&ubreve; militista merito.
    </dif>
  </snc>
  <trd lng="en">Legion of Honor</trd>
  <trdgrp lng="fr">
    <trd>l&eacute;gion d'honneur</trd>
  </trdgrp>
  <trd lng="nl">legioen van eer</trd>
  <trd lng="ru">&c_p;&c_o;&c_ch;&c_jo;&c_t;&c_n;&c_y;&c_j; &c_l;&c_je;&c_g;&c_i;&c_o;&c_n; <klr>(&c_o;&c_r;&c_d;&c_je;&c_n;)</klr></trd>
</drv>
</art>
