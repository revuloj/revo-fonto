<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: legi.xml,v 1.37 2024/01/15 16:14:09 revo Exp $">
<kap>
  <ofc>*</ofc> <rad>legi</rad>/o,
  <var><kap><rad var="on">legion</rad>/o <fnt><bib>Far1</bib></fnt></kap></var>
</kap>
<drv mrk="legi.0o">
  <kap><ofc>*</ofc><tld/>o,
    <var><kap><tld var="on"/>o</kap></var>
  </kap>
  <snc mrk="legi.0o.MIL">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Militista <ref tip="super" cel="korpus.0o.MIL">korpuso</ref> en
      <ref tip="vid" cel="romi.0o">Romio</ref>,
      enhavanta &ccirc;irka&ubreve;e 5000 soldatojn:
      <ekz>
        pli ol dek du <tld/>oj da an&gcirc;eloj
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 26:53</lok>
        </fnt>;
      </ekz>
      <ekz>
        la spiro fari&gcirc;is pli kaj pli densa kaj ricevis la formon de
        malgrandaj klaraj
        an&gcirc;eloj, kiuj <klr>[&#x2026;]</klr> lancon kaj &scirc;ildon en la manoj, ilia
        nombro fari&gcirc;is &ccirc;iam pli granda, kaj kiam Gerda finis sian pre&gcirc;on, tuta
        <tld/>o estis jam &ccirc;irka&ubreve; &scirc;i
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        anstata&ubreve; la senkompata dio kaj liaj <tld var="on"/>oj li vidis &ccirc;irka&ubreve;
        si: malplenon, silenton kaj nescion
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni renkonti&gcirc;as kun senatanoj, toge vestitaj, romaj matronoj,
        liktoroj, sklavoj,
        kruelaspekta centestro kaj soldatoj de la sepa <tld/>o
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2004/008467.html">Kampoj de Soria Antonio Machado, universala
              poeto</url></vrk>
              <bib>Monato</bib>
              <lok>2004/12, p. 23</lok>
        </fnt>;
      </ekz>
      <ekz>
        rilate la sie&gcirc;on de Numancio la klasikaj fontoj mencias, ke la
        romiaj <tld/>oj,
        konkerinte la urbon, trovis neniun vivanton
        <fnt>
          <aut>Nikolas Xil Carvalho</aut>
          <vrk><url ref="&Monato;/2005/008584.php">Kampoj de Soria</url></vrk>
          <bib>Monato</bib>
          <lok>2005/04, p. 6</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la antikveco kaj mezepoko trapasis ne nur karavanoj,
        veturantaj la&ubreve; la Silka
        Vojo, sed anka&ubreve; <tld/>oj de Aleksandro la Granda
        <fnt>
          <aut>Anatoli Ionesov</aut>
          <vrk><url ref="&Monato;/2006/008920.php">Perlo &ccirc;e la Silka Vojo</url></vrk>
           <bib>Monato</bib>
          <lok>2006/08, p. 17</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="kohort.0o">kohorto</ref>
    <trd lng="el">&lambda;&epsilon;&gamma;&epsilon;&omega_ton;&ny;&alfa;</trd>
  </snc>
  <snc mrk="legi.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <ref tip="dif" cel="mult.0o">multo</ref>
    <trd lng="el">&pi;&lambda;&eta_ton;&theta;&omikron;&sigma_fina;</trd>
    <trd lng="nl">legio</trd>
  </snc>
  <trd lng="be">&c_l;&c_je;&c_g;&c_ib;&c_jo;&c_n;</trd>
  <trd lng="bg">&c_l;&c_je;&c_g;&c_i;&c_o;&c_n;</trd>
  <trd lng="ca">legi&oacute;</trd>
  <trdgrp lng="cs">
    <trd>dav</trd>,
    <trd>legie <klr>(10 kohort)</klr></trd>,
    <trd>mno&zcaron;stv&iacute;</trd>,
    <trd>z&aacute;stup</trd>
  </trdgrp>
  <trd lng="de">Legion</trd>
  <trd lng="en">legion</trd>
  <trd lng="es">legi&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>l&eacute;gion</trd>
  </trdgrp>
  <trd lng="hu">l&eacute;gi&oacute;</trd>
  <trd lng="it">legione</trd>
  <trdgrp lng="ja">
    <trd>&#x8ECD;&#x56E3;  <pr>&#x3050;&#x3093;&#x3060;&#x3093;</pr></trd>,
    <trd>&#x30EC;&#x30AE;&#x30AA;&#x30F3;</trd>,
    <trd>&#x8ECD;&#x968A;  <pr>&#x3050;&#x3093;&#x305F;&#x3044;</pr></trd>,
    <trd>&#x90E8;&#x968A;  <pr>&#x3076;&#x305F;&#x3044;</pr></trd>,
    <trd>&#x591A;&#x6570;  <pr>&#x305F;&#x3059;&#x3046;</pr></trd>,
    <trd>&#x5927;&#x52E2;  <pr>&#x305F;&#x3044;&#x305B;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">legioen</trd>
  <trdgrp lng="pl">
    <trd>legia</trd>,
    <trd>legion</trd>
  </trdgrp>
  <trd lng="pt">legi&atilde;o</trd>
  <trd lng="ro">legiune</trd>
  <trd lng="ru">&c_l;&c_je;&c_g;&c_i;&c_o;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>dav</trd>,
    <trd>l&eacute;gia</trd>,
    <trd>mno&zcaron;stvo</trd>,
    <trd>z&aacute;stup</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43B;&c_je;&c_gu;&c_ib;&#x43E;&#x43D;</trd>,
    <trd>&c_t;&#x44C;&#x43C;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x519B;&#x56E2; <pr>j&#x16B;ntu&#xE1;n</pr></trd>,
    <trd>&#x8ECD;&#x5718; <pr>j&#x16B;ntu&#xE1;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="legi.honora0o">
  <kap>honora <tld/>o</kap>
  <snc>
    <dif>
      Plej alta franca <ref tip="super" cel="orden.0o.honora">ordeno</ref> por civila a&ubreve; militista
      merito:
      <ekz>
        bezonata nova &leftquot;fremdula <tld/>o&rightquot; <klr>[&#x2026;]</klr>
        ne mankas aventuremaj solduloj por partopreni tian <tld/>on
        <fnt>
       <aut>Werner Schad</aut>
          <vrk><url ref="&Monato;/2000/006953.php">Bezonata: nova &leftquot;fremdula
          legio&#x201D;</url></vrk>
          <bib>Monato</bib>
          <lok>2000/07, p. 27</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_O;&c_r;&c_d;&c_e;&c_n; &c_G;&c_a;&c_n;&c_a;&c_r;&c_o;&c_v;&c_a;&c_g;&c_a; &c_l;&c_je;&c_g;&c_ib;&c_jo;&c_n;&c_a;</trd>
  <trd lng="bg">&c_p;&c_o;&c_ch;&c_je;&c_t;&c_n;&c_i;&c_ja; &c_l;&c_je;&c_g;&c_i;&c_o;&c_n;</trd>
  <trd lng="ca">ordre de la Legi&oacute; d'Honor</trd>
  <trd lng="de">Ehrenlegion</trd>
  <trd lng="en">Legion of Honor</trd>
  <trd lng="es">legi&oacute;n de honor</trd>
  <trdgrp lng="fr">
    <trd>l&eacute;gion d'honneur</trd>
  </trdgrp>
  <trd lng="hu">Becs&uuml;letrend</trd>
  <trd lng="it">legion d'onore</trd>
  <trd lng="nl">legioen van eer</trd>
  <trd lng="pl">Legia Honorowa</trd>
  <trd lng="ro">Legiunea de onoare</trd>
  <trd lng="ru">&c_p;&c_o;&c_ch;&c_jo;&c_t;&c_n;&c_y;&c_j; &c_l;&c_je;&c_g;&c_i;&c_o;&c_n; <klr>(&c_o;&c_r;&c_d;&c_je;&c_n;)</klr></trd>
  <trdgrp lng="zh">
    <trd>&#x6CD5;&#x56FD;&#x8363;&#x8A89;&#x519B;&#x56E2;&#x52CB;&#x7AE0; <pr>f&#x1CE;gu&#xF3;r&#xF3;ngy&#xF9;j&#x16B;ntu&#xE1;nx&#x16B;nzh&#x101;ng</pr></trd>,
    <trd>&#x6CD5;&#x570B;&#x69AE;&#x8B7D;&#x8ECD;&#x5718;&#x52F3;&#x7AE0; <pr>f&#x1CE;gu&#xF3;r&#xF3;ngy&#xF9;j&#x16B;ntu&#xE1;nx&#x16B;nzh&#x101;ng</pr></trd>
  </trdgrp>
</drv>
</art>
