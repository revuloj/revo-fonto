<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>legi</rad>/o
</kap>
<drv mrk="legi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Militista korpuso &ccirc;e antikvaj Romanoj, enhavanta &ccirc;irka&ubreve;e 5000 soldatojn.
    </dif>
    <ref tip="sub" cel="kohort.0o">kohorto</ref>
    <refgrp tip="sin">
      <ref cel="falang.0o">falango</ref>,
      <ref cel="regime.0o">regimento</ref>.
    </refgrp>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Multego. 
    </dif>
  </snc>
  <trd lng="en">legion</trd>
  <trd lng="es">legi&oacute;n</trd>
  <trd lng="nl">legioen</trd>
  <trd lng="ru">&c_l;&c_je;&c_g;&c_i;&c_o;&c_n;</trd>
</drv>
<drv mrk="legi.honora0o">
  <kap>honora <tld/>o</kap>
  <snc>
    <dif>
      Plej alta franca ordeno por civila kaj militista merito.  
    </dif>
  </snc>
  <trd lng="en">Legion of Honor</trd>
  <trd lng="nl">legioen van eer</trd>
  <trd lng="ru">&c_p;&c_o;&c_ch;&c_jo;&c_t;&c_n;&c_y;&c_j; &c_l;&c_je;&c_g;&c_i;&c_o;&c_n; <klr>(&c_o;&c_r;&c_d;&c_je;&c_n;)</klr></trd>
</drv>
</art>
