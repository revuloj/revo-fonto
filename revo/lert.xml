<drv mrk="lert.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="lert.0ajxo.KOMUNE">
    <dif>
      Faro <tld/>a, montranta rapidecon, flekseblecon kaj senerarecon
      de la plenumanto:
      <ekz>
        <tld/>a&jcirc;oj politikaj
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> povas fanfaroni nur pro malmulto da artoj a&ubreve; <tld/>a&jcirc;oj
        <fnt>
          <bib>Rob</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi promesis al &scirc;i surprizon &ccirc;e la sekvanta koncerto
        &ndash; etan muzikan <tld/>a&jcirc;on, kiu aludos mian ka&scirc;itan
        amon al &scirc;i
        <fnt>
          <aut>Wang Chongfang</aut>,
          <vrk><url ref="http://bbs.budhano.com/m/index.php?a=read&amp;tid=1035">
          Amo per Violono</url></vrk>,
          <lok>2010-07-26</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akroba.0ajxo">akrobata&jcirc;o</ref>,
      <ref cel="art.0ajxo">arta&jcirc;o</ref>,
      <ref cel="majstr.0ajxo">majstra&jcirc;o</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_ja;&c_v;&c_a; <ind>&c_s;&c_p;&c_r;&c_y;&c_t;&c_u;</ind></trd>,
    <trd>&c_t;&c_r;&c_u;&c_k;</trd>,
    <trd>&c_v;&c_y;&c_k;&c_r;&c_u;&c_t;</trd>
  </trdgrp>
  <trd lng="de">Kunstst&uuml;ck</trd>
  <trdgrp lng="es">
    <trd>habilidad</trd>,
    <trd>destreza</trd>
  </trdgrp>
  <trd lng="fr">tour<klr> (d'adresse)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x5DE7;&#x307F;&#x306A;&#x52D5;&#x304D;
