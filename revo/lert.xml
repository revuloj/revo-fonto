    <trd>terampil</trd>
  </trdgrp>
  </snc>
  <snc mrk="lert.0a.rezulto">
    <dif>
      Farita la&ubreve; <tld/>a maniero:
      <ekz>
        multaj pastroj ekiris la vojon de l' mensogo kaj per <tld/>aj artifikoj trompas
        la simplulojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun <tld/>a saltetado &scirc;i iris rekte malsupren, al la mar&ccirc;o
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        per <tld/>a pafo <klr>[li]</klr> forpu&scirc;is la kranion
        <fnt>
          <bib>ElektFab</bib>
          <lok>Sesope oni trairas la tutan mondon</lok>
        </fnt>;
      </ekz>
      <ekz>
        tre malmultaj lasis sin kapti per <tld/>aj vortoj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en Dresden en la 17a de a&ubreve;gusto
          1908</vrk>
        </fnt>;
      </ekz>
      <ekz>
        nevideble de ekstere pro <tld/>a aran&gcirc;o de arboj kaj he&gcirc;o <klr>[trovi&gcirc;is]</klr>
        &gcirc;ardeno mirinde arte ornamita
        <fnt>
          <bib>ChR</bib>
        </fnt>;
      </ekz>
      <ekz>
        per <tld/>a admono <klr>(reklamo)</klr> alfluas la mono
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="art.0a.artefarita">arta</ref>,
      <ref cel="artifi.0a">artifika</ref>,
      <ref cel="rafin.0ita">rafinita</ref>
    </refgrp>
    <trdgrp lng="de">
