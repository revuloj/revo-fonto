<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lert</rad>/a
</kap>
<drv mrk="lert.0a">
  <kap>
    <ofc>*</ofc><tld/>a
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="kapabl.0a">Kapabla</ref> facile kaj la&ubreve;cele fari ion pro la rapideco kaj fleksebleco
      de siaj movoj:
      <ekz>
        <tld/>a metiisto, kirurgo;
      </ekz>
      <ekz>
        <tld/>aj manoj;
      </ekz>
      <ekz>
        leonoj kaj tigroj simile al <tld/>aj katoj saltadis inter la
        verdaj plektobariloj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la cikoniidoj falis, &ccirc;ar ili estis ankora&ubreve; tro
        mal<tld/>aj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        rapide kaj <tld/>e kuradis lacertoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="facil">facilmova</ref>,
      <ref cel="graci.0a">gracia</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      <ref tip="super" cel="kapabl.0a">Kapabla</ref> facile kaj la&ubreve;cele plenumi ion pro la rapideco kaj trovemeco
      de sia penso:
      <ekz>
        <tld/>a politikisto, oratoro;
      </ekz>
      <ekz>
        <tld/>a en sia profesio<fnt>Z</fnt>,
      </ekz>
      <ekz>
        fako;
      </ekz>
      <ekz>
        mal<tld/>a &scirc;telisto;
      </ekz>
      <ekz>
        ne<tld/>a junulo
        (
        <refgrp tip="sin">
          <ref cel="naiv.0a">naiva</ref>,
          <ref cel="malic">senmalica</ref>
        </refgrp>
        );
      </ekz>
      <ekz>
        <tld/>e ka&scirc;i ion<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="spert.0a">sperta</ref>,
      <ref cel="kompet.0a">kompetenta</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Farita la&ubreve; <tld/>a maniero:
      <ekz>
        <tld/>a laboro, dancado;
      </ekz>
      <ekz>per <tld/>a admono (reklamo) alfluas la mono
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        la birdetoj faris malgrandan  mal<tld/>an movi&gcirc;on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mal<tld/>a propono.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">skillful</trd>
  <trdgrp lng="es">
    <trd>h&aacute;bil</trd>,
    <trd>diestro</trd>,
    <trd>ama&ntilde;ado</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>handig</trd>,
    <trd>bekwaam</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>&aacute;gil</trd>,
    <trd>habilidoso</trd>,
    <trd>h&aacute;bil</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_m;&c_je;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_l;&c_o;&c_v;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_i;&c_s;&c_k;&c_u;&c_s;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">skicklig</trd>
</drv>
</art>
