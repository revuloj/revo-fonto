<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lert.xml,v 1.26 2021/05/29 10:57:18 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lert</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="lert.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="lert.0a.korpo">
    <dif>
      <ref tip="super" cel="kapabl.0a">Kapabla</ref> facile kaj
      la&ubreve;cele fari ion pro la rapideco kaj fleksebleco
      de siaj movoj:
      <ekz>
        mi saltas tre <tld/>e
        <fnt><bib>F</bib>, <lok>&FE; 40</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a metiisto, &hcirc;irurgo;
      </ekz>
      <ekz>
        <tld/>aj manoj;
      </ekz>
      <ekz>
        leonoj kaj tigroj simile al <tld/>aj katoj saltadis inter la
        verdaj plektobariloj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        rapide kaj <tld/>e kuradis lacertoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="facil">facilmova</ref>,
      <ref cel="graci.0a">gracia</ref>
    </refgrp>
    <trd lng="de">geschickt</trd>
  </snc>
  <snc mrk="lert.0a.menso">
    <dif>
      <ref tip="super" cel="kapabl.0a">Kapabla</ref> facile kaj
      la&ubreve;cele plenumi ion pro la rapideco kaj trovemeco
      de sia penso:
      <ekz>
        li estas ne<tld/>a kaj naiva provincano
        <fnt><bib>F</bib>, <lok>&FE; 37</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a politikisto, oratoro;
      </ekz>
      <ekz>
        <tld/>a en sia profesio<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e ka&scirc;i ion<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">f&auml;hig</trd>
  </snc>
  <snc mrk="lert.0a.rezulto">
    <dif>
      Farita la&ubreve; <tld/>a maniero:
      <ekz>
        <tld/>a laboro, dancado;
      </ekz>
      <ekz>
        per <tld/>a admono (reklamo) alfluas la mono
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="de">kunstvoll</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_m;&c_je;&c_l;&c_y;</trd>,
    <trd>&c_s;&c_p;&c_r;&c_y;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_u;&c_p;&c_r;&c_a;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_u;&c_v;&c_ib;&c_sh;&c_n;&c_y;</trd>,
    <trd>&c_u;&c_d;&c_a;&c_l;&c_y;</trd>,
    <trd>&c_h;&c_v;&c_a;&c_c;&c_k;&c_ib;</trd>,
    <trd>&c_m;&c_a;&c_j;&c_s;&c_t;&c_e;&c_r;&c_s;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="bo">&#xF66;&#xFA4;&#xFB1;&#xF44;&#xF0B;&#xF54;&#xF7C;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>dovedn&yacute;</trd>,
    <trd>obratn&yacute;</trd>,
    <trd>zb&ecaron;hl&yacute;</trd>,
    <trd>zru&ccaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="en">skillful</trd>
  <trdgrp lng="es">
    <trd>h&aacute;bil</trd>,
    <trd>diestro</trd>,
    <trd>&aacute;gil</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>adroit</trd>,
    <trd>agile</trd>,
    <trd>habile</trd>
  </trdgrp>
  <trd lng="hu">&uuml;gyes</trd>
  <trdgrp lng="nl">
    <trd>handig</trd>,
    <trd>bekwaam</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>do&sacute;wiadczony</trd>,
    <trd>wprawny</trd>,
    <trd>zaradny</trd>,
    <trd>zr&eogonek;czny</trd>,
    <trd>zwinny</trd>
  </trdgrp>   
  <trdgrp lng="pt">
    <trd>esperto</trd>,
    <trd>&aacute;gil</trd>,
    <trd>habilidoso</trd>,
    <trd>h&aacute;bil</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>&icirc;ncercat</trd>,
    <trd>priceput</trd>,
    <trd>ingenios</trd>
  </trdgrp>  
  <trdgrp lng="ru">
    <trd>&c_u;&c_m;&c_je;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_l;&c_o;&c_v;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_i;&c_s;&c_k;&c_u;&c_s;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>obratn&yacute;</trd>,
    <trd>zru&ccaron;n&yacute;</trd>,
    <trd>&scaron;ikovn&yacute;</trd>
  </trdgrp>
  <trd lng="sv">skicklig</trd>
</drv>

<drv mrk="lert.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="lert.0ajxo.KOMUNE">
    <dif>
      Faro, kiu postulas &gcirc;ustecon,
      rapidecon, senerarecon:
      <ekz>
        mi promesis al &scirc;i surprizon &ccirc;e la sekvanta koncerto
        &ndash; etan muzikan <tld/>a&jcirc;on, kiu aludos mian ka&scirc;itan
        amon al &scirc;i
        <fnt>
          <aut>Wang Chongfang</aut>,
          <vrk><url ref="http://bbs.budhano.com/m/index.php?a=read&amp;tid=1035">
          Amo per Violono</url></vrk>,
          <lok>2010-07-26</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akroba.0ajxo">akrobata&jcirc;o</ref>
    </refgrp>
  </snc>
  <trdgrp lng="es">
    <trd>habilidad</trd>,
    <trd>destreza</trd>
  </trdgrp>
  <trd lng="fr">tour<klr> (d'adresse)</klr></trd>
  <trd lng="pl">okazanie zdolno&sacute;ci</trd>     
  <trd lng="ro">demonstrarea abilit&abreve;&#x21B;ii</trd>
</drv>

<drv mrk="lert.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="lert.0eco.KOMUNE">
    <dif>
      Kapablo por rapide, facile, celtrafe fari ion:
      <ekz>
        kun <tld/>eco de l' kato glitas li sur kornicoj
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="takt.0o.gxentileco">takto<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="cs">dovednost</trd>
  <trdgrp lng="de">
    <trd>geschickt</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>agilidad</trd>,
    <trd>habilidad</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>adresse<klr> (habilet&eacute;)</klr></trd>,
    <trd>agilit&eacute;</trd>,
    <trd>habilet&eacute;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bieg&lstroke;o&sacute;&cacute;</trd>,
    <trd>sprawno&sacute;&cacute;</trd>,
    <trd>zr&eogonek;czno&sacute;&cacute;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>&icirc;ndem&acirc;nare</trd>,
    <trd>adres&abreve;</trd>,
    <trd>dexteritate</trd>
  </trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_v;&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_n;&c_o;&c_r;&c_o;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_u;&c_m;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_i;&c_s;&c_k;&c_u;&c_s;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_s;&c_t;&c_je;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">zru&ccaron;nos&tcaron;</trd>
</drv>

<drv mrk="lert.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="lert.0igxi.KOMUNE">
    <dif>
      I&gcirc;i pli kaj pli kapabla por agi rapide kaj efike:
      <ekz>
        mi laboris en magazeno senpage, por nur <tld/>i&gcirc;i
        en la altran&ccirc;ado
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">volverse <ind>h&aacute;bil</ind></trd>
  <trd lng="fr">devenir <ind>habile</ind></trd>
  <trd lng="pl">sta&cacute; si&eogonek; zwinnym</trd>     
  <trd lng="ro">devin agili</trd>
  <trd lng="ru">&c_n;&c_a;&c_l;&c_o;&c_v;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="lert.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="lert.0ulo.KOMUNE">
    <dif>
      Homo kapabla rapide, efike fari ion, pli bone ol aliaj:
      <ekz>
        farita&jcirc;o de <tld/>uloj
        <fnt><bib>MT</bib>, <lok>&Jer; 10:9</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">zru&ccaron;n&yacute; &ccaron;lov&ecaron;k</trd>
  <trdgrp lng="de">
    <trd>Meister</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>hombre h&aacute;bil</trd>,
    <trd>astuto</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>homme habile</trd>,
    <trd>malin<klr> (homme habile)</klr></trd>
  </trdgrp>
  <trd lng="pl">bieg&lstroke;y</trd>     
  <trd lng="ro">expert</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_m;&c_je;&c_l;&c_y;&c_j; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>,
    <trd>&c_u;&c_m;&c_je;&c_l;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="sk">zru&ccaron;n&yacute; &ccaron;lovek</trd>
</drv>

<drv mrk="lert.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="lert.mal0a.korpo">
    <dif>
      Korpe embarasata en siaj movoj, ne glate gestanta a&ubreve; aganta:
      <ekz>
        ili falis, &ccirc;ar ili estis ankora&ubreve; tro
        mal<tld/>aj
        <fnt><bib>Fab1</bib>, <lok>cikonioj</lok></fnt>;
      </ekz>
      <ekz>
        la <klr>birdetoj</klr> faris malgrandan mal<tld/>an movi&gcirc;on
        <fnt><bib>Fab1</bib>, <lok>cikonioj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="lert.mal0a.menso">
    <dif>
      Mense ne trafanta la &gcirc;ustajn solvojn, do ne aganta bone por
      plenumi
      siajn celojn:
      <ekz>
        mal<tld/>a &scirc;telisto;
      </ekz>
      <ekz>
        mal<tld/>a junulo.
      </ekz>
    </dif>
  </snc>
  <snc mrk="lert.mal0a.rezulto">
    <dif>
      Aspektanta kiel rezulto de mal<tld/>a agado:
      <ekz>
        mal<tld/>a propono.
      </ekz>
    </dif>
  </snc>
    <refgrp tip="vid">
      <ref cel="naiv.0a">naiva</ref>,
      <ref cel="plump.0a">plumpa</ref>,
      <ref cel="malic">senmalica</ref>
    </refgrp>
  <trdgrp lng="es">
    <trd>lerdo</trd>,
    <trd>inh&aacute;bil</trd>,
    <trd>inepto</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gauche<klr> (maladroit)</klr></trd>,
    <trd>maladroit</trd>,
    <trd>malhabile</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>niezdarny</trd>,
    <trd>nieudolny</trd>,
    <trd>niezr&eogonek;czny</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>st&acirc;ngaci</trd>,
    <trd>ne&icirc;ndem&acirc;natic</trd>,
    <trd>nepriceput</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_l;&c_o;&c_v;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_u;&c_m;&c_je;&c_l;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nemotorn&yacute;</trd>,
    <trd>ne&scaron;ikovn&yacute;</trd>,
    <trd>&tcaron;arbav&yacute;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nevzd&ecaron;lan&yacute;</trd>,
    <trd>nezru&ccaron;n&yacute;</trd>,
    <trd>ne&scaron;ikovn&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="lert.man0a">
  <kap>man<tld/>a</kap>
  <snc mrk="lert.man0a.KOMUNE">
    <dif>
      <tld lit="L"/>e uzanta siajn manojn:
      <ekz>
        li estis tiel man<tld/>a ekde la infaneco ke li 
        propramane faris miniaturon de fortika&jcirc;o por ludi 
        batalludon
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://lernularusan.wordpress.com/2013/10/23/petro-la-1-a-certe-estis-carma-viro">
          Petro la la Unua certe estis &ccirc;arma viro!</url></vrk>,
          <lok>Lernu la rusan lingvon!, 2013-10-23</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">h&aacute;bil con sus manos</trd>
  <trd lng="fr">adroit de ses mains</trd>
  <trd lng="pl">sprawny manualnie</trd>     
  <trd lng="ro">lucr&acirc;nd manual</trd>
</drv>
</art>
