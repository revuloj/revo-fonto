<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lert.xml,v 1.28 2021/12/13 15:34:56 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lert</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="lert.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="lert.0a.korpo">
    <dif>
      <ref tip="super" cel="kapabl.0a">Kapabla</ref> facile kaj
      la&ubreve;cele fari ion pro la rapideco kaj fleksebleco
      de siaj movoj:
      <ekz>
        mi saltas tre <tld/>e
        <fnt><bib>F</bib>, <lok>&FE; 40</lok></fnt>;
      </ekz>
      <ekz>
        la knaboj grandi&gcirc;is kaj <nom>Esav</nom> fari&gcirc;is <tld/>a &ccirc;asisto, 
        kampisto, kaj <nom>Jakob</nom> fari&gcirc;is homo kvieta, sidanta en la tendo
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 25:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        li pa&scirc;tis ilin <klr>[&#8230;]</klr> kaj per <tld/>aj manoj li ilin kondukis
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 78:72</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> estas &ccirc;armaj junuloj, <tld/>aj rajdistoj
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 23:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia lango estas kiel plumo de <tld/>a skribisto
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 45:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi vidas, ke vi estas <tld/>a knabo, &dash; pretigu do tie ion por man&gcirc;i!
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u <tld/>a kapreolo povas forkuri de la okulo de sperta &ccirc;asisto
        <fnt>
          <bib>Marta</bib>
        </fnt>?
      </ekz>
      <ekz>
        &ccirc;ie estas francaj &hcirc;irurgoj: unu el ili, kiu estis tre <tld/>a, prizorgis nin
        <fnt>
          <bib>Kandid</bib>
        </fnt>;
      </ekz>
      <ekz>
        verko simpla, prezentanta unu el la scenoj de la &ccirc;iutaga vivo, sed, farita de
        <tld/>a kaj inspirita mano de talenta desegnisto
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi havas ne<tld/>an bu&scirc;on kaj ne<tld/>an langon
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 4:10</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="facil">facilmova</ref>,
      <ref cel="graci.0a">gracia</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>gewandt</trd>,
      <trd>wendig</trd>,
      <trd>beh&auml;nde</trd>
    </trdgrp>
  </snc>
  <snc mrk="lert.0a.menso">
    <dif>
      <ref tip="super" cel="kapabl.0a">Kapabla</ref> facile kaj
      la&ubreve;cele plenumi ion pro la rapideco kaj trovemeco
      de sia penso:
      <ekz>
        homo <ref cel="spert.0a">sperta</ref> kaj <tld/>a
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        persono bonorda, akurata, <tld/>a en kalkulado
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        li estas ne<tld/>a kaj naiva provincano
        <fnt><bib>F</bib>, <lok>&FE; 37</lok></fnt>;
      </ekz>
      <ekz>
        li estis <tld/>a scienculo koncerne la instruon de Moseo
        <fnt>
          <bib>MT</bib>
          <lok>Ezra 7:6</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kompet.0a.kapabla">kompetenta</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>f&auml;hig</trd>,
      <trd>ge&uuml;bt</trd>
    </trdgrp>
  </snc>
  <snc mrk="lert.0a.rezulto">
    <dif>
      Farita la&ubreve; <tld/>a maniero:
      <ekz>
        multaj pastroj ekiris la vojon de l' mensogo kaj per <tld/>aj artifikoj trompas
        la simplulojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun <tld/>a saltetado &scirc;i iris rekte malsupren, al la mar&ccirc;o
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        per lerta pafo <klr>[li]</klr> forpu&scirc;is la kranion
        <fnt>
          <bib>ElektFab</bib>
          <lok>Sesope oni trairas la tutan mondon</lok>
        </fnt>;
      </ekz>
      <ekz>
        tre malmultaj lasis sin kapti per <tld/>aj vortoj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en Dresden en la 17a de a&ubreve;gusto 1908</vrk>
        </fnt>;
      </ekz>
      <ekz>
        nevideble de ekstere pro <tld/>a aran&gcirc;o de arboj kaj he&gcirc;o <klr>[trovi&gcirc;is]</klr>
        &gcirc;ardeno mirinde arte ornamita
        <fnt>
          <bib>ChR</bib>
        </fnt>;
      </ekz>
      <ekz>
        per <tld/>a admono <klr>(reklamo)</klr> alfluas la mono
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="artifi.0a">artifika</ref>,
      <ref cel="rafin.0ita">rafinita</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>kunstvoll</trd>,
      <trd>gekonnt</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_m;&c_je;&c_l;&c_y;</trd>,
    <trd>&c_s;&c_p;&c_r;&c_y;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_u;&c_p;&c_r;&c_a;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_u;&c_v;&c_ib;&c_sh;&c_n;&c_y;</trd>,
    <trd>&c_u;&c_d;&c_a;&c_l;&c_y;</trd>,
    <trd>&c_h;&c_v;&c_a;&c_c;&c_k;&c_ib;</trd>,
    <trd>&c_m;&c_a;&c_j;&c_s;&c_t;&c_e;&c_r;&c_s;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="bo">&t_sa;&t_pata;&t_yata;&t_nga;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>dovedn&yacute;</trd>,
    <trd>obratn&yacute;</trd>,
    <trd>zb&ecaron;hl&yacute;</trd>,
    <trd>zru&ccaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="de">geschickt</trd>
  <trd lng="en">skillful</trd>
  <trdgrp lng="es">
    <trd>h&aacute;bil</trd>,
    <trd>diestro</trd>,
    <trd>&aacute;gil</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>adroit</trd>,
    <trd>agile</trd>,
    <trd>habile</trd>
  </trdgrp>
  <trd lng="hu">&uuml;gyes</trd>
  <trdgrp lng="nl">
    <trd>handig</trd>,
    <trd>bekwaam</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>do&sacute;wiadczony</trd>,
    <trd>wprawny</trd>,
    <trd>zaradny</trd>,
    <trd>zr&eogonek;czny</trd>,
    <trd>zwinny</trd>
  </trdgrp>   
  <trdgrp lng="pt">
    <trd>esperto</trd>,
    <trd>&aacute;gil</trd>,
    <trd>habilidoso</trd>,
    <trd>h&aacute;bil</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>&icirc;ncercat</trd>,
    <trd>priceput</trd>,
    <trd>ingenios</trd>
  </trdgrp>  
  <trdgrp lng="ru">
    <trd>&c_u;&c_m;&c_je;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_l;&c_o;&c_v;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_i;&c_s;&c_k;&c_u;&c_s;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>obratn&yacute;</trd>,
    <trd>zru&ccaron;n&yacute;</trd>,
    <trd>&scaron;ikovn&yacute;</trd>
  </trdgrp>
  <trd lng="sv">skicklig</trd>
</drv>

<drv mrk="lert.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="lert.0ajxo.KOMUNE">
    <dif>
      Faro <tld/>a, montranta rapidecon, flekseblecon kaj senerarecon
      de la plenumanto:
      <ekz>
        <tld/>a&jcirc;oj politikaj
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> povas fanfaroni nur pro malmulto da artoj a&ubreve; <tld/>a&jcirc;oj
        <fnt>
          <bib>Rob</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi promesis al &scirc;i surprizon &ccirc;e la sekvanta koncerto
        &ndash; etan muzikan <tld/>a&jcirc;on, kiu aludos mian ka&scirc;itan
        amon al &scirc;i
        <fnt>
          <aut>Wang Chongfang</aut>,
          <vrk><url ref="http://bbs.budhano.com/m/index.php?a=read&amp;tid=1035">
          Amo per Violono</url></vrk>,
          <lok>2010-07-26</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akroba.0ajxo">akrobata&jcirc;o</ref>,
      <ref cel="art.0ajxo">arta&jcirc;o</ref>,
      <ref cel="majstr.0ajxo">majstra&jcirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="de">Kunstst&uuml;ck</trd>
  <trdgrp lng="es">
    <trd>habilidad</trd>,
    <trd>destreza</trd>
  </trdgrp>
  <trd lng="fr">tour<klr> (d'adresse)</klr></trd>
  <trd lng="pl">okazanie zdolno&sacute;ci</trd>     
  <trd lng="ro">demonstrarea abilit&abreve;&#539;ii</trd>
</drv>

<drv mrk="lert.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="lert.0eco.KOMUNE">
    <dif>
      Kapablo por rapide, facile, celtrafe fari ion:
      <ekz>
        kun <tld/>eco de l' kato glitas li sur kornicoj
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="takt.0o.gxentileco">takto<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="cs">dovednost</trd>
  <trdgrp lng="de">
    <trd>geschickt</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>agilidad</trd>,
    <trd>habilidad</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>adresse<klr> (habilet&eacute;)</klr></trd>,
    <trd>agilit&eacute;</trd>,
    <trd>habilet&eacute;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bieg&lstroke;o&sacute;&cacute;</trd>,
    <trd>sprawno&sacute;&cacute;</trd>,
    <trd>zr&eogonek;czno&sacute;&cacute;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>&icirc;ndem&acirc;nare</trd>,
    <trd>adres&abreve;</trd>,
    <trd>dexteritate</trd>
  </trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_v;&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_n;&c_o;&c_r;&c_o;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_u;&c_m;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_i;&c_s;&c_k;&c_u;&c_s;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_s;&c_t;&c_je;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">zru&ccaron;nos&tcaron;</trd>
</drv>

<drv mrk="lert.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="lert.0igxi.KOMUNE">
    <dif>
      I&gcirc;i pli kaj pli kapabla por agi rapide kaj efike:
      <ekz>
        mi laboris en magazeno senpage, por nur <tld/>i&gcirc;i
        en la altran&ccirc;ado
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">volverse <ind>h&aacute;bil</ind></trd>
  <trd lng="fr">devenir <ind>habile</ind></trd>
  <trd lng="pl">sta&cacute; si&eogonek; zwinnym</trd>     
  <trd lng="ro">devin agili</trd>
  <trd lng="ru">&c_n;&c_a;&c_l;&c_o;&c_v;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="lert.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="lert.0ulo.KOMUNE">
    <dif>
      Homo kapabla rapide, efike fari ion, pli bone ol aliaj:
      <ekz>
        farita&jcirc;o de <tld/>uloj
        <fnt><bib>MT</bib>, <lok>&Jer; 10:9</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">zru&ccaron;n&yacute; &ccaron;lov&ecaron;k</trd>
  <trdgrp lng="de">
    <trd>Meister</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>hombre h&aacute;bil</trd>,
    <trd>astuto</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>homme habile</trd>,
    <trd>malin<klr> (homme habile)</klr></trd>
  </trdgrp>
  <trd lng="pl">bieg&lstroke;y</trd>     
  <trd lng="ro">expert</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_m;&c_je;&c_l;&c_y;&c_j; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>,
    <trd>&c_u;&c_m;&c_je;&c_l;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="sk">zru&ccaron;n&yacute; &ccaron;lovek</trd>
</drv>

<drv mrk="lert.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="lert.mal0a.korpo">
    <dif>
      Korpe embarasata en siaj movoj, ne glate gestanta a&ubreve; aganta:
      <ekz>
        ili falis, &ccirc;ar ili estis ankora&ubreve; tro
        mal<tld/>aj
        <fnt><bib>Fab1</bib>, <lok>cikonioj</lok></fnt>;
      </ekz>
      <ekz>
        la <klr>birdetoj</klr> faris malgrandan mal<tld/>an movi&gcirc;on
        <fnt><bib>Fab1</bib>, <lok>cikonioj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="lert.mal0a.menso">
    <dif>
      Mense ne trafanta la &gcirc;ustajn solvojn, do ne aganta bone por
      plenumi
      siajn celojn:
      <ekz>
        mal<tld/>a &scirc;telisto;
      </ekz>
      <ekz>
        mal<tld/>a junulo.
      </ekz>
    </dif>
  </snc>
  <snc mrk="lert.mal0a.rezulto">
    <dif>
      Aspektanta kiel rezulto de mal<tld/>a agado:
      <ekz>
        mal<tld/>a propono.
      </ekz>
    </dif>
  </snc>
    <refgrp tip="vid">
      <ref cel="naiv.0a">naiva</ref>,
      <ref cel="plump.0a">plumpa</ref>,
      <ref cel="malic">senmalica</ref>
    </refgrp>
  <trdgrp lng="es">
    <trd>lerdo</trd>,
    <trd>inh&aacute;bil</trd>,
    <trd>inepto</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gauche<klr> (maladroit)</klr></trd>,
    <trd>maladroit</trd>,
    <trd>malhabile</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>niezdarny</trd>,
    <trd>nieudolny</trd>,
    <trd>niezr&eogonek;czny</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>st&acirc;ngaci</trd>,
    <trd>ne&icirc;ndem&acirc;natic</trd>,
    <trd>nepriceput</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_l;&c_o;&c_v;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_u;&c_m;&c_je;&c_l;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nemotorn&yacute;</trd>,
    <trd>ne&scaron;ikovn&yacute;</trd>,
    <trd>&tcaron;arbav&yacute;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nevzd&ecaron;lan&yacute;</trd>,
    <trd>nezru&ccaron;n&yacute;</trd>,
    <trd>ne&scaron;ikovn&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="lert.man0a">
  <kap>man<tld/>a</kap>
  <snc mrk="lert.man0a.KOMUNE">
    <dif>
      <tld lit="L"/>e uzanta siajn manojn:
      <ekz>
        li estis tiel man<tld/>a ekde la infaneco ke li 
        propramane faris miniaturon de fortika&jcirc;o por ludi 
        batalludon
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://lernularusan.wordpress.com/2013/10/23/petro-la-1-a-certe-estis-carma-viro">
          Petro la la Unua certe estis &ccirc;arma viro!</url></vrk>,
          <lok>Lernu la rusan lingvon!, 2013-10-23</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">h&aacute;bil con sus manos</trd>
  <trd lng="fr">adroit de ses mains</trd>
  <trd lng="pl">sprawny manualnie</trd>     
  <trd lng="ro">lucr&acirc;nd manual</trd>
</drv>
</art>
