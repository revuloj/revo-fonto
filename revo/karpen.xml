<art mrk="$Id: karpen.xml,v 1.24 2023/09/01 13:58:56 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>karpen</rad>/o
</kap>
<drv mrk="karpen.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="betula.0oj">betulacoj</ref>
      (<trd lng="la">Carpinus</trd>), arbo simila al fago, kun glata
      griza &scirc;elo kaj tre hela ligno:
      <ekz>
        la plej konata bonsaj-spertulo en Litovio, asertas, ke anka&ubreve;
        litovaj arboj plene ta&ubreve;gas por bonsaji&gcirc;o, la plej ta&ubreve;ga estas
        <tld/>o, kiu toleras sekecon kaj humidecon
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2004/008435.html">Etaj plantoj, egaj planoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/karpen.jpg">
    folioj de <ind>blanka <tld/>o</ind>
    <trd lng="ca">carp&iacute;</trd>
    <trd lng="fr">charme commun</trd>
    <trd lng="la">Carpinus betulus</trd>
  </bld>
  <bld lok="&WCU;/thumb/d/dc/Hainbuche.jpg/320px-Hainbuche.jpg" prm="CC-BY-SA-3.0"><tld/>fruktoj

       <fnt>
         <aut>ArtMechanic</aut>
         <lok><url ref="&WCW;/File:Hainbuche.jpg">&WCW;/File:Hainbuche.jpg</url></lok>
       </fnt>
  </bld>
  <trd lng="be">&c_g;&c_r;&c_a;&c_b;</trd>
  <trdgrp lng="br">
    <trd>faouenn-but</trd>,
    <trd>gwezenn-faou-put</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>carpinus</trd>,
    <trd>carp&iacute;</trd>
  </trdgrp>
  <trd lng="de">Hainbuche</trd>
  <trdgrp lng="en">
    <trd>hornbeam</trd>,
    <trd>yoke elm</trd>
  </trdgrp>
  <trd lng="fr">charme<klr tip="ind"> (arbre)</klr></trd>
  <trd lng="hu">gyerty&aacute;n(fa)</trd>
  <trd lng="ja">&#x30AF;&#x30DE;&#x30B7;&#x30C7;</trd>
  <trd lng="nl">haagbeuk</trd>
  <trd lng="pl">grab</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_b;</trd>
  <trd lng="uk">&#x433;&#x440;&#x430;&#x431;</trd>
</drv>
</art>
