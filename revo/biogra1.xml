<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: biogra1.xml,v 1.28 2023/10/14 08:39:48 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>biografi</rad>/o
</kap>
<drv mrk="biogra1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="verk.0o.propra">Verko</ref>
      rakontanta pri la tuta vivo de iu persono:
      <ekz>leginda ampleksa <tld/>o [...] de la
      a&ubreve;toro
      <fnt>
      <aut>Franz-Georg R&ouml;ssler</aut>:
      <vrk>Profite voja&gcirc;u kun Edwin</vrk>
      <bib>Monato</bib>
      <lok>1994/06, p. 21</lok></fnt>.</ekz>
    </dif>
    <ref tip="vid" cel="biogra.0o">biografo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ib;&c_ja;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>,
    <trd>&c_zh;&c_y;&c_c;&c_mol;&c_c;&c_ja;&c_p;&c_ib;&c_s;</trd>
  </trdgrp>
  <trd lng="bg">&c_b;&c_i;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
  <trd lng="bo">&#xF62;&#xFA3;&#xF58;&#xF0B;&#xF50;&#xF62;&#xF0B;</trd>
  <trd lng="ca">biografia</trd>
  <trdgrp lng="cs">
    <trd>biografie</trd>,
    <trd>&zcaron;ivotopis</trd>
  </trdgrp>
  <trd lng="de">Biografie</trd>
  <trd lng="en">biography</trd>
  <trd lng="es">biograf&iacute;a</trd>
  <trdgrp lng="fa">
    <trd>&a_z;&a_n;&a_d;&f_g;&f_y;&#8204;&a_n;&a_A;&a_m;&a_h;</trd>,
    <trd>&a_sh;&a_r;&a_H; &a_H;&a_A;&a_l;<klr tip="ind"> (&a_z;&a_n;&a_d;&f_g;&f_y;&#8204;&a_n;&a_A;&a_m;&a_h;)</klr></trd>,
    <trd>&a_b;&f_y;&a_w;&f_g;&a_r;&a_A;&a_f;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">biographie</trd>
  <trd lng="he">&#x5D1;&#x5D9;&#x5D5;&#x5D2;&#x5E8;&#x5E4;&#x5D9;&#x5D4;</trd>
  <trdgrp lng="hu">
    <trd>&eacute;letrajz</trd>,
    <trd>biogr&aacute;fia</trd>
  </trdgrp>
  <trd lng="ja">&#x4F1D;&#x8A18;  <pr>&#12391;&#12435;&#x304D;</pr></trd>
  <trd lng="nl">biografie</trd>
  <trd lng="pl">biografia</trd>
  <trd lng="ru">&c_b;&c_i;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>biografia</trd>,
    <trd>&zcaron;ivotopis</trd>
  </trdgrp>
  <trd lng="uk">&c_b;&c_ib;&#x43E;&c_g;&c_r;&c_a;&c_f;&c_ib;&#x44F;</trd>
</drv>

<drv mrk="biogra1.mem0o">
  <kap>mem<tld/>o</kap>
  <uzo tip="fak">BELE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="verk.0o.propra">Verko</ref>
      rakonta pri la propra vivo de la verkinto.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="auxtob.0o">a&ubreve;tobiografio</ref>
  </refgrp>
  <trd lng="be">&c_a;&c_w;&c_t;&c_a;&c_b;&c_ib;&c_ja;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_a;&c_v;&c_t;&c_o;&c_b;&c_i;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
  <trd lng="de">Autobiografie</trd>
  <trd lng="en">autobiography</trd>
  <trd lng="es">autobiograf&iacute;a</trd>
  <trdgrp lng="fa">
    <trd>&a_kh;&a_w;&a_d;&a_z;&a_n;&a_d;&f_g;&f_y;&#8204;&a_n;&a_A;&a_m;&a_h;</trd>,
    <trd>&a_H;&a_s;&a_b; &a_H;&a_A;&a_l;</trd>,
    <trd>&a_A;&a_t;&a_w;&a_b;&f_y;&a_w;&f_g;&a_r;&a_A;&a_f;&f_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>autobiographie</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&ouml;n&eacute;letrajz</trd>,
    <trd>autobiogr&aacute;fia</trd>
  </trdgrp>
  <trd lng="ca">autobiografia</trd>
  <trd lng="ja">&#x81EA;&#x4F1D;  <pr>&#12376;&#12391;&#12435;</pr></trd>
  <trd lng="nl">autobiografie</trd>
  <trd lng="pl">autobiografia</trd>
  <trd lng="ru">&c_a;&c_v;&c_t;&c_o;&c_b;&c_i;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
  <trd lng="sk">autobiografia</trd>
  <trd lng="sk">vlastn&yacute; &zcaron;ivotopis</trd>
  <trd lng="cs">vlastn&iacute; &zcaron;ivotopis</trd>
  <trd lng="uk">&c_a;&c_v;&c_t;&#x43E;&c_b;&c_ib;&#x43E;&c_g;&c_r;&c_a;&c_f;&c_ib;&#x44F;</trd>
</drv>
</art>
