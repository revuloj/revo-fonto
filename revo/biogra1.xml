<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>biografi</rad>/o
</kap>
<drv mrk="biogra1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="verk.0o.propra">Verko</ref>
      rakontanta pri la tuta vivo de iu persono:
      <ekz>leginda ampleksa <tld/>o [...] de la 
      a&ubreve;toro<fnt><aut>Franz-Georg R&ouml;ssler</aut>:
      <vrk>Profite voja&gcirc;u kun
      Edwin</vrk>, <bib>Monato</bib><lok>jaro 1994a, numero 6a, p.
      21a</lok></fnt>.</ekz>
    </dif>
    <ref tip="vid" cel="biogra.0o">biografo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ib;&c_ja;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>,
    <trd>&c_zh;&c_y;&c_c;&c_mol;&c_c;&c_ja;&c_p;&c_ib;&c_s;</trd>
  </trdgrp>
  <trd lng="bg">&c_b;&c_i;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
  <trd lng="ca">biografia</trd>
  <trd lng="de">Biografie</trd>
  <trd lng="en">biography</trd>
  <trd lng="es">biograf&iacute;a</trd>
  <trd lng="fr">biographie</trd>
  <trdgrp lng="hu">
    <trd>&eacute;letrajz</trd>,
    <trd>biogr&aacute;fia</trd>
  </trdgrp>
  <trd lng="nl">biografie</trd>
  <trd lng="pl">biografia</trd>
  <trd lng="ru">&c_b;&c_i;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>biografia</trd>,
    <trd>životopis</trd>
  </trdgrp>
</drv>

<drv mrk="biogra1.mem0o">
  <kap>mem<tld/>o</kap>
  <uzo tip="fak">BELE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="verk.0o.propra">Verko</ref>
      rakonta pri la propra vivo de la verkinto.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="auxtob.0o">a&ubreve;tobiografio</ref>
  </refgrp>
  <trd lng="be">&c_a;&c_w;&c_t;&c_a;&c_b;&c_ib;&c_ja;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_a;&c_v;&c_t;&c_o;&c_b;&c_i;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
  <trd lng="de">Autobiografie</trd>
  <trd lng="en">autobiography</trd>
  <trd lng="es">autobiograf&iacute;a</trd>
  <trdgrp lng="fr">
    <trd>autobiographie</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&ouml;n&eacute;letrajz</trd>,
    <trd>autobiogr&aacute;fia</trd>
  </trdgrp>
  <trd lng="ca">autobiografia</trd>
  <trd lng="nl">autobiografie</trd>
  <trd lng="pl">autobiografia</trd>
  <trd
  lng="ru">&c_a;&c_v;&c_t;&c_o;&c_b;&c_i;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
  <trd lng="sk">autobiografia</trd>
  <trd lng="sk">vlastný životopis</trd>
</drv>
</art>
