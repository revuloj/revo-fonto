<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: grenad1.xml,v 1.10 2019/02/02 13:10:14 revo Exp $">
<kap>
  <rad>Grenad</rad>/o
</kap>

<drv mrk="grenad1.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="grenad1.0o.urbo">
    <fnt><bib>FK</bib></fnt>
    <dif>
      Hispana <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      kaj provinco:
      <ekz>
        el <nom>Santa F&eacute;</nom> al <tld/>o jam rapidas kuriero, atingas la malri&ccirc;e vestitan
        voja&gcirc;anton apud <tld/>o kaj raportas al li, ke &scirc;ia mo&scirc;to la re&gcirc;ino de
        Kastilujo petas lin reveni
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Granada</trd>
  </snc>
  <snc mrk="grenad1.0o.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <fnt><bib>RLN</bib></fnt>
    <mlg>GD</mlg>
    <dif>
      <ref tip="lst" cel="sxtat.0o" lst="voko:&scirc;tatoj">&Scirc;tato</ref> 
      en la samnoma insulo (kaj kelkaj apudaj).
    </dif>
    <trdgrp lng="pl">
      <trd>Grenada</trd>
    </trdgrp>
    <trd lng="bg">&c_G;&c_r;&c_je;&c_n;&c_a;&c_d;&c_a;</trd>
    <trd lng="ca">Grenada</trd>
    <trd lng="de">Grenada</trd>
    <trd lng="en">Grenada</trd>
    <trd lng="fr">Grenade</trd>
    <trd lng="id">Grenada</trd> 
    <trd lng="pt">Granada</trd>
  </snc>
  <snc mrk="grenad1.0o.insulo">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" cel="insul.0o" lst="voko:insuloj">Insulo</ref> 
      en la Kariba <ref tip="malprt" cel="mar.0o">Maro</ref>,
      teritorio de <ref tip="vid" cel="grenad1.0o.sxtato">&scirc;tato
      <tld/>o</ref>.
    </dif>
    <trd lng="de">Grenada</trd>
    <trdgrp lng="pl">
      <trd>Grenada</trd>
    </trdgrp>
  </snc>
  <rim> <ref tip="hom" cel="grenad.0o">Grenado</ref> estas anka&ubreve; eksploda&jcirc;o.</rim>
</drv>

</art>

