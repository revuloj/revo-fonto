<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lesiv.xml,v 1.26 2022/06/15 16:41:04 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lesiv</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="lesiv.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lesiv.0o.HEJM">
    <dif>
      Akva <ref tip="super" cel="solv.0ajxo">solva&jcirc;o</ref> de purigaj substancoj kiel 
      <ref tip="prt" cel="potas.0o">potaso</ref> k.a.:
      <ekz>
        <klr>[mi]</klr> purigus miajn manojn per <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 9:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        kaj cia patrino? &ndash; en la subeta&gcirc;o, farante <tld/>on
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        humida varmo lin renkontis, <klr>[&#8230;]</klr> li vidis, &ccirc;e la lumo de karbo ardanta
        en la akvovarmigilo, grandajn <tld/>pelvojn kro&ccirc;itajn al la muro kaj la
        banujon
        <fnt>
          <bib>MkM</bib>
          <lok>4</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la pli efika <tld/>o entenas amason da bagateloj, tio estas negravaj informoj,
        kiuj supersatigas la menson de la legantoj
        <fnt>
          <bib>Monato</bib>
          <aut>Michel Marko</aut>
          <vrk><url ref="https://www.monato.be/2005/008537.php"
              >Kia revuo</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sap.0o">sapo</ref>
    </refgrp>
  <trd lng="de">Waschlauge</trd>
  </snc>
  <snc mrk="lesiv.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="prt" cel="alkal.0o">Alkala</ref> akva solva&jcirc;o:
      <ekz>
        por fajli vitrajn objektojn, oni metas la fajlilon
        en fortan <tld/>on de natro
        <fnt>
          <aut>-</aut>
          <vrk>El la mondo</vrk>
          <lok><url ref="http://anno.onb.ac.at/cgi-content/anno-plus?aid=e0a&amp;datum=1892&amp;page=23">
          La esperantisto, 1892:2, p. 23a</url></lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="de">alkalische <ind>L&ouml;sung</ind></trd>
  </snc>
  <trd lng="be">&c_l;&c_u;&c_g;</trd>
  <trd lng="ca">lleixiu</trd>
  <trd lng="cs">louh</trd>
  <trd lng="de">Lauge</trd>
  <trd lng="en">lye</trd>
  <trd lng="es">lej&iacute;a</trd>
  <trd lng="fr">lessive</trd>
  <trdgrp lng="hu">
    <trd>mos&oacute;szer</trd>,
    <trd>mosogat&oacute;szer</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>wasmiddel</trd>,
    <trd>detergent</trd>,
    <trd>afwasmiddel</trd>
  </trdgrp>
  <trd lng="pl">&lstroke;ug  <klr>(chemia)</klr></trd>
  <trdgrp lng="pt">
    <trd>lix&iacute;via</trd>,
    <trd>barrela</trd>,
    <trd>decoada</trd>    
  </trdgrp>
  <trd lng="ro">le&#537;ie</trd>
  <trd lng="ru">&c_shch;&c_jo;&c_l;&c_o;&c_k;</trd>
  <trd lng="sk">l&uacute;h</trd>
  <trd lng="sv">lut</trd>
  <trdgrp lng="uk">
    <trd>&c_l;&c_u;&c_g;</trd>,
    <trd>&c_l;&c_u;&c_zh;&c_n;&c_i;&c_j; &c_r;&c_o;&c_z;&c_ch;&c_i;&c_n; (&c_d;&c_l;&c_ja; &c_p;&c_r;&c_a;&c_n;&c_n;&c_ja;)</trd>,
    <trd>&c_p;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j; &c_p;&c_o;&c_r;&c_o;&c_sh;&c_o;&c_k;</trd>,
    <trd>&c_s;&c_o;&c_d;&c_a;</trd>,
    <trd>&c_m;&c_i;&c_j;&c_n;&c_i;&c_j; &c_z;&c_a;&c_s;&c_ib;&c_b;</trd>
  </trdgrp>
</drv>

<drv mrk="lesiv.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="lesiv.0i.HEJM">
    <dif>
      <ref tip="super" cel="lav.0i">Lavi</ref> en <tld/>o:
      <ekz>
        la servistino metas &ccirc;iutage mamzonon en la <tld/>otajn tola&jcirc;ojn
        <fnt>
          <bib>KrB</bib>
          <lok>En tola&jcirc;o virino vidi&gcirc;as</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i melkas bovinojn, faras froma&gcirc;on, <tld/>adas
        <fnt>
          <bib>Monato</bib>
          <aut>Christian Declerck</aut>
          <vrk><url ref="https://www.monato.be/2005/008772.php"
              >Atestas viktimo de honorkrimo</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi ne &scirc;atus la modernan evoluon, kiu strebus <tld/>i cerbojn por estigi en la
        merkata sistemo pasivajn kaj obeemajn konsumantojn
        <fnt>
          <bib>Monato</bib>
          <aut>Michel Marko</aut>
          <vrk><url ref="https://www.monato.be/2005/008537.php"
              >Kia revuo</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  <trdgrp lng="de">
    <trd>einseifen</trd>,
    <trd>einweichen</trd>,
    <trd><klr>(mit Lauge)</klr> waschen</trd>
  </trdgrp>
  </snc>
  <snc mrk="lesiv.0i.KEM">
    <uzo tip="fak">KEM</uzo><uzo tip="fak">GEOL</uzo><uzo tip="fak">EKOL</uzo>
    <dif>
      <ref tip="super" cel="sen.0igi">Senigi</ref> je iuj konsista&jcirc;oj per trafluado, kiu forsolvas ilin:
      <ekz>
        la pluvo <tld/>is rokojn kaj grundojn
        dum milionoj da jaroj anta&ubreve; ol la homo
        e&ccirc; aperis
        <fnt>
          <aut>W. F. Rolt</aut>,
          <vrk>El la Polvo de la Tero</vrk>,
          <lok>1967</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="adsorb.0i">adsorbi</ref>,
      <ref cel="sterk.0i">sterki</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="lav.for0i">forlavi</ref>,
      <ref cel="precip1.0i">precipiti</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>auslaugen</trd>,
      <trd>auswaschen <klr>(Inhaltstoffe)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_l;&c_u;&c_g;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>b&ecaron;lit</trd>,
    <trd>lou&zcaron;it</trd>,
    <trd>prom&yacute;vat</trd>,
    <trd>pr&aacute;t</trd>
  </trdgrp>
  <trd lng="fr">lessiver</trd>
  <trd lng="hu">kif&odblac;z <klr tip="amb">(ruh&aacute;t)</klr></trd>
  <trd lng="nl">inweken</trd>
  <trd lng="pl">&lstroke;ugowa&cacute;</trd>
  <trd lng="ru">&c_shch;&c_je;&c_l;&c_o;&c_ch;&c_i;&c_t;&c_mol; <klr>(&c_b;&c_je;&c_l;&c_mol;&c_jo;)</klr></trd>
  <trd lng="sk">vyvari&tcaron; bielize&ncaron; v&nbsp;l&uacute;hu</trd>
  <trdgrp lng="uk">
    <trd>&c_l;&c_u;&c_zh;&c_i;&c_t;&c_i; (&c_b;&c_ib;&c_l;&c_i;&c_z;&c_n;&c_u; &c_ib; &c_t;.&c_p;.)</trd>,
    <trd>&c_p;&c_r;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_ch;&c_i;&c_s;&c_t;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="lesiv.vaz0o">
  <kap>
    vaz<tld/>o
  </kap>
  <snc mrk="lesiv.vaz0o.HEJM">
    <dif>
      <ref cel="lesiv.0o.HEJM"><tld lit="L"/>o</ref>
      por lavado de vazoj, de man&gcirc;iloj:
      <ekz>
        ni bezonas akvon, sablopaperojn kaj vaz<tld/>on
        <fnt>
          <aut>Kiel Oni?</aut>
          <vrk><url ref="https://tubaro.aperu.net/v/ytb_xNo8yskn73s/">Farbu biciklan radrondon</url></vrk>
          <lok>2021</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">produit &agrave; vaiselle</trd>
  <trd lng="uk">&c_z;&c_a;&c_s;&c_ib;&c_b; &c_d;&c_l;&c_ja; &c_m;&c_i;&c_t;&c_t;&c_ja; &c_p;&c_o;&c_s;&c_u;&c_d;&c_u;</trd>
</drv>
</art>
