<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lesiv</rad>/o
</kap>
<drv mrk="lesiv.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Likva&jcirc;o a&ubreve; pulvoro solvata en akvo por lavi homan ha&ubreve;ton, vesta&jcirc;ojn, man&gcirc;ilaron kaj similajn.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_u;&c_g;</trd>
  <trd lng="de">Waschmittel</trd>
  <trd lng="en">lye</trd>
  <trd lng="es">lej&iacute;a</trd>
  <trd lng="fr">lessive</trd>
  <trd lng="ca">lleixiu</trd>
  <trdgrp lng="hu">
   <trd>mos&oacute;szer</trd>,
   <trd>mosogat&oacute;szer</trd>
</trdgrp>  
  <trdgrp lng="nl">
   <trd>wasmiddel</trd>,
   <trd>detergent</trd>,
   <trd>afwasmiddel</trd>
</trdgrp>
  <trdgrp lng="pt">
    <trd>lix&iacute;via</trd>,
    <trd>barrela</trd>,
    <trd>decoada</trd>    
  </trdgrp>
<trd lng="ru">&c_shch;&c_jo;&c_l;&c_o;&c_k;</trd>
  <trd lng="sv">lut</trd>
</drv>

<drv mrk="lesiv.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Boligi tola&jcirc;on en <tld/>o, por &gcirc;in lavi.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_u;&c_g;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="en">boil in <ind>lye</ind> water</trd>
  <trd lng="fr">lessiver</trd>
  <trd lng="hu">kif&odblac;z <klr tip="amb">(ruh&aacute;t)</klr></trd>  
  <trd lng="ru">&c_shch;&c_je;&c_l;&c_o;&c_ch;&c_i;&c_t;&c_mol; <klr>(&c_b;&c_je;&c_l;&c_mol;&c_jo;)</klr></trd>
</drv>
</art>
