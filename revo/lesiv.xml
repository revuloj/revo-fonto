<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>lesiv</rad>/o
</kap>
<drv mrk="lesiv.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Bolinta akvo kun potaso a&ubreve; sodo, uzata por lavi tola&jcirc;on.
    </dif>
  </snc>
  <trd lng="de">Lauge</trd>
  <trd lng="nl">loog</trd>
</drv>
<drv mrk="lesiv.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Boligi tola&jcirc;on en <tld/>o, por &gcirc;in lavi.
    </dif>
  </snc>
</drv>
</art>
