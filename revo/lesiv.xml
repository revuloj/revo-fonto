<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lesiv</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="lesiv.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lesiv.0o.HEJM">
    <dif>
      Likva&jcirc;o a&ubreve; pulvoro solvata en akvo por lavi homan ha&ubreve;ton,
      vesta&jcirc;ojn, man&gcirc;ilaron kaj similajn.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_u;&c_g;</trd>
  <trd lng="ca">lleixiu</trd>
  <trd lng="cs">louh</trd>
  <trdgrp lng="de">
  <trd>Waschmittel</trd>,
  <trd>Lauge</trd>
  </trdgrp>
  <trd lng="en">lye</trd>
  <trd lng="es">lej&iacute;a</trd>
  <trd lng="fr">lessive</trd>
  <trdgrp lng="hu">
    <trd>mos&oacute;szer</trd>,
    <trd>mosogat&oacute;szer</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>wasmiddel</trd>,
    <trd>detergent</trd>,
    <trd>afwasmiddel</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>lix&iacute;via</trd>,
    <trd>barrela</trd>,
    <trd>decoada</trd>    
  </trdgrp>
  <trd lng="ru">&c_shch;&c_jo;&c_l;&c_o;&c_k;</trd>
  <trd lng="sk">lúh</trd>
  <trd lng="sv">lut</trd>
</drv>

<drv mrk="lesiv.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="lesiv.0i.HEJM">
    <dif>
      Komplete malsekigi ion en <tld/>o, por &gcirc;in lavi.
    </dif>
  </snc>
  <snc mrk="lesiv.0i.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Senigi je iuj konsista&jcirc;oj per trafluado, kiu forsolvas ilin:
      <ekz>
        la pluvo <tld/>is rokojn kaj grundojn
        dum milionoj da jaroj anta&ubreve; ol la homo
        e&ccirc; aperis
        <fnt>
          <aut>W. F. Rolt</aut>,
          <vrk>El la Polvo de la Tero</vrk>,
          <lok>1967</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_u;&c_g;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>bělit</trd>,
    <trd>loužit</trd>,
    <trd>promývat</trd>,
    <trd>prát</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>einseifen</trd>,
    <trd>einweichen</trd>,
    <trd>(mit Lauge) waschen</trd>
  </trdgrp>
  <trd lng="fr">lessiver</trd>
  <trd lng="hu">kif&odblac;z <klr tip="amb">(ruh&aacute;t)</klr></trd>
  <trd lng="nl">inweken</trd>
  <trd lng="ru">&c_shch;&c_je;&c_l;&c_o;&c_ch;&c_i;&c_t;&c_mol; <klr>(&c_b;&c_je;&c_l;&c_mol;&c_jo;)</klr></trd>
  <trd lng="sk">vyvariť bielizeň v lúhu</trd>
</drv>
</art>
