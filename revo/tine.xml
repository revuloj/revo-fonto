<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>tine</rad>/o
</kap>
<drv mrk="tine.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genroj el familio <ref tip="super" cel="tine.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Tinea</trd>, kaj parencaj genroj),
      malgranda papilio, kies larvoj man&gcirc;as
      lanon, &scirc;tofojn, peltojn.
    </dif>
  </snc>
  <trd lng="de">Motte</trd>
</drv>
<drv mrk="tine.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="lepido.0oj">lepidopteroj</ref>
      (<trd lng="la">Tineidae</trd>).
    </dif>
  </snc>
  <trd lng="de"><klr>(Echte)</klr> Motten <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>
