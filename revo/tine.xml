<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tine</rad>/o
</kap>
<drv mrk="tine.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genroj</ref> el familio <ref
      tip="malprt"
      cel="tineed.0oj"><tld/>edoj</ref>
      (<trd lng="la">Tinea</trd>, kaj parencaj genroj),
      malgranda <ref tip="lst" cel="papili.0o"
      lst="voko:papilioj">papilio</ref>, kies larvoj man&gcirc;as
      lanon, &scirc;tofojn, peltojn.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_o;&c_l;&c_mol;</trd>
  <trd lng="ca">arna</trd>
  <trd lng="de">Motte</trd>
  <trdgrp lng="en">
    <trd>clothes-moth</trd>,
    <trd>tineid</trd>
  </trdgrp>
  <trd lng="fr">mite</trd>
  <trd lng="hu">moly</trd>
  <trd lng="nl">mot</trd>
  <trd lng="pl">m&oacute;l</trd>
  <trdgrp lng="pt">
    <trd>tra&ccedil;a</trd>,
    <trd>t&iacute;nea</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_o;&c_l;&c_mol; <klr
  tip="amb">(&c_n;&c_a;&c_s;&c_je;&c_k;&c_o;&c_m;&c_o;&c_je;)</klr></trd>
</drv>
</art>
