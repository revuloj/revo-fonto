<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vermif.xml,v 1.17 2024/05/09 23:24:43 revo Exp $">
<kap>
  <rad>vermifug</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vermif.0o">
  <kap><tld/>o</kap>
  <snc mrk="vermif.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="stl">ARK</uzo>
    <dif>
       Ordinara <ref tip="super" cel="tanacet.0o">tanaceto</ref>,
       uzata iam kiel kontra&ubreve;vermilo:
      <ekz>
        &scirc;i volis sidi&gcirc;i sur la tombo de malri&ccirc;ulo, kie floris
        la maldol&ccirc;a <tld/>o
        <fnt><bib>Fab2</bib>, <lok>Ru&gcirc;aj &scirc;uoj</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Rainfarn</trd>,
      <trd>Wurmkraut</trd>
    </trdgrp>
  </snc>
  <snc mrk="vermif.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Kontra&ubreve;verma kuracilo:
      <ekz>
        <klr>[li]</klr> defendis la uzon de herbo Mentruzo kiel <tld/>o al infanoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16299">Leonel Gonzaga</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_g;&c_l;&c_ib;&c_s;&c_t;&c_a;&c_g;&c_o;&c_n;&c_n;&c_y; &c_s;&c_r;&c_o;&c_d;&c_a;&c_k;</trd>
    <trdgrp lng="ca">
      <trd>vermicida</trd>,
      <trd>verm&iacute;fug</trd>
    </trdgrp>
    <trd lng="de">Wurmmittel</trd>
    <trd lng="el">&alfa;&ny;&theta;&epsilon;&lambda;&my;&jota;&ny;&theta;&jota;&kappa;&omikron_ton;</trd>
    <trd lng="fr">vermifuge</trd>
    <trd lng="hu">f&eacute;reghajt&oacute;</trd>
    <trd lng="nl">wormkruid</trd>
    <trd lng="pt">verm&iacute;fugo</trd>
    <trd lng="ru">&c_g;&c_l;&c_i;&c_s;&c_t;&c_o;&c_g;&c_o;&c_n;&c_n;&c_o;&c_je; &c_s;&c_r;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>
    <trd lng="sk">hl&iacute;stopudn&yacute; prostriedok</trd>
  </snc>
  <trd lng="cs">prost&rcaron;edek proti &ccaron;erv&uring;m</trd>
  <trdgrp lng="pl">
    <trd>&sacute;rodek przeciwczerwiowy</trd>,
    <trd>&sacute;rodek czerwiop&eogonek;dny</trd>
  </trdgrp>
  <trd lng="uk">&c_g;&#x43B;&c_i;&c_s;&c_t;&#x43E;&c_g;&c_ib;&#x43D;&#x43D;&c_i;&c_j; &c_z;&c_a;&c_s;&c_ib;&c_b;</trd>
</drv>
  </art>
