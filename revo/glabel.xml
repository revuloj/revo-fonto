<art mrk="$Id: glabel.xml,v 1.15 2018/12/20 16:10:19 revo Exp $">
<kap><rad>glabel</rad>/o</kap>
<drv mrk="glabel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Spaco inter la nazo kaj brovoj:
      <ekz>
        &ccirc;e la plej multaj homoj <klr>[estas]</klr> senhara loko super la naza radiko,
        kiu nomi&gcirc;as glabelo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=288460"
              >Brovo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">entrecella</trd>
  <trd lng="cs">glabella</trd>
  <trd lng="de">Glabella</trd>
  <trd lng="fr">glabelle</trd>
  <trdgrp lng="hu">
    <trd>szem&ouml;ld&ouml;kk&ouml;z</trd>,
    <trd>glabella</trd>
  </trdgrp>
  <trd lng="it">glabella</trd>
  <trd lng="nl">glabella</trd>
  <trd lng="pl">g&lstroke;adzinka</trd>
  <trdgrp lng="pt">
   <trd>glabela</trd>, 
   <trd>interc&iacute;lio</trd>, 
   <trd>mes&oacute;frio</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_l;&c_a;&c_b;&c_je;&c_l;&c_l;&c_a;</trd>,
    <trd>&c_m;&c_je;&c_zh;&c_b;&c_r;&c_o;&c_v;&c_mol;&c_je;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_p;&c_je;&c_r;&c_je;&c_n;&c_o;&c_s;&c_mol;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">glabella</trd>
</drv>
</art>
