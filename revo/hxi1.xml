<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
   <art mrk="$Id$">
     <kap><rad>&hcirc;i</rad>/</kap>

     <drv mrk="hxi1.0">
       <kap><tld/></kap>
       <snc>
         <uzo tip="fak">LIN</uzo>
         <dif>
           Nomo de la 22a litero de la greka alfabeto
          (<frm>&Chi;&chi;</frm>), 
           kiu respondas al &hcirc; en la alfabeto de Esperanto.
         </dif>
       </snc>
       <ref tip="hom" cel="hxi2.Hx0o">&Hcirc;io</ref>
       <trd lng="be">&c_h;&c_ib; <klr>(&c_g;&c_r;&c_e;&c_c;&c_k;&c_a;&c_ja; &c_l;&c_ib;&c_t;&c_a;&c_r;&c_a;)</klr></trd>
       <trd lng="fi">khii</trd>
       <trd lng="pt">khi<klr tip="amb"> (letra grega)</klr></trd>
       <trdgrp lng="pt">
         <trd>qui</trd>,
         <trd>chi</trd>
       </trdgrp>
       <trd lng="ru">&c_h;&c_i;</trd>
       <trd lng="it">chi<klr tip="amb"> (lettera greca)</klr></trd>
     </drv>

   </art>

