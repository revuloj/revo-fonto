<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
   <art mrk="$Id: hxi1.xml,v 1.14 2023/08/09 22:29:42 revo Exp $">
     <kap><rad>&hcirc;i</rad>/</kap>

 <drv mrk="hxi1.0">
   <kap><tld/></kap>
   <snc>
     <uzo tip="fak">LIN</uzo>
     <uzo tip="fak">GRA</uzo>
     <dif>
       Nomo de la 22a litero de la greka alfabeto
      (<frm>&Chi;&chi;</frm>),
       kiu respondas al &hcirc; en la alfabeto de Esperanto:
      <ekz>
        antikva bronza olelampo kun &leftquot;<tld lit="&Hcirc;"/> Ro&rightquot;, kristana simbolo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=512312">Olelampo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="&Hcirc;"/>-Roto-kruco <klr>[simbolas]</klr> la unuaj<klr>[n]</klr>
        du signoj<klr>[n]</klr> de la greka vorto por &leftquot;Kristo&rightquot;
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=561551">Kruc&scirc;tono</url></lok>
        </fnt>.
      </ekz>
     </dif>
     <ref tip="hom" cel="hx.0o">&hcirc;</ref>
   </snc>
   <trd lng="be">&c_h;&c_ib; <klr>(&c_g;&c_r;&c_e;&c_c;&c_k;&c_a;&c_ja; &c_l;&c_ib;&c_t;&c_a;&c_r;&c_a;)</klr></trd>
   <trd lng="bg">&c_h;&c_i;<klr tip="amb"> (&c_g;&c_r;&c_malmol;&c_c;&c_k;&c_i; &c_b;&c_u;&c_k;&c_v;&c_a;)</klr></trd>
   <trd lng="ca">khi<klr tip="amb"> (letra grega)</klr></trd>
   <trd lng="de">chi</trd>
   <trd lng="en">chi</trd>
   <trd lng="es">ji</trd>
   <trd lng="fi">khii</trd>
   <trd lng="fr">chi</trd>
   <trd lng="hr">hi</trd>
   <trd lng="nl">chi</trd>
   <trd lng="pl">chi <klr>(litera grecka)</klr></trd>
   <trdgrp lng="pt">
     <trd>qui</trd>,
     <trd>chi</trd>
   </trdgrp>
   <trd lng="ru">&c_h;&c_i;</trd>
   <trd lng="it">chi<klr tip="amb"> (lettera greca)</klr></trd>
   <trd lng="uk">&c_h;&c_ib;</trd>
</drv>

   </art>

