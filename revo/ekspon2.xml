<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>eksponent</rad>/o
</kap>
<drv mrk="ekspon2.0o">
  <kap><tld/>o</kap>
  <snc mrk="ekspon2.0o.algebra">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PV</bib></fnt>
    <dif>
      En <ref cel="potenc.0igo">potencigo</ref>,
      la argumento indikanta la nombron de multiplikataj faktoroj:
      <ekz>
        <uzo tip="stl">ARK</uzo>
        <ind><tld/>a funkcio</ind>
        <klr>(<ref cel="ekspon1.0o">eksponencialo</ref>)</klr>.
        <trd lng="de">Exponentialfunktion</trd>
        <trd lng="en">exponential</trd>
        <trd lng="fr">exponentielle</trd>
        <trdgrp lng="pl">
          <trd>funkcja wyk&lstroke;adnicza</trd>,
          <trd>funkcja eksponencjalna</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_e;&c_k;&c_s;&c_p;&c_o;&c_n;&c_je;&c_n;&c_c;&c_i;&c_a;&c_l;</trd>,
          <trd>&c_p;&c_o;&c_k;&c_a;&c_z;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_ja; <ind>&c_f;&c_u;&c_n;&c_k;&c_c;&c_i;&c_ja;</ind></trd>
        </trdgrp>
      </ekz>
    </dif>
    <ref tip="sin" cel="potenc.0iganto">potenciganto.</ref>
    <trdgrp lng="de">
      <trd>Exponent</trd>,
      <trd>Hochzahl</trd>
    </trdgrp>
    <trd lng="pl">wyk&lstroke;adnik</trd>
    <trd lng="ru">&c_p;&c_o;&c_k;&c_a;&c_z;&c_a;&c_t;&c_je;&c_l;&c_mol; &c_s;&c_t;&c_je;&c_p;&c_je;&c_n;&c_i;</trd>
  </snc>
  <snc mrk="ekspon2.0o.glitpunkta">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      En <ref tip="malprt" cel="glit.0punkta.koma">glitpunkta</ref>
      nombroprezento, entjero indikanta je kiom da pozicioj estas
      &scirc;ovenda la <ref cel="on.0punkto.komo">onpunkto</ref>.
    </dif>
    <trd lng="ru">&#x43f;&#x43e;&#x440;&#x44f;&#x434;&#x43e;&#x43a;
    <klr>(&#x432; &#x43f;&#x440;&#x435;&#x434;&#x441;&#x442;&#x430;&#x432;&#x43b;&#x435;&#x43d;&#x438;&#x438; &#x441; &#x43f;&#x43b;&#x430;&#x432;&#x430;&#x44e;&#x449;&#x435;&#x439; &#x437;&#x430;&#x43f;&#x44f;&#x442;&#x43e;&#x439;)</klr></trd>
  </snc>
  <trd lng="fr">exposant<klr tip="ind"> (math., info.)</klr></trd>
  <trd lng="en">exponent</trd>
</drv>
</art>
