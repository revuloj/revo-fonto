<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ekspon2.xml,v 1.29 2023/11/07 19:22:36 revo Exp $">
<kap>
  <rad>eksponent</rad>/o
</kap>
<drv mrk="ekspon2.0o">
  <kap><tld/>o</kap>
  <snc mrk="ekspon2.0o.algebra">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PV</bib></fnt>
    <dif>
      En <ref cel="potenc.0igo">potencigo</ref>,
      la argumento indikanta la nombron da multiplikataj faktoroj:
      <ekz>
        <uzo tip="stl">ARK</uzo>
        <ind><tld/>a funkcio</ind>
        <klr>(<ref cel="ekspon1.0o">eksponencialo</ref>)</klr>.
        <trd lng="bg">&c_je;&c_k;&c_s;&c_p;&c_o;&c_n;&c_je;&c_n;&c_c;&c_i;&c_a;&c_l;&c_n;&c_a; <ind>&c_f;&c_u;&c_n;&c_k;&c_c;&c_i;&c_ja;</ind></trd>
        <trd lng="ca">funci&oacute; exponencial</trd>
        <trd lng="de">Exponentialfunktion</trd>
        <trdgrp lng="en">
          <trd>exponential<klr tip="ind"> (to the base a)</klr></trd>,
          <trd>exponential <ind>function</ind><klr tip="ind"> (to the
          base a)</klr></trd>
        </trdgrp>
        <trd lng="fa">&a_t;&a_A;&a_b;&a_ayn; <ind>&a_n;&a_m;&a_A;&f_y;&f_y;</ind></trd>
        <trd lng="fr">exponentielle</trd>
        <trd lng="hu">exponenci&aacute;lis <ind>f&uuml;ggv&eacute;ny</ind></trd>
        <trd lng="nl">exponenti&euml;le <ind>functie</ind></trd>
        <trdgrp lng="pl">
          <trd>funkcja wyk&lstroke;adnicza</trd>,
          <trd>funkcja eksponencjalna</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_e;&c_k;&c_s;&c_p;&c_o;&c_n;&c_je;&c_n;&c_c;&c_i;&c_a;&c_l;</trd>,
          <trd>&c_p;&c_o;&c_k;&c_a;&c_z;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_ja;
          <ind>&c_f;&c_u;&c_n;&c_k;&c_c;&c_i;&c_ja;</ind></trd>
        </trdgrp>
      </ekz>
    </dif>
    <ref tip="sin" cel="potenc.0iganto">potenciganto.</ref>
    <trd lng="be">&c_p;&c_a;&c_k;&c_a;&c_z;&c_ch;&c_y;&c_k; &c_s;&c_t;&c_u;&c_p;&c_je;&c_n;&c_ib;</trd>
    <trdgrp lng="bg">
      <trd>&c_s;&c_t;&c_je;&c_p;&c_je;&c_n;</trd>,
      <trd>&c_s;&c_t;&c_je;&c_p;&c_je;&c_n;&c_je;&c_n; <ind>&c_p;&c_o;&c_k;&c_a;&c_z;&c_a;&c_t;&c_je;&c_l;</ind></trd>,
      <trd>&c_je;&c_k;&c_s;&c_p;&c_o;&c_n;&c_je;&c_n;&c_t;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Exponent</trd>,
      <trd>Hochzahl</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kitev&odblac;</trd>,
      <trd>hatv&aacute;nykitev&odblac;</trd>,
    </trdgrp>
    <trd lng="nl">exponent</trd>
    <trd lng="pl">wyk&lstroke;adnik</trd>
    <trd lng="pt">expoente</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_k;&c_a;&c_z;&c_a;&c_t;&c_je;&c_l;&c_mol; &c_s;&c_t;&c_je;&c_p;&c_je;&c_n;&c_i;</trd>,
      <trd>&c_e;&c_k;&c_s;&c_p;&c_o;&c_n;&c_je;&c_n;&c_t;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x6307;&#x793A;&#x6570; <pr>zh&#x1D0;sh&#xEC;sh&#xF9;</pr></trd>,
    <trd>&#x6307;&#x6807; <pr>zh&#x1D0;bi&#x101;o</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="ekspon2.0o.glitpunkta">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      En <ref tip="malprt" cel="glit.0punkta">glitpunkta</ref>
      nombroprezento, entjero indikanta je kiom da pozicioj estas
      &scirc;ovenda la <ref cel="on.0komo">onpunkto</ref>.
    </dif>
    <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_d;&c_a;&c_k; <klr tip="amb">(&c_u; &c_l;&c_ib;&c_k;&c_u;)</klr></trd>
    <trd lng="ru">&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_k;
    <klr>(&c_v; &c_p;&c_r;&c_je;&c_d;&c_s;&c_t;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_i; &c_s; &c_p;&c_l;&c_a;&c_v;&c_a;&c_ju;&c_shch;&c_je;&c_j;
    &c_z;&c_a;&c_p;&c_ja;&c_t;&c_o;&c_j;)</klr></trd>
  <trd lng="hu">kitev&odblac;</trd>
  </snc>
  <trd lng="ca">exponent</trd>
  <trdgrp lng="cs">
    <trd>exponent</trd>,
    <trd>mocnitel</trd>
  </trdgrp>
  <trd lng="en">exponent</trd>
  <trd lng="fa">&a_n;&a_m;&a_A;</trd>
  <trd lng="fr">exposant<klr tip="ind"> (math., info.)</klr></trd>
  <trd lng="ja">&#x6307;&#x6570;  <pr>&#x3057;&#x3059;&#x3046;</pr></trd>
  <trd lng="sk">mocnite&lcaron;</trd>
  <trd lng="uk">&#x43F;&#x43E;&#x43A;&c_a;&c_z;&#x43D;&c_i;&#x43A; &c_s;&c_t;&c_je;&#x43F;&c_je;&#x43D;&#x44F;</trd>
</drv>
</art>
