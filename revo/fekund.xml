<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>fekund</rad>/a
</kap>
<drv mrk="fekund.0a">
  <kap><tld/>a</kap>
  <snc num="1">
    <dif>
      Naskipova:
      <ekz><tld/>a virino;</ekz>
      <ekz><tld/>igi floron.</ekz>
    </dif>
    <trd lng="de">fruchtbar</trd>
  </snc>
  <snc num="2">
    <dif>
      Multe naskinta: <ekz><tld/>a &scirc;afino.</ekz>
    </dif>
    <trd lng="de">fruchtbar</trd>
  </snc>
  <snc num="3">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fruktodona, multefrukta, multe produktanta:
      <ekz><tld/>a tero, ideo, verkisto;</ekz>
      <ekz><tld/>igi teron per sterka&jcirc;o.</ekz>
    </dif>
    <trd lng="de">ergiebig</trd>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_o;&c_d;&c_o;&c_r;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_l;&c_o;&c_d;&c_o;&c_v;&c_i;&c_t;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
