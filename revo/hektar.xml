<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>hektar</rad>/o
</kap>
<drv mrk="hektar.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Komunuza <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de areo,
      egala al cent <ref tip="prt" cel="ar1.0o">aroj</ref> kaj
      dek mil <ref tip="prt" cel="metr.kvadrata0o">kvadrataj metroj</ref>;
      simb. ha.
    </dif>
  </snc>
  <trd lng="de">Hektar</trd>
  <trd lng="fr">hectare</trd>
  <trd lng="nl">hectare</trd>
  <trd lng="ru">&c_g;&c_je;&c_k;&c_t;&c_a;&c_r;</trd>
</drv>
</art>
