<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>hektar</rad>/o
</kap>
<drv mrk="hektar.0o">
  <kap><tld/>o</kap>
  <mlg>ha</mlg>
  <snc>
    <dif>
      Praktika <ref tip="lst" cel="unu.mezur0o"
      lst="voko:areoj">mezurunuo</ref> de
      areo, egala al cent <ref tip="prt" cel="ar1.0o">aroj</ref> kaj
      dek mil <ref tip="prt" cel="metr.kvadrata0o">kvadrataj
      metroj</ref>:
      <ekz>
        la nova tombejo, malfermita lastjare, kovras areon de du
        <tld/>oj<fnt><aut>Hamza Baksi&cacute;</aut>: <vrk>La fino de la
        mondo komenci&gcirc;as en Sarajevo</vrk>, <bib>Monato</bib>
        <lok>jaro 1993a,
        numero 10a, p. 8a</lok></fnt>.
      </ekz>
    </dif>
  </snc>                           
  <trd lng="be">&c_g;&c_je;&c_k;&c_t;&c_a;&c_r;</trd>
  <trd lng="bg">&c_h;&c_je;&c_k;&c_t;&c_a;&c_r;</trd>
  <trd lng="br">hektar</trd>
  <trd lng="de">Hektar</trd>
  <trd lng="el">
  &epsilon;&kappa;&tau;&alfa_ton;&rho;&jota;&omikron;</trd>
  <trd lng="en">hectare</trd>
  <trd lng="es">hect&aacute;rea</trd>
  <trd lng="fr">hectare</trd>
  <trd lng="hu">hekt&aacute;r</trd>
  <trd lng="it">ettaro</trd>
  <trd lng="nl">hectare</trd>
  <trd lng="pl">hektar</trd>
  <trd lng="pt">hectare</trd>
  <trd lng="ru">&c_g;&c_je;&c_k;&c_t;&c_a;&c_r;</trd>
</drv>
</art>
