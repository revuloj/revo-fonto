<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gencia1.xml,v 1.15 2023/11/11 10:00:39 revo Exp $">
<kap>
  <rad>gencianac</rad>/oj
</kap>
<drv mrk="gencia1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Gentianaceae</trd>):
      <ekz>
        la <tld/>oj <klr>[&#x2026;]</klr> konsistas el arboj, arbedoj kaj herboj kun plejofte
        paraj folioj, la infloreskoj estas cumaj, la floroj havas plejofte kvin kuni&gcirc;intajn
        petalojn kaj saman nombron de stamenoj ligitaj al la korola tubo, la du karpeloj,
        kun parieta alfiksado de la ovoloj, plejofte fari&gcirc;as kapsulo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19536">Gencianacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_y;&c_ch;&c_k;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#x2019;&c_ja;)</klr></trd>
  <trd lng="ca">gencian&agrave;cies</trd>
  <trd lng="de">Enziangew&auml;chse</trd>
  <trdgrp lng="en">
    <trd>Gentianaceae</trd>,
    <trd>gentian family</trd>
  </trdgrp>
  <trd lng="fr">gentianac&eacute;es</trd>
  <trd lng="hu">t&aacute;rnicsf&eacute;l&eacute;k</trd>
  <trd lng="it">genzianacee</trd>
  <trd lng="nl">gentiaanfamilie</trd>
  <trd lng="pt">gencian&aacute;ceas</trd>
  <trd lng="ru">
&c_g;&c_o;&c_r;&c_je;&c_ch;&c_a;&c_v;&c_k;&c_o;&c_v;&c_y;&c_je;
<klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr>
</trd>
  <trd lng="uk">&c_t;&c_i;&c_r;&#x43B;&c_i;&c_ch;&c_je;&c_v;&c_ib;</trd>
  <trd lng="zh">&#x9F8D;&#x81BD;&#x79D1; <pr>l&#xF3;ngd&#x1CE;nk&#x113;</pr></trd>
</drv>
</art>
