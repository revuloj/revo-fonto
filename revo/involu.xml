<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: involu.xml,v 1.18 2024/04/02 06:28:44 revo Exp $">
<kap>
<rad>involukr</rad>/o
</kap>
<drv mrk="involu.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Aro da <ref tip="prt" cel="brakte.0o">brakteoj</ref>
      &ccirc;e <ref tip="malprt" cel="flor.0o.BOT">floro</ref> a&ubreve;
      <ref tip="malprt" cel="inflor.0o">infloresko</ref>:
      <ekz>
        la <tld/>o de anemono, de <ref tip="vid" cel="apiac.0oj">apiacoj</ref>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_g;&c_o;&c_r;&c_t;&c_k;&c_a; <klr tip="amb">(&c_s;&c_u;&c_k;&c_v;&c_je;&c_c;&c_mol;&c_c;&c_ja;)</klr></trd>
  <trd lng="bg">&c_o;&c_b;&c_v;&c_i;&c_v;&c_k;&c_a;<klr> &c_n;&c_a; &c_s;&c_malmol;&c_c;&c_v;&c_je;&c_t;&c_i;&c_je;</klr></trd>
  <trd lng="ca">involucre</trd>
  <trdgrp lng="cs">
    <trd>involukrum</trd>,
    <trd>obal</trd>,
    <trd>povlak</trd>
  </trdgrp>
  <trd lng="de">Bl&uuml;tenh&uuml;lle</trd>
  <trd lng="en">involucre</trd>
  <trd lng="fr">involucre</trd>
  <trd lng="hu">vir&aacute;gtakar&oacute;</trd>
  <trd lng="ja">&#x7DCF;&#x82DE;  <pr>&#x305D;&#12358;&#x307B;&#12358;</pr></trd>
  <trd lng="nl">omwindsel</trd>
  <trd lng="pt">inv&oacute;lucro</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_v;&c_jo;&c_r;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_r;&c_o;&c_v; &c_i;&c_z;
    &c_p;&c_r;&c_i;&c_c;&c_v;&c_je;&c_t;&c_n;&c_i;&c_k;&c_o;&c_v;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>obal</trd>,
    <trd>povlak</trd>
  </trdgrp>
</drv>
</art>
