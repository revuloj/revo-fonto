  <trd lng="ja">&#28342;&#28082;  <pr>&#12424;&#12358;&#12360;&#12365;</pr></trd>
  <trd lng="pl">roztw&oacute;r</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_t;&c_v;&c_o;&c_r;</trd>
  <trd lng="uk">&c_r;&c_o;&c_z;&c_ch;&c_i;&c_n;</trd>
</drv>

