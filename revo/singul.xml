<vortaro>
  <art mrk="$Id$">
    <kap>
<ofc>8</ofc>
      <rad>singular</rad>/o
    </kap>
    <drv mrk="singul.0o">
      <kap><tld/>o</kap>
      <snc>
        <uzo tip="fak">GRA</uzo>
        <dif>
          Gramatika nombro montranta, ke oni parolas pri ununura
          persono a&ubreve; afero; <ref tip="sin" cel="nombr.unu0o">ununombro</ref>.
        </dif>
        <refgrp tip="vid">
          <ref cel="plural.0o">pluralo</ref>.
        </refgrp>
        <trdgrp lng="de">
            <trd>Singular</trd>,
            <trd>Einzahl</trd>
        </trdgrp>
        <trd lng="en">singular</trd>
        <trd lng="es">singular</trd>
        <trd lng="nl">enkelvoud</trd>
<trd lng="ru">&#x435;&#x434;&#x438;&#x43d;&#x441;&#x442;&#x432;&#x435;&#x43d;&#x43d;&#x43e;&#x435; &#x447;&#x438;&#x441;&#x43b;&#x43e;</trd>
        <trd lng="sv">singular</trd>
      </snc>
    </drv>
  </art>
