<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>makedon</rad>/o
  <fnt><bib>NT</bib></fnt>
</kap>

<drv mrk="makedon.0o">
  <kap><tld/>o</kap>
  <snc mrk="makedon.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="makedon.M0ujo"><tld lit="M"/>ujo</ref>:
      <ekz>
        okazis surstrataj konfrontoj inter slavo<tld/>oj kaj albano<tld/>oj en <nom>Skopje</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="http://www.monato.net/2013/011002.php">Bruligita la flago</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="makedon.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco:
      <ekz>
        oni kure kolekti&gcirc;is unuanime en la teatron, ekkaptinte Gajon 
        kaj Aristar&hcirc;on, <tld lit="M"/>ojn, kunvoja&gcirc;antojn de Pa&ubreve;lo
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 19:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni ekveturis sur la maron, havante kun ni Aristar&hcirc;on, <tld lit="M"/>on el Tesaloniko
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 27:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        la antikva sciulo Strabono (63-20 anta&ubreve; la komuna erao) notis
        en la sepa volumo de sia Geografio, ke &leftquot;iliroj kaj <tld/>oj 
        parolas saman lingva&jcirc;on [kaj] havas similajn morojn kaj kutimojn&rightquot;
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="http://www.monato.net/2009/009921.php">Ridinda aserto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        registaro, en kiu superregas etno<tld/>oj kaj iliaj partioj, ne volas dividi
        la komunan ri&ccirc;a&jcirc;on kun albanoj kaj plejeble sabotas plenumon de la <nom>Ohrid</nom>-interkonsento
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="http://www.esperanto.be/fel/2003/008198.html">Neniu paco sen interkonsento</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        oni prezentis Aleksandron la Grandan kiel sametnulon de la 
        nuntempaj <tld/>oj, anka&ubreve; patrinon <nom>Tereza</nom> kiel <tld/>inon kaj ne albaninon)
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="http://www.monato.net/2005/008489.php">Per into al onto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_k;&c_je;&c_d;&c_o;&c_n;&c_je;&c_c;</trd>
  <trd lng="ca">macedoni<klr> (subst.)</klr></trd>
  <trd lng="cs">Makedonec</trd>
  <trd lng="de">Mazedonier</trd>
  <trd lng="en">Macedonian</trd>
  <trd lng="fr">Mac&eacute;donien<klr> (subst.)</klr></trd>
  <trdgrp lng="pl">
    <trd>Macedo&nacute;czyk</trd>,
    <trd>Macedonka</trd>
  </trdgrp>
  <trd lng="pt">maced&ocirc;nio <klr tip="ind"> (subst.)</klr></trd>
  <trd lng="sk">Maced&oacute;nec</trd>
</drv>

<drv mrk="makedon.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_k;&c_je;&c_d;&c_o;&c_n;&c_s;&c_k;&c_ib;</trd>
  <trd lng="ca">macedoni<klr> (adj.)</klr></trd>
  <trd lng="cs">makedonsk&yacute;</trd>
  <trd lng="de">mazedonisch</trd>
  <trd lng="fr">mac&eacute;donien<klr> (adj.)</klr></trd>
  <trd lng="pl">macedo&nacute;ski</trd>
  <trd lng="pt">maced&ocirc;nio <klr tip="ind"> (adj.)</klr></trd>
  <trd lng="sk">maced&oacute;nsky</trd>
</drv>

<drv mrk="makedon.M0ujo">
  <kap>
    <tld lit="M"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <mlg kod="ISO-3166">MK</mlg>
  <snc mrk="makedon.M0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o.POL">&Scirc;tato</ref> en la 
      suda <ref tip="malprt" cel="euxrop1.E0o.geog">E&ubreve;ropo</ref>
      kun &ccirc;efurbo Skopjo;
      teritorio de Respubliko Makedonio (a&ubreve; alie: Iama
      Jugoslavuja Respubliko Makedonio),
      limanta kun
      <ref tip="vid" cel="alban.A0ujo">Albanujo</ref>,
      <ref tip="vid" cel="bulgar.B0ujo">Bulgarujo</ref>,
      <ref tip="vid" cel="grek.G0ujo.GEOG">Grekujo</ref>
      kaj <ref tip="vid" cel="serb.S0ujo.GEOG">Serbujo</ref>:
      <ekz>
        Bulgara abio estas <klr>[&#8230;]</klr> indi&gcirc;ena en la montaroj de Balkana 
        duoninsulo, &gcirc;i trovi&gcirc;as en Bulgarujo, norda Grekujo, Respubliko
        <tld lit="M"/>ujo, Albanujo kaj Serbujo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=383234">Bulgara abio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="makedoni.M0o.respubliko">Makedonio</ref>
  </snc>
  <snc mrk="makedon.M0ujo.historie">
    <uzo tip="fak">HIS</uzo>
    <dif>
       En la historio, lando norde de Grekujo, lo&gcirc;ata de <tld/>oj:
      <ekz>
        post kiam li vidis la vizion, ni tuj penis foriri en <tld lit="M"/>ujon,
        konkludinte, ke Dio vokis nin, por prediki al ili la evangelion  
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 16:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        Pa&ubreve;lo intencis en la spirito, trapasinte <tld lit="M"/>ujon kaj 
        la A&hcirc;ajan landon, voja&gcirc;i al <nom>Jerusalem</nom>
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 19:21</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_M;&c_a;&c_k;&c_je;&c_d;&c_o;&c_n;&c_ib;&c_ja;</trd>
  <trdgrp lng="bg">
    <trd>&c_R;&c_je;&c_p;&c_u;&c_b;&c_l;&c_i;&c_k;&c_a; <ind>&c_M;&c_a;&c_k;&c_je;&c_d;&c_o;&c_n;&c_i;&c_ja;</ind></trd>,
    <trd>&c_M;&c_a;&c_k;&c_je;&c_d;&c_o;&c_n;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>Maced&ograve;nia</trd>,
    <trd>Rep&uacute;blica de <ind>Maced&ograve;nia</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Republik <ind>Mazedonien</ind></trd>,
    <trd>Mazedonien</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Macedonia</trd>,
    <trd>Republic of <ind>Macedonia</ind></trd>,
    <trd>FYROM</trd>
  </trdgrp>
  <trd lng="fa">&a_m;&a_q;&a_d;&a_w;&a_n;&f_y;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>Mac&eacute;doine</trd>,
    <trd>R&eacute;publique de Mac&eacute;doine</trd>
  </trdgrp>
  <trd lng="nl">Macedoni&euml;</trd>
  <trdgrp lng="pl">
    <trd>Macedonia</trd>,
    <trd>Republika <ind>Macedonii</ind></trd>,
    <trd>By&lstroke;a Jugos&lstroke;owia&nacute;ska Republika
    <ind>Macedonii</ind></trd>
  </trdgrp>
  <trd lng="pt">Maced&ocirc;nia</trd>
</drv>

</art>

