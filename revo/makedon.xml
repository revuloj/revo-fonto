<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>makedon</rad>/o
</kap>

<drv mrk="makedon.0ujo">
  <kap>
    <tld lit="M"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="makedon.0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la suda <ref tip="malprt"
      cel="euxrop1.E0o.geog">E&ubreve;ropo</ref>
      kun &ccirc;efurbo Skopjo.
      Teritorio de Respubliko Makedonio (a&ubreve; alie: Iama
      Jugoslavuja 
      Respubliko Makedonio).
      ISO-3166 kodo: MK.
      &Gcirc;i limas kun
      <ref tip="vid" cel="alban.A0ujo">Albanujo</ref>,
      <ref tip="vid" cel="bulgar.B0ujo">Bulgarujo</ref>,
      <ref tip="vid" cel="grek.0ujo.GEOG">Grekujo</ref>
      kaj <ref tip="vid" cel="serb.0ujo.GEOG">Serbujo</ref>.
    </dif>
    <ref tip="sin" cel="makedoni.M0o.RESPUBLIKO">Makedonio</ref>
    <trd lng="en">FYROM</trd>
    <trdgrp lng="pl">
      <trd>Macedonia</trd>,
      <trd>Republika <ind>Macedonii</ind></trd>,
      <trd>By&lstroke;a Jugos&lstroke;owia&nacute;ska Republika
      <ind>Macedonii</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

