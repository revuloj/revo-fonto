<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gimnas.xml,v 1.22 2026/02/23 15:05:07 revo Exp $">
<kap>
  <rad>gimnast</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="gimnas.0o">
  <kap><tld/>o</kap>
  <snc mrk="gimnas.0o.gimnastikisto">
    <dif>
      <ref tip="dif" cel="gimnas1.0isto">Gimnastikisto</ref>:
      <ekz>
        <nom>Olga Korbut</nom> <klr>[&#8230;]</klr> estas belarusa, sovet-naskita <tld/>o kiu gajnis kvar
        orajn medalojn kaj du ar&gcirc;entajn dum la Someraj Olimpikoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=221156">Olga Korbut</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_m;&c_n;&c_a;&c_s;&c_t;</trd>
  <trd lng="ca">gimnasta</trd>
  <trdgrp lng="de">
    <trd>Turner</trd>,
    <trd>Gymnastiker</trd>
  </trdgrp>
  <trd lng="es">gimnasta</trd>
  <trd lng="fr">gymnaste</trd>
  <trdgrp lng="ja">
    <trd>&#x4F53;&#x64CD;&#25945;&#x5E2B;  <pr>&#x305F;&#12356;&#x305D;&#12358;&#x304D;&#12423;&#12358;&#12375;</pr></trd>,
    <trd>&#x4F53;&#x80B2;&#x5BB6;  <pr>&#x305F;&#12356;&#12356;&#x304F;&#x304B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>turner</trd>,
    <trd>gymnast</trd>
  </trdgrp>
  <trd lng="pl">gimnastyk</trd>
  <trd lng="pt">ginasta</trd>
  <trd lng="ru">&c_g;&c_i;&c_m;&c_n;&c_a;&c_s;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>cvi&ccaron;enec</trd>,
    <trd>gymnasta</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>borec</trd>,
    <trd>cvi&ccaron;enec</trd>,
    <trd>gymnasta</trd>
  </trdgrp>
  <trd lng="uk">&c_g;&c_ib;&#x43C;&#x43D;&c_a;&c_s;&c_t;</trd>
  <trd lng="zh">&#x4F53;&#x64CD;&#x8FD0;&#x52A8;&#21592;
  <pr>t&#x1D0;c&amacron;oy&#xF9;nd&#xF2;ngyu&#xE1;n</pr></trd>
</drv>

<drv mrk="gimnas.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="gimnas.0ejo.gimnastikejo">
    <dif>
      <ref tip="dif" cel="gimnas1.0ejo">Gimnastikejo</ref>:
      <ekz>
        trovi&gcirc;as <tld/>ejo proksime al mia lo&gcirc;ejo, kaj mi ofte
        preterpasas &gcirc;in piede
        <fnt>
          <aut>Felo</aut>,
          <vrk><url ref="http://felejo.blogspot.com/">
          La &ccirc;elo de Felo</url></vrk>,
          <lok>2007-04-27</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_a;&c_r;&c_t;&c_o;&c_v;&c_a;&c_ja; &c_z;&c_a;&c_l;&c_ja;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_r;&c_t;&c_z;&c_a;&c_l;&c_ja;</trd>
  </trdgrp>
  <trd lng="cs">t&ecaron;locvi&ccaron;na</trd>
  <trd lng="de">Turnhalle</trd>
  <trd lng="es">gimnasio</trd>
  <trd lng="fr">gymnase</trd>
  <trd lng="ja">&#x4F53;&#x80B2;&#39208;  <pr>&#x305F;&#12356;&#12356;&#x304F;&#x304B;&#12435;</pr></trd>
  <trd lng="pl">sala <ind>gimnastyczna</ind></trd>
  <trd lng="sk">telocvi&ccaron;&ncaron;a</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_ib;&#x43C;&#x43D;&c_a;&c_s;&c_t;&c_i;&c_ch;&#x43D;&c_i;&c_j; &c_z;&c_a;&#x43B;</trd>,
    <trd>&c_g;&c_ib;&#x43C;&#x43D;&c_a;&c_s;&c_t;&c_i;&c_ch;&#x43D;&c_i;&c_j; &#x43C;&c_a;&c_j;&c_d;&c_a;&#x43D;&c_ch;&c_i;&#x43A;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4F53;&#x80B2;&#39302; <pr>t&#x1D0;y&#xF9;gu&#x1CE;n</pr></trd>,
    <trd>&#20581;&#x8EAB;&#x5BA4; <pr>ji&#xE0;nsh&emacron;nsh&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="gimnas.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="gimnas.0ilo.gimnastikilo">
    <dif>
      <ref tip="dif" cel="gimnas1.0ilo">Gimnastikilo</ref>:
      <ekz>
        &scirc;i a&ccirc;etis <tld/>ilojn kaj ekzercas la korpon &ccirc;iumatene en la
        &ccirc;ambro
        <fnt>
          <aut>Chen Tian</aut>,
          <vrk><url ref="http://www.espero.com.cn/06s/06s-4/06s-4-7.htm">
          Ekzerci korpon...</url></vrk>,
          <lok>2004-11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_a;&c_r;&c_t;&c_o;&c_v;&c_y; &c_s;&c_n;&c_a;&c_r;&c_a;&c_d;</trd>,
    <trd>&c_g;&c_ib;&c_m;&c_n;&c_a;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_y; &c_s;&c_n;&c_a;&c_r;&c_a;&c_d;</trd>
  </trdgrp>
  <trd lng="de">Turnger&auml;t</trd>
  <trdgrp lng="es">
    <trd>material de gimnasia</trd>,
    <trd>m&aacute;quina de gimnasio</trd>
  </trdgrp>
  <trd lng="fr">mat&eacute;riel de gymnastique</trd>
  <trd lng="pl">przyrz&aogonek;d do &cacute;wicze&nacute; <ind>gimnastycznych</ind></trd>
  <trd lng="uk">&c_g;&c_ib;&#x43C;&#x43D;&c_a;&c_s;&c_t;&c_i;&c_ch;&#x43D;&c_i;&c_j; &c_s;&#x43D;&c_a;&c_r;&#x44F;&c_d;</trd>
</drv>
</art>
