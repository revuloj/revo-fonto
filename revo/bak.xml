<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>bak</rad>/i
</kap>
<drv mrk="bak.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malmoligi per varmo en cindro a&ubreve; forno.
    </dif>
    <subsnc mrk="bak.0i.KUI">
      <uzo tip="fak">KUI</uzo>
      <dif>
        <ref tip="super" cel="kuir.0i">Kuiri</ref> varmigante en cindro
        a&ubreve; forno:
        <ekz>
          <tld/>i panon, kukon, viandon;
        </ekz>
        <ekz>
          ili bakis macajn platpanojn
          <fnt><bib>MT</bib>, <lok>Eliro 12:39</lok></fnt>;
        </ekz>
        <ekz>
          mi bakis sur &gcirc;iaj karboj panon
          <fnt><bib>MT</bib>, <lok>Jesaja 44:19</lok></fnt>;
        </ekz>
        <ekz>
          &scirc;i prenis paston, knedis &gcirc;in, preparis anta&ubreve;
          liaj okuloj kaj bakis la kuketojn
          <fnt><bib>MT</bib>, <lok>II. Samuel 13:8</lok></fnt>;
        </ekz>
        <ekz>
          oni ekbruligadis la fajron kaj kiam la varmega cindro ku&scirc;is
          alte kaj ardante oni <tld/>adis en &gcirc;i la panon
          <fnt><bib>Fab1</bib>, <lok>Ligo de amikeco</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="fr">cuire<klr tip="ind"> (au four)</klr></trd>
      <trd lng="hu">s&uuml;t</trd>
    </subsnc>
    <subsnc>
      <uzo tip="fak">TEK</uzo>
      <ekz>
        <tld/>i brikojn, potojn.
      </ekz>
    </subsnc>
    <trd lng="de">backen</trd>
    <trd lng="es">hornear</trd>
    <trd lng="hu">&eacute;get<klr tip="ind"> (pl. t&eacute;gl&aacute;t)</klr></trd>
    <trd lng="sk">piec&tcaron;</trd>
    <trd lng="vo">bak&ouml;n</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Verki ion, prepari kaj elfari ion:
      <ekz>
        projektoj en rapideco kaj sen sufi&ccirc;a pripenso el<tld/>itaj
        <fnt><bib>EE</bib>, <lok>&ccirc;ap. 6</lok></fnt>;
      </ekz>
    </dif>
    <trd lng="fr">concocter<klr tip="ind"> (fig.)</klr></trd>
  </snc>
  <trd lng="cs">p&eacute;ci (chl&eacute;b)</trd>
  <trd lng="en">bake</trd>
  <trd lng="nl">bakken<klr tip="ind"> (in oven)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_ch;&c_mol; <klr>(&c_h;&c_l;&c_je;&c_b;)</klr></trd>,
    <trd>&c_o;&c_b;&c_zh;&c_i;&c_g;&c_a;&c_t;&c_mol;
    <klr>(&c_k;&c_i;&c_r;&c_p;&c_i;&c_ch;&c_i;,
    &c_g;&c_o;&c_n;&c_ch;&c_a;&c_r;&c_n;&c_u;&c_ju;
    &c_p;&c_o;&c_s;&c_u;&c_d;&c_u;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="bak.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Iu, kiu profesie bakas.
    </dif>
  </snc>
  <trd lng="de">B&auml;cker</trd>
  <trd lng="en">baker</trd>
  <trd lng="fr">boulanger</trd>
  <trd lng="hu">p&eacute;k</trd>
  <trd lng="nl">bakker</trd>
  <trd lng="ru">&c_p;&c_je;&c_k;&c_a;&c_r;&c_mol;</trd>
  <trd lng="sk">pek&aacute;r</trd>
</drv>

<drv mrk="bak.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <dif>
      Profunda, mallar&gcirc;a volba masona&jcirc;o, en kiu oni <tld/>as panon,
      potojn ktp.
    </dif>
    <trd lng="de">Backofen</trd>
    <trd lng="ru">&c_p;&c_je;&c_ch;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Malfermebla kesto en kuirforno, en kiu oni <tld/>as viandon, kukojn ktp.
    </dif>
    <trdgrp lng="de">
      <trd>Backform</trd>,
      <trd>Brotform</trd>,
      <trd>Kuchenform</trd>
    </trdgrp>
    <trd lng="ru">&c_d;&c_u;&c_h;&c_o;&c_v;&c_k;&c_a;</trd>
  </snc>
  <trd lng="fr">four <klr tip="ind">(chambre de cuisson)</klr></trd>
  <trd lng="en">oven</trd>
  <trd lng="hu">s&uuml;t&odblac;<klr tip="ind"> (t&udblac;zhelyen)</klr></trd>
  <trd lng="nl">oven</trd>
  <trd lng="sk">pec</trd>
</drv>
<adm>
aldonu bakejo; bakujo 1 = bakforno (pluraj ekz. en literaturo)
</adm>
</art>
