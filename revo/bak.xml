<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bak.xml,v 1.45 2021/03/04 19:18:48 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bak</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="bak.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="bak.0i.KOMUNE">
    <dif>
      Malmoligi per varmo en cindro a&ubreve; forno.
    </dif>
    <subsnc mrk="bak.0i.KUI">
      <uzo tip="fak">KUI</uzo>
      <dif>
        <ref tip="super" cel="kuir.0i">Kuiri</ref> varmigante en cindro
        a&ubreve; forno:
        <ekz>
          <tld/>i panon, kukon, viandon;
        </ekz>
        <ekz>
          ili <tld/>is macajn platpanojn
          <fnt><bib>MT</bib>, <lok>&Eli; 12:39</lok></fnt>;
        </ekz>
        <ekz>
          mi <tld/>is sur &gcirc;iaj karboj panon
          <fnt><bib>MT</bib>, <lok>&Jes; 44:19</lok></fnt>;
        </ekz>
        <ekz>
          &scirc;i prenis paston, knedis &gcirc;in, preparis
          anta&ubreve;
          liaj okuloj kaj <tld/>is la kuketojn
          <fnt><bib>MT</bib>, <lok>&Sa2; 13:8</lok></fnt>;
        </ekz>
        <ekz>
          oni ekbruligadis la fajron kaj kiam la varmega cindro
          ku&scirc;is
          alte kaj ardante oni <tld/>adis en &gcirc;i la panon
          <fnt><bib>Fab1</bib>, <lok>ligo de amikeco</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="br">poba&ntilde;</trd>
      <trdgrp lng="ca">
        <trd>cuinar</trd>,
        <trd>coure</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>piec</trd>,
        <trd>wypieka&cacute;</trd>
      </trdgrp>
      <trd lng="fr">cuire<klr tip="ind"> (au four)</klr></trd>
      <trd lng="hu">s&uuml;t</trd>
      <trd lng="pt">assar</trd>
      
    </subsnc>
    <subsnc mrk="bak.0i.TEK">
      <uzo tip="fak">TEK</uzo>
      <dif>
        Malmoligi objekton el argilo a&ubreve; simila tero per forta
        varmigo:
        <ekz>
          <tld/>i brikojn, potojn.
        </ekz>
      </dif>
      <trd lng="ca">coure</trd>
    </subsnc>
    <trd lng="de">backen</trd>
    <trdgrp lng="es">
      <trd>hornear</trd>,
      <trd>hornar</trd>,
      <trd>cocer</trd>
    </trdgrp>
    <trd lng="he">&lamed;&alef;&pe;&vav;&tav;
    \<ind>&alef;&pe;&he;</ind>\
    </trd>
    <trd lng="hu">&eacute;get<klr tip="ind"> (pl.
    t&eacute;gl&aacute;t)</klr>
    </trd>
    <trdgrp lng="pl">
      <trd>wypieka&cacute;</trd>,
      <trd>wypala&cacute;</trd>
    </trdgrp>
    
    <trd lng="sk">piec&tcaron;</trd>
    <trd lng="vo">bak&ouml;n</trd>
  </snc>
  <snc mrk="bak.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Verki ion, prepari kaj elfari ion:
      <ekz>
        projektoj en rapideco kaj sen sufi&ccirc;a pripenso el<tld/>itaj
        <fnt><bib>EE</bib>, <lok>&ccirc;apitro 6a</lok></fnt>;
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>preparar</trd>,
      <trd>disposar</trd>,
      <trd>apromptar</trd>
    </trdgrp>
    <trd lng="fr">concocter<klr tip="ind"> (fig.)</klr></trd>
    <trd lng="pl">piec</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ja;&c_ch;&c_y;
    <klr>(&c_h;&c_l;&c_je;&c_b;)</klr></trd>,
    <trd>&c_a;&c_b;&c_p;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_je;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>p&eacute;ci (chl&eacute;b)</trd>,
    <trd>p&aacute;lit (cihly)</trd>,
    <trd>p&eacute;ci <klr>(chl&eacute;b, maso)</klr></trd>
  </trdgrp>
  <trd lng="en">bake</trd>
  <trd lng="nl">bakken<klr tip="ind"> (in oven)</klr></trd>
  <trd lng="ro">frige</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_ch;&c_mol;
    <klr>(&c_h;&c_l;&c_je;&c_b;)</klr></trd>,
    <trd>&c_o;&c_b;&c_zh;&c_i;&c_g;&c_a;&c_t;&c_mol;
    <klr>(&c_k;&c_i;&c_r;&c_p;&c_i;&c_ch;&c_i;,
    &c_g;&c_o;&c_n;&c_ch;&c_a;&c_r;&c_n;&c_u;&c_ju;
    &c_p;&c_o;&c_s;&c_u;&c_d;&c_u;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="bak.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="bak.0ajxo.KOMUNE">
    <dif>
      &Ccirc;ia objekto, kiun oni <tld/>is.
    </dif>
    <subsnc mrk="bak.0ajxo.KUI">
      <uzo tip="fak">KUI</uzo>
      <dif>
        Man&gcirc;a&jcirc;o, kiun oni <tld/>e kuiris:
        <ekz>
          prilabora&jcirc;oj en formo de faruno <klr>...</klr> pano,
          <tld/>a&jcirc;o kaj
          faruna&jcirc;o de diversaj specoj
          <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj,
          bagateloj</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="en">baked good</trd>
      <trd lng="ca">pastisseria</trd>
      <trdgrp lng="pl">
        <trd>pieczywo</trd>,
        <trd>wypiek</trd>
      </trdgrp>
      <trd lng="ro">p&acirc;ine alb&abreve;</trd>
    </subsnc>
    <subsnc mrk="bak.0ajxo.POT">
      <dif>
        Vazo a&ubreve; alia argila&jcirc;o malmoligita per <tld/>ado.
      </dif>
      <trd lng="en">baked ware</trd>
      <trd lng="ca">terrissa</trd>
      <trd lng="pl">wypiek</trd>
    </subsnc>
  </snc>
  <trd lng="he">&#x5DE;&#x5B7;&#x5D0;&#x5B2;&#x5E4;&#x5B4;&#x5D9;&#x5B8;&#x5D4;</trd>
</drv>

<drv mrk="bak.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="bak.0isto.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Iu, kiu profesie bakas.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_k;&c_a;&c_r;</trd>
  <trdgrp lng="bg">
    <trd>&c_h;&c_l;&c_je;&c_b;&c_a;&c_r;</trd>,
    <trd>&c_p;&c_je;&c_k;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="br">pober</trd>
  <trdgrp lng="ca">
    <trd>forner</trd>,
    <trd>flequer</trd>
  </trdgrp>
  <trd lng="cs">peka&rcaron;</trd>
  <trd lng="de">B&auml;cker</trd>
  <trd lng="en">baker</trd>
  <trd lng="es">hornero</trd>
  <trd lng="fr">boulanger</trd>
  <trd lng="he">&alef;&vav;&pe;&he;</trd>
  <trd lng="hu">p&eacute;k</trd>
  <trd lng="nl">bakker</trd>
  <trdgrp lng="pl">
    <trd>piekarz</trd>
  </trdgrp>
  <trd lng="ro">brutar</trd>
  <trd lng="ru">&c_p;&c_je;&c_k;&c_a;&c_r;&c_mol;</trd>
  <trd lng="sk">pek&aacute;r</trd>
</drv>

<drv mrk="bak.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="bak.0ujo.KOMUNE">
    <dif>
      Varmimuna kava&jcirc;o en kiu oni <tld/>as:
    </dif>
    <subsnc mrk="bak.0ujo.KUI">
      <uzo tip="fak">KUI</uzo>
      <dif>
        Malfermebla kesto en
        <ref tip="malprt" cel="forn.bak0o.TEK">bakforno</ref>,
        en kiu oni <tld/>as viandon,
        kukojn kaj aliajn.
      </dif>
      <trd lng="be">&c_d;&c_u;&c_h;&c_o;&c_w;&c_k;&c_a;</trd>
      <trdgrp lng="de">
        <trd>Backform</trd>,
        <trd>Brotform</trd>,
        <trd>Kuchenform</trd>
      </trdgrp>
      <trd lng="he">&tav;&bet;&nun;&yod;&tav; &alef;&pe;&yod;&he;</trd>
      <trd lng="pl">piekarnik</trd>
      <trd lng="ru">&c_d;&c_u;&c_h;&c_o;&c_v;&c_k;&c_a;</trd>
    </subsnc>
    <subsnc mrk="bak.0ujo.POT">
      <dif>
        Profunda, mallar&gcirc;a volba masona&jcirc;o, en kiu oni
        <tld/>as panon, potojn...
      </dif>
      <trd lng="be">&c_p;&c_je;&c_ch;</trd>
      <trd lng="de">Backofen</trd>
      <trd lng="he">&tav;&nun;&vav;&resh;</trd>
      <trd lng="pl">piec</trd>
      <trd lng="ru">&c_p;&c_je;&c_ch;&c_mol;</trd>
      <trdgrp lng="uk">
        <trd>&c_p;&c_ib;&c_ch;</trd>,
        <trd>&c_d;&c_u;&c_h;&c_o;&c_v;&c_k;&c_a;</trd>
      </trdgrp>
    </subsnc>    
  </snc>
  <trd lng="br">forn</trd>
  <trd lng="ca">forn</trd>
  <trdgrp lng="cs">
    <trd>peka&rcaron;sk&aacute; pec</trd>,
    <trd>pe&ccaron;&iacute;c&iacute; trouba</trd>
  </trdgrp>
  <trd lng="de">Ofen</trd>
  <trd lng="en">oven</trd>
  <trd lng="es">horno</trd>
  <trd lng="fr">four<klr tip="ind"> (chambre de cuisson)</klr></trd>
  <trd lng="hu">s&uuml;t&odblac;<klr tip="ind">
  (t&udblac;zhelyen)</klr></trd>
  <trd lng="nl">oven</trd>
  <trdgrp lng="sk">
    <trd>pec</trd>,
    <trd>pec na pe&ccaron;enie</trd>,
    <trd>pec na vypa&lcaron;ovanie hrncov</trd>
  </trdgrp>
</drv>

<drv mrk="bak.nov0ita">
  <kap>nov<tld/>ita,
  <var><kap>fre&scirc;<tld/>ita</kap></var>,
  <var><kap>fre&scirc;e <tld/>ita</kap></var></kap>
  <snc mrk="bak.nov0ita.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      &Jcirc;us fininstruita, ankora&ubreve; ne sperta pri iu temo;
      ne <ref tip="ant" cel="hard.0ita.FIG">hardita</ref>:
      <ekz>
        he! mi estas nur nov<tld/>ita alpinisto
        <fnt>
          <aut>L. Huot-Sordot, trad. E. No&euml;l</aut>,
          <vrk>En Savojo</vrk>,
          <lok>Lingvo Internacia, 1913, p. 53a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fre&scirc;e <tld/>ita abituriento <klr>...</klr> povis sin
        imagi librosperta erudito
        <fnt><bib>KielAkvo</bib>, <lok>1:1</lok></fnt>;
      </ekz>
      <ekz>
        balo de fre&scirc;<tld/>itoj
          <fnt>
            <aut>M. Malovec</aut>,
            <vrk><url ref="http://www.eventoj.hu/steb/vortaroj/pedagogia-terminaro-malovec.htm">
            Pedagogia terminaro</url></vrk>,
            <lok>[vidita en 2011]</lok>
          </fnt>.
    </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bek.flav0ulo">flavbekulo</ref>,
      <ref cel="novic.0a.FIG">novica<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="en"><trd>freshly baked</trd><trd>green</trd>,
  <trd>fresh</trd>, <trd>new</trd></trdgrp>
  <trdgrp lng="ca">
    <trd>pipioli</trd>,
    <trd>noven&ccedil;&agrave;</trd>
  </trdgrp>
  <trd lng="es">reci&eacute;n horneado</trd>
  <trdgrp lng="fr">
    <trd>b&eacute;jaune<klr> (adj.)</klr></trd>,
    <trd>bleu<klr> (nouveau, adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&sacute;wie&zdot;o <ind>upieczony</ind></trd>
    <trd>zielony<klr> (nowicjusz)</klr></trd>
  </trdgrp>
</drv>
<adm>
  aldonu bakejo.
</adm>
</art>
