<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: konfis.xml,v 1.20 2022/02/23 18:34:14 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>konfisk</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="konfis.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="fak">JUR</uzo>
    <dif>
      Atribui per dekreto al la <ref cel="fisk.0o.kaso">fisko</ref>
      la propra&jcirc;ojn de iu por lin puni:
      <ekz>
        <tld/>i la bienojn de elmigrantoj;
      </ekz>
      <ekz>
        <tld/>i kontrabanda&jcirc;ojn.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>beschlagnahmen</trd>,
      <trd>konfiszieren</trd>,
      <trd>einziehen</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Forpreni de iu la&ubreve; rajtigita a&ubreve;toritato ion malpermesitan:
      <ekz>
        <tld/>i libron al lernanto<fnt>B</fnt>,
      </ekz>
      <ekz>
        tran&ccirc;ilon al infano;
      </ekz>
      <ekz>
        <tld/>i nefre&scirc;an viandon en la bazaro<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>einziehen</trd>
    </trdgrp>
  </snc>
   <trd lng="be">&c_k;&c_a;&c_n;&c_f;&c_ib;&c_s;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>konfiskovat</trd>,
    <trd>zabavit ve prosp&ecaron;ch st&aacute;tu</trd>,
    <trd>zabrat ve prosp&ecaron;ch st&aacute;tu</trd>
  </trdgrp>
  <trd lng="en">confiscate</trd>
  <trd lng="es">confiscar</trd>
  <trdgrp lng="fr">
    <trd>confisquer</trd>,
    <trd>saisir<klr tip="ind"> (confisquer)</klr></trd>,
    <trd>soustraire<klr tip="ind"> (confisquer)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>lefoglal</trd>,
    <trd>elkoboz</trd>,
    <trd>konfisk&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>confisqueren</trd>,
    <trd>verbeurdverklaren</trd>,
    <trd>in <ind>beslag</ind> nemen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>konfiskowa&cacute;</trd>,
    <trd>zagrabi&cacute;</trd>
  </trdgrp>   
  <trd lng="pt">confiscar</trd>
  <trd lng="ro">confisca</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_f;&c_i;&c_s;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>konfi&scaron;kova&tcaron;</trd>,
    <trd>zhaba&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="konfis.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_f;&c_ib;&c_s;&c_k;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>konfiskace</trd>,
    <trd>zabaven&iacute;</trd>,
    <trd>zabr&aacute;n&iacute; ve prosp&ecaron;ch st&aacute;tu</trd>,
    <trd>z&aacute;bor</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Konfiszierung</trd>,
    <trd>Einzug</trd>,
    <trd>Entzug</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>confiscation</trd>,
    <trd>saisie<klr tip="ind"> (confiscation)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>lefoglal&aacute;s <klr tip="ind">(elkoboz)</klr></trd>,
    <trd>konfisk&aacute;l&aacute;s</trd>,
    <trd>elkobz&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>beslagname</trd>,
    <trd>beslaglegging</trd>
  </trdgrp>
  <trd lng="pl">konfiskata</trd>     
  <trd lng="ro">confiscare</trd>
  <trdgrp lng="sk">
    <trd>konfi&scaron;k&aacute;cia</trd>,
    <trd>zabavenie</trd>
  </trdgrp>
</drv>
</art>
