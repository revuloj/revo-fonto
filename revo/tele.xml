<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tele</rad>- <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="tele.0">
  <kap><tld/>-</kap>
  <snc mrk="tele.0.TEK">
  <gra><vspec>prefikso</vspec></gra>
  <uzo tip="fak">TEK</uzo>
    <dif>
      Teknika prefikso indikanta funkciadon el distanco:
      <ekz>
        <tld/>komuniko, <tld/>objektivo, <tld/>tajpilo, <tld/>vido..
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Pse&ubreve;doprefisko en kelkaj radikoj:
      <ekz>
        telefono, telegrafo, telegramo, telepatio.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_l;&c_je;-</trd>
  <trd lng="de">Tele-, Fern-</trd>
  <trd lng="en">tele-</trd>
  <trd lng="fr">tele-</trd>
  <trdgrp lng="hu">
    <trd>tele-</trd>,
    <trd>t&aacute;v-</trd>
  </trdgrp>
  <trd lng="nl">tele-</trd>
  <trd lng="ru">&c_t;&c_je;&c_l;&c_je;-</trd>
</drv>
</art>
