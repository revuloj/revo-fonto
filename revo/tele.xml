        telefono, telegrafo, telegramo, telepatio.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Tele-, Fern-</trd>
  <trd lng="en">tele-</trd>
  <trd lng="fr">tele-</trd>
  <trd lng="nl">tele-</trd>
</drv>
