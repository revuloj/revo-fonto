<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tele.xml,v 1.12 2024/12/09 14:29:46 revo Exp $">
<kap>
  <rad>tele</rad>- <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="tele.0">
  <kap><tld/>-</kap>
  <snc mrk="tele.0.TEK">
  <gra><vspec>prefikso</vspec></gra>
  <uzo tip="fak">TEK</uzo>
    <dif>
      Teknika prefikso indikanta funkciadon el <ref cel="distan.0o.SPA">distanco</ref>:
      <ekz>
        <ref tip="vid" cel="teleko.0i"><tld/>komuniki</ref>,
        <ref cel="telekopi.0o">telekopio</ref>,
        <ref cel="telemezur.0i">telemezuri</ref>,
        <ref tip="vid" cel="teleobjektiv.0o"><tld/>objektivo</ref>,
        <ref cel="telereg.0i">teleregi</ref>,
        <tld/>tajpilo,
        <ref tip="vid" cel="televid.0o"><tld/>vido</ref>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="tele.0.pseuxdoprefikso">
    <dif>
      Pse&ubreve;doprefikso en kelkaj radikoj:
      <ekz>
        <ref cel="telefo.0o">telefono</ref>,
        <ref cel="telegr.0o">telegrafo</ref>,
        <ref cel="telegr1.0o">telegramo</ref>,
        <ref cel="telepa.0o">telepatio</ref>,
        <ref cel="telesk.0o">teleskopo</ref>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_l;&c_je;-</trd>
  <trd lng="de">Tele-, Fern-</trd>
  <trd lng="en">tele-</trd>
  <trd lng="fr">t&eacute;l&eacute;-</trd>
  <trdgrp lng="hu">
    <trd>tele-</trd>,
    <trd>t&aacute;v-</trd>
  </trdgrp>
  <trd lng="nl">tele-</trd>
  <trd lng="pl">tele-</trd>
  <trdgrp lng="pt">
    <trd>tele-</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_l;&c_je;-</trd>
  <trd lng="uk">&c_t;&c_je;&c_l;&c_je;&#8230;</trd>
</drv>
</art>
