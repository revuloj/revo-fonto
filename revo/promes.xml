<drv mrk="promes.mem0o">
  <kap>
    mem<tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="P"/>o, kiun oni donas al si mem:
      <ekz>
        mi <tld/>is al mi skribi &ccirc;iutage en tiu &ccirc;i kajero kaj mi tenos la mem<tld/>on,
        kvankam hodia&ubreve; mi ne povas
        <fnt>
          <bib>ChB</bib>
          <lok>29</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de"><klr>(guter)</klr> Vorsatz</trd>
</drv>

<drv mrk="promes.mult0a">
  <kap>
    mult<tld/>a,
    <var><kap>multe<tld/>a</kap></var>
  </kap>
  <snc>
    <dif>
      <tld lit="P"/>anta multe:
      <ekz>
        dikaj libroj kun <klr>[&#8230;]</klr> multe<tld/>aj frazoj
        <fnt>
          <bib>EE</bib>
          <lok>VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu mult<tld/>a stato kontrastas kun preska&ubreve; ignorema sinteno de la registaro
        <fnt>
          <bib>EeP</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>vielversprechend</trd>
  </trdgrp>
  <trd lng="be">&c_sh;&c_m;&c_a;&c_t;&c_a;&c_b;&c_ja;&c_c;&c_a;&c_ju;&c_ch;&c_y;</trd>
  <trd lng="en">much <ind>promising</ind></trd>
  <trd lng="id"><baz>banyak</baz> janji</trd>
  <trdgrp lng="ja">
    <trd>&#26377;&#26395;&#12394;  <pr>&#12422;&#12358;&#12412;&#12358;&#12394;</pr></trd>,
    <trd>&#35211;&#36796;&#12415;&#12398;&#12354;&#12427;
    <pr>&#12415;&#12371;&#12415;&#12398;&#12354;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wiele  <ind>obiecuj&aogonek;cy</ind></trd>,
    <trd>wielce  <ind>obiecuj&aogonek;cy</ind></trd>
  </trdgrp>
  <trd lng="uk">&c_b;&c_a;&c_g;&c_a;&c_t;&c_o;&c_o;&c_b;&c_ib;&c_c;&c_ja;&c_ju;&c_ch;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#20805;&#28385;&#24076;&#26395; <pr>ch&omacron;ngm&#462;nx&imacron;w&agrave;ng</pr></trd>,
    <trd>&#20805;&#28415;&#24076;&#26395; <pr>ch&omacron;ngm&#462;nx&imacron;w&agrave;ng</pr></trd>,
    <trd>&#21487;&#20197;&#38928;&#35211; <pr>k&ecaron;y&#464;y&ugrave;ji&agrave;n</pr></trd>,
    <trd>&#21487;&#20197;&#39044;&#35265; <pr>k&ecaron;y&#464;y&ugrave;ji&agrave;n</pr></trd>,
    <trd>&#26377;&#20026; <pr>y&#466;uw&eacute;i</pr></trd>,
