  <trd lng="pl">obietnica</trd>
  <trd lng="pt">promessa</trd>
  <trd lng="ru">&c_o;&c_b;&c_je;&c_shch;&c_a;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">s&lcaron;ub</trd>
  <trd lng="sv">l&ouml;fte</trd>
  <trd lng="uk">&#x43E;&c_b;&c_ib;&c_c;&#x44F;&#x43D;&#x43A;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x8AFE; <pr>nu&#xF2;</pr></trd>,
    <trd>&#x8BFA; <pr>nu&#xF2;</pr></trd>,
    <trd>&#x8AFE;&#x8A00; <pr>nu&#xF2;y&#xE1;n</pr></trd>,
    <trd>&#x8BFA;&#x8A00; <pr>nu&#xF2;y&#xE1;n</pr></trd>,
    <trd>&#x5141;&#x8AFE; <pr>y&#x1D4;nnu&#xF2;</pr></trd>,
    <trd>&#x5141;&#x8BFA; <pr>y&#x1D4;nnu&#xF2;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="promes.0plena">
  <kap>
    <tld/>plena,
    <var><kap>bon<tld/>a</kap></var>
  </kap>
  <snc>
    <dif>
      Tre <ref tip="sin" cel="esper.0iga">esperiga</ref>,
      <ref tip="sin" cel="auxgur.bon0a">bona&ubreve;gura</ref>:
      <ekz>
        la nova&jcirc;o jam &scirc;ajnis al <nom>Ajna</nom> pli bon<tld/>a,
