<drv mrk="promes.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Io <tld/>ita:
      <ekz>
        <nom>Jakob</nom> faris sanktan <tld/>on
        <klr>(vd <ref tip="sub" cel="vot.0o">voto</ref>)</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 28:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o devas esti plenumita
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ursa Felo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi estis ligita de <tld/>o
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekkvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi donis solenan <klr>[promeson]</klr>, ke mi silentos
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi redonas al vi vian <tld/>on, sed mi ne reprenas la mian
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de geedzi&gcirc;o
        <klr>(vd <ref cel="fiancx.0igxo">gefian&ccirc;i&gcirc;o</ref>)</klr>
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekkvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        nutri per <tld/>oj
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        sub seruro <tld/>ojn tenu, sed doninte ne reprenu
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        per <tld/>oj estus pavimita la infero
        <fnt>
          <bib>PrV</bib>
          <lok>P</lok>
        </fnt>;
      </ekz>
      <ekz>
        li faras princan <tld/>on, sed ne havas e&ccirc; speson
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estas lar&gcirc;a, plenumo estas &scirc;ar&gcirc;a
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
<!--
       <ekz>
         <ind>mult<tld/>a</ind> elpensa&jcirc;o;
         <trd lng="be">&c_sh;&c_m;&c_a;&c_t;&c_a;&c_b;&c_ja;&c_c;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
         <trd lng="ru">&c_m;&c_n;&c_o;&c_g;&c_o;&c_o;&c_b;&c_je;&c_shch;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>
       </ekz>
-->
     </dif>
    <refgrp tip="vid">
      <ref cel="dev.sin0igo">sindevigo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
     <trd>&c_a;&c_b;&c_ja;&c_c;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
     <trd>&c_a;&c_b;&c_ja;&c_c;&c_a;&c_n;&c_k;&c_a;</trd>
   </trdgrp>
  <trd lng="bo">&t_kha;&t_sa;&t_tsheg;&t_la;&t_drengbu;&t_na;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trd lng="ca">promesa</trd>
  <trdgrp lng="cs">
    <trd>promise</trd>,
    <trd>p&rcaron;&iacute;slib</trd>,
    <trd>slib</trd>,
    <trd>zasl&iacute;ben&iacute;</trd>
  </trdgrp>
  <trd lng="de">Versprechen</trd>
  <trd lng="es">promesa</trd>
  <trdgrp lng="fa">
     <trd>&a_q;&a_w;&a_l;</trd>,
     <trd>&a_w;&a_ayn;&a_d;&a_h;</trd>,
     <trd>&a_n;&a_w;&f_y;&a_d;</trd>
   </trdgrp>
  <trd lng="fr">promesse</trd>
  <trd lng="hu">&iacute;g&eacute;ret</trd>
  <trdgrp lng="id">
    <trd>ikrar</trd>,
    <trd>janji</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#32004;&#26463;  <pr>&#12420;&#12367;&#12381;&#12367;</pr></trd>,
    <trd>&#35211;&#36796;&#12415;  <pr>&#12415;&#12371;&#12415;</pr></trd>,
    <trd>&#26399;&#24453;  <pr>&#12365;&#12383;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">belofte</trd>
  <trd lng="pl">obietnica</trd>
  <trd lng="pt">promessa</trd>
  <trd lng="ru">&c_o;&c_b;&c_je;&c_shch;&c_a;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">s&lcaron;ub</trd>
  <trd lng="sv">l&ouml;fte</trd>
  <trd lng="uk">&c_o;&c_b;&c_ib;&c_c;&c_ja;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#35582; <pr>nu&ograve;</pr></trd>,
    <trd>&#35834; <pr>nu&ograve;</pr></trd>,
    <trd>&#35582;&#35328; <pr>nu&ograve;y&aacute;n</pr></trd>,
    <trd>&#35834;&#35328; <pr>nu&ograve;y&aacute;n</pr></trd>,
    <trd>&#20801;&#35582; <pr>y&#468;nnu&ograve;</pr></trd>,
    <trd>&#20801;&#35834; <pr>y&#468;nnu&ograve;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="promes.0a">
  <kap>
    <tld/>a,
    <var><kap>sukces<tld/>a</kap></var>
  </kap>
  <snc>
    <dif>
      <tld lit="P"/>anta sukcesi:
      <ekz>
        ekzistas diversaj pli a&ubreve; malpli sukces<tld/>aj trukoj solvi la problemon
        <fnt>
	<aut>Pejno Simono</aut>
	<vrk><url ref="&Monato;/2000/006813.php">Plansprache und Phraseologie</url></vrk>
	<bib>Monato</bib>
	<lok>2000/06, p. 33</lok>
        </fnt>;
      </ekz>
      <ekz>
        plej <tld/>a &scirc;anco por eviti homfarotajn konfliktojn estas do profilaktiko
        <fnt>
       <aut>Werner Schad</aut>
          <vrk><url ref="&Monato;/2000/006953.php">Bezonata: nova &leftquot;fremdula
          legio&#8221;</url></vrk>
          <bib>Monato</bib>
          <lok>2000/07, p. 27</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la kverkoj, diris kun <tld/>a koketeco la knabino
        <fnt>
          <bib>KKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni nepre demandas sin, kial iuj aliaj <klr>[&#8230;]</klr>, kiuj havis pli <tld/>an
        komencan kapitalon, kontra&ubreve;e &gcirc;isfunde enkotigis sin mem
        <fnt>
          <bib>Monato</bib>
          <aut>Petro De Smedt</aut>
          <vrk><url ref="&Monato;/2011/010296.php">Socia dokumento pri nia larmovalo</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_ja;&c_c;&c_a;&c_ju;&c_ch;&c_y;</trd>,
    <trd>&c_p;&c_e;&c_r;&c_s;&c_p;&c_e;&c_k;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">erfolgversprechend</trd>
  <trd lng="en">promising</trd>
  <trdgrp lng="id">
    <trd><ind>yang</ind> menjanjikan</trd>,
    <trd><ind>yang</ind> memberi harapan</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>obiecany</trd>,
    <trd>obiecuj&aogonek;cy sukces</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20805;&#28385;&#24076;&#26395; <pr>ch&omacron;ngm&#462;nx&imacron;w&agrave;ng</pr></trd>,
    <trd>&#20805;&#28415;&#24076;&#26395; <pr>ch&omacron;ngm&#462;nx&imacron;w&agrave;ng</pr></trd>,
    <trd>&#21487;&#20197;&#38928;&#35211; <pr>k&ecaron;y&#464;y&ugrave;ji&agrave;n</pr></trd>,
    <trd>&#21487;&#20197;&#39044;&#35265; <pr>k&ecaron;y&#464;y&ugrave;ji&agrave;n</pr></trd>,
    <trd>&#26377;&#26395; <pr>y&#466;uw&agrave;ng</pr></trd>,
    <trd>&#26377;&#28507;&#21147; <pr>y&#466;uqi&aacute;nl&igrave;</pr></trd>,
    <trd>&#26377;&#28508;&#21147; <pr>y&#466;uqi&aacute;nl&igrave;</pr></trd>,
    <trd>&#26377;&#20026; <pr>y&#466;uw&eacute;i</pr></trd>,
    <trd>&#26377;&#28858; <pr>y&#466;uw&eacute;i</pr></trd>,
    <trd>&#36828;&#22823; <pr>yu&#462;nd&agrave;</pr></trd>,
    <trd>&#36960;&#22823; <pr>yu&#462;nd&agrave;</pr></trd>,
    <trd>&#21487;&#38928;&#35211; <pr>k&ecaron;y&ugrave;ji&agrave;n</pr></trd>,
    <trd>&#21487;&#39044;&#35265; <pr>k&ecaron;y&ugrave;ji&agrave;n</pr></trd>,
    <trd>&#26377;&#25104; <pr>y&#466;uch&eacute;ng</pr></trd>,
    <trd>&#26377;&#25104;&#21151;&#24076;&#26395; <pr>y&#466;uch&eacute;ngg&omacron;ngx&imacron;w&agrave;ng</pr></trd>
