<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: litrum1.xml,v 1.14 2024/03/19 09:23:18 revo Exp $">
<kap>
  <rad>litrumac</rad>/oj
</kap>
<drv mrk="litrum1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_familioj" cel="famili.0o.BIO">Familio</ref>
      de herboj unujaraj a&ubreve; sta&ubreve;doj, kreskantaj ofte &ccirc;e akvo,
      el la ordo de mirtoparencaj
      <ref tip="lst" lst="voko:mirtoparencoj" cel="flor.0oplant">floroplantoj</ref>
      (<trd lng="la">Lythraceae</trd>) kun:
      folioj ofte kontra&ubreve;aj a&ubreve; malofte rozete a&ubreve; spirale, glatrandaj;
      floroj stelaj;
      virinaj floroj kun fundo kalikforma a&ubreve; tubforma;
      stamenoj duoble multaj kiel petaloj;
      ofte du ovolujoj; kapsuloj.
      <ekz>
        lastatempaj filogenezaj studoj montris ke la puniko apartenas al
        la familio de <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=201933">Puniko</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
     <ref cel="litrum.0o">litrumo</ref>
     <ref cel="granat.0arbo">granatarbo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_l;&c_mol;&c_ch;&c_a;&c_k;&c_o;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>,
    <trd>&c_p;&c_l;&c_a;&c_k;&c_u;&c_n;&c_o;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  </trdgrp>
  <trd lng="bg">&c_l;&c_je;&c_n;&c_i;&c_v;&c_ch;&c_je;&c_t;&c_a;</trd>
  <trd lng="ca">litr&agrave;cies</trd>
  <trd lng="de">Weiderichgew&auml;chse</trd>
  <trd lng="en">loosestrifes</trd>
  <trd lng="fr">Lythrac&eacute;es</trd>
  <trd lng="hu">f&uuml;z&eacute;nyf&eacute;l&eacute;k</trd>
  <trd lng="nl">kattenstaartfamilie</trd>
  <trd lng="pl">krwawnicowate</trd>
  <trd lng="pt">litr&aacute;ceas</trd>
  <trd lng="ro">lythraceae</trd>
  <trd lng="ru">&c_d;&c_je;&c_r;&c_b;&c_je;&c_n;&c_n;&c_i;&c_k;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&#x43F;&#x43B;&c_a;&#x43A;&c_u;&#x43D;&#x43E;&c_v;&c_ib;</trd>
</drv>
</art>
