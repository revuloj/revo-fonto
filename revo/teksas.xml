    </dif>
  </snc>
  <trd lng="cs">Texas</trd>
  <trd lng="en">Texas</trd>
  <trd lng="fr">Texas</trd>
  <trd lng="ja">&#x30C6;&#x30AD;&#x30B5;&#x30B9;&#x5DDE;  <pr>&#12390;&#x304D;&#12373;&#12377;&#12375;&#12421;&#12358;</pr></trd>
  <trd lng="pl">Teksas</trd>
  <trd lng="sk">Texas</trd>
  <trd lng="uk">&c_T;&c_je;&c_h;&c_a;&c_s; (&c_sh;&c_t;&c_a;&c_t;)</trd>
</drv>
