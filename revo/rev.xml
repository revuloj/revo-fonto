<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rev</rad>/i
</kap>
<drv mrk="rev.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc>
    <gra><vspec>x</vspec></gra>
    <dif>
      Imagi tion, kion oni forte deziras:
      <ekz>
        bona kaporalo <tld/>as esti generalo
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        tian sukceson ni ne esperis, e&ccirc; ne <tld/>is;
      </ekz>
      <ekz>
        de lia juneco li <tld/>is pri la solvado de tiu problemo;
      </ekz>
      <ekz>
        plekti <tld/>on;
      </ekz>
      <ekz>
        fine efektivi&gcirc;is mia <tld/>o;
      </ekz>
      <ekz>
        mia <tld/>o eble estas utopia<fnt>Z</fnt>;
      </ekz>
      <ekz>
        hodia&ubreve;a <tld/>o povas fari&gcirc;i morga&ubreve;a realo;
      </ekz>
      <ekz>
        &ccirc;io apartenas ankora&ubreve; al la regno de <tld/>oj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li sidis dronanta en <tld/>oj<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>antaj okuloj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a rigardo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kastel.0o">kastelo</ref>,
      <ref cel="aer.0o">aero</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_y;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>tr&auml;umen</trd>,
    <trd>schw&auml;rmen</trd>
  </trdgrp>
  <trd lng="en">fancy</trd>
  <trdgrp lng="nl">
    <trd>dag<ind>dromen</ind></trd>,
    <trd>mijmeren</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_je;&c_ch;&c_t;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="rev.dis0igxoel0igxo">
  <kap>dis<tld/>i&gcirc;o, el<tld/>i&gcirc;o</kap>
  <snc>
    <dif>
      Seniluzii&gcirc;o.
    </dif>
    <refgrp tip="vid">
      <ref cel="fantom.0o">fantomo</ref>,
      <ref cel="vent.0o">vento</ref>,
      <ref cel="songx.0o">son&gcirc;o</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_ch;&c_a;&c_r;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="nl">
    <trd>ontgoocheling</trd>,
    <trd>teleurstelling</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_o;&c_ch;&c_a;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
</drv>
</art>
