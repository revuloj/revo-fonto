<drv mrk="rev.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rev.pri0i.KOMUNE">
    <dif>
      <tld lit="R"/>i pri..., imagi, mense bildigi:
      <ekz>
        trinku iom da brando anta&ubreve; ol dormi kaj pri<tld/>i 
        feli&ccirc;a&jcirc;ojn
        <fnt>
          <aut>M. Ahado</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/fajro38/422968">
          Ebrii&gcirc;u</url></vrk>,
          <lok>2012-10-18</lok>
        </fnt>;
      </ekz>
      <ekz>
        nur desegna&jcirc;oj de la pri<tld/>ataj varoj
        <fnt>
          <bib>Monato</bib>
          <aut>Ulrich Lins</aut>
          <vrk><url ref="https://www.monato.be/2005/008536.php"
              >Nokta aventuro en Pjongjango</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        la plano tamen apena&ubreve; mencias la vortojn &leftquot;sendependeco&rightquot; kaj &leftquot;suvereneco&rightquot;, kiujn
        la albanoj en Kosovo longe pri<tld/>as kaj atendas
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="https://www.monato.be/2007/009203.php"
              >Duonkore donata</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj homoj pri<tld/>as simplan laboron kun bona salajro kaj kiel eble plej
        malmultaj respondecoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.be/2015/011413.php"
              >Ulo dormanta salajron ja kaptas</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">ertr&auml;umen</trd>
  <trd lng="fr">r&ecirc;ver de</trd>
</drv>
