<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rev</rad>/i
</kap>
<drv mrk="rev.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc mrk="rev.0i.PSI">
    <gra><vspec>x</vspec></gra>
    <dif>
      Imagi tion, kion oni forte deziras:
      <ekz>
        bona kaporalo <tld/>as esti generalo
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        tian sukceson ni ne esperis, e&ccirc; ne <tld/>is;
      </ekz>
      <ekz>
        de lia juneco li <tld/>is pri la solvado de tiu problemo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kastel.0o">kastelo</ref>,
      <ref cel="aer.0o">aero</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_y;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>tr&auml;umen</trd>,
    <trd>schw&auml;rmen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>dream</trd>,
    <trd>daydream</trd>,
    <trd>muse</trd>,
    <trd>be in a reverie</trd>
  </trdgrp>
  <trd lng="fr">r&ecirc;ver de<klr tip="amb"> (fortement
  d&eacute;sirer)</klr></trd>
  <trdgrp lng="hu">
    <trd>&aacute;br&aacute;ndozik</trd>,
    <trd>&aacute;lmodozik</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>dag<ind>dromen</ind></trd>,
    <trd>mijmeren</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>sonhar</trd>,
    <trd>devanear</trd>,
    <trd>fantasiar</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_je;&c_ch;&c_t;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="rev.0a">
  <kap><tld/>a</kap>
  <snc mrk="rev.0a.PSI">
    <dif>
      Absorbata en dol&ccirc;a pensado:
      <ekz>
        <tld/>a rigardo
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>dreamy</trd>,
    <trd>lost in thought</trd>,
    <trd>in a reverie</trd>
  </trdgrp>
  <trd lng="fr">r&ecirc;veur<klr tip="amb"> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>&aacute;br&aacute;ndos</trd>,
    <trd>&aacute;lmodoz&oacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_je;&c_ch;&c_t;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="rev.0o">
  <kap><tld/>o</kap>
  <snc mrk="rev.0o.PSI">
    <dif>
      Imago de feli&ccirc;igaj, dezirataj aferoj:
      <ekz>
        plekti <tld/>on;
      </ekz>
      <ekz>
        fine efektivi&gcirc;is mia <tld/>o;
      </ekz>
      <ekz>
        mia <tld/>o eble estas utopia<fnt>Z</fnt>;
      </ekz>
      <ekz>
        hodia&ubreve;a <tld/>o povas fari&gcirc;i morga&ubreve;a realo;
      </ekz>
      <ekz>
        &ccirc;io apartenas ankora&ubreve; al la regno de <tld/>oj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li sidis dronanta en <tld/>oj<fnt>B</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="utopi.0o">utopio</ref>
    </refgrp>
  </snc>
  <trd lng="de">Traum</trd>
  <trdgrp lng="en">
    <trd>dream</trd>,
    <trd>daydream</trd>,
    <trd>reverie</trd>
  </trdgrp>
  <trd lng="fr">r&ecirc;verie</trd>
  <trdgrp lng="hu">
    <trd>&aacute;br&aacute;nd</trd>,
    <trd>&aacute;lom <klr tip="ind">(&aacute;br&aacute;nd)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_je;&c_ch;&c_t;&c_a;</trd>
</drv>

<drv mrk="rev.0iga">
  <kap><tld/>iga</kap>
  <snc mrk="rev.0iga.PSI">
    <dif>
      Favoranta dol&ccirc;an, <tld/>an animstaton:
      <ekz>
        la <tld/>iga trankvileco de la pa&scirc;tejoj
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 1a, renkonto</lok></fnt>..
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>&eacute;vocateur<klr tip="amb"> (qui fait r&ecirc;ver)</klr></trd>,
    <trd>suggestif<klr tip="amb"> (qui fait r&ecirc;ver)</klr></trd>
  </trdgrp>
  <trd lng="hu">andal&iacute;t&oacute;</trd>
</drv>

<drv mrk="rev.dis0igxo">
  <kap>dis<tld/>i&gcirc;o</kap>
  <snc mrk="rev.dis0igxo.PSI">
    <ref tip="dif" cel="rev.el0igxo.PSI">el<tld/>i&gcirc;o</ref>
    <ekz>
      dek ok jarojn, plenigitajn de obstina laborado de la kapo,
      malsukcesoj, dis<tld/>i&gcirc;oj
      <fnt><bib>FK</bib>, <lok>4, rakontoj, nokto, (de Eube el
      Odeso)</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_ch;&c_a;&c_r;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="en">
    <trd>disillusionment</trd>,
    <trd>disappointment</trd>,
    <trd>disenchantment</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;ception</trd>,
    <trd>d&eacute;sillusion</trd>
  </trdgrp>
  <trd lng="hu">ki&aacute;br&aacute;ndul&aacute;s</trd>
  <trdgrp lng="nl">
    <trd>ontgoocheling</trd>,
    <trd>teleurstelling</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_o;&c_ch;&c_a;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="rev.el0igi">
  <kap>el<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rev.el0igi.PSI">
    <dif>
      Sobrige remeti anta&ubreve; la realon kaj senigi je vanaj esperoj.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>disillusion</trd>,
    <trd>disappoint</trd>,
    <trd>disenchant</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;tromper</trd>,
    <trd>d&eacute;sillusionner</trd>
  </trdgrp>
  <trd lng="hu">ki&aacute;br&aacute;nd&iacute;t</trd>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_o;&c_ch;&c_a;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="rev.el0igxi">
  <kap>el<tld/>i&gcirc;i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rev.el0igxi.PSI">
    <dif>
      Seni&gcirc;i je agrablaj, eble trompaj imagoj.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>become disillusioned</trd>,
    <trd>become disappointed</trd>,
    <trd>become disenchanted</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>d&eacute;tromper</ind></trd>,
    <trd>perdre ses illusions</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ki&aacute;br&aacute;ndul</trd>,
    <trd>csal&oacute;dik</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_o;&c_ch;&c_a;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="rev.el0igxo">
  <kap>el<tld/>i&gcirc;o</kap>
  <snc mrk="rev.el0igxo.PSI">
    <dif>
      Dolora konstato, ke <tld/>o ne reali&gcirc;as, ne okazos.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_ch;&c_a;&c_r;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="en">
    <trd>disillusionment</trd>,
    <trd>disappointment</trd>,
    <trd>disenchantment</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;ception</trd>,
    <trd>d&eacute;sillusion</trd>
  </trdgrp>
  <trd lng="hu">ki&aacute;br&aacute;ndul&aacute;s</trd>
  <trdgrp lng="nl">
    <trd>ontgoocheling</trd>,
    <trd>teleurstelling</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_o;&c_ch;&c_a;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="rev.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rev.for0i.PSI">
    <dif>
      Revante forgesi zorgon:
      <ekz>
        la almozulo <klr>...</klr> refre&scirc;igas sin per sukplena
        akvomelono
        kaj for<tld/>as sian vivon
        <fnt><bib>Fab4</bib>, <lok>historio el la dunoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">dream away</trd>
  <trd lng="fr">oublier<klr tip="amb"> (en r&ecirc;vant d'autre
  chose)</klr></trd>
  <trdgrp lng="hu">
    <trd>elfelejt <klr tip="ind">(bajt)</klr></trd>,
    <trd>megfeledkezik <klr tip="ind">(bajr&oacute;l)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="rev.sen0igxo">
  <kap>sen<tld/>i&gcirc;o</kap>
  <snc mrk="rev.sen0igxo.PSI">
    <ref tip="dif" cel="rev.el0igxo.PSI">el<tld/>i&gcirc;o</ref>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_ch;&c_a;&c_r;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="en">
    <trd>disillusionment</trd>,
    <trd>disappointment</trd>,
    <trd>disenchantment</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;ception</trd>,
    <trd>d&eacute;sillusion</trd>
  </trdgrp>
  <trd lng="hu">ki&aacute;br&aacute;ndul&aacute;s</trd>
  <trdgrp lng="nl">
    <trd>ontgoocheling</trd>,
    <trd>teleurstelling</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_o;&c_ch;&c_a;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
</drv>
</art>
