<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>monar&hcirc;</rad>/o, monark/o
    </kap>
    <drv mrk="monarh.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <uzo tip="fak">JUR</uzo>
      <uzo tip="fak">POL</uzo>
      <snc>
        <dif>
          Regnestro en <ref tip="malprt" cel="monarh1">monarkio</ref>:
          emiro, &hcirc;alifo, &hcirc;ano, imperiestro, princo, re&gcirc;o, sultano ktp.
        </dif>
      </snc>
      <trdgrp lng="nl">
    <trd>vorst</trd>,
    <trd>monarch</trd>
  </trdgrp>
<trd lng="ru">&c_m;&c_o;&c_n;&c_a;&c_r;&c_h;</trd>
    </drv>
    <drv mrk="monarh.0ismo">
      <kap><tld/>ismo</kap>
      <uzo tip="fak">POL</uzo>
      <snc>
        <dif>
          Monarkiismo, politika doktrino de partianoj de monarkia
          re&gcirc;imo.
        </dif>
      </snc>
      <trd lng="nl">monarchisme</trd>
   <trd lng="ru">&c_m;&c_o;&c_n;&c_a;&c_r;&c_h;&c_i;&c_z;&c_m;</trd>
    </drv>
    <drv mrk="monarh.0isto">
      <kap><tld/>isto</kap>
      <uzo tip="fak">POL</uzo>
      <snc>
        <dif>
          Partiano de monarkia re&gcirc;imo.
        </dif>
      </snc>
      <trd lng="nl">monarchist</trd>
    <trd lng="ru">&c_m;&c_o;&c_n;&c_a;&c_r;&c_h;&c_i;&c_s;&c_t;</trd>
    </drv>
  </art>
<!--
