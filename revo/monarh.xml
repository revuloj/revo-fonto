<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: monarh.xml,v 1.25 2022/05/28 12:48:12 revo Exp $">
<kap>
  <ofc>*</ofc> <rad>monar&hcirc;</rad>/o, 
  <var><kap><ofc>8</ofc><rad var="k">monark</rad>/o <fnt><bib>PV</bib></fnt></kap></var>
</kap>

<drv mrk="monarh.0o">
  <kap><ofc>*</ofc><tld/>o,
  <var><kap><ofc>8</ofc><tld var="k"/>o</kap></var>
  </kap> 
  <uzo tip="fak">JUR</uzo>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="regn.0estro">Regnestro</ref> en <ref tip="malprt" cel="monarh1">monarkio</ref>:
      <refgrp tip="sub">
        <ref cel="emir.0o">emiro</ref>, <ref cel="faraon.0o">faraono</ref>,
        <ref cel="kalif.0o">&hcirc;alifo</ref>, <ref cel="hxan.0o">&hcirc;ano</ref>, 
        <ref cel="imperi.0estro">imperiestro</ref>, <ref cel="princ.0o.regnestro">princo</ref>,
        <ref cel="regx.0o.monarko">re&gcirc;o</ref>, <ref cel="sultan.0o">sultano</ref> 
      </refgrp> ktp.:
      <ekz>
        la granda <tld/>o tre favore adia&ubreve;is sian filon kaj heredonton
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la filo de potenca <tld/>o
        <fnt>
          <aut>Wilhelm Hauff, trad. J. W. Eggleton</aut>
          <vrk><url ref="&Gutenberg;ebooks/22070"> La
          Karavano</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        fine li estis perdinta tion, kio sufi&ccirc;us por ri&ccirc;igi dudek <tld var="k"/>ojn
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        tronparoladoj de <tld var="k"/>oj en konstituciaj monarkioj
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj nordirlandaj parlamentanoj kaj publikaj funkciuloj &jcirc;uru fidelecon al brita
        <tld/>o
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006826.html"
              >Irlando: &Ccirc;u pa&scirc;oj al paco?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la imperiestro <nom>Hongwu</nom>, la unua de la dinastio <nom>Ming</nom>, en la 14a jarcento
        <klr>[&#8230;]</klr> dekretis, ke la nomo de la erao restu sama dum la regado de
        &ccirc;iu <tld/>o
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006852.html"
              >Reago: &Ccirc;u vere kristana erao?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="auxtok.0o">a&ubreve;tokrato</ref>,
      <ref cel="despot.0o">despoto</ref>
    </refgrp>
  </snc>                      
  <trd lng="be">&c_m;&c_a;&c_n;&c_a;&c_r;&c_h;</trd>
  <trd lng="bg">&c_m;&c_o;&c_n;&c_a;&c_r;&c_h;</trd>
  <trd lng="br">unpenn</trd>
  <trd lng="ca">monarca</trd>
  <trd lng="de">Monarch</trd>
  <trd lng="en">monarch</trd>
  <trd lng="es">mobarca</trd>
  <trdgrp lng="fr">
    <trd>monarque</trd>,
    <trd>souverain<klr> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>egyeduralkod&oacute;</trd>,
    <trd>monarcha</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vorst</trd>,
    <trd>monarch</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>monarcha</trd>,
    <trd>panuj&aogonek;cy</trd>
  </trdgrp>
  <trd lng="pt">monarca</trd>
  <trdgrp lng="ro">
    <trd>rege</trd>,
    <trd>monarh</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_o;&c_n;&c_a;&c_r;&c_h;</trd>
  <trd lng="uk">&c_m;&c_o;&c_n;&c_a;&c_r;&c_h;</trd>
</drv>

<drv mrk="monarh.0ismo">
  <kap><tld/>ismo, <var><kap>monarkismo</kap></var></kap>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Politika <ref tip="super" cel="doktri.0o">doktrino</ref> de partianoj de monarkia
      re&gcirc;imo; monarkiismo.
    </dif>
    <adm>
      Necesas difini "monarkiismo"!
    </adm>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_a;&c_r;&c_h;&c_ib;&c_z;&c_m;</trd>
  <trd lng="bg">&c_m;&c_o;&c_n;&c_a;&c_r;&c_h;&c_i;&c_ja;</trd>
  <trd lng="br">unpenniezh</trd>
  <trdgrp lng="ca">
    <trd>monarquia</trd>,
    <trd>monarquisme</trd>
  </trdgrp>
  <trd lng="de">Monarchismus</trd>
  <trd lng="en">monarchism</trd>
  <trdgrp lng="fr">
    <trd>monarchisme</trd>
  </trdgrp>
  <trd lng="hu">monarchizmus</trd>
  <trd lng="nl">monarchisme</trd>
  <trd lng="pl">monarchizm</trd>
  <trd lng="ro">regalism</trd>
  <trd lng="ru">&c_m;&c_o;&c_n;&c_a;&c_r;&c_h;&c_i;&c_z;&c_m;</trd>
  <trd lng="uk">&c_m;&c_o;&c_n;&c_a;&c_r;&c_h;&c_ib;&c_z;&c_m;</trd>
</drv>

<drv mrk="monarh.0isto">
  <kap><tld/>isto, <var><kap>monarkisto</kap></var></kap>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Partiano de monar&hcirc;ia re&gcirc;imo:
      <ekz>
        la nuna brita &ccirc;efministro, <nom>Tony Blair</nom>, estas <tld var="k"/>isto, kiu defendas la re&gcirc;inon
        kontra&ubreve; kritikado
        <fnt>
          <bib>Monato</bib>
          <aut>W. H. Simcock</aut>
          <vrk><url ref="https://www.monato.be/2005/008702.php"
              >Monarkia mistiko</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="respub.0isto">respublikisto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_a;&c_r;&c_h;&c_ib;&c_s;&c_t;</trd>
  <trd lng="bg">&c_m;&c_o;&c_n;&c_a;&c_r;&c_h;&c_i;&c_s;&c_t;</trd>
  <trd lng="br">unpennelour</trd>
  <trd lng="ca">mon&agrave;rquic<klr> (subst.)</klr></trd>
  <trd lng="de">Monarchist</trd>
  <trd lng="en">monarchist</trd>
  <trdgrp lng="fr">
    <trd>monarchiste</trd>
  </trdgrp>
  <trd lng="hu">monarchista</trd>
  <trd lng="nl">monarchist</trd>
  <trd lng="pl">monarchista</trd>
  <trd lng="ro">monarhist</trd>
  <trd lng="ru">&c_m;&c_o;&c_n;&c_a;&c_r;&c_h;&c_i;&c_s;&c_t;</trd>
  <trd lng="uk">&c_m;&c_o;&c_n;&c_a;&c_r;&c_h;&c_ib;&c_s;&c_t;</trd>
</drv>
</art>
