      <trd>&c_p;&c_e;&c_r;&c_s;&c_a;&c_n;&c_a;&c_zh;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Person</trd>,
      <trd>Rolle</trd>,
      <trd>Figur</trd>
    </trdgrp>
    <trd lng="en">character</trd>
    <trdgrp lng="id">
      <trd>figur</trd>,
      <trd>karakter</trd>,
      <trd>peran</trd>,
      <trd>tokoh<klr> (dlm film)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_je;&c_r;&c_s;&c_o;&c_n;&c_a;&c_zh;</trd>
  </snc>
  <snc mrk="person.0o.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      <ref tip="sin" cel="hipost.0o.KRI">Hipostazo</ref>:
      <ekz>
        unu sola Dio en tri <tld/>oj;
      </ekz>
      <ekz>
        en Kristo kunekzistas du <tld/>oj distingitaj, unu homa alia dia
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=376224">Hipostazo (filozofio)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la Sankta Triunuo ja estas unu dio, kvankam tri <tld/>oj apartenas al &gcirc;i
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=816">Dio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_ib;&c_p;&c_a;&c_s;&c_t;&c_a;&c_s;&c_mol;</trd>
    <trdgrp lng="de">
      <trd>Hypostase</trd>,
