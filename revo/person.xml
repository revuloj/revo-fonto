  <snc mrk="person.0o.rolanto">
    <dif>
      Imagita rolanto de rakonto, romano, teatra&jcirc;o, filmo
      a&ubreve; alia verko fikcia:
      <ekz>
        <tld/>oj:
        <nom>Anton Antonovi&ccirc; Skvoznik-Dmu&hcirc;anonskij</nom>, urbestro,
        <nom>Anna Andrejevna</nom>, lia edzino <klr>[&#8230;]</klr>
        <fnt><bib>Revizoro</bib></fnt>;
      </ekz>
      <ekz>
        mia unua heroo estis Brun&ccirc;jo Urso,
        <ind>&ccirc;ef<tld/>o</ind>
        de ilustrita felietono por infanoj, kiu aperas &ccirc;iuvespere
        en la gazeto
        <fnt>
        <aut>W. F. Hermans</aut>,
        <vrk><url ref="http://mihxil.komputilo.org/8/wimshurst.html">La elektrilo de
        Wimshurst</url></vrk>
        </fnt>;
        <trd lng="be">&c_g;&c_a;&c_l;&c_o;&c_w;&c_n;&c_y; <ind>&c_g;&c_je;&c_r;&c_o;&c_j;</ind></trd>
        <trdgrp lng="pl">
          <trd>osoba</trd>,
          <trd>bohater <klr> (np. ksi&aogonek;&zdot;ki)</klr></trd>
        </trdgrp>
        <trd lng="pt">personagem</trd>
        <trd lng="ro">persoana</trd>
        <trd lng="ru">&c_g;&c_l;&c_a;&c_v;&c_n;&c_y;&c_j; &c_g;&c_je;&c_r;&c_o;&c_j;</trd>
      </ekz>
      <ekz>
        roman<tld/>o.
      </ekz>
    </dif>
    <ref tip="sin" cel="rol.0ulo">rolulo</ref>
    <refgrp tip="vid">
      <ref cel="figur.0o.aspekto">figuro<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
