        <trd lng="en">signalement</trd>
        <trdgrp lng="fr">
          <trd>caract&eacute;ristique personnelle</trd>,
          <trd>signe distinctif</trd>
        </trdgrp>
        <trd lng="pl">dane osobowe</trd>
