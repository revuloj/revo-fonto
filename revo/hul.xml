<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hul.xml,v 1.17 2022/03/20 17:24:56 revo Exp $">
<kap>
  <rad>hul</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="hul.0o">
  <kap><tld/>o</kap>
  <snc mrk="hul.0o.KOMUNE">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Mara <ref tip="super" cel="ond.0o">ondo</ref>movo, esti&gcirc;anta sen &scirc;a&ubreve;mrompi&gcirc;o:
      <ekz>
        mirinda loko estas la erikejo, &mdash; li diris &ccirc;irka&ubreve;rigardante al la
        ondanta montetaro, longaj verdaj <tld/>oj kun krestoj el zigzaga
        granito supren&scirc;a&ubreve;mantaj je fantaziaj sputoj
        <fnt>
          <aut>A. Conan Doyle trad. W. Auld</aut>,
          <vrk>La &ccirc;ashundo de la Baskerviloj</vrk>,
          <lok>p. 69</lok>
        </fnt>;
      </ekz>
      <ekz>
        popolamaso mu&gcirc;anta kiel <tld/>o
        <fnt>
          <vrk><url ref="https://tekstaro.com/t?nomo=mondediplo-2002">Malproksime de Karakaso, la atingoj de la agrarreformo</url></vrk>
          <lok>Le Monde diplomatique en Esperanto 2002-2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">D&uuml;nung</trd>
    <trd lng="en">swell</trd>
    <trd lng="fr">houle</trd>
    <trd lng="ru">&c_m;&c_jo;&c_r;&c_t;&c_v;&c_a;&c_ja; &c_z;&c_y;&c_b;&c_mol;</trd>
  </snc>
  <snc mrk="hul.0o.MAR">
    <uzo tip="fak">SHI</uzo><uzo tip="fak">AVI</uzo>
    <dif>
      La &ccirc;efa strukturo kaj floskorpo de <ref tip="malprt" cel="sxip.0o">&scirc;ipo</ref> 
      a&ubreve; de <ref tip="malprt" cel="hidrop.0o">hidroplano</ref>.
    </dif>
    <refgrp tip="vid">
      <ref cel="fuzela.0o">fuzela&gcirc;o</ref>,
      <ref cel="sxel.0o">&scirc;elo</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_k;&c_o;&c_r;&c_p;&c_u;&c_s; <klr tip="amb">(&c_k;&c_a;&c_r;&c_a;&c_b;&c_l;&c_ja;,
      &c_g;&c_ib;&c_d;&c_r;&c_a;&c_p;&c_l;&c_ja;&c_n;&c_a;)</klr></trd>,
      <trd>&c_f;&c_ju;&c_z;&c_e;&c_l;&c_ja;&c_zh;</trd>
    </trdgrp>
    <trdgrp lng="cs">
      <trd>vln&ecaron;n&iacute;</trd>,
      <trd>&ccaron;e&rcaron;en&iacute;</trd>
    </trdgrp>
    <trd lng="de">Schiffsk&ouml;rper</trd>
    <trd lng="en">hull</trd>
    <trd lng="fr">coque<klr tip="ind"> (d'un navire, d'un
    hydravion)</klr></trd>
    <trd lng="it">scafo</trd>
    <trdgrp lng="nl">
      <trd>romp<klr tip="ind"> (v.e.schip)</klr></trd>,
      <trd>scheepsromp</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>casco</trd>,
      <trd>quilha</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_r;&c_p;&c_u;&c_s;</trd>,
      <trd>&c_f;&c_ju;&c_z;&c_je;&c_l;&c_ja;&c_zh;</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>gondola lietadla</trd>,
      <trd>trup</trd>,
      <trd>&ccaron;lnku</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="uk">
    <trd>&#x431;&#x440;&#x438;&#x436;&#x456;</trd>,
    <trd>&#x445;&#x432;&#x438;&#x43B;&#x44E;&#x432;&#x430;&#x43D;&#x43D;&#x44F; (&#x43D;&#x430; &#x43C;&#x43E;&#x440;&#x456;)</trd>,
    <trd>&#x43A;&#x43E;&#x440;&#x43F;&#x443;&#x441;</trd>,
    <trd>&#x444;&#x44E;&#x437;&#x435;&#x43B;&#x44F;&#x436;</trd>
  </trdgrp>
</drv>
</art>
