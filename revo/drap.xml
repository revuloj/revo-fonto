<art mrk="$Id: drap.xml,v 1.14 2015/07/04 08:37:32 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>drap</rad>/o
</kap>
<drv mrk="drap.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Teksa&jcirc;o el lano uzata por vestoj:
      <ekz>kia <tld/>o, tia vesto<fnt><bib>PrV</bib></fnt>.</ekz>  
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>drap</trd>,
    <trd>drapé</trd>,
    <trd>sukno</trd>
  </trdgrp>
  <trd lng="de">Tuch</trd>
  <trd lng="en">wool cloth</trd>
  <trd lng="fa">&a_m;&a_A;&a_h;&a_w;&a_t;</trd>
  <trd lng="fr">drap</trd>
  <trdgrp lng="pl">
    <trd>materia&lstroke;<klr> (we&lstroke;niany)</klr></trd>,
    <trd>sukno</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>pano</trd>,
    <trd>tecido de l&atilde;</trd>,
    <trd>fazenda <klr tip="amb">(para vestu&aacute;rio)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_r;&c_a;&c_p;</trd>,
    <trd>&c_s;&c_u;&c_k;&c_n;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">súkno</trd>
</drv>
</art>
