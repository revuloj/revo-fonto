<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: drap.xml,v 1.19 2024/02/14 09:23:36 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>drap</rad>/o
</kap>
<drv mrk="drap.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc mrk="drap.0o.TEKS">
    <dif>
      <ref tip="super" cel="teks.0ajxo.TEKS">Teksa&jcirc;o</ref> el <ref cel="lan.0o">lano</ref> uzata por
      vestoj k.a.:
      <ekz>
        nefulita <tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 9:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        alportu al mi metron da nigra <tld/>o
        <fnt>
          <bib>F</bib>
          <lok>&FE; 32</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>randa&jcirc;oj &ccirc;irka&ubreve; la pordo gardis kontra&ubreve; &ccirc;ia trablovo
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova strata lanterno</lok>
        </fnt>;
      </ekz>
      <ekz>
        granda tablo, kovrita per verda <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        en &ccirc;iuj salonoj <klr>[&#8230;]</klr> estis sternitaj <tld/>aj kovra&jcirc;oj, por ke oni ne
        a&ubreve;du la pa&scirc;ojn
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>itaj per ru&gcirc;a &scirc;tofo koridoroj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i estis vestita per blanka, tre malpeza, preska&ubreve; diafana <tld/>a&jcirc;o
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        invalido en griza palto el soldata <tld/>o
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        kia <tld/>o, tia vesto<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_a;&c_p; <klr>(&c_s;&c_u;&c_k;&c_n;&c_o;)</klr></trd>
  <trdgrp lng="cs">
    <trd>drap</trd>,
    <trd>drap&eacute;</trd>,
    <trd>sukno</trd>
  </trdgrp>
  <trd lng="de">Tuch <klr>(Wollgewebe)</klr></trd>
  <trd lng="en">wool cloth</trd>
  <trd lng="fa">&a_m;&a_A;&a_h;&a_w;&a_t;</trd>
  <trd lng="fr">drap</trd>
  <trd lng="ja">&#x30E9;&#x30B7;&#x30E3;</trd>
  <trdgrp lng="pl">
    <trd>materia&lstroke;<klr> (we&lstroke;niany)</klr></trd>,
    <trd>sukno</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>pano</trd>,
    <trd>tecido de l&atilde;</trd>,
    <trd>fazenda <klr tip="amb">(para vestu&aacute;rio)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_r;&c_a;&c_p;</trd>,
    <trd>&c_s;&c_u;&c_k;&c_n;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">s&uacute;kno</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_u;&c_k;&c_n;&c_o;</trd>,
    <trd>&c_d;&c_r;&c_a;&c_p;</trd>
  </trdgrp>
  <trd lng="zh">&#x624B;&#x5E15; <pr>sh&#x1D2;up&#xE0;</pr></trd>
</drv>
</art>
