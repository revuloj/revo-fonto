<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vapor</rad>/o
</kap>
<drv mrk="vapor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Gasa nubo supreni&gcirc;anta el varmi&gcirc;anta fluida&jcirc;o:
      <ekz>
        el la kaldrono <klr>...</klr> eliras <tld/>o
        <fnt><bib>F</bib>, <lok>&FE; 25</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>o de akvo, etero;
      </ekz>
      <ekz>
        malaperi kiel <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kuiriston <tld/>o satigas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>havi iom da <tld/>o en la kapo</ind>
        <klr>(esti ebrieta)</klr>
        <trd lng="fr">&ecirc;tre <ind>gris</ind></trd>
        <trd lng="nl">beschonken zijn</trd>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        densa nebulo lacigis la rigardon per siaj molaj
        <tld/>a&jcirc;oj<fnt>B</fnt>;
      </ekz>
      <ekz>
        la &scirc;ipo foriris kun plen<tld/>o
        <klr>(plej rapide)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;</trd>  
  <trdgrp lng="br">
    <trd>aezhenn</trd>,
    <trd>burezh</trd>
  </trdgrp>
  <trd lng="de">Dampf</trd>
  <trd lng="es">vapor</trd>
  <trd lng="fr">vapeur</trd>
  <trd lng="hu">g&odblac;z</trd>
  <trdgrp lng="nl">
    <trd>stoom</trd>,
    <trd>damp</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>para</trd>,
    <trd>opar<klr> (w przypadku wody w terenia, np. jezior)</klr></trd>
  </trdgrp>
  <trd lng="pt">vapor</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;</trd>
  <trd lng="sk">para</trd>
</drv>

<drv mrk="vapor.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Eliganta <tld/>on.
    </dif>                         
    <trd lng="br">aezhennus</trd>
    <trd lng="de">dampfend</trd>
    <trd lng="fr">fumant<klr> (produisant de la vapeur)</klr></trd>
    <trd lng="hu">g&odblac;z-</trd>
    <trdgrp lng="nl">
      <trd>dampend</trd>,
      <trd>stomend</trd>
    </trdgrp>
    <trd lng="pl">paruj&aogonek;cy</trd>
    <trd lng="pt">vapor&iacute;fero</trd>
  </snc>
  <snc>
    <dif>
      Aspekte simila al <tld/>o:
      <ekz>
        <tld/>a nebulo, tulo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vat.0i">vati</ref>
    </refgrp>                    
    <trd lng="br">aezhennel</trd> 
    <trdgrp lng="de">
      <trd>nebelig</trd>
      <trd>gasf&ouml;ig</trd>
    </trdgrp>
    <trd lng="fr">vaporeux</trd>
    <trdgrp lng="hu">
      <trd>g&odblac;z&ouml;s</trd>,
      <trd>p&aacute;r&aacute;s</trd>
    </trdgrp>
    <trd lng="nl">nevelig</trd>
    <trd lng="pt">vaporoso</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="es">vaporoso</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_r;&c_o;&c_v;&c_o;&c_j;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="vapor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Eligi <tld/>on:
      <ekz>
        plej bonguste <tld/>is rostita ansero, far&ccirc;ita per prunoj kaj
pomoj
        <fnt><bib>Fab2</bib>, <lok>knabineto kun alumetoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_p;&c_u;&c_s;&c_k;&c_a;&c_c;&c_mol;
<ind>&c_p;&c_a;&c_r;&c_u;</ind></trd>     
  <trd lng="br">divogedi&ntilde;<klr tip="amb"> (en ur zisteurel
aezhenn)</klr></trd>
  <trd lng="de">dampfen</trd>
  <trd lng="fr">fumer<klr tip="amb"> (faire de la vapeur)</klr></trd>
  <trd lng="hu">g&odblac;z&ouml;l&ouml;g</trd>
  <trdgrp lng="nl">
    <trd>dampen</trd>,
    <trd>stomen</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_s;&c_p;&c_u;&c_s;&c_k;&c_a;&c_t;&c_mol;
&c_p;&c_a;&c_r;</trd>
</drv>

<drv mrk="vapor.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari ion <tld/>o:
      <ekz>
        <tld/>igi akvon en kaldrono.
      </ekz>
    </dif>
    <trd lng="de">abdampfen</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_p;&c_a;&c_r;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_p;&c_a;&c_r;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>                     
  <trd lng="br">aezhenna&ntilde;<klr tip="ind"> (v.k.)</klr></trd>
  <trd lng="es">evaporar</trd>
  <trdgrp lng="fr">
    <trd>brumiser</trd>,
    <trd>&eacute;vaporer</trd>,
    <trd>vaporiser</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>g&odblac;z&ouml;l&ouml;gtet</trd>,
    <trd>p&aacute;rologtat</trd>
  </trdgrp>
  <trd lng="nl">doen <ind>verdampen</ind></trd>
  <trd lng="pl">parowa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_p;&c_a;&c_r;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_s;&c_p;&c_a;&c_r;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_p;&c_a;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="pt">vaporizar</trd>
  <trd lng="sk">vyparova&tcaron;</trd>
</drv>

<drv mrk="vapor.0igilo">
  <kap><tld/>igilo</kap> <fnt><bib>EBV</bib></fnt>
  <snc>
    <dif>
      Aparato por <tld/>igi flua&jcirc;on:
      <ekz>
        <tld/>igilo por parfumoj.
      </ekz>
    </dif>
  </snc>                 
  <trd lng="br">difruer</trd>
  <trdgrp lng="fr">
    <trd>bombe<klr tip="amb"> (vaporisateur)</klr></trd>,
    <trd>brumisateur</trd>,
    <trd>vaporisateur</trd>
  </trdgrp>
  <trd lng="nl">stoommachine</trd>
  <trdgrp lng="pl">
    <trd>parownik</trd>,
    <trd>waporyzator</trd>
  </trdgrp>
  <trd lng="pt">vaporizador</trd>
</drv>

<drv mrk="vapor.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Fari&gcirc;i <tld/>o:
      <ekz>
        la akvo <tld/>i&gcirc;as je la ordinara temperaturo.
      </ekz>
    </dif>
    <trd lng="de">verdampfen</trd>
  </snc>                
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_p;&c_a;&c_r;&c_y;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_p;&c_a;&c_r;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>aezhenna&ntilde;<klr tip="ind"> (v.k.)</klr></trd>,
    <trd>diaezhenna&ntilde;</trd>
  </trdgrp>
  <trd lng="es">evaporarse</trd>
  <trdgrp lng="fr">
    <trd>s'<ind>&eacute;vaporer</ind></trd>
  </trdgrp>
  <trd lng="hu">p&aacute;rolog</trd>
  <trd lng="nl">verdampen</trd>
  <trd lng="pl">parowa&cacute;</trd>
  <trd lng="pt">evaporar</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_p;&c_a;&c_r;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_i;&c_s;&c_p;&c_a;&c_r;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">vyparova&tcaron; sa</trd>
</drv>

<drv mrk="vapor.el0igxi">
  <kap>el<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Malaperi <tld/>i&gcirc;ante:
      <ekz>
        la akvo el la kaserolo el<tld/>i&gcirc;is.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="esting">estingi&gcirc;i</ref>,
      <ref cel="cxerp">el&ccirc;erpi&gcirc;i</ref>
    </refgrp>
    <trd lng="de">verdampfen</trd>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_p;&c_a;&c_r;&c_y;&c_c;&c_c;&c_a;</trd>   
  <trdgrp lng="br">
    <trd>aezhenna&ntilde;<klr tip="ind"> (v.k.)</klr></trd>,
    <trd>diaezhenna&ntilde;</trd>
  </trdgrp>
  <trd lng="es">evaporarse</trd>
  <trdgrp lng="fr">
    <trd>s'<ind>&eacute;vaporer</ind></trd>
  </trdgrp>
  <trd lng="hu">elp&aacute;rolog</trd>
  <trd lng="nl">uitdampen</trd>
  <trd lng="pl">wyparowa&cacute;</trd>
  <trd lng="pt">evaporar</trd>
  <trd lng="ru">&c_i;&c_s;&c_p;&c_a;&c_r;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="sk">vypari&tcaron; sa</trd>
</drv>

<drv mrk="vapor.for0igxi">
  <kap>for<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <ref tip="dif" cel="vapor.el0igxi">el<tld/>i&gcirc;i</ref>
    <ekz>
      <uzo tip="stl">FIG</uzo>
      &ccirc;u via kura&gcirc;o jam for<tld/>i&gcirc;is?
    </ekz>
    <trd lng="de">verdampfen</trd>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_p;&c_a;&c_r;&c_y;&c_c;&c_c;&c_a;</trd>  
  <trdgrp lng="br">
    <trd>aezhenna&ntilde;<klr tip="ind"> (v.k.)</klr></trd>,
    <trd>diaezhenna&ntilde;</trd>
  </trdgrp>
  <trd lng="es">evaporarse</trd>
  <trdgrp lng="fr">
    <trd>s'<ind>&eacute;vaporer</ind></trd>
  </trdgrp>
  <trd lng="hu">elp&aacute;rolog</trd>
  <trd lng="pt">evaporar</trd>
  <trd lng="nl">verdampen</trd>
  <trd lng="pl">wyparowa&cacute;</trd>
  <trd lng="ru">&c_i;&c_s;&c_p;&c_a;&c_r;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="sk">vypari&tcaron; sa</trd>
</drv>
</art>
