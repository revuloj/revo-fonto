<drv mrk="vapor.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Eliganta <tld/>on, funkcianta per <tld/>o:
      <ekz>
        funkciados, kiel ordinare en Ameriko, ma&scirc;inoj <tld/>aj, pumpantaj la akvon en la
        kvanto de 135&#x202F;000 litroj en minuto
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la nigran <tld/>an bola&jcirc;on krevigis fajro, el la densa mallumo &scirc;prucis en la
        alton la potenca blokego
        <fnt>
          <bib>MkM</bib>
          <lok>25</lok>
        </fnt>.
      </ekz>
    </dif>                         
    <trd lng="br">aezhennus</trd>
    <trd lng="ca">vapor&iacute;fer</trd>
    <trd lng="de">dampfend</trd>
    <trd lng="fa">&a_b;&a_kh;&a_A;&a_r;&a_d;&a_A;&a_r;</trd>
    <trd lng="fr">fumant<klr> (produisant de la vapeur)</klr></trd>
    <trd lng="hu">g&odblac;z-</trd>
    <trd lng="id">ber<ind>uap</ind></trd>
    <trd lng="it">fumante<klr> (di vapore)</klr></trd>
    <trdgrp lng="nl">
      <trd>dampend</trd>,
      <trd>stomend</trd>
    </trdgrp>
    <trd lng="pl">paruj&aogonek;cy</trd>
    <trd lng="pt">vapor&iacute;fero</trd>
  </snc>
  <snc>
    <dif>
      Aspekte simila al <tld/>o:
      <ekz>
        <tld/>a nebulo, tulo;
      </ekz>
      <ekz>
        la drinkejo estas varma, <tld/>a, haladza, multaj malri&ccirc;uloj babilas, balbutadas
        tie
        <fnt>
          <bib>LGA</bib>
          <lok>V.</lok>
        </fnt>;
      </ekz>
      <ekz>
        nur unu homo restis sur la monteto en la <tld/>a bolado de tondro, akvo kaj fajro
        <fnt>
          <bib>MkM</bib>
          <lok>16</lok>
        </fnt>;
      </ekz>
      <ekz>
        brila korpo &scirc;vebis fikse en la <tld/>a blanko de la sennuba, tamen tute pala
        &ccirc;ielo
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 6 (104)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vat.0i">vati</ref>
    </refgrp>                    
    <trd lng="br">aezhennel</trd>
    <trd lng="ca">vapor&oacute;s</trd> 
    <trdgrp lng="de">
      <trd>nebelig</trd>
      <trd>gasf&ouml;rmig</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_b;&a_kh;&a_A;&a_r;&a_A_madda;&a_l;&a_w;&a_d;</trd>,
      <trd>&a_m;&a_h;&#x200C;&a_A_madda;&a_l;&a_w;&a_d;</trd>
    </trdgrp>
    <trd lng="fr">vaporeux</trd>
    <trdgrp lng="hu">
      <trd>g&odblac;z&ouml;s</trd>,
      <trd>p&aacute;r&aacute;s</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>vaporoso</trd>,
      <trd>gassoso</trd>,
      <trd>ovattato<klr> (come vapore)</klr></trd>
    </trdgrp>
    <trd lng="nl">nevelig</trd>
    <trd lng="pt">vaporoso</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">parn&iacute;</trd>
  <trd lng="es">vaporoso</trd>
  <trdgrp lng="ja">
    <trd>&#x84B8;&#x6C17;&#x306E;  <pr>&#x3058;&#x3087;&#x3046;&#x304D;&#x306E;</pr></trd>,
    <trd>&#x84B8;&#x6C17;&#x306E;&#x305F;&#x3061;&#x3053;&#x3081;&#x305F;  <pr>&#x3058;&#x3087;&#x3046;&#x304D;&#x306E;&#x305F;&#x3061;&#x3053;&#x3081;&#x305F;</pr></trd>,
    <trd>&#x304A;&#x307C;&#x308D;&#x306B;&#x304B;&#x3059;&#x3093;&#x3060;</trd>,
    <trd>&#x900F;&#x3051;&#x308B;&#x3088;&#x3046;&#x306B;&#x8584;&#x3044;  <pr>&#x3059;&#x3051;&#x308B;&#x3088;&#x3046;&#x306B;&#x3046;&#x3059;&#x3044;</pr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_r;&c_o;&c_v;&c_o;&c_j;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">parn&yacute;</trd>
  <trd lng="tr">buhar ile ilgili</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_a;&c_r;&#x43E;&c_v;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_a;&c_r;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_a;&c_r;&#x43A;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="vapor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="vapor.0i.KOMUNE">
    <dif>
      Eligi <tld/>on:
      <ekz>
        plej bonguste <tld/>is rostita ansero, far&ccirc;ita per prunoj
        kaj pomoj
        <fnt><bib>Fab2</bib>, <lok>knabineto kun alumetoj</lok></fnt>;
      </ekz>
      <ekz>
        la fulgokovrita poto &scirc;a&ubreve;mis kaj <tld/>is super la flamanta kenfajro
        <fnt>
          <bib>HsT</bib>
          <lok>Feli&ccirc;a homo</lok>
        </fnt>;
      </ekz>
      <ekz>
        lasu vin mortigi per la ambroziaj odoroj, kiuj <tld/>as el lia fa&ubreve;ko
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_p;&c_u;&c_s;&c_k;&c_a;&c_c;&c_mol;
<ind>&c_p;&c_a;&c_r;&c_u;</ind></trd>
  <trd lng="br">divogedi&ntilde;<klr tip="amb"> (en ur zisteurel
aezhenn)</klr></trd>
  <trd lng="ca">desprendre <ind>vapor</ind></trd>
  <trdgrp lng="cs">
    <trd>odpa&rcaron;ovat</trd>,
    <trd>odpa&rcaron;ovat se</trd>,
    <trd>pa&rcaron;it <klr>(se)</klr></trd>,
    <trd>vypa&rcaron;ovat</trd>
  </trdgrp>
  <trd lng="de">dampfen</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_kh;&a_A;&a_r; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_b;&a_kh;&a_A;&a_r; &a_b;&f_y;&a_r;&a_w;&a_n;
    &a_d;&a_A;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">fumer<klr tip="amb"> (faire de la vapeur)</klr></trd>
  <trd lng="hu">g&odblac;z&ouml;l&ouml;g</trd>
  <trd lng="id">ber<ind>uap</ind></trd>
  <trd lng="it">fumare<klr> (emettere vapore)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x84B8;&#x6C17;&#x3092;&#x51FA;&#x3059;  <pr>&#x3058;&#x3087;&#x3046;&#x304D;&#x3092;&#x3060;&#x3059;</pr></trd>,
    <trd>&#x6E6F;&#x6C17;&#x3092;&#x7ACB;&#x3066;&#x308B;  <pr>&#x3086;&#x3052;&#x3092;&#x305F;&#x3066;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>dampen</trd>,
    <trd>stomen</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_s;&c_p;&c_u;&c_s;&c_k;&c_a;&c_t;&c_mol;
&c_p;&c_a;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>odparova&tcaron; sa</trd>,
    <trd>pari&tcaron; sa</trd>,
    <trd>vyparova&tcaron; sa</trd>
  </trdgrp>
  <trd lng="tr">buhar &ccedil;&inodot;karmak</trd>
  <trd lng="uk">&c_v;&c_i;&#x43F;&c_a;&c_r;&#x43E;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>
</drv>

<drv mrk="vapor.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vapor.0igi.KOMUNE">
    <dif>
      Fari ion <tld/>o:
      <ekz>
        <tld/>igi akvon en kaldrono.
      </ekz>
    </dif>
    <ref tip="ant" cel="kondens.0i">kondensi</ref>
  </snc>
  <snc mrk="vapor.0igi.FIG">
    <dif>
      Forigi kiel <tld/>o:
      <ekz>
        kiam Leopoldo vidis, ke sur la frunto de lia edzino pasas unu el tiuj ombroj,
        <klr>[&#x2026;]</klr> li havis neniun malfacilecon por &gcirc;in <tld/>igi per blovo de
        &scirc;macado
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i devis]</klr> <tld/>igi el la spirito de Petro la malbonan impreson
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
