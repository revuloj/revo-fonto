  <gra><vspec>ntr</vspec></gra>
  <snc mrk="vapor.el0igxi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tute malaperi kiel <tld/>o:
      <ekz>
        &ccirc;u via iometo da kura&gcirc;o jam el<tld/>i&gcirc;is
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>?
      </ekz>
      <ekz>
        kun la lasta afabla saluto de l&#x2019; &scirc;ipestro el<tld/>i&gcirc;is definitive Germanio kaj
        komenci&gcirc;is Sovetunio
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        miaj vivenergioj for<tld/>i&gcirc;is en malplena, senenhava vivo
        <fnt>
          <bib>VaK</bib>
          <lok>Sesa &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia bona humoro subite for<tld/>i&gcirc;is, la doloro de la rememoroj premis mian koron
        <fnt>
          <bib>VaK</bib>
          <lok>Dekna&ubreve;a &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="esting">estingi&gcirc;i</ref>,
      <ref cel="cxerp">el&ccirc;erpi&gcirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_p;&c_a;&c_r;&c_y;&c_c;&c_c;&c_a; <klr>(&c_z;&c_u;&c_s;&c_ib;&c_m;)</klr></trd>   
