<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vapor</rad>/o
</kap>
<drv mrk="vapor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Gasa nubo supreni&gcirc;anta el varmi&gcirc;anta fluida&jcirc;o:
      <ekz>
        <tld/>o de akvo, etero;
      </ekz>
      <ekz>
        malaperi kiel <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>kuiriston <tld/>o satigas
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz><uzo tip="stl">FIG</uzo>
        havi iom da <tld/>o en la kapo
        <klr>(esti ebrieta)</klr>
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        densa nebulo lacigis la rigardon per siaj molaj
        <tld/>a&jcirc;oj<fnt>B</fnt>;
      </ekz>
      <ekz>
        la &scirc;ipo foriris kun plen<tld/>o
        <klr>(plej rapide)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Dampf</trd>
  <trdgrp lng="fr">
    <trd>vapeur</trd>
  </trdgrp>
  <trd lng="nl">damp</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;</trd>
</drv>

<drv mrk="vapor.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Eliganta <tld/>on.
    </dif>
    <trd lng="de">dampfend</trd>
    <trdgrp lng="fr">
      <trd>fumant<klr> (produisant de la vapeur)</klr></trd>,
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Aspekte simila al <tld/>o:
      <ekz>
        <tld/>a nebulo, tulo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vat.0i">vati</ref>
    </refgrp>
    <trdgrp lng="fr">
      <trd>vaporeux</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_r;&c_o;&c_v;&c_o;&c_j;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="vapor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Eligi <tld/>on:
      <ekz>
        sur la tablo plej bonguste <tld/>is rostita ansero<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">dampfen</trd>
  <trd lng="ru">&c_i;&c_s;&c_p;&c_u;&c_s;&c_k;&c_a;&c_t;&c_mol; &c_p;&c_a;&c_r;</trd>
</drv>

<drv mrk="vapor.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari ion <tld/>o:
      <ekz>
        <tld/>igi akvon en kaldrono;
      </ekz>
      <ekz>
        <tld/>igilo por parfumoj.
      </ekz>
    </dif>
    <trd lng="de">abdampfen</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>brumiser</trd>,
    <trd>&eacute;vaporer</trd>,
    <trd>vaporiser</trd>
  </trdgrp>
  <trd lng="nl">verdampen</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_p;&c_a;&c_r;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_s;&c_p;&c_a;&c_r;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_p;&c_a;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="vapor.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Fari&gcirc;i <tld/>o:
      <ekz>
        la akvo <tld/>i&gcirc;as je la ordinara temperaturo.
      </ekz>
    </dif>
    <trd lng="de">verdampfen</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>s'<ind>&eacute;vaporer</ind></trd>
  </trdgrp>
  <trd lng="nl">verdampen</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_p;&c_a;&c_r;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_i;&c_s;&c_p;&c_a;&c_r;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="vapor.el0igxifor0igxi">
  <kap>el<tld/>i&gcirc;i, for<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Malaperi <tld/>i&gcirc;ante:
      <ekz>
        la akvo el la kaserolo el<tld/>i&gcirc;is;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;u via kura&gcirc;o jam for<tld/>i&gcirc;is?
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="for.0igxi">fori&gcirc;i</ref>,
      <ref cel="esting">estingi&gcirc;i</ref>,
      <ref cel="cxerp">el&ccirc;erpi&gcirc;i</ref>,
      <ref cel="sven.0i">sveni</ref>.
    </refgrp>
    <trd lng="de">verdampfen</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>s'<ind>&eacute;vaporer</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_s;&c_p;&c_a;&c_r;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="vapor.0sxipo">
  <kap><tld/>&scirc;ipo</kap>
  <snc>
    <dif>
      &Scirc;ipo movata per akva <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Dampfer</trd>
  <trdgrp lng="fr">
    <trd>bateau &agrave; vapeur</trd>,
    <trd>vapeur<klr> (bateau)</klr></trd>
  </trdgrp>
  <trd lng="nl">stoomboot</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_o;&c_h;&c_o;&c_d;</trd>
</drv>
</art>
