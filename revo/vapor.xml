<drv mrk="vapor.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="vapor.0igxi.KOMUNE">
    <dif>
      Fari&gcirc;i <tld/>o:
      <ekz>
        la akvo <tld/>i&gcirc;as je la ordinara temperaturo;
      </ekz>
      <ekz>
        la akvo inter la potoj <tld/>i&gcirc;as kaj tiel malvarmigas la internan poton
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2010/010175.php">Simplaj solvoj por premaj problemoj</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fizikistoj fandas malgrandan kvanton da purega cezio, de kiu ioma kvanto
        <tld/>i&gcirc;as
        <fnt>
          <aut>Walter Klag</aut>
            <vrk><url ref="&Monato;/2000/007024.php">Pli ekzakte ne eblas: Radi-regata
            horlo&gcirc;o</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 13</lok>
          </fnt>.
      </ekz>
    </dif>
    <trd lng="de">verdampfen</trd>
  </snc>
  <snc mrk="vapor.0igxi.FIG">
    <dif>
      <ref tip="super" cel="aper.mal0i">Malaperi</ref> kiel <tld/>o:
      <ekz>
        &scirc;ia timo rapide <tld/>i&gcirc;is
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        malkompreni&gcirc;o, kiu rapide <tld/>i&gcirc;os
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> trovis en tiu ago de sindono regalon de kalumnioj, kiun
        kompreneble ili ne lasis <tld/>i&gcirc;i
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj zorgoj iom post iom <tld/>i&gcirc;is kaj fari&gcirc;is malrealaj kiel forgesitaj son&gcirc;oj
        <fnt>
          <bib>HsT</bib>
          <lok>Feli&ccirc;a homo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la knabjaraj romantikaj revoj <klr>[&#8230;]</klr> anta&ubreve; longe <tld/>i&gcirc;is
        <fnt>
          <bib>HsT</bib>
          <lok>Kuni&gcirc;o</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="solv.0igxi">solvi&gcirc;i</ref>
    </refgrp>
    <trd lng="de">sich <ind>verfl&uuml;chtigen</ind></trd>
  </snc>
  <trdgrp lng="ca">
    <trd>vaporitzar-se</trd>,
    <trd>evaporar-se</trd>
  </trdgrp>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_p;&c_a;&c_r;&c_y;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_p;&c_a;&c_r;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>aezhenna&ntilde;<klr tip="ind"> (v.k.)</klr></trd>,
    <trd>diaezhenna&ntilde;</trd>
  </trdgrp>
  <trd lng="en">vaporize</trd>
  <trd lng="es">evaporarse</trd>
  <trdgrp lng="fa">
    <trd>&a_t;&a_b;&a_kh;&f_y;&a_r; &a_sh;&a_d;&a_n;</trd>,
    <trd>&a_b;&a_kh;&a_A;&a_r; &a_sh;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>&eacute;vaporer</ind></trd>
