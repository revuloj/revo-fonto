<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>skorpi</rad>/o
</kap>
<drv mrk="skorpi.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la filumo
      <ref tip="super" cel="artik.0pieduloj">artikpieduloj</ref>
      (<trd lng="la">Scorpiones</trd>), kun pin&ccirc;iloj kaj
      sessegmenta abdomeno fini&gcirc;anta je venena pikilo.
    </dif>
  </snc>
  <trd lng="de">Skorpione</trd>
  <trd lng="nl">schorpioen</trd>
  <trd lng="pl">skorpion</trd>
</drv>
<drv mrk="skorpi.S0o">
  <kap><tld lit="S"/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Stelaro kaj signo de la <ref tip="malprt" cel="zodiak.0o">zodiako</ref>
      (<trd lng="la">Scorpio</trd>).
    </dif>
  </snc>
  <trd lng="de">Skorpion</trd>
  <trd lng="nl">schorpioen</trd>
  <trd lng="pl">skorpion</trd>
</drv>
</art>
