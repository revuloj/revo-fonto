<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>skorpi</rad>/o
</kap>
<drv mrk="skorpi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Iu el la ordo <tld/>uloj
      (<trd lng="la">Euscorpius</trd>, <trd lng="la">Buthus</trd> kaj.a.).
    </dif>
  </snc>
  <trd lng="de">Skorpion</trd>
</drv>
<drv mrk="skorpi.S0o">
  <kap><tld lit="S"/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Stelaro kaj signo de la <ref tip="malprt" cel="zodiak.0o">zodiako</ref>
      (<trd lng="la">Scorpius</trd>).
    </dif>
  </snc>
  <trd lng="de">Skorpion</trd>
</drv>
<drv mrk="skorpi.0uloj">
  <kap><tld/>uloj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo de <ref tip="super" cel="artik.0uloj">artikuloj</ref>
      (<trd lng="la">Scorpiones</trd>), kun pin&ccirc;iloj kaj
      sesesegmenta abdomeno fini&gcirc;anta je venena pikilo.
    </dif>
  </snc>
  <trd lng="de">Skorpione</trd>
</drv>
</art>
