<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>skorpi</rad>/o
</kap>
<drv mrk="skorpi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Iu genro el la familio <tld/>edoj
      (<trd lng="la">Euscorpius</trd>, <trd lng="la">Buthus</trd> k.a.), 
      kies vosto, ordinare suprenfleksita, fini&gcirc;as
      per venena piikilo.
    </dif>
  </snc>
  <trd lng="de">Skorpion</trd>
</drv>
<drv mrk="skorp.S0o">
  <kap><tld lit="S"/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Stelaro kaj signo de la <ref tip="malprt" cel="zodiak.0o">zodiako</ref>
      (<trd lng="la">Scorpius</trd>).
    </dif>
  </snc>
  <trd lng="de">Skorpion</trd>
</drv>
<drv mrk="skorp.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <tld/>uloj (<trd lng="la">Scorpionidae</trd>).
    </dif>
  </snc>
</drv>
</art>
