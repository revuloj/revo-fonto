<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>skorpi</rad>/o
</kap>
<drv mrk="skorpi.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la filumo
      <ref tip="super" cel="artik.0pieduloj">artikpieduloj</ref>
      (<trd lng="la">Scorpiones</trd>), kun pin&ccirc;iloj kaj
      sessegmenta abdomeno fini&gcirc;anta je venena pikilo.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_a;&c_r;&c_p;&c_ib;&c_jo;&c_n;</trd>
  <trd lng="de">Skorpione</trd>
  <trd lng="hu">skorpi&oacute;k<klr tip="ind"> (rend></klr></trd>
  <trd lng="nl">schorpioenen</trd>
  <trd lng="pl">skorpiony</trd>
  <trd lng="fr">scorpions</trd>
  <trd lng="ru">&c_s;&c_k;&c_o;&c_r;&c_p;&c_i;&c_o;&c_n;</trd>
</drv>

<drv mrk="skorpi.S0o">
  <kap><tld lit="S"/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Stelaro kaj signo de la <ref tip="malprt"
      cel="zodiak.0o">zodiako</ref>
      (<trd lng="la">Scorpio</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_S;&c_k;&c_a;&c_r;&c_p;&c_ib;&c_jo;&c_n;</trd>  
  <trd lng="de">Skorpion</trd>
  <trd lng="he">&mem;&zayin;&lamed; &ayin;&qof;&resh;&bet;</trd>
  <trd lng="hu">Skorpi&oacute;</trd>
  <trd lng="nl">schorpioen</trd>
  <trd lng="pl">skorpion</trd>
  <trd lng="fr">scorpion</trd>
  <trd lng="ru">&c_S;&c_k;&c_o;&c_r;&c_p;&c_i;&c_o;&c_n;</trd>
</drv>
</art>
