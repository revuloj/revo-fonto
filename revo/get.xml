<vortaro>
  <art mrk="$Id: get.xml,v 1.8 2023/11/11 10:00:40 afido Exp $">
    <kap>
      <rad>get</rad>/o
    </kap>

<drv mrk="get.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Apartigita <ref tip="super" cel="kvarta.0o">kvartalo</ref>, kie lo&gcirc;as
      precipe anoj de iu aparta etno a&ubreve; malri&ccirc;a klaso:
      <ekz>
        kelkaj informoj aperis en la kvina kajero <klr>[&#x2026;]</klr> &leftquot;Hebreo en la <tld/>o&rightquot;
        <fnt>
          <bib>Plumamikoj</bib>
        </fnt>;
      </ekz>
      <ekz>
        eklo&gcirc;antoj de &ccirc;i laborista <tld/>o decidis trabori mem fenestrojn rigarde al la
        montaro
        <fnt>
          <bib>MMa</bib>
        </fnt>;
      </ekz>
      <ekz>
        sekvis <tld/>oj en &ccirc;iuj grandaj urboj por &leftquot;kolekti&rightquot; e&ubreve;ropajn judojn,
        <klr>[&#x2026;]</klr> konataj estas la <tld/>oj de Varsovio, <nom>&Lstroke;&oacute;d&zdot;</nom>, Krakovo,
        <nom>Lublin</nom>
        kaj la koncentrejo, nomata <tld/>o, en <nom>Theresienstadt</nom>
        <fnt>

          <aut>Josef Maria Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2003/007936.html">Jubileo hontinda</url></vrk>
          <bib>Monato</bib>
          <lok>2003/03, p. 15</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la juda <tld/>o vidi&gcirc;as kelkaj butik&scirc;ildoj skribitaj en la esperanta:
        &leftquot;Vesta&jcirc;oj&rightquot;, &leftquot;Razejo&rightquot;
        <fnt>

          <aut>J.M. Parisot-Delourmel</aut>
          <vrk><url ref="&Monato;/2004/008286.html">Charles Chaplin kaj Esperanto</url></vrk>
            <bib>Monato</bib>
          <lok>2004/05, p. 6</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fanar.F0o.GEOG">Fanaro<sncref/></ref>,
      <ref cel="jud.0ejo">judejo</ref>,
      <ref cel="urb.lad0o">ladurbo</ref>
    </refgrp>
    <trd lng="de">Ghetto</trd>
    <trd lng="en">Ghetto</trd>
    <trd lng="fr">Ghetto</trd>
    <trd lng="nl">getto</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_je;&c_t;&c_a;</trd>,
    <trd>&c_gu;&c_je;&c_t;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30B2;&#x30C3;&#x30C8;&#x30FC;</trd>,
    <trd>&#x30E6;&#x30C0;&#x30E4;&#x4EBA;&#x5C45;&#x4F4F;&#x533A;
    <pr>&#x3086;&#x3060;&#x3084;&#x3058;&#x3093;&#x304D;&#x3087;&#x3058;&#x3085;&#x3046;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="uk">&c_gu;&c_je;&c_t;&c_t;&#x43E;</trd>
  <trdgrp lng="zh">
    <trd>&#x805A;&#x5C45;&#x70B9; <pr>j&#xF9;j&#x16B;di&#x1CE;n</pr></trd>,
    <trd>&#x72B9;&#x592A;&#x4EBA;&#x533A; <pr>y&#xF3;ut&#xE0;ir&#xE9;nq&#x16B;</pr></trd>
  </trdgrp>
</drv>

