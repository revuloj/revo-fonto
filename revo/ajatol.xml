<vortaro>
<art mrk="$Id: ajatol.xml,v 1.13 2021/05/06 21:50:48 revo Exp $">
<kap>
  <rad>ajatol</rad>/o <fnt><bib>NePiv</bib></fnt>,
  <var><kap><rad var="h">ajatolah</rad>/o <fnt><vrk><url ref="https://www.majstro.com/Web/Majstro/showEntryInfo.php?k=78243">Majstro</url></vrk></fnt></kap>
  </var>
</kap>

<drv mrk="ajatol.0o">
  <kap><tld/>o,
  <var><kap><tld var="h"/>o</kap></var></kap>
  <snc mrk="ajatol.0o.ISL">
    <uzo tip="fak">ISL</uzo>
    <dif>
      Altranga kleriko en <ref tip="vid"
      cel="sxijaism.0o">&scirc;ijaismo</ref>:
      <ekz>
        en 1979 <klr>[&#8230;]</klr> radikala <tld/>o <nom>Komejni</nom> ekregis en Irano
        <fnt>
        <aut>S. Maul</aut>
        <vrk>Murdi je nomo de dio</vrk>
        <bib>Monato</bib>
        <lok>2001:10a, p. 5a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la teokratio de la <tld var="h"/>oj
        <fnt>
          <aut>S. Halimi, trad. -</aut>
          <vrk><url ref="https://eo.mondediplo.com/imprimer.php3?id_article=1691"
              >Kelkfoje la maleblo okazas</url></vrk>
          <lok>Le Monde Diplomatique, 2011-01-01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_ja;&c_t;&c_a;&c_l;&c_a;</trd>
  <trd lng="bg">&c_a;&c_ja;&c_t;&c_o;&c_l;&c_a;&c_h;</trd>
  <trd lng="ca">aiatol&middot;l&agrave;</trd>
  <trd lng="cs">ajatoll&aacute;h</trd>
  <trd lng="de">Ajatollah</trd>
  <trd lng="en">ayatollah</trd>
  <trd lng="es">ayatol&aacute;</trd>
  <trd lng="fa">&a_A_madda;&f_y;&a_t;&zwnj;&a_A;&a_l;&a_l;&a_shadda;&a_h;</trd>
  <trd lng="fr">ayatollah</trd>
  <trd lng="id">ayatullah</trd>
  <trd lng="pl">ajatollah</trd>
  <trd lng="ro">Ajatollah</trd>
  <trd lng="sk">ajatoll&aacute;h</trd>
</drv>
</art>

