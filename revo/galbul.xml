<art mrk="$Id: galbul.xml,v 1.18 2023/08/31 14:29:50 revo Exp $">
<kap>
  <rad>galbul</rad>/o
</kap>
<drv mrk="galbul.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="galbul1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Galbula</trd>):
      <ekz>
        <tld/>oj <klr>[&#x2026;]</klr> estas insektoman&gcirc;antaj birdoj de Sudameriko (kaj
        Centrameriko) <klr>[&#x2026;]</klr> grimpantaj sur arboj, sed lo&gcirc;antaj en kava&jcirc;oj en
        la tero, okulfrape koloraj kaj metale brilaj, kun longa rekta beko kaj
        mallongaj, rondaj flugiloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=265004">Galbuloj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/6/60/Rufous-tailed_jacamar_(Galbula_ruficauda)_male_2.JPG/320px-Rufous-tailed_jacamar_(Galbula_ruficauda)_male_2.JPG" prm="CC BY-SA 4.0"><tld/>o (Galbula ruficauda rufoviridis), Pantanal, Brazilo
        <fnt>
          <aut>Charles J Sharp, propra verko</aut>
          <lok><url ref="&WCW;/File:Rufous-tailed_jacamar_(Galbula_ruficauda)_male_2.JPG">&WCW;/File:Rufous-tailed_jacamar_(Galbula_ruficauda)_male_2.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_a;&c_k;&c_a;&c_m;&c_a;&c_r;</trd>,
    <trd>&c_ja;&c_k;&c_a;&c_m;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="de">Glanzvogel</trd>
  <trdgrp lng="es">
    <trd>jacamar</trd>,
    <trd>jacamar&aacute;</trd>,
    <trd>yacamar&aacute;</trd>
  </trdgrp>
  <trd lng="fr">jacamar</trd>
  <trd lng="hu">jakam&aacute;r</trd>
  <trd lng="ja">&#x30AD;&#x30EA;&#x30CF;&#x30B7;</trd>
  <trd lng="nl">glansvogel</trd>
  <trd lng="pt">galbul&iacute;deo</trd>
  <trd lng="ru">&c_zh;&c_a;&c_k;&c_a;&c_m;&c_a;&c_r;</trd>
  <trd lng="uk">&#x44F;&#x43A;&c_a;&#x43C;&c_a;&c_r;&c_a;</trd>
</drv>
