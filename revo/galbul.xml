<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>galbul</rad>/o
</kap>
<drv mrk="galbul.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="galbul.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Galbula</trd>).
    </dif>
  </snc>
  <trd lng="de">Glanzvogel</trd>
</drv>
<drv mrk="galbul.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="peg.0oformaj">pegoformaj
      birdoj</ref> (<trd lng="la">Galbulidae</trd>), vivantaj en tropika Ameriko
      kun longa beko por kapti insektojn, precipe papiliojn, pluraj kun orverde
      brilanta plumaro.
    </dif>
  </snc>
  <trd lng="de">Glanzv&ouml;gel <klr tip="ind">(Familie)</klr></trd>
  <trd lng="en">jacamars</trd>
</drv>
</art>

