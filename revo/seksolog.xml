<art mrk="$Id: seksolog.xml,v 1.6 2022/04/08 15:42:27 revo Exp $">
<kap>
  <rad>seksolog</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="seksolog.0o">
  <kap><tld/>o</kap>
  <snc mrk="seksolog.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="dif" cel="seksologi.0isto.MED">Seksologiisto</ref>:
      <ekz>
        post studado en Kanado li enprofundi&gcirc;is en la seksan vivon de 
        la homoj kaj fari&gcirc;is <tld/>o
        <fnt>
          <aut>Mirejo</aut>,
          <vrk><url ref="http://mirejo.blogspot.com/2009/03/yamamoto-senji.html">
          Yamamoto Senji</url></vrk>,
          <lok>2009-03-05</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Magnus Hirschfeld</nom> <klr>[&#8230;]</klr> estis germana kuracisto kaj <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=641812">Magnus Hirschfeld</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_k;&c_s;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="de">Sexologe</trd>
  <trd lng="en">sexologist</trd>
  <trd lng="fr">sexologue</trd>
  <trd lng="it">sessuologo</trd>
  <trd lng="uk">&c_s;&c_je;&#x43A;&c_s;&#x43E;&#x43B;&#x43E;&c_g;</trd>
