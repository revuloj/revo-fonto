<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>masiv</rad>/a
</kap>
<drv mrk="masiv.0a">
  <kap>
    <ofc>*</ofc><tld/>a
  </kap>
  <snc>
    <dif>
      Konsistanta el densa, senintermanka materio, ne havanta malplena&jcirc;ojn:
      <ekz>
        vazo el <tld/>a oro.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fortik.0a">fortika</ref>,
      <ref cel="solid.0a">solida</ref>,
      <ref cel="kompak.0a">kompakta</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">massief</trd>
  <trd lng="ru">&c_m;&c_a;&c_s;&c_s;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="masiv.0o">
  <kap>
    <tld/>(a&jcirc;)o
  </kap>
  <snc>
    <dif>
      Aran&gcirc;o el materiaj a&jcirc;oj tre densigitaj:
      <ekz>
        <tld/>o da floroj, da montoj
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="blok.0o">bloko</ref>,
      <ref cel="mas.0o">maso</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">massief</trd>
  <trd lng="ru">&c_m;&c_a;&c_s;&c_s;&c_i;&c_v;</trd>
</drv>
</art>
