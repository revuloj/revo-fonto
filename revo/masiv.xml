<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: masiv.xml,v 1.18 2022/03/23 14:01:06 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>masiv</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="masiv.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="masiv.0a.FOR">
    <dif>
      Konsistanta el densa, senintermanka materio,
      ne havanta malplena&jcirc;ojn:
      <ekz>
        vazo el <tld/>a oro.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fortik.0a">fortika</ref>,
      <ref cel="kompak.0a">kompakta</ref>,
      <ref cel="solid.0a">solida</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_s;&c_ib;&c_w;&c_n;&c_y;</trd>
  <trd lng="br">tolzennek</trd>
  <trdgrp lng="cs">
    <trd>masivn&iacute;</trd>,
    <trd>mohutn&yacute;</trd>
  </trdgrp>
  <trd lng="de">massiv</trd>
  <trdgrp lng="en">
    <trd>massive</trd>,
    <trd>heavy</trd>,
    <trd>large</trd>
  </trdgrp>
  <trd lng="fr">massif<klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>t&ouml;m&ouml;r</trd>,
    <trd>massz&iacute;v</trd>
  </trdgrp>
  <trd lng="nl">massief</trd>
  <trdgrp lng="pl">
    <trd>lity</trd>,
    <trd>masywny</trd>,
    <trd>zbity</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>maci&ccedil;o</trd>,
    <trd>compacto</trd>,
    <trd>denso</trd>
  </trdgrp>
  <trd lng="ro">masiv</trd>
  <trd lng="ru">&c_m;&c_a;&c_s;&c_s;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>mas&iacute;vny</trd>,
    <trd>pevn&yacute;</trd>,
    <trd>&tcaron;a&zcaron;k&yacute;</trd>
  </trdgrp>
  <trd lng="uk">&#x43C;&#x430;&#x441;&#x438;&#x432;&#x43D;&#x438;&#x439;</trd>
</drv>

<drv mrk="masiv.0o">
  <kap><tld/>o, <var><kap><tld/>a&jcirc;o</kap></var></kap>
  <snc mrk="masiv.0o.FOR">
    <dif>
      Aran&gcirc;o el materiaj a&jcirc;oj tre densigitaj:
      <ekz>
        <tld/>o da floroj, da montoj
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="blok.0o">bloko</ref>,
      <ref cel="mas.0o">maso</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_s;&c_ib;&c_w;</trd>
  <trd lng="br">tolzenn</trd>
  <trd lng="de">Massiv</trd>
  <trd lng="fr">massif<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>massz&iacute;vum</trd>,
    <trd>t&ouml;mb</trd>
  </trdgrp>
  <trd lng="nl">massief</trd>
  <trd lng="pl">masyw</trd>
  <trd lng="ro">masiv</trd>
  <trd lng="ru">&c_m;&c_a;&c_s;&c_s;&c_i;&c_v;</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x430;&#x441;&#x438;&#x432; (&#x433;&#x456;&#x440;&#x441;&#x44C;&#x43A;&#x438;&#x439;</trd>,
    <trd>&#x43B;&#x456;&#x441;&#x43E;&#x432;&#x438;&#x439; &#x456; &#x442;.&#x43F;.)</trd>
  </trdgrp>
</drv>
</art>
