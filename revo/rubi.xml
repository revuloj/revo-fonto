<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rubi.xml,v 1.18 2023/08/21 08:30:46 revo Exp $">
<kap>
  <rad>rubiac</rad>/oj
</kap>
<drv mrk="rubi.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="rubi.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref> de <ref
      tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Rubiaceae</trd>):
      <ekz>
        <tld/>oj <klr>[&#8230;]</klr> enhavas indoalkaloidojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=27215">Rubiacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
       <ref cel="gali.0o">galio</ref>,
       <ref cel="kaf.0arbo">kafarbo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_e;&c_n;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  <trd lng="bg">&c_b;&c_r;&c_o;&c_sh;&c_o;&c_v;&c_i;</trd>
  <trd lng="ca">rubi&agrave;cies</trd>
  <trd lng="de">R&ouml;tegew&auml;chse</trd>
  <trd lng="fa">&a_r;&a_w;&a_n;&a_A;&a_s;&f_y;&a_A;&a_n;</trd>
  <trd lng="fr">rubiac&eacute;es</trd>
  <trdgrp lng="hu">
    <trd>buz&eacute;rf&eacute;l&eacute;k</trd>,
    <trd>galajf&eacute;l&eacute;k</trd>,
    <trd>k&aacute;v&eacute;f&eacute;l&eacute;k</trd>
  </trdgrp>
  <trd lng="nl">walstrofamilie</trd>
  <trd lng="pl">marzanowate</trd>
  <trd lng="pt">rubi&aacute;ceas</trd>
  <trd lng="ru">&c_m;&c_a;&c_r;&c_je;&c_n;&c_o;&c_v;&c_y;&c_je;
  <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr>
  </trd>
  <trd lng="uk">&#x43C;&c_a;&c_r;&c_je;&#x43D;&#x43E;&c_v;&c_ib;</trd>
</drv>
</art>
