<kap>
  <rad>Balduen</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="balduen.0o">
  <kap><tld/>o</kap>
  <snc mrk="balduen.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        aliancano estis lia onklo <tld/>o
        <fnt>
          <vrk><url
          ref="http://members.chello.cz/malovec/HKAROLO.HTM">La epoko de
          Karlo la IVa</url></vrk>,
          <lok>[vidita je 2006-05-05]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Balduin</trd>
  <trd lng="fr">Baudouin</trd>
  <trd lng="it">Baldovino</trd>
  <trd lng="pt">Baldu&iacute;no</trd>
</drv>
</art>

