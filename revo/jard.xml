<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>jard</rad>/o
</kap>
<drv mrk="jard.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="jard.0o.mezurunuo">
    <dif>
      Anglosaksa mezurunuo de longo.
    </dif>
    <subsnc mrk="jard.0o.mezurunuo.brita">
      <dif>
        La brita jardo egalas ekde 1922 al 0,9143984 metro.
      </dif>
    </subsnc>
    <subsnc mrk="jard.0o.mezurunuo.usona">
      <dif>
        La usona egalas ekde 1959 ekzakte al 0,9144 metro.
      </dif>
    </subsnc>
    <trd lng="de">Yard</trd>
    <trd lng="en">yard</trd>
    <trd lng="es">yarda</trd>
    <trdgrp lng="fr">
      <trd>yard</trd>
    </trdgrp>
    <trd lng="hu">yard</trd>
    <trd lng="nl">voet<klr tip="ind"> (lengtem.)</klr></trd>
    <trd lng="pt">jarda</trd>
    <trd lng="ru">&c_ja;&c_r;&c_d;</trd>
    <trd lng="sv">yard</trd>
  </snc>
  <snc mrk="jard.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Ligna stango al kiu estas fiksata
      <ref tip="vid" cel="vel.0o.MAR">velo</ref>.
    </dif>
    <refgrp tip="sin">
      <ref cel="stang.vel0o">velstango</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="mast.0o">masto</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Mast</trd>,
      <trd>Schiffsmast</trd>,
      <trd>Segelmast</trd>
    </trdgrp>
    <trd lng="en">spar</trd>
    <trd lng="es">verga</trd>
    <trdgrp lng="fr">
      <trd>espar</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>vitorlar&uacute;d</trd>,
      <trd>kereszt&aacute;rboc</trd>
    </trdgrp>
    <trd lng="nl">ra</trd>
    <trd lng="pt">verga</trd>
    <trd lng="ru">&c_r;&c_je;&c_ja;</trd>
    <trdgrp lng="sv">
      <trd>mast</trd>,
      <trd>bom</trd>
    </trdgrp>
  </snc>
</drv>
</art>
