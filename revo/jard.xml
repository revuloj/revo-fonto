<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>jard</rad>/o
</kap>
<drv mrk="jard.0o">
  <kap><tld/>o</kap>
  <snc mrk="jard.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <ref tip="dif" cel="stang.vel0o.MAR">velstango</ref>
  </snc>
  <rim mrk="jard1.0o.RIM_HOM">
    <ref tip="hom" cel="jard1.0o">Jardo</ref> anka&ubreve; estas la nomo de
    mezurunuo de longo.
  </rim>
  <trd lng="be">&c_r;&c_e;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Mast</trd>,
    <trd>Schiffsmast</trd>,
    <trd>Segelmast</trd>
  </trdgrp>
  <trd lng="en">spar</trd>
  <trd lng="es">verga</trd>
  <trdgrp lng="fr">
    <trd>espar</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vitorlar&uacute;d</trd>,
    <trd>kereszt&aacute;rboc</trd>
  </trdgrp>
  <trd lng="nl">ra</trd>
  <trd lng="pt">verga</trd>
  <trd lng="ru">&c_r;&c_je;&c_ja;</trd>
  <trdgrp lng="sv">
    <trd>mast</trd>,
    <trd>bom</trd>
  </trdgrp>
</drv>
</art>
