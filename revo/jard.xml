<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>jard</rad>/o
</kap>
<drv mrk="jard.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Anglosaksa mezurunuo de longo. La brita jardo egalas ekde 1922 al
      0,9143984 de metro. La usona egalas ekde 1959 ekzakte al 0,9144
      metro.
    </dif>
    <trd lng="de">Yard</trd>
    <trd lng="en">yard</trd>
    <trd lng="nl">voet<klr tip="ind"> (lengtem.)</klr></trd>
    <trd lng="ru">&c_ja;&c_r;&c_d;</trd>
    <trd lng="sv">yard</trd>
  </snc>
  <snc>
    <uzo tip="fak">MAR</uzo>
    <dif>
      Ligna stango al kiu estas fiksata velo. (= velstango)
    </dif>
    <trdgrp lng="de">
      <trd>Mast</trd>,
      <trd>Segelmast</trd>
    </trdgrp>
    <trd lng="en">spar</trd>
    <trd lng="nl">ra</trd>
    <trdgrp lng="sv">
      <trd>mast</trd>,
      <trd>bom</trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_je;&c_ja;</trd>
  </snc>
</drv>
</art>
