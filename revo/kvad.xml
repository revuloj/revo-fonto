<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>kvad</rad>/o <fnt><bib>Viki</bib></fnt>
</kap>
<drv mrk="kvad.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">AUT</uzo>
    <dif>
    <ref tip="lst" cel="vetur.0ilo" lst="voko:veturiloj">Veturilo</ref>
    ofte kun dikaj
    radringoj, simila al
    motorciklo a&ubreve; mopedo kun kvar radoj, uzata kiel eta traktoro en
    agrikulturo kaj armeo, sed &ccirc;efe por amuzi&gcirc;o, sporto.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>atv<klr> (all-terrain vehicle)</klr></trd>,
    <trd>quad</trd>
    <trd>quadricycle</trd>
    <trd>wheeler</trd>
  </trdgrp>
  <trd lng="es">quad</trd>
  <trdgrp lng="fr">
    <trd>quad</trd>,
    <trd>vtt<klr> (v&eacute;hicule tout-terrain)</klr></trd>
  </trdgrp>
</drv>
</art>

