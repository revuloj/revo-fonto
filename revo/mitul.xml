<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>mitul</rad>/o
</kap>
<drv mrk="mitul.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="mitul.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Mytilus</trd>), konko, kies unu mara specio estas multe   
      man&gcirc;ata.
    </dif>
  </snc>
  <trd lng="de">Miesmuschel</trd>
  <trd lng="fr">moule</trd>
  <trd lng="nl">mossel</trd>
</drv>
<drv mrk="mitul.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo
      <ref tip="super" cel="brank.faden0uloj">fadenbrankuloj</ref>
      (<trd lng="la">Mytilidae</trd>).
    </dif>
  </snc>
</drv>
</art>
