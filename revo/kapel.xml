  <trd lng="sk">kapl&aacute;n</trd>
  <trdgrp lng="cs">
    <trd>kaplan</trd>,
    <trd>kur&aacute;t</trd>
  </trdgrp>
  <trd lng="pl">kapelan</trd>
  <trdgrp lng="ro">
    <trd>capelan</trd>,
    <trd>preot</trd>
  </trdgrp>
-->
  <trd lng="ja">&#x793C;&#x62DD;&#x5802;&#x4ED8;&#x304D;&#x53F8;&#x796D;  <pr>&#x308C;&#x3044;&#x306F;&#x3044;&#x3069;&#x3046;&#x3064;&#x304D;&#x3057;&#x3055;&#x3044;</pr></trd>
</drv>

