<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>kapel</rad>/o
    </kap>

    <drv mrk="kapel.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="kapel.0o.neregula">
        <uzo tip="fak">KRI</uzo>
        <dif>
          Pre&gcirc;ejo, kiu ne estas regula <ref cel="kirk.0o">kirko</ref>.
        </dif>
        <subsnc mrk="kapel.0o.senaltara">
          <dif>
            Senaltara pre&gcirc;ejo, kie nur kelkaj diservoj estas farataj
            (sed ne la e&ubreve;&hcirc;aristio):
            <ekz>
              <klr>(la kavernaj grotoj)</klr> aspektis kvaza&ubreve; tombaj
              <tld/>oj kun mutaj orgentuboj kaj &scirc;toni&gcirc;intaj standardoj
              <fnt><bib>Fab1</bib>, <lok>&Gcirc;ardeno de la
              paradizo</lok></fnt>;
            </ekz><ekz>
              tiu supre sur la monto, la silenta <tld/>eto<fnt>Z</fnt>.
            </ekz>
          </dif>
        </subsnc>
        <subsnc mrk="kapel.0o.eksterparohxa">
          <dif>
            Pre&gcirc;ejo, kiu estas nek paro&hcirc;a, nek prioreja.
          </dif>
          <trd lng="ru">&c_k;&c_a;&c_p;&c_je;&c_l;&c_l;&c_a;</trd>
        </subsnc>
        <trd lng="be">&c_k;&c_a;&c_p;&c_l;&c_ib;&c_c;&c_a;</trd>
        <trd lng="de">Kapelle</trd>
        <trd lng="en">chapel</trd>
        <trd lng="fr">chapelle<klr tip="ind"> (petite
        &eacute;glise)</klr></trd>
        <trd lng="hu">k&aacute;polna</trd>
        <trd lng="nl">kapel</trd>
        <trd lng="ru">&c_ch;&c_a;&c_s;&c_o;&c_v;&c_n;&c_ja;</trd>
      </snc>
      <snc mrk="kapel.0o.kromaltarejo">
        <ref tip="dif" cel="altar.krom0ejo">kromaltarejo</ref>
        <trd lng="en">chapel</trd>
        <trd lng="fr">chapelle<klr tip="ind"> (dans une
        &eacute;glise)</klr></trd>
        <trd lng="de">Kapelle</trd>
        <trd lng="nl">kapel</trd>
        <trd lng="hu">k&aacute;polna</trd>
      </snc>
      <snc mrk="kapel.0o.hejma">
        <ref tip="dif" cel="pregx.hejma0ejo">hejma pre&gcirc;ejo</ref>
        <ekz>
          <ind><tld/>ano</ind>
          <klr>(pastro dungita por diservi en tia &ccirc;ambro)</klr>.
          <trd lng="fr">chapelain</trd>
        </ekz>
        <rim>
          &Ccirc;i tiun provon derivi la sencon de &leftquot;kapelano&rightquot; el <tld/>o
          mi
          opinias tute fu&scirc;a.
          <aut>Sergio Pokrovskij</aut>
        </rim>
        <trd lng="de">Hauskapelle</trd>
        <trd lng="en">chapel</trd>
        <trd lng="fr">chapelle<klr tip="ind"> (priv&eacute;e)</klr></trd>
        <trd lng="hu">h&aacute;zi <ind>k&aacute;polna</ind></trd>
        <trd lng="nl">kapel</trd>
      </snc>
      <snc mrk="kapel.0o.MUZ">
        <uzo tip="fak">MUZ</uzo>
        <dif>
          Aro de la muzikistoj, ludantaj a&ubreve; kantantaj en <tld/>o;
          pre&gcirc;eja &hcirc;oro:
          <ekz>
            <ind><tld/>estro</ind> <klr>(= &hcirc;orestro)</klr>.
            <trd lng="fr">ma&icirc;tre de chapelle</trd>
          </ekz>
        </dif>
        <trd lng="be">&c_k;&c_a;&c_p;&c_e;&c_l;&c_a;</trd>
        <trd lng="de">Kapelle</trd>
        <trd lng="en">chapel</trd>
        <trd lng="fr">chapelle<klr tip="ind"> (groupe de
        musiciens)</klr></trd>
        <trd lng="hu">templomi <ind>&eacute;nekkar/zenekar</ind></trd>
        <trd lng="nl">muziekkapel</trd>
        <trd lng="ru">&c_k;&c_a;&c_p;&c_je;&c_l;&c_l;&c_a;<klr> (&c_m;&c_u;&c_z;.)</klr></trd>
      </snc>
      <rim>
        Tiuj multaj kaj inter si senrilataj sencoj de unu sama vorto
        estas surprizaj por homo en kies lingvo tiuj senrilataj
        nocioj estas precize nomataj per apartaj vortoj.
        <aut>Sergio Pokrovskij</aut>
      </rim>
      <trd lng="cs">kapela</trd>
      <trd lng="es">capilla</trd>
    </drv>
  </art>
