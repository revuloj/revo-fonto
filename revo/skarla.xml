<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>skarlat</rad>/o
</kap>
<drv mrk="skarla.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Sange ru&gcirc;a <ref tip="super" cel="kolor.0o.propre">koloro</ref>.
    </dif>
  </snc>
  <trd lng="de">Scharlachrot</trd>
  <trdgrp lng="fr">
    <trd>&eacute;carlate<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="nl">scharlaken</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_l;&c_y;&c_j; &c_c;&c_v;&c_je;&c_t;</trd>,
    <trd>&c_p;&c_u;&c_n;&c_c;&c_o;&c_v;&c_y;&c_j; &c_c;&c_v;&c_je;&c_t;</trd>,
    <trd>&c_ja;&c_r;&c_k;&c_o;-&c_k;&c_r;&c_a;&c_s;&c_n;&c_y;&c_j; &c_c;&c_v;&c_je;&c_t;</trd>,
    <trd>&c_b;&c_a;&c_g;&c_r;&c_ja;&c_n;&c_y;&c_j; &c_c;&c_v;&c_je;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="skarla.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Sange ru&gcirc;a:
      <ekz>
        &leftquot;kiakolora la skarpo?&rightquot; demandis la knabino.
        &leftquot;Skarlata.&rightquot;<fnt><bib>RugxDom</bib>
        <lok>&ccirc;apitro 35a, p. 498a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>&eacute;carlate<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>
</art>
