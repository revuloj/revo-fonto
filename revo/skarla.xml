<vortaro>
<art mrk="$Id: skarla.xml,v 1.16 2022/03/27 17:06:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>skarlat</rad>/o
</kap>
<drv mrk="skarla.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Sange ru&gcirc;a <ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">koloro</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_u;&c_n;&c_s;&c_o;&c_v;&c_y;
    &c_k;&c_o;&c_l;&c_je;&c_r;</trd>,
    <trd>&c_ja;&c_r;&c_k;&c_a;-&c_ch;&c_y;&c_r;&c_v;&c_o;&c_n;&c_y;
    &c_k;&c_o;&c_l;&c_je;&c_r;</trd>,
    <trd>&c_b;&c_a;&c_r;&c_v;&c_o;&c_v;&c_y;
    &c_k;&c_o;&c_l;&c_je;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nach</trd>,
    <trd>&scaron;arlat</trd>,
    <trd>&scaron;arlatov&aacute; &ccaron;erve&ncaron;</trd>
  </trdgrp>
  <trd lng="de">Scharlachrot</trd>
  <trd lng="en">scarlet</trd>
  <trd lng="es">escarlata<klr> (sust.)</klr></trd>
  <trdgrp lng="fr">
    <trd>&eacute;carlate<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="hu">skarl&aacute;tv&ouml;r&ouml;s sz&iacute;n</trd>
  <trd lng="nl">scharlaken</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_l;&c_y;&c_j; &c_c;&c_v;&c_je;&c_t;</trd>,
    <trd>&c_p;&c_u;&c_n;&c_c;&c_o;&c_v;&c_y;&c_j;
    &c_c;&c_v;&c_je;&c_t;</trd>,
    <trd>&c_ja;&c_r;&c_k;&c_o;-&c_k;&c_r;&c_a;&c_s;&c_n;&c_y;&c_j;
    &c_c;&c_v;&c_je;&c_t;</trd>,
    <trd>&c_b;&c_a;&c_g;&c_r;&c_ja;&c_n;&c_y;&c_j;
    &c_c;&c_v;&c_je;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">&scaron;arl&aacute;tov&aacute; &ccaron;erve&ncaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x431;&#x430;&#x433;&#x440;&#x44F;&#x43D;&#x435;&#x446;&#x44C;</trd>,
    <trd>&#x431;&#x430;&#x433;&#x440;&#x44F;&#x43D;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="skarla.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Sange <ref tip="super" cel="rugx.0a">ru&gcirc;a</ref>:
      <ekz>
        &leftquot;kiakolora la skarpo?&rightquot; demandis la knabino.
        &leftquot;Skarlata.&rightquot;<fnt><bib>RugxDom</bib>
        <lok>&ccirc;apitro 35a, p. 498a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_u;&c_n;&c_s;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_ja;&c_r;&c_k;&c_a;-&c_ch;&c_y;&c_r;&c_v;&c_o;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_a;&c_r;&c_v;&c_o;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">&scaron;arlatov&yacute;</trd>
  <trd lng="en">scarlet</trd>
  <trd lng="es">escarlata<klr> (adj.)</klr></trd>
  <trdgrp lng="fr">
    <trd>&eacute;carlate<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="hu">skarl&aacute;tv&ouml;r&ouml;s</trd>
  <trd lng="sk">&scaron;arl&aacute;tov&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x44F;&#x441;&#x43A;&#x440;&#x430;&#x432;&#x43E;-&#x447;&#x435;&#x440;&#x432;&#x43E;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x44F;&#x441;&#x43D;&#x43E;-&#x447;&#x435;&#x440;&#x432;&#x43E;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x431;&#x430;&#x433;&#x440;&#x44F;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x43A;&#x440;&#x438;&#x432;&#x430;&#x432;&#x43E;-&#x447;&#x435;&#x440;&#x432;&#x43E;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>
</art>
