<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bust.xml,v 1.22 2022/03/13 20:13:44 revo Exp $">
<kap>
<ofc>1</ofc>
<rad>bust</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="bust.0o">
  <kap><tld/>o</kap>
  <snc mrk="bust.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Supera parto de la homa <ref tip="malprt" cel="korp.0o.ANA">korpo</ref>,
      kun la kapo kaj la brusto. 
    </dif>
    <trd lng="de">Torso</trd>
    <trd lng="en">torso</trd>
    <trd lng="hu">fels&odblac; <ind>test</ind></trd>
    <trd lng="pl">tors</trd>
  </snc>
  <snc mrk="bust.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      <ref tip="super" cel="skulpt.0ajxo">Skulpta&jcirc;o</ref>,
      prezentanta &ccirc;i tiun parton, ofte sen brakoj:
      <ekz>
        kontra&ubreve; la tombo de <nom>Galilei</nom> trovi&gcirc;as la tombo de <nom>Michel-Angelo</nom>, sur la monumento
        trovi&gcirc;as lia <tld/>o kaj anka&ubreve; tri figuroj: la Skulpturo, la Pentrarto kaj la
        Ar&hcirc;itekturo
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz> 
      <ekz>
        Alfredo skulptis la <tld/>ojn de <nom>Kala</nom> kaj de la bopatrino
        <fnt>
          <bib>Fab4</bib>
          <lok>&leftquot;Bela&rightquot;</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tors.0o">torso</ref>,
      <ref cel="trunk.0o">trunko</ref>,
      <ref cel="tali.0o">talio</ref>.
    </refgrp>
      <trd lng="de">B&uuml;ste</trd>
    <trd lng="en">bust</trd>
    <trd lng="hu">mellszobor</trd>
  </snc>
  <trd lng="be">&c_b;&c_ju;&c_s;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>bysta</trd>,
    <trd>poprs&iacute;</trd>
  </trdgrp>
  <trd lng="es">busto</trd>
  <trd lng="fr">buste</trd>
  <trd lng="he">&#x5D7;&#x5DC;&#x5E7; &#x5E2;&#x5DC;&#x5D9;&#x5D5;&#x5DF; &#x5E9;&#x5DC; &#x5D4;&#x5D2;&#x5D5;&#x5E3;</trd>
  <trd lng="it">busto</trd>
  <trd lng="pl">popiersie</trd>
  <trd lng="ru">&c_b;&c_ju;&c_s;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>busta</trd>,
    <trd>poprsie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x431;&#x44E;&#x441;&#x442;</trd>,
    <trd>&#x43F;&#x43E;&#x433;&#x440;&#x443;&#x434;&#x434;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
