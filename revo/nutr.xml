      <ekz>
        Lina konas lin de &ccirc;iam: &scirc;i estis lia mam<tld/>istino
        <fnt>
          <aut>R. Schwartz</aut>,
          <vrk>&Ccirc;e l' entombigo de l' povra Aleksandro</vrk>,
          <lok>Nica Literatura Revuo, 1:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        A&ubreve;skultu do, Azio, mam<tld/>istino nia,
        Afriko sfinkse muta, tribar' Oceania <klr>[&#x2026;]</klr>
        <fnt>
          <aut>N. &Hcirc;o&hcirc;lov</aut>,
          <vrk>E&ubreve;ropo</vrk>,
          <lok>La Tajdo, 1928</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="nutr.0istino"><tld/>istino</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_r;&c_m;&c_ib;&c_c;&c_je;&c_l;&c_mol;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_m;&c_k;&c_a;</trd>
