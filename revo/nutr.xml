  </snc>
  <trd lng="be">&c_h;&c_ib;&c_b;&c_n;&c_a;&c_je; <ind>&c_h;&c_a;&c_r;&c_ch;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</ind></trd>
  <trdgrp lng="de">
