<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>nutr</rad>/i
</kap>
<drv mrk="nutr.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Doni al vivanta esta&jcirc;o la man&gcirc;a&jcirc;on necesajn por subteni la fortojn
      kaj da&ubreve;rigi la vivon:
      <ekz>
        <tld/>i infanon, &ccirc;evalon, fi&scirc;ojn;
      </ekz>
      <ekz>
        estas sufi&ccirc;e da lakto por <tld/>i vin kaj por subteni la vivon de
        viaj servantinoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la lando facile <tld/>as sian lo&gcirc;antaron;
      </ekz>
      <ekz>
        la tero <tld/>as la vegeta&jcirc;ojn;
      </ekz>
      <ekz>
        sin <tld/>i per legomoj kaj fruktoj;
      </ekz>
      <ekz>
        pastron <tld/>as pre&gcirc;ejo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        (f) <tld/>i fajron<fnt>Z</fnt>;
      </ekz>
      <ekz>
        (f) malsa&gcirc;ulo <tld/>as sin per malsa&gcirc;eco<fnt>Z</fnt>;
      </ekz>
      <ekz>
        (f) <tld/>i iun per promesoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        sin <tld/>i per akvo kaj amo, per espero, per &hcirc;imeroj.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="asimil.0i">asimili</ref>,
      <ref cel="mangx.0i">man&gcirc;i</ref>,
      <ref cel="pasxt.0i">pa&scirc;ti</ref>.
    </refgrp>
    <trd lng="de">ern&auml;hren</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Helpi al da&ubreve;ro de iu sento, deziro a&ubreve; intenco:
      <ekz>
        <tld/>i la spiriton per altaj pensoj;
      </ekz>
      <ekz>
        <tld/>i sian amon per espero;
      </ekz>
      <ekz>
        <tld/>i esperon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tio <tld/>is lian malamon, &ccirc;agrenon;
      </ekz>
      <ekz>
        tio povas herede <tld/>i intergentan disecon<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">n&auml;hren</trd>
  </snc>
  <trd lng="en">nourish</trd>
  <trd lng="nl">voeden</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_r;&c_m;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_i;&c_t;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="nutr.0ajxo">
  <kap><tld/>(a&jcirc;)o</kap>
  <snc>
    <dif>
      Tio, kio servas por <tld/>i:
      <ekz>
        tio &ccirc;i estos bona
        <tld/>a&jcirc;o por viaj ostoj<fnt>Z</fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Nahrung</trd>
  <trd lng="nl">voedsel</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_i;&c_shch;&c_a;</trd>,
    <trd>&c_p;&c_i;&c_t;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="nutr.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Povanta <tld/>i:
      <ekz>
        lakto estas pli <tld/>a ol vino<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">nahrhaft</trd>
  <trd lng="nl">voedzaam</trd>
  <trd lng="ru">&c_p;&c_i;&c_t;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
