<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>naiv</rad>/a
</kap>
<drv mrk="naiv.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Sensperte malka&scirc;ema kaj kredema kiel infano:
      <ekz>
        <tld/>a knabino;
      </ekz>
      <ekz>
        ne ekzistas <tld/>a vulpo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        iom <tld/>a entuziasmo;
      </ekz>
      <ekz>
        <tld/>aj iluzioj;
      </ekz>
      <ekz>
        en tiu tempo tiuj &ccirc;i <tld/>aj kontra&ubreve;paroloj estis rigardataj kiel
        veroj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="sv">
      <trd>naiv</trd>,
      <trd>barnslig</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Tute la&ubreve;natura, senartifika:
      <ekz>
        <tld/>a respondo, rakonto, desegna&jcirc;o;
      </ekz>
      <ekz>
        la <tld/>aj amoj de pa&scirc;tistoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">naiv</trd>
  <trd lng="nl">na&iuml;ef</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_o;&c_d;&c_u;&c_sh;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="naiv.0ega">
  <kap><tld/>ega</kap>
  <snc>
    <dif>
      Tro<tld/>a, stultete <tld/>a.
    </dif>
  </snc>
</drv>
</art>
