<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>naiv</rad>/a
</kap>
<drv mrk="naiv.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Sensperte malka&scirc;ema kaj kredema kiel infano:
      <ekz>
        <tld/>a knabino;
      </ekz>
      <ekz>
        ne ekzistas <tld/>a vulpo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        iom <tld/>a entuziasmo;
      </ekz>
      <ekz>
        <tld/>aj iluzioj;
      </ekz>
      <ekz>
        en tiu tempo tiuj &ccirc;i <tld/>aj kontra&ubreve;paroloj estis
rigardataj kiel
        veroj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>naive</trd>,
      <trd>artless</trd>,
      <trd>ingenuous</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>na&iacute;v</trd>,
      <trd>gyan&uacute;tlan</trd>
    </trdgrp>    
    <trdgrp lng="sv">
      <trd>naiv</trd>,
      <trd>barnslig</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Tute la&ubreve;natura, senartifika:
      <ekz>
        <tld/>a respondo, rakonto, desegna&jcirc;o;
      </ekz>
      <ekz>
        la <tld/>aj amoj de pa&scirc;tistoj.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>innocent</trd>,
      <trd>natural</trd>,
      <trd>simple</trd>,
      <trd>unaffected</trd>,
      <trd>unsophisticated</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>term&eacute;szetes</trd>,
      <trd>mesterk&eacute;letlen</trd>
    </trdgrp>    
  </snc>
  <trd lng="be">&c_n;&c_a;&c_ib;&c_w;&c_n;&c_y;</trd>
  <trdgrp lng="de">
     <trd>unbefangen</trd>,
     <trd>naiv</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ing&eacute;nu</trd>,
    <trd>na&iuml;f</trd>,
    <trd>simple <klr> (na&iuml;f)</klr></trd>
  </trdgrp>
  <trd lng="nl">na&iuml;ef</trd>
  <trdgrp lng="pt">
    <trd>inocente</trd>,
    <trd>ing&ecirc;nuo</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_o;&c_d;&c_u;&c_sh;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="naiv.0ega">
  <kap><tld/>ega</kap>
  <snc>
    <dif>
      Tro<tld/>a, stultete <tld/>a.
    </dif>
  <trdgrp lng="de">
     <trd>einf&auml;ltig</trd>,
     <trd>leichtgl&auml;ubig</trd>,
     <trd>ziemlich naiv</trd>
  </trdgrp>
    <trdgrp lng="en">
      <trd>green</trd>,
      <trd>silly</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>cr&eacute;dule</trd>,
      <trd>niais</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>egy&uuml;gy&udblac;</trd>,
      <trd>hisz&eacute;keny</trd>,
      <trd>balga</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="naiv.0ismo">
  <kap><tld/>ismo</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Pentrarta stilo praktikata de neakademiaj artistoj la&ubreve;
      popola tradicio, favoranta detalajn bildojn de vila&gcirc;aj scenoj
      a&ubreve; pejza&gcirc;oj kun nelerta desegno, pimpaj koloroj.
    </dif>
  </snc>
  <trd lng="de">naive <ind>Kunst</ind></trd>
  <trdgrp lng="fr">
    <trd>art <ind>na&iuml;f</ind></trd>
  </trdgrp>
  <trd lng="hu">na&iacute;v<ind>m&udblac;v&eacute;szet</ind></trd>
  <trd lng="ru">&c_n;&c_a;&c_i;&c_v;&c_n;&c_o;&c_je;
&c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>
</art>
