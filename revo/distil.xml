<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>distil</rad>/i
</kap>
<drv mrk="distil.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <uzo tip="fak">KEM</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Purigi substancon vaporigante per varmo &gcirc;iajn vapori&gcirc;emajn
      erojn kaj poste refluidigante la vaporojn per malvarmo:
      <ekz>
        <tld/>i brandon;
      </ekz>
      <ekz>
        produkti alkoholon per <ind><tld/>ado</ind>;
        <trd lng="fr">distillation</trd>
      </ekz>
      <ekz>
        <tld/>ita akvo;
      </ekz>
      <ekz>
        <ind><tld/>ilo</ind>.
        <trd lng="fr">alambic</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="en">distill</trd>
  <trd lng="fr">distiller<klr tip="ind"> (soumettre &agrave; distillation)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_i;&c_s;&c_t;&c_i;&c_l;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_g;&c_o;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
