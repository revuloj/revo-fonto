<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: liki.xml,v 1.6 2024/02/12 21:20:48 afido Exp $">
    <kap>
      <rad>Liki</rad>/o <fnt><bib>NT</bib></fnt>
    </kap>

<drv mrk="liki.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <snc mrk="liki.0o.antikva_regiono">
    <dif>
      Antikva <ref tip="lst" lst="voko:regionoj" cel="region.0o.land">regiono</ref>
      en la sudokcidento de Malgrandazio:
      <ekz>
        transveturinte la maron apud Kilikio kaj Pamfilio, ni alvenis al <nom>Mira</nom> en <tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 27:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        kunpremante la gefilojn &ccirc;e sia sino la patrino fu&gcirc;is al <tld/>o, konata lando de
        la lupoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=100354">Leto</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
    lok="&WCU;/thumb/8/8b/Putzger_Kleinasien_Zuschnitt.jpg/640px-Putzger_Kleinasien_Zuschnitt.jpg"
    prm="PD">mapo de antikva Malgrandazio kun romiaj provincoj
        <fnt>
          <aut>Friedrich Wilhelm Putzger</aut>
          <lok><url
          ref="&WCW;/File:Putzger_Kleinasien_Zuschnitt.jpg">&WCW;/File:Putzger_Kleinasien_Zuschnitt.jpg</url></lok>
        </fnt>
        <mrk stl="left:25%;top:67.5%;width:8%;border-bottom:3px dotted blue; background:none"/>
    </bld>
    <trd lng="de">Lykien</trd>
    <trd lng="en">Lycia</trd>
    <trd lng="fr">Lycie</trd>
    <trdgrp lng="zh">
    <trd>&#21525;&#x57FA;&#x4E9A; <pr>l&#x1DA;j&#x12B;y&#xE0;</pr></trd>,
    <trd>&#21570;&#x57FA;&#x4E9E; <pr>l&#x1DA;j&#x12B;y&#xE0;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_L;&c_ib;&c_k;&c_ib;&c_ja;</trd>
  <trd lng="uk">&#x41B;&c_ib;&#x43A;&c_ib;&#x44F;</trd>
</drv>

  </art>

