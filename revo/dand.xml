<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dand.xml,v 1.33 2024/07/18 05:52:33 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>dand</rad>/o
   <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="dand.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="vir.0o.sekso">Viro</ref>,
      kies precipa zorgo estas <ref tip="prt" cel="elegan.0eco">elegante</ref>
      observi modon:
      <ekz>
        juna <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        forgesu modajn <tld/>ojn
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        forgesu kavajn vortojn de <tld/>a kavaliro
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        vin vestu bone, sed ne kiel <tld/>o
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiuj &ccirc;i <tld/>oj, nomataj anka&ubreve; &leftquot;parizanoj&#8221;, &ccirc;ar ili pasigis tempon en la franca
        &ccirc;efurbo Parizo, sin vestas per luksaj vesta&jcirc;oj, e&ccirc; frakoj
        <fnt>
          <bib>Monato</bib>
          <aut>Jean-Louis Madzella/pg</aut>
          <vrk><url ref="&Monato;/2006/009015.php">La stilo estas la homo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o el re&gcirc;a hundejo
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="koket.0ulo">koketulo</ref>,
      <ref cel="pav.0o.FIG">pavo<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
   <trd>&c_d;&c_e;&c_n;&c_d;&c_z;&c_ib;</trd>,
   <trd>&c_f;&c_r;&c_a;&c_n;&c_t;</trd>,
   <trd>&c_f;&c_a;&c_r;&c_s;&c_u;&c_n;</trd>
 </trdgrp>
  <trd lng="ca">dandi</trd>
  <trdgrp lng="cs">
    <trd>dandy</trd>,
    <trd>hejsek</trd>,
    <trd>nadutec</trd>,
    <trd>pan&aacute;k</trd>,
    <trd>p&aacute;sek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Dandy</trd>,
    <trd>Geck</trd>,
    <trd>Modenarr</trd>,
    <trd>Stutzer</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>dandy</trd>,
    <trd>fashionista</trd></trdgrp>
  <trdgrp lng="es">
    <trd>dandy</trd>,
    <trd>dandi</trd></trdgrp>
  <trdgrp lng="fr">
    <trd>dandy</trd>,
    <trd>&eacute;l&eacute;gant<klr tip="ind">
    (dandy)</klr></trd></trdgrp>
  <trd lng="he">&gimel;&nun;&dalet;&resh;&fnun;</trd>
  <trdgrp lng="hu">
    <trd>dendi</trd>,
    <trd>divath&odblac;s</trd>,
    <trd>piperk&odblac;c</trd>,
    <trd>fics&uacute;r</trd>
 </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12375;&#12419;&#12428;&#30007;  <pr>&#12375;&#12419;&#12428;&#12362;&#12392;&#12371;</pr></trd>,
    <trd>&#20234;&#36948;&#30007;  <pr>&#12384;&#12390;&#12362;&#12392;&#12371;</pr></trd>,
    <trd>&#12480;&#12531;&#12487;&#12451;&#12540;</trd>
  </trdgrp>
  <trdgrp lng="nl">
   <trd>dandy</trd>,
   <trd>fat</trd>
 </trdgrp>
  <trdgrp lng="pl">
   <trd>dandys</trd>,
   <trd>elegant</trd>,
   <trd>dandy<klr> (wym. dandi)</klr></trd>,
   <trd>bikiniarz</trd>,
   <trd>fircyk</trd>,
   <trd>gogu&sacute;</trd>,
   <trd>lalu&sacute;</trd>,
   <trd>modni&sacute;</trd>
 </trdgrp>
  <trdgrp lng="pt">
   <trd>d&acirc;ndi</trd>,
   <trd>metrossexual</trd>,
   <trd>almofadinha</trd>,
   <trd>janota</trd>
 </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_shch;&c_jo;&c_g;&c_o;&c_l;&c_mol;</trd>,
    <trd>&c_f;&c_r;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_d;&c_je;&c_n;&c_d;&c_i;</trd>
 </trdgrp>
  <trdgrp lng="sk">
    <trd>fi&ccaron;&uacute;r</trd>,
    <trd>svet&aacute;k</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_f;&c_r;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_d;&c_je;&c_n;&c_d;&c_ib;</trd>,
    <trd>&c_ch;&c_je;&c_p;&c_u;&c_r;&c_u;&c_n;</trd>
  </trdgrp>
</drv>

<drv mrk="dand.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>Sin <ref tip="super" cel="vest.0i.TEKS">vesti</ref> la&ubreve; la plej moderna a&ubreve; altgrada modo
    por evento a&ubreve; speciala okaza&jcirc;o:
    </dif>
    <ekz>
      la dua &mdash; juna homo, <tld/>e vestista, videble &jcirc;us sidi&gcirc;inta apud la tableto
      <fnt>
        <bib>Fundamenta Krestomatio de la lingvo Esperanto (ne Zamenhofaj partoj)</bib>
        <aut>La Dua Eldono</aut>
        <vrk><url ref="https://eo.wikipedia.org/wiki/Fundamenta_Krestomatio">Vikipedia
        Pa&gcirc;o</url></vrk>
        <lok>III Anekdotoj</lok>
      </fnt>
    </ekz>
    <ekz>
      vidi&gcirc;is sur la kontra&ubreve;a flanko de l&#8217; rivereto, je sama distanco, figuro de juna <tld/>e
      vestita sinjoro
      <fnt>
        <bib>Mirinda Amo</bib>
        <aut>H.A. Luyken</aut>
        <vrk><url ref="https://books.google.com/books?id=oqlVAAAAYAAJ">Mirinda Amo &ccirc;e Google
        Books</url></vrk>
        <lok>&Ccirc;apitro 5</lok>
      </fnt>
    </ekz>
    <ekz>
      li havis belforman viza&gcirc;on kaj estis iom <tld/>e vestita
      <fnt>
        <bib>Infanoj en Torento: Dua Libro en la Torento-trilogio</bib>
        <aut>Stellan Engholm</aut>
        <vrk><url ref="http://esperanto.davidgsimpson.com/inko/117-8.pdf">Infanoj en
        Torento</url></vrk>
        <lok>Dua Parto p.176</lok>
      </fnt>;
    </ekz>
    <ekz>
      kun <tld/>a aplombo kaj dol&ccirc;e galanta tono li komencis la sekvantan alparolon
      <fnt>
        <bib>RBa</bib>
        <lok>&Ccirc;apitro III</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_r;&c_s;&c_ib;&c_s;&c_t;&c_a;</trd>,
    <trd>&c_f;&c_r;&c_a;&c_n;&c_t;&c_a;&c_v;&c_a;&c_t;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>dandyhaft</trd>,
    <trd>geckenhaft</trd>
  </trdgrp>
  <trd lng="en">fancily (dressed)</trd>
</drv>

<drv mrk="dand.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="kondut.0i">Konduti</ref>
      kiel <tld/>o:
      <ekz>
        li tra<tld/>is en la vojo la monon
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="parad.0i">paradi</ref>,
      <ref cel="pav.0i">pavi</ref>,
      <ref cel="koket">koketi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_r;&c_s;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="ca">ser un <ind>dandi</ind></trd>
  <trd lng="cs">chovat se sv&ecaron;t&aacute;cky</trd>
  <trd lng="he">&lamed;&he;&tav;&gimel;&nun;&dalet;&resh;</trd>
  <trdgrp lng="ja">
    <trd>&#12375;&#12419;&#12428;&#30007;&#12406;&#12427;  <pr>&#12375;&#12419;&#12428;&#12362;&#12392;&#12371;&#12406;&#12427;</pr></trd>,
    <trd>&#20234;&#36948;&#12406;&#12427;  <pr>&#12384;&#12390;&#12406;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zich als een <ind>dandy</ind> gedragen</trd>,
    <trd>zich als een <ind>fat</ind> gedragen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_shch;&c_je;&c_g;&c_o;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_f;&c_r;&c_a;&c_n;&c_t;&c_i;&c_t;&c_mol;</trd>
 </trdgrp>
  <trd lng="sk">chova&tcaron; sa svet&aacute;cky</trd>
  <trdgrp lng="uk">
    <trd>&c_f;&c_r;&c_a;&c_n;&c_t;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_h;&c_i;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
</art>

