<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>dand</rad>/o
</kap>
<drv mrk="dand.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc>
      <dif>
         Viro, kies precipa zorgo estas elegante observi modon. 
      </dif>
      <refgrp tip="vid">
         <ref cel="bird.0o">birdo</ref>.
      </refgrp>
   </snc>
  <trdgrp lng="be">
     <trd>&c_d;&c_e;&c_n;&c_d;&c_z;&c_ib;</trd>, 
     <trd>&c_f;&c_r;&c_a;&c_n;&c_t;</trd>, 
     <trd>&c_f;&c_a;&c_r;&c_s;&c_u;&c_n;</trd>
   </trdgrp>
  <trd lng="ca">dandi</trd>
  <trdgrp lng="cs">
    <trd>dandy</trd>,
    <trd>hejsek</trd>,
    <trd>nadutec</trd>,
    <trd>panák</trd>,
    <trd>pásek</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Dandy</trd>,
      <trd>Geck</trd>,
      <trd>Modenarr</trd>,
      <trd>Stutzer</trd>
   </trdgrp>
  <trdgrp lng="en">
      <trd>dandy</trd>,
      <trd>fashionista</trd></trdgrp>
  <trdgrp lng="es">
      <trd>dandy</trd>,
      <trd>dandi</trd></trdgrp>
  <trdgrp lng="fr">
      <trd>dandy</trd>,
      <trd>&eacute;l&eacute;gant<klr tip="ind">
      (dandy)</klr></trd></trdgrp>
  <trdgrp lng="hu">
      <trd>dendi</trd>,
      <trd>divath&odblac;s</trd>,
      <trd>piperk&odblac;c</trd>,
      <trd>fics&uacute;r</trd>
   </trdgrp>
  <trdgrp lng="nl">
     <trd>dandy</trd>,
     <trd>fat</trd>
   </trdgrp>
  <trdgrp lng="pl">
     <trd>dandys</trd>,
     <trd>elegant</trd>,
     <trd>dandy<klr> (wym. dandi)</klr></trd>,
     <trd>bikiniarz</trd>,
     <trd>fircyk</trd>,
     <trd>gogu&sacute;</trd>,
     <trd>lalu&sacute;</trd>,
     <trd>modni&sacute;</trd>
   </trdgrp>
  <trdgrp lng="pt">
     <trd>d&acirc;ndi</trd>,
     <trd>metrossexual</trd>,
     <trd>almofadinha</trd>,
     <trd>janota</trd>
   </trdgrp>
  <trdgrp lng="ru">
      <trd>&c_shch;&c_jo;&c_g;&c_o;&c_l;&c_mol;</trd>,
      <trd>&c_f;&c_r;&c_a;&c_n;&c_t;</trd>,
      <trd>&c_d;&c_je;&c_n;&c_d;&c_i;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>fičúr</trd>,
    <trd>sveták</trd>
  </trdgrp>
</drv>

<drv mrk="dand.0i">
   <kap><tld/>i<fnt>Z</fnt></kap>
   <gra><vspec>ntr</vspec></gra>
   <snc>
      <dif>
         Konduti kiel <tld/>o. 
      </dif>
      <refgrp tip="vid">
         <ref cel="parad.0i">paradi</ref>,
         <ref cel="pav.0i">pavi</ref>,
         <ref cel="koket">koketi</ref>.
      </refgrp>
   </snc>
  <trd lng="be">&c_f;&c_a;&c_r;&c_s;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="ca">ser un <ind>dandi</ind></trd>
  <trd lng="cs">chovat se světácky</trd>
  <trdgrp lng="nl">
     <trd>zich als een <ind>dandy</ind> gedragen</trd>,
     <trd>zich als een <ind>fat</ind> gedragen</trd>
   </trdgrp>
  <trdgrp lng="ru">
      <trd>&c_shch;&c_je;&c_g;&c_o;&c_l;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_f;&c_r;&c_a;&c_n;&c_t;&c_i;&c_t;&c_mol;</trd>
   </trdgrp>
  <trd lng="sk">chovať sa svetácky</trd>
</drv>
</art>

