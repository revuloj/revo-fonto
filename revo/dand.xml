<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>dand</rad>/o
</kap>
<drv mrk="dand.0o">
<kap><ofc>*</ofc><tld/>o</kap>
<snc>
<dif>
Viro, kies precipa zorgo estas elegante observi modon. 
</dif>
<refgrp tip="vid">
  <ref cel="bird.0o">birdo</ref>.
</refgrp>
</snc>
<trdgrp lng="fr">
  <trd>dandy</trd>,
  <trd>&eacute;l&eacute;gant<klr tip="ind"> (dandy)</klr></trd></trdgrp> 
<trdgrp lng="ru">
  <trd>&c_shch;&c_jo;&c_g;&c_o;&c_l;&c_mol;</trd>,
  <trd>&c_f;&c_r;&c_a;&c_n;&c_t;</trd>,
  <trd>&c_d;&c_je;&c_n;&c_d;&c_i;</trd>
</trdgrp>
</drv>
<drv mrk="dand.0i">
<kap><tld/>i<fnt>Z</fnt></kap>
<gra><vspec>ntr</vspec></gra>
<snc>
<dif>
Konduti kiel <tld/>o. 
</dif>
<refgrp tip="vid">
<ref cel="parad.0i">paradi</ref>,
<ref cel="pav.0i">pavi</ref>,
<ref cel="koket">koketi</ref>.
</refgrp>
</snc>
<trdgrp lng="ru">
  <trd>&c_shch;&c_je;&c_g;&c_o;&c_l;&c_ja;&c_t;&c_mol;</trd>,
  <trd>&c_f;&c_r;&c_a;&c_n;&c_t;&c_i;&c_t;&c_mol;</trd>
</trdgrp>
</drv>
</art>
