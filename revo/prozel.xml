<art mrk="$Id: prozel.xml,v 1.21 2024/02/17 07:52:37 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>prozelit</rad>/o
</kap>
<drv mrk="prozel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">REL</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="konver3.0ito">Konvertito</ref>, varbito:
      <ekz>
        vi &ccirc;irka&ubreve;iras maron kaj teron, por varbi unu <tld/>on
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 23:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        multaj el la Judoj kaj el la piaj <tld/>oj sekvis Pa&ubreve;lon kaj Barnabason, kiuj,
        alparolante ilin, ur&gcirc;e admonis ilin persisti en la graco de Dio
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 13:43</lok>
        </fnt>;
      </ekz>
       <ekz>
        por akiri <tld/>ojn, la societo faras, kion &gcirc;i povas, ligante utila&jcirc;on kun
        diversaj agrabla&jcirc;oj
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="rekrut.0o.varbito">rekruto<sncref/></ref>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_z;&c_e;&c_l;&c_ib;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>novov&ecaron;rec obecn&ecaron;</trd>,
    <trd>nov&yacute; stoupenec &zcaron;idovstv&iacute;</trd>,
    <trd>prozelyta</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Proselyt</trd>,
    <trd>Bekehrter</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>proselyte</trd>,
    <trd>convert</trd>
  </trdgrp>
  <trd lng="fr">pros&eacute;lyte</trd>
  <trdgrp lng="hu">
    <trd>&aacute;tt&eacute;rt</trd>,
    <trd>prozelita</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x65B0;&#x4FE1;&#x8005;  <pr>&#x3057;&#x3093;&#x3057;&#x3093;&#x3058;&#x3083;</pr></trd>,
    <trd>&#x65B0;&#x52A0;&#x5165;&#x8005;  <pr>&#x3057;&#x3093;&#x304B;&#x306B;&#x3085;&#x3046;&#x3057;&#x3083;</pr></trd>,
    <trd>&#x6539;&#x5B97;&#x8005;  <pr>&#x304B;&#x3044;&#x3057;&#x3085;&#x3046;&#x3057;&#x3083;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>proseliet</trd>,
    <trd>bekeerling</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>prozelita</trd>,
    <trd>nowonawr&oacute;cony</trd>
  </trdgrp>
  <trd lng="pt">pros&eacute;lito</trd>
  <trdgrp lng="ro">
    <trd>prozelit</trd>,
    <trd>nou convertit</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_z;&c_je;&c_l;&c_i;&c_t;</trd>,
    <trd>&c_n;&c_o;&c_v;&c_o;&c_o;&c_b;&c_r;&c_a;&c_shch;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">prozelyta</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&#x43E;&c_z;&c_je;&#x43B;&c_ib;&c_t;</trd>,
    <trd>&#x43D;&#x43E;&c_v;&#x43E;&#x43D;&c_a;&c_v;&c_je;&c_r;&#x43D;&c_je;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="prozel.0ismo">
  <kap><tld/>ismo <fnt>Z</fnt></kap>
  <snc>
    <dif>
      Fervoro por konverti, varbi:
      <ekz>
        <tld/>ismo kuntrenas konflikton inter du rajtoj: rajto libere esprimi sian
        opiniaron kaj rajto ne ricevi instrua&jcirc;ojn nedemanditajn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=469340">Prozelitismo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_z;&c_e;&c_l;&c_ib;&c_t;&c_y;&c_z;&c_m;</trd>
  <trd lng="de">Bekehrungseifer</trd>
  <trd lng="en">proselytism</trd>
  <trd lng="fr">pros&eacute;lytisme</trd>
  <trdgrp lng="hu">
    <trd>prozelitizmus</trd>,
    <trd>hitt&eacute;r&iacute;t&odblac;i buzgalom</trd>
  </trdgrp>
  <trd lng="ja">&#x71B1;&#x5FC3;&#x306A;&#x52E7;&#x8A98; 
  <pr>&#x306D;&#x3063;&#x3057;&#x3093;&#x306A;&#x304B;&#x3093;&#x3086;&#x3046;</pr></trd>
  <trdgrp lng="nl">
    <trd>proselitisme</trd>,
    <trd>proselietenmakerij</trd>
  </trdgrp>
  <trd lng="pl">prozelityzm</trd>
  <trd lng="ro">prozelitism</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_z;&c_je;&c_l;&c_i;&c_t;&c_i;&c_z;&c_m;</trd>
  <trd lng="uk">&#x43F;&c_r;&#x43E;&c_z;&c_je;&#x43B;&c_ib;&c_t;&c_i;&c_z;&#x43C; (&#x43F;&c_r;&c_a;&c_g;&#x43D;&c_je;&#x43D;&#x43D;&#x44F;
  &#x43F;&#x43E;&c_sh;&c_i;&c_r;&c_i;&c_t;&c_i; &c_s;&c_v;&#x43E;&c_jeu; &c_v;&c_ib;&c_r;&#x43E;&c_s;&#x43F;&#x43E;&c_v;&c_ib;&c_d;&c_a;&#x43D;&#x43D;&#x44F;)</trd>
  <trdgrp lng="zh">
    <trd>&#x4F7F;&#x6539;&#x5B97;&#x6559;&#x4FE1;&#x4EF0; <pr>sh&#x1D0;g&#x1CE;iz&#x14D;ngji&#xE0;ox&#xEC;ny&#x1CE;ng</pr></trd>,
    <trd>&#x4F7F;&#x53D8;&#x8282; <pr>sh&#x1D0;bi&#xE0;nji&#xE9;</pr></trd>,
    <trd>&#x4F7F;&#x8B8A;&#x7BC0; <pr>sh&#x1D0;bi&#xE0;nji&#xE9;</pr></trd>,
    <trd>&#x6539;&#x4FE1;&#x4EF0;&#x8005; <pr>g&#x1CE;ix&#xEC;ny&#x1CE;ngzh&#x11B;</pr></trd>
  </trdgrp>
</drv>
