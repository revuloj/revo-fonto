<art mrk="$Id: prozel.xml,v 1.23 2025/09/16 06:00:21 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>prozelit</rad>/o
<fnt><bib>JED</bib></fnt></kap>
<drv mrk="prozel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">REL</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="konver3.0ito">Konvertito</ref>, varbito:
      <ekz>
        vi &ccirc;irka&ubreve;iras maron kaj teron, por varbi unu <tld/>on
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 23:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        multaj el la Judoj kaj el la piaj <tld/>oj sekvis Pa&ubreve;lon kaj Barnabason, kiuj,
        alparolante ilin, ur&gcirc;e admonis ilin persisti en la graco de Dio
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 13:43</lok>
        </fnt>;
      </ekz>
       <ekz>
        por akiri <tld/>ojn, la societo faras, kion &gcirc;i povas, ligante utila&jcirc;on kun
        diversaj agrabla&jcirc;oj
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="rekrut.0o.varbito">rekruto<sncref/></ref>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_z;&c_e;&c_l;&c_ib;&c_t;</trd>
  <trd lng="ca">pros&egrave;lit</trd>
  <trdgrp lng="cs">
    <trd>novov&ecaron;rec obecn&ecaron;</trd>,
    <trd>nov&yacute; stoupenec &zcaron;idovstv&iacute;</trd>,
    <trd>prozelyta</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Proselyt</trd>,
    <trd>Bekehrter</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>proselyte</trd>,
    <trd>convert</trd>
  </trdgrp>
  <trd lng="fr">pros&eacute;lyte</trd>
  <trdgrp lng="hu">
    <trd>&aacute;tt&eacute;rt</trd>,
    <trd>prozelita</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#26032;&#20449;&#32773;  <pr>&#12375;&#12435;&#12375;&#12435;&#12376;&#12419;</pr></trd>,
    <trd>&#26032;&#21152;&#20837;&#32773;  <pr>&#12375;&#12435;&#12363;&#12395;&#12421;&#12358;&#12375;&#12419;</pr></trd>,
    <trd>&#25913;&#23447;&#32773;  <pr>&#12363;&#12356;&#12375;&#12421;&#12358;&#12375;&#12419;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>proseliet</trd>,
    <trd>bekeerling</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>prozelita</trd>,
    <trd>nowonawr&oacute;cony</trd>
  </trdgrp>
  <trd lng="pt">pros&eacute;lito</trd>
  <trdgrp lng="ro">
    <trd>prozelit</trd>,
    <trd>nou convertit</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_z;&c_je;&c_l;&c_i;&c_t;</trd>,
    <trd>&c_n;&c_o;&c_v;&c_o;&c_o;&c_b;&c_r;&c_a;&c_shch;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">prozelyta</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_o;&c_z;&c_je;&c_l;&c_ib;&c_t;</trd>,
    <trd>&c_n;&c_o;&c_v;&c_o;&c_n;&c_a;&c_v;&c_je;&c_r;&c_n;&c_je;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="prozel.0ismo">
  <kap><tld/>ismo <fnt>Z</fnt></kap>
  <snc>
    <dif>
      Fervoro por konverti, varbi:
      <ekz>
        <tld/>ismo kuntrenas konflikton inter du rajtoj: rajto libere esprimi sian
        opiniaron kaj rajto ne ricevi instrua&jcirc;ojn nedemanditajn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=469340">Prozelitismo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_z;&c_e;&c_l;&c_ib;&c_t;&c_y;&c_z;&c_m;</trd>
  <trd lng="ca">proselitisme</trd>
  <trd lng="de">Bekehrungseifer</trd>
  <trd lng="en">proselytism</trd>
  <trd lng="es">proselitismo</trd>
  <trd lng="fr">pros&eacute;lytisme</trd>
  <trdgrp lng="hu">
    <trd>prozelitizmus</trd>,
    <trd>hitt&eacute;r&iacute;t&odblac;i buzgalom</trd>
  </trdgrp>
  <trd lng="ja">&#29105;&#24515;&#12394;&#21223;&#35480; 
  <pr>&#12397;&#12387;&#12375;&#12435;&#12394;&#12363;&#12435;&#12422;&#12358;</pr></trd>
  <trdgrp lng="nl">
    <trd>proselitisme</trd>,
    <trd>proselietenmakerij</trd>
  </trdgrp>
  <trd lng="pl">prozelityzm</trd>
  <trd lng="ro">prozelitism</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_z;&c_je;&c_l;&c_i;&c_t;&c_i;&c_z;&c_m;</trd>
  <trd lng="uk">&c_p;&c_r;&c_o;&c_z;&c_je;&c_l;&c_ib;&c_t;&c_i;&c_z;&c_m; (&c_p;&c_r;&c_a;&c_g;&c_n;&c_je;&c_n;&c_n;&c_ja;
  &c_p;&c_o;&c_sh;&c_i;&c_r;&c_i;&c_t;&c_i; &c_s;&c_v;&c_o;&c_jeu;
  &c_v;&c_ib;&c_r;&c_o;&c_s;&c_p;&c_o;&c_v;&c_ib;&c_d;&c_a;&c_n;&c_n;&c_ja;)</trd>
  <trdgrp lng="zh">
    <trd>&#20351;&#25913;&#23447;&#25945;&#20449;&#20208; <pr>sh&#464;g&#462;iz&#333;ngji&agrave;ox&igrave;ny&#462;ng</pr></trd>,
    <trd>&#20351;&#21464;&#33410; <pr>sh&#464;bi&agrave;nji&eacute;</pr></trd>,
    <trd>&#20351;&#35722;&#31680; <pr>sh&#464;bi&agrave;nji&eacute;</pr></trd>,
    <trd>&#25913;&#20449;&#20208;&#32773; <pr>g&#462;ix&igrave;ny&#462;ngzh&ecaron;</pr></trd>
  </trdgrp>
</drv>
