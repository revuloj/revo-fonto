<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>higroskop</rad>/o
</kap>
<drv mrk="higros.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      Instrumento, permesanta proksimume taksi la aeran malsekecon per
      facile videbla efiko de tiu malsekeco (&scirc;an&gcirc;o de koloro,
      de dimensioj ks).
    </dif>
    <refgrp tip="vid">
      <ref cel="higrom.0o">higrometro</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Hygrometer</trd>,
    <trd>Feuchtigkeitsmeter</trd>,
    <trd>Feuchtigkeitsmesser</trd>
  </trdgrp>
  <trd lng="el">&ypsilon;&gamma;&rho;&omikron;&sigma;&kappa;&omikron_ton;&pi;&jota;&omikron;</trd>
  <trd lng="fr">hygroscope</trd>
  <trdgrp lng="nl">
    <trd>hygroscoop</trd>,
    <trd>vochtigheidsmeter</trd>
  </trdgrp>
  <trd lng="pl">higroskop</trd>
  <trd lng="pt">higrosc&oacute;pio</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_i;&c_g;&c_r;&c_o;&c_s;&c_k;&c_o;&c_p;</trd>,
    <trd>&c_g;&c_i;&c_g;&c_r;&c_o;&c_m;&c_je;&c_t;&c_r;</trd>
  </trdgrp>
</drv>
</art>
