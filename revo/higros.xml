<art mrk="$Id: higros.xml,v 1.19 2023/11/15 17:08:00 revo Exp $">
<kap>
  <rad>higroskop</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="higros.0o">
  <kap><tld/>o</kap>
  <snc mrk="higros.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Instrumento, permesanta proksimume taksi la aeran malsekecon per
      facile videbla efiko de tiu malsekeco (&scirc;an&gcirc;o de koloro,
      de dimensioj ks):
      <ekz>
        seka folio de papero eble enhavas 10%
        da akvo en <klr>...</klr> <tld/>a, nepalpebla formo 
        <fnt>
          <aut>W. F. Rolt</aut>,
          <vrk>El la Polvo de la Tero</vrk>,
          <lok>1967</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="higrom.0o">higrometro</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_g;&c_r;&c_a;&c_s;&c_k;&c_o;&c_p;</trd>
  <trd lng="cs">hygroskop</trd>
  <trdgrp lng="de">
    <trd>Hygrometer</trd>,
    <trd>Feuchtigkeitsmeter</trd>,
    <trd>Feuchtigkeitsmesser</trd>
  </trdgrp>
  <trd lng="el">&ypsilon;&gamma;&rho;&omikron;&sigma;&kappa;&omikron_ton;&pi;&jota;&omikron;</trd>
  <trd lng="fr">hygroscope</trd>
  <trdgrp lng="hu">
    <trd>l&eacute;gnedvess&eacute;gjelz&odblac;</trd>,
    <trd>higroszk&oacute;p</trd>
  </trdgrp>
  <trd lng="it">igroscopio</trd>
  <trd lng="ja">&#x691C;&#x6E7F;&#x5668;  <pr>&#x3051;&#x3093;&#x3057;&#x3064;&#x304D;</pr></trd>
  <trdgrp lng="nl">
    <trd>hygroscoop</trd>,
    <trd>vochtigheidsmeter</trd>
  </trdgrp>
  <trd lng="pl">higroskop</trd>
  <trd lng="pt">higrosc&oacute;pio</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_i;&c_g;&c_r;&c_o;&c_s;&c_k;&c_o;&c_p;</trd>,
    <trd>&c_g;&c_i;&c_g;&c_r;&c_o;&c_m;&c_je;&c_t;&c_r;</trd>
  </trdgrp>
  <trd lng="sk">hygroskop</trd>
  <trd lng="uk">&#x433;&#x456;&#x433;&#x440;&#x43E;&#x441;&#x43A;&#x43E;&#x43F;</trd>
  <trd lng="zh">&#x6E7F;&#x5EA6;&#x8BA1; <pr>sh&#x12B;d&#xF9;j&#xEC;</pr></trd>
</drv>
</art>
