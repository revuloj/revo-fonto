<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>violon&ccirc;el</rad>/o
</kap>
<drv mrk="violon1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Granda <ref tip="super" cel="violon.0o">violono</ref> malsupera je
      unu kvinto je la aldo:
      <ekz>
        de la domo alflugis al ili malrapidaj sonoj de <tld/>o
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 9a, p.
45a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd
lng="be">&c_v;&c_ib;&c_ja;&c_l;&c_ja;&c_n;&c_ch;&c_e;&c_l;&c_mol;</trd>
  <trd lng="de">Cello</trd>
  <trd
lng="el">&beta;&jota;&omikron;&lambda;&omikron;&ny;&tau;&sigma;&epsilon_ton;&lambda;&omikron;</trd>
  <trd lng="en">cello</trd>
  <trdgrp lng="fr">
    <trd>violoncelle</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csell&oacute;</trd>,
    <trd>gordonka</trd>
  </trdgrp>
  <trd lng="nl">cello</trd>
  <trd lng="pt">violoncelo</trd>
  <trd lng="ru">&c_v;&c_i;&c_o;&c_l;&c_o;&c_n;&c_ch;&c_je;&c_l;&c_mol;</trd>
</drv>
</art>
