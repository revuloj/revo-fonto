<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>koriandr</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="korian.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="api.0acoj">apiacoj</ref>
      (<trd lng="la">Coriandrum</trd>), kies globetformaj fruktoj
      estas uzataj en preparado de kelkaj likvoroj, parfumoj kaj kiel
      spica&jcirc;o:
      <ekz>
        la manao estis kiel semo de <tld/>o
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Nombroj 11, 7</lok>)</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Koriander</trd>
  <trd lng="en">coriander</trd>
  <trd lng="fr">coriandre</trd>
  <trdgrp lng="ru">
        <trd>&c_k;&c_i;&c_n;&c_z;&c_a;</trd>,
        <trd>&c_k;&c_o;&c_r;&c_i;&c_a;&c_n;&c_d;&c_r;</trd></trdgrp>
</drv>
</art>
