<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: korian.xml,v 1.26 2023/12/03 09:58:58 revo Exp $">
<kap>
  <rad>koriandr</rad>/o <fnt><bib>MT</bib></fnt>
</kap>
<drv mrk="korian.0o">
  <kap>
  <tld/>o
  </kap>
  <snc mrk="korian.0o.genro">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">Coriandrum</trd>), kies globetformaj fruktoj
      estas uzataj en preparado de kelkaj likvoroj, parfumoj kaj kiel
      spico:
      <ekz>
        &gcirc;i estis kiel la semo de <tld/>o, blanka
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 16:31</lok>
        </fnt>;
      </ekz>
      <ekz>
        la manao estis kiel semo de <tld/>o
        <fnt><bib>MT</bib>, <lok>Nombroj 11:7</lok></fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/f/f6/Illustration_Coriandrum_sativum0.jpg/381px-Illustration_Coriandrum_sativum0.jpg" prm="PD">botanika tabulo de <tld/>o (Coriandrum sativum)
        <fnt>
          <aut>Prof. Dr. Otto Wilhelm Thom&eacute;, www.biolib.de Original book source: Prof. Dr. Otto
          Wilhelm Thom&eacute; Flora von Deutschland, &Ouml;sterreich und der Schweiz 1885, Gera, Germany</aut>
          <lok><url ref="&WCW;/File:Illustration_Coriandrum_sativum0.jpg">&WCW;/File:Illustration_Coriandrum_sativum0.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <snc mrk="korian.0o.spico">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="lst" cel="spic.0o" lst="voko:spicoj">Spico</ref> el la
      folioj a&ubreve; la sekigitaj fruktoj de la
      <ref tip="malprt" cel="korian.0o.genro"><tld/>o<sncref/></ref>:
      <ekz>
        fortaj gustiga&jcirc;oj kiel <tld/>o kaj kuracao estigis al la tritikaj bierspecoj
        herban kaj fre&scirc;an, acid-dol&ccirc;an guston
        <fnt>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="&Monato;/2001/007144.php">Bierlando Belgio</url></vrk>
          <bib>Monato</bib>
          <lok>2001/01, p. 18</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_ja;&c_n;&c_d;&c_r;&c_a;</trd>
  <trd lng="de">Koriander</trd>
  <trdgrp lng="en">
    <trd>coriander</trd>,
    <trd>cilantro</trd>
    <trd>Chinese parsley</trd>,
    <trd>dhania</trd>
  </trdgrp>
  <trd lng="es">cilantro</trd>
  <trd lng="fa">&f_g;&a_sh;&a_n;&f_y;&a_z;</trd>
  <trd lng="fr">coriandre</trd>
  <trdgrp lng="hu">
    <trd>koriander</trd>,
    <trd>cig&aacute;nypetrezselyem</trd>
  </trdgrp>
  <trd lng="id">ketumbar</trd>
  <trdgrp lng="ja">
    <trd>&#x30B3;&#x30A8;&#x30F3;&#x30C9;&#x30ED;</trd>,
    <trd>&#x30B3;&#x30EA;&#x30A2;&#x30F3;&#x30C0;&#x30FC;</trd>
  </trdgrp>
  <trd lng="pl">kolendra</trd>
  <trdgrp lng="pt">
    <trd>coentro</trd>,
    <trd>coriandro</trd>
  </trdgrp>
  <trd lng="ro">coriandrul</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_n;&c_z;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_i;&c_a;&c_n;&c_d;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9999;&#x83DC; <pr>xi&#x101;ngc&#xE0;i</pr></trd>,
    <trd>&#x837D; <pr>su&#x12B;</pr></trd>,
    <trd>&#x80E1;&#x837D; <pr>h&#xFA;su&#x12B;</pr></trd>,
    <trd>&#x82AB;&#x837D; <pr>y&#xE1;nsui</pr></trd>,
    <trd>&#x80E1;&#x82AB; <pr>h&#xFA;yu&#xE1;n</pr></trd>,
    <trd>&#x9999;&#x837D; <pr>xi&#x101;ngsu&#x12B;</pr></trd>
  </trdgrp>
</drv>
</art>
