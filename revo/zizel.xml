<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>zizel</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="zizel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="zizel.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Eta ron&gcirc;ulo el stepoj de Orienta E&ubreve;ropo &gcirc;is Siberio
      grupe lo&gcirc;anta en subteraj fosa&jcirc;oj;
      <ref tip="super" cel="genr.0o.BIO">genro</ref> el familio 
      <ref tip="malprt" cel="sciure.0oj">sciuredoj</ref>
      (<trd lng="la">Citellus</trd>):
      <ekz>
        <tld/>o <klr>...</klr> bezonas stepajn biotopojn,
        kie estas nur malalta herbo
        <fnt>
          <aut>O. Arno&scaron;t Fischer</aut>,
          <vrk><url
          ref="http://www.ipernity.com/doc/192647/album/244567?view=1">
          Mambestoj</url></vrk>,
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_s;&c_l;&c_ib;&c_k;</trd>
  <trd lng="bg">&c_l;&c_a;&c_l;&c_u;&c_g;&c_je;&c_r;</trd>
  <trdgrp lng="ca">
    <trd>g&egrave;nere Citellus</trd>,
    <trd>suslic</trd>
  </trdgrp>
  <trd lng="de">Ziesel</trd>
  <trd lng="en">suslik</trd>
  <trdgrp lng="fr">
    <trd>souslik</trd>,
    <trd>spermophile</trd>
  </trdgrp>
  <trd lng="hu">&uuml;rge</trd>
  <trd lng="nl">Syberische <ind>marmot</ind></trd>
  <trd lng="pt">zizel</trd>
  <trd lng="ru">&c_s;&c_u;&c_s;&c_l;&c_i;&c_k;</trd>
  <trd lng="sv">sisel</trd>
  <trdgrp lng="it">
    <trd>citello</trd>,
    <trd>spermophilus</trd>
  </trdgrp>
</drv>
</art>
