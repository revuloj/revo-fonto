<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>zizel</rad>/o
</kap>
<drv mrk="zizel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="zizel.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="sciure.0oj">sciuredoj</ref>
      (<trd lng="la">Citellus</trd>), etaj ron&gcirc;uloj el stepoj de
      Orienta
      E&ubreve;ropo &gcirc;is Siberio grupe lo&gcirc;antaj en subteraj fosa&jcirc;oj.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_s;&c_l;&c_ib;&c_k;</trd>
  <trd lng="de">Ziesel</trd>
  <trd lng="en">suslik</trd>
  <trdgrp lng="fr">
    <trd>souslik</trd>,
    <trd>spermophile</trd>
  </trdgrp>
  <trd lng="hu">&uuml;rge</trd>
  <trd lng="nl">Syberische <ind>marmot</ind></trd>
  <trd lng="pt">zizel</trd>
  <trd lng="ru">&c_s;&c_u;&c_s;&c_l;&c_i;&c_k;</trd>
  <trd lng="sv">sisel</trd>
</drv>
</art>
