<drv mrk="grand.0igi">
  <kap>
    <tld/>igi
  </kap>
  <snc mrk="grand.0igi.dimensie">
    <dif>
      <ref cel="kresk.0igi.pligrandigi">Kreskigi</ref> la&ubreve;dimensie:
      <ekz>
        ili <klr>[&#8230;]</klr> <tld/>igas la fran&gcirc;ojn de siaj vestoj
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 23:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvaza&ubreve; per <klr>[&#8230;]</klr> via komercado vi <tld/>igus vian ri&ccirc;eco
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 28:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igi sian regnon
        <fnt>
          <bib>Ifigenio</bib>
          <lok>Ifigenio en Ta&ubreve;rido</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="skal.0i">skali</ref>
  </snc>
  <snc mrk="grand.0igi.grade">
    <dif>
      Kreskigi la&ubreve;grade, la&ubreve;intense, la&ubreve;kvalite:
      <ekz>
        Mi benos vin kaj <tld/>igos vian nomon
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 12:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        la klereco de la oratoro <tld/>igas lian a&ubreve;toritaton
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>igis la severecon de sia vidvineco &gcirc;is la plej ekstremaj limoj
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dektria</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">zu <ind>Gr&ouml;&szlig;e</ind> verhelfen</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_v;&c_ja;&c_l;&c_ib;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
