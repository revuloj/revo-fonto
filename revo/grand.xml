  <trd lng="bg">&c_m;&c_a;&c_l;&c_malmol;&c_k;</trd> 
  <trd lng="bo">&#xF46;&#xF74;&#xF44;&#xF0B;&#xF46;&#xF74;&#xF44;</trd>
  <trdgrp lng="ca">
    <trd>baix</trd>,
    <trd>pla</trd>,
    <trd>trivial</trd>
  </trdgrp>
  <trd lng="cs">mal&yacute;</trd>
  <trd lng="de">klein</trd>
  <trd lng="el">&my;&jota;&kappa;&rho;&omikron_ton;</trd>
  <trdgrp lng="en"> 
    <trd>little</trd>, 
    <trd>small</trd> 
  </trdgrp>
  <trdgrp lng="es"> 
    <trd>peque&ntilde;o</trd>, 
    <trd>chico</trd>, 
    <trd>peque&ntilde;a</trd>, 
    <trd>chica</trd> 
  </trdgrp>
  <trd lng="fr">petit</trd>
  <trd lng="hu">kicsi</trd>
  <trd lng="ja">&#x5C0F;&#x3055;&#x3044; [ 
  <ind>&#x3061;&#x3044;&#x3055;&#x3044;</ind>]</trd>
  <trd lng="lat">parvus</trd>
  <trd lng="ne">&#x938;&#x93E;&#x928;&#x93E;</trd>
  <trd lng="nl">klein</trd>
  <trd lng="pl">ma&lstroke;y</trd>
  <trdgrp lng="ru"> 
    <trd>&c_m;&c_a;&c_l;&c_y;&c_j;</trd>, 
    <trd>&c_m;&c_a;&c_l;&c_je;&c_n;&c_mol;&c_k;&c_i;&c_j;</trd> 
  </trdgrp>
  <trd lng="sl">majhen</trd>
  <trd lng="sk">mal&yacute;</trd>
  <trd lng="sv">liten</trd>
  <trd lng="tp">lili</trd>
  <trd lng="vo">smalik</trd>
  <trd lng="zh">&#x5C0F;<klr> [xi&#x1CE;o]</klr></trd>
