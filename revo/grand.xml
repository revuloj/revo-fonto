<?xml version="1.0"?> 
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd"> 

<vortaro> 
<art mrk="$Id$"> 
<kap> 
  <ofc>*</ofc> 
  <rad>grand</rad>/a <fnt><bib>PV</bib></fnt> 
</kap> 
<drv mrk="grand.0a"> 
  <kap><ofc>*</ofc><tld/>a</kap> 
  <snc mrk="grand.0a.dimensio"> 
    <dif> 
      Superanta la ordinarajn dimensiojn: 
      <ekz> 
        <tld/>a <klr>(vasta)</klr> tablo, vazo, &ccirc;ambro, kampo, 
        muro; 
      </ekz> 
      <ekz> 
        <tld/>a <klr>(alta)</klr> monto, turo, homo, besto; 
      </ekz> 
      <ekz> 
        <tld/>a <klr>(longa)</klr> rivero, bastono, da&ubreve;ro; 
      </ekz> 
      <ekz> 
        fi&scirc;o pli <tld/>a mal<tld/>an englutas 
        <fnt><bib>PrV</bib></fnt>; 
      </ekz> 
      <ekz> 
        <tld/>a telero, malplena kulero<fnt><bib>PrV</bib></fnt>; 
      </ekz> 
      <ekz> 
        <tld/>a kapitalo. 
      </ekz> 
    </dif> 
    <refgrp tip="vid"> 
      <ref cel="amplek">ampleksa</ref>, 
      <ref cel="elefan">elefanta</ref>, 
      <ref cel="gigant.0a">giganta</ref>, 
      <ref cel="kolos.0a">kolosa</ref> 
    </refgrp> 
          <trdgrp lng="pl"> 
            <trd>wielki</trd>, 
            <trd>du&zdot;y</trd> 
          </trdgrp> 
  </snc> 
  <snc mrk="grand.0a.grado"> 
    <dif> 
      Superanta la ordinaran gradon, intensa, altkvalita: 
      <ekz> 
        <tld/>a vento, kunveno, kvanto da akvo; 
      </ekz> 
      <ekz> 
        <tld/>a poeto, verko, talento; 
      </ekz> 
      <ekz> 
        Ludoviko, Frederiko la <tld/>a; 
      </ekz> 
      <ekz> 
        <tld/>a doloro, kura&gcirc;o, gloro, &gcirc;ojo kolero, 
        feli&ccirc;o, fiereco, honto, miro, profito, ri&ccirc;eco, 
        valoro, venko, malvenko; 
      </ekz> 
      <ekz> 
        <tld/>aj malbonoj, <tld/>aj rimedoj<fnt><bib>PrV</bib></fnt>; 
      </ekz> 
      <ekz> 
        <tld/>a ofico, <tld/>aj zorgoj<fnt><bib>PrV</bib></fnt>. 
      </ekz> 
    </dif> 
    <refgrp tip="vid"> 
      <ref cel="eminen1.0a">eminenta</ref>, 
      <ref cel="fort.0a">forta</ref>, 
      <ref cel="grav.0a">grava</ref>, 
      <ref cel="potenc.0a">potenca</ref>, 
      <ref cel="tro.0a">troa</ref> 
    </refgrp> 
          <trdgrp lng="pl"> 
            <trd>wielki</trd> 
          </trdgrp> 
    <trd lng="pt">gra&uacute;do<klr tip="ind"> (adj.)</klr></trd> 
  </snc> 
  <trdgrp lng="be"> 
    <trd>&c_v;&c_ja;&c_l;&c_ib;&c_k;&c_ib;</trd>, 
    <trd>&c_b;&c_u;&c_j;&c_n;&c_y;</trd>, 
    <trd>&c_v;&c_je;&c_l;&c_ib;&c_z;&c_a;&c_r;&c_n;&c_y;</trd> 
  </trdgrp> 
  <trdgrp lng="bg"> 
    <trd>&c_v;&c_i;&c_s;&c_o;&c_k;</trd>, 
    <trd>&c_g;&c_o;&c_l;&c_ja;&c_m;</trd>, 
    <trd>&c_v;&c_je;&c_l;&c_i;&c_k;</trd> 
  </trdgrp> 
  <trd lng="cs">velk&yacute;</trd> 
  <trd lng="de">gro&szlig;</trd> 
  <trdgrp lng="en"> 
    <trd>big</trd>, 
    <trd>large</trd> 
  </trdgrp> 
  <trd lng="es">grande</trd> 
  <trd lng="fr">grand</trd> 
  <trd lng="ja">&#22823;&#12365;&#12356; [ 
  <ind>&#12362;&#12362;&#12365;&#12356;</ind>]</trd> 
  <trd lng="nl">groot</trd> 
  <trd lng="pt">grande</trd> 
  <trdgrp lng="ru"> 
    <trd>&c_b;&c_o;&c_l;&c_mol;&c_sh;&c_o;&c_j;</trd>, 
    <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_i;&c_j;</trd> 
  </trdgrp> 
  <trd lng="sv">stor</trd> 
  <trd lng="tp">suli</trd> 
  <trd lng="vo">gletik</trd> 
  <trd lng="zh">&#22823;<klr> [d&agrave;]</klr></trd> 
</drv> 

<drv mrk="grand.0o"> 
  <kap><tld/>o</kap> 
  <snc mrk="grand.0o.abstrakta"> 
    <dif> 
      Tio, kio estas <tld/>a, rigardata en abstrakta, absoluta maniero: 
      <ekz> 
        <klr>[Schiller : ]</klr> la poeto de noblaj virinaj koroj, 
        <klr>[...]</klr> la prikantanto de la <tld/>o kaj belo 
        <fnt><bib>Fab4</bib>, <lok>Malnova pre&gcirc;eja sonorilo</lok> 
        </fnt>. 
      </ekz> 
    </dif> 
    <trd lng="pt">grandeza</trd> 
  </snc> 
  <snc mrk="grand.0o.FIZ"> 
    <uzo tip="fak">FIZ</uzo> 
    <ref tip="dif" cel="grand.fizika0o">fizika <tld/>o</ref> 
    <trd lng="pt">grandeza<klr tip="amb"> (f&iacute;sica)</klr></trd> 
  </snc> 
  <trd lng="be">&c_v;&c_je;&c_l;&c_ib;&c_ch;&c_y;&c_n;&c_ja;</trd> 
  <trd lng="bg">&c_g;&c_o;&c_l;&c_je;&c_m;&c_i;&c_n;&c_a;</trd> 
  <trd lng="de">Gr&ouml;&szlig;e</trd> 
  <trd lng="en">magnitude</trd> 
  <trd lng="fr">grandeur</trd> 
  <trd lng="nl">grootte</trd> 
  <trd lng="pl">wielko&sacute;&cacute;</trd> 
  <trd lng="ru">&c_v;&c_je;&c_l;&c_i;&c_ch;&c_i;&c_n;&c_a;</trd> 
  <trd lng="vo">glet</trd> 
</drv> 

<drv mrk="grand.0igxi"> 
  <kap><tld/>i&gcirc;i</kap> 
  <snc> 
    <dif> 
      Kreski. 
    </dif> 
  </snc> 
  <trd lng="be"> 
  &c_p;&c_a;&c_v;&c_ja;&c_l;&c_ib;&c_ch;&c_v;&c_a;&c_c;&c_c;&c_a;</trd> 
  <trd lng="de">wachsen</trd> 
  <trd lng="en">grow</trd> 
  <trdgrp lng="fr"> 
    <trd>augmenter</trd>, 
    <trd>croitre</trd>, 
    <trd>grandir<klr> (intr.)</klr></trd> 
  </trdgrp> 
  <trd lng="nl">groeien</trd> 
  <trdgrp lng="pl"> 
    <trd>powi&eogonek;ksza&cacute; si&eogonek;</trd>, 
    <trd>rosn&aogonek;&cacute;</trd>, 
    <trd>urasta&cacute;</trd> 
  </trdgrp> 
  <trd lng="pt">crescer</trd> 
  <trd lng="ru"> 
  &c_u;&c_v;&c_je;&c_l;&c_i;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;
</trd> 
  <trd lng="sv">v&auml;xa</trd> 
</drv> 

<drv mrk="grand.0egulo"> 
  <kap><tld/>egulo</kap> 
  <snc> 
    <ref tip="dif" cel="gigant.0o">giganto</ref> 
  </snc> 
  <trdgrp lng="be"> 
    <trd>&c_v;&c_o;&c_l;&c_a;&c_t;</trd>, 
    <trd>&c_g;&c_ib;&c_g;&c_a;&c_n;&c_t;</trd> 
  </trdgrp> 
  <trd lng="de">Riese</trd> 
  <trd lng="es">gigante</trd> 
  <trd lng="fr">g&eacute;ant</trd> 
  <trd lng="nl">reus</trd> 
  <trdgrp lng="pl"> 
    <trd>wielkolud</trd>, 
    <trd>olbrzym</trd> 
  </trdgrp> 
  <trd lng="pt">gigante<klr tip="ind"> (subst.)</klr></trd> 
  <trdgrp lng="ru"> 
    <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_a;&c_n;</trd>, 
    <trd>&c_g;&c_i;&c_g;&c_a;&c_n;&c_t;</trd> 
  </trdgrp> 
  <trd lng="vo">gletel</trd> 
</drv> 

<drv mrk="grand.0ulo"> 
  <kap><tld/>ulo</kap> 
  <snc mrk="grand.0ulo.nobelo"> 
    <ref tip="dif" cel="sinjor.grand0o">grandsinjoro</ref> 
  </snc> 
  <trdgrp lng="fr"> 
    <trd>grand<klr> (aristocrate)</klr></trd> 
  </trdgrp> 
  <trd lng="nl">grand seigneur</trd> 
  <trd lng="pl"><klr>kto&sacute; </klr>wielki</trd> 
  <trd lng="ru">&c_g;&c_r;&c_a;&c_n;&c_d;</trd> 
</drv> 

<drv mrk="grand.mal0a"> 
  <kap>mal<tld/>a</kap> 
  <snc mrk="grand.mal0a.dimensio"> 
    <dif> 
      Ne atinganta la ordinarajn dimensiojn: 
      <ekz> 
        mal<tld/>a arbo, skatolo. 
      </ekz> 
    </dif> 
    <refgrp tip="vid"> 
      <ref cel="et.0a">eta</ref>, 
      <ref cel="lilipu.0a">liliputa</ref>, 
      <ref cel="mikros.0a">mikroskopa</ref>, 
      <ref cel="miniat">miniatura</ref>. 
    </refgrp> 
    <trdgrp lng="pt"> 
     <trd>pequeno<klr tip="ind"> (adj.)</klr></trd>, 
     <trd>mi&uacute;do</trd> 
    </trdgrp> 
  </snc> 
  <snc mrk="grand.mal0a.grado"> 
    <dif> 
      Malaltgrada, malaltkvalita: 
      <ekz> 
        mal<tld/>a animo, bruo. 
      </ekz> 
    </dif> 
    <refgrp tip="vid"> 
      <ref cel="bagate">bagatela</ref> 
    </refgrp> 
    <trd lng="pt">vil</trd> 
  </snc> 
  <trdgrp lng="be"> 
    <trd>&c_m;&c_a;&c_l;&c_y;</trd>, 
    <trd>&c_m;&c_a;&c_l;&c_je;&c_n;&c_mol;&c_k;&c_ib;</trd>, 
    <trd>&c_d;&c_r;&c_o;&c_b;&c_n;&c_y;</trd> 
  </trdgrp> 
  <trd lng="de">klein</trd> 
  <trdgrp lng="en"> 
    <trd>little</trd>, 
    <trd>small</trd> 
  </trdgrp> 
  <trd lng="es">peque&ntilde;o</trd> 
  <trd lng="fr">petit</trd> 
  <trd lng="ja">&#23567;&#12373;&#12356; [ 
  <ind>&#12385;&#12356;&#12373;&#12356;</ind>]</trd> 
  <trd lng="nl">klein</trd> 
  <trd lng="pl">ma&lstroke;y</trd> 
  <trdgrp lng="ru"> 
    <trd>&c_m;&c_a;&c_l;&c_y;&c_j;</trd>, 
    <trd>&c_m;&c_a;&c_l;&c_je;&c_n;&c_mol;&c_k;&c_i;&c_j;</trd> 
  </trdgrp> 
  <trd lng="sv">liten</trd> 
  <trd lng="tp">lili</trd> 
  <trd lng="vo">smalik</trd> 
  <trd lng="zh">&#23567;<klr> [xi&#462;o]</klr></trd> 
</drv> 

<drv mrk="grand.mal0egulo"> 
  <kap>mal<tld/>egulo</kap> 
  <snc> 
    <dif> 
      Tre malaltkreska homo. 
    </dif> 
    <refgrp tip="vid"> 
      <ref cel="et.0ulo">etulo</ref>, 
      <ref cel="nan.0o">nano</ref>, 
      <ref cel="pigme.0o">pigmeo</ref> 
    </refgrp> 
  </snc> 
  <trd lng="be">&c_k;&c_a;&c_r;&c_l;&c_ib;&c_k;</trd> 
  <trd lng="de">Zwerg</trd> 
  <trd lng="es">enano</trd> 
  <trd lng="fr">nain</trd> 
  <trd lng="ja">&#23567;&#20154; [<ind>&#12371;&#12403;&#12392;</ind>] 
  </trd> 
  <trd lng="nl">dwerg</trd> 
  <trdgrp lng="pl"> 
    <trd>karze&lstroke;</trd>, 
    <trd>liliput</trd>, 
    <trd>maluch</trd>, 
    <trd>kurdupel<klr> (obra&zacute;liwie)</klr></trd>, 
    <trd>konus<klr> (przest.)</klr></trd> 
  </trdgrp> 
  <trd lng="pt">an&atilde;o</trd> 
  <trd lng="ru">&c_k;&c_a;&c_r;&c_l;&c_i;&c_k;</trd> 
  <trd lng="sv">dv&auml;rg</trd> 
</drv> 

<drv mrk="grand.mez0a"> 
  <kap>mez<tld/>a</kap> 
  <snc mrk="grand.mez0a.KOMUNE"> 
    <dif> 
      Inter <tld/>a kaj mal<tld/>a, modere <tld/>a: 
      <ekz> 
        lia patro posedis mez<tld/>an ferfabrikon 
        <fnt> 
          <aut>Kurisu K.</aut>, 
          <vrk>Debuti</vrk>, 
          <bib>Monato</bib>, 
          <lok>jaro 2002a, numero 2a</lok> 
        </fnt>. 
      </ekz> 
    </dif> 
  </snc> 
  <trd lng="fr">moyen<klr> (de grandeur moyenne)</klr></trd> 
  <trdgrp lng="pl"> 
    <trd>&sacute;redni</trd>, 
    <trd>&sacute;rednio du&zdot;y</trd> 
  </trdgrp> 
</drv> 

<drv mrk="grand.po0e"> 
  <kap>po<tld/>e</kap> 
  <uzo tip="klr">(a&ccirc;eti, vendi)</uzo> 
  <snc mrk="grand.po0e.EKON"> 
    <uzo tip="fak">EKON</uzo> 
    <dif> 
      La&ubreve; granda kvanto. 
    </dif> 
    <refgrp tip="sin"> 
      <ref cel="detal.mal0e.EKON">maldetale</ref> 
    </refgrp> 
  </snc> 
  <trdgrp lng="be"> 
    <trd>&c_g;&c_u;&c_r;&c_t;&c_o;&c_m;</trd>, 
    <trd>&c_o;&c_p;&c_t;&c_a;&c_m;</trd> 
  </trdgrp> 
  <trd lng="en">wholesale</trd> 
  <trdgrp lng="es"> 
    <trd>al por mayor</trd> 
    <trd>por <ind>mayoreo</ind></trd> 
  </trdgrp> 
  <trd lng="fr">en <ind>gros</ind></trd> 
  <trd lng="it">all'<ind>ingrosso</ind></trd> 
  <trd lng="nl">in het <ind>groot</ind></trd> 
  <trdgrp lng="pl"> 
    <trd>hurtem</trd>, 
    <trd>hurtowo</trd> 
  </trdgrp> 
  <trd lng="pt">por <ind>atacado</ind></trd> 
  <trd lng="ru">&c_o;&c_p;&c_t;&c_o;&c_m;</trd> 
</drv> 

<drv mrk="grand.pomal0e"> 
  <kap>pomal<tld/>e</kap> 
  <uzo tip="klr">(a&ccirc;eti, vendi)</uzo> 
  <snc mrk="grand.pomal0e.EKON"> 
    <uzo tip="fak">EKON</uzo> 
    <dif> 
      La&ubreve; malgranda kvanto, ordinare inter 
      provizanto kaj fina konsumanto. 
    </dif> 
    <refgrp tip="sin"> 
      <ref cel="detal.0e.EKON">detale</ref> 
    </refgrp> 
  </snc> 
  <trdgrp lng="be"> 
    <trd>&c_u; &c_r;&c_a;&c_z;&c_d;&c_r;&c_o;&c_b;</trd>, 
    <trd>&c_u; &c_r;&c_o;&c_z;&c_n;i&c_c;&c_u;</trd> 
  </trdgrp> 
  <trdgrp lng="en"> 
    <trd>retail</trd> 
    <trd>at <ind>retail</ind></trd> 
    <trd>in small quantities</trd> 
  </trdgrp> 
  <trdgrp lng="es"> 
    <trd>al <ind>por menor</ind></trd> 
    <trd>por <ind>menoreo</ind></trd> 
  </trdgrp> 
  <trd lng="fr">au <ind>d&eacute;tail</ind></trd> 
  <trd lng="nl">in het <ind>klein</ind></trd> 
  <trdgrp lng="pl"> 
    <trd>detalicznie</trd>, 
    <trd>w <ind>detalu</ind></trd> 
  </trdgrp> 
  <trdgrp lng="pt"> 
   <trd>a <ind>varejo</ind></trd>, 
   <trd>a <ind>retalho</ind></trd> 
  </trdgrp> 
  <trd lng="ru">&c_v; &c_r;&c_o;&c_z;&c_n;&c_i;&c_c;&c_u;</trd> 
</drv> 

<drv mrk="grand.fizika0o"> 
  <kap>fizika <tld/>o</kap> 
  <tezrad fak="FIZ"/> 
  <snc mrk="grand.fizika0o.FIZ"> 
    <uzo tip="fak">FIZ</uzo> 
    <dif> 
      Eco de fizika sistemo, esprimebla per pli-malpli <tld/>a 
      nombro a&ubreve; vektoro: 
      <ekz> 
        longo, maso, tempo estas bazaj fizikaj <tld/>oj. 
      </ekz> 
    </dif> 
  </snc> 
  <trd lng="fr">grandeur physique</trd> 
  <trd lng="nl">fysieke <ind>grootheid</ind></trd> 
  <trd lng="pl"><ind>wielko&sacute;&cacute;</ind> fizyczna</trd> 
  <trd lng="pt">grandeza f&iacute;sica</trd> 
  <trd lng="ru">&c_f;&c_i;&c_z;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja; 
  <ind>&c_v;&c_je;&c_l;&c_i;&c_ch;&c_i;&c_n;&c_a;</ind></trd> 
</drv> 
