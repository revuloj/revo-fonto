<drv mrk="grand.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="grand.0igxi.dimensie">
    <dif>
      <ref tip="sin" cel="kresk.0i">Kreski</ref> la&ubreve;dimensie:
      <ekz>
        la knaboj <tld/>i&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 25:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ri&ccirc;eco de la maljuna fra&ubreve;lino pli kaj pli <tld/>i&gcirc;is
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        la blua strieto en la sudo proksimi&gcirc;as kaj <tld/>i&gcirc;as
        <fnt>
          <bib>Tor</bib>
          <lok>V.</lok>
        </fnt>;
      </ekz>
      <ekz>
        kia naski&gcirc;is, tia <tld/>i&gcirc;is
        <fnt>
          <bib>PrV</bib>
        </fnt>.
