<drv mrk="grand.mez0a"> 
  <kap>mez<tld/>a</kap> 
  <snc mrk="grand.mez0a.KOMUNE"> 
    <dif> 
      Inter <tld/>a kaj mal<tld/>a, modere <tld/>a: 
      <ekz> 
        lia patro posedis mez<tld/>an ferfabrikon 
        <fnt> 
          <aut>Kurisu K.</aut>
          <vrk>Debuti</vrk>
          <bib>Monato</bib>
          <lok>2002:2</lok> 
        </fnt>. 
      </ekz> 
    </dif> 
  </snc>
  <trdgrp lng="ca">
    <trd>intermedi</trd>,
    <trd>mitj&agrave;</trd>
  </trdgrp>
  <trd lng="cs">st&rcaron;edn&iacute; velikosti <klr>(adj.)</klr></trd>
  <trdgrp lng="es"> 
    <trd>mediano</trd>, 
    <trd>mediana</trd> 
  </trdgrp> 
  <trd lng="fr">moyen<klr> (de grandeur moyenne)</klr></trd>
  <trdgrp lng="id">
    <trd><ind>rata</ind>-rata</trd>,
    <trd>[<ind>tengah</ind>] menengah</trd>
  </trdgrp>
  <trdgrp lng="pl"> 
    <trd>&sacute;redni</trd>, 
    <trd>&sacute;rednio du&zdot;y</trd> 
  </trdgrp>
  <trd lng="sk">strednej ve&lcaron;kosti</trd>
</drv> 

<drv mrk="grand.po0e"> 
  <kap>po<tld/>e</kap> 
  <uzo tip="klr">(a&ccirc;eti, vendi)</uzo> 
  <snc mrk="grand.po0e.EKON"> 
    <uzo tip="fak">EKON</uzo> 
    <dif> 
      La&ubreve; granda kvanto,
      <ref tip="sin" cel="detal.mal0e.EKON">maldetale</ref>. 
    </dif> 
  </snc>
  <trdgrp lng="be"> 
    <trd>&c_g;&c_u;&c_r;&c_t;&c_o;&c_m;</trd>, 
    <trd>&c_o;&c_p;&c_t;&c_a;&c_m;</trd> 
  </trdgrp>
  <trd lng="bg">&c_n;&c_a; &c_je;&c_d;&c_r;&c_o;</trd> 
  <trd lng="ca">a l'engr&ograve;s</trd>
  <trdgrp lng="cs">
    <trd>podle mno&zcaron;stv&iacute;</trd>,
    <trd>ve velk&eacute;m</trd>
  </trdgrp>
  <trd lng="en">wholesale</trd>
  <trdgrp lng="es"> 
    <trd>al por mayor</trd> 
    <trd>por <ind>mayoreo</ind></trd> 
  </trdgrp>
  <trd lng="fr">en <ind>gros</ind></trd>
  <trdgrp lng="id">
    <trd><ind>besar</ind>-besaran</trd>,
