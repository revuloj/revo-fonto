<drv mrk="grand.mal0a"> 
  <kap>mal<tld/>a</kap> 
  <snc mrk="grand.mal0a.dimensio"> 
    <dif> 
      Ne atinganta la ordinarajn dimensiojn: 
      <ekz> 
        mal<tld/>a arbo, skatolo. 
      </ekz> 
    </dif> 
    <refgrp tip="vid"> 
      <ref cel="et.0a">eta</ref>, 
      <ref cel="lilipu.0a">liliputa</ref>, 
      <ref cel="mikros.0a">mikroskopa</ref>, 
      <ref cel="miniat">miniatura</ref>. 
    </refgrp> 
    <trdgrp lng="ca">
      <trd>petit</trd>,
      <trd>menut</trd>
    </trdgrp>
    <trd lng="id">kecil</trd>
    <trdgrp lng="pt"> 
     <trd>pequeno<klr tip="ind"> (adj.)</klr></trd>, 
     <trd>mi&uacute;do</trd> 
    </trdgrp> 
  </snc> 
  <snc mrk="grand.mal0a.grado"> 
    <dif> 
      Malaltgrada, malaltkvalita: 
      <ekz> 
        mal<tld/>a animo, bruo. 
      </ekz> 
    </dif> 
    <refgrp tip="vid"> 
      <ref cel="bagate">bagatela</ref> 
    </refgrp> 
    <trd lng="pt">vil</trd> 
  </snc>
  <trdgrp lng="be"> 
    <trd>&c_m;&c_a;&c_l;&c_y;</trd>, 
    <trd>&c_m;&c_a;&c_l;&c_je;&c_n;&c_mol;&c_k;&c_ib;</trd>, 
    <trd>&c_d;&c_r;&c_o;&c_b;&c_n;&c_y;</trd> 
