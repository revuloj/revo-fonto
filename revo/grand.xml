<drv mrk="grand.0a"> 
  <kap><ofc>*</ofc><tld/>a</kap> 
  <snc mrk="grand.0a.dimensio"> 
    <dif> 
      Superanta la ordinarajn dimensiojn: 
      <ekz> 
        <tld/>a <klr>(vasta)</klr> tablo, vazo, &ccirc;ambro, kampo, 
        muro; 
      </ekz> 
      <ekz> 
        <tld/>a <klr>(alta)</klr> monto, turo, homo, besto; 
      </ekz> 
      <ekz> 
        <tld/>a <klr>(longa)</klr> rivero, bastono, da&ubreve;ro; 
      </ekz> 
      <ekz> 
        fi&scirc;o pli <tld/>a mal<tld/>an englutas 
        <fnt><bib>PrV</bib></fnt>; 
      </ekz> 
      <ekz> 
        <tld/>a telero, malplena kulero<fnt><bib>PrV</bib></fnt>; 
      </ekz> 
      <ekz> 
        <tld/>a kapitalo. 
      </ekz> 
    </dif> 
    <refgrp tip="vid"> 
      <ref cel="amplek">ampleksa</ref>, 
      <ref cel="elefan">elefanta</ref>, 
      <ref cel="gigant.0a">giganta</ref>, 
      <ref cel="kolos.0a">kolosa</ref> 
    </refgrp> 
          <trdgrp lng="pl"> 
            <trd>wielki</trd>, 
            <trd>du&zdot;y</trd> 
          </trdgrp> 
  </snc> 
  <snc mrk="grand.0a.grado"> 
    <dif> 
      Superanta la ordinaran gradon, intensa, altkvalita: 
      <ekz> 
        <tld/>a vento, kunveno, kvanto da akvo; 
      </ekz> 
      <ekz> 
        <tld/>a poeto, verko, talento; 
      </ekz> 
      <ekz> 
        Ludoviko, Frederiko la <tld/>a; 
      </ekz> 
      <ekz> 
        <tld/>a doloro, kura&gcirc;o, gloro, &gcirc;ojo kolero, 
        feli&ccirc;o, fiereco, honto, miro, profito, ri&ccirc;eco, 
        valoro, venko, malvenko; 
      </ekz> 
      <ekz> 
        <tld/>aj malbonoj, <tld/>aj rimedoj<fnt><bib>PrV</bib></fnt>; 
      </ekz> 
      <ekz> 
        <tld/>a ofico, <tld/>aj zorgoj<fnt><bib>PrV</bib></fnt>. 
      </ekz> 
    </dif> 
    <refgrp tip="vid"> 
      <ref cel="eminen1.0a">eminenta</ref>, 
      <ref cel="fort.0a">forta</ref>, 
      <ref cel="grav.0a">grava</ref>, 
      <ref cel="potenc.0a">potenca</ref>, 
      <ref cel="tro.0a">troa</ref> 
    </refgrp> 
          <trd lng="es">gran</trd> 
    <trdgrp lng="pl"> 
            <trd>wielki</trd> 
          </trdgrp> 
    <trd lng="pt">gra&uacute;do<klr tip="ind"> (adj.)</klr></trd> 
  </snc>
