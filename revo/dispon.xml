<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dispon.xml,v 1.25 2021/03/08 15:14:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>dispon</rad>/i
</kap>
<drv mrk="dispon.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc>
    <gra><vspec>x</vspec></gra>
    <dif>
      Uzi, la&ubreve; sia pla&ccirc;o, iun a&ubreve; ion:
      <ekz>
        vi povas <tld/>i pri mi;
      </ekz>
      <ekz>
        mi <tld/>as malmultan tempon;
      </ekz>
      <ekz>
        &ccirc;u tiuj se&gcirc;oj estas ankora&ubreve; <tld/>eblaj?
      </ekz>
      <ekz>
        mi estas al via <ind><tld/>o</ind>;
        <trd lng="ca">disposici&oacute;</trd>
      </ekz>
      <ekz>
        havi sub sia <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &ccirc;iu ano <tld/>as nur unu vo&ccirc;on.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="decid.0i">decidi</ref>,
      <ref cel="estr.0i">estri</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="propon.0i">proponi</ref>,
      <ref cel="man.0o">mano</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_r;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_r;&c_a;&c_z;&c_p;&c_o;&c_l;&c_a;&c_g;&c_a;&c_m; (&c_s; &c_n;&c_je;&c_shch;&c_o;)</trd>
  <trd lng="ca">disposar de</trd>
  <trdgrp lng="cs">
    <trd>disponovat</trd>,
    <trd>&ccaron;init opat&rcaron;en&iacute;</trd>
  </trdgrp>
  <trd lng="de">verf&uuml;gen (&uuml;ber)</trd>
  <trdgrp lng="en">
    <trd>have at one's disposal</trd>,
    <trd>have available</trd>
  </trdgrp>
  <trd lng="es">disponer</trd>
  <trdgrp lng="fa">
    <trd>&a_d;&a_r; <ind>&a_A;&a_kh;&a_t;&f_y;&a_A;&a_r;</ind>
    &a_d;&a_A;&a_sh;&a_t;&a_n;</trd>,
    <trd>&a_d;&a_r; <ind>&a_d;&a_s;&a_t;&a_r;&a_s;</ind>
    &a_d;&a_A;&a_sh;&a_t;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">disposer (de)</trd>
  <trd lng="hu">rendelkezik<klr tip="ind">(vmivel)</klr></trd>
  <trdgrp lng="pl">
    <trd>dysponowa&cacute;</trd>,
    <trd>mie&cacute; do swojej <ind>dyspozycji</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>dispor de</trd>,
    <trd>ter &agrave; <ind>disposi&ccedil;&atilde;o</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_o;&c_l;&c_a;&c_g;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">disponova&tcaron;</trd>
</drv>

<drv mrk="dispon.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Meti ion sub (je) ies <tld/>o:
      <ekz>
        <tld/>igi &ccirc;ambron al societo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_z;&c_v;&c_o;&c_l;&c_ib;&c_c;&c_mol;
    &c_r;&c_a;&c_s;&c_p;&c_a;&c_r;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_r;&c_je;&c_d;&c_o;&c_s;&c_t;&c_a;&c_v;&c_ja;&c_m;</trd>
  <trdgrp lng="ca">
    <trd>fornir</trd>,
    <trd>disposar (per a)</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>d&aacute;t k dispozici</trd>,
    <trd>nab&iacute;zet k u&zcaron;&iacute;v&aacute;n&iacute;</trd>,
    <trd>p&rcaron;edkl&aacute;dat</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>verf&uuml;gbar machen</trd>,
     <trd>zur <ind>Verf&uuml;gung</ind> stellen</trd>,
     <trd>bereitstellen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>make available</trd>,
    <trd>provide</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>hacer disponible</trd>,
    <trd>poner a disposici&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_d;&a_r; <ind>&a_A;&a_kh;&a_t;&f_y;&a_A;&a_r;</ind>
    &f_g;&a_dh;&a_A;&a_sh;&a_t;&a_n;</trd>,
    <trd>&a_d;&a_r; <ind>&a_d;&a_s;&a_t;&a_r;&a_s;</ind>
    &f_g;&a_dh;&a_A;&a_sh;&a_t;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fournir<klr tip="ind"> (mettre &agrave;
    disposition)</klr></trd>,
    <trd>mettre &agrave; <ind>disposition</ind></trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5EA;&#x5EA; &#x5D6;&#x5DB;&#x5D5;&#x5EA; &#x5E9;&#x5D9;&#x5DE;&#x5D5;&#x5E9;</trd>
  <trd lng="hu">rendelkez&eacute;sre bocs&aacute;t</trd>
  <trdgrp lng="pl">
    <trd>zadysponowa&cacute;</trd>,
    <trd>odda&cacute; do <klr>czyjej&sacute;</klr>
    <ind>dyspozycji</ind></trd>,
    <trd>udost&eogonek;pni&cacute;</trd>
  </trdgrp>
  <trd lng="pt">disponibilizar</trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_o;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>prik&aacute;za&tcaron;</trd>,
    <trd>umiestni&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="dispon.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="dispon.0ajxo.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Aparato, <ref tip="vid" cel="arangx.0ajxo.TEK">aran&gcirc;a&jcirc;o</ref>
      kiu estas parto de
      <ref tip="malprt" cel="komput.0a_sistemo">komputa sistemo</ref>
      kaj estas <tld/>igebla al ties taskoj.
    </dif>
  </snc>
  <trd lng="bg">&c_u;&c_s;&c_t;&c_r;&c_o;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="ca">dispositiu</trd>
  <trdgrp lng="en">
    <trd>device</trd>,
    <trd>facility</trd>,
    <trd>equippment</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Einrichtung</trd>,
     <trd>Vorrichtung</trd>,
     <trd>Ausstattung</trd>,
     <trd>Baugruppe</trd>,
     <trd>Einheit</trd>,
     <trd>Ger&auml;t</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>dispositivo</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_A;&a_b;&a_z;&a_A;&a_r;</trd>,
    <trd>&a_d;&a_s;&a_t;&f_g;&a_A;&a_h;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>unit&eacute;</trd>, <trd>dispositif</trd>
  </trdgrp>
  <trd lng="he">&#x5DE;&#x5B6;&#x5D6;&#x5B6;&#x5D2;</trd>
  <trdgrp lng="hu">
    <trd>eszk&ouml;z</trd>,
    <trd>k&eacute;sz&uuml;l&eacute;k</trd>
  </trdgrp>
  <trd lng="pl">urz&aogonek;dzenei</trd>
  <trd lng="pt">dispositivo</trd>
  <trd lng="ru">&c_u;&c_s;&c_t;&c_r;&c_o;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>

<drv mrk="dispon.0ebla">
  <kap><tld/>ebla</kap>
  <snc>
    <dif>
      Neokupata, libera, vakanta, havebla:
      <ekz>
        La taksio estas <tld/>ebla. Mi estas <tld/>ebla.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_n;&c_a;&c_l;&c_i;&c_ch;&c_je;&c_n;</trd>,
    <trd>&c_n;&c_a; &c_r;&c_a;&c_z;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verf&uuml;gbar</trd>
    <trd>frei <klr>(verf&uuml;gbar)</klr></trd>
  </trdgrp>
  <trd lng="en">available</trd>
  <trdgrp lng="es">
    <trd>disponible</trd>
  </trdgrp>
</drv>

<drv mrk="dispon.0ebleco">
  <kap><tld/>ebleco</kap>
  <snc>
    <dif>
      Havebleco
    </dif>
  </snc>
  <trd lng="bg">&c_n;&c_a;&c_l;&c_i;&c_ch;&c_n;&c_o;&c_s;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>disponibilita</trd>,
    <trd>mo&zcaron;nost nakl&aacute;d&aacute;n&iacute;</trd>,
    <trd>upot&rcaron;ebitelnost</trd>
  </trdgrp>
  <trd lng="de">Verf&uuml;gbarkeit</trd>
  <trd lng="en">availability</trd>
  <trdgrp lng="es">
    <trd>disponibilidad</trd>
  </trdgrp>
  <trd lng="sk">upotrebite&lcaron;nos&tcaron;</trd>
</drv>

</art>
