<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dispon.xml,v 1.28 2021/12/29 18:34:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>dispon</rad>/i
</kap>
<drv mrk="dispon.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="hav.0i.proprajxon">Havi</ref> ion en sufi&ccirc;a kvanto 
      kaj preta por uzi:
      <ekz>
        <klr>[mi]</klr> donas balojn, plenajn tridek unu balojn, pli da noktoj mi ne
        <tld/>as
        <fnt>
          <bib>Fab4</bib>
          <lok>Dekdu per la po&scirc;to</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iujare li <tld/>os tute sufi&ccirc;an monon por la&ubreve;dezira uzo
        <fnt>
          <bib>HsT</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi uzis la lastan rimedon, kiun mi povis ankora&ubreve; <tld/>i
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        niaj prauloj <tld/>is nek pri skribo, nek pri literaturo
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu metion <tld/>as, mizeron ne konas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="posed.0i">posedi</ref>,
      <ref cel="proviz.0i.havigi">provizi<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_m;&c_je;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_r;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_c;&c_a;</trd>
    </trdgrp>
    <trd lng="bg">&c_r;&c_a;&c_z;&c_p;&c_o;&c_l;&c_a;&c_g;&c_a;&c_m; (&c_s; &c_n;&c_je;&c_shch;&c_o;)</trd>
    <trd lng="ca">disposar de</trd>
    <trdgrp lng="cs">
      <trd>disponovat</trd>,
      <trd>&ccaron;init opat&rcaron;en&iacute;</trd>
    </trdgrp>
    <trd lng="de">verf&uuml;gen (&uuml;ber)</trd>
    <trdgrp lng="en">
      <trd>have at one's disposal</trd>,
      <trd>have available</trd>
    </trdgrp>
    <trd lng="es">disponer</trd>
    <trdgrp lng="fa">
      <trd>&a_d;&a_r; <ind>&a_A;&a_kh;&a_t;&f_y;&a_A;&a_r;</ind>
      &a_d;&a_A;&a_sh;&a_t;&a_n;</trd>,
      <trd>&a_d;&a_r; <ind>&a_d;&a_s;&a_t;&a_r;&a_s;</ind>
      &a_d;&a_A;&a_sh;&a_t;&a_n;</trd>
    </trdgrp>
    <trd lng="fr">disposer (de)</trd>
    <trd lng="hu">rendelkezik <klr tip="ind">(vmivel)</klr></trd>
    <trdgrp lng="pl">
      <trd>dysponowa&cacute;</trd>,
      <trd>mie&cacute; do swojej <ind>dyspozycji</ind></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>dispor de</trd>,
      <trd>ter &agrave; <ind>disposi&ccedil;&atilde;o</ind></trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_o;&c_l;&c_a;&c_g;&c_a;&c_t;&c_mol;</trd>
    <trd lng="sk">disponova&tcaron;</trd>
  </snc>
  <snc mrk="dispon.0i.decidi">
    <dif>
      <ref tip="super" cel="decid.0i">Decidi</ref> pri io a&ubreve; iu la&ubreve; sia a&ubreve;toritato kaj pla&ccirc;o:
      <ekz>
        la sinjoro havas la rajton <tld/>i la trezorojn de <klr>[s]</klr>iaj anta&ubreve;uloj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia vivo apartenas al &scirc;i sola: ci ne havas rajton <tld/>i pri &gcirc;i
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        min sendas la alta estraro, kiu <tld/>as pri vivo kaj morto
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        li pa&scirc;is meze de ili <klr>[&#8230;]</klr> kun tia certeco, kvaza&ubreve; li mem povus <tld/>i
        favorojn
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve;pla&ccirc;e <tld/>anta sian tagon
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        li jam dedi&ccirc;is pli longan tempon <klr>[&#8230;]</klr> ol li rajtis <tld/>i, kaj ne
        &scirc;atis la ideon de plua perdo
        <fnt>
          <bib>KKl</bib>
        </fnt>;
      </ekz>
      <ekz>
        kie mi <tld/>as, tie mi ordonas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        homo proponas, Dio <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="estr.0i">estri</ref>,
      <ref cel="propon.0i">proponi</ref>
    </refgrp>
    <trd lng="de">etwas <ind>verf&uuml;gen</ind></trd>
  </snc>
  <snc mrk="dispon.0i.bonfarta">
    <uzo tip="stl">ARK</uzo>
    <dif>
      Sufi&ccirc;e bonfarta por akcepti viziton, por priparoli gravan aferon:
      <ekz>
        mi zorgos, ke ili ne venu tien &ccirc;i, mi diros, ke vi ne estas nun <tld/>ita
        <fnt>
          <bib>Hamlet</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="konven.0i.alsituacio">konveni<sncref/></ref>
    <trd lng="de">p&auml;sslich</trd>
  </snc>
</drv>


<drv mrk="dispon.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Uzeblo de io:
      <ekz>
        homo potenca, kiu povis havi al sia <tld/>o la <klr>[tutan]</klr> policon
        <fnt>
          <bib>QuV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Verf&uuml;gung</trd>
  <trdgrp lng="en">
    <trd>disposal</trd>,
    <trd>availability</trd>
  </trdgrp>
  <trd lng="fr">disposition</trd>
</drv>


<drv mrk="dispon.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Meti ion sub (je) ies <tld/>o:
      <ekz>
        <tld/>igi &ccirc;ambron al societo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_z;&c_v;&c_o;&c_l;&c_ib;&c_c;&c_mol;
    &c_r;&c_a;&c_s;&c_p;&c_a;&c_r;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_r;&c_je;&c_d;&c_o;&c_s;&c_t;&c_a;&c_v;&c_ja;&c_m;</trd>
  <trdgrp lng="ca">
    <trd>fornir</trd>,
    <trd>disposar (per a)</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>d&aacute;t k dispozici</trd>,
    <trd>nab&iacute;zet k u&zcaron;&iacute;v&aacute;n&iacute;</trd>,
    <trd>p&rcaron;edkl&aacute;dat</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>verf&uuml;gbar machen</trd>,
     <trd>zur <ind>Verf&uuml;gung</ind> stellen</trd>,
     <trd>bereitstellen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>make available</trd>,
    <trd>provide</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>hacer disponible</trd>,
    <trd>poner a disposici&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_d;&a_r; <ind>&a_A;&a_kh;&a_t;&f_y;&a_A;&a_r;</ind>
    &f_g;&a_dh;&a_A;&a_sh;&a_t;&a_n;</trd>,
    <trd>&a_d;&a_r; <ind>&a_d;&a_s;&a_t;&a_r;&a_s;</ind>
    &f_g;&a_dh;&a_A;&a_sh;&a_t;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fournir<klr tip="ind"> (mettre &agrave;
    disposition)</klr></trd>,
    <trd>mettre &agrave; <ind>disposition</ind></trd>
  </trdgrp>
  <trd lng="he">&lamed;&tav;&tav; &zayin;&kaf;&vav;&tav;
  &shin;&yod;&mem;&vav;&shin;</trd>
  <trd lng="hu">rendelkez&eacute;sre bocs&aacute;t</trd>
  <trdgrp lng="pl">
    <trd>zadysponowa&cacute;</trd>,
    <trd>odda&cacute; do <klr>czyjej&sacute;</klr>
    <ind>dyspozycji</ind></trd>,
    <trd>udost&eogonek;pni&cacute;</trd>
  </trdgrp>
  <trd lng="pt">disponibilizar</trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_o;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>prik&aacute;za&tcaron;</trd>,
    <trd>umiestni&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="dispon.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="dispon.0ajxo.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Aparato, <ref tip="vid" cel="arangx.0ajxo.TEK">aran&gcirc;a&jcirc;o</ref>
      kiu estas parto de
      <ref tip="malprt" cel="komput.0a_sistemo">komputa sistemo</ref>
      kaj estas <tld/>igebla al ties taskoj.
    </dif>
  </snc>
  <trd lng="bg">&c_u;&c_s;&c_t;&c_r;&c_o;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="ca">dispositiu</trd>
  <trdgrp lng="en">
    <trd>device</trd>,
    <trd>facility</trd>,
    <trd>equippment</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Einrichtung</trd>,
     <trd>Vorrichtung</trd>,
     <trd>Ausstattung</trd>,
     <trd>Baugruppe</trd>,
     <trd>Einheit</trd>,
     <trd>Ger&auml;t</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>dispositivo</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_A;&a_b;&a_z;&a_A;&a_r;</trd>,
    <trd>&a_d;&a_s;&a_t;&f_g;&a_A;&a_h;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>unit&eacute;</trd>, <trd>dispositif</trd>
  </trdgrp>
  <trd lng="he">&mem;&#1462;&zayin;&#1462;&gimel;</trd>
  <trdgrp lng="hu">
    <trd>eszk&ouml;z</trd>,
    <trd>k&eacute;sz&uuml;l&eacute;k</trd>
  </trdgrp>
  <trd lng="pl">urz&aogonek;dzenei</trd>
  <trd lng="pt">dispositivo</trd>
  <trd lng="ru">&c_u;&c_s;&c_t;&c_r;&c_o;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>

<drv mrk="dispon.0ebla">
  <kap><tld/>ebla</kap>
  <snc>
    <dif>
      Neokupata, libera, vakanta, havebla:
      <ekz>
        La taksio estas <tld/>ebla. Mi estas <tld/>ebla.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_n;&c_a;&c_l;&c_i;&c_ch;&c_je;&c_n;</trd>,
    <trd>&c_n;&c_a; &c_r;&c_a;&c_z;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verf&uuml;gbar</trd>
    <trd>frei <klr>(verf&uuml;gbar)</klr></trd>
  </trdgrp>
  <trd lng="en">available</trd>
  <trdgrp lng="es">
    <trd>disponible</trd>
  </trdgrp>
</drv>

<drv mrk="dispon.0ebleco">
  <kap><tld/>ebleco</kap>
  <snc>
    <dif>
      Havebleco
    </dif>
  </snc>
  <trd lng="bg">&c_n;&c_a;&c_l;&c_i;&c_ch;&c_n;&c_o;&c_s;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>disponibilita</trd>,
    <trd>mo&zcaron;nost nakl&aacute;d&aacute;n&iacute;</trd>,
    <trd>upot&rcaron;ebitelnost</trd>
  </trdgrp>
  <trd lng="de">Verf&uuml;gbarkeit</trd>
  <trd lng="en">availability</trd>
  <trdgrp lng="es">
    <trd>disponibilidad</trd>
  </trdgrp>
  <trd lng="sk">upotrebite&lcaron;nos&tcaron;</trd>
</drv>

</art>
