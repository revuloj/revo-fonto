<kap>
  <ofc>*</ofc>
  <rad>dispon</rad>/i
</kap>
<drv mrk="dispon.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="dispon.0i.havi">
    <dif>
      <ref tip="super" cel="hav.0i.proprajxon">Havi</ref> ion en sufi&ccirc;a kvanto
      kaj preta por uzi:
      <ekz>
        <klr>[mi]</klr> donas balojn, plenajn tridek unu balojn, pli da noktoj mi ne
        <tld/>as
        <fnt>
          <bib>Fab4</bib>
          <lok>Dekdu per la po&scirc;to</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iujare li <tld/>os tute sufi&ccirc;an monon por la&ubreve;dezira uzo
        <fnt>
          <bib>HsT</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi uzis la lastan rimedon, kiun mi povis ankora&ubreve; <tld/>i
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        niaj prauloj <tld/>is nek pri skribo, nek pri literaturo
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu metion <tld/>as, mizeron ne konas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="posed.0i">posedi</ref>
    </refgrp>
    <trd lng="bg">&c_r;&c_a;&c_z;&c_p;&c_o;&c_l;&c_a;&c_g;&c_a;&c_m; (&c_s; &c_n;&c_je;&c_shch;&c_o;)</trd>
    <trd lng="ca">disposar de</trd>
    <trdgrp lng="cs">
      <trd>disponovat</trd>,
      <trd>&ccaron;init opat&rcaron;en&iacute;</trd>
    </trdgrp>
    <trd lng="de">verf&uuml;gen (&uuml;ber)</trd>
    <trdgrp lng="en">
      <trd>have at one's disposal</trd>,
      <trd>have available</trd>
    </trdgrp>
    <trd lng="es">disponer</trd>
    <trdgrp lng="fa">
      <trd>&a_d;&a_r; <ind>&a_A;&a_kh;&a_t;&f_y;&a_A;&a_r;</ind>
      &a_d;&a_A;&a_sh;&a_t;&a_n;</trd>,
      <trd>&a_d;&a_r; <ind>&a_d;&a_s;&a_t;&a_r;&a_s;</ind>
      &a_d;&a_A;&a_sh;&a_t;&a_n;</trd>
    </trdgrp>
    <trd lng="fr">disposer (de)</trd>
    <trd lng="hu">rendelkezik <klr tip="ind">(vmivel)</klr></trd>
    <trdgrp lng="pl">
      <trd>dysponowa&cacute;</trd>,
      <trd>mie&cacute; do swojej <ind>dyspozycji</ind></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>dispor de</trd>,
      <trd>ter &agrave; <ind>disposi&ccedil;&atilde;o</ind></trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_o;&c_l;&c_a;&c_g;&c_a;&c_t;&c_mol;</trd>
    <trd lng="sk">disponova&tcaron;</trd>
  </snc>
  <snc mrk="dispon.0i.decidi">
    <dif>
      <ref tip="super" cel="decid.0i">Decidi</ref> pri io a&ubreve; iu la&ubreve; sia a&ubreve;toritato kaj pla&ccirc;o:
      <ekz>
        la sinjoro havas la rajton <tld/>i la trezorojn de <klr>[s]</klr>iaj anta&ubreve;uloj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia vivo apartenas al &scirc;i sola: ci ne havas rajton <tld/>i pri &gcirc;i
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        min sendas la alta estraro, kiu <tld/>as pri vivo kaj morto
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        li pa&scirc;is meze de ili <klr>[&#8230;]</klr> kun tia certeco, kvaza&ubreve; li mem povus <tld/>i
        favorojn
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve;pla&ccirc;e <tld/>anta sian tagon
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        li jam dedi&ccirc;is pli longan tempon <klr>[&#8230;]</klr> ol li rajtis <tld/>i, kaj ne
        &scirc;atis la ideon de plua perdo
        <fnt>
          <bib>KKl</bib>
        </fnt>;
      </ekz>
      <ekz>
        kie mi <tld/>as, tie mi ordonas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        homo proponas, Dio <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dekret.0i">dekreti</ref>,
      <ref cel="estr.0i">estri</ref>,
      <ref tip="super" cel="komand.0i">komandi</ref>,
      <ref cel="propon.0i">proponi</ref>,
      <ref cel="met.sub0i">submeti</ref>
    </refgrp>
    <trd lng="de">etwas <ind>verf&uuml;gen</ind></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_r;&c_a;&c_d;&c_zh;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x81EA;&#30001;&#x306B;&#x4F7F;&#12358;  <pr>&#12376;&#12422;&#12358;&#x306B;&#12388;&#x304B;&#12358;</pr></trd>,
    <trd>&#x51E6;&#29702;&#12377;&#x308B;  <pr>&#12375;&#12423;&#x308A;&#12377;&#x308B;</pr></trd>,
    <trd>&#x51E6;&#x7F6E;&#12434;&#12392;&#x308B;  <pr>&#12375;&#12423;&#12385;&#12434;&#12392;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_a;&c_t;&c_i; &c_v; &c_s;&c_v;&#x43E;&c_jeu;&#x43C;&c_u; &c_r;&#x43E;&c_z;&#x43F;&#x43E;&c_r;&#x44F;&c_d;&c_zh;&c_je;&#x43D;&#x43D;&c_ib;</trd>,
    <trd>&c_r;&#x43E;&c_z;&#x43F;&#x43E;&c_r;&#x44F;&c_d;&c_zh;&c_a;&c_t;&c_i;&c_s;&#x44F; (&c_ch;&c_i;&#x43C;-&#x43D;.)</trd>
  </trdgrp>
</drv>

<drv mrk="dispon.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="dispon.0o.uzeblo">
    <dif>
      Uzeblo pri io:
      <ekz>
        li diris al siaj dis&ccirc;iploj, ke malgranda &scirc;ipeto estu je lia <tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>Marko 3:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        por la kuracado de la konjunktivo mem ni anta&ubreve; <klr>[&#8230;]</klr> havas por nia
        <tld/>o la nitran ar&gcirc;enton, kiun oni uzas por &scirc;mirado
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi havas la tempon je via <tld/>o kaj la monon necesan
        <fnt>
          <bib>KKl</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili nur havas kelkajn minutojn je sia <tld/>o
        <fnt>
          <bib>KKl</bib>
        </fnt>;
      </ekz>
      <ekz>
        mia havo estas je via <tld/>o
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rimed.0o">rimedo</ref>,
      <ref cel="serv.0o">servo</ref>,
      <ref cel="uz.frukt0o">fruktuzo</ref>,
      <ref cel="uz.0o">uzo</ref>
    </refgrp>
  </snc>
  <snc mrk="dispon.0i.decideblo">
    <dif>
      Decideblo pri io a&ubreve; iu:
      <ekz>
        homo potenca, kiu povis havi al sia <tld/>o la <klr>[tutan]</klr> policon
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>David</nom> metis trionon de la popolo sub la <tld/>on de <nom>Joab</nom>
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 18:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        li sin metas en la <tld/>on de la ju&gcirc;istaro
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi estas je via <tld/>o
        <fnt>
          <bib>KKl</bib>
        </fnt>;
      </ekz>
      <ekz>
        mia tuta tempo estas al via <tld/>o
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>;
      </ekz>
      <ekz>
        seso da viroj, kiuj por sufi&ccirc;e da ar&gcirc;ento sin metus absolute je &ccirc;ies <tld/>o
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        por trankviligi la popolon oni faris novajn <tld/>ojn pri la disdonado de greno
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        li haltu en Kumoj kaj atendu tie pluajn <tld/>ojn
        <fnt>
          <bib>QuV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dekret.0o">dekreto</ref>,
      <ref cel="ordon.0o.volo">ordono</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_p;&c_a;&c_r;&c_a;&c_d;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je; <klr>(&c_n;&c_a;&c_p;&c_r;. &c_m;&c_a;&c_jo;&c_m;&c_a;&c_s;&c_mol;&c_c;&c_ju;)</klr></trd>
  <trd lng="de">Verf&uuml;gung</trd>
  <trdgrp lng="en">
    <trd>disposal</trd>,
    <trd>availability</trd>
  </trdgrp>
  <trd lng="fr">disposition</trd>
  <trdgrp lng="ja">
    <trd>&#x81EA;&#30001;&#x88C1;&#x91CF;  <pr>&#12376;&#12422;&#12358;&#12373;&#12356;&#x308A;&#12423;&#12358;</pr></trd>,
    <trd>&#x4F7F;&#29992;  <pr>&#12375;&#12424;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="uk">&c_r;&#x43E;&c_z;&#x43F;&#x43E;&c_r;&#x44F;&c_d;&c_zh;&c_je;&#x43D;&#x43D;&#x44F; (&c_ch;&c_i;&#x43C;-&#x43D;.)</trd>
</drv>

<drv mrk="dispon.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="dispon.0ajxo.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Aparato, <ref tip="vid" cel="arangx.0ajxo.TEK">aran&gcirc;a&jcirc;o</ref>
      kiu estas parto de <ref tip="malprt" cel="sistem.komputa_0o">komputa sistemo</ref>
      kaj estas <tld/>igebla al ties taskoj:
      <ekz>
        la kerno, peliloj de <tld/>a&jcirc;oj kaj plej gravaj sistemaj programoj, kia la &scirc;elo,
        estas disvolvataj en la sama koda arbo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=137582">FreeBSD</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        transistoro estas duonkondukta&jcirc;a <tld/>a&jcirc;o uzata por amplifado kaj &scirc;altado de
        elektronikaj signaloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=14973">Transistoro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rimed.0o">rimedo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_l;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_a;&c_p;&c_a;&c_r;&c_a;&c_t;</trd>
  </trdgrp>
  <trd lng="bg">&c_u;&c_s;&c_t;&c_r;&c_o;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="ca">dispositiu</trd>
  <trdgrp lng="en">
    <trd>device</trd>,
    <trd>facility</trd>,
    <trd>equippment</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Einrichtung</trd>,
     <trd>Vorrichtung</trd>,
     <trd>Ausstattung</trd>,
     <trd>Baugruppe</trd>,
     <trd>Einheit</trd>,
     <trd>Ger&auml;t</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>dispositivo</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_A;&a_b;&a_z;&a_A;&a_r;</trd>,
    <trd>&a_d;&a_s;&a_t;&f_g;&a_A;&a_h;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>unit&eacute;</trd>, <trd>dispositif</trd>
  </trdgrp>
  <trd lng="he">&mem;&#x5B6;&zayin;&#x5B6;&gimel;</trd>
  <trdgrp lng="hu">
    <trd>eszk&ouml;z</trd>,
    <trd>k&eacute;sz&uuml;l&eacute;k</trd>
  </trdgrp>
  <trd lng="pl">urz&aogonek;dzenei</trd>
  <trd lng="pt">dispositivo</trd>
  <trd lng="ru">&c_u;&c_s;&c_t;&c_r;&c_o;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>
  <trdgrp lng="zh">
    <trd>&#x53D1;&#x884C;&#x503A;&#21048;&#26465;&#x4EF6;
    <pr>f&amacron;x&#xED;ngzh&#xE0;iqu&#xE0;nti&#xE1;oji&#xE0;n</pr></trd>,
    <trd>&#x4F53;&#x52BF; <pr>t&#x1D0;sh&#xEC;</pr></trd>,
    <trd>&#x8B66;&#22577; <pr>j&#x1D0;ngb&#xE0;o</pr></trd>,
    <trd>&#x8A2D;&#20633; <pr>sh&#xE8;bei</pr></trd>,
    <trd>&#x8A2D;&#x65BD; <pr>sh&#xE8;sh&#x12B;</pr></trd>,
    <trd>&#x4FDD;&#x62A4;&#32773; <pr>b&#x1CE;oh&#xF9;zh&#x11B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="dispon.0ebla">
  <kap><tld/>ebla</kap>
  <snc>
    <dif>
      Neokupata, <ref tip="sin" cel="liber.0a.vaka">libera</ref>,
      <ref cel="vak.0a">vaka</ref>, <ref tip="super" cel="hav.0ebla">havebla</ref>:
      <ekz>
        neniu kale&scirc;o montri&gcirc;is tuje <tld/>ebla
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> havis liton <tld/>eblan
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        tuj kiam li havis minuton <tld/>eblan, li realiris al sia observa posteno
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        okupi la tutan <tld/>eblan spacon
        <fnt>
          <bib>ChB</bib>
        </fnt>;
      </ekz>
      <ekz>
        malgra&ubreve; ia limigiteco de la <tld/>eblaj rimedoj <klr>[&#8230;]</klr> &ccirc;iam pli kreskanta
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u <nom>Arne Kristiansen</nom> estas <tld/>ebla? venigu lin tuj
        <fnt>
          <bib>CKv</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_s;&c_t;&c_u;&c_p;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_z;&c_a;&c_n;&c_ja;&c_t;&c_y;</trd>,
    <trd>&c_n;&c_a;&c_ja;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_n;&c_a;&c_l;&c_i;&c_ch;&c_je;&c_n;</trd>,
    <trd>&c_n;&c_a; &c_r;&c_a;&c_z;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verf&uuml;gbar</trd>
    <trd>frei <klr>(verf&uuml;gbar)</klr></trd>
  </trdgrp>
  <trd lng="en">available</trd>
  <trdgrp lng="es">
    <trd>disponible</trd>
  </trdgrp>
  <trd lng="ja">&#x81EA;&#30001;&#x306B;&#x4F7F;&#12360;&#x308B;  <pr>&#12376;&#12422;&#12358;&#x306B;&#12388;&#x304B;&#12360;&#x308B;</pr></trd>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_a;&#x44F;&c_v;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_ib;&#x43B;&#x44C;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43D;&c_je;&c_z;&c_a;&c_j;&#x43D;&#x44F;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x53EF;&#22788;&#29702; <pr>k&#x11B;ch&#x1D4;l&#x1D0;</pr></trd>,
    <trd>&#x53EF;&#x81EA;&#30001;&#x4F7F;&#29992; <pr>k&#x11B;z&#xEC;y&#xF3;ush&#x1D0;y&#xF2;ng</pr></trd>,
    <trd>&#x53EF;&#x652F;&#x914D; <pr>k&#x11B;zh&#x12B;p&#xE8;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="dispon.0ebleco">
  <kap><tld/>ebleco</kap>
  <snc>
    <dif>
      Havebleco por uzado:
      <ekz>
        <tld/>ebleco de materialoj
        <fnt>
          <bib>Kon11</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>ebleco de teknika<klr>[&jcirc;oj]</klr> trovis baron en la multekosteco
        <fnt>
          <vrk><url ref="https://kurso.com.br/eo/savilo.html">Kurso de Esperanto - Artikoloj | La
          Lasta Savilo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_s;&c_t;&c_u;&c_p;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_ja;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_n;&c_a;&c_l;&c_i;&c_ch;&c_n;&c_o;&c_s;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>disponibilita</trd>,
    <trd>mo&zcaron;nost nakl&aacute;d&aacute;n&iacute;</trd>,
    <trd>upot&rcaron;ebitelnost</trd>
  </trdgrp>
  <trd lng="de">Verf&uuml;gbarkeit</trd>
  <trd lng="en">availability</trd>
  <trdgrp lng="es">
    <trd>disponibilidad</trd>
  </trdgrp>
  <trd lng="sk">upotrebite&lcaron;nos&tcaron;</trd>
  <trd lng="zh">&#x53EF;&#29992;&#29575; <pr>k&#x11B;y&#xF2;ngshu&#xE0;i</pr></trd>
</drv>

<drv mrk="dispon.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Meti ion sub (je) ies <tld/>o:
      <ekz>
        li estis ordoninta <tld/>igi terareon al la privata persono
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        ina&ubreve;gura parolado <klr>[&#8230;]</klr> afable <tld/>igita por tiu &ccirc;i eldono
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Nikolaj Rytjkov</nom> komplete <tld/>igis sian teatran talenton al la Internacia Lingvo
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
