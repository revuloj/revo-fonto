<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>dispon</rad>/i
</kap>
<drv mrk="dispon.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc>
    <gra><vspec>x</vspec></gra>
    <dif>
      Uzi, la&ubreve; sia pla&ccirc;o, iun a&ubreve; ion:
      <ekz>
        vi povas <tld/>i pri mi;
      </ekz>
      <ekz>
        mi <tld/>as malmultan tempon;
      </ekz>
      <ekz>
        &ccirc;u tiuj se&gcirc;oj estas ankora&ubreve; <tld/>eblaj?
      </ekz>
      <ekz>
        mi estas al via <tld/>o;
      </ekz>
      <ekz>
        havi sub sia <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &ccirc;iu ano <tld/>as nur unu vo&ccirc;on.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="decid.0i">decidi</ref>,
      <ref cel="estr.0i">estri</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="propon.0i">proponi</ref>,
      <ref cel="man.0o">mano</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">disponovat</trd>
  <trd lng="en">dispose</trd>
  <trd lng="fr">disposer (de)</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_o;&c_l;&c_a;&c_g;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="dispon.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Meti ion sub (je) ies <tld/>o:
      <ekz>
        <tld/>igi &ccirc;ambron al societo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>fournir<klr tip="ind"> (mettre &agrave; disposition)</klr></trd>,
    <trd>mettre &agrave; <ind>disposition</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_o;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="dispon.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="dispon.0ajxo.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Aparato, <ref tip="vid" cel="arangx.0ajxo.TEK">aran&gcirc;a&jcirc;o</ref>
      kiu estas parto de
      <ref tip="malprt" cel="komput.0a.sistemo">komputa sistemo</ref>
      kaj estas <tld/>igebla al ties taskoj.
    </dif>
  </snc>
  <trd lng="en">device</trd>
  <trdgrp lng="fr">
    <trd>unit&eacute;</trd>, <trd>dispositif</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_s;&c_t;&c_r;&c_o;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>
</art>
