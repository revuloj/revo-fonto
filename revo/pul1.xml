<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pul1.xml,v 1.13 2023/09/03 12:45:14 revo Exp $">
<kap>
  <rad>pul</rad>/o
  <fnt>
    <bib>PIV2</bib>
  </fnt>
</kap>

<drv mrk="pul1.0o">
  <kap>
    <tld/>o
    <fnt>
      <vrk>
        <url ref="&Monato;/landoj.php">La landoj
        de l' tero</url>
      </vrk>
  </fnt>
  </kap>
  <snc mrk="pul1.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Eta <ref tip="lst" cel="unu.mon0o" lst="voko:monunuoj">monunuo</ref> de <ref tip="vid" cel="afgan.A0ujo.GEOG">Afganujo</ref>.
      <ekz>
        1 <ref tip="super" cel="afgani.0o">afganio</ref> = 100
        puloj
        <fnt>
          <vrk>
            <url ref="&Monato;/landoj.php">La
            landoj de l' tero</url>
          </vrk>
       </fnt>
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="pul.0o">pulo (insekto)</ref>
  <trd lng="be">&c_p;&c_u;&c_l; <klr tip="amb">(&c_m;&c_a;&c_n;&c_e;&c_t;&c_a;)</klr></trd>
  <trd lng="ca">pul</trd>
  <trd lng="de">Pul</trd>
  <trd lng="fr">pul</trd>
  <trd lng="ja">&#x30CE;&#x30DF;</trd>
  <trdgrp lng="pl">
    <trd>pul</trd>
  </trdgrp>
  <trd lng="nl">pul</trd>
  <trd lng="uk">&c_b;&#x43B;&#x43E;&c_h;&c_a;</trd>
</drv>

</art>

