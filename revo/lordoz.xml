<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>lordoz</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="lordoz.0o">
  <kap><tld/>o</kap>
  <snc mrk="lordoz.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Alventra kurbo de la spino:
      <ekz>
        graspugeco <klr>...</klr> estas ligita kun forta ventra <tld/>o
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="https://eo.wikipedia.org/wiki/Graspugeco">
          </url>graspugeco, 2016-04-22</lok>
        </fnt>.
      </ekz>
      <refgrp tip="vid">
        <ref cel="cifoz.0o.ANA">cifozo</ref>,
        <ref cel="skolioz.0o.ANA">skoliozo</ref>
      </refgrp>
    </dif>
  </snc>
  <snc mrk="lordoz.0o.ETO">
    <dif>
      Refleksa kurbi&gcirc;o de la spino &ccirc;e mamulaj inoj,
      kiu faciligas koiton.
    </dif>
  </snc>
  <trd lng="en">lordosis</trd>
  <trd lng="fr">lordose</trd>
</drv>
</art>

