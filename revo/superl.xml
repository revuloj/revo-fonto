<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: superl.xml,v 1.16 2015/07/04 08:37:44 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>superlativ</rad>/o
</kap>
<drv mrk="superl.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">GRA</uzo>
    <dif>
      Adjektiva a&ubreve; adverba formo esprimanta tre altan gradon (<tld/>o
      absoluta per <ekz>tre</ekz>) a&ubreve; kompare la plej altan gradon
      (rilata <tld/>o per <ekz>plej</ekz>).
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_j;&c_v;&c_y;&c_sh;&c_e;&c_j;&c_sh;&c_a;&c_ja; &c_s;&c_t;&c_u;&c_p;&c_je;&c_n;&c_mol;
  &c_p;&c_a;&c_r;&c_a;&c_w;&c_n;&c_a;&c_n;&c_mol;&c_n;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>3. stupeň</trd>,
    <trd>superlativ</trd>
  </trdgrp>
  <trd lng="de">Superlativ</trd>
  <trd lng="en">superlative</trd>
  <trd lng="es">superlativo</trd>
  <trdgrp lng="fa">
    <trd>&a_S;&a_f;&a_t; &a_ayn;&a_A;&a_l;&f_y;</trd>,
    <trd>&a_S;&a_f;&a_t; &a_b;&a_r;&f_y;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">superlatif</trd>
  <trdgrp lng="hu">
    <trd>fels&odblac;fok</trd>,
    <trd>szuperlat&iacute;vusz</trd>
  </trdgrp>
  <trd lng="it">superlativo</trd>
  <trdgrp lng="nl">
    <trd>superlatief</trd>,
    <trd>overtreffende <ind>trap</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_v;&c_o;&c_s;&c_h;&c_o;&c_d;&c_n;&c_a;&c_ja; &c_s;&c_t;&c_je;&c_p;&c_je;&c_n;&c_mol;
  <klr tip="amb">(&c_p;&c_r;&c_i;&c_l;&c_a;&c_g;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_g;&c_o;,
  &c_n;&c_a;&c_r;&c_je;&c_ch;&c_i;&c_ja;)</klr></trd>
  <trd lng="sk">superlatív</trd>
</drv>
</art>
