<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: kokakola.xml,v 1.2 2025/10/29 08:41:56 afido Exp $">
    <kap>
      <rad>Kokakola</rad>/o
    </kap>

<drv mrk="kokakola.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Varmarko de usondevena <ref tip="super" cel="kola.0o.KUI">kolao</ref>:
      <ekz>
        li nutras sin per hamburgeroj kaj <tld lit="k"/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk>Proksimaj kaj malproksimaj najbaroj</vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili havas ne <tld lit="k"/>on sed kokainon en siaj barbaraj hararoj
        <fnt>
          <bib>Monato</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Coca-Cola</trd>
    <trd lng="en">Coca-Cola</trd>
    <trd lng="fr">Coca-Cola</trd>
  </snc>
<trd lng="be">&c_k;&c_o;&c_k;&c_a;-&c_k;&c_o;&c_l;&c_a;</trd>
</drv>

  </art>

