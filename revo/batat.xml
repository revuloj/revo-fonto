<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: batat.xml,v 1.21 2023/10/29 19:11:00 revo Exp $">
<kap>
  <rad>batat</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>,
  <var><kap><rad>patat</rad>/o <fnt><bib>PV</bib></fnt></kap></var>
</kap>
<drv mrk="batat.0o">
  <kap><tld/>o,
    <var><kap>patato</kap></var>
  </kap>
  <snc mrk="batat.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Specio el familio
      <ref tip="super" cel="konvol1.0oj">konvolvulacoj</ref>
      kun grandaj tuberoj (<trd lng="la">Ipomoea batatas</trd>).
    </dif>
    <trd lng="de">Knollenwinde</trd>
    <trd lng="hu">bat&aacute;ta</trd>
  </snc>
  <snc mrk="batat.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      La man&gcirc;ebla dol&ccirc;a tubero de
      <ref tip="malprt" cel="batat.0o.BOT"><tld/>o</ref>:
      <ekz>
        ka&ubreve;ri&gcirc;inte sur la kampo, li dis&scirc;ovis rampantajn foliojn, 
        de&scirc;iris <tld/>ojn, kaj peze &jcirc;etis ilin en la sitelon
        <fnt>
          <aut>Mo Yan, trad. Vejdo</aut>
          <vrk>Travidebla Ru&gcirc;a Rafano</vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>batata</trd>,
      <trd>moniato</trd>
    </trdgrp>
    <trd lng="de">Batate</trd>
    <trd lng="es">batata</trd>
    <trd lng="hu">&eacute;desburgonya</trd>
    <trdgrp lng="zh">
    <trd>&#x5730;&#x74DC; <pr>d&#xEC;gu&#x101;</pr></trd>,
    <trd>&#x756A;&#x85AF; <pr>f&#x101;nsh&#x1D4;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_t;&c_a;&c_t;</trd>,
    <trd>&c_s;&c_a;&c_l;&c_o;&c_d;&c_k;&c_a;&c_ja;
    &c_b;&c_u;&c_l;&c_mol;&c_b;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>batata</trd>,
    <trd>povijnice jedl&aacute;</trd>,
    <trd>sladk&yacute; brambor</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>sweet <ind>potato</ind></trd>
  </trdgrp>
  <trd lng="fr">patate douce</trd>
  <trdgrp lng="nl">
    <trd>bataat</trd>,
    <trd>zoete <ind>aardappel</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
      <trd>batat</trd>,
      <trd>s&lstroke;odki ziemniak</trd>
    </trdgrp>
  <trdgrp lng="pt">
    <trd>batata</trd>,
    <trd>batata-doce</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_t;&c_a;&c_t;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_d;&c_k;&c_i;&c_j;
    &c_k;&c_a;&c_r;&c_t;&c_o;&c_f;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">topinambur</trd>
</drv>
</art>
