<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>batat</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="batat.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">BOT</uzo>
   <snc mrk="batat.0o.BOT">
     <dif>
       Specio el familio <ref tip="super" 
cel="konvol.0acoj">konvolvulacoj</ref>
       (<trd lng="la">Ipomoea batatas</trd>), kun grandaj tuberoj.
     </dif>
     <trd lng="de">Knollenwinde</trd>
   </snc>
   <snc mrk="batat.0o.tubero">
     <dif>
       La man&gcirc;ebla dol&ccirc;a tubero de
       <ref tip="malprt" cel="batat.0o.BOT"><tld/>o</ref>.
     </dif>
     <trd lng="de">Batate</trd>
     <trd lng="fr">patate douce</trd>
   </snc>
   <trd lng="ru">&c_b;&c_a;&c_t;&c_a;&c_t;</trd>
</drv>
</art>
