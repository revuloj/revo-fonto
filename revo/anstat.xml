<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>anstata&ubreve;</rad>
</kap>
<drv mrk="anstat.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Prepozicio signifanta:
      en la loko de, plenumante la rolon, la taskon de:
      <ekz>
        vi povas uzi citronon <tld/> vinagro;
      </ekz>
      <ekz>ne servas larmo <tld/> armo<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        vi dormas <tld/> labori;
      </ekz>
      <ekz>
        li batis Petron <tld/> (bati) Pa&ubreve;lon<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_m;&c_je;&c_s;&c_t;</trd>
  <trd lng="cs">nam&iacute;sto</trd>
  <trdgrp lng="en">
    <trd>instead of</trd>,
    <trd>in lieu of</trd>,
    <trd>in place of</trd>
  </trdgrp>
  <trd lng="de">statt</trd>
  <trd lng="es">en <ind>lugar</ind> de</trd>
  <trd lng="fr">au <ind>lieu</ind> de</trd>
  <trd lng="nl">in <ind>plaats</ind> van</trd>
  <trd lng="ru">&c_v;&c_m;&c_je;&c_s;&c_t;&c_o;</trd>
  <trd lng="sk">namiesto</trd>
</drv>

<drv mrk="anstat.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Esti en la loko de; esti uzata <tld/>; plenumi ies rolon a&ubreve;
      taskon:
      <ekz>
        mielo <tld/>as sukeron;
      </ekz>
      <ekz>
        nenio <tld/>as la sanon;
      </ekz>
      <ekz>
        dum mia foresto mia kolego <tld/>os min.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_m;&c_ja;&c_sh;&c_ch;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>ersetzen</trd>, <trd>vertreten</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>replace</trd>,
    <trd>substitute for</trd>,
    <trd>take the place of</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>remplacer</trd>,
    <trd>suppl&eacute;er</trd>,
    <trd>tenir <ind>lieu</ind> (de)</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vervangen<klr tip="ind"> (plaats innemen van)</klr></trd>,
    <trd>plaats innemen van</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_m;&c_je;&c_shch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_je;&c_n;&c_ja;&c_t;&c_mol; <klr>(&c_s;&c_o;&c_b;&c_o;&c_j;)</klr></trd>
  </trdgrp>
  <trd lng="sk">nahradi&tcaron;</trd>
</drv>

<drv mrk="anstat.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago de tiu, kiu <tld/>as.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_m;&c_je;&c_n;&c_a;</trd>
  <trdgrp lng="en">
    <trd>replacement</trd>,
    <trd>substitution</trd>
  </trdgrp>
  <trd lng="fr">remplacement</trd>
  <trd lng="ru">&c_z;&c_a;&c_m;&c_je;&c_n;&c_a;</trd>
</drv>

<drv mrk="anstat.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="A"/>anta.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>replacement</trd>,
    <trd>substitute</trd>,
    <trd>ersatz</trd>
  </trdgrp>
  <trd lng="de">Ersatz-</trd>
  <trdgrp lng="fr">
    <trd>rempla&ccedil;ant</trd>,
    <trd>de <ind>rechange</ind></trd>,
    <trd>suppl&eacute;ant</trd>
  </trdgrp>
<trd lng="nl">vervangend</trd>
  <trd lng="sk">n&aacute;hradn&yacute;</trd>
</drv>

<drv mrk="anstat.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      <tld lit="A"/> tio.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_m;&c_je;&c_s;&c_t;</trd>
  <trd lng="de">anstatt</trd>
  <trd lng="en">instead</trd>
  <trdgrp lng="fr">
    <trd>en <ind>remplacement</ind></trd>,
    <trd>&agrave; la <ind>place</ind></trd>
  </trdgrp>
  <trd lng="nl">in <ind>plaats</ind> van</trd>
  <trd lng="ru">&c_v;&c_z;&c_a;&c_m;&c_je;&c_n;</trd>
  <trd lng="sk">namiesto</trd>
</drv>

<drv mrk="anstat.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Meti en la lokon de; uzi <tld/>; doni al iu la rolon de:
      <ekz>
        <tld/>igi dornobarilon per muro, oficiston per oficistino.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_m;&c_ja;&c_n;&c_ja;&c_c;&c_mol;</trd>
  <trd lng="de">ersetzen</trd>
  <trdgrp lng="en">
    <trd>replace</trd>,
    <trd>put in place of</trd>,
    <trd>substitute</trd>,
    <trd>make a substitution</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>remplacer</trd>,
    <trd>substituer (&agrave;)</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vervangen<klr tip="ind"> (i.d.plaats stellen)</klr></trd>,
    <trd>in de <ind>plaats</ind> stellen van</trd>
  </trdgrp>
<trd lng="ru">&c_z;&c_a;&c_m;&c_je;&c_n;&c_ja;&c_t;&c_mol;</trd>
  <trd lng="sk">nahradi&tcaron;</trd>
</drv>
</art>
