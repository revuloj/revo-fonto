<vortaro>
<art mrk="$Id: cxasi.xml,v 1.28 2025/07/05 14:00:20 revo Exp $">
<kap>
  <ofc>9</ofc>
  <rad>&ccirc;asi</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>

<drv mrk="cxasi.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Baza <ref tip="super" cel="fram.0o">framo</ref> de rada veturilo, 
      portanta karoserion, motoron, aksojn, radojn ktp:
      <ekz>
        subskribita estas interkonsento pri komuna produktado de batalveturiloj <klr>[&#8230;]</klr>, 
        Pollando liveros <tld/>ojn kaj Slovakio senhomajn batalturojn
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2015/011547.php">Armitaj amikoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        koncepto de <tld/>o kun centra tubo kaj movigaksoj, kiun oni anka&ubreve; markas
        kiel spinframo, sed en la mondo per pli konata markigo Tatra-koncepto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=101250">Tatra</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        alveninte &ccirc;e la elektota loko, ili faris provizoran boaton el vagona <tld/>o kaj ba&scirc;oj por
        transiri la riveron
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=121883">Fortika&jcirc;o Calgary</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kadr.0o">kadro</ref>,
      <ref cel="armatu.0o">armaturo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_a;&c_s;&c_ib;</trd>
  <trd lng="ca">xass&iacute;s</trd>
  <trdgrp lng="de">
    <trd>Fahrgestell</trd>,
    <trd>Chassis</trd>
  </trdgrp>
  <trd lng="en">chassis</trd>
  <trd lng="es">chas&iacute;s</trd>
  <trd lng="fr">ch&acirc;ssis</trd>
  <trd lng="he">&shin;&#1473;&#1460;&lamed;&dalet;&#1464;&he;</trd>
  <trd lng="hu">alv&aacute;z</trd>
  <trd lng="it">telaio<klr> (meccanica)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#12471;&#12515;&#12540;&#12471;&#12540;</trd>,
    <trd>&#36554;&#21488;  <pr>&#12375;&#12419;&#12384;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">chassis</trd>
  <trdgrp lng="pl">
    <trd>podwozie</trd>,
    <trd>chassis</trd>,
    <trd>rama<klr> pojazdu</klr></trd>
  </trdgrp>
  <trd lng="pt">chassi</trd>
  <trd lng="ru">&c_sh;&c_a;&c_s;&c_s;&c_i;</trd>
  <trd lng="uk">&c_sh;&c_a;&c_s;&c_ib;</trd>
  <trdgrp lng="zh">
    <trd>&#36554;&#26550; <pr>ch&#275;ji&agrave;</pr></trd>,
    <trd>&#27773;&#36554;&#26550; <pr>q&igrave;ch&#275;ji&agrave;</pr></trd>,
    <trd>&#24213;&#30424; <pr>d&#464;p&aacute;n</pr></trd>
  </trdgrp>
</drv>
</art>
