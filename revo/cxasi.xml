<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&ccirc;asi</rad>/o
</kap>

<drv mrk="cxasi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Suba parto de veturilo, kun aksoj kaj radoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="fram.0o">framo</ref>,
      <ref cel="kadr.0o">kadro</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_a;&c_s;&c_ib;</trd>
  <trdgrp lng="de">
    <trd>Fahrgestell</trd>,
    <trd>Chassis</trd>
  </trdgrp>
  <trd lng="en">chassis</trd>
  <trd lng="es">chas&iacute;s</trd>
  <trd lng="fr">ch&acirc;ssis</trd>
  <trd lng="hu">alv&aacute;z</trd>
  <trd lng="nl">chassis</trd>
  <trd lng="pt">chassi</trd>
  <trd lng="ru">&c_sh;&c_a;&c_s;&c_s;&c_i;</trd>
</drv>
</art>
