<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ronk.xml,v 1.19 2022/03/26 19:19:20 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ronk</rad>/i
</kap>
<drv mrk="ronk.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Eligi bruon spirante dum la dormo.
    </dif>
    <trd lng="be">&c_h;&c_r;&c_a;&c_p;&c_c;&c_ib;</trd>
    <trdgrp lng="ca">
      <trd>roncar</trd>,
      <trd>rinxolar</trd>
    </trdgrp>
    <trd lng="bg">&c_h;&c_malmol;&c_r;&c_k;&c_a;&c_m;</trd>
    <trd lng="en">snore</trd>
    <trdgrp lng="es">
      <trd>roncar</trd>
    </trdgrp>
    <trd lng="hu">horkol</trd>    
    <trd lng="nl">snurken</trd>
    <trd lng="pt">roncar</trd>
    <trd lng="ru">&c_h;&c_r;&c_a;&c_p;&c_je;&c_t;&c_mol;</trd>
    <trd lng="sv">snarka</trd>
  </snc>
  <snc>
    <dif>
      Eligi bruon similantan al <tld/>ado;
      <ref tip="sin" cel="grunt.0i">grunti</ref>:
      <ekz>
        li <tld/>is kiel porko, kiel kolera leonido<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_o;&c_h;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_y;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_f;&c_y;&c_r;&c_k;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="bg">&c_g;&c_r;&c_u;&c_h;&c_t;&c_ja;</trd>
    <trdgrp lng="ca">
      <trd>roncar</trd>,
      <trd>remorejar</trd>
    </trdgrp>
    <trd lng="en">grunt</trd>
    <trdgrp lng="es">
      <trd>roncar</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>szuszog</trd>,
      <trd>f&uacute;jtat</trd>,
      <trd>horkant</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>knorren</trd>,
      <trd>grommen</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_h;&c_r;&c_ju;&c_k;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_f;&c_y;&c_r;&c_k;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_r;&c_y;&c_ch;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>chrochtat</trd>,
    <trd>chr&aacute;pat</trd>,
    <trd>frkat <klr>(k&uring;&ncaron;)</klr></trd>
  </trdgrp>
  <trd lng="de">schnarchen</trd>
  <trd lng="fr">ronfler</trd>
  <trdgrp lng="sk">
    <trd>chrochta&tcaron;</trd>,
    <trd>chr&aacute;pa&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x445;&#x440;&#x43E;&#x43F;&#x442;&#x438;</trd>,
    <trd>&#x445;&#x440;&#x44E;&#x43A;&#x430;&#x442;&#x438; (&#x43F;&#x440;&#x43E; &#x441;&#x432;&#x438;&#x43D;&#x44E;)</trd>,
    <trd>&#x444;&#x438;&#x440;&#x43A;&#x430;&#x442;&#x438; (&#x43F;&#x440;&#x43E; &#x43A;&#x43E;&#x43D;&#x44F;)</trd>
  </trdgrp>
</drv>
</art>
