<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ronk.xml,v 1.25 2023/12/21 13:32:29 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ronk</rad>/i
</kap>

<drv mrk="ronk.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="ronk.0.SON">
    <dif>
      Bruo de forta, embarasata spirado:
      <ekz>
        &leftquot;<tld/>, <tld/>&rightquot; ronkis Sinjorino Urso, &leftquot;<tld/>, <tld/>, <tld/>&rightquot;
        &leftquot;Ho, ne!&rightquot; diris Sinjoro Urso, &leftquot;mi ne povas toleri tion.&rightquot;
        <fnt>
           <aut>J. Murphy, trad. Nikola kaj Martin Markarian</aut>
          <vrk><url ref="http://www.phys.ens.fr/~jacobsen/infanlibroj/Ursoj.pdf">
          Paco .. finfine</url></vrk>
          <lok>[vidita en 2023]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;-&c_r;-&c_r;!</trd>
  <trd lng="fr">ron<klr> (ronflement)</klr></trd>
</drv>

<drv mrk="ronk.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ronk.0i.FIZL">
    <dif>
      Eligi bruon <ref tip="vid" cel="spir.0i.FIZL">spirante</ref> dum la dormo:
      <ekz>
        a&ubreve;skultu <klr>[&#8230;]</klr> vi povas <tld/>i, <tld/>i la tutan nokton, sed mi ne
        povas dormi e&ccirc; horkvaronon
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio</lok>
        </fnt>;
      </ekz>
      <ekz>
        tie ku&scirc;is la lupo sub arbo kaj <tld/>is tiel, ke la bran&ccirc;oj tremis
        <fnt>
          <bib>ElektFab</bib>
          <lok>La sep kapridoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i eka&ubreve;dis kvaza&ubreve; en dormo <tld/>an, nehaltigeblan tusadon
        <klr>(kp <ref cel="sterto.0o">stertoro</ref>)</klr>
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la maristo dume <tld/>etadis tute ne turmentante sian cerbon pri filozofiaj tezoj
        <fnt>
          <bib>IdO</bib>
          <lok>Kvara Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_h;&c_r;&c_a;&c_p;&c_c;&c_ib;</trd>
    <trdgrp lng="ca">
      <trd>roncar</trd>,
      <trd>rinxolar</trd>
    </trdgrp>
    <trd lng="bg">&c_h;&c_malmol;&c_r;&c_k;&c_a;&c_m;</trd>
    <trd lng="en">snore</trd>
    <trdgrp lng="es">
      <trd>roncar</trd>
    </trdgrp>
    <trd lng="hu">horkol</trd>
    <trd lng="nl">snurken</trd>
    <trd lng="pl">chrapa&cacute;</trd>
    <trd lng="pt">roncar</trd>
    <trd lng="ro">sfor&abreve;i</trd>
    <trd lng="ru">&c_h;&c_r;&c_a;&c_p;&c_je;&c_t;&c_mol;</trd>
    <trd lng="sv">snarka</trd>
  </snc>
  <snc mrk="ronk.0i.SON">
    <dif>
      Eligi bruon similantan al <tld/>ado:
      <ekz>
        <klr>[li]</klr> eka&ubreve;dis <tld/>adon de &ccirc;evalo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ser&ccirc;os ilin eksplodis la princo, li kuris en la &ccirc;ambro kaj <tld/>is, kiel
        kolera leonido
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>adoj de la flamado sonorigis la atmosferon
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        la blekegantaj ondegoj, enprofundi&gcirc;ante en la kavernon <klr>[&#8230;]</klr> naskis
        senpa&ubreve;zan <tld/>adon, kiu supreniris el la teraj interna&jcirc;oj
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Deka</lok>
        </fnt>;
      </ekz>
      <ekz>
        eta gramofono alte levi&gcirc;as en la aero kaj <tld/>e a&ubreve;digas malkompreneblajn sonojn
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la vapor&scirc;ipeto maldiligente siblis <klr>[&#8230;]</klr>, <tld/>e fajfinte, fine
        albordi&gcirc;is,
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        tio estas tor&ccirc;oj!
        kaj li turnis sin al la sklavoj:
        atentu, ke la muloj ne <tld/>u!
        <fnt>
          <aut>H. Sienkiewicz, trad. Lidja Zamenhof</aut>
          <vrk>Quo Vadis</vrk>
          <lok>1933</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="anhel.0i">anheli</ref>,
      <ref cel="grunt.0i">grunti</ref>,
      <ref cel="ror.0i">rori</ref>,
      <ref cel="snuf.0egi">snufegi</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_r;&c_o;&c_h;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_y;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_f;&c_y;&c_r;&c_k;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="bg">&c_g;&c_r;&c_u;&c_h;&c_t;&c_ja;</trd>
    <trdgrp lng="ca">
      <trd>roncar</trd>,
      <trd>remorejar</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>schnauben</trd>,
      <trd>r&ouml;cheln</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>grunt</trd>,
      <trd>snort</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>roncar</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>szuszog</trd>,
      <trd>f&uacute;jtat</trd>,
      <trd>horkant</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>knorren</trd>,
      <trd>grommen</trd>
    </trdgrp>
    <trd lng="pl">chrapa&cacute;</trd>
    <trd lng="ro">sfor&abreve;i</trd>
    <trdgrp lng="ru">
      <trd>&c_h;&c_r;&c_ju;&c_k;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_f;&c_y;&c_r;&c_k;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_r;&c_y;&c_ch;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>chrochtat</trd>,
    <trd>chr&aacute;pat</trd>,
    <trd>frkat <klr>(k&uring;&ncaron;)</klr></trd>
  </trdgrp>
  <trd lng="de">schnarchen</trd>
  <trdgrp lng="fr">
    <trd>ren&acirc;cler</trd>,
    <trd>ronfler</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12356;&#12403;&#12365;&#12434;&#12363;&#12367;</trd>,
    <trd>&#12372;&#12358;&#12372;&#12358;&#12358;&#12394;&#12427;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chrochta&tcaron;</trd>,
    <trd>chr&aacute;pa&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_h;&c_r;&c_o;&c_p;&c_t;&c_i;</trd>,
    <trd>&c_h;&c_r;&c_ju;&c_k;&c_a;&c_t;&c_i; (&c_p;&c_r;&c_o; &c_s;&c_v;&c_i;&c_n;&c_ju;)</trd>,
    <trd>&c_f;&c_i;&c_r;&c_k;&c_a;&c_t;&c_i; (&c_p;&c_r;&c_o; &c_k;&c_o;&c_n;&c_ja;)</trd>
  </trdgrp>
</drv>
</art>
