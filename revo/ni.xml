<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>ni</rad>
</kap>
<drv mrk="ni.0">
   <kap><ofc>*</ofc><tld/></kap>
   <snc>
     <dif>
       <ref tip="super" cel="pronom.0o">Pronomo</ref>
       de la unua persono en multenombro.
     </dif>
   </snc>
  <trdgrp lng="kek">
   <trd>aa&#700;o</trd>
   <trd>ha&#700;o</trd>
   <trd>laa&#700;o</trd>
   </trdgrp>
  <trd lng="be">&c_m;&c_y;</trd>
  <trd lng="br">ni</trd>
  <trd lng="cs">n&yacute; <klr>(&rcaron;eck&eacute; p&iacute;smeno)</klr></trd>
  <trd lng="de">wir</trd>
  <trd lng="el">&epsilon;&my;&epsilon;&jota_ton;&sigma_fina;</trd>
  <trd lng="en">we</trd>
  <trd lng="es">nosotros</trd>
  <trd lng="fi">me</trd>
  <trd lng="fr">nous</trd>
  <trd lng="fy">wy</trd>
  <trdgrp lng="he">
    <trd>&alef;&nun;&vav;</trd>,
    <trd>&alef;&nun;&het;&nun;&vav;</trd>
  </trdgrp>
  <trd lng="hu">mi</trd>
  <trdgrp lng="id">
    <trd>kami</trd>,
    <trd>kita</trd>
  </trdgrp>
  <trd lng="io">ni</trd>
  <trd lng="it">noi</trd>
  <trd lng="nl">wij</trd>
  <trd lng="pl">my</trd>
  <trd lng="pt">n&oacute;s</trd>
  <trd lng="ru">&c_m;&c_y;</trd>
  <trd lng="sk">n&yacute; <klr>(gr&eacute;cke p&iacute;smeno)</klr></trd>
  <trd lng="sv">vi</trd>
  <trdgrp lng="tp">
    <trd>mi</trd>
    <trd>mi mute</trd>
  </trdgrp>
  <trd lng="vo">obs</trd>
  <trdgrp lng="zh">
     <trd>&#25105;&#20204;</trd>,
     <trd><klr>tr. </klr>&#25105;&#20497;<klr> [w&#466;men]</klr></trd>
   </trdgrp>
</drv>

<drv mrk="ni.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Poseda adj. a&ubreve; pron. signifanta: &leftquot;posedata de ni,
      rilata al ni&rightquot;.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_sh;</trd>
    (<trd>&c_n;&c_a;&c_sh;&c_a;</trd>)
  </trdgrp>
  <trdgrp lng="br">
    <trd>hol</trd>,
    <trd>hon</trd>,
    <trd>hor</trd>
  </trdgrp>
  <trd lng="cs">n&aacute;&scaron;</trd>
  <trd lng="de">unser</trd>
  <trd lng="el">&my;&alfa;&sigma_fina;</trd>
  <trd lng="en">our</trd>
  <trd lng="es">nuestro</trd>
  <trdgrp lng="fi">
    <trd>meid&auml;n</trd>,
    <trd>meid&auml;n -mme</trd>,
    <trd>-<ind>mme</ind></trd>
  </trdgrp>
  <trd lng="fr">notre</trd>
  <trd lng="fy">&uacute;s</trd>
  <trd lng="he">&shin;&lamed;&nun;&vav;</trd>
  <trdgrp lng="hu">
    <trd>mi -nk</trd>,
    <trd>mi&eacute;nk</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>milik</ind> kami</trd>,
    <trd><ind>milik</ind> kita</trd>
  </trdgrp>
  <trd lng="io">nia</trd>
  <trd lng="it">nostro</trd>
  <trd lng="nl">ons</trd>
  <trdgrp lng="pl">
    <trd>nasz</trd>,
    <trd>nasze</trd>,
    <trd>nasi</trd>
  </trdgrp>
  <trd lng="pt">nosso</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_sh;</trd>,
    <trd>&c_n;&c_a;&c_sh;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_sh;&c_je;</trd>
    <trd>&c_n;&c_a;&c_sh;&c_i;</trd>
  </trdgrp>
  <trd lng="sk">na&scaron;e</trd>
  <trd lng="sv">v&aring;r</trd>
  <trdgrp lng="tp">
    <trd>mi</trd>
    <trd>mi mute</trd>
  </trdgrp>
  <trdgrp lng="vo">
    <trd>obsik</trd>,
    <trd>obsa</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#25105;&#20204;&#30340;</trd>,
    <trd><klr>tr. </klr>&#25105;&#20497;&#30340;<klr>
    [w&#466;mende]</klr></trd>
  </trdgrp>
</drv>
</art>
