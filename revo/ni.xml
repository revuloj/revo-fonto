<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ni</rad>
</kap>
<drv mrk="ni.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Plurala pron. de unua persono.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_y;</trd>
  <trd lng="de">wir</trd>
  <trd lng="en">we</trd>
  <trd lng="es">nosotros</trd>
  <trd lng="fr">nous</trd>
  <trd lng="nl">wij</trd>
  <trd lng="pl">my</trd>
  <trd lng="ru">&c_m;&c_y;</trd>
  <trd lng="sv">vi</trd>
</drv>

<drv mrk="ni.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Poseda adj. a&ubreve; pron. signifanta: &leftquot;posedata de ni, rilata al ni&rightquot;.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_sh;</trd>
    (<trd>&c_n;&c_a;&c_sh;&c_a;</trd>)
  </trdgrp>
  <trd lng="de">unser</trd>
  <trd lng="en">our</trd>
  <trd lng="es">nuestro</trd>
  <trd lng="fr">notre</trd>
  <trd lng="nl">ons</trd>
  <trd lng="ru">&c_n;&c_a;&c_sh;</trd>
  <trd lng="sv">v&aring;r</trd>
</drv>
</art>
