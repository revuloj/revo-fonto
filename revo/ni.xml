<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ni</rad>
</kap>
<drv mrk="ni.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Plurala pron. de unua persono.
    </dif>
  </snc>                  
  <trd lng="be">&c_m;&c_y;</trd>
  <trd lng="br">ni</trd>
  <trd lng="de">wir</trd>
  <trd lng="el">&epsilon;&my;&epsilon;&jota_ton;&sigma_fina;</trd>
  <trd lng="en">we</trd>
  <trd lng="es">nosotros</trd>
  <trd lng="fr">nous</trd>
  <trdgrp lng="he">
    <trd>&alef;&nun;&vav;</trd>,
    <trd>&alef;&nun;&het;&nun;&vav;</trd>
  </trdgrp>
  <trd lng="hu">mi</trd>
  <trd lng="it">noi</trd>
  <trd lng="nl">wij</trd>
  <trd lng="pl">my</trd>
  <trd lng="pt">n&oacute;s</trd>
  <trd lng="ru">&c_m;&c_y;</trd>
  <trd lng="sv">vi</trd>
  <trd lng="vo">obs</trd>
  <trdgrp lng="zh">
    <trd>&#25105;&#20204;</trd>,
    <trd><klr>tr. </klr>&#25105;&#20497;<klr> [w&#466;men]</klr></trd>
  </trdgrp>
</drv>

<drv mrk="ni.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Poseda adj. a&ubreve; pron. signifanta: &leftquot;posedata de ni, rilata al ni&rightquot;.
    </dif>
  </snc>            
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_sh;</trd>
    (<trd>&c_n;&c_a;&c_sh;&c_a;</trd>)
  </trdgrp>
  <trdgrp lng="br">
    <trd>hol</trd>,
    <trd>hon</trd>,
    <trd>hor</trd>
  </trdgrp>
  <trd lng="de">unser</trd>
  <trd lng="el">&my;&alfa;&sigma_fina;</trd>
  <trd lng="en">our</trd>
  <trd lng="es">nuestro</trd>
  <trd lng="fr">notre</trd>
  <trd lng="he">&shin;&lamed;&nun;&vav;</trd>
  <trdgrp lng="hu">
    <trd>mi -nk</trd>,
    <trd>mi&eacute;nk</trd>
  </trdgrp>
  <trd lng="it">nostro</trd>
  <trd lng="nl">ons</trd>
  <trd lng="pt">nosso</trd>
  <trd lng="ru">&c_n;&c_a;&c_sh;</trd>
  <trd lng="sv">v&aring;r</trd>
  <trd lng="vo">obsik</trd>
  <trdgrp lng="zh">
    <trd>&#25105;&#20204;&#30340;</trd>,
    <trd><klr>tr. </klr>&#25105;&#20497;&#30340;<klr> [w&#466;mende]</klr></trd>
  </trdgrp>
</drv>
</art>
