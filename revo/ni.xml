          <bib>MT</bib>
          <lok>Genezo 31:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj parencoj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 31:32</lok>
        </fnt>;
      </ekz>
