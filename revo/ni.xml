<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ni.xml,v 1.32 2020/10/04 09:11:01 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>ni</rad>
</kap>
<drv mrk="ni.0">
   <kap><ofc>*</ofc><tld/></kap>
   <snc>
     <dif>
      <ref tip="super" cel="pronom.0o">Pronomo</ref>
      de la unua persono en pluralo, t.e.
      mi kaj alia(j), do vi, li, &scirc;i resp. ili:
      <ekz>
        <tld/> estas fratoj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 13:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        al <tld/> apartenas la akvo
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 26:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        a&ubreve;skultu <tld/>n
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 23:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/> konstruu al <tld/> urbon
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 11:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        vivanton <tld/> malhonoras, mortinton <tld/> adoras
        <klr>(kp <ref tip="super" cel="oni.0">oni</ref>)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>n &ccirc;iam ravas, kion <tld/> ne havas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trdgrp lng="kek">
   <trd>aa&#700;o</trd>
   <trd>ha&#700;o</trd>
   <trd>laa&#700;o</trd>
   </trdgrp>
  <trd lng="be">&c_m;&c_y;</trd>
  <trd lng="bo">&#3908;&#3851;&#3930;&#3964;&#3851;</trd>
  <trd lng="br">ni</trd>
  <trd lng="cs">n&yacute; <klr>(&rcaron;eck&eacute; p&iacute;smeno)</klr></trd>
  <trd lng="de">wir</trd>
  <trd lng="el">&epsilon;&my;&epsilon;&jota_ton;&sigma_fina;</trd>
  <trd lng="en">we</trd>
  <trd lng="es">nosotros</trd>
  <trd lng="fi">me</trd>
  <trd lng="fr">nous</trd>
  <trd lng="fy">wy</trd>
  <trdgrp lng="he">
    <trd>&alef;&nun;&vav;</trd>,
    <trd>&alef;&nun;&het;&nun;&vav;</trd>
  </trdgrp>
  <trd lng="hu">mi</trd>
  <trdgrp lng="id">
    <trd>kami</trd>,
    <trd>kita</trd>
  </trdgrp>
  <trd lng="io">ni</trd>
  <trd lng="it">noi</trd>
  <trdgrp lng="ja">
    <trd>&#31169;&#12383;&#12385; [<ind>&#12431;&#12383;&#12375;&#12383;&#12385;</ind>]</trd>,
    <trd>&#12431;&#12428;&#12431;&#12428;</trd>
  </trdgrp>
  <trd lng="nl">wij</trd>
  <trd lng="pl">my</trd>
  <trd lng="pt">n&oacute;s</trd>
  <trd lng="ru">&c_m;&c_y;</trd>
  <trd lng="sk">n&yacute; <klr>(gr&eacute;cke p&iacute;smeno)</klr></trd>
  <trd lng="sv">vi</trd>
  <trdgrp lng="tp">
    <trd>mi<klr> (ajnnombra)</klr></trd>,
    <trd>mi tu<klr> (dunombra)</klr></trd>,
    <trd>mi mute<klr> (plurnombra)</klr></trd>
  </trdgrp>
  <trd lng="vo">obs</trd>
  <trdgrp lng="zh">
     <trd>&#25105;&#20204;</trd>,
     <trd><klr>tr. </klr>&#25105;&#20497;<klr> [w&#466;men]</klr></trd>
   </trdgrp>
</drv>

<drv mrk="ni.0a">
  <kap><ofc>*</ofc><tld/>a <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url
  ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5123">ni'a</url></lok></fnt></kap>
  <snc>
    <dif>
      Poseda adjektivo a&ubreve; pronomo signifanta: &leftquot;posedata de ni,
      rilata al ni&rightquot;:
      <ekz>
        <tld/>a patro, <tld/>aj infanoj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 31:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj parencoj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 31:32</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a familio
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 43:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj malamikoj
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 1:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a Dio  
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 3:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj faroj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 5:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        malproksima de <tld/>a domo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        en <tld/>a loko ne estas <klr>[&#8230;]</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 29:26</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/> donos al vi <tld/>an konsenton, se vi <klr>[&#8230;]</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 34:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        li signifoklarigis al <tld/> <tld/>ajn son&gcirc;ojn, al &ccirc;iu li klarigis la&ubreve; lia son&gcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 41:12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_sh;</trd>
    (<trd>&c_n;&c_a;&c_sh;&c_a;</trd>)
  </trdgrp>
  <trdgrp lng="br">
    <trd>hol</trd>,
    <trd>hon</trd>,
    <trd>hor</trd>
  </trdgrp>
  <trd lng="cs">n&aacute;&scaron;</trd>
  <trd lng="de">unser</trd>
  <trd lng="el">&my;&alfa;&sigma_fina;</trd>
  <trd lng="en">our</trd>
  <trd lng="es">nuestro</trd>
  <trdgrp lng="fi">
    <trd>meid&auml;n</trd>,
    <trd>meid&auml;n -mme</trd>,
    <trd>-<ind>mme</ind></trd>
  </trdgrp>
  <trd lng="fr">notre</trd>
  <trd lng="fy">&uacute;s</trd>
  <trd lng="he">&shin;&lamed;&nun;&vav;</trd>
  <trdgrp lng="hu">
    <trd>mi -nk</trd>,
    <trd>mi&eacute;nk</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>milik</ind> kami</trd>,
    <trd><ind>milik</ind> kita</trd>
  </trdgrp>
  <trd lng="io">nia</trd>
  <trd lng="it">nostro</trd>
  <trdgrp lng="ja">
    <trd>&#31169;&#12383;&#12385;&#12398; [<ind>&#12431;&#12383;&#12375;&#12383;&#12385;&#12398;</ind>]</trd>,
    <trd>&#12431;&#12428;&#12431;&#12428;&#12398;</trd>
  </trdgrp>
  <trd lng="nl">ons</trd>
  <trdgrp lng="pl">
    <trd>nasz</trd>,
    <trd>nasze</trd>,
    <trd>nasi</trd>
  </trdgrp>
  <trd lng="pt">nosso</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_sh;</trd>,
    <trd>&c_n;&c_a;&c_sh;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_sh;&c_je;</trd>
    <trd>&c_n;&c_a;&c_sh;&c_i;</trd>
  </trdgrp>
  <trd lng="sk">na&scaron;e</trd>
  <trd lng="sv">v&aring;r</trd>
  <trdgrp lng="tp">
    <trd>mi<klr> (ajnnombra)</klr></trd>,
    <trd>pi mi tu<klr> (dunombra)</klr></trd>,
    <trd>pi mi mute<klr> (plurnombra)</klr></trd>
  </trdgrp>
  <trdgrp lng="vo">
    <trd>obsik</trd>,
    <trd>obsa</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#25105;&#20204;&#30340;</trd>,
    <trd><klr>tr. </klr>&#25105;&#20497;&#30340;<klr>
    [w&#466;mende]</klr></trd>
  </trdgrp>
</drv>
</art>
