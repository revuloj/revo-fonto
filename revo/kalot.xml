<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kalot.xml,v 1.9 2018/12/15 18:10:17 revo Exp $">
<kap>
  <rad>kalot</rad>/o
</kap>
<drv mrk="kalot.0o">
  <kap><tld/>o</kap>
  <snc mrk="kalot.0o.cxapeto">
    <dif>
      &Ccirc;apeto portata precipe de altaj klerikoj:
      <ekz>
        sendepende &ccirc;u la devotuloj ka&scirc;as sian verton sub <tld/>o, sian hipokritan mienon sub vualo a&ubreve; pendumas sin sur krucifikson
        <fnt>
          <bib>Monato</bib>
          <aut>Gilbert Ledon</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007175.html">Muroj dumiljara solidas</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a &ccirc;apo
        <fnt>
          <bib>Monato</bib>
          <aut>Gilbert Ledon</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007175.html">Muroj dumiljara solidas</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Pileolus</trd>,
      <trd>Scheitelk&auml;ppchen</trd>
    </trdgrp>
  </snc>
  <snc mrk="kalot.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <ref tip="dif" cel="cxap.verto0o.MAT">verto&ccirc;apo.</ref>
    <trd lng="de">Kugelkappe</trd>
  </snc>
  <trd lng="cs">kalota</trd>
  <trdgrp lng="de">
    <trd>Kalotte</trd>
  </trdgrp>
  <trd lng="en">spherical <ind>cap</ind></trd>
  <trd lng="fr">calotte<klr tip="ind"> (sph&eacute;rique)</klr></trd>
  <trd lng="hu">g&ouml;mbs&uuml;veg</trd>
  <trd lng="nl">kalot</trd>
  <trd lng="pl">czasza<klr tip="ind"> (kulista)</klr></trd>
  <trd lng="pt">calota</trd>
  <trd lng="ru">&c_sh;&c_a;&c_p;&c_k;&c_a; &c_s;&c_f;&c_je;&c_r;&c_y;</trd>
  <trd lng="sk">kalota</trd>
</drv>
</art>

