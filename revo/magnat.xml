    <uzo tip="fak">EKON</uzo>
    <dif>
      Eminenta potenculo en industrio, komerco, financo.
    </dif>
    <trdgrp lng="de">
      <trd>Wirtschaftsboss</trd>,
      <trd>Magnat [arkaika]</trd>,
      <trd>Industriekapit&auml;n [eksmoda]</trd>
    </trdgrp>
    <trd lng="en">magnate</trd>
    <trdgrp lng="hu">
      <trd>m&aacute;gn&aacute;s</trd>,
      <trd>iparm&aacute;gn&aacute;s</trd>,
      <trd>iparb&aacute;r&oacute;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_g;&c_n;&c_a;&c_t;</trd>
  <trd lng="fr">magnat</trd>
  <trd lng="nl">magnaat</trd>
  <trd lng="ru">&c_m;&c_a;&c_g;&c_n;&c_a;&c_t;</trd>
</drv>
</art>
