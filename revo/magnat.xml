<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>magnat</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="magnat.0o">
  <kap><tld/>o</kap>
  <snc mrk="magnat.0o.POL">
    <dif>
      Eminenta <ref tip="super" cel="nobel.0o.POL">nobelo</ref> en
      Hungarujo kaj Polujo.
    </dif>
    <trdgrp lng="hu">
      <trd>b&aacute;r&oacute;</trd>,
      <trd>m&aacute;gn&aacute;s</trd>,
      <trd>f&odblac;nemes</trd>
    </trdgrp>
    <trd lng="pt">magnata</trd>
  </snc>
  <snc mrk="magnat.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Eminenta potenculo en industrio, komerco, financo:
      <ekz>
        ili estas <klr>...</klr> fabrikposedantoj, industriaj <tld/>oj
        <fnt><bib>SatirRak</bib>, <lok>la fluidumo de la
        &ccirc;iovido</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Wirtschaftsboss</trd>,
      <trd>Magnat [arkaika]</trd>,
      <trd>Industriekapit&auml;n [eksmoda]</trd>
    </trdgrp>
    <trd lng="en">magnate</trd>
    <trdgrp lng="hu">
      <trd>m&aacute;gn&aacute;s</trd>,
      <trd>iparm&aacute;gn&aacute;s</trd>,
      <trd>iparb&aacute;r&oacute;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>magnata</trd>,
      <trd>chef&atilde;o</trd>,
      <trd>maioral</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_g;&c_n;&c_a;&c_t;</trd>
  <trd lng="fr">magnat</trd>
  <trd lng="nl">magnaat</trd>
  <trd lng="ru">&c_m;&c_a;&c_g;&c_n;&c_a;&c_t;</trd>
</drv>
</art>
