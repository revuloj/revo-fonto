</kap>
<drv mrk="magnat.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Eminenta <ref tip="super" cel="nobel.0o">nobelo</ref> en Hungarujo
      kaj Polujo.
    </dif>
    <trdgrp lng="hu">
      <trd>b&aacute;r&oacute;</trd>,
      <trd>m&aacute;gn&aacute;s</trd>,
      <trd>f&odblac;nemes</trd>
