<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>onobrik</rad>/o
</kap>
<drv mrk="onobri.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Onobrychis</trd>). Unu specio estas uzata kiel
      fura&gcirc;a herbo.
    </dif>
  </snc>
  <trd lng="de">Esparsette</trd>
  <trdgrp lng="hu">
    <trd>baltacim</trd>,
    <trd>eszparzetta</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>esparcette</trd>,
    <trd>hanenkam</trd>
  </trdgrp>
  <trd lng="pt">on&oacute;briquis</trd>
  <trd lng="ru">&c_e;&c_s;&c_p;&c_a;&c_r;&c_c;&c_je;&c_t;</trd>
</drv>
</art>
