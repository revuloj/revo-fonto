<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: flustr.xml,v 1.24 2023/08/31 13:11:30 revo Exp $">
<kap><ofc>3</ofc>
<rad>flustr</rad>/i</kap>
<drv mrk="flustr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malla&ubreve;te paroli:
      <ekz>
        la &ccirc;efepiskopo <tld/>is malbonajn vortojn en la orelon al la
        re&gcirc;o
        <fnt><bib>Fab1</bib>, <lok>Sova&gcirc;aj cignoj</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ctl>sed la aero donas al vi la spiradon!</ctl> <tld/>is la vo&ccirc;o
        de la poeto
        <fnt><bib>Fab1</bib>, <lok>Galo&scirc;oj de feli&ccirc;o</lok></fnt>.
      </ekz>
      <ekz>
        li altiris &scirc;in, <klr>[&#x2026;]</klr> kaj <tld/>is: iru al la pordeto
        <klr>[&#x2026;tie]</klr> ni renkontos unu la alian
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        bele, tre bele! li <tld/>is al si mem
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        subite lia interno estis lumigita de nova sento, kvaza&ubreve; vo&ccirc;o <tld/>us al li: nun
        vi estas plena&gcirc;a
        <fnt>
          <bib>HsT</bib>
          <lok>Heredanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la nekonato minacis Ivanon per la fingro kaj <tld/>is: &leftquot;&scirc;&scirc;!&rightquot;
        <fnt>
          <bib>MkM</bib>
          <lok>13</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        pro &scirc;i mi ne <tld/>os <klr>(<ref cel="perfid.el0i">elperfidos</ref>)</klr>
        al iu unu vorton pri tiu malbenita afero
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="suflor.0i">suflori</ref>
    </refgrp>
    <trd lng="be">&c_sh;&c_a;&c_p;&c_t;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="de">
      <trd>fl&uuml;stern</trd>,
      <trd>leise <ind>sprechen</ind></trd>
    </trdgrp>
  <trdgrp lng="es">
    <trd>cuchichear</trd>,
    <trd>susurrar</trd>,
    <trd>murmurar</trd>
  </trdgrp>
    <trd lng="hu">s&uacute;g</trd>
    <trd lng="nl">fluisteren</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <uzo tip="klr">(io)</uzo>
    <dif>
      <ref tip="dif" cel="murmur.0eti">Murmureti</ref>, brueti:
      <ekz>
        &scirc;iajn plej turmentajn dolorojn forlavis la <tld/>antaj ondoj
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi a&ubreve;das la <tld/>an melodion de arboj
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>?
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="susur.0i">susuri</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_sh;&c_a;&c_p;&c_t;&c_a;&c_c;&c_mol; <klr>(&c_p;&c_je;&c_r;&c_a;&c_n;.)</klr></trd>,
      <trd>&c_sh;&c_a;&c_m;&c_a;&c_c;&c_je;&c_c;&c_mol;</trd>,
      <trd>&c_sh;&c_a;&c_l;&c_ja;&c_s;&c_mol;&c_c;&c_je;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">murmeln</trd>
    <trdgrp lng="es">
      <trd>murmurar</trd>,
      <trd>susurrar</trd>
    </trdgrp>
    <trd lng="hu">susorog</trd>
    <trd lng="nl">ruisen</trd>
    <trd lng="ru">&c_sh;&c_je;&c_l;&c_je;&c_s;&c_t;&c_je;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="flustr.0i.FON">
    <uzo tip="fak">FON</uzo>
    <dif>
      Paroli per <tld/>o<sncref ref="flustr.0o.FON"/>.
    </dif>
    <trd lng="be">&c_sh;&c_a;&c_p;&c_t;&c_a;&c_c;&c_mol;</trd>
    <trd lng="de">fl&uuml;stern</trd>
    <trd lng="nl">fluisteren</trd>
    <trd lng="hu">suttog</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>&scaron;eptat</trd>,
    <trd>po&scaron;eptat</trd>,
    <trd>za&scaron;eptat</trd>,
    <trd>&scaron;eptnout</trd>,
    <trd>&scaron;ustit</trd>
  </trdgrp>
  <trd lng="en">whisper</trd>
  <trdgrp lng="fr">
    <trd>chuchoter</trd>,
    <trd>murmurer</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3055;&#x3055;&#x3084;&#x304F;</trd>,
    <trd>&#x5C0F;&#x58F0;&#x3067;&#x8A71;&#x3059;  [<ind>&#x3053;&#x3054;&#x3048;&#x3067;&#x306F;&#x306A;&#x3059;</ind>]</trd>,
    <trd>&#x3055;&#x3089;&#x3055;&#x3089;&#x97F3;&#x3092;&#x305F;&#x3066;&#x308B;  [<ind>&#x3055;&#x3089;&#x3055;&#x3089;&#x304A;&#x3068;&#x3092;&#x305F;&#x3066;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="pl">szepta&cacute;</trd>
  <trdgrp lng="pt">
    <trd>cochichar</trd>,
    <trd>segredar</trd>,
    <trd>bisbilhotar</trd>,
    <trd>ciciar</trd>,
    <trd>murmurar</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_je;&c_p;&c_t;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>po&scaron;u&scaron;k&aacute;va&tcaron;</trd>,
    <trd>&scaron;epka&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_sh;&c_je;&c_p;&c_o;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_sh;&c_je;&c_p;&c_o;&c_t;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_sh;&c_je;&c_p;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_sh;&c_je;&c_l;&c_je;&c_s;&c_t;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_sh;&c_a;&c_r;&c_u;&c_d;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_d;&c_z;&c_ju;&c_r;&c_ch;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="flustr.0o">
  <kap><tld/>o</kap>
  <snc mrk="flustr.0o.KOMUNE">
    <dif>
      Malla&ubreve;ta parolo de <tld/>anto:
      <ekz>
        lia parolado fari&gcirc;is <tld/>o
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ne povis a&ubreve;di la <tld/>on de la paro
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        li a&ubreve;dis <tld/>ojn proksime kaj kiel eble plej silente forrampis en alia direkto
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekmurmuris la <tld/>o de pre&gcirc;oj
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LXXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia vo&ccirc;o fari&gcirc;is pli kaj pli malla&ubreve;ta, <klr>[&#x2026;]</klr> post momento &gcirc;i redukti&gcirc;is
        al <tld/>eto
        <fnt>
          <bib>CKv</bib>
          <lok>23</lok>
        </fnt>;
      </ekz>
      <ekz>
        la publiko renkontis lin per nedifinebla <tld/>o, trakurinta la vicojn
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">s&uacute;g&aacute;s</trd>
  </snc>
  <snc mrk="flustr.0o.FON">
    <uzo tip="fak">FON</uzo>
    <dif>
      Parolmaniero, meza inter la vo&ccirc;o kaj la spiro, kiu konsistas
      el da&ubreve;ra bruero, produktata de la spirblovo, kiam &gcirc;i frotas
      sed ne vibrigas la malfermatajn vo&ccirc;kordojn.
    </dif>
    <trd lng="hu">suttog&aacute;s</trd>
  </snc>
  <trd lng="be">&c_sh;&c_e;&c_p;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>&scaron;elest</trd>,
    <trd>&scaron;epot</trd>,
    <trd>&scaron;ust&ecaron;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Fl&uuml;stern</trd><trdgrp lng="de">
    <trd>Fl&uuml;stern</trd>,
    <trd>Murmeln</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>susurro</trd>,
    <trd>cuchicheo</trd>,
    <trd>murmuro</trd>
  </trdgrp>
  <trd lng="fr">chuchotement</trd>
  <trd lng="ja">&#x3055;&#x3055;&#x3084;&#x304D;</trd>
  <trd lng="nl">het <ind>fluisteren</ind></trd>
  <trdgrp lng="pl">
    <trd>szept</trd>,
    <trd>szmer</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_jo;&c_p;&c_o;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>&scaron;elest</trd>,
    <trd>&scaron;epot</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_sh;&c_je;&c_p;&c_ib;&c_t;</trd>,
    <trd>&c_sh;&c_je;&c_p;&c_o;&c_t;&c_ib;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_sh;&c_a;&c_r;&c_u;&c_d;&c_ib;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_d;&c_z;&c_ju;&c_r;&c_ch;&c_a;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="flustr.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Karakterizata per <tld/>o:
      <ekz>
        <uzo tip="fak">FON</uzo>
        <tld/>a vokalo, konsonanto.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">chuchot&eacute;</trd>
  <trd lng="hu">suttog&oacute;</trd>
  <trd lng="ja">&#x3055;&#x3055;&#x3084;&#x304D;&#x58F0;&#x306E;  [<ind>&#x3055;&#x3055;&#x3084;&#x304D;&#x3053;&#x3048;&#x306E;</ind>]</trd>
  <trd lng="nl">fluister-</trd>
  <trdgrp lng="de">
    <trd>Fl&uuml;ster-</trd>,
    <trd>fl&uuml;sternd</trd>
  </trdgrp>
  <trd lng="pl">szeptany</trd>
</drv>
</art>
