<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Mars</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="mars.0o">
  <kap><tld/>o</kap>
  <snc mrk="mars.0o.mita">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Romia <ref tip="super" cel="di.0o.politeisma">dio</ref> de milito.
      Analogo de helena <ref tip="vid" cel="ares.0o">Areso</ref>.
    </dif>
    <trdgrp lng="zh">
      <trd>&#33639;&#24785;</trd>,
      <trd><klr>tr. </klr>&#29074;&#24785;<klr> [y&iacute;nghu&ograve;]</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="mars.0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      La kvara (la&ubreve; distanco de la <ref tip="vid"
      cel="sun.0o.nia">Suno</ref>)
      <ref tip="super" cel="planed.0o">planedo</ref>
      de la <ref tip="malprt" cel="sun.0sistemo">sunsistemo</ref>:
      <ekz>
        dum multaj jaroj oni kredis, ke ekzistas vivo sur la planedo <tld/>o
        <fnt>
        <aut>Amri Wandel</aut>,
        <vrk>&Ccirc;u ni estas solaj?</vrk>,
        <lok><url ref="http://esperanto.org/AEK/biblioteko.html">Virtuala
        Biblioteko de AEKo</url>, [vidita je 2006-01-24]</lok>
        </fnt>.        
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dejm.0o">Dejmo</ref>,
      <ref cel="fob.0o">Fobo</ref>
    </refgrp>
    <trd lng="zh">&#28779;&#26143;<klr> [Hu&#466;x&#299;ng]</klr></trd>
  </snc>
  <trd lng="be">&c_M;&c_a;&c_r;&c_s;</trd>
  <trd lng="de">Mars</trd>
  <trd lng="el">&Alfa_acute;&rho;&eta;&sigma_fina;</trd>
  <trd lng="en">Mars</trd>
  <trd lng="es">Marte</trd>
  <trd lng="fr">Mars</trd>
  <trd lng="hu">Mars</trd>  
  <trd lng="it">Marte</trd>
  <trd lng="nl">Mars</trd>  
  <trd lng="pt">Marte</trd>
  <trd lng="ru">&c_M;&c_a;&c_r;&c_s;</trd>
</drv>

<drv mrk="mars.0a">
  <kap><tld lit="m"/>a</kap>
  <snc mrk="mars.0a.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Rilata al <tld/>o<sncref ref="mars.0o.mita"/>.
    </dif>
    <trd lng="be">&c_m;&c_a;&c_r;&c_s;&c_a;&c_w;</trd>
    <trd lng="ru">&c_m;&c_a;&c_r;&c_s;&c_o;&c_v;</trd>
  </snc>
  <snc mrk="mars.0a.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Rilata al <tld/>o<sncref ref="mars.0o.AST"/>.
    </dif>
    <trd lng="be">&c_m;&c_a;&c_r;&c_s;&c_ib;&c_ja;&c_n;&c_s;&c_k;&c_ib;</trd>
    <trd lng="ru">&c_m;&c_a;&c_r;&c_s;&c_i;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
  <trd lng="de">Mars-</trd>
  <trd lng="en">Martian</trd>
  <trd lng="es">marciano</trd>
  <trd lng="fr">martien<klr> (adj.)</klr></trd>
  <trd lng="hu">Mars-</trd>  
  <trd lng="it">marziano</trd>
  <trd lng="nl">Mars-</trd>  
  <trd lng="pt">marciano</trd>
</drv>

<drv mrk="mars.0ano">
  <kap><tld lit="m"/>ano</kap>
  <snc>
    <dif>
      Lo&gcirc;anto de <tld/>o<sncref ref="mars.0o.AST"/>
      (en sciencfikcia literaturo).
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_s;&c_ib;&c_ja;&c_n;&c_ib;&c_n;</trd>
  <trd lng="de">Marsbewohner</trd>
  <trd lng="en">Martian</trd>
  <trd lng="es">marciano</trd>
  <trd lng="fr">martien<klr> (subst.)</klr></trd>
  <trd lng="hu">marslak&oacute;</trd>  
  <trd lng="it">marziano</trd>
  <trdgrp lng="nl">
     <trd>marsbewoner</trd>,
     <trd>marsmannetje</trd>
  </trdgrp>
  <trd lng="pt">marciano</trd>
  <trd lng="ru">&c_m;&c_a;&c_r;&c_s;&c_i;&c_a;&c_n;&c_i;&c_n;</trd>
</drv>
</art>
