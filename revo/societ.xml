<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>societ</rad>/o
</kap>
<drv mrk="societ.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="societ.0o.institucio">
    <dif>
      Da&ubreve;ra kuni&gcirc;o de pli malpli multaj personoj sub komuna regularo por
      atingi komunan celon, &ccirc;u plezuran, &ccirc;u profitan, &ccirc;u komercan:
      <ekz>
        Esperantismo devas esti movado ne nur <tld/>a sed socia.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Gesellschaft</trd>,
      <trd>Verein</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>gezelschap</trd>,
      <trd>vereniging</trd>
    </trdgrp>
  </snc>
  <snc mrk="societ.0o.medio">
    <dif>
      Kutimaj interrilatoj inter sammoraj personoj:
      <ekz>
        mi malamas <tld/>on de malbonfarantoj, Kaj kun malpiuloj mi ne
        sidos
        <fnt><bib>MT</bib>, <lok>&Psa; 26:5</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mond.0umo">mondumo</ref>
    </refgrp>
  </snc>
  <snc mrk="societ.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Jura persono kun laule&gcirc;e difinitaj rajtoj kaj devoj:
      <ekz>
        akcia <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_v;&c_a;&c_r;&c_y;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_m;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_p;&c_o;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_g;&c_u;&c_r;&c_t;</trd>
  </trdgrp>
  <trd lng="cs">spolek</trd>
  <trd lng="fr">soci&eacute;t&eacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_u;&c_zh;&c_o;&c_k;</trd>,
    <trd>&c_o;&c_b;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_o;&c_b;&c_malmol;&c_je;&c_d;&c_i;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_k;&c_o;&c_m;&c_p;&c_a;&c_n;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="societ.akci0o">
  <kap>akcia <tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>JV</bib></fnt>
  <snc>
    <dif>
      Societo (3), kies posedantoj estas identigitaj surbaze de akcioj.
      Akcia kompanio.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Aktiengesellschaft</trd>,
    <trd>AG</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>joint-stock company</trd>,
    <trd>company with share capital</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>SA<klr> (soci&eacute;t&eacute; par actions)</klr></trd>,
    <trd>soci&eacute;t&eacute; par actions<klr> (SA)</klr></trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>aktiebolag</trd>,
    <trd>AB</trd>
  </trdgrp>
</drv>
</art>
