<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pulv.xml,v 1.20 2022/07/04 22:31:13 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pulv</rad>/o
</kap>
<drv mrk="pulv.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Eksplodema pulvoro el miksitaj salpetro, karbo kaj sulfuro:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li ne elpensis la <tld/>on<fnt>Z</fnt>
        <klr>(ne estas inteligenta)</klr>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="dinami2.0o">dinamito</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_o;&c_r;&c_a;&c_h;</trd>
  <trd lng="ca">p&oacute;lvora</trd>
  <trd lng="cs">st&rcaron;eln&yacute; prach</trd>
  <trd lng="de">Schie&szlig;pulver</trd>
  <trd lng="en">gunpowder</trd>
  <trd lng="es">p&oacute;lvora</trd>
  <trd lng="fa">&a_b;&a_A;&a_r;&a_w;&a_t;</trd>
  <trd lng="fr">poudre<klr> (explosive)</klr></trd>
  <trd lng="hu">l&odblac;por</trd>
  <trdgrp lng="nl">
    <trd>kruit</trd>,
    <trd>buskruit</trd>
  </trdgrp>
  <trd lng="pl">proch czarny</trd>
  <trd lng="pt">p&oacute;lvora</trd>
  <trd lng="ro">praf de pu&#537;c&abreve;</trd>
  <trd lng="ru">&c_p;&c_o;&c_r;&c_o;&c_h;</trd>
  <trd lng="sk">pu&scaron;n&yacute; prach</trd>
  <trd lng="uk">&#x43F;&#x43E;&c_r;&#x43E;&c_h;</trd>
</drv>
</art>
