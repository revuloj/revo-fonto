<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pulv.xml,v 1.24 2024/02/17 12:12:49 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pulv</rad>/o
</kap>
<drv mrk="pulv.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Eksplodema <ref tip="super" cel="pulvor.0o">pulvoro</ref> uzata
      por akceli pafa&jcirc;on:
      <ekz>
        unu fajrero estas sufi&ccirc;a, por eksplodigi <tld/>on
        <fnt>
          <bib>F</bib>
          <lok>&FE; 41</lok>
        </fnt>;
      </ekz>
      <ekz>
        blua fumo de <tld/>o tiri&gcirc;is kiel nuboj tra inter la mallumaj arboj
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        tondris la kanonoj, <klr>[&#x2026;]</klr> la &scirc;ipoj staris envolvitaj en fumo de <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Dano Holger</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni plenigu da <tld/>o la subterajn galeriojn de la malnova fortika&jcirc;o
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>on ne elpensis
        <klr>(ne estas inteligenta)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        eksplodema kiel <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="eksplo.0ajxo">eksploda&jcirc;o</ref>,
      <ref cel="dinami2.0o">dinamito</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_o;&c_r;&c_a;&c_h;</trd>
  <trd lng="ca">p&oacute;lvora</trd>
  <trd lng="cs">st&rcaron;eln&yacute; prach</trd>
  <trd lng="de">Schie&szlig;pulver</trd>
  <trd lng="en">gunpowder</trd>
  <trd lng="es">p&oacute;lvora</trd>
  <trd lng="fa">&a_b;&a_A;&a_r;&a_w;&a_t;</trd>
  <trd lng="fr">poudre<klr> (explosive)</klr></trd>
  <trd lng="hu">l&odblac;por</trd>
  <trd lng="ja">&#x706B;&#x85AC;  <pr>&#x304B;&#x3084;&#x304F;</pr></trd>
  <trdgrp lng="nl">
    <trd>kruit</trd>,
    <trd>buskruit</trd>
  </trdgrp>
  <trd lng="pl">proch czarny</trd>
  <trd lng="pt">p&oacute;lvora</trd>
  <trd lng="ro">praf de pu&#x219;c&abreve;</trd>
  <trd lng="ru">&c_p;&c_o;&c_r;&c_o;&c_h;</trd>
  <trd lng="sk">pu&scaron;n&yacute; prach</trd>
  <trd lng="uk">&c_p;&c_o;&c_r;&c_o;&c_h;</trd>
  <trdgrp lng="zh">
    <trd>&#x706B;&#x836F; <pr>hu&#x1D2;y&#xE0;o</pr></trd>,
    <trd>&#x706B;&#x85E5; <pr>hu&#x1D2;y&#xE0;o</pr></trd>
  </trdgrp>
</drv>
</art>
