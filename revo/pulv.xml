<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pulv</rad>/o
</kap>
<drv mrk="pulv.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Eksplodema pulvoro el miksitaj salpetro, karbo kaj sulfuro:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li ne elpensis la <tld/>on<fnt>Z</fnt>
        <klr>(ne estas inteligenta)</klr>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="dinami2.0o">dinamito</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Schie&szlig;pulver</trd>
  <trdgrp lng="nl">
    <trd>kruit</trd>,
    <trd>buskruit</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_r;&c_o;&c_h;</trd>
</drv>
</art>
