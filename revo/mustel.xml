<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>mustel</rad>/o
</kap>
<drv mrk="mustel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="mustel.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Mustela</trd>), al kiu apartenas
      <refgrp tip="sub">
        <ref cel="ermen.0o">ermeno</ref>
        <ref cel="fur.0o">furo</ref>,
        <ref cel="putor.0o">putoro</ref>,
        kaj. a.
      </refgrp>
    </dif>
  </snc>
</drv>

</art>
