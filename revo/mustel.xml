<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mustel</rad>/o
</kap>
<drv mrk="mustel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="mustel1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Mustela</trd>), al kiu apartenas
      <refgrp tip="sub">
        <ref cel="ermen.0o">ermeno</ref>
        <ref cel="fur.0o">furo</ref>,
        <ref cel="putor.0o">putoro</ref>,
        k.a.
      </refgrp>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="hu">meny&eacute;t</trd>
  <trd lng="nl">wezel</trd>
  <trdgrp lng="pt">
    <trd>mustela</trd>,
    <trd>doninha</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_u;&c_n;&c_i;&c_c;&c_a;</trd>
</drv>
</art>
