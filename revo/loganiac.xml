<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: loganiac.xml,v 1.1 2025/11/28 08:40:32 afido Exp $">
    <kap>
      <rad>loganiac</rad>/oj
      <fnt><bib>PIV3</bib></fnt>
    </kap>

<drv mrk="loganiac.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_familioj" cel="famili.0o.BIO">Familio</ref>
      de lignecaj vegeta&jcirc;oj (<trd lng="la">Loganiaceae</trd>), kun kelkaj tre venenaj specioj,
      el la klaso de <ref tip="malprt" cel="magnoli.0oj">magnolioj</ref>:
      <ekz>
        striknino <klr>[&#x2026;]</klr> el <tld lit="L"/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19017"
              >Alkaloido</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        multaj <tld/>oj estas venenaj, inkluzive de la vomnukso
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=675082"
              >Loganiacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Brechnussgew&auml;chse</trd>,
      <trd>Loganiengew&auml;chse</trd>
    </trdgrp>
  </snc>
</drv>

  </art>

