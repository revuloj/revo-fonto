<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: smeral.xml,v 1.25 2025/05/05 06:40:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>smerald</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="smeral.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="smeral.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Multekosta <ref tip="super" cel="sxton.juvel0o.MIN">
      juvel&scirc;tono</ref> de verda koloro,
      speco de <ref tip="super" cel="beril.0o.MIN">
      berilo</ref>:
      <ekz>
        glavo <klr>[&#8230;]</klr> ornamita per <tld/>oj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        multekostaj &scirc;tonoj vin kovris: rubeno, topazo, diamanto, sukceno, berilo,
        jaspiso, safiro, smeraldo, ametisto
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 28:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la tero balda&ubreve; kovri&gcirc;is de <tld/>a verda&jcirc;o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la venkita maro vasti&gcirc;is &ccirc;irka&ubreve;e, kiel <tld/>a ebena&jcirc;o
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i metis en lian manon skatoleton, eltran&ccirc;itan el unu sola <tld/>o
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kato <klr>[&#8230;]</klr>, malmultekosta &ccirc;ina ludilo, <klr>[&#8230;]</klr> gaje rigardis
        al &scirc;i per la rondaj <tld/>aj okuloj
        <fnt>
          <bib>Kon11</bib>
          <lok>Rakonto</lok>
        </fnt>;
      </ekz>
      <ekz>
        Irlando, la tiel nomata Verda (a&ubreve; <tld lit="S"/>a) Insulo
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2006/008827.php"
              >Pri kutimoj kaj kalsonoj</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/d/dd/(Muzo)_Emerald_crystal_in_its_matrix.jpg/317px-(Muzo)_Emerald_crystal_in_its_matrix.jpg"
      prm="CC BY-SA 3.0"
      ><tld/>a kristalo, 2cm longa, minejo Muzo, Kolombio
        <fnt>
          <aut>Didier Descouens, propra verko</aut>
          <lok><url ref="&WCW;/File:(Muzo)_Emerald_crystal_in_its_matrix.jpg"
              >https://commons.wikimedia.org/wiki/File:(Muzo)_Emerald_crystal_in_its_matrix.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_m;&c_a;&c_r;&c_a;&c_g;&c_d;</trd>,
    <trd>&c_ib;&c_z;&c_u;&c_m;&c_r;&c_u;&c_d;</trd>
  </trdgrp>
  <trd lng="bg">&c_i;&c_z;&c_u;&c_m;&c_r;&c_u;&c_d;</trd>
  <trd lng="ca">maragda</trd>
  <trdgrp lng="cs">
    <trd>odr&uring;da berylu</trd>,
    <trd>smaragd</trd>
  </trdgrp>
  <trd lng="de">Smaragd</trd>
  <trd lng="en">emerald</trd>
  <trd lng="fa">&a_z;&a_m;&a_r;&a_d;</trd>
  <trd lng="fr">&eacute;meraude</trd>
  <trd lng="hu">smaragd</trd>
  <trd lng="id">zamrud</trd>
  <trdgrp lng="ja">
    <trd>&#12456;&#12513;&#12521;&#12523;&#12489;</trd>,
    <trd>&#32736;&#29577;  <pr>&#12377;&#12356;&#12366;&#12423;&#12367;</pr></trd>
  </trdgrp>
  <trd lng="nl">smaragd</trd>
  <trd lng="pl">szmaragd</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_u;&c_m;&c_r;&c_u;&c_d;</trd>,
    <trd>&c_s;&c_m;&c_a;&c_r;&c_a;&c_g;&c_d;</trd>
  </trdgrp>
  <trd lng="sk">smaragd</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_m;&c_a;&c_r;&c_a;&c_g;&c_d;</trd>,
    <trd>&c_ib;&c_z;&c_u;&c_m;&c_r;&c_u;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#31062;&#27597;&#32160; <pr>z&#468;m&#468;l&ugrave;:</pr></trd>,
    <trd>&#31062;&#27597;&#32511; <pr>z&#468;m&#468;l&ugrave;:</pr></trd>,
    <trd>&#32160;&#26609;&#29577; <pr>l&#476;zh&ugrave;y&ugrave;</pr></trd>,
    <trd>&#32511;&#26609;&#29577; <pr>l&#476;zh&ugrave;y&ugrave;</pr></trd>,
    <trd>&#32736;&#32160;&#33394; <pr>cu&igrave;l&#476;s&egrave;</pr></trd>,
    <trd>&#32736;&#32511;&#33394; <pr>cu&igrave;l&#476;s&egrave;</pr></trd>
  </trdgrp>
</drv>
</art>
