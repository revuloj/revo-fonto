<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>smerald</rad>/o
</kap>
<drv mrk="smeral.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="smeral.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Multekosta <ref tip="super" 
      cel="sxton.juvel0o.MIN">juvel&scirc;tono</ref> de verda koloro:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>aj herbejoj<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_m;&c_a;&c_r;&c_a;&c_g;&c_d;</trd>, 
    <trd>&c_ib;&c_z;&c_u;&c_m;&c_r;&c_u;&c_d;</trd>
  </trdgrp>
  <trd lng="de">Smaragd</trd>
  <trd lng="fr">&eacute;meraude</trd>
  <trd lng="hu">smaragd</trd>
  <trd lng="nl">smaragd</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_u;&c_m;&c_r;&c_u;&c_d;</trd>,
    <trd>&c_s;&c_m;&c_a;&c_r;&c_a;&c_g;&c_d;</trd>
  </trdgrp>
</drv>
</art>
