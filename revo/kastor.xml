<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kastor</rad>/o
</kap>
<drv mrk="kastor.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="kastor1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Castor</trd>), granda rodulo kun lar&gcirc;a plata vosto,
      rodanta arbojn kaj konstruanta lo&gcirc;ejojn kun subakvaj enirejoj
      kaj digoj por reguligi la akvoaltecon.
    </dif>
  </snc>
  <trd lng="de">Biber</trd>
  <trd lng="en">beaver</trd>
  <trd lng="nl">bever</trd>
  <trd lng="ru">&c_b;&c_o;&c_b;&c_r;</trd>
</drv>
</art>
