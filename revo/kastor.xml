<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kastor.xml,v 1.27 2022/05/07 09:30:09 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kastor</rad>/o
</kap>
<drv mrk="kastor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kastor.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda <ref tip="lst" cel="rongx.0ulo" lst="voko:ron&gcirc;uloj">ron&gcirc;ulo</ref>
      el Kanado kaj E&ubreve;ropo kun lar&gcirc;a plata vosto,
      ron&gcirc;anta arbojn 
      kaj konstruanta lo&gcirc;ejojn kun subakvaj enirejoj kaj 
      <ref cel="dig.0o">digoj</ref> por reguligi la akvoaltecon;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio 
      <ref tip="malprt" cel="kastor1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Castor</trd>):
      <ekz>
        li estis vestita en peltomantelo kun <tld/>a
        <klr>(<tld/>fela)</klr> kolumo
        <fnt><bib>Metrop</bib></fnt>;
      </ekz>
      <ekz>
        Kanado prezentis <klr>[&#8230;]</klr> sciurojn, <tld/>ojn, alkojn, lupojn, faritajn el
        ligno
        <fnt>
          <bib>Kon11</bib>
          <aut>&Jcirc;enja Amis</aut>
          <vrk>Lando de Espero</vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/6/6b/American_Beaver.jpg/239px-American_Beaver.jpg" prm="CC BY-SA
    2.0">Amerika <tld/>o
        <fnt>
          <aut>Steve el Va&scirc;intono, Usono</aut>
          <lok><url ref="&WCW;/File:American_Beaver.jpg">&WCW;/File:American_Beaver.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <ref tip="hom" cel="kastor2.0o">Kastoro (frato de Polukso)</ref>
  <trd lng="be">&c_b;&c_a;&c_b;&c_jo;&c_r;</trd>
  <trd lng="br">avank</trd>
  <trd lng="ca">castor</trd>
  <trd lng="cs">bobr</trd>
  <trd lng="de">Biber</trd>
  <trd lng="en">beaver</trd>
  <trd lng="es">castor</trd>
  <trd lng="fr">castor</trd>
  <trd lng="hu">h&oacute;d</trd>
  <trd lng="id">berang-berang</trd>
  <trd lng="nl">bever</trd>
  <trd lng="pl">b&oacute;br</trd>
  <trd lng="ru">&c_b;&c_o;&c_b;&c_r;</trd>
  <trd lng="sk">bobor</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&#x43E;&c_b;&c_je;&c_r;</trd>,
    <trd>&c_b;&#x43E;&c_b;&c_r;&#x43E;&c_v;&c_je; &c_h;&c_u;&c_t;&c_r;&#x43E;</trd>
  </trdgrp>
</drv>
</art>
