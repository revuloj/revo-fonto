<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>kastor</rad>/o
</kap>
<drv mrk="kastor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="kastor.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Castor</trd>), granda rodulo kun lar&gcirc;a plata vosto,
      rodanta arbojn kaj konstruanta lo&gcirc;ejojn kun subakvaj enirejoj
      kaj digoj por reguligi la akvoaltecon.
    </dif>
  </snc>
  <trd lng="de">Biber</trd>
  <trd lng="en">beaver</trd>
</drv>
<drv mrk="kastor.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="rod.0uloj">roduloj</ref>
      (<trd lng="la">Catoridae</trd>), kun la genro <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Biber</trd>
  <trd lng="en">beavers</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
