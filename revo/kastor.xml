<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kastor</rad>/o
</kap>
<drv mrk="kastor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kastor.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super"
      cel="kastor1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Castor</trd>), granda ron&gcirc;ulo el Kanado kaj
      E&ubreve;ropo kun lar&gcirc;a plata vosto, ron&gcirc;anta arbojn kaj konstruanta
      lo&gcirc;ejojn kun subakvaj enirejoj kaj digoj por reguligi la
      akvoaltecon:
      <ekz>
        li estis vestita en peltomantelo kun <tld/>a
        <klr>(<tld/>fela)</klr> kolumo
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_b;&c_jo;&c_r;</trd>         
  <trd lng="br">avank</trd>
  <trd lng="ca">castor</trd>
  <trd lng="de">Biber</trd>
  <trd lng="en">beaver</trd>
  <trd lng="fr">castor</trd>
  <trd lng="hu">h&oacute;d</trd>
  <trd lng="nl">bever</trd>
  <trd lng="ru">&c_b;&c_o;&c_b;&c_r;</trd>
</drv>
</art>
