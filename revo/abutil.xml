<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: abutil.xml,v 1.2 2023/09/01 13:28:42 afido Exp $">
    <kap>
      <rad>abutil</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="abutil.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="abutil.0o.genro">
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio <ref tip="malprt" cel="malvac.0oj">malvacoj</ref> (<trd lng="la">Abutilon</trd>),
      de tropikaj kaj
      subtropikaj specioj, pluraj el kiuj estas kultivataj ornamcele:
      <ekz>
        <tld/>o <klr>[&#8230;]</klr> estas kultivataj por ornamo a&ubreve; (ekz. A. theophrastii, el
        &Ccirc;inio) por fibroj utiligataj por <ref cel="jut.0o">juto</ref>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=298842"
              >Abutilo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/0/08/Starr_070215-4594_Abutilon_grandifolium.jpg/320px-Starr_070215-4594_Abutilon_grandifolium.jpg"
      prm="CC BY 3.0"
      ><tld/>o (Abutilon grandifolium), Maui, Waihee
        <fnt>
          <aut>Forest &amp; Kim Starr, Plants of Hawaii, Image 070215-4594 from
          http://www.hear.org/starr/plants/images/image/?q=070215-4594</aut>
          <lok><url ref="&WCW;/File:Starr_070215-4594_Abutilon_grandifolium.jpg"
              >https://commons.wikimedia.org/wiki/File:Starr_070215-4594_Abutilon_grandifolium.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="be">&c_a;&c_b;&c_u;&c_t;&c_y;&c_l;&c_jo;&c_n;</trd>
    <trd lng="de">Abutilon</trd>
    <trd lng="en">Abutilon</trd>
    <trd lng="fr">Abutilon</trd>
  </snc>
</drv>

  </art>

