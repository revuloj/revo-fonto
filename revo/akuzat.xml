<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: akuzat.xml,v 1.54 2023/10/12 18:04:56 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>akuzativ</rad>/o
</kap>

<drv mrk="akuzat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">GRA</uzo>
  <snc mrk="akuzat.0o.kazo">
    <dif>
      <ref tip="super" cel="kaz.0o.GRA">Kazo</ref> indikanta la rektan
      objekton en frazo, en Esperanto anka&ubreve; la direkton, tempon kc, kaj
      signata en Esperanto per la <ref tip="dif" cel="n.0">fina&jcirc;o
      -n</ref>:
      <ekz>
        se ni iam post prepozicio uzas la <tld/>on, la akuzativo tie
        dependas ne de la prepozicio, sed de aliaj ka&ubreve;zoj
        <fnt>
          <bib>F</bib>
          <lok>&FE; 28</lok>
        </fnt>;
      </ekz>
      <ekz>
        anstata&ubreve; la vorto &leftquot;je&rightquot; ni povas anka&ubreve; uzi <tld/>on sen
        prepozicio
        <fnt>
          <bib>F</bib>
          <lok>&FE; 29</lok>
        </fnt>;
      </ekz>
      <ekz>
        la forigo de <klr>[&#8230;]</klr> la <tld/>o, kion mi anta&ubreve;
        <esc>dekses</esc> jaroj proponis, por <klr>[&#8230;]</klr> faciligi la
        propagandon, <klr>[&#8230;]</klr> en la nuna tempo, kaj tiom pli anta&ubreve;
        la okuloj de <klr>[&#8230;]</klr> komitato, devas aperi kiel tute ne
        akceptebla
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista en
          Washington en la 15a de a&ubreve;gusto 1910</vrk>
        </fnt>;
      </ekz>
      <ekz>
        kazoj ekzistas nur du: nominativo kaj <tld/>o, la lasta estas
        ricevata el la nominativo per la aldono de la fini&gcirc;o &leftquot;n&rightquot;
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">ESP</uzo>
        latinidlingvanoj ofte eraras <klr>[&#8230;]</klr> &ccirc;e postmetita subjekto kaj uzas
        erare <tld/>on: *&scirc;ajnas eraron, ke ...* (&leftquot;<ind>sisterona <tld/>o</ind>&rightquot; 
        tion nomis <nom>R. Schwartz</nom> la&ubreve; tiu-urbeta gazeteto, en kiu svarmis tiaj eraroj)
        <fnt>
          <bib>PAG</bib>
        </fnt>;
        <trd lng="de">sisteronischer Akkusativ</trd>
      </ekz>
      <ekz>
        la sumera lingvo ne estas <tld/>ema
        <fnt>
          <bib>Monato</bib>
          <aut>Thomas Genty</aut>
          <vrk><url ref="&Monato;/2014/011245.php">Emesal: &ccirc;u la plej antikva
          planlingvo?</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ikara lingvo ne klare distingas inter nominativo kaj <tld/>o,
        subjekto kaj predikato, verboj transitivaj kaj netransitivaj
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj'</aut>
          <vrk><url ref="&Monato;/2015/011355.php">Komunika&jcirc;o de Ikaro</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_y; &c_s;&c_k;&c_l;&c_o;&c_n;</trd>
  <trdgrp lng="br">
    <trd>akuzativ</trd>,
    <trd>tro-damall</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>akuzativ</trd>,
    <trd>&ccaron;tvrt&yacute; p&aacute;d</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Akkusativ</trd>,
    <trd>Wenfall</trd>,
    <trd>Objektkasus</trd>,
    <trd>vierter Fall</trd>
  </trdgrp>
  <trdgrp lng="el">
    <trd>&alfa;&jota;&tau;&jota;&alfa;&tau;&jota;&kappa;&eta_ton; &pi;&tau;&omega_ton;&sigma;&eta;</trd>,
    <trd>&alfa;&jota;&tau;&jota;&alfa;&tau;&jota;&kappa;&eta_ton;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>accusative case</trd>,
    <trd>accusative<klr tip="ind"> (noun)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>caso acusativo</trd>,
    <trd>acusativo<klr tip="ind"> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="fi">akkusatiivi</trd>
  <trdgrp lng="fr">
    <trd>accusatif<klr tip="ind"> (subst.)</klr></trd>,
    <trd>C.O.D.</trd>,
    <trd>COD</trd>
  </trdgrp>
  <trd lng="fy">akkusatyf</trd>
  <trdgrp lng="grc">
    <trd>&alfa;&jota_psili;&tau;&jota;&alfa;&tau;&jota;&kappa;&eta_ton; &pi;&tau;&omega_circ;&sigma;&jota;&sigma_fina;</trd>,
    <trd>&alfa;&jota_psili;&tau;&jota;&alfa;&tau;&jota;&kappa;&eta_ton;</trd>
  </trdgrp>
  <trd lng="he">&mem;&alef;&shin;&yod;&fmem;</trd>
  <trd lng="io">akuzativo</trd>
  <trdgrp lng="hu">
    <trd>t&aacute;rgyeset</trd>,
    <trd>accusativus</trd>
  </trdgrp>
  <trd lng="io">akuzativo</trd>
  <trd lng="it">accusativo</trd>
  <trd lng="ja">&#x5BFE;&#x683C; <pr>&#x305F;&#12356;&#x304B;&#x304F;</pr></trd>
  <trd lng="ko">&#xB300;&#xACA9;</trd>
  <trd lng="lat">casus accusativus</trd>
  <trdgrp lng="nl">
    <trd>accusatief<klr tip="ind"> (zn.)</klr></trd>,
    <trd>accusativus</trd>,
    <trd>vierde naamval</trd>
  </trdgrp>
  <trd lng="pl">biernik</trd>
  <trdgrp lng="pt">
    <trd>caso acusativo</trd>,
    <trd>acusativo<klr tip="ind"> (subst.)</klr></trd>,
    <trd>quarto caso</trd>
  </trdgrp>
  <trd lng="ro">acuzativ</trd>
  <trd lng="ru">&c_v;&c_i;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_p;&c_a;&c_d;&c_je;&c_zh;</trd>
  <trdgrp lng="sk">
    <trd>akuzat&iacute;v</trd>,
    <trd>&scaron;tvrt&yacute; <ind>p&aacute;d</ind></trd>
  </trdgrp>
  <trd lng="sv">ackusativ</trd>
  <trd lng="uk">&c_z;&#x43D;&c_a;&c_h;&c_ib;&c_d;&#x43D;&c_i;&c_j; &c_v;&c_ib;&c_d;&#x43C;&c_ib;&#x43D;&#x43E;&#x43A;</trd>
  <trdgrp lng="vo">
    <trd>kimifal</trd>,
    <trd>kusatif</trd>
  </trdgrp>
  <trd lng="zh">&#x5BBE;&#x683C; <pr>bÄ«ng&eacute;</pr></trd>
</drv>

<drv mrk="akuzat.0a">
  <kap>
    <tld/>a
  </kap>
  <snc mrk="akuzat.0a.rilata">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a fina&jcirc;o
        <fnt>
	<aut>Christian Declerck</aut>
	<vrk><url ref="&Monato;/2000/006843.php">Kiun vojon?</url></vrk>
	<bib>Monato</bib>
	<lok>2000/04, p. 4</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a objekto
        <fnt>
	<aut>Andr&eacute; Cherpillod</aut>
	<vrk><url ref="&Monato;/2000/007059.php">La influo de la hebrea lingvo sur Esperanto</url></vrk>
	<bib>Monato</bib>
	<lok>2000/12, p. 18-19</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la &ccirc;uva&scirc;a ne ekzistas specifa <tld/>a kazo, por &gcirc;i oni uzas
        jen absolutivon, jen dativon
        <fnt>
          <bib>Monato</bib>
          <aut>H&egrave;ctor Al&ograve;s i Font</aut>
          <vrk><url ref="&Monato;/2013/011015.php">La turka: pli granda, pli diversa
          familio</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_v;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_g;&c_a; &c_s;&c_k;&c_l;&c_o;&c_n;&c_u;</trd>
  </trdgrp>
  <trd lng="en">accusative<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="es">acusativo<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="fr">accusatif<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="ja">&#x5BFE;&#x683C;&#x306E; <pr>&#x305F;&#12356;&#x304B;&#x304F;&#x306E;</pr></trd>
  <trd lng="nl">accusatief<klr tip="ind"> (bn.)</klr></trd>
  <trd lng="pt">acusativo<klr tip="ind"> (adj.)</klr></trd>
</drv>
</art>

