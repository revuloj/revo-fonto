<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>riz</rad>/o
</kap>
<drv mrk="riz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">AGR</uzo>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Oryza</trd>),
      liveranta nutrajn grajnojn, kulturata precipe en Azio.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_y;&c_s;</trd>
  <trd lng="de">Reis</trd>
  <trd lng="en">rice</trd>
  <trd lng="fr">riz</trd>
  <trd lng="hu">rizs</trd>
  <trd lng="nl">rijst</trd>
  <trd lng="pl">ry&zdot;</trd>
  <trd lng="pt">arroz</trd>
  <trd lng="ru">&c_r;&c_i;&c_s;</trd>
  <trd lng="sv">ris</trd>
  <trdgrp lng="zh">
    <trd>&#31859;<klr> [m&#464;]</klr></trd>,
  </trdgrp>
</drv>

<drv mrk="riz.0ejo">
  <kap><tld/>ejo</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Malseka kampo en kiu oni kulturas <tld/>on.
    </dif>
    <refgrp tip="sin">
      <ref cel="riz.0kampo"><tld/>kampo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_y;&c_s;&c_a;&c_v;&c_a;&c_je; &c_p;&c_o;&c_l;&c_je;</trd>,
    <trd>&c_r;&c_y;&c_s;&c_a;&c_v;&c_y; &c_ch;&c_e;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Reisfeld</trd>
  <trdgrp lng="fr">
    <trd>rizi&egrave;re</trd>
  </trdgrp>
  <trd lng="hu">rizsf&ouml;ld</trd>
  <trd lng="nl">rijstveld</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_i;&c_s;&c_o;&c_v;&c_o;&c_je; &c_p;&c_o;&c_l;&c_je;</trd>,
    <trd>&c_r;&c_i;&c_s;&c_o;&c_v;&c_y;&c_j; &c_ch;&c_je;&c_k;</trd>
  </trdgrp>
  <trd lng="sv">risf&auml;lt</trd>
</drv>

<drv mrk="riz.0kampo">
  <kap><tld/>kampo</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <ref tip="dif" cel="riz.0ejo"><tld/>ejo</ref>
    <ekz>
      en provincaj regionoj en Japanio etendi&gcirc;as <tld/>kampoj kaj digoj
      &ccirc;irka&ubreve; vila&gcirc;oj<fnt><aut>Hori Yasuo</aut>: <vrk>K-riz-kampoj</vrk>,
      <bib>Monato</bib><lok>jaro 1999a, numero 9a, p. 13a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_y;&c_s;&c_a;&c_v;&c_a;&c_je; &c_p;&c_o;&c_l;&c_je;</trd>,
    <trd>&c_r;&c_y;&c_s;&c_a;&c_v;&c_y; &c_ch;&c_e;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Reisfeld</trd>
  <trdgrp lng="fr">
    <trd>rizi&egrave;re</trd>
  </trdgrp>
  <trd lng="hu">rizsf&ouml;ld</trd>
  <trd lng="nl">rijstveld</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_i;&c_s;&c_o;&c_v;&c_o;&c_je; &c_p;&c_o;&c_l;&c_je;</trd>,
    <trd>&c_r;&c_i;&c_s;&c_o;&c_v;&c_y;&c_j; &c_ch;&c_je;&c_k;</trd>
  </trdgrp>
  <trd lng="sv">risf&auml;lt</trd>
</drv>
</art>
