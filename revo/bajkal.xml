<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bajkal.xml,v 1.20 2023/10/14 08:39:48 revo Exp $">
<kap>
  <rad>Bajkal</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="bajkal.B0o">
  <kap><tld/>o</kap>
  <snc mrk="bajkal.B0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      La plej granda volumene kaj la plej profunda (1741&#x202F;m)
      nesalakva <ref tip="super" cel="lag.0o">lago</ref>
      de la Tero, situanta sude de la
      Orienta <ref tip="malprt" cel="siberi.S0o">Siberio</ref>:
      <ekz>
        transna&gcirc;is en julio la lagon <tld/>o, en Rusio, 
        ultratriatlonisto <nom>Vidmantas Urbonas</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2012/010489.php">Akvo estas vivo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_B;&c_a;&c_j;&c_k;&c_a;&c_l;</trd>
  <trd lng="bg">&c_B;&c_a;&c_j;&c_k;&c_a;&c_l;</trd>
  <trd lng="ca">Baikal</trd>
  <trd lng="cs">Bajkal</trd>
  <trd lng="de">Baikalsee</trd>
  <trd lng="en">Baikal</trd>
  <trd lng="fa">&a_d;&a_r;&f_y;&a_A;&f_ch;&a_h;&a_hamza_sure; <ind>&a_b;&a_A;&f_y;&f_k;&a_A;&a_l;</ind></trd>
  <trd lng="fr">Ba&iuml;kal</trd>
  <trd lng="hu">Bajk&aacute;l-t&oacute;</trd>
  <trd lng="ja">&#x30D0;&#x30A4;&#x30AB;&#x30EB;&#x6E56;  <pr>&#x30D0;&#x30A4;&#x30AB;&#x30EB;&#x3053;</pr></trd>
  <trd lng="pl">Bajka&lstroke;</trd>
  <trd lng="ru">&c_B;&c_a;&c_j;&c_k;&c_a;&c_l;</trd>
  <trdgrp lng="sk">
    <trd>&c_B;&c_a;&c_j;&c_k;&c_a;&c_l;</trd>,
    <trd>Bajkal</trd>,
    <trd>Bajkalsk&eacute; jazero</trd>
  </trdgrp>
  <trd lng="uk">&#x411;&#x430;&#x439;&#x43A;&#x430;&#x43B;</trd>
</drv>
</art>
