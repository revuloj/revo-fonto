<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tauxrotrag.xml,v 1.4 2024/01/12 23:25:43 revo Exp $">
<kap>
  <rad>ta&ubreve;rotrag</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="tauxrotrag.0o">
  <kap><tld/>o</kap>
  <snc mrk="tauxrotrag.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="eland.0o.ZOO">Elando</ref>:
     <ekz>
        la voja&gcirc;antoj lernis distingi iliajn specojn &mdash; longokornajn
        oriksojn, grandegajn kaj kvietajn ru&gcirc;ajn <tld/>ojn
        <fnt><aut>I. Efremov, trad. J. Finkel</aut>, <vrk><url
        ref="http://www.esperanto.mv.ru/Efremov/arko.html">La Granda
        Arko</url></vrk>, <lok>2006</lok></fnt>;
      </ekz>
      <ekz>
        la okcidenta subspecio de la giganta <tld/>o nur postvivas en Senegalo kaj Malio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=455796"
              >Okcident-sudana savano</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
<trd lng="de">Elenantilope</trd>
<trdgrp lng="pl">
  <trd>eland</trd>,
  <trd>bongo</trd>,
  <trd>oreas</trd>
</trdgrp>
</drv>
</art>

