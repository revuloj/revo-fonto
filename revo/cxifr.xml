<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cxifr.xml,v 1.24 2022/04/26 16:27:52 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>&ccirc;ifr</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="cxifr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <uzo tip="fak">TEL</uzo>
  <uzo tip="fak">KOMP</uzo>
  <snc>
    <dif>
      Sekreta interkonsentita skribsignaro:
      <ekz>
        skribi <tld/>e, per <tld/>o;
      </ekz>
      <ekz>
        <ind><tld/>a telegramo</ind>
        <fnt>
          <bib>FK</bib>
        </fnt>;
        <trd lng="it">telegramma cifrato</trd>
      </ekz>
      <ekz>
        &ccirc;e la surmara navigado ekzistas tuta aparta <tld/>o: <frm>AKMIN EBWAX</frm> signifas &leftquot;mi
        alvenos vendrede&rightquot;, <klr>[&#x2026;]</klr> ekzistas anka&ubreve; trilitera <tld/>o kiu estas
        pli kutima, ili bazi&gcirc;as sur la angla tiusence ke la alfabeta ordo de la
        <tld/>ovortoj kongruas kun la alfabeta ordo de la anglaj esprimoj
        <fnt>
          <bib>Monato</bib>
          <aut>Fieder Weigold</aut>
          <vrk><url ref="&Steloj;monato2/007268.html">Morti miskomprene (3)</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o<ref cel="sxlos.0ilo">&scirc;losilo</ref> <klr>[estas]</klr> parametro de <tld/>ado, 
        determinanta unu el la eblaj varioj de la <tld/>o, por mal<tld/>ado necesas scii la <tld/>an
        algoritmon kaj la <ind><tld/>o&scirc;losilo</ind>n
        <fnt>
          <bib>KompLeks</bib>
        </fnt>.
        <trd lng="de">Schl&uuml;ssel <klr>(Chiffre)</klr></trd>
      </ekz>
    </dif>
    <ref tip="sin" cel="kod.0o.SCI">kodo</ref>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_f;&c_r;</trd>
  <trd lng="bg">&c_sh;&c_i;&c_f;&c_malmol;&c_r;</trd>
  <trdgrp lng="de">
    <trd>Chiffre</trd>,
    <trd>Verschl&uuml;sselung</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cipher</trd>,
    <trd>code</trd>,
    <trd>secret writing</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>cifra</trd>,
    <trd>c&oacute;digo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>chiffre<klr tip="ind"> (code secret)</klr></trd>,
    <trd>code<klr tip="ind"> (secret)</klr></trd>
  </trdgrp>
  <trd lng="he">&tsadi;&vav;&pe;&fnun;</trd>
  <trdgrp lng="hu">
    <trd>rejtjel</trd>,
    <trd>titkos&iacute;r&aacute;s</trd>
  </trdgrp>
  <trd lng="it">cifra<klr> (di codice segreto)</klr></trd>
  <trd lng="nl">sleutel<klr tip="ind"> (v.code)</klr></trd>
  <trdgrp lng="pl">
    <trd>szyfr</trd>,
    <trd>kod</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>cifra</trd>,
    <trd>c&oacute;digo</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_i;&c_f;&c_r;</trd>
  <trd lng="sk">&scaron;ifra</trd>
  <trd lng="sv">chiffer</trd>
  <trdgrp lng="uk">
    <trd>&#x448;&#x438;&#x444;&#x440;</trd>,
    <trd>&#x43A;&#x43E;&#x434;</trd>,
    <trd>&#x442;&#x430;&#x439;&#x43D;&#x43E;&#x43F;&#x438;&#x441;</trd>
  </trdgrp>
</drv>

<drv mrk="cxifr.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">MIL</uzo>
  <uzo tip="fak">TEL</uzo>
  <uzo tip="fak">KOMP</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="skrib.trans0i.alisisteme">Transskribi</ref> ion, ekz-e tekston, en <tld/>on:
      <ekz>
        la Sistemo <tld/>ada kaj telegrafada de <nom>A. Walter</nom> 
        <klr>[&#x2026;]</klr> en maniero de sekreta skribado prezentas &ccirc;iujn necesajn ideojn
        en formo tabela
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        f-ino Kruko aperigas <tld/>itan anonceton en Heroldo de Bervalo
        <fnt>
          <bib>KrB</bib>
        </fnt>;
      </ekz>
      <ekz>
        se via tekokomputilo enhavas informojn, kiuj prefere ne iru en aliajn manojn,
        tiam zorgu, ke tiuj datenoj estu tenataj en <tld/>ita formo kaj estu
        alireblaj nur per pasvorto
        <fnt>
          <bib>Monato</bib>
          <aut>Rob Ebenau</aut>
          <vrk><url ref="&Monato;/2005/008528.php">Kiel protekti kontra&ubreve; perdo kaj &scirc;telo?</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="kod.0i.SCI">kodi</ref>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_f;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_p;&c_i;&c_sh;&c_a; &c_s; <ind>&c_sh;&c_i;&c_f;&c_malmol;&c_r;</ind></trd>
  <trdgrp lng="de">
    <trd>verschl&uuml;sseln</trd>,
    <trd>chiffrieren</trd>,
    <trd>kodieren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>encrypt</trd>,
    <trd>encode</trd>
  </trdgrp>
  <trd lng="es">cifrar</trd>
  <trdgrp lng="fr">
    <trd>chiffrer <klr tip="ind">(avec un code secret)</klr></trd>,
    <trd>crypter</trd>,
    <trd>encrypter</trd>
  </trdgrp>
  <trd lng="he">&lamed;&he;&tsadi;&pe;&yod;&fnun;</trd>
  <trdgrp lng="hu">
    <trd>rejtjelez</trd>,
    <trd>sifr&iacute;roz</trd>,
    <trd>k&oacute;dol</trd>
  </trdgrp>
  <trd lng="it">cifrare</trd>
  <trdgrp lng="nl">
    <trd>versleutelen</trd>,
    <trd>encrypteren</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>szyfrowa&cacute;</trd>,
    <trd>kodowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>cifrar</trd>,
    <trd>codificar</trd>,
    <trd>criptografar</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_i;&c_f;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">&scaron;ifrova&tcaron;</trd>
  <trd lng="sv">chiffrera</trd>
  <trd lng="uk">&#x448;&#x438;&#x444;&#x440;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
</drv>

<drv mrk="cxifr.mal0i">
  <kap>mal<tld/>i</kap>
  <uzo tip="fak">MIL</uzo>
  <uzo tip="fak">TEL</uzo>
  <uzo tip="fak">KOMP</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="skrib.trans0i.alisisteme">Transskribi</ref> ion de <tld/>o en klaran lingvon:
      <ekz>
        la anonco promesas plurtavolan lega&jcirc;on, kiun oni povas mal<tld/>i diversmaniere
        <fnt>
          <bib>Monato</bib>
          <aut>Anna kaj Mati Pentus</aut>
          <vrk><url ref="&Monato;/2011/010229.php">Vagadoj urbaj kaj mensaj</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu frazeologismo, estante eble nur pa&ubreve;sa&jcirc;o de loka idioma&jcirc;o, havas la dan&gcirc;eran
        potencialon i&gcirc;i anka&ubreve; esperanta idioma&jcirc;o, rezulte &gcirc;i internacie ne plu estos
        mal&ccirc;ifrebla de &ccirc;iu
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk><url ref="&Steloj;monato2/006813.html">Sa&gcirc;a kapo duonvorton komprenas</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[li]</klr> asertas ke historie tute ne certas &ccirc;u entute Jesuo iam celis
        fondi eklezion disde judismo, sendube tiu ne tute mal&ccirc;ifrebla lia aserto
        trovas ankora&ubreve; subtenantojn
        <fnt>
          <bib>Monato</bib>
          <aut>Pastro Duilio Magnani</aut>
          <vrk><url ref="&Steloj;monato2/007299.html">Fondi eklezion?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="decxif.0i">de&ccirc;ifri</ref>,
      <ref cel="kod.mal0i.SCI">malkodi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_sh;&c_y;&c_f;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_d;&c_je;&c_sh;&c_i;&c_f;&c_r;&c_i;&c_r;&c_a;&c_m;</trd>
  <trdgrp lng="de">
    <trd>entschl&uuml;sseln</trd>,
    <trd>dechiffrieren</trd>,
    <trd>dekodieren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>decrypt</trd>,
    <trd>decode</trd>
  </trdgrp>
  <trd lng="es">descifrar</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;chiffrer <klr tip="ind">(avec un code secret)</klr></trd>,
    <trd>d&eacute;crypter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megfejt<klr tip="ind"> (titkos&iacute;r&aacute;st)</klr></trd>,
    <trd>dek&oacute;dol</trd>,
    <trd>desifr&iacute;roz</trd>
  </trdgrp>
  <trd lng="it">decifrare</trd>
  <trdgrp lng="pl">
    <trd>deszyfrowa&cacute;</trd>,
    <trd>dekodowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>decrypten</trd>,
    <trd>ontsleutelen</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>decrifar</trd>,
    <trd>decodificar</trd>,
    <trd>descriptografar</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>de&scaron;ifrova&tcaron;</trd>,
    <trd>l&uacute;&scaron;ti&tcaron; <klr>(&scaron;ifru)</klr></trd>
  </trdgrp>
</drv>
</art>
