<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>3</ofc>
<rad>&ccirc;ifr</rad>/o
</kap>
<drv mrk="cxifr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <uzo tip="fak">KOMP</uzo>
  <snc>
    <dif>
      Sekreta interkonsentita skribsignaro:
      <ekz>
        skribi <tld/>e, per <tld/>o;
      </ekz>
      <ekz>
        <tld/>a telegramo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>chiffre <klr tip="ind">(code secret)</klr></trd>,
    <trd>code <klr tip="ind">(secret)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_i;&c_f;&c_r;</trd>
  <trd lng="sv">chiffer</trd>
</drv>
<drv mrk="cxifr.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">MIL</uzo>
  <uzo tip="fak">KOMP</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Transskribi ion, ekz-e tekston, en <tld/>on.
    </dif>
  </snc>
  <trd lng="de">chiffrieren</trd>
  <trd lng="en">encrypt</trd>
  <trdgrp lng="fr">
    <trd>chiffrer <klr tip="ind">(avec un code secret)</klr></trd>,
    <trd>crypter</trd>,
    <trd>encrypter</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_i;&c_f;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">chiffrera</trd>
</drv>
<drv mrk="cxifr.mal0i">
  <kap>mal<tld/>i</kap>
  <uzo tip="fak">MIL</uzo>
  <uzo tip="fak">KOMP</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Transskribi ion de <tld/>o en klaran lingvon.
    </dif>
    <refgrp tip="sin">
      <ref cel="decxif.0i">de&ccirc;ifri</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>d&eacute;chiffrer <klr tip="ind">(avec un code secret)</klr></trd>,
    <trd>d&eacute;crypter</trd>
  </trdgrp>
</drv>
</art>
