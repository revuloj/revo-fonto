<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: moskit.xml,v 1.31 2024/08/20 06:36:19 revo Exp $">
<kap>
  <ofc>8</ofc><rad>moskit</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="moskit.0o">
  <kap><tld/>o</kap>
  <snc mrk="moskit.0o.ZOO">
  <uzo tip="fak">ZOO</uzo>
    <dif>
      Pikanta <ref tip="super" cel="dipter.0oj">diptera</ref> insekto,
      precipe el la familio de
      <ref tip="vid" cel="kuled.0oj">kuledoj</ref>:
      <ekz>
        somere sur areo de 600&nbsp;km<sup>2</sup> tie vivis nur serpentoj
        kaj insektoj, &ccirc;efe <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>John Goodhand</aut>
          <vrk><url ref="&Monato;/2005/008509.php">Nekutima
          feriejo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj transdonantaj malarion &ndash; mortis a&ubreve; almena&ubreve; estis
        dumtempe subpremitaj per DDT
        <fnt>
          <bib>Monato</bib>
          <aut>Joyce Bunting</aut>
          <vrk><url ref="&Monato;/2013/010846.php">Taktiko por noveca
          tritiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        alproksimi&gcirc;as la somero kaj &ndash; kiel okazas jam de dudeko da jaroj
        &ndash;
        prezenti&gcirc;as denove en pluraj italaj regionoj timigilo nomata
        &leftquot;<ind>tigro-<tld/>o</ind>&#x201D; <klr>(vd <ref cel="aed1.0o">aedo</ref>)</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2011/010308.php">Spertoj
          vespertaj</url></vrk>
        </fnt>.
        <trd lng="de">Tigerm&uuml;cke</trd>
        <trdgrp lng="id">
          <trd><ind>nyamuk</ind> macan</trd>,
          <trd><ind>nyamuk</ind> hutan</trd>,
          <trd><ind>nyamuk</ind> demam berdarah</trd>
        </trdgrp>
      </ekz>
    </dif>
    <ref tip="sin" cel="kul.0o.KOMUNE">kulo<sncref/></ref>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_s;&c_k;&c_ib;&c_t;</trd>
  <trd lng="bg">&c_k;&c_o;&c_m;&c_a;&c_r;</trd>
  <trd lng="bo">&t_da;&t_shabchu;&t_ga;&t_tsheg;&t_sa;&t_bata;&t_rata;&t_nga;&t_tsheg;</trd>
  <trd lng="cs">moskyt</trd>
  <trdgrp lng="de">
    <trd>Moskito</trd>,
    <trd>Stechm&uuml;cke</trd>
  </trdgrp>
  <trd lng="en">mosquito</trd>
  <trd lng="es">mosquito (familia cul&iacute;cidos)</trd>
  <trd lng="fr">moustique</trd>
  <trd lng="hu">moszkit&oacute;</trd>
  <trd lng="id">nyamuk</trd>
  <trd lng="io">moskito</trd>
  <trd lng="it">zanzara</trd>
  <trd lng="ja">&#x868A;  <pr>&#x304B;</pr></trd>
  <trdgrp lng="nl">
    <trd>mug</trd>,
    <trd>muskiet</trd>,
    <trd>steekmug</trd>
  </trdgrp>
  <trd lng="pl">moskit</trd>
  <trdgrp lng="pt">
    <trd>mosquito</trd>,
    <trd>muri&ccedil;oca</trd>,
    <trd>pernilongo</trd>
  </trdgrp>
  <trd lng="ro">&#x21B;&acirc;n&#x21B;ar</trd>
  <trd lng="ru">&c_m;&c_o;&c_s;&c_k;&c_i;&c_t;</trd>
  <trd lng="sk">moskyt</trd>
  <trd lng="uk">&c_m;&c_o;&c_s;&c_k;&c_ib;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#x868A;&#x5B50; <pr>w&#xE9;nzi</pr></trd>,
    <trd>&#x868A; <pr>w&#xE9;n</pr></trd>
  </trdgrp>
</drv>

</art>
