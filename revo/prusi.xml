<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: prusi.xml,v 1.11 2024/02/17 07:52:38 revo Exp $">
<kap><rad>Prusi</rad>/o</kap>

<drv mrk="prusi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="prusi.0o.prusujo">
    <dif>
      <ref tip="dif" cel="prus.P0ujo">Prusujo</ref>:
      <ekz>
        la konkero de <tld/>o da&ubreve;ris pli ol 50 jaroj kaj ka&ubreve;zis
        multajn mortojn
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Ordeno_de_germanaj_kavaliroj"/>Ordeno de germanaj kavaliroj</vrk>.
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_P;&c_r;&c_u;&c_s;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_P;&c_r;&c_u;&c_s;&c_i;&c_ja;</trd>
  <trd lng="cs">Prusko</trd>
  <trd lng="de">Preu&szlig;en</trd>
  <trd lng="en">Prussia</trd><trd lng="en">Prussia</trd>
  <trd lng="fr">Prusse</trd>
  <trd lng="hu">Poroszorsz&aacute;g</trd>
  <trd lng="ja">&#x30D7;&#x30ED;&#x30A4;&#x30BB;&#x30F3;&#x738B;&#x56FD;  <pr>&#x30D7;&#x30ED;&#x30A4;&#x30BB;&#x30F3;&#x304A;&#x3046;&#x3053;&#x304F;</pr></trd>
  <trd lng="nl">Pruisen</trd>
  <trd lng="pl">Prusy</trd>
  <trd lng="ru">&c_P;&c_r;&c_u;&c_s;&c_s;&c_i;&c_ja;</trd>
  <trd lng="sk">Prusko</trd>
  <trdgrp lng="zh">
    <trd>&#x666E;&#x9B6F;&#x58EB; <pr>P&#x1D4;l&#x1D4;sh&#xEC;</pr></trd>,
    <trd>&#x666E;&#x9C81;&#x58EB; <pr>P&#x1D4;l&#x1D4;sh&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="prusi.0a">
  <kap><tld/>a</kap>
  <snc mrk="prusi.0a.prusuja">
    <dif>
      <ref tip="dif" cel="prus.0uja">Prusuja</ref>:
      <ekz>
        a&ubreve;stra &ccirc;efmar&scirc;alo <nom>Leopold Daun</nom> konis forton
        de <tld/>a armeo kaj strebis la proksimi&gcirc;antan
        kolizion malproksimigi
        <fnt>
          <aut> Miloslav &Scaron;v&aacute;&ccaron;ek</aut>
          <vrk><url ref="http://mujweb.cz/Spolecnost/Svacek/diobenu-71-5.htm">Memorinda monumento</url></vrk>.
          <lok>Dio benu, n-ro 71 (2008:3)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_u;&c_s;&c_k;&c_ib;</trd>
  <trd lng="en">Prussian</trd>
  <trd lng="fr">prussien<klr> (adj.)</klr></trd>
  <trd lng="pl">pruski</trd>
  <trd lng="ru">&c_p;&c_r;&c_u;&c_s;&c_s;&c_k;&c_i;&c_j;<klr>
  (&c_p;&c_r;&c_u;&c_s;&c_s;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j;)</klr></trd>
</drv>

<drv mrk="prusi.0ano">
  <kap><tld/>ano</kap>
  <snc mrk="prusi.0ano.prusujano">
    <dif>
      <ref tip="dif" cel="prus.0ujano">Prusujano</ref>:
      <ekz>
        same kiel faris anta&ubreve;e la <tld/>anoj, la fa&scirc;istoj komencis
        e&ccirc; pli aktivan germanigon
        <fnt><vrk>Pola lingvo</vrk>.
          <lok><url ref="http://www.lingvo.info/lf/pola.php?kat=pol">Lingva
          Prismo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_u;&c_s;&c_a;&c_k;</trd>
  <trd lng="de">Preu&szlig;e</trd>
  <trd lng="en">Prussian</trd>
  <trd lng="fr">Prussien<klr> (subst.)</klr></trd>
  <trd lng="hu">porosz</trd>
  <trd lng="pl">Prusak</trd>
  <trd lng="ru">&c_p;&c_r;&c_u;&c_s;&c_s;&c_a;&c_k;</trd>
</drv>

</art>

