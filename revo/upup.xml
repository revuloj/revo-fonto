<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: upup.xml,v 1.6 2024/02/20 01:21:43 afido Exp $">
    <kap>
      <rad>upup</rad>/o <fnt><bib>MT</bib></fnt>
    </kap>

<drv mrk="upup.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="upup.0o.birdo">
    <dif>
      Okulfrapa <ref tip="lst" lst="voko:birdoj" cel="bird.0o.ZOO">birdo</ref>
      kun granda supren levebla plumkrono, longa maldika, malsupren kurbi&gcirc;anta beko,
      kaj okulfrapaj nigraj-blankaj flugiloj kaj rektangula vosto;
      malmultspecia <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">genro</ref>
      (<trd lng="la">Upupa</trd>) el la ordo de <ref cel="bucer.0o">bucero</ref>formaj birdoj:
      <ekz>
        abomenu inter la birdo <klr>[&#8230;]</klr> la cikonion kaj la ardeon <klr>[&#8230;]</klr>
        kaj la <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 11:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        birdo de la jaro 1976 &dash; <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=61182">Birdo de la jaro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
    lok="&WCU;/thumb/d/d3/African_Hoopoe%2C_Upupa_africana_(Upupa_epops)_at_Marakele_National_Park%2C_Limpopo%2C_South_Africa_(16218185517).jpg/367px-African_Hoopoe%2C_Upupa_africana_(Upupa_epops)_at_Marakele_National_Park%2C_Limpopo%2C_South_Africa_(16218185517).jpg"
    prm="CC BY 2.0">Afrika <tld/>o, Nacia Parko Marakele, Limpopo, Sud-Afriko
        <fnt>
          <aut>Derek Keats el Johannesburg</aut>
          <lok><url
          ref="&WCW;/File:African_Hoopoe,_Upupa_africana_(Upupa_epops)_at_Marakele_National_Park,_Limpopo,_South_Africa_(16218185517).jpg">&WCW;/File:African_Hoopoe,_Upupa_africana_(Upupa_epops)_at_Marakele_National_Park,_Limpopo,_South_Africa_(16218185517).jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="be">&c_u;&c_d;&c_o;&c_d;</trd>
    <trd lng="de">Wiedehopf</trd>
    <trd lng="en">hoopoe</trd>
    <trd lng="it">upupa</trd>
    <trd lng="pl">dudek<klr> (ptak)</klr></trd>
    <trd lng="ru">&c_u;&c_d;&c_o;&c_d;</trd>
  </snc>
  <trd lng="ja">&#x30E4;&#x30C4;&#x30AC;&#x30B7;&#x30E9;</trd>
</drv>
  </art>

