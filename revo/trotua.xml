<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>trotuar</rad>/o
</kap>
<drv mrk="trotua.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="trotua.0o.strata">
    <dif>
      Irejo, ordinare tegita per &scirc;tonplatoj a&ubreve; bitumo, rezervata por
      piedirantoj amba&ubreve;flanke de strato a&ubreve; &scirc;oseo.
    </dif>
    <trdgrp lng="de">
      <trd>B&uuml;rgersteig</trd>,
      <trd>Gehweg</trd>,
      <trd>Trottoir</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>pavement</trd>,
      <trd>sidewalk</trd>
    </trdgrp>
    <trd lng="fr">trottoir</trd>
    <trdgrp lng="nl">
      <trd>stoep</trd>,
      <trd>trottoir</trd>
    </trdgrp>
    <trd lng="ru">&c_t;&c_r;&c_o;&c_t;&c_u;&c_a;&c_r;</trd>
    <trd lng="sv">trottoar</trd>
  </snc>
  <snc mrk="trotua.0o.fervoja">
    <dif>
      Fervoja alirejo,
      <ref tip="sin" cel="kaj1.0o.FER">kajo<sncref
      ref="kaj1.0o.FER"/></ref>.
    </dif>
    <trd lng="nl">perron</trd>
    <trd lng="ru">&c_p;&c_je;&c_r;&c_r;&c_o;&c_n;</trd>
  </snc>
</drv>
</art>
