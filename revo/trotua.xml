<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>trotuar</rad>/o
</kap>
<drv mrk="trotua.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="trotua.0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      Irejo, ordinare tegita per &scirc;tonplatoj a&ubreve; asfalto, rezervata por
      piedirantoj flanke de la &scirc;osea parto de <ref tip="malprt"
      cel="strat.0o">strato</ref>:
      <ekz>
        nur piedirantoj rajtas preteriri sur la kontra&ubreve;a
        <tld/>o
        <fnt><aut>Walter Klag</aut>: <vrk>Bariloj kontra&ubreve;
        bomboj</vrk>, <bib>Monato</bib>, <lok>jaro 1999a, numero 3a, p.
        7a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_r;&c_a;&c_t;&c_u;&c_a;&c_r;</trd>
    <trdgrp lng="de">
      <trd>B&uuml;rgersteig</trd>,
      <trd>Gehweg</trd>,
      <trd>Trottoir</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>pavement</trd>,
      <trd>sidewalk</trd>
    </trdgrp>
    <trd lng="fr">trottoir</trd>
    <trd lng="hu">j&aacute;rda</trd>    
    <trdgrp lng="nl">
      <trd>stoep</trd>,
      <trd>trottoir</trd>
    </trdgrp>
    <trd lng="ru">&c_t;&c_r;&c_o;&c_t;&c_u;&c_a;&c_r;</trd>
    <trd lng="sv">trottoar</trd>
  </snc>
  <snc mrk="trotua.0o.FER">
    <uzo tip="fak">FER</uzo>
    <dif>
      <ref tip="dif" cel="kaj1.0o.FER">kajo <sncref/></ref>
    </dif>
    <trd lng="be">&c_p;&c_e;&c_r;&c_o;&c_n;</trd>
    <trd lng="fr">quai<klr> (de gare)</klr></trd>
    <trd lng="hu">peron</trd>    
    <trd lng="nl">perron</trd>
    <trd lng="ru">&c_p;&c_je;&c_r;&c_r;&c_o;&c_n;</trd>
  </snc>
</drv>
</art>
