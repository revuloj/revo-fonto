<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>acipenser</rad>/o
</kap>
<drv mrk="acipenser.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super"
      cel="acipen1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Acipenser</trd>). El la frajo de kelkaj &gcirc;iaj
      specioj oni faras kaviaron.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_je;&c_t;&c_r;</trd>
  <trdgrp lng="ca">
    <trd>esturi&oacute;</trd>,
    <trd>esterlet</trd>
  </trdgrp>
  <trd lng="en">sturgeon</trd>
  <trd lng="de">St&ouml;r</trd>
  <trd lng="fr">esturgeon</trd>
  <trd lng="hu">tok(hal)</trd>
  <trd lng="it">storione</trd>
  <trd lng="nl">steur</trd>
  <trd lng="pt">acip&ecirc;nser</trd>
  <trd lng="ru">&c_o;&c_s;&c_jo;&c_t;&c_r;</trd>
  <trd lng="sv">st&ouml;r</trd>
</drv>

<drv mrk="acipenser.0formaj">
  <kap><tld/>formaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO"
      lst="voko:zoologiaj_ordoj">Ordo</ref> de <ref tip="malprt"
      cel="fisx.osto0oj">ostofi&scirc;oj</ref>
      (<trd lng="la">Acipenseriformes</trd>), kun parte kartilaga
      skeleto.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_je;&c_t;&c_r;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja; <klr
  tip="amb">(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="ca">acipenseriformes</trd>
  <trd lng="de">st&ouml;rartige Fische</trd>
  <trd lng="fr">acipens&eacute;riformes</trd>
  <trd lng="hu">tokalak&uacute;ak</trd>
  <trd lng="it">storioniformi</trd>
  <trd lng="nl">steurachtigen</trd>
  <trd lng="ru">&c_o;&c_s;&c_je;&c_t;&c_r;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je; <klr
  tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trd lng="sv">st&ouml;rartade fiskar</trd>
</drv>
</art>
