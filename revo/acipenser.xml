<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>acipenser</rad>/o
</kap>
<drv mrk="acipenser.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="acipenser.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Acipenser</trd>). El la frajo de kelkaj &gcirc;iaj
      specioj oni faras kaviaron.
    </dif>
  </snc>
  <trd lng="de">St&ouml;r</trd>
</drv>
<drv mrk="acipenser.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo 
      <ref tip="super" cel="acipenser.0formaj">acipenserformaj</ref>
      fi&scirc;oj (<trd lng="la">Acipenseridae</trd>), kun plata kapo
      kaj sendenta bu&scirc;o.
    </dif>
  </snc>
  <trd lng="de">St&ouml;re</trd>
</drv>
<drv mrk="acipenser.0formaj">
  <kap><tld/>formaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo de <ref tip="super" cel="fisx.osto0oj">ostofi&scirc;oj</ref>
      (<trd lng="la">Acipenseriformes</trd>), kun parte kartilaga skeleto.
    </dif>
  </snc>
  <trd lng="de">St&ouml;rartige Fische</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>