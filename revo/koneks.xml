<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: koneks.xml,v 1.34 2024/02/05 09:23:49 revo Exp $">
<kap>
  <rad>koneks</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="koneks.0a">
  <kap><tld/>a</kap>
  <snc mrk="koneks.0a.KOMUNE">
    <dif>
      Abstrakte, koncepte, mense interligita; proksime interrilata kun analoga afero:
      <ekz>
        tio estas <tld/>aj demandoj, kiujn oni ne povas aparte studi;
      </ekz>
      <ekz>
        mi pensis, ke la brueto kaj la brilanta objekto estas <tld/>aj
        <fnt>
          <bib>Monato</bib>
          <aut>J&uacute;lia Sigmond</aut>
          <vrk><url ref="&Monato;/2009/009693.php">La mistera nokto</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        sinsekvo de eventoj, impresoj, prenitaj decidoj preska&ubreve; sen<tld/>aj
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2014/011151.php">Kiu &ccirc;i mi?</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="dis.ne0igebla">nedisigebla</ref>
    <refgrp tip="vid">
      <ref cel="koalic.0a">koalicia</ref>,
      <ref cel="koher.0a">kohera</ref>,
      <ref cel="konekt.0ite">konektita</ref>
    </refgrp>
    <trd lng="fa">&a_m;&a_r;&a_t;&a_b;&a_T;</trd>
    <trdgrp lng="fr">
      <trd>connexe<klr tip="ind"> (reli&eacute;)</klr></trd>,
      <trd>aff&eacute;rant</trd>,
      <trd>reli&eacute;<klr tip="ind"> (connexe)</klr></trd>,
      <trd>rattach&eacute;<klr tip="ind"> (reli&eacute;)</klr></trd>,
      <trd>voisin<klr tip="ind"> (rattach&eacute;)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_z;&c_a;&c_i;&c_m;&c_o;&c_s;&c_v;&c_ja;&c_z;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="koneks.0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="koneks.0a.topologio">
      <fnt><bib>EKV</bib><lok>&para;217</lok></fnt>
      <dif>
        <klr>(p.p. <ref cel="spac.topologia0o">topologia
        spaco</ref>)</klr>
        Tia, ke nur la tuta spaco kaj la malplena aro estas samtempe
        <ref cel="ferm.mal0ita.topologio">malfermitaj</ref>
        kaj <ref cel="ferm.0ita.topologio">fermitaj</ref>.
      </dif>
      <trd lng="de">zusammenh&auml;ngend<klr tip="ind"> (-er topologischer
      Raum)</klr></trd>
      <trd lng="en">connected<klr tip="ind"> (topological
      space)</klr></trd>
      <trd lng="fr"><klr tip="ind">(espace topologique)
      </klr>connexe</trd>
      <trd lng="pl"><klr tip="amb">(przestrze&nacute; topologiczna)
      </klr>sp&oacute;jna</trd>
      <trd lng="ro">mul&#x21B;ime conex&abreve;</trd>
      <trd lng="ru">&c_s;&c_v;&c_ja;&c_z;&c_n;&c_o;&c_je;<klr tip="amb">
      (&c_t;&c_o;&c_p;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je;
      &c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
    </subsnc>
    <subsnc mrk="koneks.0a.grafeo">
      <fnt><bib>KompLeks</bib></fnt>
      <dif>
        <klr>(p.p. <ref cel="grafe.0o.strukturo">grafeo</ref>)</klr>
        Tia, ke por ajnaj du verticoj de &gcirc;i &ccirc;iam ekzistas
        <ref cel="cxen.0o.grafeo">&ccirc;eno</ref>, kiu ilin ligas.
      </dif>
      <trd lng="de">zusammenh&auml;ngend<klr tip="ind"> (-er
      Graph)</klr></trd>
      <trd lng="en">connected<klr tip="ind"> (graph)</klr></trd>
      <trd lng="fr"><klr tip="ind">(graphe) </klr>connexe</trd>
      <trd lng="pl"><klr tip="ind">(graf) </klr>sp&oacute;jny</trd>
      <trd lng="ru">&c_s;&c_v;&c_ja;&c_z;&c_n;&c_y;&c_j;<klr tip="ind"> (&c_g;&c_r;&c_a;&c_f;)</klr></trd>
    </subsnc>
    <trd lng="fa">&a_h;&a_m;&a_b;&a_n;&a_d;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_l;&c_u;&c_ch;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_u;&c_z;&c_a;&c_je;&c_m;&c_a;&c_z;&c_l;&c_u;&c_ch;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_mol;&c_v;&c_ja;&c_z;&c_a;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>souvisl&yacute;</trd>,
    <trd>souvis&iacute;c&iacute;</trd>,
    <trd>spojit&yacute;</trd>,
    <trd>vz&aacute;jemn&ecaron; z&aacute;visl&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verbunden</trd>,
    <trd>verkn&uuml;pft</trd>,
    <trd>zusammenh&auml;ngend</trd>
  </trdgrp>
  <trd lng="ja">&#x5BC6;&#x63A5;&#x306B;&#x95A2;&#36899;&#12375;&#x305F; 
  <pr>&#x307F;&#12387;&#x305B;&#12388;&#x306B;&#x304B;&#12435;&#x308C;&#12435;&#12375;&#x305F;</pr></trd>
  <trd lng="nl">samenhangend</trd>
  <trdgrp lng="pl">
    <trd>po&lstroke;&aogonek;czony</trd>,
    <trd>sp&oacute;jny</trd>,
    <trd>zwi&aogonek;zany</trd>
  </trdgrp>
  <trd lng="pt">conexo</trd>
  <trdgrp lng="ro">
    <trd>conectat</trd>,
    <trd>unit</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>spojit&yacute;</trd>,
    <trd>s&uacute;visiaci</trd>,
    <trd>s&uacute;visl&yacute;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7DB0; <pr>w&#x1CE;n</pr></trd>,
    <trd>&#x65E0;&#x95F4; <pr>w&#xFA;ji&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="koneks.0o">
  <kap><tld/>o</kap>
  <snc mrk="koneks.0o.KOMUNE">
    <dif>
      Konstanta, proksima <ref tip="super" cel="rilat.0o.interligo">rilato</ref>
      abstrakta, koncepta, mensa;  <ref tip="vid" cel="lig.0o.kioligas">ligo<sncref/></ref>:
      <ekz>
        la hejdelberga makzelo estas absolute senmentona <klr>[&#8230;]</klr> tiu fakto, en
        <tld/>o kun la muskola strukturo de la lango, indikas, ke tre ver&scirc;ajne la
        Hejdelbergulo ne povis <klr>[&#8230;]</klr> paroli
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi a&ubreve;dos mian nomon menciata en proksima <tld/>o kun la nomoj de tiuj personoj
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 4 (66)</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun <klr>[fiziko kaj pedagogio]</klr> la fizikdidaktiko havas
        senperajn <tld/>ojn
        <fnt>
          <aut>J. Brockmeyer</aut>,
          <vrk><url ref="https://scienca-revuo.info/article/download/481/460">
          Scienca Objekto kaj Metodoj de la Fakdidaktikoj</url></vrk>,
          <lok>Scienca Revuo, 1988 (155)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="asoci.0o.kunligiteco">asocio<sncref/></ref>,
      <ref cel="fac.inter0o">interfaco</ref>,
      <ref cel="junt.0o.kunigo">junto<sncref/></ref>,
      <ref cel="komuni1.0igxo">komuniki&gcirc;o</ref>,
      <ref cel="konekt.0o">konekto</ref>,
      <ref cel="kontak.0o">kontakto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_v;&c_ja;&c_z;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;&iacute;poj</trd>,
    <trd>spoj</trd>,
    <trd>spojen&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Zusammenhang</trd>,
    <trd>Verbindung</trd>
  </trdgrp>
  <trd lng="fr">accointance</trd>
  <trd lng="pl">zwi&aogonek;zek</trd>
  <trd lng="ro">unitate</trd>
  <trdgrp lng="sk">
    <trd>pr&iacute;poj</trd>,
    <trd>spoj</trd>,
    <trd>spojenie</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8FDE;&#x8D2F;&#24615; <pr>li&#xE1;ngu&#xE0;nx&#xEC;ng</pr></trd>,
    <trd>&#20851;&#32852; <pr>gu&amacron;nli&#xE1;n</pr></trd>,
    <trd>&#x9ECF;&#x7D50; <pr>ni&#xE1;nji&#xE9;</pr></trd>,
    <trd>&#x7D27;&#x5BC6;&#x7ED3;&#21512; <pr>j&#x1D0;nm&#xEC;ji&#xE9;h&#xE9;</pr></trd>,
    <trd>&#20851;&#x7CFB;&#x5BC6;&#x4E0D;&#x53EF;&#20998; <pr>gu&amacron;nxim&#xEC;b&#xF9;k&#x11B;f&emacron;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="koneks.0ega">
  <kap><tld/>ega</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>KompLeks</bib></fnt>
  <snc>
    <dif>
      <klr>(p.p. <ref cel="grafe.orientita0o">orientita
      grafeo</ref>)</klr>
      Tia, ke por ajnaj du verticoj de &gcirc;i &ccirc;iam ekzistas
      <ref cel="voj.0o.grafeo">vojo</ref>, kiu ilin ligas.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_o;&c_c;&c_n;&c_a; <ind>&c_z;&c_l;&c_u;&c_ch;&c_a;&c_n;&c_y;</ind></trd>
  <trd lng="cs">siln&ecaron; souvisl&yacute; <klr>(orientovan&yacute; graf)</klr></trd>
  <trd lng="de">stark zusammenh&auml;ngend<klr tip="ind"> (-er
  Graph)</klr></trd>
  <trd lng="en">strongly connected<klr tip="ind"> (graph)</klr></trd>
  <trd lng="fa">&a_q;&a_w;&f_y;-<ind>&a_h;&a_m;&a_b;&a_n;&a_d;</ind></trd>
  <trd lng="fr"><klr tip="ind">(graphe) </klr>fortement
  <ind>connexe</ind></trd>
  <trd lng="pl"><klr tip="ind">(digraf) </klr>silnie
  <ind>sp&oacute;jny</ind></trd>
  <trd lng="ro">spa&#x21B;iul puternic consecvent</trd>
  <trd lng="ru">&c_s;&c_i;&c_l;&c_mol;&c_n;&c_o; <ind>&c_s;&c_v;&c_ja;&c_z;&c_n;&c_y;&c_j;</ind><klr tip="ind">
  (&c_g;&c_r;&c_a;&c_f;)</klr></trd>
  <trd lng="sk">silne s&uacute;visl&yacute; <klr>(orientovan&yacute; graf)</klr></trd>
</drv>

<drv mrk="koneks.0ekun">
  <kap><tld/>e kun</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="koneks.0ekun.KOMUNE">
    <dif>
      <ref tip="vid" cel="lig.0ekun.KOMUNE">Lige kun</ref>,
      <ref tip="vid" cel="rilat.0eal">rilate al</ref>:
      <ekz>
        de kie aperis la nazpin&ccirc;ilo <klr>[&#8230;]</klr> kelkfoje ni vidas &gcirc;in <tld/>e kun la
        kuracisto, kaj nur kun li
        <fnt>
          <bib>KFl</bib>
          <lok>VIII. &Scirc;tupoj al idealo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e kun tio, ke lupojn minacas kompleta ekstermo,
        lastatempe alti&gcirc;is la interesi&gcirc;o pri ili
        <fnt>
          <aut>J. &Scirc;uvajev</aut>,
          <vrk><url ref="&STEB;zoologio/konduto_de_bestoj/f874-p30.htm">
          Pri la konduto de bestoj</url></vrk>,
          <lok>Fokuso, 1987:1, p. 2a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kun.0e0">kune kun</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_u; <ind>&c_s;&c_u;&c_v;&c_ja;&c_z;&c_ib;</ind> &c_z;</trd>
  <trdgrp lng="de">
    <trd>im <ind>Zusammenhang</ind> mit</trd>,
    <trd><ind>verbunden</ind> mit</trd>
  </trdgrp>
  <trd lng="fr">en lien avec</trd>
  <trd lng="pl">w zwi&aogonek;zku z</trd>
  <trd lng="ro">&icirc;n leg&abreve;tur&abreve; cu</trd>
  <trdgrp lng="zh">
    <trd>&#x968F;&#30528; <pr>su&#xED;zhe</pr></trd>,
    <trd>&#x968F;&#x4E4B; <pr>su&#xED;zh&#x12B;</pr></trd>,
    <trd>&#20855;&#x4F53; <pr>j&#xF9;t&#x1D0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="koneks.simple0a">
  <kap>simple <tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>EKV</bib><lok>&para;398</lok></fnt>
  <snc>
    <dif>
      <klr>(p.p. <ref cel="koneks.voj0a">voj<tld/>a</ref>
      <ref cel="spac.topologia0o">topologia spaco</ref>)</klr>
      Tia, ke &ccirc;iu <ref cel="voj.fermita0o">fermita vojo</ref> en
      &gcirc;i estas <ref cel="homoto.0a.vojoj">homotopa</ref> al punkto.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_o;&c_s;&c_t;&c_a; <ind>&c_z;&c_l;&c_u;&c_ch;&c_a;&c_n;&c_y;</ind></trd>
  <trd lng="cs">jednodu&scaron;e souvisl&yacute;</trd>
  <trd lng="de">einfach <ind>zusammenh&auml;ngend</ind><klr tip="ind"> (-er
  Raum)</klr></trd>
  <trd lng="en">simply <ind>connected</ind><klr tip="ind">
  (space)</klr></trd>
  <trd lng="fa">&a_s;&a_A;&a_d;&a_h;-<ind>&a_h;&a_m;&a_b;&a_n;&a_d;</ind></trd>
  <trd lng="fr"><klr tip="ind">(espace) </klr>simplement
  <ind>connexe</ind></trd>
  <trd lng="pl"><klr tip="amb">(przestrze&nacute;) </klr>jednosp&oacute;jna</trd>
  <trd lng="ro">spa&#x21B;iu pur &#537;i simplu conectat</trd>
  <trd lng="ru">&c_o;&c_d;&c_n;&c_o;&c_s;&c_v;&c_ja;&c_z;&c_n;&c_o;&c_je;<klr tip="amb">
  (&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">jednoducho s&uacute;visl&yacute;</trd>
</drv>

<drv mrk="koneks.voj0a">
  <kap>voj<tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>EKV</bib><lok>&para;460</lok></fnt>
  <snc>
    <dif>
      <klr>(p.p. <ref cel="spac.topologia0o">topologia
      spaco</ref>)</klr>
      Tia, ke por ajnaj du punktoj <frm am="a"><k>a</k></frm> kaj
      <frm am="b"><k>b</k></frm>
      de &gcirc;i ekzistas <ref cel="voj.0o.bildigo">vojo</ref> en tiu spaco,
      kies komenca kaj fina punktoj estas <frm am="a"><k>a</k></frm> kaj
      <frm am="b"><k>b</k></frm>:
      <ekz>
        <ref cel="konvek.0a.subaro">konveksa</ref> spaco estas
        voj<tld/>a;
      </ekz>
      <ekz>
        voj<tld/>a spaco estas anka&ubreve;
        <ref cel="koneks.0a.topologio"><tld/>a</ref>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_n;&c_je;&c_j;&c_n;&c_a; <ind>&c_z;&c_l;&c_u;&c_ch;&c_a;&c_n;&c_y;</ind></trd>
  <trd lng="de">wegzusammenh&auml;ngend<klr tip="ind"> (-er
  Raum)</klr></trd>
  <trdgrp lng="en">
    <trd>path-connected<klr tip="ind"> (space)</klr></trd>,
    <trd>pathwise <ind>connected</ind><klr tip="ind">
    (space)</klr></trd>
  </trdgrp>
  <trd lng="fa">&a_m;&a_s;&f_y;&a_r;&f_y;-<ind>&a_h;&a_m;&a_b;&a_n;&a_d;</ind></trd>
  <trd lng="fr"><klr tip="ind">(espace) </klr>connexe par arcs</trd>
  <trd lng="pl"><klr tip="amb">(przestrze&nacute;) </klr>sp&oacute;jna &lstroke;ukowo</trd>
  <trd lng="ro">arc spa&#x21B;iu consecvent</trd>
  <trd lng="ru">&c_l;&c_i;&c_n;&c_je;&c_j;&c_n;&c_o;&c_s;&c_v;&c_ja;&c_z;&c_n;&c_o;&c_je;<klr tip="amb">
  (&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
