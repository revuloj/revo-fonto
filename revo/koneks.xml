<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>koneks</rad>/a
</kap>
<drv mrk="koneks.0a">
  <kap><tld/>a</kap>
  <snc mrk="koneks.0a.KOMUNE">
    <dif>
      Interligita; proksime interrilata kun analoga afero:
      <ekz>
        tio estas <tld/>aj demandoj, kiujn oni ne povas aparte studi.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>connexe<klr tip="ind"> (reli&eacute;)</klr></trd>,
      <trd>aff&eacute;rant</trd>,
      <trd>reli&eacute;<klr tip="ind"> (connexe)</klr></trd>,
      <trd>rattach&eacute;<klr tip="ind"> (reli&eacute;)</klr></trd>,
      <trd>voisin<klr tip="ind"> (rattach&eacute;)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_z;&c_a;&c_i;&c_m;&c_o;&c_s;&c_v;&c_ja;&c_z;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="koneks.0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="koneks.0a.topologio">
      <fnt><bib>EKV</bib><lok>&para;217</lok></fnt>
      <dif>
        <klr>(p.p. <ref cel="spac.topologia0o">topologia spaco</ref>)</klr>
        Tia, ke nur la tuta spaco kaj la malplena aro estas samtempe
        <ref cel="ferm.mal0ita.topologio">malfermitaj</ref>
        kaj <ref cel="ferm.0ita.topologio">fermitaj</ref>.
      </dif>
      <trd lng="de">zusammenh&auml;ngend<klr tip="ind"> (-er topologischer Raum)</klr></trd>
      <trd lng="en">connected<klr tip="ind"> (topological space)</klr></trd>
      <trd lng="fr"><klr tip="ind">(espace topologique) </klr>connexe</trd>
      <trd lng="pl"><klr tip="amb">(przestrze&nacute; topologiczna) </klr>sp&oacute;jna</trd>
      <trd lng="ru">&c_s;&c_v;&c_ja;&c_z;&c_n;&c_o;&c_je;<klr tip="amb"> (&c_t;&c_o;&c_p;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; &c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
    </subsnc>
    <subsnc mrk="koneks.0a.grafeo">
      <fnt><bib>KompLeks</bib></fnt>
      <dif>
        <klr>(p.p. <ref cel="grafe.0o.strukturo">grafeo</ref>)</klr>
        Tia, ke por ajnaj du verticoj de &gcirc;i &ccirc;iam ekzistas
        <ref cel="cxen.0o.grafeo">&ccirc;eno</ref>, kiu ilin ligas.
      </dif>
      <trd lng="de">zusammenh&auml;ngend<klr tip="ind"> (-er Graph)</klr></trd>
      <trd lng="en">connected<klr tip="ind"> (graph)</klr></trd>
      <trd lng="fr"><klr tip="ind">(graphe) </klr>connexe</trd>
      <trd lng="pl"><klr tip="ind">(graf) </klr>sp&oacute;jny</trd>
      <trd lng="ru">&c_s;&c_v;&c_ja;&c_z;&c_n;&c_y;&c_j;<klr tip="ind"> (&c_g;&c_r;&c_a;&c_f;)</klr></trd>
    </subsnc>
  </snc>
  <trdgrp lng="de">
    <trd>verbunden</trd>,
    <trd>verkn&uuml;pft</trd>,
    <trd>zusammenh&auml;ngend</trd>
  </trdgrp>
  <trd lng="nl">samenhangend</trd>
  <trd lng="pt">conexo</trd>
</drv>

<drv mrk="koneks.0ega">
  <kap><tld/>ega</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>KompLeks</bib></fnt>
  <snc>
    <dif>
      <klr>(p.p. <ref cel="grafe.orientita0o">orientita grafeo</ref>)</klr>
      Tia, ke por ajnaj du verticoj de &gcirc;i &ccirc;iam ekzistas
      <ref cel="voj.0o.grafeo">vojo</ref>, kiu ilin ligas.
    </dif>
  </snc>
  <trd lng="de">stark zusammenh&auml;ngend<klr tip="ind"> (-er Graph)</klr></trd>
  <trd lng="en">strongly connected<klr tip="ind"> (graph)</klr></trd>
  <trd lng="fr"><klr tip="ind">(graphe) </klr>fortement <ind>connexe</ind></trd>
  <trd lng="pl"><klr tip="ind">(digraf) </klr>silnie <ind>sp&oacute;jny</ind></trd>
  <trd lng="ru">&c_s;&c_i;&c_l;&c_mol;&c_n;&c_o; <ind>&c_s;&c_v;&c_ja;&c_z;&c_n;&c_y;&c_j;</ind><klr tip="ind"> (&c_g;&c_r;&c_a;&c_f;)</klr></trd>
</drv>

<drv mrk="koneks.simple0a">
  <kap>simple <tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>EKV</bib><lok>&para;398</lok></fnt>
  <snc>
    <dif>
      <klr>(p.p. <ref cel="koneks.voj0a">voj<tld/>a</ref>
      <ref cel="spac.topologia0o">topologia spaco</ref>)</klr>
      Tia, ke &ccirc;iu <ref cel="voj.fermita0o">fermita vojo</ref> en
      &gcirc;i estas <ref cel="homoto.0a.vojoj">homotopa</ref> al punkto.
    </dif>
  </snc>
  <trd lng="de">einfach <ind>zusammenh&auml;ngend</ind><klr tip="ind"> (-er Raum)</klr></trd>
  <trd lng="en">simply <ind>connected</ind><klr tip="ind"> (space)</klr></trd>
  <trd lng="fr"><klr tip="ind">(espace) </klr>simplement <ind>connexe</ind></trd>
  <trd lng="pl"><klr tip="amb">(przestrze&nacute;) </klr>jednosp&oacute;jna</trd>
  <trd lng="ru">&c_o;&c_d;&c_n;&c_o;&c_s;&c_v;&c_ja;&c_z;&c_n;&c_o;&c_je;<klr tip="amb"> (&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>

<drv mrk="koneks.voj0a">
  <kap>voj<tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>EKV</bib><lok>&para;460</lok></fnt>
  <snc>
    <dif>
      <klr>(p.p. <ref cel="spac.topologia0o">topologia spaco</ref>)</klr>
      Tia, ke por ajnaj du punktoj <frm><k>a</k></frm> kaj <frm><k>b</k></frm>
      de &gcirc;i ekzistas <ref cel="voj.0o.bildigo">vojo</ref> en tiu spaco, 
      kies komenca kaj fina punktoj estas <frm><k>a</k></frm> kaj 
      <frm><k>b</k></frm>:
      <ekz>
        <ref cel="konvek.0a.subaro">konveksa</ref> spaco estas
        voj<tld/>a;
      </ekz>
      <ekz>
        voj<tld/>a spaco estas anka&ubreve;
        <ref cel="koneks.0a.topologio"><tld/>a</ref>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">wegzusammenh&auml;ngend<klr tip="ind"> (-er Raum)</klr></trd>
  <trdgrp lng="en">
    <trd>path-connected<klr tip="ind"> (space)</klr></trd>,
    <trd>pathwise <ind>connected</ind><klr tip="ind"> (space)</klr></trd>
  </trdgrp>
  <trd lng="fr"><klr tip="ind">(espace) </klr>connexe par arcs</trd>
  <trd lng="pl"><klr tip="amb">(przestrze&nacute;) </klr>sp&oacute;jna &lstroke;ukowo</trd>
  <trd lng="ru">&c_l;&c_i;&c_n;&c_je;&c_j;&c_n;&c_o;&c_s;&c_v;&c_ja;&c_z;&c_n;&c_o;&c_je;<klr tip="amb"> (&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
