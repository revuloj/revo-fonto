<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fakultativ.xml,v 1.15 2025/08/02 22:00:21 revo Exp $">
<kap>
  <rad>fakultativ</rad>/a
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="fakultativ.0a">
  <kap><tld/>a</kap>
  <uzo tip="klr">(oficeja stilo)</uzo>
  <snc mrk="fakultativ.0a.KOMUNE">
    <dif>
      <refgrp tip="dif">
        <ref cel="dev.ne0iga.KOMUNE">Nedeviga</ref>,
        <ref cel="nepr.mal0a.KOMUNE">malnepra</ref>
      </refgrp>:
      <ekz>
        parto A &#8208; devigaj elementoj /
        parto B &#8208; <tld/>aj elementoj
        <fnt>
        <aut>-</aut>,
        <vrk><url ref="http://www.recim.org/ascop/projekto.htm">
        Kondi&ccirc;aro [...] de ASKOP</url></vrk>,
        <lok>2007-04</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi skribis al mi <klr>[&#8230;]</klr> por konsciigi min pri la
        intereso
        enkonduki <tld/>an ekzamenon de Esperanto en la abiturienta
        ekzameno
        <fnt>
          <aut>trad. Lee J.-k.</aut>,
          <vrk><url ref="http://www.esperanto.co.kr/bbs/zboard.php?id=mia_taglibro">
          Traduka&jcirc;oj/Nova&jcirc;oj</url></vrk>,
          <lok>2007-05-06</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a da&ubreve;rigo post la semajnfina kurso
        <fnt>
          <vrk><url ref="http://www.eventoj.hu/2007.htm">Esperanto-aran&gcirc;oj en 2007</url></vrk>
          <lok>Internacia Kalendaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vizito al la akvario estas <tld/>a, kaj la kosto de la
        enirbileto ne estas
        inkluzivita
        <fnt>
          <vrk><url ref="http://www.esperanto-bjelovar.hr/kresko0518.pdf">KRESKO</url></vrk>
          <lok>esperanto-bjelovar.hr</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="opci.0o.KOMUNE">opcio<sncref/></ref>,
       <ref cel="altern2.0o.elekto">alternativo</ref>,
       <ref cel="vol.liber0a">libervola</ref>,
       <ref cel="okaz.0a">okaza</ref>
    </refgrp>
    <trdgrp lng="pl">
      <trd>fakultatywny</trd>,
      <trd>dobrowolny</trd>,
      <trd>dowolny</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>facultatif</trd>,
      <trd>optionnel</trd>
    </trdgrp>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>libervola</ke>, <ke>nedeviga</ke>.
  </rim>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_k;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_a;&c_b;&c_a;&c_v;&c_ja;&c_z;&c_k;&c_o;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">facultatiu</trd>
  <trdgrp lng="cs">
    <trd>fakultativn&iacute;</trd>,
    <trd>nepovinn&yacute;</trd>,
    <trd>v&yacute;b&ecaron;rov&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>fakultativ</trd>,
    <trd>optional</trd>
  </trdgrp>
  <trd lng="en">optional</trd>
  <trd lng="es">facultativo</trd>
  <trd lng="sk">nepovinn&yacute;</trd>
  <trdgrp lng="zh">
    <trd>&#21487;&#20219;&#24847;&#36873;&#25321; <pr>k&ecaron;r&egrave;ny&igrave;xu&#462;nz&eacute;</pr></trd>,
    <trd>&#20219;&#36873; <pr>r&egrave;nxu&#462;n</pr></trd>,
    <trd>&#36873;&#20462; <pr>xu&#462;nxi&umacron;</pr></trd>,
    <trd>&#22914;&#26377;&#21017;&#22635; <pr>r&uacute;y&#466;uz&eacute;ti&aacute;n</pr></trd>,
    <trd>&#21487;&#36873;&#36141; <pr>k&ecaron;xu&#462;ng&ograve;u</pr></trd>,
    <trd>&#21487;&#20379;&#29992;&#25142;&#36984;&#29992; <pr>k&ecaron;g&omacron;ngy&ograve;ngh&ugrave;xu&#462;ny&ograve;ng</pr></trd>,
    <trd>&#29992;&#25142;&#21487;&#36984;&#36092;&#30340; <pr>y&ograve;ngh&ugrave;k&ecaron;xu&#462;ng&ograve;ude</pr></trd>,
    <trd>&#29992;&#25142;&#21487;&#36984;&#36092; <pr>y&ograve;ngh&ugrave;k&ecaron;xu&#462;ng&ograve;u</pr></trd>,
    <trd>&#36873;&#37197; <pr>xu&#462;np&egrave;i</pr></trd>
  </trdgrp>
</drv>
</art>

