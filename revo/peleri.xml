<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>pelerin</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="peleri.0o">
  <kap><tld/>o</kap>
  <snc mrk="peleri.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Pli malpli mallonga, pendanta, sur&scirc;ultra, senmanika mantelo
      kovranta
      dorson kaj bruston:
      <ekz>
        ili <klr>...</klr> portas <tld/>etojn kiel veraj leterportistoj
        <fnt>
        <aut>Karel &Ccaron;apek, trad. Josef Vondrou&scaron;ek</aut>,
        <vrk><url
        ref="http://mujweb.cz/Kultura/malovec/naufabel.htm">Na&ubreve;
        fabeloj</url></vrk>,
        <lok>Fabelo po&scirc;tista, [2002?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Pelerine</trd>
  <trdgrp lng="en">
    <trd>pelerine</trd>,
    <trd>cape</trd>,
    <trd>tippet</trd>
  </trdgrp>
  <trd lng="es">pelerina</trd>
  <trd lng="fr">p&egrave;lerine</trd>
  <trdgrp lng="hu">
    <trd>pelerin</trd>,
    <trd>k&ouml;rgall&eacute;r</trd>
  </trdgrp>
  <trd lng="nl">pelerine</trd>
  <trd lng="pt">pelerine</trd>
  <trd lng="ru">&c_p;&c_je;&c_l;&c_je;&c_r;&c_i;&c_n;&c_a;</trd>
  <trd lng="sv">pelerin</trd>
</drv>
</art>
