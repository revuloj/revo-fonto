<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>pelerin</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="peleri.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Pli malpli mallonga, pendanta, sur&scirc;ultra, senmanika mantelo kovranta
      dorson kaj bruston.
    </dif>
  </snc>
  <trd lng="de">Pelerine</trd>
  <trdgrp lng="en">
    <trd>pelerine</trd>,
    <trd>cape</trd>,
    <trd>tippet</trd>
  </trdgrp>
  <trd lng="es">pelerina</trd>
  <trdgrp lng="hu">
    <trd>pelerin</trd>,
    <trd>k&ouml;rgall&eacute;r</trd>
  </trdgrp>
  <trd lng="nl">pelerine</trd>
  <trd lng="pt">pelerine</trd>
  <trd lng="ru">&c_p;&c_je;&c_l;&c_je;&c_r;&c_i;&c_n;&c_a;</trd>
</drv>
</art>
