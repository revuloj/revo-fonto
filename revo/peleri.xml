<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: peleri.xml,v 1.24 2024/02/17 06:43:05 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>pelerin</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="peleri.0o">
  <kap><tld/>o</kap>
  <snc mrk="peleri.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Pli malpli mallonga, pendanta, sur&scirc;ultra, senmanika <ref cel="mantel.0o.senmanika">mantelo</ref>
      kovranta dorson kaj bruston:
      <ekz>
        <nom>Agripina</nom> tiris sian <tld/>on pli firme &ccirc;irka&ubreve; siaj &scirc;ultroj
        <fnt>
          <bib>MJA</bib>
          <lok>Parto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <klr>[&#x2026;]</klr> portas <tld/>etojn kiel veraj leterportistoj
        <fnt>
        <aut>Karel &Ccaron;apek, trad. Josef Vondrou&scaron;ek</aut>,
        <vrk><url ref="http://mujweb.cz/Kultura/malovec/naufabel.htm">Na&ubreve;
        fabeloj</url></vrk>,
        <lok>Fabelo po&scirc;tista, [2002?]</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="palt.duon0o">duonpalto</ref>,
      <ref cel="poncx.0o">pon&ccirc;o</ref>,
      <ref cel="sxal.0o">&scirc;alo</ref>,
      <ref cel="vest.super0o">supervesto</ref>
    </refgrp>
    <bld lok="&WCU;/thumb/0/0f/Pelerine1911.jpg/330px-Pelerine1911.jpg" prm="CC BY-SA 2.0"><tld/>o
        <fnt>
          <lok><url ref="&WCW;/File:Pelerine1911.jpg">https://commons.wikimedia.org/wiki/File:Pelerine1911.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ja;&c_r;&c_y;&c_n;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>esclavina</trd>,
    <trd>pelerina</trd>
  </trdgrp>
  <trd lng="cs">peler&iacute;na</trd>
  <trd lng="de">Pelerine</trd>
  <trdgrp lng="en">
    <trd>pelerine</trd>,
    <trd>cape</trd>,
    <trd>tippet</trd>
  </trdgrp>
  <trd lng="es">pelerina</trd>
  <trd lng="fr">p&egrave;lerine</trd>
  <trdgrp lng="hu">
    <trd>pelerin</trd>,
    <trd>k&ouml;rgall&eacute;r</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30B1;&#x30FC;&#x30D7;</trd>,
    <trd>&#x77ED;&#x30DE;&#x30F3;&#x30C8;  <pr>&#x305F;&#x3093;&#x307E;&#x3093;&#x3068;</pr></trd>,
    <trd>&#x30DA;&#x30EB;&#x30EA;&#x30FC;&#x30CC;</trd>
  </trdgrp>
  <trd lng="nl">pelerine</trd>
  <trd lng="pl">peleryna</trd>
  <trd lng="pt">pelerine</trd>
  <trd lng="ru">&c_p;&c_je;&c_l;&c_je;&c_r;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>peler&iacute;na</trd>,
    <trd>pl&aacute;&scaron;&tcaron; bez ruk&aacute;vov</trd>
  </trdgrp>
  <trd lng="sv">pelerin</trd>
  <trd lng="uk">&c_p;&c_je;&c_l;&c_je;&c_r;&c_i;&c_n;&c_a;</trd>
  <trd lng="zh">&#x62AB;&#x80A9; <pr>p&#x12B;ji&#x101;n</pr></trd>
</drv>
</art>
