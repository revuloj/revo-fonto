<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>bastard</rad>/o
</kap>
<drv mrk="bastar.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEN</uzo>
  <snc>
    <dif>
      Infano naskita ekster la&ubreve;le&gcirc;a edzeco:
      <ekz>
        <uzo tip="stl">FIG</uzo><tld/>inoj<klr>...</klr>! Vin tondro kaj
        fulmo frapu
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 59a, p. 298a</lok></fnt>!
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>uneheliches Kind</trd>,
     <trd>Bastard<klr> [pej]</klr></trd>,
     <trd>Kethel<klr> [ark]</klr></trd>
  </trdgrp>
  <trd lng="es">bastardo</trd>
  <trd lng="fr">b&acirc;tard<klr tip="ind"> (rejeton)</klr></trd>
  <trd lng="he">&mem;&mem;&zayin;&resh;</trd>
  <trdgrp lng="hu">
    <trd>fatty&uacute;</trd>,
    <trd>t&ouml;rv&eacute;nytelen gyermek</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_n;&c_je;&c_b;&c_r;&c_a;&c_ch;&c_n;&c_y;&c_j; &c_r;&c_je;&c_b;&c_jo;&c_n;&c_o;&c_k;</trd>,
    <trd>&c_n;&c_je;&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_o;&c_r;&c_o;&c_zh;&c_d;&c_je;&c_n;&c_n;&c_y;&c_j; &c_r;&c_je;&c_b;&c_jo;&c_n;&c_o;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_b;&c_o;&c_ch;&c_n;&c_y;&c_j; &c_s;&c_y;&c_n;</trd>,
    <trd>&c_u;&c_b;&c_l;&c_ju;&c_d;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">bastard</trd>
</drv>

<drv mrk="bastar.0eco">
  <kap><tld/>eco</kap>
  <uzo tip="fak">GEN</uzo>
  <snc>
    <dif>
      Stato de <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>uneheliche Kindschaft</trd>
  </trdgrp>
  <trd lng="fr">b&acirc;tardise</trd>
  <trd lng="he">&mem;&mem;&zayin;&resh;&vav;&tav;</trd>
  <trd lng="ru">&c_n;&c_je;&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_o;&c_r;&c_o;&c_zh;&c_d;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>
</art>
