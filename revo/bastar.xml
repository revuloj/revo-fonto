<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bastar.xml,v 1.26 2023/10/29 19:11:00 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>bastard</rad>/o
</kap>
<drv mrk="bastar.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEN</uzo>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="bastar.0o.infano">
    <dif>
      <ref tip="super" cel="infan.0o.GEN">Infano</ref> naskita ekster la&ubreve;le&gcirc;a edzeco:
      <ekz>
        silente min sekvu, <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>!
      </ekz>
      <ekz>
        vi estas <tld/>oj, kaj ne filoj
        <fnt>
          <bib>NT</bib>
          <lok>Hebreoj 12:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        pereu liaj infanoj kaj plenigu lian domon <tld/>oj, kiuj kra&ccirc;os sur lin kaj lin
        elpelos
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo><tld/>inoj<klr>[&#x2026;]</klr>! vin tondro kaj
        fulmo frapu
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 59a, p.
        298a</lok></fnt>!
      </ekz>
    </dif>
    <trd lng="zh">&#x79C1;&#x5B69;&#x5B50; <pr>s&#x12B;h&#xE1;iz&#x1D0;</pr></trd>
</snc>
  <trdgrp lng="cs">
    <trd>bastard</trd>,
    <trd>levobo&ccaron;ek</trd>,
    <trd>panchart</trd>,
    <trd>parchant</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>uneheliches Kind</trd>,
     <trd>Bastard<klr> [pej]</klr></trd>,
     <trd>Kethel<klr> [ark]</klr></trd>
  </trdgrp>
  <trd lng="en">bastard</trd>
  <trd lng="es">bastardo</trd>
  <trd lng="fr">b&acirc;tard<klr tip="ind"> (rejeton)</klr></trd>
  <trd lng="he">&mem;&mem;&zayin;&resh;</trd>
  <trdgrp lng="hu">
    <trd>fatty&uacute;</trd>,
    <trd>t&ouml;rv&eacute;nytelen gyermek</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5AE1;&#x51FA;&#x3067;&#x306A;&#x3044;&#x5B50;  <pr>&#x3061;&#x3083;&#x304F;&#x3057;&#x3085;&#x3064;&#x3067;&#x306A;&#x3044;&#x3053;</pr></trd>,
    <trd>&#x5EB6;&#x5B50;  <pr>&#x3057;&#x3087;&#x3057;</pr></trd>,
    <trd>&#x79C1;&#x751F;&#x5B50;  <pr>&#x3057;&#x305B;&#x3044;&#x3057;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>b&eogonek;kart<klr> (obra&zacute;l.)</klr></trd>,
    <trd>bastard<klr> (rzadko)</klr></trd>,
    <trd>nie&sacute;lubne dziecko</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_n;&c_je;&c_b;&c_r;&c_a;&c_ch;&c_n;&c_y;&c_j;
    &c_r;&c_je;&c_b;&c_jo;&c_n;&c_o;&c_k;</trd>,
    <trd>&c_n;&c_je;&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_o;&c_r;&c_o;&c_zh;&c_d;&c_je;&c_n;&c_n;&c_y;&c_j;
    &c_r;&c_je;&c_b;&c_jo;&c_n;&c_o;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_b;&c_o;&c_ch;&c_n;&c_y;&c_j;
    &c_s;&c_y;&c_n;</trd>,
    <trd>&c_u;&c_b;&c_l;&c_ju;&c_d;&c_o;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bastard</trd>,
    <trd>neman&zcaron;elsk&eacute; die&tcaron;a</trd>
  </trdgrp>
  <trd lng="tr">pi&ccedil;</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&c_j;&c_s;&c_t;&c_r;&#x44E;&#x43A;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_b;&c_a;&c_t;&c_ch;&c_je;&#x43D;&#x43A;&#x43E;</trd>,
    <trd>&#x43D;&c_je;&c_sh;&#x43B;&#x44E;&c_b;&#x43D;&c_a; &c_d;&c_i;&c_t;&c_i;&#x43D;&c_a;</trd>,
    <trd>&c_s;&c_h;&c_r;&c_je;&c_shch;&c_je;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43F;&#x43E;&#x43C;&c_ib;&c_s;&#x44C;</trd>,
    <trd>&c_g;&c_ib;&c_b;&c_r;&c_i;&c_d;</trd>
  </trdgrp>
</drv>

<drv mrk="bastar.0eco">
  <kap><tld/>eco</kap>
  <uzo tip="fak">GEN</uzo>
  <snc mrk="bastar.0eco.stato">
    <dif>
      Stato de <tld/>o:
      <ekz>
        &ccirc;u vere la puno de la <tld/>eco estas morto
        <fnt>
          <vrk><url ref="http://verkoj.com/verkistoj/marianne-pierquin-kaj-said-balucimarianne-pierquin/mamurzo/">Mamurzo
          - Verkoj</url></vrk>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>bastardstv&iacute;</trd>,
    <trd>nepoda&rcaron;enost</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>uneheliche Kindschaft</trd>
  </trdgrp>
  <trd lng="es">bastard&iacute;a</trd>
  <trd lng="fr">b&acirc;tardise</trd>
  <trd lng="he">&mem;&mem;&zayin;&resh;&vav;&tav;</trd>
  <trdgrp lng="ja">
    <trd>&#x5EB6;&#x751F; <pr>&#x3057;&#x3087;&#x305B;&#x3044;</pr></trd>,
    <trd>&#x79C1;&#x751F;  <pr>&#x3057;&#x305B;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_je;&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_o;&c_r;&c_o;&c_zh;&c_d;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">nepodarenos&tcaron;</trd>
</drv>
</art>
