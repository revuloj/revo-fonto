<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>bastard</rad>/o
</kap>
<drv mrk="bastar.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Infano naskita ekster la&ubreve;le&gcirc;a edzeco.
    </dif>
  </snc>
  <trd lng="fr">b&acirc;tard<klr tip="ind"> (rejeton)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_n;&c_je;&c_b;&c_r;&c_a;&c_ch;&c_n;&c_y;&c_j; &c_r;&c_je;&c_b;&c_jo;&c_n;&c_o;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_b;&c_o;&c_ch;&c_n;&c_y;&c_j; &c_s;&c_y;&c_n;</trd>,
    <trd>&c_u;&c_b;&c_l;&c_ju;&c_d;&c_o;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="bastar.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Stato de <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">b&acirc;tardise</trd>
</drv>
</art>
