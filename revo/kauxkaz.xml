<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kauxkaz.xml,v 1.19 2024/10/17 06:36:22 revo Exp $">
<kap><ofc>10</ofc>
  <rad>Ka&ubreve;kaz</rad>/o
</kap>

<drv mrk="kauxkaz.K0o">
   <kap><ofc>10</ofc><tld/>o</kap>
   <uzo tip="fak">GEOG</uzo>
   <snc mrk="kauxkaz.K0o.terstrio">
     <dif>
       Terstrio inter <ref cel="kaspi.K0o">Kaspio</ref>
       oriente kaj la maroj Azova kaj Nigra okcidente, kies
       norda parto situas en
       <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref> kaj la
       suda,
       en la <ref tip="malprt" cel="azi.OkcidentaA0o">Okcidenta
       Azio</ref>:
       <ekz>
         terura estas la stato de aferoj en la multelingva <tld/>o
         <fnt>
         <aut>&Z;</aut>:
         <vrk>La parolado anta&ubreve; la Dua Kongreso
         Esperantista</vrk>,
         <lok>&Gcirc;enevo, 1906-08-28.</lok>
         </fnt>;
       </ekz>
      <ekz>
        mia plej juna filino <klr>[&#8230;]</klr> laboris en <tld/>o por la Internacia Ru&gcirc;a
        Kruco
        <fnt>
          <bib>BonaLingvo</bib>
          <lok>Postparolo Al La Dua Eldono</lok>
        </fnt>.
      </ekz>
     </dif>
     <trd lng="de">Kaukasien</trd>
     <trdgrp lng="en">
       <trd>Caucasus</trd>,
       <trd>Caucasia</trd>
     </trdgrp>
     <trd lng="hu">Kauk&aacute;zus</trd>
     <trd lng="fr">Caucase</trd>
   </snc>
   <snc mrk="kauxkaz.K0o.montaro">
     <dif>
       <ref tip="dif" cel="kauxkaz.K0aMontaro"><tld/>a montaro</ref>:
       <ekz>
        la generalo <nom>Jermolov</nom>, en &ccirc;erkesa mantelo minace sulkigis la brovojn, rigardante
        la malproksimajn montojn de <tld/>o
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>VIII</lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_K;&c_a;&c_w;&c_k;&c_a;&c_z;</trd>
   <trd lng="de">Kaukasus</trd>
   <trd lng="en">Caucasus</trd>
   <trd lng="nl">Kaukasus</trd>
   <trd lng="pl">Kaukaz</trd>
   <trd lng="ru">&c_K;&c_a;&c_v;&c_k;&c_a;&c_z;</trd>
  <trd lng="uk">&c_g;&c_o;&c_r;&c_i; &c_K;&c_a;&c_v;&c_k;&c_a;&c_z;</trd>
  <trdgrp lng="zh">
    <trd>&#39640;&#21152;&#32034; <pr>G&amacron;oji&amacron;su&#466;</pr></trd>,
    <trd>&#39640;&#21152;&#32034; <pr>g&amacron;oji&amacron;su&#466;</pr></trd>,
    <trd>&#39640;&#21152;&#32034;&#23665;&#33032; <pr>g&amacron;oji&amacron;su&#466;sh&amacron;nm&agrave;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kauxkaz.0a">
   <kap><ofc>10</ofc><tld lit="k"/>a</kap>
   <snc>
     <dif>
       Rilata al <tld/>o,<sncref ref="kauxkaz.K0o.terstrio"/>:
       <ekz>
         la <ind><tld lit="k"/>aj lingvoj</ind>
         <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
         </fnt>;
         <trd lng="be">&c_k;&c_a;&c_w;&c_k;&c_a;&c_s;&c_k;&c_ib;&c_ja; &c_m;&c_o;&c_v;&c_y;</trd>
         <trd lng="hu">kauk&aacute;zusi nyelvek</trd>
         <trd lng="nl">Kaukasische talen</trd>
         <trd lng="ru">&c_k;&c_a;&c_v;&c_k;&c_a;&c_z;&c_s;&c_k;&c_i;&c_je;
         &c_ja;&c_z;&c_y;&c_k;&c_i;</trd>
       </ekz>
       <ekz>
        <tld lit="k"/>aj rakontoj
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        dol&ccirc;a <tld lit="k"/>a &ccirc;ampano
        <fnt>
          <bib>BonaLingvo</bib>
          <lok>Postparolo Al La Dua Eldono</lok>
        </fnt>;
      </ekz>
      <ekz>
        Osetio &ndash; <tld lit="k"/>a respubliko en Ruslando
        <fnt>
          <bib>LOdE</bib>
          <lok>2004, &#8470; 4 (114)</lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_k;&c_a;&c_w;&c_k;&c_a;&c_s;&c_k;&c_ib;</trd>
   <trd lng="de">kaukasisch</trd>
   <trd lng="nl">Kaukasisch</trd>
   <trd lng="fr">caucasien</trd>
   <trd lng="pl">kaukaski</trd>
   <trd lng="ru">&c_k;&c_a;&c_v;&c_k;&c_a;&c_z;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="kauxkaz.K0aMontaro">
   <kap><tld/>a montaro</kap>
   <snc>
   <uzo tip="fak">GEOG</uzo>
     <dif>
       La <ref tip="lst" lst="voko:montaroj" cel="mont.0aro">montaro</ref> kiu okupas
       la sudan parton de
       <ref tip="malprt" cel="kauxkaz.K0o.terstrio"><tld/>o<sncref/></ref>.
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_K;&c_a;&c_w;&c_k;&c_a;&c_z;</trd>,
     <trd>&c_K;&c_a;&c_w;&c_k;&c_a;&c_s;&c_k;&c_ib;&c_ja; &c_g;&c_o;&c_r;&c_y;</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>Kaukasus</trd>,
     <trd>Kaukasusgebirge</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>Caucasus</trd>,
     <trd>Caucasus Mountains</trd>
   </trdgrp>
   <trd lng="fr">Monts de <ind>Transcaucasie</ind></trd>
   <trdgrp lng="hu">
     <trd>Kauk&aacute;zus</trd>,
     <trd>Kauk&aacute;zusi-hegys&eacute;g</trd>
   </trdgrp>
   <trd lng="pl">Kaukaz</trd>
   <trdgrp lng="ru">
     <trd>&c_K;&c_a;&c_v;&c_k;&c_a;&c_z;</trd>,
     <trd>&c_K;&c_a;&c_v;&c_k;&c_a;&c_z;&c_s;&c_k;&c_i;&c_je;
     &c_g;&c_o;&c_r;&c_y;</trd>
   </trdgrp>
  <trdgrp lng="zh">
    <trd>&#39640;&#21152;&#32034;&#23665;&#33032; <pr>G&amacron;oji&amacron;su&#466;Sh&amacron;nm&agrave;i</pr></trd>,
    <trd>&#39640;&#21152;&#32034; <pr>g&amacron;oji&amacron;su&#466;</pr></trd>,
    <trd>&#39640;&#21152;&#32034;&#23665;&#33032; <pr>g&amacron;oji&amacron;su&#466;sh&amacron;nm&agrave;i</pr></trd>
  </trdgrp>
</drv>
</art>
