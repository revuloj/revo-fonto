<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Ka&ubreve;kaz</rad>/o
    </kap>
    <drv mrk="kauxkaz.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc mrk="kauxkaz.0o.terstrio">
	<dif>
          Terstrio inter <ref cel="kaspi.0o">Kaspio</ref>
          oriente kaj la maroj Azova kaj Nigra okcidente, kies
          norda parto situas en
          <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref> kaj la
suda,
          en la <ref tip="malprt" cel="azi.A0o.okcidenta">Okcidenta
Azio</ref>:
          <ekz>
            terura estas la stato de aferoj en la multelingva <tld/>o
            <fnt>
              <aut>&Z;</aut>:
              <vrk>La parolado anta&ubreve; la Dua Kongreso
Esperantista</vrk>,
              <lok>&Gcirc;enevo, 1906-08-28.</lok>
            </fnt>
          </ekz>
	</dif>
        <trdgrp lng="en">
          <trd>Caucasus</trd>,
          <trd>Caucasia</trd></trdgrp>
        <trd lng="hu">Kauk&aacute;zus</trd>
      </snc>
      <snc mrk="kauxkaz.0o.montaro">
	<dif>
          <ref tip="dif" cel="kauxkaz.0aMontaro"><tld/>a montaro</ref>.
	</dif>
        <trd lng="en">Caucasus</trd>
      </snc>
      <trd lng="ru">&c_K;&c_a;&c_v;&c_k;&c_a;&c_z;</trd>
    </drv>
    <drv mrk="kauxkaz.0a">
      <kap><tld lit="k"/>a</kap>
      <snc>
        <dif>
          Rilata al <tld/>o,<sncref ref="kauxkaz.0o.terstrio"/>:
          <ekz>
            la <ind><tld lit="k"/>aj lingvoj</ind><fnt>PIV</fnt>.
            <trd lng="hu">kauk&aacute;zusi</trd>
            <trd
lng="ru">&c_k;&c_a;&c_v;&c_k;&c_a;&c_z;&c_s;&c_k;&c_i;&c_je;
&c_ja;&c_z;&c_y;&c_k;&c_i;</trd>
          </ekz>
        </dif>
      </snc>
      <trd lng="ru">&c_k;&c_a;&c_v;&c_k;&c_a;&c_z;&c_s;&c_k;&c_i;&c_j;</trd>
    </drv>
    <drv mrk="kauxkaz.0aMontaro">
      <kap><tld/>a montaro</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
        <dif>
          La montaro kiu okupas la sudan parton de
          <ref tip="malprt" cel="kauxkaz.0o.terstrio"><tld/>o</ref>,
          <sncref ref="kauxkaz.0o.terstrio"/>.
        </dif>
      </snc>
      <trdgrp lng="en">
        <trd>Caucasus</trd>,
        <trd>Caucasus Mountains</trd></trdgrp>
      <trdgrp lng="hu">
        <trd>Kauk&aacute;zus</trd>,
        <trd>Kauk&aacute;zusi-hegys&eacute;g</trd></trdgrp>
      <trdgrp lng="ru">
        <trd>&c_K;&c_a;&c_v;&c_k;&c_a;&c_z;</trd>,
        <trd>&c_K;&c_a;&c_v;&c_k;&c_a;&c_z;&c_s;&c_k;&c_i;&c_je;
&c_g;&c_o;&c_r;&c_y;</trd></trdgrp>
    </drv>
  </art>

