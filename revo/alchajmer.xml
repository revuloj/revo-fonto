<vortaro>
  <art mrk="$Id: alchajmer.xml,v 1.4 2023/04/28 07:24:35 afido Exp $">
    <kap>
      <rad>Alchajmer</rad>/o,
      <var><kap><rad var="e">Alchejmer</rad>/o</kap></var>
    </kap>

<drv mrk="alchajmer.0o">
  <kap><tld/>o, <var><kap><tld var="e"/>o</kap></var></kap>
  <snc>
    <uzo tip="fak">PERS</uzo>
    <dif>
      Germanlingve <trd lng="de">Alois <ind>Alzheimer</ind></trd> (1864&#8239;-&#8239;1915), <ref tip="lst"
      lst="voko:historiaj_personoj" cel="psikia.0o">psikiatro</ref> 
      kaj nervopatologo, kiu priskribis formon de demenco nomita la&ubreve; li; metonimie tiu <ref
      cel="demenc.0o">demenco</ref> mem:
      <ekz>
        post la t.n. <tld lit="a"/>a demenco, la parkinsona morbo estas la plej ofta
        nervodegenera malsano en la mondo
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2012/010783.php"
              >&Ccirc;u vakcino &ccirc;e la horizonto?</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        se la vivespero alti&gcirc;is, tio traduki&gcirc;as anka&ubreve; <klr>[&#8230;]</klr> per eksplodo de
        pensiul-malsanoj: kanceroj, <klr>[&#8230;]</klr> senilaj demencoj, <tld lit="a" var="e"/>oj
        <fnt>
          <bib>MD02</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_A;&c_l;&c_mol;&c_c;&c_g;&c_a;&c_j;&c_m;&c_e;&c_r;</trd>
    <trd lng="en"><ind>Alzheimer</ind>, Alois</trd>
    <trd lng="fr"><ind>Alzheimer</ind>, Alois</trd>
  </snc>
</drv>

  </art>

