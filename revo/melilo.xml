<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: melilo.xml,v 1.19 2023/09/02 06:04:11 revo Exp $">
<kap>
  <rad>melilot</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="melilo.0o">
  <kap><tld/>o</kap>
  <snc mrk="melilo.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Jara herbo el E&ubreve;razio kaj Nordafriko kun dol&ccirc;odoraj floroj,
      uzataj kiel fura&gcirc;o kaj kuracilo, 
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref> el familio <ref tip="malprt" cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Melilotus</trd>):
      <ekz>
        kaj bela roso fluas, rozoj floras, /
        cerefolioj verdas delikataj /
        kaj <tld/>o dol&ccirc;e bonodoras
        <fnt>
        <aut>Sapfo, trad. K. Kalocsay</aut>,
        <vrk>Mnasidika</vrk>,
        <lok>en: <url ref="http://egalite.freeweb.hu/kalocsay/tutmonda.htm">
        Tutmonda sonoro</url>, 1981</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_r;&c_k;&c_u;&c_n;</trd>,
    <trd>&c_t;&c_o;&c_m;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">melilot</trd>
  <trd lng="cs">komonice</trd>
  <trd lng="de">Steinklee</trd>
  <trd lng="en">melilot</trd>
  <trd lng="fr">m&eacute;lilot</trd>
  <trdgrp lng="hu">
    <trd>somk&oacute;r&oacute;</trd>,
    <trd>m&eacute;zkerep</trd>
  </trdgrp>
  <trd lng="ja">&#x30B7;&#x30CA;&#x30AC;&#x30EF;&#x30CF;&#x30AE;</trd>
  <trd lng="nl">honingklaver</trd>
  <trd lng="pl">nostrzyk</trd>
  <trdgrp lng="pt">
    <trd>meliloto</trd>,
    <trd>trevo-cheiroso</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_o;&c_n;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">komonica</trd>
  <trd lng="uk">&c_b;&c_u;&c_r;&#x43A;&c_u;&#x43D;</trd>
</drv>
</art>
