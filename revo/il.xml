<drv mrk="il.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="il.0aro.TEK">
    <dif>
      <ref tip="vid" cel="sortim.0o.KOMUNE">Sortimento</ref>
      de <tld/>oj utilaj por iu tasko:
      <ekz>
        la <tld/>aro de pa&scirc;tisto
        <fnt>
          <bib>MT</bib>
          <lok>Ze&hcirc;arja 11:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i malfermis longforman skatolon, en kiu trovi&gcirc;is <tld/>aro por
        desegnado
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        <klr>[lia]</klr> tuta lo&gcirc;eja <tld/>aro konsistis nur el bototir<tld/>o kaj
        komb<tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolumo</lok>
        </fnt>;
      </ekz>
      <ekz>
        metiisto ne povas bone labori, se li ne majstras sian <tld/>aron
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&gcirc;i]</klr> enhavis tutan <tld/>aron de kolektisto, pinglojn, prenilojn,
        kartonojn, korkoligna&jcirc;ojn kaj &ccirc;iuspecajn pulvorojn
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="garnit.0o.KOMUNE">garnituro</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_b;&c_o;&c_r; &c_p;&c_r;&c_y;&c_l;&c_a;&c_d;&c_a;&c_w;</trd>,
    <trd>&c_ib;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_e;&c_n;&c_t;&c_a;&c_r;&c_y;&c_j;</trd>
