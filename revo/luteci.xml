<art mrk="$Id$">
<kap>
  <rad>luteci</rad>/o
</kap>
<drv mrk="luteci.0o">
  <kap>
    <tld/>o
  </kap>
  <mlg kod="IUPAC">Lu</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
      lst="voko:&hcirc;emiaj_elementoj" val="71">elemento</ref> kun
      atomnumero 71, atompezo 174,97; 
      malofta ar&gcirc;entgriza <ref tip="super"
cel="metal.0o">metalo</ref>, lantanoido.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ju;&c_t;&c_e;&c_c;&c_y;&c_j;</trd>      
  <trd lng="br">lutesiom</trd>
  <trd lng="de">Lutetium</trd>
  <trd lng="es">lutecio</trd>
  <trd lng="fa">&a_l;&a_w;&a_t;&a_s;&f_y;&a_m;</trd>
  <trd lng="fr">lut&eacute;cium</trd>
  <trd lng="hu">lut&eacute;cium</trd>
  <trd lng="la">Lutetium</trd>
  <trd lng="nl">lutecium</trd>
  <trd lng="pl">lutet</trd>
  <trd lng="pt">lut&eacute;cio</trd>
  <trd lng="ru">&c_l;&c_ju;&c_t;&c_je;&c_c;&c_i;&c_j;</trd>
  <trd lng="sv">lutetium</trd>
</drv>
</art>
