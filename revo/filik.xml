<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>filik</rad>/oj
</kap>
<drv mrk="filik.0oj">
  <kap><tld/>oj</kap>
  <snc>
    <dif>
      Klaso de <ref tip="super" cel="filik.0oplantoj"><tld/>oplantoj</ref>
      (<trd lng="la">Polypodiopsida</trd>), plurjaraj herboj 
      kun plej ofte dividataj folioj, june helike enrulitaj.
    </dif>
  </snc>
  <trd lng="de">Farne</trd>
</drv>
<drv mrk="filik.0oplantoj">
  <kap><tld/>oplantoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Divizio de la <ref tip="super" cel="plant.0oj">plantoj</ref>
      (<trd lng="la">Polypodiophyta</trd>), sporoplantoj kun rizomo
      kaj angioj.
    </dif>
  </snc>
  <trd lng="de">Farnpflanzen</trd>
  <trd lng="en">ferns</trd>
</drv>
</art>
