<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: miceli.xml,v 1.6 2024/01/26 17:40:06 revo Exp $">
<kap>
  <rad>miceli</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="miceli.0o">
  <kap><tld/>o</kap>
  <snc mrk="miceli.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Nefrukta parto de 
      <ref tip="malprt" cel="fung.0o.BIO">fungo</ref>,
      konsistanta el ordinare subteraj kaj blankaj
      fadenetoj:
      <ekz>
        <tld/>o konsistanta el hifoj senkoloraj
        <fnt>
          <aut>P. Navarro&ndash;Rosin&eacute;s, Claude Roux, X. Llimona</aut>,
          <vrk><url ref="http://lichenologue.org/fichiers/docs/1994Navarro_Roux_FunpojSquamarina.pdf">
          Nelikenifiintaj fungoj &ccirc;e Squamarina</url></vrk>,
          <lok>Bull. Soc. linn. Provence, 1994</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ib;&c_c;&c_e;&c_l;&c_ib;&c_j;</trd>,
    <trd>&c_g;&c_r;&c_y;&c_b;&c_n;&c_ja;</trd>,
    <trd>&c_g;&c_r;&c_y;&c_b;&c_n;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mycelium</trd>,
    <trd>podhoub&#xED;</trd>
  </trdgrp>
  <trd lng="en">mycelium</trd>
  <trd lng="fr">mycelium</trd>
  <trdgrp lng="sk">
    <trd>myc&#xE9;lium</trd>,
    <trd>podhubie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_r;&c_i;&c_b;&#x43D;&c_i;&c_c;&#x44F;</trd>,
    <trd>&#x43C;&c_ib;&c_c;&c_je;&#x43B;&c_ib;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x83CC;&#x4E1D; <pr>j&#x16B;ns&#x12B;</pr></trd>,
    <trd>&#x83CC;&#x7D72; <pr>j&#x16B;ns&#x12B;</pr></trd>
  </trdgrp>
</drv>
</art>

