  <trd lng="am">&#x12A5;&#x1231;</trd>
  <trd lng="ar">&#x647;&#x648;</trd>
  <trd lng="az">vaxt</trd>
  <trd lng="bn">&#x98F;&#x99F;&#x9BE;</trd>
  <trd lng="bs">to</trd>
