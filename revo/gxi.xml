</drv>

<drv mrk="gxi.0a">
   <kap><tld/>a</kap>
   <snc>
   <uzo tip="fak">GRA</uzo>
     <dif>
       Apartenanta al <tld/>:
       <ekz>
         domo kaj <tld/>aj dependa&jcirc;oj.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="ties.0">ties</ref>.
     </refgrp>
   </snc>
   <trd lng="el">&tau;&omikron;&ypsilon;</trd>
   <trd lng="en">its</trd>
   <trd lng="be">&c_ja;&c_g;&c_o;</trd>
   <trd lng="de">sein <klr tip="ind">(Besitz)</klr></trd>
   <trdgrp lng="fi">
     <trd>sen</trd>
   </trdgrp>
   <trd lng="fr">son<klr tip="ind"> (adj. poss.)</klr></trd>
   <trd lng="fy">syn</trd>
   <trdgrp lng="he">
     <trd>&shin;&lamed; &zayin;&vav;</trd>,
     <trd>&shin;&lamed; &zayin;&he;</trd>,
