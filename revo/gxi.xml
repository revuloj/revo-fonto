<drv mrk="gxi.0a">
  <kap><tld/>a</kap>
  <snc>
    <uzo tip="fak">GRA</uzo>
    <dif>
      Apartenanta al <tld/>:
      <ekz>
        Stefano a&ubreve;dis virvo&ccirc;an zumadon, sed ne sukcesis kapti <tld/>an sencon
        <fnt>
          <bib>ChB</bib>
          <lok>11</lok>
        </fnt>;
      </ekz>
      <ekz>
        la arbo estas <klr>[&#8230;]</klr> &ccirc;arma por la okuloj, <klr>[&#8230;]</klr> kaj &scirc;i prenis
        de <tld/>aj fruktoj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 3:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bur&gcirc;ono malfermi&gcirc;is <klr>[&#8230;]</klr> kaj en <tld/>a mezo ku&scirc;is &ccirc;arma infano
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        seruro gardas kontra&ubreve; &scirc;telo, &ccirc;ar tio estas <tld/>a celo
        <fnt>
          <bib>Fab4</bib>
          <lok>Abocolibro</lok>
        </fnt>;
      </ekz>
      <ekz>
        al &ccirc;iu besto pla&ccirc;as <tld/>a nesto
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ties.0">ties</ref>
    </refgrp>
  </snc>
