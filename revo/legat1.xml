<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>legat</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="legat1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Vo&ccirc;a a&ubreve; muzikila sonigo,
      kiu ligas la sinsekvajn tonojn kun kontinua efekto:
      <ekz>
        la malo de <tld/>o estas stakato, alian funkcion havas ligaturo:
        &gcirc;i ligas
        sinsekvajn tonojn de sama tonalto al longa tono
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=258596">Legato
          (muziko)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="ant">
    <ref cel="stakat.0o">stakato</ref>,
  </refgrp>
  <refgrp tip="hom">
    <ref cel="legat.0o">legato</ref> (ekleziulo)
  </refgrp>
  <trd lng="be">&c_l;&c_je;&c_g;&c_a;&c_t;&c_a;</trd>
  <trd lng="de">Legato</trd>
  <trdgrp lng="fr">
    <trd>legato</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>legato</trd>,
    <trd>k&ouml;t&ouml;tt el&odblac;ad&aacute;sm&oacute;d</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>legato</trd>
  </trdgrp>
  <trd lng="nl">legato</trd>
  <trd lng="pl">legato</trd>     
  <trd lng="ro">legato</trd>
  <trd lng="ru">&c_l;&c_je;&c_g;&c_a;&c_t;&c_o;</trd>
  <trd lng="sk">p&aacute;pe&zcaron;sk&yacute; vyslanec</trd>
  <trdgrp lng="cs">
    <trd>leg&aacute;t</trd>,
    <trd>pape&zcaron;sk&yacute; vyslanec</trd>
  </trdgrp>
</drv>

<drv mrk="legat1.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Lige, ne disigante la sinsekvajn tonojn.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_je;&c_g;&c_a;&c_t;&c_a;</trd>
  <trd lng="cs">legato</trd>
  <trd lng="de">gebunden</trd>
  <trdgrp lng="fr">
    <trd>legato</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>legato</trd>,
    <trd>k&ouml;t&ouml;tten</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>legato</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>legato</trd>,
    <trd>gebonden</trd>
  </trdgrp>
  <trd lng="pl">powi&aogonek;zane</trd>
  <trdgrp lng="ro">
    <trd>&icirc;nrudit</trd>,
    <trd>legat</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_je;&c_g;&c_a;&c_t;&c_o;</trd>
  <trd lng="sk">legato</trd>
</drv>
</art>

