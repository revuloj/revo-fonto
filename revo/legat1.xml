<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>legat</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="legat1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Vo&ccirc;a a&ubreve; muzikila sonigo,
      kiu ligas la sinsekvajn tonojn kun kontinua efekto.
    </dif>
  </snc>
  <refgrp tip="ant">
    <ref cel="stakat.0o">stakato</ref>
  </refgrp>
  <refgrp tip="hom">
    <ref cel="legat.0o">legato</ref>
  </refgrp>
  <trd lng="be">&c_l;&c_je;&c_g;&c_a;&c_t;&c_a;</trd>
  <trdgrp lng="fr">
    <trd>legato</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>legato</trd>,
    <trd>k&ouml;t&ouml;tt el&odblac;ad&aacute;sm&oacute;d</trd>
  </trdgrp>  
  <trdgrp lng="it">
    <trd>legato</trd>
  </trdgrp>
</drv>

<drv mrk="legat1.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Lige, ne disigante la sinsekvajn tonojn.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_je;&c_g;&c_a;&c_t;&c_a;</trd>
  <trdgrp lng="fr">
    <trd>legato</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>legato</trd>,
    <trd>k&ouml;t&ouml;tten</trd>
  </trdgrp>  
  <trdgrp lng="it">
    <trd>legato</trd>
  </trdgrp>
</drv>
</art>

