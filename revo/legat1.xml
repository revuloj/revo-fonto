<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>legat</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="legat1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Vo&ccirc;a a&ubreve; muzikila sonigo,
      kiu ligas la sinsekvajn tonojn kun kontinua efekto.
    </dif>
  </snc>
  <refgrp tip="ant">
    <ref cel="stakat.0o">stakato</ref>
  </refgrp>
  <refgrp tip="hom">
    <ref cel="legat.0o">legato</ref>
  </refgrp>
  <trdgrp lng="fr">
    <trd>legato</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>legato</trd>
  </trdgrp>
</drv>

<drv mrk="legat1.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Lige, ne disigante la sinsekvajn tonojn.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>legato</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>legato</trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
-->
</vortaro>