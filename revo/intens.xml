<drv mrk="intens.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Alta grado de aktiveco:
      <ekz>
        oni distingas la <tld/>econ, la altecon, la tembron kaj la flekseblecon de la
        vo&ccirc;o
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj okuloj iomete ru&gcirc;i&gcirc;is pro la <tld/>eco de liaj sentoj
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lernanto povas parte mem decidi, en kiu rapideco, sinsekvo kaj <tld/>eco
        li/&scirc;i trastudu la enhavojn
        <fnt>
          <bib>Kon11</bib>
          <lok>Herzberg, la Esperanto-urbo kiu proponas lingvan kaj movadan klerigadon</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_n;&c_t;&c_e;&c_n;&c_s;&c_y;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_r;&c_u;&c_zh;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">intenzita</trd>
  <trd lng="de">Intensit&auml;t</trd>
  <trd lng="en">intensity</trd>
  <trd lng="es">intensidad</trd>
  <trd lng="fa">&a_sh;&a_d;&a_t;</trd>
  <trd lng="fr">intensit&eacute;</trd>
  <trdgrp lng="hu">
    <trd>intenzt&aacute;s</trd>,
    <trd>er&odblac;ss&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6FC0;&#x3057;&#x3055;  <pr>&#x306F;&#x3052;&#x3057;&#x3055;</pr></trd>,
    <trd>&#x5F37;&#x3055;  <pr>&#x3064;&#x3088;&#x3055;</pr></trd>,
    <trd>&#x5F37;&#x70C8;  <pr>&#x304D;&#x3087;&#x3046;&#x308C;&#x3064;</pr></trd>
  </trdgrp>
  <trd lng="nl">intensiteit</trd>
  <trd lng="pt">intensidade</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_t;&c_je;&c_n;&c_s;&c_i;&c_v;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_r;&c_ja;&c_zh;&c_jo;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_ja;&c_r;&c_k;&c_o;&c_s;&c_t;&c_mol;<klr tip="amb"> (&c_o;
&c_c;&c_v;&c_je;&c_t;&c_je;)</klr></trd>,
    <trd>&c_n;&c_a;&c_s;&c_y;&c_shch;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;<klr tip="amb"> (&c_o; &c_c;&c_v;&c_je;&c_t;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="sk">intenzita</trd>
  <trdgrp lng="uk">
    <trd>&c_ib;&c_n;&c_t;&c_je;&c_n;&c_s;&c_i;&c_v;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_r;&c_u;&c_zh;&c_je;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5F37;&#x5EA6; <pr>qi&aacute;ngd&ugrave;</pr></trd>,
    <trd>&#x5BC6;&#x96C6;&#x5EA6; <pr>m&igrave;j&iacute;d&ugrave;</pr></trd>,
    <trd>&#x5F37;&#x70C8; <pr>qi&aacute;ngli&egrave;</pr></trd>,
    <trd>&#x660E;&#x6697;&#x5EA6; <pr>m&iacute;ng&agrave;nd&ugrave;</pr></trd>,
