<kap>
  <ofc>4</ofc>
  <rad>intens</rad>/a
</kap>
<drv mrk="intens.0a">
  <kap><tld/>a</kap>
  <snc mrk="intens.0a.strecxa">
    <dif>
      Plenumata a&ubreve; funkcianta energie, vigle, fortostre&ccirc;e:
      <ekz>
        <tld/>a laboro, terapio, trafiko;
      </ekz>
      <ekz>
        la instruistino, same kiel &scirc;iaj lernantoj, estis post kvartaga
        <tld/>a instruado tre laca
        <fnt><bib>PMEG</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fort.0a.intensa">forta<sncref/></ref>,
      <ref cel="ag.0ema">agema</ref>,
      <ref cel="vigl.0a.energia">vigla<sncref/></ref>,
      <ref cel="viv.0a.vigla">viva<sncref/></ref>
    </refgrp>
    <trdgrp lng="hu">
      <trd>intenz&iacute;v</trd>,
      <trd>er&odblac;teljes</trd>
    </trdgrp>
  </snc>
  <snc mrk="intens.0a.impresa">
    <dif>
      Klare, forte sentata:
      <ekz>
        <tld/>a lumo, koloro, malvarmo, sono, emocio;
      </ekz>
      <ekz>
        &gcirc;uste &ccirc;ar ni trovas Esperanton io mirinda, mirakla,
        ri&ccirc;iga por la animo kaj la koro <klr>...</klr> &gcirc;uste pro tio
        &scirc;prucas en ni <tld/>a deziro diskonigi &gcirc;in, kiel
        trezoron
        <fnt>
        <aut>Claude Piron</aut>: <vrk>Kien esperanto?</vrk>
        <lok>Prelego kasede prezentita okaze de kunveno en
        Cully, Svislando, en septembro 1994.</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="klar.0a">klara</ref>,
      <ref cel="bril.0a">brila</ref>,
      <ref cel="dens.0a">densa</ref>,
      <ref cel="fort.0a.intensa">forta<sncref/></ref>
    </refgrp>
    <trd lng="hu">er&odblac;s</trd>
    <trdgrp lng="ru">
      <trd>&c_ja;&c_r;&c_k;&c_i;&c_j;<klr tip="amb"> (&c_o; 
&c_s;&c_v;&c_je;&c_t;&c_je;, &c_c;&c_v;&c_je;&c_t;&c_je;, 
&c_p;&c_je;&c_r;&c_je;&c_zh;&c_i;&c_v;&c_a;&c_n;&c_i;&c_i;)</klr></trd>,
      <trd>&c_n;&c_a;&c_s;&c_y;&c_shch;&c_je;&c_n;&c_n;&c_y;&c_j;<klr tip="amb"> 
(&c_o; &c_c;&c_v;&c_je;&c_t;&c_je;)</klr></trd>
      <trd>&c_s;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="intens.0a.rendimenta">
    <dif>
      Rezultiganta grandan <ref cel="rendiment.0o">rendimenton</ref>,
      <ref cel="efik.0a">efika</ref>:
      <ekz>
        <tld/>a terkulturado.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>intenz&iacute;v</trd>,
      <trd>belterjes</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_n;&c_t;&c_e;&c_n;&c_s;&c_y;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_r;&c_u;&c_zh;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_m;&c_o;&c_c;&c_n;&c_y;</trd>,
    <trd>&c_ja;&c_r;&c_k;&c_ib;</trd>,
    <trd>&c_z;&c_y;&c_r;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>intenzivn&iacute;</trd>,
    <trd>prudk&yacute;</trd>,
    <trd>siln&yacute;</trd>
  </trdgrp>
  <trd lng="de">intensiv</trd>
  <trd lng="es">intenso</trd>
  <trd lng="fa">&a_sh;&a_d;&f_y;&a_d;</trd>
  <trdgrp lng="fr">
    <trd>intense</trd>,
    <trd>intensif</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5F37;&#x70C8;&#x306A;  [<ind>&#x304D;&#x3087;&#x3046;&#x308C;&#x3064;&#x306A;</ind>]</trd>,
    <trd>&#x6FC0;&#x3057;&#x3044;  [<ind>&#x306F;&#x3052;&#x3057;&#x3044;</ind>]</trd>,
    <trd>&#x5F37;&#x5EA6;&#x306E;  [<ind>&#x304D;&#x3087;&#x3046;&#x3069;&#x306E;</ind>]</trd>,
    <trd>&#x96C6;&#x4E2D;&#x7684;&#x306A;  [<ind>&#x3057;&#x3085;&#x3046;&#x3061;&#x3085;&#x3046;&#x3066;&#x304D;&#x306A;</ind>]</trd>,
    <trd>&#x96C6;&#x7D04;&#x7684;&#x306A;  [<ind>&#x3057;&#x3085;&#x3046;&#x3084;&#x304F;&#x3066;&#x304D;&#x306A;</ind>]</trd>,
    <trd>&#x5FB9;&#x5E95;&#x7684;&#x306A;  [<ind>&#x3066;&#x3063;&#x3066;&#x3044;&#x3066;&#x304D;&#x306A;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>intens</trd>,
    <trd>sterk</trd>
  </trdgrp>
  <trd lng="pt">intenso<klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_t;&c_je;&c_n;&c_s;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_r;&c_ja;&c_zh;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_u;&c_s;&c_i;&c_l;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>intenz&iacute;vny</trd>,
    <trd>prenikav&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x456;&#x43D;&#x442;&#x435;&#x43D;&#x441;&#x438;&#x432;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x43D;&#x430;&#x43F;&#x440;&#x443;&#x436;&#x435;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x43F;&#x43E;&#x441;&#x438;&#x43B;&#x435;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="intens.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Alta grado de aktiveco:
      <ekz>
        <tld/>eco de varmo, lumo;
      </ekz>
      <ekz>
        elektra <tld/>eco;
      </ekz>
      <ekz>
        la <tld/>eco de la febro malalti&gcirc;is.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_n;&c_t;&c_e;&c_n;&c_s;&c_y;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_r;&c_u;&c_zh;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">intenzita</trd>
  <trd lng="de">Intensit&auml;t</trd>
  <trd lng="es">intensidad</trd>
  <trd lng="fa">&a_sh;&a_d;&a_t;</trd>
  <trd lng="fr">intensit&eacute;</trd>
  <trdgrp lng="hu">
    <trd>intenzt&aacute;s</trd>,
    <trd>er&odblac;ss&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6FC0;&#x3057;&#x3055;  [<ind>&#x306F;&#x3052;&#x3057;&#x3055;</ind>]</trd>,
    <trd>&#x5F37;&#x3055;  [<ind>&#x3064;&#x3088;&#x3055;</ind>]</trd>,
    <trd>&#x5F37;&#x70C8;  [<ind>&#x304D;&#x3087;&#x3046;&#x308C;&#x3064;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">intensiteit</trd>
  <trd lng="pt">intensidade</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_t;&c_je;&c_n;&c_s;&c_i;&c_v;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_r;&c_ja;&c_zh;&c_jo;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_ja;&c_r;&c_k;&c_o;&c_s;&c_t;&c_mol;<klr tip="amb"> (&c_o; 
&c_c;&c_v;&c_je;&c_t;&c_je;)</klr></trd>,
    <trd>&c_n;&c_a;&c_s;&c_y;&c_shch;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;<klr tip="amb"> (&c_o; &c_c;&c_v;&c_je;&c_t;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="sk">intenzita</trd>
  <trdgrp lng="uk">
    <trd>&#x456;&#x43D;&#x442;&#x435;&#x43D;&#x441;&#x438;&#x432;&#x43D;&#x456;&#x441;&#x442;&#x44C;</trd>,
    <trd>&#x43D;&#x430;&#x43F;&#x440;&#x443;&#x436;&#x435;&#x43D;&#x456;&#x441;&#x442;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="intens.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Pliigi la aktivecon; plifortigi la efikon:
      <ekz>
        estas necese <tld/>igi la laboron por fini la konstruon en
        la difinita templimo;
      </ekz>
      <ekz>
        <tld/>igi la lumon, la elektran fluon, la propagandon,
        la protestojn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akcel.0i">akceli</ref>,
      <ref cel="insist.0i">insisti</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_n;&c_t;&c_e;&c_n;&c_s;&c_y;&c_f;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_k;&c_a;&c_r;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_z;&c_m;&c_a;&c_c;&c_n;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">intenzifikovat</trd>
  <trd lng="de">intensivieren</trd>
  <trd lng="es">intensificar</trd>
  <trdgrp lng="fa">
    <trd>&a_sh;&a_d;&a_t; &a_b;&a_kh;&a_sh;&f_y;&a_d;&a_n;</trd>,
    <trd>&a_t;&a_sh;&a_d;&f_y;&a_d; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>intensifier</trd>,
    <trd>amplifier</trd>,
    <trd>renforcer<klr tip="ind"> (intensifier)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>intenz&iacute;vebb&eacute; tesz</trd>,
    <trd>feler&odblac;s&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5F37;&#x3081;&#x308B;  [<ind>&#x3064;&#x3088;&#x3081;&#x308B;</ind>]</trd>,
    <trd>&#x6FC0;&#x3057;&#x304F;&#x3059;&#x308B;  [<ind>&#x306F;&#x3052;&#x3057;&#x304F;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verhogen</trd>,
    <trd>versterken</trd>
  </trdgrp>
  <trd lng="pt">intensificar</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_t;&c_je;&c_n;&c_s;&c_i;&c_f;&c_i;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_s;&c_i;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_s;&c_i;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>stup&ncaron;ova&tcaron;</trd>,
    <trd>zintenz&iacute;vni&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x456;&#x43D;&#x442;&#x435;&#x43D;&#x441;&#x438;&#x444;&#x456;&#x43A;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x456;&#x434;&#x441;&#x438;&#x43B;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>
