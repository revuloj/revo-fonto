<drv mrk="intens.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Pliigi la aktivecon; plifortigi la efikon:
      <ekz>
        &scirc;ia informo <klr>[&#x2026;]</klr> <tld/>igis la malbonhumoron
        <fnt>
          <bib>ChV</bib>
          <lok>19</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igo kaj vastigo de internacia kunlaboro
        <fnt>
          <bib>EeP</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la nova situacio <tld/>igos la konkurencon
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2003/008044.html"
              >Internaciaj bankoj stabiligas siajn poziciojn</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Estraro de UEA signife <tld/>igu sian apogon al la projekto Interkulturo
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#x2116;10</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akcel.0i">akceli</ref>,
      <ref cel="insist.0i">insisti</ref>,
      <ref cel="fokus.0i">fokusi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_n;&c_t;&c_e;&c_n;&c_s;&c_y;&c_f;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_k;&c_a;&c_r;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_z;&c_m;&c_a;&c_c;&c_n;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">intenzifikovat</trd>
  <trd lng="de">intensivieren</trd>
  <trd lng="en">intensify</trd>
  <trd lng="es">intensificar</trd>
  <trdgrp lng="fa">
    <trd>&a_sh;&a_d;&a_t; &a_b;&a_kh;&a_sh;&f_y;&a_d;&a_n;</trd>,
    <trd>&a_t;&a_sh;&a_d;&f_y;&a_d; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>intensifier</trd>,
    <trd>amplifier</trd>,
    <trd>renforcer<klr tip="ind"> (intensifier)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>intenz&iacute;vebb&eacute; tesz</trd>,
    <trd>feler&odblac;s&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5F37;&#x3081;&#x308B;  <pr>&#x3064;&#x3088;&#x3081;&#x308B;</pr></trd>,
    <trd>&#x6FC0;&#x3057;&#x304F;&#x3059;&#x308B;  <pr>&#x306F;&#x3052;&#x3057;&#x304F;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verhogen</trd>,
    <trd>versterken</trd>
  </trdgrp>
  <trd lng="pt">intensificar</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_t;&c_je;&c_n;&c_s;&c_i;&c_f;&c_i;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_s;&c_i;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_s;&c_i;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>stup&ncaron;ova&tcaron;</trd>,
    <trd>zintenz&iacute;vni&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ib;&c_n;&c_t;&c_je;&c_n;&c_s;&c_i;&c_f;&c_ib;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_s;&c_i;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5F37;&#x5316; <pr>qi&aacute;nghu&agrave;</pr></trd>,
    <trd>&#x52A0;&#x7D27; <pr>ji&amacron;j&#x1D0;n</pr></trd>,
    <trd>&#x4F7F;&#x7D27;&#x5F20; <pr>sh&#x1D0;j&#x1D0;nzh&amacron;ng</pr></trd>,
    <trd>&#x4F7F;&#x7D27;&#x51D1; <pr>sh&#x1D0;j&#x1D0;nc&ograve;u</pr></trd>,
    <trd>&#x6FC0;&#x5316; <pr>j&imacron;hu&agrave;</pr></trd>
  </trdgrp>
