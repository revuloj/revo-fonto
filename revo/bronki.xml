<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bronki.xml,v 1.26 2023/10/14 08:39:48 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>bronkit</rad>/o
</kap>
<drv mrk="bronki.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Bronka <ref tip="lst" cel="inflam.0o" lst="voko:malsanoj">inflamo</ref>:
      <ekz>
        la infano havas brulumon de la spira kanalo, tiel nomatan <tld/>on
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        koronvirusoj estas grandaj RNA-enhavantaj virusoj, ili <klr>[&#x2026;]</klr> ka&ubreve;zas
        <tld/>on, hepatiton, gastroenteriton, encefaliton ktp
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2005/008721.php">De kie venis kaj kial &ccirc;esis netipa pne&ubreve;monio?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en la stadio de aidoso, kandidozaj &scirc;an&gcirc;oj jam portas disvastigitan karakteron
        (kandidoza ezofagito, <tld/>o, sepso kaj aliaj)
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2007/008991.php">La &ccirc;efaj murdistoj de HIV-infektitoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="varm.mal0umo">malvarmumo</ref>
    <refgrp tip="vid">
       <ref cel="tus.0i">tusi</ref>,
       <ref cel="katar.0o">kataro</ref>
    </refgrp>
    <trd lng="be">&c_b;&c_r;&c_a;&c_n;&c_h;&c_ib;&c_t;</trd>
    <trd lng="en">bronchitis</trd>
    <trd lng="es">bronquitis</trd>
    <trd lng="fa">&a_b;&a_r;&a_n;&a_sh;&f_y;&a_t;</trd>
    <trd lng="fr">bronchite</trd>
    <trdgrp lng="hu">
      <trd>houml;rghurut</trd>,
      <trd>bronchitis</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>bronkitis</trd>,
      <trd><ind>radang</ind> cabang tenggorok</trd>
    </trdgrp>
    <trd lng="it">bronchite</trd>
    <trd lng="nl">bronchitis</trd>
    <trdgrp lng="pl">
      <trd>bronchit</trd>,
      <trd>zapalenie <ind>oskrzeli</ind></trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_r;&c_o;&c_n;&c_h;&c_i;&c_t;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>bronchitida</trd>,
    <trd>z&aacute;n&ecaron;t pr&uring;du&scaron;ek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bronchitis</trd>,
    <trd>Husten <klr>(Bronchitis)</klr></trd>
  </trdgrp>
  <trd lng="he">&#x5D1;&#x5BC;&#x5B0;&#x5E8;&#x5D5;&#x5B9;&#x5E0;&#x5DB;&#x5B4;&#x5D9;&#x5D8;&#x5B4;&#x5D9;&#x5E1;</trd>
  <trd lng="ja">&#x6C17;&#x7BA1;&#x652F;&#x708E;  <pr>&#x304D;&#x304B;&#x3093;&#x3057;&#x3048;&#x3093;</pr></trd>
  <trd lng="sk">z&aacute;pal priedu&scaron;iek</trd>
  <trd lng="uk">&#x431;&#x440;&#x43E;&#x43D;&#x445;&#x456;&#x442;</trd>
</drv>
</art>
