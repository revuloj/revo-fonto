<drv mrk="flegm.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="trankv.0o.anima">Trankvilo</ref>,
      sinrega senpasieco evitanta koleron, timon ktp:
      <ekz>
        diskuti kun <tld/>o;
      </ekz>
      <ekz>
        <nom>Jacques Vach&eacute;</nom> <klr>[&#x2026;]</klr> estis konata pro sia <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=148630">Jacques Vach&eacute;</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="apati.0o">apatio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_f;&c_l;&c_je;&c_g;&c_m;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>flegma</trd>,
    <trd>calma</trd>,
    <trd>assossec</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Phlegma</trd>,
    <trd>Gleichg&uuml;ltigkeit</trd>,
    <trd>Tr&auml;gheit</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>apathy</trd>,
    <trd>stolidity</trd>,
    <trd>calmness</trd>,
    <trd>composure</trd>
    <trd>phlegm</trd>,
  </trdgrp>
  <trdgrp lng="fr">
    <trd>flegme</trd>,
    <trd>placidit&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>flegma</trd>,
    <trd>hidegv&eacute;r</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7121;&#x611F;&#x52D5;  <pr>&#x3080;&#x304B;&#x3093;&#x3069;&#x3046;</pr></trd>,
    <trd>&#x51B7;&#x6DE1;  <pr>&#x308C;&#x3044;&#x305F;&#x3093;</pr></trd>,
    <trd>&#x51B7;&#x9759;  <pr>&#x308C;&#x3044;&#x305B;&#x3044;</pr></trd>,
    <trd>&#x6C88;&#x7740;  <pr>&#x3061;&#x3093;&#x3061;&#x3083;&#x304F;</pr></trd>,
    <trd>&#x7C98;&#x6DB2;  <pr>&#x306D;&#x3093;&#x3048;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="nl">flegma</trd>
  <trdgrp lng="pl">
    <trd>flegma</trd>,
    <trd>powolno&sacute;&cacute; charakteru</trd>,
    <trd>spok&oacute;j</trd>,
    <trd>zimna krew <klr>(pot.)</klr></trd>
  </trdgrp>
<trdgrp lng="pt">
<trd>fleuma</trd>,
<trd>calma</trd>,
<trd>frieza</trd>,
<trd>impassibilidade</trd>
</trdgrp>
  <trd lng="ru">&c_f;&c_l;&c_je;&c_g;&c_m;&c_a;</trd>
  <trdgrp lng="uk">
    <trd>&c_f;&#x43B;&c_je;&c_g;&#x43C;&c_a;</trd>,
    <trd>&#x43D;&c_je;&c_z;&c_v;&#x43E;&c_r;&c_u;&c_sh;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&c_b;&c_a;&c_j;&c_d;&c_u;&c_zh;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&c_h;&#x43E;&#x43B;&#x43E;&c_d;&#x43D;&#x43E;&#x43A;&c_r;&#x43E;&c_v;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>
  </trdgrp>
  <trd lng="zh">&#x75F0; <pr>t&#xE1;n</pr></trd>
</drv>

<drv mrk="flegm.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Restanta senpasia, ne facile incitebla:
      <ekz>
        <tld/>a tono, karaktero;
      </ekz>
      <ekz>
        ne estu anka&ubreve; tro <tld/>a, sed lasu vian propran senton esti via gvidanto:
        konformigu la gestojn al la vortoj kaj la vortojn al la gestoj
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">PSI</uzo>
        <tld/>a <ref tip="super" cel="temper.0o">temperamento</ref>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_l;&c_je;&c_g;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="ca">
    <trd>flegm&agrave;tic</trd>,
    <trd>calmat</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>flegmatick&yacute;</trd>,
    <trd>lhostejn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>phlegmatisch</trd>,
    <trd>gleichg&uuml;ltig</trd>,
    <trd>tr&auml;ge</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>phlegmatic</trd>,
    <trd>impassive</trd>,
    <trd>stolid</trd>,
    <trd>calm</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>flegmatique</trd>,
    <trd>placide</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>flegmatikus</trd>,
    <trd>egykedv&udblac;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7121;&#x611F;&#x52D5;&#x306A;  <pr>&#x3080;&#x304B;&#x3093;&#x3069;&#x3046;&#x306A;</pr></trd>,
    <trd>&#x51B7;&#x6DE1;&#x306A;  <pr>&#x308C;&#x3044;&#x305F;&#x3093;&#x306A;</pr></trd>,
    <trd>&#x51B7;&#x9759;&#x306A;  <pr>&#x308C;&#x3044;&#x305B;&#x3044;&#x306A;</pr></trd>,
    <trd>&#x843D;&#x3061;&#x7740;&#x304D;&#x306F;&#x3089;&#x3063;&#x305F;  <pr>&#x304A;&#x3061;&#x3064;&#x304D;&#x306F;&#x3089;&#x3063;&#x305F;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>onverstoorbaar</trd>,
    <trd>flegmatisch</trd>,
    <trd>flegmatiek</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>flegmatyczny</trd>,
    <trd>powolny</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_l;&c_je;&c_g;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>chladnokrvn&yacute;</trd>,
    <trd>flegmatick&yacute;</trd>
  </trdgrp>
  <trd lng="uk">&c_f;&#x43B;&c_je;&c_g;&#x43C;&c_a;&c_t;&c_i;&c_ch;&#x43D;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x6F20;&#x7136; <pr>m&#xF2;r&#xE1;n</pr></trd>,
    <trd>&#x758F;&#x61D2; <pr>sh&#x16B;l&#x1CE;n</pr></trd>,
    <trd>&#x6162;&#x8F49; <pr>m&#xE0;nzhu&#x1CE;n</pr></trd>,
    <trd>&#x6020;&#x60F0; <pr>d&#xE0;idu&#xF2;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="flegm.0e">
  <kap>
    <tld/>e
  </kap>
  <snc>
    <dif>
      Senpasie:
      <ekz>
        kaj se <nom>Hiram</nom> havas por via nobleco bonan aferon? demandis <tld/>e la gasto
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ministrestro <tld/>e kaj hezite komencis formi novan ministraron
        <fnt>
          <bib>Monato</bib>
          <aut>Karlo Juh&aacute;sz</aut>
          <vrk><url ref="&Monato;/2004/008466.html">Nova viza&gcirc;o, novaj &scirc;ancoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_l;&c_je;&c_g;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_a;</trd>
  <trdgrp lng="de">
    <trd>phlegmatisch</trd>,
    <trd>gleichg&uuml;ltig</trd>,
    <trd>tr&auml;ge</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7121;&#x611F;&#x52D5;&#x306B;  <pr>&#x3080;&#x304B;&#x3093;&#x3069;&#x3046;&#x306B;</pr></trd>,
    <trd>&#x51B7;&#x6DE1;&#x306B;  <pr>&#x308C;&#x3044;&#x305F;&#x3093;&#x306B;</pr></trd>,
    <trd>&#x51B7;&#x9759;&#x306B;  <pr>&#x308C;&#x3044;&#x305B;&#x3044;&#x306B;</pr></trd>,
    <trd>&#x843D;&#x3061;&#x7740;&#x304D;&#x306F;&#x3089;&#x3063;&#x3066;  <pr>&#x304A;&#x3061;&#x3064;&#x304D;&#x306F;&#x3089;&#x3063;&#x3066;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="flegm.0i">
  <kap>
    <tld/>i
  </kap>
  <snc>
    <dif>
      Konduti sen montri pasion:
      <ekz>
        mi <tld/>as, aplikante &ccirc;iujn stereotipojn &ndash; malvarman sangon, superan lipon
        rigidan
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2011/010541.php">Grincantaj genuoj ... kaj &gcirc;entileco</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_l;&c_je;&c_g;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_a; &c_p;&c_a;&c_v;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol; &c_s;&c_ja;&c_b;&c_je;</trd>
</drv>

<drv mrk="flegm.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Homo havanta la <tld/>an <ref cel="temper.0o">temperamenton</ref>:
      <ekz>
        kaj mi pensas, ke oftaj interrilatoj kun velka <tld/>ulo
        farus min pasia revulo
        <fnt><bib>Mary</bib></fnt>;
      </ekz>
      <ekz>
        alivorte &leftquot;senerekti&gcirc;ulo&#x201D; a&ubreve; &leftquot;amor<tld/>ulo&#x201D;, kiu la&ubreve; la opinio de la
        insultanto
        ne kapablas sufi&ccirc;e feli&ccirc;igi sian edzinon
        <fnt>
          <bib>Monato</bib>
          <aut>Pejno Simono</aut>
          <vrk><url ref="&Monato;/2013/011040.php">Toter Lehrer, guter Lehrer</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_l;&c_je;&c_g;&c_m;&c_a;&c_t;&c_y;&c_k;</trd>
  <trd lng="ca">flegm&agrave;tic<klr> (psic.)</klr></trd>
  <trd lng="cs">flegmatik</trd>
  <trd lng="en">phlegmatic</trd>
  <trdgrp lng="ja">
    <trd>&#x7121;&#x611F;&#x52D5;&#x306A;&#x4EBA;  <pr>&#x3080;&#x304B;&#x3093;&#x3069;&#x3046;&#x306A;&#x3058;&#x3093;</pr></trd>,
    <trd>&#x6C88;&#x7740;&#x51B7;&#x9759;&#x306A;&#x4EBA;  <pr>&#x3061;&#x3093;&#x3061;&#x3083;&#x304F;&#x308C;&#x3044;&#x305B;&#x3044;&#x306A;&#x3058;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">flegmaticus</trd>
  <trd lng="pl">flegmatyk</trd>
  <trd lng="ru">&c_f;&c_l;&c_je;&c_g;&c_m;&c_a;&c_t;&c_i;&c_k;</trd>
  <trd lng="sk">flegmatik</trd>
  <trd lng="uk">&c_f;&#x43B;&c_je;&c_g;&#x43C;&c_a;&c_t;&c_i;&#x43A;</trd>
  <trd lng="zh">&#x6162;&#x6027;&#x5B50; <pr>m&#xE0;nx&#xEC;ngzi</pr></trd>
</drv>

