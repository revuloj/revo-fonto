<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>flegm</rad>/o
</kap>
<drv mrk="flegm.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Malvarmsango, trankvila sinreganta  senpasieco, neemociemo:
      <ekz>
        diskuti kun <tld/>o;
      </ekz>
      <ekz>
        <ind><tld/>a</ind> tono, karaktero.
         <trdgrp lng="de">
            <trd>phlegmatisch</trd>,
            <trd>gleichg&uuml;ltig</trd>,
            <trd>tr&auml;ge</trd></trdgrp>
         <trdgrp lng="fr">
           <trd>flegmatique</trd>,
           <trd>placide</trd></trdgrp> 
         <trd lng="ru">&c_f;&c_l;&c_je;&c_g;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd> homo<fnt>B</fnt>,
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="apati.0o">apatio</ref>,
      <ref cel="indife">indiferenteco</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Phlegma</trd>,
     <trd>Gleichg&uuml;ltigkeit</trd>,
     <trd>Tr&auml;gheit</trd></trdgrp>
  <trdgrp lng="fr">
    <trd>flegme</trd>,
    <trd>placidit&eacute;</trd></trdgrp> 
  <trd lng="ru">&c_f;&c_l;&c_je;&c_g;&c_m;&c_a;</trd>
</drv>
</art>
