<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>flegm</rad>/o
</kap>
<drv mrk="flegm.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Malvarmsango, trankvila sinreganta  senpasieco, neemociemo:
      <ekz>
        diskuti kun <tld/>o;
      </ekz>
      <ekz>
        <ind><tld/>a</ind><trd lng="ru">&c_f;&c_l;&c_je;&c_g;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd> homo<fnt>B</fnt>,
      </ekz>
      <ekz>
        tono, karaktero.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="apati.0o">apatio</ref>,
      <ref cel="indife">indiferenteco</ref>.
    </refgrp>
  </snc>
  <trd lng="ru">&c_f;&c_l;&c_je;&c_g;&c_m;&c_a;</trd>
</drv>
</art>
