<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>flegm</rad>/o
    </kap>

    <drv mrk="flegm.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc>
        <dif>
          <ref tip="super" cel="trankv.0o.anima">Trankvilo</ref>, sinrega
          senpasieco evitanta koleron, timon...:
          <ekz>
            diskuti kun <tld/>o.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="apati.0o">apatio</ref>
        </refgrp>
      </snc>
      <trd lng="be">&c_f;&c_l;&c_je;&c_g;&c_m;&c_a;</trd>
      <trdgrp lng="de">
        <trd>Phlegma</trd>,
        <trd>Gleichg&uuml;ltigkeit</trd>,
        <trd>Tr&auml;gheit</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>flegme</trd>,
        <trd>placidit&eacute;</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>flegma</trd>,
        <trd>hidegv&eacute;r</trd>
      </trdgrp>
      <trd lng="nl">flegma</trd>
  <trdgrp lng="pt">
    <trd>fleuma</trd>,
    <trd>calma</trd>,
    <trd>frieza</trd>,
    <trd>impassibilidade</trd>
  </trdgrp>
      <trd lng="ru">&c_f;&c_l;&c_je;&c_g;&c_m;&c_a;</trd>
    </drv>

    <drv mrk="flegm.0a">
      <kap><tld/>a</kap>
      <snc>
        <dif>
          Restanta senpasia, ne facile incitebla:
          <ekz>
            <tld/>a tono, karaktero;
          </ekz><ekz>
            <uzo>PSI</uzo>
            <tld/>a <ref tip="super" cel="temper.0o">temperamento</ref>;
          </ekz><ekz>
            <tld/>a homo<fnt>B</fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_f;&c_l;&c_je;&c_g;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
      <trdgrp lng="de">
        <trd>phlegmatisch</trd>,
        <trd>gleichg&uuml;ltig</trd>,
        <trd>tr&auml;ge</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>flegmatique</trd>,
        <trd>placide</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>flegmatikus</trd>,
        <trd>egykedv&udblac;</trd>
      </trdgrp>
      <trd lng="nl">flegmatisch</trd>
      <trd lng="ru">&c_f;&c_l;&c_je;&c_g;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
    </drv>

    <drv mrk="flegm.0ulo">
      <kap><tld/>ulo</kap>
      <snc>
        <dif>
          Homo havanta la <tld/>an <ref cel="temper.0o">temperamenton</ref>:
          <ekz>
            kaj mi pensas, ke oftaj interrilatoj kun velka <tld/>ulo
            farus min pasia revulo
            <fnt><bib>Mary</bib></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="pl">flegmatyk</trd>
      <trd lng="ru">&c_f;&c_l;&c_je;&c_g;&c_m;&c_a;&c_t;&c_i;&c_k;</trd>
    </drv>

  </art>
