<drv mrk="mastr.0umi">
  <kap><ofc>*</ofc><tld/>umi <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="&AdE;/fundamento/universala_vortaro.html#av-5105">mastr'um'</url></lok></fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="mastr.0umi.KOMUNE">
    <dif>
      Zorgi pri &ccirc;io, kio koncernas la ekonomion de hejmo:
      <ekz>
        &scirc;i <tld/>umis kaj zorgis pri la ordo en la domo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>umis en la princa domo, kvaza&ubreve; en sia
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi, tute ne sciante <tld/>umi, tiel malbonigis &ccirc;iujn miajn 
        aferojn, ke fine mia bieno pro &scirc;uldoj estis vendita
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
	<tld/>uma progreso de la kapitalismo nenion donas al la
	laboristaro;
      </ekz>
      <ekz>
	vila&gcirc;<tld/>uma kolektivo.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="sistem.mastrum0o"><tld/>uma sistemo (de komputilo)</ref>
  <trd lng="en">keep house</trd>
  <trd lng="be">&c_g;&c_a;&c_s;&c_p;&c_a;&c_d;&c_a;&c_r;&c_y;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>hospoda&rcaron;it</trd>,
    <trd>obhospoda&rcaron;ovat</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>haushalten</trd>,
      <trd>wirtschaften</trd>
    </trdgrp>
  <trd lng="fr">tenir<klr> (administrer)</klr></trd>
  <trd lng="nl">huishouden</trd>
  <trdgrp lng="pl">
    <trd>zarz&aogonek;dza&cacute;</trd>,
    <trd>gospodarzy&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
      <trd>&c_h;&c_o;&c_z;&c_ja;&c_j;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_h;&c_o;&c_z;&c_ja;&c_j;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_je;&c_s;&c_t;&c_i; &c_h;&c_o;&c_z;&c_ja;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>
    </trdgrp>
  <trdgrp lng="sk">
    <trd>gazdova&tcaron;</trd>,
    <trd>hospod&aacute;ri&tcaron;</trd>,
    <trd>vies&tcaron; dom&aacute;cnos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
