<drv mrk="mastr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="mastr.0i.KOMUNE">
    <dif>
      Esti <tld/>o super, agi kiel <tld/>o de; regi, administri:
      <ekz>
        nia nova grafikisto <klr>[&#x2026;]</klr> mirinde rapide ek<tld/>is la
        novan taskon
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2005/008662.php">Akurateco</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en tiuj bone strukturitaj landoj oni relative bone <tld/>is la
        problemojn, anka&ubreve; ne mankis helpiloj kaj helpantoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2010/010139.php">&Ccirc;u
          kulpas nur la naturo?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        iun tagon, kiam li jam bone <tld/>is la uzadon de la ma&scirc;ineto,
        li vizitis min por danki min pro mia helpo
        <fnt>
          <bib>Monato</bib>
          <aut>G. O. &Ograve;sh&oacute;-Davies</aut>
          <vrk><url ref="&Monato;/2012/010421.php">Po&scirc;telefono</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">be master over</trd>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_l;&c_o;&c_d;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol; &c_g;&c_a;&c_s;&c_p;&c_a;&c_d;&c_a;&c_r;&c_o;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>beza&ntilde; <ind>mestr</ind> (war)</trd>,
    <trd>mestronia&ntilde;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ovl&aacute;dat</trd>,
    <trd>&rcaron;&iacute;dit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>leiten</trd>,
    <trd>f&uuml;hren</trd>,
    <trd>verwalten</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>ser amo</trd>,
    <trd>ser capataz</trd>
  </trdgrp>
  <trd lng="fr">diriger<klr> (administrer)</klr></trd>
  <trd lng="nl">heersen</trd>
  <trdgrp lng="pl">
    <trd>szefowa&cacute;</trd>,
    <trd>rz&aogonek;dzi&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_l;&c_a;&c_d;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_l;&c_a;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol; &c_h;&c_o;&c_z;&c_ja;&c_i;&c_n;&c_o;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ovl&aacute;da&tcaron;</trd>,
    <trd>riadi&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
