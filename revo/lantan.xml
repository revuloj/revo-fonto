<kap>
  <rad>lantan</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="lantan.0o">
  <kap>
    <tld/>o
  </kap>
  <mlg kod="IUPAC">La</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="57">elemento</ref> kun
      atomnumero 57, atompezo 138,91;
      malofta <ref tip="super" cel="metal.0o">metalo</ref>:
      <ekz>
        <tld/>o estas venena <klr>[&#x2026;]</klr>, mola ar&gcirc;ent-kolora metalo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=28650">Lantano</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_n;&c_t;&c_a;&c_n;</trd>
  <trd lng="bg">&c_l;&c_a;&c_n;&c_t;&c_a;&c_n;</trd>
  <trd lng="br">lantan</trd>
  <trd lng="de">Lanthan</trd>
  <trd lng="en">lanthanum</trd>
  <trd lng="es">lantano</trd>
  <trd lng="fr">lanthane</trd>
  <trd lng="hu">lant&aacute;n</trd>
  <trd lng="id">lantanum</trd>
  <trd lng="it">lantanio</trd>
  <trd lng="ja">&#x30E9;&#x30F3;&#x30BF;&#x30F3;</trd>
  <trd lng="la">Lanthanum</trd>
  <trd lng="nl">lanthaan</trd>
  <trd lng="pl">lantan</trd>
  <trd lng="pt">lant&acirc;nio</trd>
  <trd lng="ru">&c_l;&c_a;&c_n;&c_t;&c_a;&c_n;</trd>
  <trd lng="sv">lantan</trd>
  <trd lng="uk">&#x43B;&#x430;&#x43D;&#x442;&#x430;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x946D; <pr>l&#xE1;n</pr></trd>,
    <trd>&#x9567; <pr>l&#xE1;n</pr></trd>
