<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ornitorink</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ornito1.0o">
  <kap><tld/>o</kap>
  <snc mrk="ornito1.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="mam.pra0ulo"
      lst="voko:pramamuloj">Pramamulo</ref>
      kun kastorsimila korpoformo,
      kun plata vosto kaj lar&gcirc;a <ref tip="prt"
      cel="bek.0o">beko</ref>simila 
      bu&scirc;o; metanta ovojn kaj vivanta en orienta <ref tip="malprt"
      cel="auxstr1.0o">A&ubreve;stralio</ref> 
      kaj <ref tip="malprt" cel="tasman.0o">Tasmanio</ref>; sola vivanta
      <ref tip="lst" cel="speci.0o.BIO" 
      lst="voko:zoologiaj_specioj">specio</ref> kaj 
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> 
      de familio
      <ref tip="malprt" cel="ornito2.0oj">ornitorinkedoj</ref>
      (<trd lng="la">Ornithorhynchus anatinus</trd>):
      <ekz>
        la stranga A&ubreve;stralia <tld/>o demetas 
        <ref tip="prt" cel="ov.0o">ovojn</ref>
        kaj poste mamnutras la idojn
        <fnt>
        <aut>R. Harry</aut>,
        <vrk>La Diplomato kiu Ridis</vrk>,
        <lok>1997</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_ch;&c_k;&c_a;&c_n;&c_o;&c_s;</trd>
  <trd lng="bg">&c_p;&c_t;&c_i;&c_c;&c_je;&c_ch;&c_o;&c_v;&c_k;&c_a;</trd>
  <trd lng="cs">ptakopysk</trd>
  <trd lng="de">Schnabeltier</trd>
  <trd lng="el">&omikron;&rho;&ny;&jota;&theta;&omikron_ton;&rho;&ypsilon;&gamma;&chi;&omikron;&sigma_fina;</trd>
  <trd lng="fr">ornithorynque</trd>
  <trd lng="nl">vogelbekdier</trd>
  <trd lng="hu">cs&odblac;r&ouml;s <ind>eml&odblac;s</ind></trd>
  <trd lng="pt">ornitorrinco</trd>
  <trd lng="ru">&c_u;&c_t;&c_k;&c_o;&c_n;&c_o;&c_s;</trd>
</drv>
</art>
