<art mrk="$Id: ekstra.xml,v 1.27 2023/11/07 19:22:36 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>ekstrakt</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ekstra.0o">
  <kap><tld/>o</kap>
  <snc mrk="ekstra.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Elemento eltirita el malsimpla <ref tip="super" cel="substa.0o.FIZ">substanco</ref>:
      <ekz>
        el la grasa&jcirc;o fari&gcirc;as sapo kaj nutra&jcirc;-<tld/>o
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        mirtel<tld/>o al Japanio, kie &gcirc;i estas uzata por plibonigi vidkapablon
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2005/008789.php">Laboro kaj la bero</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        drogherbaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Steven K. Smith</aut>
          <vrk><url ref="&Monato;/2009/009659.php">La Helpanto</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="esenc.0o">esenco</ref>,
      <ref cel="tinktu.0o">tinkturo</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_e;&c_k;&c_s;&c_t;&c_r;&c_a;&c_k;&c_t;</trd>,
      <trd>&c_v;&c_y;&c_c;&c_ja;&c_zh;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="bg">&c_je;&c_k;&c_s;&c_t;&c_r;&c_a;&c_k;&c_t;</trd>
    <trdgrp lng="de">
       <trd>Auszug</trd>,
       <trd>Extrakt</trd>
    </trdgrp>
    <trd lng="es">extracto</trd>
    <trd lng="fr">extrait<klr tip="ind"> (substance
    extraite)</klr></trd>
    <trd lng="nl">extract</trd>
    <trdgrp lng="pl">
      <trd>ekstrakt</trd>,
      <trd>wyci&aogonek;g</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_e;&c_k;&c_s;&c_t;&c_r;&c_a;&c_k;&c_t;</trd>,
      <trd>&c_v;&c_y;&c_t;&c_ja;&c_zh;&c_k;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x62BD;&#x51FA;&#x7269; <pr>ch&#x14D;uch&#x16B;w&#xF9;</pr></trd>,
    <trd>&#x63D0;&#x53D6;&#x7269; <pr>t&#xED;q&#x1D4;w&#xF9;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="ekstra.0o.TIP">
    <dif>
      <ref tip="super" cel="tir.el0ajxo">Eltira&jcirc;o</ref> el libro, verko:
      <ekz>
        samjare aperis <tld/>oj el verkoj de <nom>Degu&ccirc;i</nom>
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        diapozitivoj kaj muzikaj <tld/>oj kompletigis tiun &ccirc;i parton de la ampleksa
        programo
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        bendo kun <tld/>o de bonlingvismo
        <fnt>
          <bib>Kon11</bib>
        </fnt>;
      </ekz>
      <ekz>
        jen estas <tld/>oj el mia taglibro
        <fnt>
          <bib>Monato</bib>
          <aut>W. H. Simcock</aut>
          <vrk><url ref="&Monato;/2003/008033.html">Esti homa kaj literaturema</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fragme.0o.verkoparto">fragmento</ref>,
      <ref cel="pec.0o.verka">peco<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_t;&c_r;&c_y;&c_m;&c_k;&c_a;</trd>,
      <trd>&c_v;&c_y;&c_n;&c_ja;&c_t;&c_k;&c_a;</trd>,
      <trd>&c_u;&c_r;&c_y;&c_v;&c_a;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="bg">
      <trd>&c_i;&c_z;&c_v;&c_a;&c_d;&c_k;&c_a;</trd>,
      <trd>&c_o;&c_t;&c_k;&c_malmol;&c_s; <klr>(fragmento)</klr></trd>
    </trdgrp>
    <trdgrp lng="de">
       <trd>Auszug</trd>,
       <trd>Zusammenfassung</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>extrait<klr tip="ind"> (d'un texte)</klr></trd>,
      <trd>fragment<klr tip="ind"> (d'un texte)</klr></trd>,
      <trd>morceau<klr tip="ind"> (d'un texte)</klr></trd>,
      <trd>passage<klr tip="ind"> (d'un texte)</klr></trd>
    </trdgrp>
    <trd lng="nl">uitreksel</trd>
    <trdgrp lng="pl">
      <trd>wypis</trd>,
      <trd>fragment</trd>,
      <trd>wyb&oacute;r</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_y;&c_d;&c_je;&c_r;&c_zh;&c_k;&c_a;</trd>,
      <trd>&c_i;&c_z;&c_v;&c_l;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_o;&c_t;&c_r;&c_y;&c_v;&c_o;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x7247;&#x6BB5; <pr>pi&#xE0;ndu&#xE0;n</pr></trd>,
    <trd>&#x7BC0;&#x9078; <pr>ji&#xE9;xu&#x1CE;n</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="cs">
    <trd>extrakt</trd>,
    <trd>v&yacute;luh</trd>,
    <trd>v&yacute;pis</trd>,
    <trd>v&yacute;tah</trd>,
    <trd>v&yacute;ta&zcaron;ek</trd>,
    <trd>v&yacute;var</trd>,
    <trd>v&yacute;&ncaron;atek</trd>
  </trdgrp>
  <trd lng="en">extract</trd>
  <trd lng="hu">kivonat</trd>
  <trdgrp lng="ja">
    <trd>&#x62BD;&#x51FA;&#x7269;  <pr>&#x3061;&#x3085;&#x3046;&#x3057;&#x3085;&#x3064;&#x3076;&#x3064;</pr></trd>,
    <trd>&#x30A8;&#x30AD;&#x30B9;</trd>,
    <trd>&#x629C;&#x7C8B;  <pr>&#x3070;&#x3063;&#x3059;&#x3044;</pr></trd>,
    <trd>&#x5F15;&#x7528;&#x6587;  <pr>&#x3044;&#x3093;&#x3088;&#x3046;&#x3076;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>extrakt</trd>,
    <trd>tres&tcaron;</trd>,
    <trd>v&yacute;pis</trd>,
    <trd>v&yacute;&tcaron;a&zcaron;ok</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x438;&#x442;&#x44F;&#x436;&#x43A;&#x430;</trd>,
    <trd>&#x435;&#x43A;&#x441;&#x442;&#x440;&#x430;&#x43A;&#x442;</trd>,
    <trd>&#x432;&#x438;&#x442;&#x44F;&#x433;</trd>,
    <trd>&#x432;&#x438;&#x43F;&#x438;&#x441;&#x43A;&#x430;</trd>,
    <trd>&#x443;&#x440;&#x438;&#x432;&#x43E;&#x43A;</trd>
  </trdgrp>
</drv>

<drv mrk="ekstra.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ekstra.0i.KEM">
    <uzo tip="fak">KEM</uzo><uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="tir.el0i.KOMUNE">Eltiri</ref> specialan elementon,
      substancon el malsimpla substanco:
      <ekz>
        li <tld/>as el la vitrokorpo la substancon, kiu miksite en la man&gcirc;a&jcirc;on de
        senapetituloj, redonas la apetiton
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        necesas ne nur trovi, sed anka&ubreve; prilabori tiujn planta&jcirc;ojn, por <tld/>i la
        efika&jcirc;ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2007/009080.php">Kial la malestimo je la afrika medicino?</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        por <tld/>i pli multe da nafto kaj gaso, oni de post 1947 disrompas &scirc;tonaron per
        akvo kaj granda premo
        <fnt>
          <bib>Monato</bib>
          <aut>Werner Fuss</aut>
          <vrk><url ref="&Monato;/2014/011348.php">Novtipa gasminado: &ccirc;u vere dan&gcirc;era?</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        karbonhidrata biopolimero <tld/>ata de la hinda guar-arbo
        <fnt>
          <bib>Monato</bib>
          <aut>Werner Fuss</aut>
          <vrk><url ref="&Monato;/2014/011348.php">Novtipa gasminado: &ccirc;u vere dan&gcirc;era?</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        el sukerkano oni <tld/>as sukeron, <klr>[&#x2026;]</klr> el olivoj oni <tld/>as
        olivoleon
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 8-9 (106-107)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dekokt.0i">dekokti</ref>,
      <ref cel="distil.0i">distili</ref>,
      <ref cel="infuz.0i">infuzi</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_e;&c_k;&c_s;&c_t;&c_r;&c_a;&c_g;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
      <ind>&c_e;&c_s;&c_t;&c_r;&c_a;&c_k;&c_t;</ind></trd>,
      <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
      <ind>&c_v;&c_y;&c_c;&c_ja;&c_zh;&c_k;&c_u;</ind></trd>
    </trdgrp>
    <trd lng="bg">&c_i;&c_z;&c_v;&c_l;&c_i;&c_ch;&c_a;&c_m;</trd>
    <trd lng="bg">&c_je;&c_k;&c_s;&c_t;&c_r;&c_a;&c_h;&c_i;&c_r;&c_a;&c_m;</trd>
    <trdgrp lng="de">
       <trd>einen Auszug <ind>herstellen</ind></trd>,
       <trd>extrahieren</trd>
    </trdgrp>
    <trd lng="es">extraer</trd>
    <trdgrp lng="fr">
      <trd>extraire<klr tip="ind"> (isoler une substance)</klr></trd>,
      <trd>isoler<klr tip="ind"> (une substance)</klr></trd></trdgrp>
    <trdgrp lng="hu">
      <trd>kivon<klr tip="ind"> (extrah&aacute;l)</klr></trd>,
      <trd>extrah&aacute;l</trd></trdgrp> 
    <trdgrp lng="pl">
      <trd>ekstraktowa&cacute;</trd>,
      <trd>robi&cacute; <ind>wyci&aogonek;g</ind></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_e;&c_k;&c_s;&c_t;&c_r;&c_a;&c_g;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_o;&c_l;&c_u;&c_ch;&c_a;&c_t;&c_mol;
      &c_v;&c_y;&c_t;&c_ja;&c_zh;&c_k;&c_u;</trd>
    </trdgrp>
  </snc>
  <snc mrk="ekstra.0i.TIP">
    <dif>
      <ref tip="super" cel="apart.0igi">Apartigi</ref> pecon el libro a&ubreve; 
      &gcirc;enerale eron el multeco:
      <ekz>
        tiun simplan kodon la leganto <klr>[&#x2026;]</klr> povos <tld/>i el la dialogo &ndash; la
        kodvortoj staras kursive
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        gravas scipovi <tld/>i el &gcirc;i la plej celtrafajn pa&gcirc;ojn
        <fnt>
          <bib>MD02</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cit.0i.ripeti">citi</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
      <ind>&c_v;&c_y;&c_p;&c_ib;&c_s;&c_k;&c_u;</ind></trd>,
      <trd>&c_v;&c_y;&c_p;&c_ib;&c_s;&c_v;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_sh;&c_t;&c_o;&c_s;&c_mol;&c_c;&c_ib;
      &c_a;&c_d;&c_n;&c_je;&c_k;&c_u;&c_l;&c_mol;)</klr></trd>
    </trdgrp>
    <trd lng="bg">&c_je;&c_k;&c_s;&c_t;&c_r;&c_a;&c_h;&c_i;&c_r;&c_a;&c_m;</trd>
    <trdgrp lng="de">
       <trd>einen Auszug <ind>anfertigen</ind></trd>,
       <trd>zusammenfassen</trd>
    </trdgrp>
    <trd lng="fr">extraire<klr tip="ind"> (un passage d'un
    texte)</klr></trd>
    <trdgrp lng="hu">
      <trd>kivonatot k&eacute;sz&iacute;t</trd>,
      <trd>kivon</trd></trdgrp>
    <trdgrp lng="pl">
      <trd>robi&cacute; <ind>wypis</ind><klr> (z tekstu)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol;
    &c_v;&c_y;&c_p;&c_i;&c_s;&c_k;&c_u; <klr>(&c_i;&c_z;
    &c_k;&c_n;&c_i;&c_g;&c_i;)</klr></trd>
  </snc>
  <snc mrk="ekstra.0i.FIG">
    <uzo tip="stl">RAR</uzo>
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="tir.el0i.rezultigi">Eltiri</ref> <ref cel="konsek1.0o">konsekvenco</ref>n el io:
      <ekz>
        logiko de sencoj <tld/>itaj el faktoj
        <fnt>
          <bib>KFl</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_a;&c_s;&c_t;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_c;&c_ja;&c_g;&c_n;&c_u;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_d;&c_a;&c_b;&c_y;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
       <trd>herausziehen</trd>,
       <trd>ableiten</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;gager<klr tip="ind"> (une conclusion)</klr></trd>,
      <trd>d&eacute;duire</trd></trdgrp> 
    <trd lng="hu">levon<klr tip="ind"> (szab&aacute;lyt)</klr></trd>
    <trdgrp lng="pl">
      <trd>wyci&aogonek;ga&cacute;</trd>
    </trdgrp>
    <trd lng="ru">&c_i;&c_z;&c_v;&c_l;&c_je;&c_ch;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>extrahovat</trd>,
    <trd>vypracovat anotaci</trd>,
    <trd>z&iacute;skat v&yacute;ta&zcaron;ek</trd>
  </trdgrp>
  <trd lng="en">extract</trd>
  <trd lng="he">&lamed;&het;&lamed;&ftsadi;</trd>
  <trdgrp lng="ja">
    <trd>&#x62BD;&#x51FA;&#x3059;&#x308B;  <pr>&#x3061;&#x3085;&#x3046;&#x3057;&#x3085;&#x3064;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x629C;&#x7C8B;&#x3059;&#x308B;  <pr>&#x3070;&#x3063;&#x3059;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5F15;&#x7528;&#x3059;&#x308B;  <pr>&#x3044;&#x3093;&#x3088;&#x3046;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">extraheren</trd>
  <trdgrp lng="sk">
    <trd>vybera&tcaron;</trd>,
    <trd>vyh&lcaron;ad&aacute;va&tcaron; inform&aacute;cie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x434;&#x435;&#x440;&#x436;&#x443;&#x432;&#x430;&#x442;&#x438; &#x432;&#x438;&#x442;&#x44F;&#x436;&#x43A;&#x443;</trd>,
    <trd>&#x435;&#x43A;&#x441;&#x442;&#x440;&#x430;&#x43A;&#x442;</trd>,
    <trd>&#x435;&#x43A;&#x441;&#x442;&#x440;&#x430;&#x433;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x440;&#x43E;&#x431;&#x438;&#x442;&#x438; &#x432;&#x438;&#x43F;&#x438;&#x441;&#x43A;&#x438;</trd>,
    <trd>&#x434;&#x43E;&#x431;&#x443;&#x432;&#x430;&#x442;&#x438; &#x43A;&#x43E;&#x440;&#x456;&#x43D;&#x44C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x63D0;&#x70BC; <pr>t&#xED;li&#xE0;n</pr></trd>,
    <trd>&#x8403;&#x53D6; <pr>cu&#xEC;q&#x1D4;</pr></trd>
  </trdgrp>
</drv>
