<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: upanisxad.xml,v 1.6 2024/11/22 12:53:43 revo Exp $">
<kap>
   <rad>upani&scirc;ad</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="upanisxad.0o">
   <kap><tld/>o</kap>
   <snc mrk="upanisxad.0o.HIN">
     <uzo tip="fak">HIN</uzo>
     <dif>
       Iu la sanktaj tekstoj de hinduismo, kiuj filozofie komentas
       la ritajn partojn de la <ref tip="vid"
cel="ved.0o.HIN">vedoj</ref>:
       <ekz>
         certajn aspektojn de <klr>[jogo]</klr> ni trovas en
ankora&ubreve; pli
         malnovaj <tld/>oj
         <fnt>
           <aut>L. Cyboran, trad. K. &Sacute;liwi&nacute;ski</aut>,
           <vrk><url ref="">Filozofio de Jogo</url>, fragmentoj</vrk>,
           <lok>[vidita en 2006-12]</lok>
         </fnt>;
       </ekz>
       <ekz>
         aperis <ctl>Els Upanishads</ctl> <klr>...</klr> antologio de la
         plej elstaraj <tld/>oj
         <fnt>
           <aut>-</aut>,
           <vrk>Upani&scirc;adoj katalunigitaj</vrk>,
           <lok><url ref="">Kataluna Esperantisto, 2006-01 &gcirc;is 03,
           numero 337a</url>, p. 7a</lok>
         </fnt>
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_u;&c_p;&c_a;&c_n;&c_ib;&c_sh;&c_a;&c_d;&c_a;</trd>
   <trd lng="bg">&c_u;&c_p;&c_a;&c_n;&c_i;&c_sh;&c_a;&c_d;</trd>
   <trd lng="ca">upanixad</trd>
   <trd lng="en">upanishad</trd>
   <trd lng="fi">upani&scaron;ad</trd>
   <trd lng="fr">upanishad</trd>
   <trd lng="it">Upanishad</trd>
   <trd lng="nl">upanishad</trd>
   <trd lng="pl">upaniszady {pl}</trd>
</drv>
</art>

