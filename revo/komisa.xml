<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: komisa.xml,v 1.22 2022/05/25 08:39:02 revo Exp $">
<kap>
  <ofc>1</ofc><rad>komisar</rad>/o <fnt><bib>FK</bib></fnt>,
  <var><kap><rad var="i">komisari</rad>/o <fnt><bib>Fab3</bib></fnt></kap></var>
</kap>

<drv mrk="komisa.0o">
  <kap><tld/>o, <var><kap><tld var="i"/>o</kap></var></kap>
  <snc mrk="komisa.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Tiu, kiu estas komisiita de &scirc;tato a&ubreve; alia organizo por
      plenumi specialan neda&ubreve;ran funkcion:
      <ekz>
        tri sinjoroj, elegante vestitaj, venis en hotelon, prezentante sin kiel <tld/>oj
        Amerikaj de la ekspozicio Antverpena
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi estas <tld var="i"/>o de translo&gcirc;i&gcirc;oj
        <fnt>
          <bib>Fab4</bib>
          <lok>Dekdu per la po&scirc;to</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de la registaro apud la parlamento
        <klr>(por klarigi kaj apogi specialan le&gcirc;oprojekton)</klr>;
      </ekz>
      <ekz>
        UN-<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2010/010085.php">Kura&gcirc;u malobei!</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de balo, de festo, de ekspozicio
        <klr>(komisiita por &gcirc;in prepari kaj prizorgi)</klr>.
      </ekz>
    </dif>
    <trd lng="en">commissary</trd>
    <trdgrp lng="nl">
      <trd>commissaris</trd>,
      <trd>ceremoniemeester</trd>
    </trdgrp>
  </snc>
  <snc mrk="komisa.0o.oficisto">
    <dif>
      &Scirc;tata oficisto kun administraj a&ubreve; justicaj taskoj:
      <ekz>
        skribu sur <klr>[koverton]</klr> la adreson de la bienmastro: &Gcirc;enerala milita
        <tld var="i"/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Kora sufero</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o estas Salvo Montalbano kaj liaj aventuroj estas tradukitaj en multajn
        lingvojn
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2012/010685.php">Fikciaj fi&scirc;oj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en preska&ubreve; &ccirc;iuj <klr>[liaj romanoj]</klr> la protagonisto 
        estas superkapabla policisto <tld/>o San-Antonio
        <fnt><aut>B. Kolker</aut>:
          <vrk><url ref="&Monato;/rec/mpma.html">
          Parizo slangas en Esperanto</url></vrk>,
          <bib>Monato</bib>,
          <lok>1997:05</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">commissioner</trd>
    <trd lng="nl">commissaris</trd>
  </snc>
  <ref tip="sin" cel="komisi.0ito">komisiito</ref>
  <trd lng="be">&c_k;&c_a;&c_m;&c_ib;&c_s;&c_a;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>komisa&rcaron;</trd>,
    <trd>pov&ecaron;&rcaron;enec</trd>,
    <trd>po&rcaron;adatel</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kommissar</trd>,
    <trd>Beauftragter</trd>
  </trdgrp>
  <trd lng="fr">commissaire</trd>
  <trd lng="pl">komisarz</trd>
  <trdgrp lng="pt">
    <trd>comiss&aacute;rio</trd>,
    <trd>encarregado</trd>
  </trdgrp>
  <trd lng="ro">comisar</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_i;&c_s;&c_s;&c_a;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>komis&aacute;r</trd>,
    <trd>poverenec</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&#x43E;&#x43C;&c_ib;&c_s;&c_a;&c_r;</trd>
</drv>

<drv mrk="komisa.polic0o">
  <kap>polic<tld/>o,
  <var><kap>polica <tld/>o<fnt>Z</fnt></kap></var></kap>
  <snc mrk="komisa.polic0o.KOMUNE">
    <dif>
      <ref tip="super" cel="polic.0isto">Policisto</ref> kun alta rango; &ccirc;efenketisto:
      <ekz>
        polic<tld/>o Roberto <nom>Elis</nom> enketas
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2007/009132.php">Nova krimromano el
          A&ubreve;stralio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la polic<tld/>o vivis fore sur la &ccirc;eftero kaj 
        neniam estis vokita pro juraj aferoj
        <fnt>
          <aut>A. Strindberg, trad. S. Johansson</aut>,
          <vrk>Insulanoj de Hems&ouml;</vrk>,
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_ib;&c_s;&c_a;&c_r; &c_p;&c_a;&c_l;&c_ib;&c_c;&c_y;&c_ib;</trd>
  <trdgrp lng="de">
    <trd>Polizeikommissar</trd>,
    <trd>leitender <ind>Ermittler</ind></trd>
  </trdgrp>
  <trd lng="fr">commissaire de police</trd>
  <trd lng="pl">komisarz policji</trd>     
  <trd lng="ro">comisar de poli&tcedil;ie</trd>
</drv>

<drv mrk="komisa.vic0o">
  <kap>vic<tld/>o</kap>
  <snc mrk="komisa.vic0o.KOMUNE">
    <dif>
      Vica, helpanta <tld/>o:
      <ekz>
        <nom>Li&ubreve;</nom> estas enoficigita kiel Vic<tld/>o de 
        Propagando por la loka Revolucia Partio
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_k;&c_a;&c_m;&c_ib;&c_s;&c_a;&c_r;</trd>
  <trdgrp lng="de">
    <trd>Vizekommissar</trd>,
    <trd>Stellvertreter</trd>
  </trdgrp>
  <trd lng="fr">commissaire adjoint</trd>
  <trd lng="pl">podkomisarz</trd>     
  <trd lng="ro">comisar adjunct de poli&#x21B;ie</trd>
</drv>
</art>
