<vortaro>
<art mrk="$Id$">
<kap>
  <rad>sekstant</rad>/o
</kap>
<drv mrk="seksta.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Astronomia instrumento, per kiu oni observas angulon inter du
      punktoj, kaj plej ofte la alton de astro super la horizonto de la
      maro.
    </dif>
  </snc>
  <trd lng="de">Sextant</trd>
  <trd lng="en">sextant</trd>
  <trd lng="fr">sextant</trd>
  <trd lng="nl">sextant</trd>
  <trd lng="ru">&c_s;&c_je;&c_k;&c_s;&c_t;&c_a;&c_n;&c_t;</trd>
</drv>
</art>
