<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: string.xml,v 1.7 2020/03/22 21:10:34 revo Exp $">
<kap>
  <rad>string</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="string.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="string.0i.cxirkaupremi">
    <dif>
      <ref tip="dif" cel="prem.cxirkaux0i">&Ccirc;irka&ubreve;premi</ref>:
      <ekz>
        la kapristo <klr>[&#8230;]</klr> <tld/>is al li la kolon per la du
        manoj
        <fnt>
        <bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 52a, p. 382a</lok>
        </fnt>;
      </ekz>
      <ekz>
        toraka doloro malanta&ubreve; la sternumo, priskribata de pacientoj
        kiel doloro
        <tld/>anta a&ubreve; opresa
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.be/2013/011124.php">Atentu
          la koron</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        la abismoj <tld/>i&gcirc;as, kaj fori&gcirc;as la historia maljusteco
        <fnt>
          <bib>Monato</bib>
          <aut>Dmitriy Vlassov</aut>
          <vrk><url ref="https://www.monato.be/2010/009991.php">Forfalas
          la landlimoj</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu situacio <tld/>as Italion same kiel makzelo
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.be/2013/010777.php">&Gcirc;is la
          tero tremas</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="strang1.0i">strangoli</ref>
    <refgrp tip="vid">
       <ref cel="prem.kun0i">kunpremi</ref>,
       <ref cel="lig.kun0i">kunligi</ref>,
       <ref cel="volv.cxirkaux0i">&ccirc;irka&ubreve;volvi</ref>,
       <ref cel="cxen.0i">&ccirc;eni</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>podv&aacute;zat</trd>,
    <trd>p&rcaron;ilo&zcaron;it tlakov&yacute; obvaz</trd>,
    <trd>za&scaron;krtit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>einschn&uuml;ren</trd>,
    <trd>einengen</trd>,
    <trd>zusammendr&uuml;cken</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>comprimer</trd>,
    <trd>&eacute;treindre</trd>,
    <trd>serrer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&ouml;sszeszor&iacute;t</trd>,
    <trd>megszor&iacute;t</trd>,
    <trd>szorongat</trd>
  </trdgrp>
  <trd lng="pt">estrangular</trd>

  <trdgrp lng="sk">
    <trd>podviaza&tcaron;</trd>,
    <trd>pri&scaron;krti&tcaron;</trd>,
    <trd>stla&ccaron;i&tcaron;</trd>,
    <trd>zviera&tcaron;</trd>
  </trdgrp>
</drv>
</art>
