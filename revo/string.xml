    <trd>megszor&iacute;t</trd>,
    <trd>szorongat</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#32224;&#12417;&#12388;&#12369;&#12427;  <pr>&#12375;&#12417;&#12388;&#12369;&#12427;</pr></trd>,
    <trd>&#25569;&#12426;&#12375;&#12417;&#12427;  <pr>&#12395;&#12366;&#12426;&#12375;&#12417;&#12427;</pr></trd>,
    <trd>&#25265;&#12365;&#12375;&#12417;&#12427;  <pr>&#12384;&#12365;&#12375;&#12417;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
