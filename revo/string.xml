  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_i;&c_s;&#1082;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_t;&c_i;&c_s;&#1082;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_t;&#1103;&c_g;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_t;&#1103;&c_g;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#1086;&c_b;&c_t;&c_i;&c_s;&#1082;&c_a;&c_t;&c_i;</trd>,
    <trd>&#1086;&c_b;&c_t;&#1103;&c_g;&c_a;&c_t;&c_i;</trd>,
    <trd>&#1085;&c_a;&#1082;&#1083;&c_a;&c_d;&c_a;&c_t;&c_i; &#1087;&#1086;&c_v;&#8217;&#1103;&c_z;&#1082;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#25265;&#32039; <pr>b&#224;oj&#464;n</pr></trd>,
    <trd>&#25265;&#32202; <pr>b&#224;oj&#464;n</pr></trd>
  </trdgrp>
</drv>
