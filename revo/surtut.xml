<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: surtut.xml,v 1.26 2023/09/04 15:14:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>surtut</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="surtut.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="surtut.0o.TEKS">
  <uzo tip="fak">TEKS</uzo>
    <dif>
      En la e&ubreve;ropa modo, <ref tip="super" cel="vest.super0o">supervesto</ref>
      (<ref tip="lst" lst="voko:vesta&jcirc;oj" cel="kaftan.0o">kaftano</ref>)
      kun la baskoj atingantaj genuojn (iam iom pli longa, iam
      malpli), kun kolumo, iom malstrikta &ccirc;e la talio, kun butonoj:
      <ekz>
        mi volas alkudri butonon al mia <tld/>o
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la po&scirc;o de mia pantalono mi portas monujon, kaj en la
        po&scirc;o de mia <tld/>o mi portas paperujon <klr>(t.e.
        biletujon)</klr>
        <fnt><bib>F</bib>, <lok>&FE; 40.</lok></fnt>;
      </ekz>
      <ekz>
        mi donos al vi <tld/>on kaj mantelon, kiun vi devos porti dum &ccirc;i tiu tempo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ursa Felo</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekrapidonta al la vizitanto, kiun li ekvidis en la anta&ubreve;&ccirc;ambro donantan al lakeo
        sian <tld/>on
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        doganistoj, kiuj en siaj ru&gcirc;aj <tld/>oj <klr>[&#x2026;]</klr> saltadis de &scirc;ipo al &scirc;ipo
        <fnt>
          <bib>RBa</bib>
        </fnt>;
      </ekz>
      <ekz>
        brilas sur lia kapo la &ccirc;apelo, brilas sur la manoj la gantoj kun
        duobla kunkudro, brilas sur la brusto ora &ccirc;eneto kaj
        balanci&gcirc;as gracie sur la malluma fono de la <tld/>o
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        li sidis sur la parada se&gcirc;o, tenante unu manon en la brusto de l&#x2019; <tld/>o kaj la
        duan en la neordigitaj haroj
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        multo da gastoj en nigraj <tld/>oj kaj frakoj aliras
        anta&ubreve;e al la mano de <nom>Anna Andrejevna</nom>
        <fnt>
        <aut>N. V. Gogol, trad. L. L. &Z;</aut>, <vrk>La revizoro</vrk>,
        <lok>akto 5a, sceno 5a</lok>;
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i kaptis sian <tld/>on, sukcesis surmeti kaj butonumi
        &gcirc;in, kaj kuris nudpiede tra la koridoro
        <fnt>
        <aut>M. Boulton</aut>, <vrk>Ebrivirgeco</vrk>,
        <lok>La nica literatura revuo, 3:3</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="mantel.0o.kunmanika">mantelo<sncref/></ref>
    <refgrp tip="vid">
      <ref cel="reding.0o">redingoto</ref>,
      <ref cel="jak.0o">jako</ref>,
      <ref cel="palt.0o">palto</ref>,
      <ref cel="sutan.0o">sutano</ref>,
      <ref cel="tunik.0o">tuniko</ref>
    </refgrp>
    <bld lok="&WCU;/thumb/8/84/Man's_silk_and_wool_twill_frock_coat_c._1820.jpg/180px-Man's_silk_and_wool_twill_frock_coat_c._1820.jpg" prm="PD">vira <tld/>o, Francujo, &ccirc;irka&ubreve; 1820
        <fnt>
          <aut>LACMA Image Library</aut>
          <lok><url ref="&WCW;/File:Man's_silk_and_wool_twill_frock_coat_c._1820.jpg">&WCW;/File:Man's_silk_and_wool_twill_frock_coat_c._1820.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="de">
      <trd><ofc>*</ofc>Rock <klr>(Obergewand)</klr></trd>,
      <trd>Gehrock</trd>,
      <trd>Mantel</trd>
    </trdgrp>
    <trd lng="pl"><ofc>*</ofc>surdut</trd>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_r;&c_d;&c_u;&c_t;</trd>
  <trd lng="cs">svrchn&iacute;k <klr>(kab&aacute;t)</klr></trd>
  <trd lng="en"><ofc>*</ofc>over-coat</trd>
  <trdgrp lng="es">
    <trd>abrigo</trd>,
    <trd>sobretodo</trd>,
    <trd>gab&aacute;n</trd>,
    <trd>chaquet&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd><ofc>*</ofc>redingote</trd>,
    <trd>pardessus</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kab&aacute;t</trd>,
    <trd>k&ouml;nt&ouml;s</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30AA;&#x30FC;&#x30D0;&#x30FC;  [<ind>&#x304A;&#x30FC;&#x3070;&#x30FC;</ind>]</trd>,
    <trd>&#x5916;&#x5957;  [<ind>&#x304C;&#x3044;&#x3068;&#x3046;</ind>]</trd>,
    <trd>&#x30B3;&#x30FC;&#x30C8;  [<ind>&#x3053;&#x30FC;&#x3068;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">overjas</trd>
  <trd lng="ru"><ofc>*</ofc>&c_s;&c_ju;&c_r;&c_t;&c_u;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>pl&aacute;&scaron;&tcaron;</trd>,
    <trd>prevle&ccaron;n&iacute;k</trd>,
    <trd>zvrchn&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&#x44E;&c_r;&c_t;&c_u;&#x43A;</trd>,
    <trd>&#x43F;&#x43B;&c_a;&c_shch; (&c_z; &#x43A;&c_a;&#x43F;&c_t;&c_u;&c_r;&#x43E;&#x43C;)</trd>
  </trdgrp>
</drv>


<drv mrk="surtut.nokto0o">
  <kap>nokto<tld/>o, <var><kap>nokta <tld/>o</kap></var></kap>
  <snc mrk="surtut.nokto0o.TEKS">
  <uzo tip="fak">TEKS</uzo>
    <dif>
      Negli&gcirc;a <ref tip="sin" cel="hxalat.0o">&hcirc;alato</ref>
      ofte uzata kiel <ref tip="super" cel="vest.nokto0o.TEKS">noktovesto</ref>:
      <ekz>
        vasta blusilka sabata nokto<tld/>o
        <fnt>
          <bib>RBa</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> havis neniajn aliajn vestojn krom paro da pantofloj kaj malnova
        nokta <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Fluganta kofro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la doktoro en nokta <tld/>o kaj pantofloj <klr>[&#x2026;]</klr> sidis en la ava se&gcirc;o
        <fnt>
          <bib>BdV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Nachtrock</trd>,
      <trd>Schlafrock</trd>,
      <trd>Morgenmantel</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_ch;&c_n;&c_y; &c_h;&c_a;&c_l;&c_a;&c_t;</trd>
  <trd lng="cs">&zcaron;upan</trd>
  <trd lng="es">bata de noche</trd>
  <trd lng="fr">robe de chambre</trd>
  <trdgrp lng="hu">
    <trd>h&aacute;l&oacute;k&ouml;nt&ouml;s</trd>,
    <trd>h&aacute;zikab&aacute;t</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30CA;&#x30A4;&#x30C8;&#x30AC;&#x30A6;&#x30F3;</trd>,
    <trd>&#x90E8;&#x5C4B;&#x7740;  [<ind>&#x3078;&#x3084;&#x304E;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">kamerjas</trd>
  <trd lng="ru">&c_d;&c_o;&c_m;&c_a;&c_sh;&c_n;&c_i;&c_j; &c_h;&c_a;&c_l;&c_a;&c_t;</trd>
  <trd lng="sk">&zcaron;upan</trd>
  <trd lng="uk">&#x43D;&c_ib;&c_ch;&#x43D;&c_i;&c_j; &c_h;&c_a;&#x43B;&c_a;&c_t;</trd>
</drv>
</art>
