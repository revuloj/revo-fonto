<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>surtut</rad>/o
</kap>
<drv mrk="surtut.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Longa vira vesto kun longa basko tute &ccirc;irka&ubreve;anta la korpon:
      <ekz>
        la ru&gcirc;aj <tld/>oj de la doganistoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="reding.0o">redingoto</ref>
    <refgrp tip="super">
      <ref cel="vest.super0osur0o">supervesto</ref>,
      <ref cel="palt.0o">palto</ref>,
      <ref cel="mantel.0o">mantelo</ref>.
    </refgrp>
  </snc>
  <trd lng="en">overcoat</trd>
  <trd lng="nl">overjas</trd>
  <trd lng="ru">&c_s;&c_ju;&c_r;&c_t;&c_u;&c_k;</trd>
</drv>

<drv mrk="surtut.nokto0o">
  <kap>
    nokto<tld/>o
  </kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Negli&gcirc;a hejma vira robo:
      <ekz>
        vasta blusilka nokto<tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">kamerjas</trd>
  <trd lng="ru">&c_d;&c_o;&c_m;&c_a;&c_sh;&c_n;&c_i;&c_j; &c_h;&c_a;&c_l;&c_a;&c_t;</trd>
</drv>
</art>
