<drv mrk="fabrik.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fabrik.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="vid" cel="produk.0i.TEK">Produkti</ref>
      komerca&jcirc;ojn en
      <tld/>o:
      <ekz>
        <tld/>i meblojn
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        tie oni <tld/>is la famajn vazojn el blanka kaj nigra argilo
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 8a</lok></fnt>.
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi <tld/>as mian tezon: jen estas &ccirc;io
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u aferojn por la milito kaj vi fari&gcirc;os milionulo
        <fnt>
          <bib>HsT</bib>
          <lok>La nepo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_r;&c_a;&c_b;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_d;&c_u;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>oberia&ntilde;</trd>,
    <trd>farda&ntilde;<klr tip="ind"> (oberia&ntilde;)</klr></trd>
  </trdgrp>
  <trd lng="ca">fabricar</trd>
  <trd lng="cs">pr&uring;myslov&ecaron; vyr&aacute;b&ecaron;t</trd>
  <trdgrp lng="de">
    <trd>fabrizieren</trd>,
    <trd>produzieren</trd>,
    <trd>herstellen</trd>,
    <trd>anfertigen</trd>
  </trdgrp>
  <trd lng="en">manufacture</trd>
  <trd lng="es">fabricar</trd>
  <trdgrp lng="fr">
    <trd>fabriquer<klr tip="ind"> (produire)</klr></trd>,
    <trd>produire<klr tip="ind"> (fabriquer)</klr></trd>
  </trdgrp>
  <trd lng="hu">gy&aacute;rt</trd>
  <trd lng="io">fabrikar</trd>
  <trdgrp lng="ja">
    <trd>&#x88FD;&#x9020;&#x3059;&#x308B;  <pr>&#x305B;&#x3044;&#x305E;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x88FD;&#x4F5C;&#x3059;&#x308B;  <pr>&#x305B;&#x3044;&#x3055;&#x304F;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x751F;&#x7523;&#x3059;&#x308B;  <pr>&#x305B;&#x3044;&#x3055;&#x3093;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">fabriceren</trd>
  <trdgrp lng="pl">
