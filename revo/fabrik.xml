<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fabrik.xml,v 1.25 2021/05/28 12:14:57 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fabrik</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fabrik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fabrik.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Ejo, en kiu oni produktas pogrande komerca&jcirc;ojn:
      <ekz>
        <tld/>o de &scirc;uoj;
      </ekz>
      <ekz>
        konata <tld/>o de &ccirc;ampano
        <fnt><bib>FK</bib>, <lok>3, anekdotoj</lok></fnt>;
      </ekz>
      <ekz>
        paper<tld/>oj
        <fnt><bib>InfanTorent2</bib>, <lok>dua parto, &ccirc;apitro
        2a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="manufa.0o">manufakturo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_b;&c_r;&c_y;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_o;&c_d;</trd>
  </trdgrp>
  <trd lng="bo">&#xF56;&#xF5F;&#xF7C;&#xF0B;&#xF42;&#xFB2;&#xFAD;&#xF0B;</trd>
  <trdgrp lng="br">
    <trd>greanti</trd>,
    <trd>labouradeg</trd>
  </trdgrp>
  <trd lng="ca">f&agrave;brica</trd>
  <trdgrp lng="cs">
    <trd>tov&aacute;rna</trd>,
    <trd>z&aacute;vod</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Fabrik</trd>,
    <trd>Werk</trd>
  </trdgrp>
  <trd lng="en">factory</trd>
  <trd lng="es">f&aacute;brica</trd>
  <trdgrp lng="fr">
    <trd>fabrique</trd>,
    <trd>usine</trd>
  </trdgrp>
  <trd lng="hu">gy&aacute;r</trd>
  <trd lng="io">fabrikerio</trd>
  <trd lng="nl">fabriek</trd>
  <trdgrp lng="pl">
    <trd>fabryka</trd>,
    <trd>wytw&oacute;rnia</trd>
  </trdgrp>
  <trd lng="pt">f&aacute;brica</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_a;&c_b;&c_r;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_o;&c_d;</trd>
  </trdgrp>
  <trd lng="sk">tov&aacute;re&ncaron;</trd>
  <trd lng="sv">fabrik</trd>
  <trd lng="tr">fabrika</trd>
  <trd lng="vo">fabl&uuml;d</trd>
</drv>

<drv mrk="fabrik.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fabrik.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="vid" cel="produk.0i.TEK">Produkti</ref>
      komerca&jcirc;ojn en
      <tld/>o:
      <ekz>
        tie oni <tld/>is la famajn vazojn el blanka kaj nigra argilo
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 8a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_r;&c_a;&c_b;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_d;&c_u;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>oberia&ntilde;</trd>,
    <trd>farda&ntilde;<klr tip="ind"> (oberia&ntilde;)</klr></trd>
  </trdgrp>
  <trd lng="ca">fabricar</trd>
  <trd lng="cs">pr&uring;myslov&ecaron; vyr&aacute;b&ecaron;t</trd>
  <trdgrp lng="de">
    <trd>fabrizieren</trd>,
    <trd>produzieren</trd>,
    <trd>herstellen</trd>
  </trdgrp>
  <trd lng="en">manufacture</trd>
  <trd lng="es">fabricar</trd>
  <trdgrp lng="fr">
    <trd>fabriquer<klr tip="ind"> (produire)</klr></trd>,
    <trd>produire<klr tip="ind"> (fabriquer)</klr></trd>
  </trdgrp>
  <trd lng="hu">gy&aacute;rt</trd>
  <trd lng="io">fabrikar</trd>
  <trd lng="nl">fabriceren</trd>
  <trdgrp lng="pl">
    <trd>profukowa&cacute;</trd>,
    <trd>wytwarza&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>vyrobi&tcaron;</trd>,
    <trd>vyr&aacute;ba&tcaron;</trd>
  </trdgrp>
  <trd lng="tr">&uuml;retmek</trd>
  <trd lng="vo">fabl&uuml;d&ouml;n</trd>
</drv>

<drv mrk="fabrik.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="fabrik.0ado.EKON">
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_t;&c_v;&c_o;&c_r;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="br">oberiadur</trd>
  <trd lng="ca">fabricaci&oacute;</trd>
  <trdgrp lng="cs">
    <trd>fabrikace</trd>,
    <trd>tov&aacute;rensk&aacute; v&yacute;roba</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Fabrikation</trd>,
    <trd>Produktion</trd>,
    <trd>Herstellung</trd>
  </trdgrp>
  <trd lng="es">fabricaci&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>fabrication</trd>,
    <trd>production</trd>
  </trdgrp>
  <trd lng="hu">gy&aacute;t&aacute;s</trd>
  <trd lng="nl">het <ind>fabriceren</ind></trd>
  <trdgrp lng="pl">
    <trd>profukcja</trd>,
    <trd>wytw&oacute;rstwo</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_o;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="sk">tov&aacute;rensk&aacute; v&yacute;roba</trd>
  <trd lng="tr">&uuml;retim</trd>
</drv>

<drv mrk="fabrik.0anto">
  <kap><tld/>anto</kap>
  <snc mrk="fabrik.0anto.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Posedanto kaj estro de <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_b;&c_r;&c_y;&c_k;&c_a;&c_n;&c_t;</trd>
  <trdgrp lng="br">
    <trd>oberier</trd>,
    <trd>farder</trd>
  </trdgrp>
  <trd lng="ca">fabricant</trd>
  <trdgrp lng="cs">
    <trd>hanl. fabrikant</trd>,
    <trd>tov&aacute;rn&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Fabrikant</trd>,
    <trd>Fabrikbesitzer</trd>,
    <trd>Produzent</trd>,
    <trd>Hersteller</trd>
  </trdgrp>
  <trd lng="en">manufacturer</trd>
  <trd lng="es">fabricante</trd>
  <trdgrp lng="fr">
    <trd>constructeur<klr tip="ind"> (fabriquant)</klr></trd>,
    <trd>fabriquant</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gy&aacute;ros</trd>,
    <trd>gy&aacute;rt&oacute;</trd>
  </trdgrp>
  <trd lng="nl">fabrikant</trd>
  <trdgrp lng="pl">
    <trd>fabrykant<klr> (przest.)</klr></trd>,
    <trd>producent</trd>,
    <trd>wytw&oacute;rca</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_a;&c_b;&c_r;&c_i;&c_k;&c_a;&c_n;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>tov&aacute;rnik</trd>,
    <trd>v&yacute;robca</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>&uuml;retici</trd>,
    <trd>&uuml;reten</trd>
  </trdgrp>
</drv>

<drv mrk="fabrik.pens0o">
  <kap>pens<tld/>o</kap>
  <snc mrk="fabrik.pens0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Privata asocio a&ubreve; fonda&jcirc;o, kiu studas sociajn kaj politikajn 
      temojn kaj pri ili publikigas raportojn, rekomendojn:
      <ekz>
        la  Centro por Politikaj Alternativoj <klr>...</klr>
        estas pens<tld/>o pri civila socio en Srilanko 
        <fnt>
          <aut>Rezwan, trad. T. Nee</aut>,
          <vrk><url ref="eo.globalvoicesonline.org/2014/11/1964">
          Informgrafiko: 5 faktoj pri tamila komunumo en la nordo de 
          Srilanko</url></vrk>,
          <lok>GlobalVoices, 2014-11-23</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Denkfabrik</trd>,
    <trd>Forschungsinstitut</trd>
  </trdgrp>
  <trd lng="en">think tank</trd>
  <trdgrp lng="fr">
    <trd>club de r&eacute;flexion</trd>,
    <trd>laboratoire d'id&eacute;es</trd>
  </trdgrp>
</drv>
</art>
