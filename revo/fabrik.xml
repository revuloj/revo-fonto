    <trd>&#x88FD;&#x9020;&#x6240;  <pr>&#x305B;&#x3044;&#x305E;&#x3046;&#x3057;&#x3087;</pr></trd>,
    <trd>&#x88FD;&#x4F5C;&#x6240;  <pr>&#x305B;&#x3044;&#x3055;&#x304F;&#x3057;&#x3087;</pr></trd>
  </trdgrp>
  <trd lng="nl">fabriek</trd>
