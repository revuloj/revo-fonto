    <trd>&#x7DCF;&#x5408;&#x7814;&#x7A76;&#x6240;  <pr>&#x305D;&#x3046;&#x3054;&#x3046;&#x3051;&#x3093;&#x304D;&#x3085;&#x3046;&#x3057;&#x3087;</pr></trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7814;&#x7A76;&#x9662; <pr>y&#xE1;nji&#xF9;yu&#xE0;n</pr></trd>,
    <trd>&#x667A;&#x56CA;&#x673A;&#x6784; <pr>zh&#xEC;n&#xE1;ngj&#x12B;g&#xF2;u</pr></trd>,
    <trd>&#x667A;&#x5E93; <pr>zh&#xEC;k&#xF9;</pr></trd>
  </trdgrp>
</drv>
