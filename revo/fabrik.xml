<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fabrik</rad>/o
</kap>
<drv mrk="fabrik.0o">
<kap><ofc>*</ofc><tld/>o</kap>
<uzo tip="fak">EKON</uzo>
<snc>
<dif>
Ejo, en kiu oni produktas pogrande komerca&jcirc;ojn:
<ekz><tld/>o de drapo, &scirc;uoj, ludiloj, medikamentoj.</ekz>
</dif>
<refgrp tip="vid">
<ref cel="manufa.0o">manufakturo</ref>.
</refgrp>
</snc>
<trdgrp lng="be">
  <trd>&c_f;&c_a;&c_b;&c_r;&c_y;&c_k;&c_a;</trd>,
  <trd>&c_z;&c_a;&c_v;&c_o;&c_d;</trd>
</trdgrp>
  <trdgrp lng="de">
    <trd>Fabrik</trd>,
    <trd>Werk</trd></trdgrp>
  <trd lng="es">f&aacute;brica</trd>
  <trdgrp lng="fr">
    <trd>fabrique</trd>,
    <trd>usine</trd></trdgrp> 
  <trd lng="hu">gy&aacute;r</trd>
  <trd lng="nl">fabriek</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_a;&c_b;&c_r;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_o;&c_d;</trd>
  </trdgrp>
  <trd lng="sv">fabrik</trd>
  <trd lng="vo">fabl&uuml;d</trd>
</drv>
<drv mrk="fabrik.0i">
<kap><tld/>i</kap>
<gra><vspec>tr</vspec></gra>
<uzo tip="fak">EKON</uzo>
<snc>
<dif>
Produkti komerca&jcirc;ojn en <tld/>o. 
</dif>
</snc>
<trdgrp lng="be">
  <trd>&c_v;&c_y;&c_r;&c_a;&c_b;&c_l;&c_ja;&c_c;&c_mol;</trd>,
  <trd>&c_p;&c_r;&c_a;&c_d;&c_u;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
</trdgrp>
  <trdgrp lng="de">
    <trd>fabrizieren</trd>,
    <trd>produzieren</trd>,
    <trd>herstellen</trd></trdgrp>
  <trd lng="es">fabricar</trd>
  <trdgrp lng="fr">
    <trd>fabriquer<klr tip="ind"> (produire)</klr></trd>,
    <trd>produire<klr tip="ind"> (fabriquer)</klr></trd></trdgrp> 
  <trd lng="hu">gy&aacute;rt</trd>
  <trd lng="nl">fabriceren</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>
  <trd lng="vo">fabl&uuml;d&ouml;n</trd>
</drv>
<drv mrk="fabrik.0ado">
<kap><tld/>ado</kap>
<snc>
<dif>
Ago <tld/>i. 
</dif>
</snc>
<trd lng="be">&c_v;&c_y;&c_t;&c_v;&c_o;&c_r;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>Fabrikation</trd>,
    <trd>Produktion</trd>,
    <trd>Herstellung</trd></trdgrp>
  <trd lng="es">fabricaci&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>fabrication</trd>,
    <trd>production</trd></trdgrp> 
  <trd lng="hu">gy&aacute;t&aacute;s</trd>
  <trd lng="nl">het <ind>fabriceren</ind></trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_o;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>
<drv mrk="fabrik.0anto">
<kap><tld/>anto</kap>
<uzo tip="fak">EKON</uzo>
<snc>
<dif>
Posedanto kaj estro de <tld/>o. 
</dif>
</snc>
<trd lng="be">&c_f;&c_a;&c_b;&c_r;&c_y;&c_k;&c_a;&c_n;&c_t;</trd>
  <trdgrp lng="de">
    <trd>Fabrikant</trd>,
    <trd>Fabrikbesitzer</trd>,
    <trd>Produzent</trd>,
    <trd>Hersteller</trd></trdgrp>
  <trd lng="nl">fabrikant</trd>
  <trd lng="es">fabricante</trd>  
  <trdgrp lng="fr">
    <trd>constructeur<klr tip="ind"> (fabriquant)</klr></trd>,
    <trd>fabriquant</trd></trdgrp> 
  <trdgrp lng="hu">
    <trd>gy&aacute;ros</trd>,
    <trd>gy&aacute;rt&oacute;</trd></trdgrp>
  <trd lng="ru">&c_f;&c_a;&c_b;&c_r;&c_i;&c_k;&c_a;&c_n;&c_t;</trd>
</drv>
</art>
