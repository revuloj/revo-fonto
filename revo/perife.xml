<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: perife.xml,v 1.26 2024/02/17 06:43:06 revo Exp $">
<kap>
  <rad>periferi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="perife.0o">
  <kap><tld/>o</kap>
  <snc mrk="perife.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="rand.0o.KOMUNE">Rando</ref> de areo, regiono plej for de la centro:
      <ekz>
        &scirc;i lo&gcirc;as en la kampara <tld/>o de la metropolo
        <fnt>
          <bib>KKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        la mizeruloj, la <tld/>anoj de la urbeta socio
        <fnt>
          <bib>KKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        la man&ccirc;uroj estis &ccirc;asantoj el la nordorienta <tld/>o de la &ccirc;ina civilizo
        <fnt>
          <aut>Sabeo Fu/Yamasaki Seik&ocirc;</aut>
            <vrk><url ref="&Monato;/2001/007025.php">Formortanta lingvo de malplimulto en
            &Ccirc;inio</url></vrk>
            <bib>Monato</bib>
            <lok>2001/01, p. 22</lok>
          </fnt>;
      </ekz>
      <ekz>
        en 1808 en la urba <tld/>o <klr>[de <nom>&Oacute;bidos</nom>]</klr> okazis la unua venko de
        Napoleono
        <fnt>
          <bib>Monato</bib>
          <aut>William Simcock</aut>
          <vrk><url ref="&Monato;/2003/007873.html">Impresoj pri Portugalio</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        la parkumejo kaj &gcirc;ia <tld/>o estos elfositaj
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="&Monato;/2012/010707.php">Nova spuro pri perditaj
          fosilioj</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        minoritataj lingvoj, kondamnitaj al socia <tld/>o de ties parolantoj
        <fnt>
          <bib>Monato</bib>
          <aut>Maritza Guti&eacute;rrez</aut>
          <vrk><url ref="&Monato;/2009/009768.php">Kia lingva ekologio por da&ubreve;ripova
          evoluo?</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;ajnas do al mi tre facil-anime postuli al tuta religia komunumo, ke &gcirc;i aktive
        kontra&ubreve;staru fremdan kutimon, e&ccirc; plej a&ccirc;an, endemian en unu el &gcirc;iaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2006/008983.php">Bato sub la zono (4)</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="cxirka.0ajxo.KOMUNE">&ccirc;irka&ubreve;a&jcirc;o<sncref/></ref>
    <refgrp tip="vid">
      <ref cel="urb.antaux0o">anta&ubreve;urbo</ref>,
      <ref cel="lim.0o.spaca">limo<sncref/></ref>,
      <ref cel="margxe.0o">mar&gcirc;eno</ref>,
      <ref cel="markiz.0ujo">markizujo</ref>,
      <ref cel="provin.0o.necxefurbo">provinco<sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Au&szlig;enseite</trd>,
      <trd>Umgebung</trd>
    </trdgrp>
  </snc>
  <snc mrk="perife.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>VdE</bib></fnt>
    <dif>
      <klr>(de ebena <ref cel="surfac.0o.MAT">surfaco</ref>)</klr>
      &Gcirc;ia <ref tip="super" cel="rand.0o.figuro">rando</ref>.
    </dif>
    <refgrp tip="vid">
      <ref cel="cirkon.0o">cirkonferenco</ref>,
      <ref cel="kontur.0o">konturo</ref>,
      <ref cel="perime.0o">perimetro</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_r;&c_y;&c_f;&c_e;&c_r;&c_y;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>obrys</trd>,
    <trd>okrajov&aacute; &ccaron;&aacute;st m&ecaron;sta</trd>,
    <trd>periferie</trd>
  </trdgrp>
  <trd lng="de">Peripherie</trd>
  <trd lng="en">periphery</trd>
  <trd lng="es">periferia</trd>
  <trd lng="fr">p&eacute;riph&eacute;rie</trd>
  <trdgrp lng="hu">
    <trd>ker&uuml;let<klr tip="ind"> (sz&eacute;l)</klr></trd>,
    <trd>sz&eacute;l</trd>,
    <trd>perif&eacute;ria</trd>
  </trdgrp>
  <trd lng="ja">&#x5468;&#x56F2;  <pr>&#x3057;&#x3085;&#x3046;&#x3044;</pr></trd>
  <trdgrp lng="nl">
    <trd>periferie</trd>,
    <trd>omtrek</trd>,
    <trd>buitenkant</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kraw&eogonek;d&zacute;</trd>,
    <trd>brzeg</trd>
  </trdgrp>
  <trd lng="pt">periferia <klr tip="ind">(geom.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_i;&c_f;&c_je;&c_r;&c_i;&c_ja;</trd>,
    <trd>&c_o;&c_k;&c_r;&c_a;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>okrajov&aacute; &ccaron;as&tcaron; mesta</trd>,
    <trd>perif&eacute;ria</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_je;&c_r;&c_i;&c_f;&c_je;&c_r;&c_ib;&#x44F;</trd>,
    <trd>&#x43E;&#x43A;&c_r;&c_a;&c_ji;&#x43D;&c_a;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_i;&c_f;&c_je;&c_r;&c_ib;&#x44F;</trd>,
    <trd>&#x43E;&#x43A;&c_r;&c_a;&c_ji;&#x43D;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5468;&#x8FB9; <pr>zh&#x14D;ubi&#x101;n</pr></trd>,
    <trd>&#x5468;&#x908A; <pr>zh&#x14D;ubi&#x101;n</pr></trd>,
    <trd>&#x5916;&#x56F4; <pr>w&#xE0;iw&#xE9;i</pr></trd>,
    <trd>&#x5916;&#x570D; <pr>w&#xE0;iw&#xE9;i</pr></trd>,
    <trd>&#x5468;&#x8FB9; <pr>zh&#x14D;ubi&#x101;n</pr></trd>,
    <trd>&#x9031;&#x908A; <pr>zh&#x14D;ubi&#x101;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="perife.0a">
  <kap><tld/>a</kap>
  <snc mrk="perife.0a.KOMUNE">
    <dif>
      Estanta en <tld/>o, flanka,
      <ref tip="sin" cel="centr.mal0a.periferia">malcentra</ref>,
      <ref cel="rang.dua0a.OPI">duaranga</ref>:
      <ekz>
        li havis sian kabineton en <tld/>a kvartalo
        <fnt>
          <bib>ChR</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tiuj]</klr> okupoj estis kvaza&ubreve; <tld/>aj momentoj en vivo, kies centro
        estis la reciproka malkovro de <klr>[&#x2026;]</klr> &gcirc;u-eblecoj
        <fnt>
          <bib>ChM</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi revenis al la plej <tld/>aj urbaj domoj apena&ubreve; en tempo por savi min sub
        pordegan volbon, kiam ekfuriozis la &scirc;tormo
        <fnt>
          <bib>Kiso</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>&Gcirc;oja</nom> perceptis <klr>[&#x2026;]</klr> malproksiman, <tld/>an zumadon
        <fnt>
          <bib>ChC</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a vidkampo
        <fnt>
          <aut>&Eacute;. Benois</aut>,
          <vrk><url ref="http://edouard.benois.pagesperso-orange.fr/dr-es.htm">
          La disi&gcirc;o de retino</url></vrk>,
          <lok>[vidita en 2013]</lok>
        </fnt>;     
      </ekz>
      <ekz>
        <klr>[ili]</klr> estis <tld/>uloj de la bur&gcirc;a socio
        <fnt>
          <bib>KKr</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="cxirka.0a">&ccirc;irka&ubreve;a</ref>,
      <ref cel="rand.0a.KOMUNE">randa<sncref/></ref>
    </refgrp>
    <refgrp tip="vid">
       <ref cel="ekster.0a.randa">ekstera<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_e;&c_r;&c_y;&c_f;&c_e;&c_r;&c_y;&c_j;&c_n;&c_y;</trd>,
    <trd>&c_u;&c_s;&c_k;&c_r;&c_a;&c_ib;&c_n;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">perifern&iacute; okrajov&yacute;</trd>
  <trdgrp lng="de">
    <trd>peripher</trd>,
    <trd>entfernt</trd>
  </trdgrp>
  <trd lng="en">peripheral</trd>
  <trdgrp lng="es">
    <trd>perif&eacute;rico</trd>,
    <trd>perif&eacute;rica</trd>
  </trdgrp>
  <trd lng="fr">p&eacute;riph&eacute;rique</trd>
  <trdgrp lng="ja">
    <trd>&#x5468;&#x56F2;&#x306E;  <pr>&#x3057;&#x3085;&#x3046;&#x3044;&#x306E;</pr></trd>,
    <trd>&#x5468;&#x8FBA;&#x306E;  <pr>&#x3057;&#x3085;&#x3046;&#x3078;&#x3093;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>peryferyjny</trd>,
    <trd>skrajny</trd>,
    <trd>brzegowy</trd>
  </trdgrp>
  <trd lng="sk">okrajov&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_je;&c_r;&c_i;&c_f;&c_je;&c_r;&c_i;&c_ch;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_i;&c_f;&c_je;&c_r;&c_ib;&c_j;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_d;&c_a;&#x43B;&c_je;&#x43D;&c_i;&c_j; &c_v;&c_ib;&c_d; &c_c;&c_je;&#x43D;&c_t;&c_r;&c_u;</trd>,
    <trd>&#x43E;&#x43A;&c_r;&c_a;&c_ji;&#x43D;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5916;&#x56F4; <pr>w&#xE0;iw&#xE9;i</pr></trd>,
    <trd>&#x5916;&#x570D; <pr>w&#xE0;iw&#xE9;i</pr></trd>
  </trdgrp>
</drv>
</art>
