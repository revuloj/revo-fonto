<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>periferi</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="perife.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(de <ref cel="surfac.0o.MAT">surfaco</ref>)</klr>
      &Gcirc;ia <ref tip="super" cel="rand.0o.figuro">rando</ref>.
    </dif>
    <refgrp tip="vid">
      <ref cel="cirkon.0o">cirkonferenco</ref>,
      <ref cel="kontur.0o">konturo</ref>,
      <ref cel="perime.0o">perimetro</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Peripherie</trd>
  <trdgrp lng="nl">
    <trd>periferie</trd>,
    <trd>omtrek</trd>,
    <trd>buitenkant</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_i;&c_f;&c_je;&c_r;&c_i;&c_ja;</trd>,
    <trd>&c_o;&c_k;&c_r;&c_a;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
</drv>
</art>
