<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: krizok.xml,v 1.16 2023/11/14 09:28:26 revo Exp $">
<kap>
  <rad>krizokal</rad>/o
</kap>
<drv mrk="krizok.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="aloj.0o">Alojo</ref> el kupro, zinko, stano kaj plumbo, kiu imitas oron:
      <ekz>
        ili perceptas min krizalido el <tld/>o
        <fnt>
          <aut>Christian Declerck</aut>
          <vrk><url
          ref="https://books.google.de/books?id=DyRtwCfPNaIC&amp;lpg=PA48&amp;ots=IW9u_7RvCS&amp;dq=krizokalo&amp;hl=de&amp;pg=PA48#v=onepage&amp;q=krizokalo&amp;f=false">Spitaj
          - kiel hidrargo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="tombak.0o">tombako</ref>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_m;&c_b;&c_a;&c_k;</trd>,
    <trd>&c_t;&c_a;&c_m;&c_p;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Goldkupfer</trd>
  <trd lng="en">pinchbeck</trd>
  <trd lng="fr">chrysocale</trd>
  <trdgrp lng="hu">
    <trd>tombak</trd>,
    <trd>hamisarany</trd>
  </trdgrp>
  <trd lng="ja">&#x6A21;&#36896;&#x91D1;  <pr>&#12418;&#x305E;&#12358;&#x304B;&#x306D;</pr></trd>
  <trdgrp lng="nl">
    <trd>pinsbek</trd>,
    <trd>semilor</trd>
  </trdgrp>
  <trd lng="pl">tombak</trd>
  <trd lng="pt">crisocola</trd>
  <trd lng="ru">&c_t;&c_o;&c_m;&c_p;&c_a;&c_k;</trd>
  <trd lng="uk">&c_t;&#x43E;&#x43C;&#x43F;&c_a;&#x43A; (&c_s;&#x43F;&#x43B;&c_a;&c_v;)</trd>
</drv>
</art>
