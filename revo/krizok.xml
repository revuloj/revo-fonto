<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>krizokal</rad>/o
</kap>
<drv mrk="krizok.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Alojo el kupro, zinko, stano kaj plumbo, kiu imitas oron.
    </dif>
  </snc>
  <trd lng="de">Goldkupfer</trd>
  <trd lng="en">pinchbeck</trd>
  <trdgrp lng="hu">
    <trd>tombak</trd>,
    <trd>hamisarany</trd>
  </trdgrp>
  <trd lng="pt">crisocola</trd>
  <trd lng="ru">&c_t;&c_o;&c_m;&c_p;&c_a;&c_k;</trd>
</drv>
</art>
