<kap>
  <ofc>*</ofc>
  <rad>cigared</rad>/o
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="cigare.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Cilindreto da fum<ref tip="prt" cel="tabak.0o.fumado">tabako</ref>,
      envolvita en maldika papero:
      <ekz>
        E&ubreve;doksio rulis <tld/>on per siaj fingroj, flavi&gcirc;intaj pro la
        tabako
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 13a, p.
        70a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_g;&c_a;&c_r;&c_e;&c_t;&c_a;</trd>
  <trd lng="bo">&#3920;&#3851;&#3928;&#3906;&#3851;</trd>
  <trd lng="br">sigaretenn</trd>
  <trdgrp lng="ca">
    <trd>cigarreta</trd>,
    <trd>cigarret</trd>
  </trdgrp>
  <trd lng="cs">cigareta</trd>
  <trd lng="de">Zigarette</trd>
  <trd lng="el">&tau;&sigma;&jota;&gamma;&alfa_ton;&rho;&omikron;</trd>
  <trd lng="en">cigarette</trd>
  <trdgrp lng="es">
    <trd>cigarro</trd>, <trd>cigarrillo</trd>
  </trdgrp>
  <trd lng="fr">cigarette</trd>
  <trd lng="he">&samekh;&#1460;&yod;&gimel;&#1463;&resh;&#1460;&yod;&#1464;&he;</trd>
  <trd lng="hu">cigaretta</trd>
  <trd lng="it">sigaretta</trd>
  <trd lng="ja">&#32025;&#24059;&#12365;&#12383;&#12400;&#12371;  <pr>&#12363;&#12415;&#12414;&#12365;&#12383;&#12400;&#12371;</pr></trd>
  <trd lng="nl">sigaret</trd>
  <trd lng="pl">papieros</trd>
  <trd lng="pt">cigarro</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_i;&c_g;&c_a;&c_r;&c_je;&c_t;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_p;&c_i;&c_r;&c_o;&c_s;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">cigareta</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&c_gu;&c_a;&c_r;&c_je;&c_t;&c_a;</trd>,
    <trd>&c_c;&c_i;&c_g;&c_a;&c_r;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21367;&#28895; <pr>ju&#462;ny&#257;n</pr></trd>,
    <trd>&#28895;&#21367; <pr>y&#257;nju&#462;n</pr></trd>,
    <trd>&#33784; <pr>y&#257;n</pr></trd>,
    <trd>&#32025;&#29017; <pr>zh&#464;y&#257;n</pr></trd>,
    <trd>&#39321;&#28895; <pr>xi&#257;ngy&#257;n</pr></trd>,
    <trd>&#28895; <pr>y&#257;n</pr></trd>,
    <trd>&#28895;&#21367;&#20799; <pr>y&#257;nju&#462;nr</pr></trd>
  </trdgrp>
</drv>
</art>
