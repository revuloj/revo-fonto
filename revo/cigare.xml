<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cigare.xml,v 1.20 2023/08/31 10:10:26 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>cigared</rad>/o
</kap>
<drv mrk="cigare.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Cilindreto da fum<ref tip="prt" cel="tabak.0o.fumado">tabako</ref>,
      envolvita en maldika papero:
      <ekz>
        E&ubreve;doksio rulis <tld/>on per siaj fingroj, flavi&gcirc;intaj pro la
        tabako
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 13a, p.
        70a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_g;&c_a;&c_r;&c_e;&c_t;&c_a;</trd>
  <trd lng="bo">&#xF50;&#xF0B;&#xF58;&#xF42;&#xF0B;</trd>
  <trd lng="br">sigaretenn</trd>
  <trd lng="cs">cigareta</trd>
  <trd lng="de">Zigarette</trd>
  <trd lng="el">&tau;&sigma;&jota;&gamma;&alfa_ton;&rho;&omikron;</trd>
  <trd lng="en">cigarette</trd>
  <trdgrp lng="es">
    <trd>cigarro</trd>, <trd>cigarrillo</trd>
  </trdgrp>
  <trd lng="fr">cigarette</trd>
  <trd lng="he">&#x5E1;&#x5B4;&#x5D9;&#x5D2;&#x5B7;&#x5E8;&#x5B4;&#x5D9;&#x5B8;&#x5D4;</trd>
  <trd lng="hu">cigaretta</trd>
  <trd lng="it">sigaretta</trd>
  <trd lng="ja">&#x7D19;&#x5DFB;&#x304D;&#x305F;&#x3070;&#x3053;  [<ind>&#x304B;&#x307F;&#x307E;&#x304D;&#x305F;&#x3070;&#x3053;</ind>]</trd>
  <trd lng="nl">sigaret</trd>
  <trd lng="pl">papieros</trd>
  <trd lng="pt">cigarro</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_i;&c_g;&c_a;&c_r;&c_je;&c_t;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_p;&c_i;&c_r;&c_o;&c_s;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">cigareta</trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x438;&#x491;&#x430;&#x440;&#x435;&#x442;&#x430;</trd>,
    <trd>&#x446;&#x438;&#x433;&#x430;&#x440;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>
</art>
