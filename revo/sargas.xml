<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: sargas.xml,v 1.7 2023/07/20 21:23:28 revo Exp $">
    <kap>
      <rad>sargas</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="sargas.0o">
  <kap><tld/>o</kap>
  <snc mrk="sargas.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Multspecia <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">genro</ref> de maraj
      <ref tip="malprt" cel="alg.brun0oj">brunalgoj</ref> (<trd lng="la">Sargassum</trd>), kelkaj
      fiksitaj aliaj flosantaj en varmaj maroj, ofte kun aerhavaj na&gcirc;vezikoj:
      <ekz>
        la densaj amasoj de <tld/>oj havigas favoran medion por <klr>[&#8230;]</klr> maraj
        organismoj, multaj el kiuj ankora&ubreve; estas nekonataj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=261216">Sargaso</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la <ind>Maro <tld lit="S"/>a</ind>
        <klr>(parto de Atlantiko oriente de Florido fama pro tapi&scirc;oj de <tld/>o)</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Alexander Shlafer</aut>
          <vrk><url ref="&Monato;/2010/009749.php">Plia sukceso de Jacques Yvart</url></vrk>
          <lok>2010</lok>
        </fnt>.
        <trd lng="de">Sargassosee</trd>
        <trd lng="en">Sargasso Sea</trd>
        <trd lng="fr">mer des Sargasses</trd>
        <trd lng="pl">Morze <ind>Sargassowe</ind></trd>
      </ekz>
    </dif>
    <ref tip="vid" cel="fuk.0o">fuko</ref>
    <bld
    lok="&WCU;/thumb/4/41/Svenska_vetenskapsakademien_handlingar_(1888)_(14799366013).jpg/542px-Svenska_vetenskapsakademien_handlingar_(1888)_(14799366013).jpg"
    prm="PD">Botanika tabulo de <tld/>o (Sargassum hystrix kaj braciferum), Kungl. Svenska
      vetenskapsakademien,
        1739
        <fnt>
          <aut>Kungl. Svenska vetenskapsakademien,
          https://www.flickr.com/photos/internetarchivebookimages/14799366013/Source book page:
          https://archive.org/stream/svenskavetenskap23kung/svenskavetenskap23kung#page/n296/mode/1up</aut>
          <lok><url
          ref="&WCW;/File:Svenska_vetenskapsakademien_handlingar_(1888)_(14799366013).jpg">&WCW;/File:Svenska_vetenskapsakademien_handlingar_(1888)_(14799366013).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_r;&c_g;&c_a;&c_s;&c_y;</trd>,
    <trd>&c_s;&c_a;&c_r;&c_g;&c_a;&c_s;&c_a;&c_v;&c_y;&c_ja; &c_v;&c_o;&c_d;&c_a;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ib;</trd>
  </trdgrp>
  <trd lng="de">Golftang</trd>
  <trd lng="en">Sargassum</trd>
  <trd lng="fr">Sargasse</trd>
  <trd lng="pl">gronorost  <klr>(wodorost)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_a;&c_r;&c_g;&c_a;&c_s;&#x43E;</trd>,
    <trd>&c_s;&c_a;&c_r;&c_g;&c_a;&c_s;&c_u;&#x43C; (&c_v;&#x43E;&c_d;&#x43E;&c_r;&c_ib;&c_s;&c_t;&#x44C;)</trd>
  </trdgrp>
</drv>
</art>

