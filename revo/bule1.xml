<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bule1.xml,v 1.13 2018/11/13 05:10:15 revo Exp $">
<kap>
<rad>Bule</rad>/o
</kap>
<drv mrk="bule1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <fnt><bib>KompLeks</bib></fnt>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <dif>
      Anglalingve: George <trd lng="en">Boole</trd>. Angla
      matematikisto (1815&#173;&ndash;1864).
    </dif>
  </snc>
  <trd lng="be">&c_B;&c_u;&c_l;&c_mol;</trd>
  <trd lng="bg">&c_B;&c_u;&c_l;</trd>
  <trd lng="cs">Boole G.</trd>
  <trd lng="de">Boole</trd>
  <trd lng="fr">Boole</trd>
  <trd lng="hu">Boole</trd>
  <trd lng="it">Boole</trd>
  <trd lng="nl">George <ind>Boole</ind></trd>
  <trd lng="pl">Boole</trd>
  <trd lng="ru">&c_B;&c_u;&c_l;&c_mol;</trd>
  <trd lng="sk">Boole G.</trd>
</drv>

<drv mrk="bule1.b0a">
  <kap><tld lit="b"/>a</kap>
  <snc mrk="bule1.b0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <ref tip="vid" cel="algebr.bulea0o"><tld lit="b"/>a algebro</ref>
    <trdgrp lng="ru">
      <trd>&c_B;&c_u;&c_l;&c_ja;</trd>,
      <trd>&c_b;&c_u;&c_l;&c_je;&c_v;</trd>
    </trdgrp> 
  </snc>
  <snc mrk="bule1.b0a.KOMP">
  <uzo tip="fak">KOMP</uzo>
    <fnt><bib>KompLeks</bib></fnt>
    <fnt><bib>PIV2</bib></fnt>
    <dif>
      <ref cel="tip.datum0o">Datumtipo</ref> kies du nuraj valoroj
      estas <frm><k>vero</k></frm> kaj <frm><k>malvero</k></frm>,
      super kiuj estas difinitaj la <em><tld lit="b"/>aj operacioj</em>
      <klr>(tiuj de <ref cel="algebr.bulea0o"><tld lit="b"/>a
      algebro</ref>)</klr>,
      kaj el kiuj eblas fari <em><tld lit="b"/>ajn esprimojn</em>:
      <ekz>
         en <ref cel="c.0.KOMP">C</ref> la <tld lit="b"/>ajn valorojn
         oni prezentas per <frm>0</frm> kaj <frm>1</frm>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_b;&c_u;&c_l;&c_je;&c_v;</trd>
    </trdgrp> 
  </snc>
  <trdgrp lng="be">
    <trd>&c_B;&c_u;&c_l;&c_ja;</trd>,
    <trd>&c_b;&c_u;&c_l;&c_je;&c_w;</trd>,
    <trd>&c_b;&c_u;&c_l;&c_je;&c_w;&c_s;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="bg">&c_b;&c_u;&c_l;&c_je;&c_v;</trd>
  <trd lng="de">Boolesche</trd>
  <trdgrp lng="en">
    <trd>Boole['s]</trd>,
    <trd>Boolean</trd></trdgrp> 
  <trd lng="es">booleano</trd>
  <trdgrp lng="fr">
    <trd>de <ind>Boole</ind></trd>,
    <trd>bool&eacute;en</trd></trdgrp> 
  <trdgrp lng="hu">
    <trd>Boole-</trd>,
    <trd>Boole-f&eacute;le</trd></trdgrp>
  <trd lng="it">booleano</trd>
  <trd lng="nl">booleaans</trd>
  <trdgrp lng="pl">
    <trd>Boole'a</trd>
  </trdgrp>
  <trd lng="sk">boolovsk&aacute; <klr>(algebra)</klr></trd>
</drv>
</art>

