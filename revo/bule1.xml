<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>Bule</rad>/o
</kap>
<drv mrk="bule1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <dif>
      Anglalingve:
      George <trd lng="en">Boole</trd>, 1815-1864. Angla matematikisto.
    </dif>
  </snc>
  <trd lng="be">&c_B;&c_u;&c_l;&c_mol;</trd>
  <trd lng="de">Boole</trd>
  <trd lng="fr">Boole</trd>
  <trd lng="hu">Boole</trd>
  <trd lng="nl">George <ind>Boole</ind></trd>
  <trd lng="pl">Boole</trd>
  <trd lng="ru">&c_B;&c_u;&c_l;&c_mol;</trd>
</drv>

<drv mrk="bule1.b0a">
  <kap><tld lit="b"/>a</kap>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <refgrp tip="vid">
      <ref cel="algebr.bulea0o"><tld lit="b"/>a algebro</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_B;&c_u;&c_l;&c_ja;</trd>,
    <trd>&c_b;&c_u;&c_l;&c_je;&c_w;</trd>,
    <trd>&c_b;&c_u;&c_l;&c_je;&c_w;&c_s;&c_k;&c_ib;</trd>
  </trdgrp>
    <trd lng="de">Boolesche</trd>
    <trdgrp lng="en">
      <trd>Boole['s]</trd>,
      <trd>Boolean</trd></trdgrp> 
    <trd lng="es">booleano</trd>
    <trdgrp lng="fr">
      <trd>de <ind>Boole</ind></trd>,
      <trd>bool&eacute;en</trd></trdgrp> 
    <trdgrp lng="hu">
      <trd>Boole-</trd>,
      <trd>Boole-f&eacute;le</trd></trdgrp>
    <trd lng="nl">booleaans</trd>
    <trdgrp lng="pl">
      <trd>Boole'a</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_B;&c_u;&c_l;&c_ja;</trd>,
      <trd>&c_b;&c_u;&c_l;&c_je;&c_v;</trd></trdgrp> 
    <trd lng="sk">boolovsk&aacute; <klr>(algebra)</klr></trd>
</drv>
</art>

