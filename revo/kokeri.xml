<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kokerik</rad>/o
  <fnt>C</fnt>
</kap>
<drv mrk="kokeri.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Kokokrio. 
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>crow</trd>,
    <trd>cock-a-doodle-doo</trd>
  </trdgrp>
  <trd lng="nl">gekraai</trd>
  <trd lng="ru">&c_k;&c_u;&c_k;&c_a;&c_r;&c_je;&c_k;&c_u;</trd>
</drv>
<drv mrk="kokeri.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Krii (pp <ref tip="vid" cel="kok.0o">koko</ref>).
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>crow</trd>,
    <trd>cock-a-doodle-doo</trd>
  </trdgrp>
  <trd lng="nl">kraaien</trd>
  <trd lng="ru">&c_k;&c_u;&c_k;&c_a;&c_r;&c_je;&c_k;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
