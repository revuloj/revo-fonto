<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kokeri.xml,v 1.21 2020/09/09 06:10:35 revo Exp $">
<kap>
  <rad>kokerik</rad>/i <fnt><aut>P. Christaller</aut>,
  <vrk>Deutsch-Esperanto W&ouml;rterbuch</vrk>, <lok>1910</lok></fnt>,
  <var><kap><rad var="i">kikerik</rad>/i <fnt><bib>NePiv</bib></fnt></kap></var>,
  <var><kap><rad var="ii">kikeriki</rad>! <fnt><bib>ElektFab</bib></fnt></kap></var>
</kap>
<drv mrk="kokeri.0i">
  <kap><tld/>i, <var><kap><tld var="i"/>i</kap></var></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kokeri.0i.ZOO">
    <dif>
      <ref tip="super" cel="blek.0i.beste">Bleki</ref> kiel 
      <ref tip="vid" cel="kok.0o">koko</ref>:  
      <ekz>
        kiel pruvo de sia senkulpeco, la rostita koko <klr>[&#8230;]</klr> levi&gcirc;os kaj
        ek<tld/>os
        <fnt>
          <bib>Monato</bib>
          <aut>William Simcock</aut>
          <vrk><url ref="https://www.monato.be/2003/007873.html"
              >Impresoj pri Portugalio</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pentrita koko povis <tld/>i kaj la pentrita plugilo povis plugi teron...
        <fnt>
          <vrk><url
          ref="https://www.scribd.com/document/365423567/Malgranda-Stacidomo-Kaj-Aliaj-Mikronoveloj"
              >Malgranda Stacidomo Kaj Aliaj Mikronoveloj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li &ccirc;irka&ubreve;rigardis <klr>[&#8230;]</klr> <tld var="i"/>antan kokon, 
        saltantan korvon
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_k;&c_a;&c_r;&c_e;&c_k;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">kokrhat</trd>
  <trd lng="de">kr&auml;hen</trd>
  <trdgrp lng="en">
    <trd>crow</trd>,
    <trd>cock-a-doodle-doo</trd>
  </trdgrp>
  <trd lng="fa">&a_q;&a_w;&a_q;&a_w;&a_l;&f_y;&zwnj;&a_q;&a_w; &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>chanter<klr tip="ind"> (en p. d'un coq)</klr></trd>,
    <trd>faire <ind>cocorico</ind></trd>
  </trdgrp>
  <trd lng="hu">kukor&eacute;kol</trd>
  <trd lng="nl">kraaien<klr tip="ind"> (als een haan)</klr></trd>
  <trd lng="pl">pia&cacute;<klr tip="ind"> (o kogucie)</klr></trd>
  <trd lng="ru">&c_k;&c_u;&c_k;&c_a;&c_r;&c_je;&c_k;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">kikir&iacute;ka&tcaron;</trd>
</drv>

<drv mrk="kokeri.0">
  <kap>
    <tld/>o!, <var><kap><tld var="ii"/>!</kap></var>
  </kap>
  <gra><vspec>ekkrio</vspec></gra>
  <snc mrk="kokeri.0.ekkrio">
    <dif>
      Ekkrio de koko:
      <ekz>
        &ccirc;u vi a&ubreve;das kokokrion, malgranda <nom>Tuk</nom>? <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Malgranda Tuk</lok>
        </fnt>!
      </ekz>
      <ekz>
        <tld/>o! li kriis, la kokidoj balda&ubreve; fari&gcirc;os grandaj kokinoj, se mi tion
        proklamos en la monda kokinejo
        <fnt>
          <bib>Fab4</bib>
          <lok>Korta koko kaj ventkoko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld var="ii"/>! nia ora knabino estas tie &ccirc;i
        <fnt>
          <bib>ElektFab</bib>
          <lok>La maljunulino Holle</lok>
        </fnt>;
      </ekz>
      <ekz>
        la koko vekita de la bruo vigle ekkriis sur la trabo: <tld lit="K" var="ii"/>
        <fnt>
          <bib>ElektFab</bib>
          <lok>La bremenaj muzikistoj</lok>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Kikeriki!</trd>
</drv>

<drv mrk="kokeri.0o">
  <kap><tld/>o</kap>
  <snc mrk="kokeri.0o.ZOO">
    <dif>
      Kokokrio:
      <ekz>
        ili amba&ubreve; a&ubreve;dis <tld/>ojn
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 88a, volumo 3a, p.
        129a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_k;&c_a;&c_r;&c_e;&c_k;&c_u;</trd>
  <trd lng="cs">kokrh&aacute;n&iacute;</trd>
  <trdgrp lng="de">
    <trd>Hahnenschrei</trd>,
    <trd>Kikeriki</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>crow</trd>,
    <trd>cock-a-doodle-doo</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_q;&a_w;&a_q;&a_w;&a_l;&f_y;&zwnj;&a_q;&a_w;</trd>,
    <trd>&a_b;&a_A;&a_n;&f_g; &a_kh;&a_r;&a_w;&a_s;</trd>
  </trdgrp>
  <trd lng="fr">cocorico</trd>
  <trd lng="hu">kukor&eacute;kol&aacute;s</trd>
  <trd lng="nl">gekraai</trd>
  <trd lng="pl">pianie<klr tip="ind"> (koguta)</klr></trd>
  <trd lng="ru">&c_k;&c_u;&c_k;&c_a;&c_r;&c_je;&c_k;&c_u;</trd>
  <trd lng="sk">kikir&iacute;kanie</trd>
</drv>
</art>
