<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kokerik</rad>/i
  <fnt>C</fnt>
</kap>
<drv mrk="kokeri.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kokeri.0i.ZOO">
    <dif>
      <ref tip="super" cel="blek.0i.beste">Bleki</ref> kiel <ref tip="vid" cel="kok.0o">koko</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_k;&c_a;&c_r;&c_e;&c_k;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">kr&auml;hen</trd>
  <trdgrp lng="en">
    <trd>crow</trd>,
    <trd>cock-a-doodle-doo</trd>
  </trdgrp>
  <trd lng="fr">chanter<klr> (coq)</klr></trd>
  <trd lng="nl">kraaien</trd>
  <trd lng="ru">&c_k;&c_u;&c_k;&c_a;&c_r;&c_je;&c_k;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="kokeri.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Kokokrio:
      <ekz>
        ili amba&ubreve; audis <tld/>ojn
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 88a, volumo 3a, p. 129a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_k;&c_a;&c_r;&c_e;&c_k;&c_u;</trd>
  <trdgrp lng="de">
    <trd>Hahnenschrei</trd>,
    <trd>Kikeriki</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>crow</trd>,
    <trd>cock-a-doodle-doo</trd>
  </trdgrp>
  <trd lng="fr">cocorico</trd>
  <trd lng="nl">gekraai</trd>
  <trd lng="ru">&c_k;&c_u;&c_k;&c_a;&c_r;&c_je;&c_k;&c_u;</trd>
</drv>
</art>
