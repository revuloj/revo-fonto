<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: marak.xml,v 1.7 2024/01/19 06:28:33 revo Exp $">
<kap>
  <rad>marak</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="marak.0o">
  <kap><tld/>o</kap>
  <snc mrk="marak.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Kubdevena <ref tip="lst" lst="voko:muzikiloj" cel="muzik.0ilo.muzikinstrumento">muzikilo</ref>,
      eta sfero el kukurbo, ligno a&ubreve; metalo,
      entenanta grenojn a&ubreve; &scirc;tonetojn kaj provizita per tenilo,
      kiun oni skuas:
      <ekz>
        ru&gcirc;vestitaj viroj kun grandiozaj maskoj surkape vigle dancas
        anta&ubreve; la pre&gcirc;ejo kaj per <tld/>oj kaj sonoriletoj bruas
        <fnt>
          <aut>L. Gabalda</aut>,
          <vrk>Venezuelo &ndash; kutimoj kaj tradicio</vrk>,
          <lok><url ref="http://www.hary.cz/AEH//Ant-1-09.html">
          Anta&ubreve;en, 2009-03-01 &gcirc;is 17</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_a;&c_k;&c_a;&c_s;&c_y;</trd>
  <trd lng="ca">maraca</trd>
  <trdgrp lng="de">
    <trd>Maraca</trd>,
    <trd>Rumbakugel</trd>
  </trdgrp>
  <trd lng="fr">maracas</trd>
  <trd lng="nl">maracas <klr>(instrument)</klr></trd>
  <trd lng="pl">marakasy {pl}</trd>
  <trd lng="ro">maracas <klr>(instrument)</klr></trd>
  <trd lng="uk">&c_m;&c_a;&c_r;&c_a;&c_k;&c_a;&c_s; (&c_u;&c_d;&c_a;&c_r;&c_n;&c_i;&c_j; &c_ib;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;)</trd>
</drv>
</art>

