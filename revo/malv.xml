<art mrk="$Id: malv.xml,v 1.27 2022/03/23 14:01:05 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>malv</rad>/o
</kap>
<drv mrk="malv.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="malvac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Malva</trd>), kun rozviolaj floroj:
      <ekz>
        la junaj folioj de <tld/>o, kun milda gusto, estas bona
        anstata&ubreve;anto de laktuko, dum la pli malnovaj folioj estas
        pli bonaj kiam kuiritaj, floroj estas vaste uzataj en salatoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=287550">Malvo
          (floro)</url></lok>
        </fnt>;
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/a/ad/Malva_sylvestris2.jpg/446px-Malva_sylvestris2.jpg" prm="CC-BY-SA-3.0"><tld/>o (Malva sylvestris var. mauritiana)

         <fnt>
           <aut>Kenpei, propra foto</aut>
           <lok><url ref="&WCW;/File:Malva_sylvestris2.jpg">&WCW;/File:Malva_sylvestris2.jpg</url></lok>
         </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_mol;&c_v;&c_a;</trd>
  <trd lng="br">malvenn<klr tip="ind">(bleunienn)</klr></trd>
  <trd lng="ca">malva</trd>
  <trd lng="de">Malve</trd>
  <trd lng="en">mallow</trd>
  <trd lng="fr">mauve</trd>
  <trd lng="hu">m&aacute;lyva</trd>
  <trdgrp lng="nl"><trd>malva</trd>,
    <trd>groot <ind>kaasjeskruid</ind></trd>
  </trdgrp>
  <trd lng="pl">&sacute;laz</trd>  
  <trd lng="pt">malva</trd>
  <trdgrp lng="ro">
    <trd>nalb&abreve;-mare</trd>,
    <trd>m&abreve;l&abreve;u&#x21B;</trd>,
    <trd>col&abreve;ceii-babei <klr>(pl.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_l;&c_mol;&c_v;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_v;&c_i;&c_r;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sv">malva</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x430;&#x43B;&#x430;&#x447;&#x438;&#x43A;&#x438;</trd>,
    <trd>&#x43C;&#x430;&#x43B;&#x44C;&#x432;&#x430;</trd>,
    <trd>&#x43F;&#x440;&#x43E;&#x441;&#x43A;&#x443;&#x440;&#x43D;&#x44F;&#x43A;</trd>
  </trdgrp>
</drv>
</art>
