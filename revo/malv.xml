<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>malv</rad>/o
</kap>
<drv mrk="malv.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="malv.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Malva</trd>), kun rozviolaj floroj.
    </dif>
  </snc>
  <bld lok="../bld/malv.jpg">
    <tld/>ofloroj <klr>(Malva sylvestris)</klr>
  </bld>
  <trd lng="de">Malve</trd>
  <trd lng="fr">mauve</trd>
</drv>
<drv mrk="malv.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Malvaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Malvengew&auml;chse</trd>
</drv>
</art>
