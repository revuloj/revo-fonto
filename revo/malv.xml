<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>malv</rad>/o
</kap>
<drv mrk="malv.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="malvac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Malva</trd>), kun rozviolaj floroj.
    </dif>
  </snc>
  <bld lok="../bld/malv.jpg">
    <tld/>ofloroj <klr>(Malva sylvestris)</klr>
  </bld>
  <trd lng="de">Malve</trd>
  <trd lng="fr">mauve</trd>
  <trd lng="nl">malva</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_l;&c_mol;&c_v;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_v;&c_i;&c_r;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sv">malva</trd>
</drv>
</art>
