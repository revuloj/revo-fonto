<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>silent</rad>/i
</kap>
<drv mrk="silent.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="silent.0i.malparoli">
    <dif>
      Ne paroli:
      <ekz>
        homo prudenta <tld/>as<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>u, ne movu la bu&scirc;on kaj ne ekpepu<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mut.0a">muta</ref>.
    </refgrp>
    <trd lng="de">schweigen</trd>
    <trdgrp lng="fr">
      <trd>se <ind>taire</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="silent.0i.malbrui">
    <dif>
      Ne brui:
      <ekz>
        se <tld/>as dra&scirc;ejo, malpacas lo&gcirc;ejo
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="bru.0i.soni">brui</ref>
    </refgrp>
    <trd lng="de">still sein</trd>
    <trdgrp lng="fr">
      <trd>faire <ind>silence</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="silent.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Sin ne montri; ne plu efiki; &ccirc;esi:
      <ekz>
        kie regas la forto, tie la rajto <tld/>as<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kontra&ubreve; nehavo e&ccirc; ju&gcirc;o <tld/>as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        anta&ubreve; tiu granda nacia verko <tld/>is &ccirc;iuj internaj malpacoj kaj
        kvereloj;
      </ekz>
      <ekz>
        niaj personaj gustoj devas absolute <tld/>i<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>as nun &ccirc;iu disputo religia a&ubreve; politika<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>se <ind>taire</ind><klr> (fig. : cesser)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="cs">ml&ccaron;et</trd>
  <trd lng="en">silent</trd>
  <trdgrp lng="nl">
    <trd>zwijgen</trd>,
    <trd>stil zijn</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_o;&c_l;&c_ch;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="silent.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Stato de iu a&ubreve; io, kiu <tld/>as:
      <ekz>
        regis tomba <tld/>o<fnt>B</fnt>;
      </ekz>
      <ekz>
        paca <tld/>o, ke ne blovas e&ccirc; vento
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>o estas konsento
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        parolo estas ar&gcirc;ento, oron similas <tld/>o
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="romp.0i">rompi</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Stille</trd>
  <trdgrp lng="fr">
    <trd>silence</trd>
  </trdgrp>
  <trd lng="nl">stilte</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_l;&c_ch;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_t;&c_i;&c_sh;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="silent.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="S"/>anta:
      <ekz>
        <tld/>a interkonsento<fnt>Z</fnt>;
      </ekz>
      <ekz>
        fari&gcirc;is <tld/>e<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kvaza&ubreve; <tld/>e komisiita, mi vokas al &ccirc;iuj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">still</trd>
  <trdgrp lng="fr">
    <trd>silencieux</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>stil</trd>,
    <trd>stilzwijgend</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_l;&c_ch;&c_a;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_m;&c_o;&c_l;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_sh;&c_u;&c_m;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_i;&c_h;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="silent.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <dif>
      Kutimanta <tld/>i:
      <ekz>
        pri sia persono li estas &ccirc;iam <tld/>ema<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>discret<klr> (silencieux)</klr></trd>
  </trdgrp>
  <trd lng="nl">zwijgzaam</trd>
  <trd lng="ru">&c_m;&c_o;&c_l;&c_ch;&c_a;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
</drv>

<drv mrk="silent.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Devigi iun a&ubreve; ion <tld/>i:
      <ekz>
        <tld/>igi la envion kaj kalumnion.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>faire <ind>taire</ind></trd>,
    <trd>r&eacute;duire au <ind>silence</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol; <ind>&c_z;&c_a;&c_m;&c_o;&c_l;&c_ch;&c_a;&c_t;&c_mol;</ind></trd>,
    <trd>&c_z;&c_a;&c_g;&c_l;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="silent.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ne paroli pri io:
      <ekz>
        pri<tld/>i sekreton, amon.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">verschweigen</trd>
  <trdgrp lng="fr">
    <trd>passer sous <ind>silence</ind></trd>,
    <trd>taire<klr> (quelque chose)</klr></trd>
  </trdgrp>
  <trd lng="nl">verzwijgen</trd>
  <trd lng="ru">&c_z;&c_a;&c_m;&c_a;&c_l;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
