  <rad>silent</rad>/i
</kap>

<drv mrk="silent.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="silent.0i.malparoli">
    <dif>
      Ne <ref tip="ant" cel="parol.0i.diri">paroli</ref>:
      <ekz>
        estas tempo por <tld/>i, kaj tempo por paroli
        <fnt>
          <bib>MT</bib>
          <lok>Predikanto 3:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        la popolo <tld/>is kaj nenion respondis
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 18:36</lok>
        </fnt>;
      </ekz>
      <ekz>
        homo prudenta <tld/>as
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 11:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kamparano ek<tld/>is: li rimarkis la proksimi&gcirc;antan sekvantaron de la ministro
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        se ne &ccirc;eestas interpretanto, oni <tld/>u en la eklezio
        <fnt>
          <bib>NT</bib>
          <lok>I. Korintanoj 14:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u, &gcirc;is mi finos!
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>!
      </ekz>
      <ekz>
        mastro elbabilis, gastoj ne <tld/>os
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu pri &scirc;telo <tld/>as, tiu &scirc;telon konsentas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        batanto povas argumenti, batato devas <tld/>i
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mut.0a">muta</ref>.
    </refgrp>
    <trd lng="es">callarse</trd>
    <trdgrp lng="fr">
      <trd>se <ind>taire</ind></trd>
    </trdgrp>
    <trd lng="hu">hallgat</trd>
  <trdgrp lng="id">
    <trd>bungkam</trd>,
    <trd>diam</trd>
  </trdgrp>
  </snc>
  <snc mrk="silent.0i.malbrui">
    <dif>
      Ne <ref tip="ant" cel="bru.0i.soni">brui</ref>:
      <ekz>
        la pre&gcirc;ejaj sonoriloj sonoris por festo kaj &gcirc;ojo, nur unu sonorilo <tld/>is
        <fnt>
          <bib>Fab4</bib>
          <lok>Malnova pre&gcirc;eja sonorilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu venteto kvieti&gcirc;is, &ccirc;iu birdo <tld/>is
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de la jaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la granda plumba pezilo atingis la plankon, kaj bum! anka&ubreve; la horlo&gcirc;o ek<tld/>is
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de unu patrino</lok>
        </fnt>;
      </ekz>
      <ekz>
        se <tld/>as dra&scirc;ejo, malpacas lo&gcirc;ejo
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>faire <ind>silence</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>csendes</trd>,
      <trd>csendben van</trd>
    </trdgrp>
  <trdgrp lng="id">
    <trd>hening</trd>,
    <trd>lengang</trd>,
    <trd>senyap</trd>,
    <trd>sepi</trd>,
    <trd>sunyi</trd>,
    <trd>tenang</trd>
  </trdgrp>
  </snc>
  <snc mrk="silent.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Sin ne <ref cel="montr.0i.prezenti">montri</ref>;
      ne plu efiki; <ref tip="sin" cel="halt.0i">halti</ref>:
      <ekz>
        <tld/>as nun &ccirc;iu disput' religia
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista en Boulogne sur Mer en la 5a de a&ubreve;gusto
          1905</vrk>
        </fnt>;
      </ekz>
      <ekz>
        vantaj senka&ubreve;zaj gustoj naciaj devas en afero internacia <klr>[&#x2026;]</klr> absolute
        silenti
        <fnt>
          <bib>LR</bib>
          <lok>Unua Parto &ndash; Leksikologio</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bendo abrupte <tld/>is <klr>[&#x2026;]</klr> io teknike fu&scirc;i&gcirc;is
        <fnt>
          <bib>CKv</bib>
        </fnt>;
      </ekz>
      <ekz>
        kie regas la forto, tie rajto <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        cent jarojn <tld/>is kaj subite sin prezentis
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kontra&ubreve; nehavo e&ccirc; ju&gcirc;o <tld/>as
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>se <ind>taire</ind><klr> (fig. : cesser)</klr></trd>
    </trdgrp>
    <trd lng="hu">hallgat</trd>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_w;&c_ch;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_m;&c_malmol;&c_l;&c_ch;&c_a;</trd>
  <trd lng="bo">&t_kha;&t_tsheg;&t_kha;&t_ra;&t_tsheg;&t_sa;&t_data;&t_naro;&t_da;&t_tsheg;</trd>
  <trd lng="cs">ml&ccaron;et</trd>
  <trdgrp lng="de">
    <trd>schweigen</trd>,
    <trd><ind>still</ind> sein</trd>
  </trdgrp>
  <trd lng="en">silent</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_A;&f_k;&a_t; &a_b;&a_w;&a_d;&a_n;</trd>,
    <trd>&a_s;&f_k;&a_w;&a_t; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x9ED9;&#x3063;&#x3066;&#x3044;&#x308B;  <pr>&#x3060;&#x307E;&#x3063;&#x3066;&#x3044;&#x308B;</pr></trd>,
    <trd>&#x53E3;&#x3092;&#x3064;&#x3050;&#x3080;  <pr>&#x304F;&#x3061;&#x3092;&#x3064;&#x3050;&#x3080;</pr></trd>,
    <trd>&#x9759;&#x304B;&#x306B;&#x3057;&#x3066;&#x3044;&#x308B;  <pr>&#x3057;&#x305A;&#x304B;&#x306B;&#x3057;&#x3066;&#x3044;&#x308B;</pr></trd>,
    <trd>&#x97F3;&#x3092;&#x7ACB;&#x3066;&#x306A;&#x3044;  <pr>&#x304A;&#x3068;&#x3092;&#x305F;&#x3066;&#x306A;&#x3044;</pr></trd>,
    <trd>&#x4F11;&#x6B62;&#x3057;&#x3066;&#x3044;&#x308B;  <pr>&#x304D;&#x3085;&#x3046;&#x3057;&#x3057;&#x3066;&#x3044;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zwijgen</trd>,
    <trd>stil zijn</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>by&cacute;  <ind>cicho</ind></trd>,
    <trd>cichn&aogonek;&cacute;</trd>,
    <trd>milcze&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_o;&c_l;&c_ch;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>ml&ccaron;a&tcaron;</trd>,
    <trd>&ccaron;u&scaron;a&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&#x43C;&#x43E;&c_v;&c_ch;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x6C89;&#x9ED8; <pr>ch&#xE9;nm&#xF2;</pr></trd>,
    <trd>&#x4E0D;&#x51FA;&#x58F0; <pr>b&#xF9;ch&#x16B;sh&#x113;ng</pr></trd>,
    <trd>&#x4E0D;&#x51FA;&#x8072; <pr>b&#xF9;ch&#x16B;sh&#x113;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="silent.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Stato de iu a&ubreve; io, kiu <tld/>as:
      <ekz>
        kiam fari&gcirc;is granda <tld/>o, li parolis al ili en la Hebrea lingvo
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 21:40</lok>
        </fnt>;
      </ekz>
      <ekz>
        estis nokto, kaj &ccirc;ie regis <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>An&gcirc;elo</lok>
        </fnt>;
      </ekz>
      <ekz>
        sankta <tld/>o disvasti&gcirc;is en la arbaro
        <fnt>
          <bib>Fab2</bib>
          <lok>Sonorilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        momento da <tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>tomba <tld/>o</ind> regas super tiu urbo
        <fnt>
          <bib>Fab2</bib>
          <lok>Malgranda Tuk</lok>
        </fnt>;
        <trd lng="de">Grabesstille</trd>
      </ekz>
      <ekz>
        la <tld/>on interrompis ekkrio
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o kaj tondro alternis
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de la jaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>o estas konsento</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="de"><ind>Schweigen</ind> ist Zustimmung</trd>
        <trd lng="hu">a hallgat&aacute;s f&eacute;l beleegyez&eacute;s</trd>
      </ekz>
      <ekz>
        paca <tld/>o, ke ne blovas e&ccirc; vento
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>parolo estas ar&gcirc;ento, oron similas <tld/>o</ind>
        <fnt><bib>PrV</bib></fnt>
        <trd lng="de">Reden ist Silber, <ind>Schweigen</ind> ist Gold</trd>
        <trd lng="hu">besz&eacute;lni ez&uuml;st, hallgatni arany</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kviet.0o">kvieto</ref>,
      <ref cel="trankv.0o">trankvilo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_w;&c_ch;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_c;&c_ib;&c_sh;&c_y;&c_n;&c_ja;</trd>
  </trdgrp>
  <trd lng="bo">&t_kha;&t_shabchu;&t_tsheg;&t_sa;&t_gigu;&t_ma;&t_tsheg;&t_sa;&t_gigu;&t_ma;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>hud. pomlka</trd>,
    <trd>ml&ccaron;en&iacute;</trd>,
    <trd>silentium</trd>,
    <trd>ticho</trd>
  </trdgrp>
  <trd lng="de">Stille</trd>
  <trd lng="en">silence</trd>
  <trd lng="es">silencio</trd>
  <trd lng="fa">&a_s;&f_k;&a_w;&a_t;</trd>
  <trdgrp lng="fr">
    <trd>silence</trd>
  </trdgrp>
  <trd lng="hu">csend</trd>
  <trdgrp lng="id">
    <trd><baz>hening</baz>keheningan</trd>,
    <trd><baz>lengang</baz>kelengangan</trd>,
    <trd><baz>senyap</baz>kesenyapan</trd>,
    <trd><baz>sepi</baz>kesepian</trd>,
    <trd><baz>sunyi</baz>kesunyian</trd>,
    <trd><baz>tenang</baz>ketenangan</trd>
  </trdgrp>
  <trd lng="it">silenzio</trd>
  <trdgrp lng="ja">
    <trd>&#x6C88;&#x9ED9;  <pr>&#x3061;&#x3093;&#x3082;&#x304F;</pr></trd>,
    <trd>&#x7121;&#x8A00;  <pr>&#x3080;&#x3054;&#x3093;</pr></trd>,
    <trd>&#x9759;&#x5BC2;  <pr>&#x3057;&#x3058;&#x307E;</pr></trd>,
    <trd>&#x9759;&#x3051;&#x3055;  <pr>&#x3057;&#x305A;&#x3051;&#x3055;</pr></trd>,
    <trd>&#x4F11;&#x6B62;  <pr>&#x304D;&#x3085;&#x3046;&#x3057;</pr></trd>
  </trdgrp>
  <trd lng="ne">&#x92E;&#x94C;&#x928;</trd>
  <trd lng="nl">stilte</trd>
  <trdgrp lng="pl">
    <trd>cisza</trd>,
    <trd>bezruch</trd>,
    <trd>spok&oacute;j</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_l;&c_ch;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_t;&c_i;&c_sh;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">ml&ccaron;anie</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x43E;&c_v;&c_ch;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_t;&c_i;&c_sh;&c_a;</trd>,
    <trd>&c_s;&#x43F;&#x43E;&#x43A;&c_ib;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="silent.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="S"/>anta:
      <ekz>
        mi estis <ref cel="mut.0a.dummomenta">muta</ref> kaj silenta, mi <tld/>is e&ccirc; pri bono
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 39:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        originala infano, <tld/>a kaj pensanta
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj stellumaj noktoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Rozo de la tombo de Homero</lok>
        </fnt>;
      </ekz>
      <ekz>
        la maro estas glata kiel spegulo kaj <tld/>a
        <fnt>
          <bib>Fab2</bib>
          <lok>Bildo el kastelremparo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a amo
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>a interkonsento</ind>
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de la jaro</lok>
        </fnt>;
        <trd lng="de"><ind>schweigen</ind>de &Uuml;bereinkunft</trd>
        <trd lng="hu">hallgat&oacute;lagos meg&aacute;llapod&aacute;s</trd>
      </ekz>
      <ekz>
        la malgranda esta&jcirc;o momenton ripozis en <tld/>a feli&ccirc;o
        <fnt>
          <bib>Fab4</bib>
          <lok>Lasta son&gcirc;o de maljuna kverko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <ind><tld/>aj fulmoj</ind> ekbrilis pli kaj pli malofte
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
        <trd lng="de">Wetterleuchten</trd>
      </ekz>
      <ekz>
        vi revenos? ree li kapklinis, meditante pri la signifo de tiu <tld/>a promeso
        <fnt>
          <bib>HsT</bib>
        </fnt>;
      </ekz>
      <ekz>
        kian sencon do havas &scirc;ia <tld/>a lukto por savi la saveblan
        <fnt>
          <bib>HsT</bib>
        </fnt>?
      </ekz>
      <ekz>
        kvaza&ubreve; <tld/>e komisiita <klr>[&#x2026;]</klr>, mi vokas al
        &ccirc;iuj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Tria Kongreso Esperantista en Cambridge en la 12a de a&ubreve;gusto
          1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        akvo <tld/>a subfosas la bordon
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne timu hundon bojantan, timu hundon <tld/>an
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kviet.0a">kvieta</ref>,
      <ref cel="lauxt.mal0a">malla&ubreve;ta</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_w;&c_k;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_b;&c_ja;&c_z;&c_g;&c_u;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_ja;&c_z;&c_m;&c_o;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_ja;&c_s;&c_sh;&c_u;&c_m;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_m;&c_o;&c_w;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_c;&c_ib;&c_h;&c_ib;</trd>
  </trdgrp>
  <trd lng="bo">&t_kha;&t_shabchu;&t_tsheg;&t_sa;&t_gigu;&t_ma;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>ml&ccaron;&iacute;c&iacute;</trd>,
    <trd>tich&yacute;</trd>
  </trdgrp>
  <trd lng="de">still</trd>
  <trd lng="es">silencioso</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_A;&f_k;&a_t;</trd>,
    <trd>&a_kh;&a_A;&a_m;&a_w;&a_sh;</trd>,
    <trd>&a_b;&f_y;&#x200C;&a_S;&a_d;&a_A;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>silencieux</trd>
  </trdgrp>
  <trd lng="hu">csendes</trd>
  <trdgrp lng="id">
    <trd>diam</trd>,
    <trd>hening</trd>,
    <trd>lengang</trd>,
    <trd>senyap</trd>,
    <trd>sepi</trd>,
    <trd>sunyi</trd>,
    <trd>tenang</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6C88;&#x9ED9;&#x3057;&#x305F;  <pr>&#x3061;&#x3093;&#x3082;&#x304F;&#x3057;&#x305F;</pr></trd>,
    <trd>&#x7121;&#x8A00;&#x306E;  <pr>&#x3080;&#x3054;&#x3093;&#x306E;</pr></trd>,
    <trd>&#x9759;&#x304B;&#x306A;  <pr>&#x3057;&#x305A;&#x304B;&#x306A;</pr></trd>,
    <trd>&#x97F3;&#x3092;&#x7ACB;&#x3066;&#x306A;&#x3044;  <pr>&#x304A;&#x3068;&#x3092;&#x305F;&#x3066;&#x306A;&#x3044;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>stil</trd>,
    <trd>stilzwijgend</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>cichy</trd>,
    <trd>g&lstroke;uchy</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_l;&c_ch;&c_a;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_m;&c_o;&c_l;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_sh;&c_u;&c_m;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_i;&c_h;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ml&ccaron;iaci</trd>,
    <trd>tich&yacute;</trd>
