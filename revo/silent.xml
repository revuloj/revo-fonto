<drv mrk="silent.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Devigi iun a&ubreve; ion <tld/>i:
      <ekz>
        liajn gajajn &scirc;ercojn kaj viglan babiladon nek la malsatojaroj nek la maljuneco
        povis <tld/>igi
        <fnt>
          <bib>HsT</bib>
        </fnt>;
      </ekz>
      <ekz>
        malla&ubreve;taj kaj rapide <tld/>igataj ekridoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mistero, <tld/>igita tra eternaj tempoj
        <fnt>
          <bib>NT</bib>
          <lok>Romanoj 16:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi <tld/>igu la nesciadon de homoj malsa&gcirc;aj
        <fnt>
          <bib>NT</bib>
          <lok>I. Petro 2:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i denove venkis sin mem, &scirc;i forpremis kaj <tld/>igis la &scirc;in turmentantajn
        honton,
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        min <tld/>igas via nuda glavo
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        superrega kordoloro <tld/>igis unu, kaj respekta kunsento la alian
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_m;&c_u;&c_s;&c_ib;&c_c;&c_mol;
&c_m;&c_a;&c_w;&c_ch;&c_a;&c_c;&c_mol;</trd>,
