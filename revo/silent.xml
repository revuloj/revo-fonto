<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: silent.xml,v 1.26 2022/01/18 18:35:51 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>silent</rad>/i
</kap>

<drv mrk="silent.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="silent.0i.malparoli">
    <dif>
      Ne <ref tip="ant" cel="parol.0i.diri">paroli</ref>:
      <ekz>
        estas tempo por <tld/>i, kaj tempo por paroli
        <fnt>
          <bib>MT</bib>
          <lok>Predikanto 3:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        la popolo <tld/>is kaj nenion respondis
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 18:36</lok>
        </fnt>;
      </ekz>
      <ekz>
        homo prudenta <tld/>as
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 11:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kamparano ek<tld/>is: li rimarkis la proksimi&gcirc;antan sekvantaron de la ministro
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        se ne &ccirc;eestas interpretanto, oni <tld/>u en la eklezio
        <fnt>
          <bib>NT</bib>
          <lok>I. Korintanoj 14:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u, &gcirc;is mi finos!
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>!
      </ekz>
      <ekz>
        mastro elbabilis, gastoj ne <tld/>os
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu pri &scirc;telo <tld/>as, tiu &scirc;telon konsentas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        batanto povas argumenti, batato devas <tld/>i
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mut.0a">muta</ref>.
    </refgrp>    
    <trd lng="es">callarse</trd>
    <trdgrp lng="fr">
      <trd>se <ind>taire</ind></trd>
    </trdgrp>
    <trd lng="hu">hallgat</trd>
  </snc>
  <snc mrk="silent.0i.malbrui">
    <dif>
      Ne <ref tip="ant" cel="bru.0i.soni">brui</ref>:
      <ekz>
        la pre&gcirc;ejaj sonoriloj sonoris por festo kaj &gcirc;ojo, nur unu sonorilo <tld/>is
        <fnt>
          <bib>Fab4</bib>
          <lok>Malnova pre&gcirc;eja sonorilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu venteto kvieti&gcirc;is, &ccirc;iu birdo <tld/>is
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de la jaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la granda plumba pezilo atingis la plankon, kaj bum! anka&ubreve; la horlo&gcirc;o ek<tld/>is
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de unu patrino</lok>
        </fnt>;
      </ekz>
      <ekz>
        se <tld/>as dra&scirc;ejo, malpacas lo&gcirc;ejo
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>    
    <trdgrp lng="fr">
      <trd>faire <ind>silence</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>csendes</trd>,
      <trd>csendben van</trd>
    </trdgrp>
  </snc>
  <snc mrk="silent.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Sin ne <ref cel="montr.0i.prezenti">montri</ref>; 
      ne plu efiki; <ref tip="sin" cel="halt.0i">halti</ref>:
      <ekz>
        <tld/>as nun &ccirc;iu disput' religia
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista en Boulogne sur Mer en la 5a de a&ubreve;gusto 1905</vrk>
        </fnt>;
      </ekz>
      <ekz>
        vantaj senka&ubreve;zaj gustoj naciaj devas en afero internacia <klr>[&#8230;]</klr> absolute
        silenti
        <fnt>
          <bib>LR</bib>
          <lok>Unua Parto &ndash; Leksikologio</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bendo abrupte <tld/>is <klr>[&#8230;]</klr> io teknike fu&scirc;i&gcirc;is
        <fnt>
          <bib>CKv</bib>
        </fnt>;
      </ekz>
      <ekz>
        kie regas la forto, tie rajto <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        cent jarojn <tld/>is kaj subite sin prezentis
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kontra&ubreve; nehavo e&ccirc; ju&gcirc;o <tld/>as
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>se <ind>taire</ind><klr> (fig. : cesser)</klr></trd>
    </trdgrp>
    <trd lng="hu">hallgat</trd>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_w;&c_ch;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_m;&c_malmol;&c_l;&c_ch;&c_a;</trd>
  <trd lng="bo">&t_kha;&t_tsheg;&t_kha;&t_ra;&t_tsheg;&t_sa;&t_data;&t_naro;&t_da;&t_tsheg;</trd>
  <trd lng="cs">ml&ccaron;et</trd>
  <trdgrp lng="de">
    <trd>schweigen</trd>,
    <trd><ind>still</ind> sein</trd>
  </trdgrp>
  <trd lng="en">silent</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_A;&f_k;&a_t; &a_b;&a_w;&a_d;&a_n;</trd>,
    <trd>&a_s;&f_k;&a_w;&a_t; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zwijgen</trd>,
    <trd>stil zijn</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_o;&c_l;&c_ch;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>ml&ccaron;a&tcaron;</trd>,
    <trd>&ccaron;u&scaron;a&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="silent.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Stato de iu a&ubreve; io, kiu <tld/>as:
      <ekz>
        regis tomba <tld/>o<fnt>B</fnt>;
      </ekz>
      <ekz>
        paca <tld/>o, ke ne blovas e&ccirc; vento
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind><tld/>o estas konsento</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">a hallgat&aacute;s f&eacute;l
beleegyez&eacute;s</trd>
      </ekz>
      <ekz>
        <ind>parolo estas ar&gcirc;ento, oron similas <tld/>o</ind>
        <fnt><bib>PrV</bib></fnt>
        <trd lng="hu">besz&eacute;lni ez&uuml;st, hallgatni arany</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="romp.0i">rompi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_w;&c_ch;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_c;&c_ib;&c_sh;&c_y;&c_n;&c_ja;</trd>
  </trdgrp>
  <trd lng="bo">&t_kha;&t_shabchu;&t_tsheg;&t_sa;&t_gigu;&t_ma;&t_tsheg;&t_sa;&t_gigu;&t_ma;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>hud. pomlka</trd>,
    <trd>ml&ccaron;en&iacute;</trd>,
    <trd>silentium</trd>,
    <trd>ticho</trd>
  </trdgrp>
  <trd lng="de">Stille</trd>
  <trd lng="en">silence</trd>
  <trd lng="es">silencio</trd>
  <trd lng="fa">&a_s;&f_k;&a_w;&a_t;</trd>
  <trdgrp lng="fr">
    <trd>silence</trd>
  </trdgrp>
  <trd lng="hu">csend</trd>
  <trd lng="it">silenzio</trd>
  <trd lng="ne">&#2350;&#2380;&#2344;</trd>
  <trd lng="nl">stilte</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_l;&c_ch;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_t;&c_i;&c_sh;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">ml&ccaron;anie</trd>
</drv>

<drv mrk="silent.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="S"/>anta:
      <ekz>
        <ind><tld/>a interkonsento</ind><fnt>Z</fnt>;
        <trd lng="hu">hallgat&oacute;lagos meg&aacute;llapod&aacute;s</trd>
      </ekz>
      <ekz>
        fari&gcirc;is <tld/>e<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kvaza&ubreve; <tld/>e komisiita, mi vokas al
&ccirc;iuj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_w;&c_k;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_b;&c_ja;&c_z;&c_g;&c_u;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_ja;&c_z;&c_m;&c_o;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_ja;&c_s;&c_sh;&c_u;&c_m;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_m;&c_o;&c_w;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_c;&c_ib;&c_h;&c_ib;</trd>
  </trdgrp>
  <trd lng="bo">&t_kha;&t_shabchu;&t_tsheg;&t_sa;&t_gigu;&t_ma;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>ml&ccaron;&iacute;c&iacute;</trd>,
    <trd>tich&yacute;</trd>
  </trdgrp>
  <trd lng="de">still</trd>
  <trd lng="es">silencioso</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_A;&f_k;&a_t;</trd>,
    <trd>&a_kh;&a_A;&a_m;&a_w;&a_sh;</trd>,
    <trd>&a_b;&f_y;&zwnj;&a_S;&a_d;&a_A;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>silencieux</trd>
  </trdgrp>
  <trd lng="hu">csendes</trd>
  <trdgrp lng="nl">
    <trd>stil</trd>,
    <trd>stilzwijgend</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_l;&c_ch;&c_a;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_m;&c_o;&c_l;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_sh;&c_u;&c_m;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_i;&c_h;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ml&ccaron;iaci</trd>,
    <trd>tich&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="silent.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <dif>
      Kutimanta <tld/>i:
      <ekz>
        pri sia persono li estas &ccirc;iam <tld/>ema<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_w;&c_k;&c_l;&c_ib;&c_v;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>ml&ccaron;enliv&yacute;</trd>,
    <trd>m&aacute;lomluvn&yacute;</trd>
  </trdgrp>
  <trd lng="es">discreto<klr> (silencioso)</klr></trd>
  <trd lng="fa">&f_k;&a_m;&zwnj;&a_H;&a_r;&a_f;</trd>
  <trdgrp lng="fr">
    <trd>discret<klr> (silencieux)</klr></trd>,
    <trd>taciturne</trd>
  </trdgrp>
  <trd lng="hu">hallgatag</trd>
  <trd lng="nl">zwijgzaam</trd>
  <trd lng="ru">&c_m;&c_o;&c_l;&c_ch;&c_a;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
  <trd lng="sk">ml&ccaron;anliv&yacute;</trd>
</drv>

<drv mrk="silent.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Devigi iun a&ubreve; ion <tld/>i:
      <ekz>
        <tld/>igi la envion kaj kalumnion.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_m;&c_u;&c_s;&c_ib;&c_c;&c_mol;
&c_m;&c_a;&c_w;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_l;&c_u;&c_sh;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>uml&ccaron;et</trd>,
    <trd>zav&rcaron;&iacute;t n&ecaron;komu &uacute;sta</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>zum Schweigen bringen</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>silenciar</trd>,
    <trd>acallar</trd>,
    <trd>hacer <ind>callar</ind></trd>
  </trdgrp>
  <trd lng="fa">&a_s;&a_A;&f_k;&a_t; &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>faire <ind>taire</ind></trd>,
    <trd>r&eacute;duire au <ind>silence</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>elhallgattat</trd>,
    <trd>lecsendes&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol;
<ind>&c_z;&c_a;&c_m;&c_o;&c_l;&c_ch;&c_a;&c_t;&c_mol;</ind></trd>,
    <trd>&c_z;&c_a;&c_g;&c_l;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>uml&ccaron;a&tcaron;</trd>,
    <trd>ut&iacute;&scaron;i&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="silent.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ne paroli pri io:
      <ekz>
        pri<tld/>i sekreton, amon.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_m;&c_o;&c_w;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>poml&ccaron;et</trd>,
    <trd>zaml&ccaron;et</trd>,
    <trd>zatajit</trd>
  </trdgrp>
  <trd lng="de">verschweigen</trd>
  <trd lng="es">callar</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&a_s;&f_k;&a_w;&a_t; &f_g;&a_dh;&a_A;&a_sh;&a_t;&a_n;</trd>,
    <trd>&f_p;&a_n;&a_h;&a_A;&a_n; &a_d;&a_A;&a_sh;&a_t;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>passer sous <ind>silence</ind></trd>,
    <trd>taire<klr> (quelque chose)</klr></trd>
  </trdgrp>
  <trd lng="hu">elhallgat <klr tip="amb">(vki el&odblac;tt)</klr></trd>
  <trd lng="nl">verzwijgen</trd>
  <trd lng="ru">&c_z;&c_a;&c_m;&c_a;&c_l;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>zaml&ccaron;a&tcaron;</trd>,
    <trd>zataji&tcaron;</trd>
  </trdgrp>
</drv>
</art>
