<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tenac.xml,v 1.7 2022/03/28 12:03:04 revo Exp $">
<kap>
  <rad>tenac</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="tenac.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="tenac.0a.KOMUNE">
    <dif>
      <ref tip="super" cel="persis.0ema">Persiste</ref> eltenema, 
      &scirc;irorezista, romporezista:
      <ekz>
        la baleno <klr>[&#x2026;]</klr> diris al la Ruza Fi&scirc;o:
        &leftquot;&ccirc;i tiu homo  estas tre <tld/>a, kaj krome li
        ka&ubreve;zas al mi singulton, kion mi faru?&rightquot;
        <fnt>
          <aut>R. Kipling, trad. W. F. Pilger</aut>,
          <vrk>Tiel do - rakontoj</vrk>,
          <lok>Vulpo-libroj, 1992</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur <tld/>a roko, kiu el la maro vertikale levi&gcirc;as,
        ari&gcirc;is &gcirc;is &ccirc;irka&ubreve; kvindek domoj
        <fnt>
          <aut>E. Kumi&ccaron;i&cacute;, trad. S. Rukelj</aut>,
          <vrk>
          <url ref="http://free-vz.t-com.hr/Stanko/Surprizitaj.pdf">
          Surprizitaj nuptofestantoj</url></vrk>,
          <lok>inko, 2001</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fer.0a.FIG">fera<sncref/></ref>,
      <ref cel="firm.0a">firma</ref>,
      <ref cel="fortik.0a.materialo">fortika</ref>,
      <ref cel="sxance.ne0ebla">ne&scirc;ancelebla</ref>,
      <ref cel="rezist.0ema">rezistema</ref>
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>tena&ccedil;</trd>,
    <trd>porfidi&oacute;s</trd>,
    <trd>vigor&oacute;s</trd>,
    <trd>indeleble</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hou&zcaron;evnat&yacute;</trd>,
    <trd>lepkav&yacute;</trd>,
    <trd>lp&iacute;c&iacute;</trd>,
    <trd>pevn&yacute;</trd>,
    <trd>p&rcaron;ilnav&yacute;</trd>,
    <trd>tuh&yacute;</trd>,
    <trd>vazk&yacute;</trd>,
    <trd>visk&oacute;zn&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>z&auml;h <klr>(widerstandsf&auml;hig)</klr></trd>,
    <trd>widerstandsf&auml;hig</trd>,
    <trd>robust</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>r&eacute;sistant</trd>,
    <trd>robuste</trd>,
    <trd>tenace</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>spoisty</trd>,
    <trd>ci&aogonek;gliwy</trd>,
    <trd>wytrzyma&lstroke;y</trd>,
    <trd>stabilny</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>h&uacute;&zcaron;evnat&yacute;</trd>,
    <trd>lepkav&yacute;</trd>,
    <trd>pevn&yacute;</trd>,
    <trd>tuh&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x2019;&#x44F;&#x437;&#x43A;&#x438;&#x439;</trd>,
    <trd>&#x43B;&#x438;&#x43F;&#x43A;&#x438;&#x439;</trd>,
    <trd>&#x491;&#x43B;&#x435;&#x439;&#x43A;&#x438;&#x439;</trd>,
    <trd>&#x491;&#x43B;&#x435;&#x44E;&#x432;&#x430;&#x442;&#x438;&#x439;</trd>,
    <trd>&#x441;&#x442;&#x456;&#x439;&#x43A;&#x438;&#x439;</trd>
  </trdgrp>
</drv>
</art>

