<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>kosmetik</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kosmet.0o">
  <kap><tld/>o</kap>
  <snc mrk="kosmet.0o.KOS">
    <dif>
      <ref tip="super" cel="art.0o.tehxniko">Arto</ref> higiene flegi 
      kaj beligi la ha&ubreve;ton, la harojn kaj la dentojn.
    </dif>
    <trdgrp lng="de">
      <trd>Kosmetik</trd>,
      <trd>Sch&ouml;nheitspflege</trd>
    </trdgrp>
    <trd lng="en">cosmetics</trd>
    <trdgrp lng="hu">
      <trd>kozmetika</trd>,
      <trd>sz&eacute;ps&eacute;g&aacute;pol&aacute;s</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>schoonheidsverzorging</trd>,
      <trd>cosmetiek</trd>
    </trdgrp>
    <trd lng="pt">cosm&eacute;tica</trd>
  </snc>
  <snc mrk="kosmet.0o.kosmetikajxo">
    <ref tip="dif" cel="kosmet.0ajxo.KOS"><tld/>a&jcirc;o</ref>
    <trdgrp lng="de">
      <trd>Kosmetikum</trd>,
      <trd>Sch&ouml;nheitsmittel</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>cosmetic</trd>,
      <trd>beauty preparation</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kozmetikum</trd>,
      <trd>sz&eacute;p&iacute;t&odblac;szer</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>schoonheidsmiddel<klr tip="ind"> (ontr.)</klr></trd>,
      <trd>cosmeticum<klr tip="ind"> (ontr.)</klr></trd>
    </trdgrp>
    <trd lng="pt">cosm&eacute;tico</trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_s;&c_m;&c_e;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="fr">cosm&eacute;tique</trd>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_m;&c_je;&c_t;&c_i;&c_k;&c_a;</trd>
</drv>

<drv mrk="kosmet.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="kosmet.0ajxo.KOS">
    <dif>
      Substanco uzata en <tld/>o:
      <ekz>
        sub flava ombrelo ili elsakigis kosmetika&jcirc;ojn kaj atenteme 
        beligis sin
        <fnt>
          <aut>Min Zuhua</aut>,
          <vrk><url
          ref="http://www.chinareport.com.cn/18s/02c/02-23/23-m-3.htm">
          Konkurso de ji-oj pri beleco</url></vrk>,
          <lok>nacimalplimultoj de &Ccirc;inio, 2004-12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">cosm&eacute;tique<klr> (produit)</klr></trd>
     <trdgrp lng="nl">
      <trd>schoonheidsmiddel</trd>,
      <trd>cosmeticum</trd>
    </trdgrp>
</drv>
</art>
