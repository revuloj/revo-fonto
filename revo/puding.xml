<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: puding.xml,v 1.20 2022/05/04 18:30:34 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>puding</rad>/o
</kap>
<drv mrk="puding.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc mrk="puding.0o.bakita">
    <dif>
      Angla <ref tip="super" cel="kuk.0o">kuko</ref>,
      el faruno a&ubreve; pano, ovoj, lakto, bovograso, kruda sukero,
      sekvinberoj, rumo kaj diversaj aroma&jcirc;oj kaj spica&jcirc;oj:
      <ekz>
        &ccirc;u la <tld/>o el pomoj ne sufi&ccirc;e elbaki&gcirc;is?
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la origina maniero por prepari iun ajn <tld/>on estis, ke oni
        plenigis iun tolan
        sakon) per ka&ccirc;o el lakto, faruno kaj / a&ubreve; diversaj aliaj aferoj
        kaj ke oni
        metis la fermitan sakon en bolantan akvon
        <fnt>
          <bib>Monato</bib>
          <aut>Petro De Smet'</aut>
          <vrk><url ref="&Monato;/2008/009292.php">Sidante
          en pudingo,
          mi alvokas miajn savantojn!</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/26/The_bread_pudding_I_made_for_my_father_on_his_birthday.jpg/640px-The_bread_pudding_I_made_for_my_father_on_his_birthday.jpg" prm="CC BY 2.0">pan<tld/>o
        <fnt>
          <aut>Shawn Lea el Jackson, Usono</aut>
          <lok><url ref="&WCW;/File:The_bread_pudding_I_made_for_my_father_on_his_birthday.jpg">&WCW;/File:The_bread_pudding_I_made_for_my_father_on_his_birthday.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <snc mrk="puding.0o.flauxno">
    <dif>
      <ref tip="dif" cel="flauxn.0o">Fla&ubreve;no</ref>:
      <ekz>
        <tld/>o estas dol&ccirc;a zakusko, kiu estas preparata el lakto,
        sukero kaj pudinga
        pulvoro (kiu konsistas &ccirc;efe el amelo)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=300495">Pudingo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/c/c4/Vanillepudding_mit_frischen_Erdbeeren.JPG/320px-Vanillepudding_mit_frischen_Erdbeeren.JPG" prm="CC BY-SA 3.0">vanila <tld/>odeserto kun fragoj
        <fnt>
          <aut>4028mdk09, propra verko</aut>
          <lok><url ref="&WCW;/File:Vanillepudding_mit_frischen_Erdbeeren.JPG">&WCW;/File:Vanillepudding_mit_frischen_Erdbeeren.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_d;&c_y;&c_n;&c_g;</trd>
  <trd lng="ca">p&uacute;ding</trd>
  <trdgrp lng="cs">
    <trd>n&aacute;kyp</trd>,
    <trd>pudink</trd>
  </trdgrp>
  <trd lng="de">Pudding</trd>
  <trd lng="en">pudding</trd>
  <trd lng="fa">&f_p;&a_w;&a_d;&f_y;&a_n;&f_g;</trd>
  <trd lng="fr">pudding</trd>
  <trd lng="hu">puding</trd>
  <trd lng="pl">puding</trd>
  <trd lng="pt">pudim <klr tip="amb">(ingl&ecirc;s)</klr></trd>
  <trd lng="ru">&c_p;&c_u;&c_d;&c_i;&c_n;&c_g;</trd>
  <trd lng="sk">puding</trd>
  <trd lng="uk">&#x43F;&#x443;&#x434;&#x438;&#x43D;&#x491;</trd>
</drv>
</art>

