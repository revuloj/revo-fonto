<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
 <ofc>*</ofc>
 <rad>dom</rad>/o
</kap>
<drv mrk="dom.0o">
 <kap><ofc>*</ofc><tld/>o</kap>
 <snc num="1">
  <dif>
   Konstruo, kie lo&gcirc;as homoj:
   <ekz>mi restos hodia&ubreve; <tld/>e<fnt>Z</fnt>;</ekz>
   <ekz>iri <tld/>en<fnt>Z</fnt>;</ekz>
   <ekz><tld/>aj &scirc;uoj;</ekz>
   <ekz><tld/>besto.</ekz>
  </dif>
  <refgrp tip="sin">
   <ref cel="kaban.0o">kabano</ref>,
   <ref cel="kajut.0o">kajuto</ref>,
   <ref cel="barak.0o">barako</ref>,
   <ref cel="palac.0o">palaco</ref>.
  </refgrp>
 </snc>
 <snc num="2">
  <dif>
   Hejmo. 
  </dif>
 </snc>
 <snc num="3">
  <dif>
   Nobela, eminenta familianaro:
   <ekz>edzoligi&gcirc;i kun <tld/>o de nobelo<fnt>Z</fnt></ekz>
  </dif>
  <refgrp tip="sin">
   <ref cel="dinast.0o">dinastio</ref>.
  </refgrp>
 </snc>
 <trd lng="cs">d&ugrave;m</trd>
 <trd lng="de">Haus</trd>
 <trd lng="en">house</trd>
 <trd lng="fr">maison</trd>
 <trd lng="nl">huis</trd>
 <trd lng="pl">dom</trd>
 <trd lng="tr">ev</trd>
 <trd lng="sv">hus</trd>
</drv>
<drv mrk="dom.0ano">
 <kap><tld/>ano</kap>
 <snc num="1">
  <dif>
   Kunlo&gcirc;anto en iu <tld/>o, sam<tld/>ano. 
  </dif>
 </snc>
 <snc num="2">
  <dif>
   Kunvivanto en iu hejmo, samhejmano.  
  </dif>
 </snc>
</drv>
</art>
