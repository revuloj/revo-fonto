<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
 <ofc>*</ofc>
 <rad>dom</rad>/o
</kap>
<drv mrk="dom.0o">
 <kap><ofc>*</ofc><tld/>o</kap>
 <snc num="1">
  <dif>
   Konstruo, kie lo&gcirc;as homoj:
   <ekz><tld/>aj &scirc;uoj;</ekz>
   <ekz>
     <ind><tld/>besto</ind>.
     <trd lng="fr">animal domestique</trd>
   </ekz>
  </dif>
  <refgrp tip="vid">
   <ref cel="kaban.0o">kabano</ref>,
   <ref cel="kajut.0o">kajuto</ref>,
   <ref cel="barak.0o">barako</ref>,
   <ref cel="palac.0o">palaco</ref>.
  </refgrp>
  <trdgrp lng="fr">
    <trd>maison<klr tip="ind"> (construction)</klr></trd>,
    <trd>b&acirc;timent</trd></trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_m;</trd>,
    <trd>&c_z;&c_d;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_i;&c_z;&c_b;&c_a;</trd>
  </trdgrp>
 </snc>
 <snc num="2">
  <uzo tip="stl">ARK</uzo>
  <dif>
   <ref tip="dif" cel="hejm.0o">Hejmo</ref>: 
   <ekz>mi restos hodia&ubreve; <tld/>e<fnt>Z</fnt>;</ekz>
   <ekz>iri <tld/>en<fnt>Z</fnt>.</ekz>
  </dif>
  <trd lng="fr">domicile</trd>
  <trd lng="ru">&c_d;&c_o;&c_m;</trd>
 </snc>
 <snc num="3">
  <dif>
   Nobela, eminenta familianaro:
   <ekz>edzoligi&gcirc;i kun <tld/>o de nobelo<fnt>Z</fnt>.</ekz>
  </dif>
  <refgrp tip="vid">
   <ref cel="dinast.0o">dinastio</ref>.
  </refgrp>
  <trdgrp lng="fr">
    <trd>maison<klr tip="ind"> (lign&eacute;e)</klr></trd>,
    <trd>lign&eacute;e</trd></trdgrp> 
  <trd lng="ru">&c_d;&c_o;&c_m;</trd>
 </snc>
 <trd lng="cs">d&ugrave;m</trd>
 <trd lng="de">Haus</trd>
 <trd lng="en">house</trd>
 <trd lng="fr">maison</trd>
 <trd lng="nl">huis</trd>
 <trd lng="pl">dom</trd>
 <trd lng="tr">ev</trd>
 <trd lng="sv">hus</trd>
</drv>
<drv mrk="dom.0ano">
 <kap><tld/>ano</kap>
 <snc num="1">
  <dif>
   Kunlo&gcirc;anto en iu <tld/>o, sam<tld/>ano. 
  </dif>
 </snc>
 <snc num="2">
  <dif>
   Kunvivanto en iu hejmo, samhejmano.  
  </dif>
 </snc>
</drv>
</art>
