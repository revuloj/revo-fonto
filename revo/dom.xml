<drv mrk="dom.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="dom.0o.konstruajxo">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Tuta <ref tip="super" cel="konstr.0ajxo.domo">konstrua&jcirc;o</ref>,
      destinita por homa lo&gcirc;ado (<tld/>o kun unu a&ubreve; pluraj
      lo&gcirc;ejoj), por oficejoj (urbo<tld/>o) a&ubreve; publikaj 
      servoj (staci<tld/>o, Esperanto-<tld/>o):
      <ekz>
        anta&ubreve; la <tld/>o staras arbo
        <fnt><bib>F</bib> <lok>&FE; 8</lok></fnt>;
      </ekz>
      <ekz>
        bona estas <tld/>o nova kaj amiko malnova
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        apud propra <tld/>o &scirc;telisto ne &scirc;telas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        dronanto <tld/>on proponas, savito e&ccirc; brikon ne donas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        tola&jcirc;on malpuran lavu en la <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la administranto de mia <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 15:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        cirkumcidata estu &ccirc;iu naskita en via <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 17:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u en la <tld/>o de via patro estas loko por ni,
        por tradormi la nokton
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 24:23</lok>
        </fnt>?
      </ekz>
      <ekz>
        anta&ubreve; la sojlo de la <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 19:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>fasado
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007014.html">A&ubreve;strio: Orfi&gcirc;inta beleco</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        plenokupaj <tld/>mastrinoj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9501-sterogramoj.txt">Stereogramoj</url></vrk>
          <lok>95/01</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref cel="best.dom0o">dombesto</ref>
        <fnt>
          <bib>Monato</bib>
          <aut>Jean-Yves Santerre</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006864.html">Dombestoj: &Ccirc;evalrasoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>plantoj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006878.html">Dommastrumo: &Gcirc;ardenumi</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_o;&c_m;</trd>,
      <trd>&c_b;&c_u;&c_d;&c_y;&c_n;&c_a;&c_k;</trd>,
      <trd>&c_ib;&c_z;&c_b;&c_a;</trd>,
      <trd>&c_h;&c_a;&c_t;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="bo">
      <trd>&#xF41;&#xF44;&#xF0B;</trd>,
      <trd>&#xF41;&#xF44;&#xF0B;&#xF54;&#xF0B;</trd>
    </trdgrp>
    <trd lng="br">ti</trd>
    <trd lng="ca">casa</trd>
    <trdgrp lng="fr">
      <trd>maison<klr tip="ind"> (construction)</klr></trd>,
      <trd>b&acirc;timent</trd>
    </trdgrp>
    <trd lng="hu">h&aacute;z</trd>
    <trdgrp lng="pl">
      <trd>dom</trd>,
      <trd>domostwo</trd>,
      <trd>budynek</trd>,
      <trd>blok<klr> (np. mieszkalny)</klr></trd>,
      <trd>rezydencja</trd>
    </trdgrp>
    <trd lng="pt">casa</trd>
    <trdgrp lng="ro">
      <trd>cas&abreve;</trd>,
      <trd>locuin&tcedil;&abreve;</trd>,
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_o;&c_m;</trd>,
      <trd>&c_z;&c_d;&c_a;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_i;&c_z;&c_b;&c_a;</trd>
    </trdgrp>
    <trd lng="tp">tomo</trd>
  </snc>
  <snc mrk="dom.0o.GEN">
    <tezrad fak="GEN"/>
    <uzo tip="fak">GEN</uzo>
    <dif>
      Nobela, eminenta familianaro:
      <ekz>
        edzoligi&gcirc;i kun <tld/>o de nobelo
        <fnt>
          <aut>Moli&egrave;re, trad. &Z;</aut>
          <vrk><url ref="https://www.americanroads.us/esperanto/Georgo_Dandin_Moliere.pdf">Georgo Dandin</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ni gardis honeste la honoron de nia <tld/>o, ni povas kun pura konscienco
        partopreni en la komuna festo de nia familio
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvina Kongreso Esperantista en Barcelona en la 6a de septembro
          1909</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="dinast.0o">dinastio</ref>
    </refgrp>
    <trd lng="be">&c_d;&c_o;&c_m;</trd>
    <trd lng="br">tiegezh<klr tip="ind"> (lignez)</klr></trd>
    <trdgrp lng="fr">
      <trd>maison<klr tip="ind"> (lign&eacute;e)</klr></trd>,
      <trd>lign&eacute;e</trd>
    </trdgrp>
    <trd lng="hu">h&aacute;z</trd>
    <trd lng="pl">dom</trd>
    <trd lng="pt">casa</trd>
    <trd lng="ru">&c_d;&c_o;&c_m;</trd>
  </snc>
  <snc mrk="dom.0o.hejmo">
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="dif" cel="hejm.0o">Hejmo</ref>:
      <ekz>
        mi restos hodia&ubreve; <tld/>e
        <fnt><bib>F</bib>, <lok>&FE; 28</lok></fnt>;
      </ekz>
      <ekz>
        jam estas tempo iri <tld/>en
        <fnt><bib>F</bib>, <lok>&FE; 28</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_o;&c_m;</trd>,
      <trd>&c_d;&c_a;&c_m;&c_a;&c_sh;&c_n;&c_ib; &c_a;&c_ch;&c_a;&c_g;
      </trd>
    </trdgrp>
    <trdgrp lng="br">
      <trd>k&ecirc;r<klr tip="ind"> (annez)</klr></trd>,
      <trd>annez</trd>
    </trdgrp>
    <trd lng="en">home</trd>
    <trd lng="fr">domicile</trd>
    <trd lng="hu">otthon</trd>
    <trd lng="pl">dom<klr> rodzinny</klr></trd>
    <trdgrp lng="pt">
      <trd>casa</trd>,
      <trd>lar</trd>,
      <trd>domic&iacute;lio</trd>
    </trdgrp>
    <trd lng="ru">&c_d;&c_o;&c_m;</trd>
  </snc>
  <trd lng="af">huis</trd>
  <trd lng="am">&#x1264;&#x1275;</trd>
  <trd lng="ar">&#x645;&#x646;&#x632;&#x644;</trd>
  <trd lng="az">ev</trd>
  <trd lng="bg">&c_k;&c_mol;&c_shch;&c_a;</trd>
  <trd lng="bn">&#x998;&#x9B0;</trd>
  <trd lng="bs">ku&#x107;a</trd>
  <trd lng="co">g&#xEE;te</trd>
  <trdgrp lng="cs">
    <trd>bar&aacute;k</trd>,
    <trd>d&uring;m</trd>,
    <trd>d&uring;m</trd>
  </trdgrp>
  <trd lng="da">hus</trd>
  <trd lng="de">Haus</trd>
  <trd lng="en">house</trd>
  <trd lng="es">casa</trd>
