<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>dom</rad>/o
</kap>
<drv mrk="dom.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="dom.0o.konstruajxo">
  <uzo tip="fak">ARKI</uzo>
    <dif>
      Tuta <ref tip="super" cel="konstr.0ajxo">konstrua&jcirc;o</ref>,
      destinita por homa lo&gcirc;ado (<tld/>o kun unu a&ubreve; pluraj
      lo&gcirc;ejoj), por oficejoj (
      <ekz>
        urbo<tld/>o
      </ekz>
      ) a&ubreve;
      publikaj servoj (
      <ekz>
        staci<tld/>o
      </ekz>
      ,
      <ekz>
        Esperanto-<tld/>o
      </ekz>
      ).
    </dif>
    <refgrp tip="sub">
      <ref cel="kaban.0o">kabano</ref>,
      <ref cel="barak.0o">barako</ref>,
      <ref cel="palac.0o">palaco</ref>.
    </refgrp>
    <trd lng="br">an ti</trd>
    <trdgrp lng="fr">
      <trd>maison<klr tip="ind"> (construction)</klr></trd>,
      <trd>b&acirc;timent</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_o;&c_m;</trd>,
      <trd>&c_z;&c_d;&c_a;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_i;&c_z;&c_b;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="dom.0o.hejmo">
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="dif" cel="hejm.0o">Hejmo</ref>:
      <ekz>
        mi restos hodia&ubreve; <tld/>e<fnt>Z</fnt>;
      </ekz>
      <ekz>
        iri <tld/>en<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="fr">domicile</trd>
    <trd lng="ru">&c_d;&c_o;&c_m;</trd>
  </snc>
  <snc mrk="dom.0o.dinastio">
    <dif>
      Nobela, eminenta familianaro:
      <ekz>
        edzoligi&gcirc;i kun <tld/>o de nobelo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="dinast.0o">dinastio</ref>.
    </refgrp>
    <trdgrp lng="fr">
      <trd>maison<klr tip="ind"> (lign&eacute;e)</klr></trd>,
      <trd>lign&eacute;e</trd>
    </trdgrp>
    <trd lng="ru">&c_d;&c_o;&c_m;</trd>
  </snc>
  <trd lng="cs">d&#x16F;m</trd>
  <trd lng="de">Haus</trd>
  <trd lng="en">house</trd>
  <trd lng="es">casa</trd>
  <trd lng="fr">maison</trd>
  <trd lng="nl">huis</trd>
  <trd lng="pl">dom</trd>
  <trd lng="tr">ev</trd>
  <trd lng="sv">hus</trd>
</drv>

<drv mrk="dom.0ano">
  <kap><tld/>ano</kap>
  <snc>
    <dif>
      Kunlo&gcirc;anto en iu <tld/>o, sam<tld/>ano.
    </dif>
    <trd lng="de">Hausbewohner</trd>
    <trdgrp lng="fr">
      <trd>voisin d'immeuble</trd>
    </trdgrp>
    </snc>
  <snc>
    <dif>
      Kunvivanto en iu hejmo, samhejmano.
    </dif>
    <trd lng="de">Mitbewohner</trd>
    <trdgrp lng="fr">
      <trd>commensal</trd>
      <trd>familier<klr> (du m&ecirc;me foyer)</klr></trd>
    </trdgrp>
    </snc>
</drv>

<drv mrk="dom.staci0o">
  <kap>staci<tld/>o</kap>
  <uzo tip="fak">ARKI</uzo>
  <uzo tip="fak">FER</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="konstr.0ajxo">Konstrua&jcirc;o</ref>,
      por halto de vagonaroj, el- kaj eni&gcirc;ado de pasa&gcirc;eroj
      a&ubreve; komerca&jcirc;oj.
    </dif>
  </snc>
  <trd lng="de">Bahnhof</trd>
  <trdgrp lng="fr">
    <trd>gare</trd>
  </trdgrp>
  <trd lng="nl">stationsgebouw</trd>
  <trd lng="ru">&c_v;&c_o;&c_k;&c_z;&c_a;&c_l;</trd>
</drv>
</art>
