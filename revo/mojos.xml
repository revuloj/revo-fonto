<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mojos.xml,v 1.14 2024/01/26 19:05:52 revo Exp $">
<kap>
    <rad>mojos</rad>/a
    <fnt><bib>Viki</bib></fnt>
</kap>
<drv mrk="mojos.0a">
  <kap><tld/>a</kap>
  <snc mrk="mojos.0a.junstila">
    <dif>
      Akronimo de
      <em>modernjunstila</em>, t.e. <ctl>mjs-a</ctl>; impresanta a&ubreve;
      pla&ccirc;anta al junuloj:
      <ekz>
        menciinda estas la mojosa &jcirc;urnalo La NASKa Fasko
        <fnt>
          <bib>LOdE</bib>, <lok>2013 N&deg;8-9 (226-227)</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>toll</trd>,
    <trd>genial</trd>,
    <trd>geil <klr tip="ind">(cool)</klr></trd>,
    <trd>abgefahren <klr tip="ind">(cool)</klr></trd>,
    <trd>angesagt <klr tip="ind">(cool)</klr></trd>,
    <trd><klr>(voll)</klr> krass <klr tip="ind">(cool)</klr></trd>,
    <trd><klr>(voll)</klr> schick <klr tip="ind">(cool)</klr></trd>,
    <trd>total in <klr tip="ind">(cool)</klr></trd>,
  </trdgrp>
  <trd lng="en">cool</trd>
  <trdgrp lng="es">
    <trd>chulo</trd>,
    <trd>majo</trd>,
    <trd>genial</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>relax</trd>,
    <trd>sympa</trd>
  </trdgrp>
  <trd lng="nl">cool</trd>
  <trdgrp lng="pl">
    <trd>fajny</trd>,
    <trd>wspania&lstroke;y</trd>,
    <trd>&sacute;wietny</trd>,
    <trd>super</trd>,
    <trd>cool</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4FCA; <pr>z&#xF9;n</pr></trd>,
    <trd>&#x4F18;&#x79C0;&#x7684;&#x4E1C;&#x897F; <pr>y&#x14D;uxi&#xF9;ded&#x14D;ngxi</pr></trd>,
    <trd>&#x512A;&#x79C0;&#x7684;&#x6771;&#x897F; <pr>y&#x14D;uxi&#xF9;ded&#x14D;ngxi</pr></trd>,
    <trd>&#x5E26;&#x611F; <pr>d&#xE0;ig&#x1CE;n</pr></trd>,
    <trd>&#x5E36;&#x611F; <pr>d&#xE0;ig&#x1CE;n</pr></trd>,
    <trd>&#x7D66;&#x529B; <pr>g&#x11B;il&#xEC;</pr></trd>,
    <trd>&#x7ED9;&#x529B; <pr>g&#x11B;il&#xEC;</pr></trd>,
    <trd>&#x9177;&#x6583; <pr>k&#xF9;b&#xEC;</pr></trd>,
    <trd>&#x9177;&#x6BD9; <pr>k&#xF9;b&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mojos.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      En <tld/>a maniero.
    </dif>
  </snc>
  <trdgrp lng="pl">
    <trd>wspaniale</trd>,
    <trd>&sacute;wietnie</trd>,
    <trd>super</trd>,
    <trd>cool</trd>
  </trdgrp>
</drv>

<drv mrk="mojos.0i">
  <kap><tld/>i</kap>
  <snc>
    <dif>
      Esti <tld/>a.
    </dif>
  </snc>
  <trd lng="en">be cool</trd>
  <trdgrp lng="pl">
    <trd>by&cacute; <ind>fajnym  <klr>(m&lstroke;odzie&zdot;owe)</klr></ind></trd>,
    <trd>by&cacute;  <ind>super</ind></trd>
  </trdgrp>
</drv>

<drv mrk="mojos.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Stato de tio, kio estas <tld/>a:
      <ekz>
        <tld lit="M"/>eco kaj Muziko: Muzaiko!
      </ekz>
    </dif>
  </snc>
  <trd lng="en">coolness</trd>
  <trd lng="es">algo fant&aacute;stico</trd>
  <trd lng="nl">coolheid</trd>
  <trdgrp lng="pl">
    <trd>opanowanie</trd>,
    <trd>fantastyczno&sacute;&cacute;</trd>
  </trdgrp>
</drv>

<drv mrk="mojos.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      <tld lit="M"/>a homo.
    </dif>
  </snc>
  <trd lng="pl">spoko go&sacute;&cacute;</trd>
</drv>

<drv mrk="mojos.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      <tld lit="M"/>a renkontejo, renkonto, retpa&gcirc;o; junulejo:
      <ekz>
         MOJOsejo, alternativa semajno por gejunuloj
         <klr>(gejunula renkonto en kastelo Gresiljono)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">cool place</trd>
  <trd lng="es">sitio de reuni&oacute;n de moda</trd>
  <trdgrp lng="pl">
    <trd> modne miejsce  <ind>spotka&nacute;</ind></trd>,
    <trd>fajna miejsc&oacute;wka</trd>
  </trdgrp>
</drv>

</art>

