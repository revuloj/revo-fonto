<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>histrik</rad>/o
</kap>
<drv mrk="histri.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="histri.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Hystrix</trd>).
    </dif>
  </snc>
  <trd lng="de">Stachelschwein</trd>
</drv>
<drv mrk="histri.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="rod1.0uloj">roduloj</ref>
      (<trd lng="la">Hystricidae</trd>), kies haroj estas transformitaj
      al pingloj.
    </dif>
  </snc>
  <trd lng="de">Stachelschweine</trd>
</drv>
</art>
