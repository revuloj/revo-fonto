<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>aka&jcirc;u</rad>/o
</kap>
<drv mrk="akajxu.0o">
  <kap><tld/>o <fnt>PIV</fnt></kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      La pom-, pir- a&ubreve; korforma, fruktosimile
      &scirc;velinta flortigo de la
      <ref tip="malprt" cel="akajxu.0ujo"><tld/>ujo</ref>,
      sur kiu sidas la <tld/>nuksoj, kaj kiu estas uzata por
      konfita&jcirc;oj kaj alkohola&jcirc;oj.
    </dif>
    <trd lng="de">Cashewapfel</trd>
    <trd lng="fi">acajouomena</trd>
    <trd lng="fr">pomme de <ind>cajou</ind></trd>
    <trd lng="it">pome di <ind>Acagi&ugrave;</ind></trd>
    <trd lng="pt">caju</trd>
    <trdgrp lng="sv">
      <trd>njurfrukt</trd>,
      <trd>acajoufrukt</trd>
    </trdgrp>
  </snc>
</drv>
<drv mrk="akajxu.0ujo">
  <kap><tld/>arbo, <tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="anakar.0o">anakardio</ref>
      (<trd lng="la">Anacardium occidentale</trd>).
    </dif>
    <trdgrp lng="de">
      <trd>Nierenbaum</trd>,
      <trd>Cashewbaum</trd>,
      <trd>Akajoubaum</trd>
    </trdgrp>
    <trd lng="fr">anacardier</trd>
    <trd lng="nl">acajouboom</trd>
  </snc>
</drv>
<drv mrk="akajxu.0nukso">
  <kap><tld/>nukso</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Renforma semgrajno de la
      <ref tip="malprt" cel="akajxu.0ujo"><tld/>ujo</ref>, kies
      oleri&ccirc;a interno estas man&gcirc;ebla.
    </dif>
    <trdgrp lng="de">
      <trd>Cashewnuss</trd>,
      <trd>Akajounuss</trd>,
      <trd>Kaschunuss</trd>
    </trdgrp>
    <trd lng="en">cashew nut</trd>
    <trd lng="es">nuez de caoba</trd>
    <trd lng="fi">cashewp&auml;hkin&auml;</trd>
    <trd lng="fr">noix de <ind>cajou</ind></trd>
    <trdgrp lng="it">
      <trd>noce dell' <ind>anacardio</ind></trd>,
      <trd>noce di <ind>Acagi&ugrave;</ind></trd>
    </trdgrp>
    <trd lng="nl">cashewnoot</trd>
    <trd lng="pl">orzech <ind>cashew</ind></trd>
    <trd lng="pt">castanha de <ind>caju</ind></trd>
    <trd lng="sv">cashewn&ouml;t</trd>
  </snc>
</drv>
</art>
