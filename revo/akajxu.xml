<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>aka&jcirc;u</rad>/o
</kap>
<drv mrk="akajxu.0o">
<kap><tld/>o</kap>
<uzo tip="fak">KUI</uzo>
<snc>
<dif>
&Scirc;ajnfrukto de la tropika <tld/>-arbo. La frukto konsistas el <tld/>-pomo kaj <tld/>-nukso.
</dif>
<trd lng="es">anacardo</trd>
<trd lng="it">anacardio</trd>
</snc>
</drv>

<drv mrk="akajxu.0arbo">
   <kap><tld/>-arbo</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       Genro (<trd lng="la">Anacardium occidentale</trd>) el la familio
       <ref tip="super" cel="kotile.0acoj">dukotiledonaj</ref>
       plantoj.
     </dif>
   <trdgrp lng="de"><trd>Nierenbaum</trd>,
      <trd>Cashewbaum</trd>,
      <trd>Akajoubaum</trd></trdgrp>
   <trd lng="fr">anacardier</trd>
   <trd lng="nl">acajouboom</trd>
   </snc>
   </drv>
  
<drv mrk="akajxu.0nukso">
   <kap><tld/>-nukso</kap>
   <uzo tip="fak">KUI</uzo>
   <snc>
     <dif>
       Nukso de la <tld/>-arbo.
     </dif>
   <trdgrp lng="de"><trd>Cashewnu&szlig;</trd>,
      <trd>Akajounu&szlig;</trd>,
      <trd>Kaschunu&szlig;</trd></trdgrp>
   <trd lng="en">cashew nut</trd>
   <trd lng="es">nuez de caoba</trd>
   <trd lng="fi">cashewp&auml;hkin&auml;</trd>
   <trd lng="fr">noix de <ind>cajou</ind></trd>
   <trdgrp lng="it"><trd>noce dell' <ind>anacardio</ind></trd>,
      <trd>noce di <ind>Acagi&ugrave;</ind></trd></trdgrp>
   <trd lng="nl">cashewnoot</trd>
   <trd lng="pl">orzech <ind>cashew</ind></trd>
   <trd lng="pt">castanha de <ind>caju</ind></trd>
   <trd lng="sv">cashewn&ouml;t</trd> 
   </snc>
   </drv>
   
<drv mrk="akajxu.0pomo">
   <kap><tld/>-pomo</kap>
   <uzo tip="fak">KUI</uzo>
   <snc>
     <dif>
       Frukto de la <tld/>-arbo, entenanta la <tld/>-nukson.
     </dif>
   <trd lng="fi">acajouomena</trd>
   <trd lng="fr">pomme de <ind>cajou</ind></trd>
   <trd lng="it">pome di <ind>Acagi&ugrave;</ind></trd>
   <trd lng="pt">caju</trd>
   <trdgrp lng="sv"><trd>njurfrukt</trd>, 
          <trd>acajoufrukt</trd></trdgrp> 
   </snc>
   </drv>
   
</art>

<!--
$Log$
-->
</vortaro>