<art mrk="$Id: perige.xml,v 1.23 2023/10/20 14:25:20 revo Exp $">
<kap>
  <rad>perige</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="perige.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Tiu <ref tip="super" cel="punkt.0o.loko">punkto</ref>
      de la <ref tip="malprt" cel="orbit.0o.AST">orbito</ref>
      de ter-&ccirc;irka&ubreve;anta objekto, &ccirc;e kiu tiu
      estas plej proksima al la tero:
      <ekz>
        <nom>Sputnik 1</nom> <klr>[estas]</klr> la unua artefarita satelito de la Tero, lan&ccirc;ita la
        4-an de oktobro 1957 en Sovetunio, parametroj de la flugo: <klr>[&#8230;]</klr>
        <tld/>o &mdash; 228&#x202F;km, apogeo &mdash; 947&#x202F;km
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=35347">Sputnik 1</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="apoge.0o">apogeo</ref>
    <ref tip="vid" cel="nadir.0o">nadiro</ref>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_r;&c_y;&c_g;&c_je;&c_j;</trd>
  <trd lng="bg">&c_p;&c_je;&c_r;&c_i;&c_g;&c_je;&c_j;</trd>
  <trd lng="ca">perigeu</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;&iacute;zem&iacute;</trd>,
    <trd>perigeum</trd>,
    <trd>p&rcaron;&iacute;zem&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Erdn&auml;he</trd>,
    <trd>Perigeum</trd>,
    <trd>Perig&auml;um</trd>
  </trdgrp>
  <trd lng="en">perigee</trd>
  <trd lng="es">perigeo</trd>
  <trd lng="fr">p&eacute;rig&eacute;e</trd>
  <trdgrp lng="hu">
    <trd>f&ouml;ldk&ouml;zel</trd>,
    <trd>perigeum</trd>
  </trdgrp>
  <trd lng="it">perigeo</trd>
  <trd lng="ja">&#x8FD1;&#22320;&#x70B9;
  <pr>&#x304D;&#12435;&#12385;&#12390;&#12435;</pr></trd>
  <trd lng="nl">perigeum</trd>
  <trd lng="pl">perygeum</trd>
  <trd lng="pt">perigeu</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_i;&c_g;&c_je;&c_j;</trd>
  <trd lng="sk">perigeum</trd>
  <trd lng="uk">&#x43F;&c_je;&c_r;&c_i;&c_g;&c_je;&c_j;</trd>
</drv>
</art>
