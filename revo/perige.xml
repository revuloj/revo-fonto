<art mrk="$Id$">
<kap>
  <rad>perige</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="perige.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Tiu punkto de la orbito de
      ter-&ccirc;irka&ubreve;anta objekto, &ccirc;e kiu tiu
      estas plej proksima al la tero.
    </dif>
    <refgrp tip="vid">
      <ref cel="apoge.0o">apogeo</ref>,
      <ref cel="zenit.0o">zenito</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_r;&c_y;&c_g;&c_je;&c_j;</trd>
  <trd lng="ca">perigeu</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;&iacute;zem&iacute;</trd>,
    <trd>perigeum</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Erdn&auml;he</trd>,
    <trd>Perigeum</trd>,
    <trd>Perig&auml;um</trd>
  </trdgrp>
  <trd lng="en">perigee</trd>
  <trd lng="es">perigeo</trd>
  <trd lng="fr">p&eacute;rig&eacute;e</trd>
  <trdgrp lng="hu">
    <trd>f&ouml;ldk&ouml;zel</trd>,
    <trd>perigeum</trd>
  </trdgrp>
  <trd lng="it">perigeo</trd>
  <trd lng="ja">&#12365;&#12435;&#12385;&#12390;&#12435;</trd>
  <trd lng="nl">perigeum</trd>
  <trd lng="pl">perygeum</trd>
  <trd lng="pt">perigeu</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_i;&c_g;&c_je;&c_j;</trd>
</drv>
</art>
