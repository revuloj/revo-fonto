<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>perige</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="perige.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Punkto de la orbito de planedo, en kiu &gcirc;i estas plej proksima de la
      tero.
    </dif>
    <refgrp tip="vid">
      <ref cel="apoge.0o">apogeo</ref>,
      <ref cel="zenit.0o">zenito</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Erdn&auml;he</trd>,
    <trd>Perig&auml;um</trd>
  </trdgrp>
  <trd lng="en">perigee</trd>
  <trd lng="nl">perigeum</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_i;&c_g;&c_je;&c_j;</trd>
</drv>
</art>
