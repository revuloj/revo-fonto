<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap><rad>Figar</rad>/o <fnt><bib>PIV2</bib></fnt></kap>

    <drv mrk="figar.0o">
      <kap><tld/>o</kap>
      <snc mrk="figar.0o.barbiro">
        <uzo tip="fak">BELE</uzo>
        <dif>
          Nomo de drampersono el tri verkoj de
          <ref cel="bomarsxe.0o">Bomar&scirc;eo</ref>:
          <ekz>
            Ekzistas cetere multe da miaj verkoj: <ctl>Edzi&gcirc;o de
            <tld/>o</ctl>,
            <ctl>Roberto-Diablo</ctl>, <ctl>Norma</ctl>
            <fnt>
              <aut>N.V. Gogol, tr. &Z;</aut>: <vrk>La revizoro</vrk>.
              <lok>3:6</lok>.
            </fnt>.
          </ekz>
        </dif>
      </snc>

      <snc mrk="figar.0o.gazeto">
        <dif>
          Nomo de pariza taggazeto, aludanta la &ccirc;iofarulon
          <tld/>o<sncref ref="Figar.0o.barbiro"/>:
          <ekz>
            <tld/>Josefino apena&ubreve; veki&gcirc;is kaj havis okulojn ankora&ubreve;
            dormoplenajn, kiam Viktoro, dis&scirc;irinte la banderolon de
            <ctl><tld/>o</ctl> anta&ubreve; momento ricevita, legis la&ubreve;te
            artikolon tiel titolitan <klr>...</klr>
            <fnt>
              <aut>Henri Vallienne</aut>:
              <vrk>Kastelo de Prelongo</vrk>.
              <lok>&Ccirc;ap. 10a</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="fr">Figaro</trd>
      <trd lng="ru">&c_F;&c_i;&c_g;&c_a;&c_r;&c_o;</trd>
    </drv>

  </art>

