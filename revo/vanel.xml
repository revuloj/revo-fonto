<kap>
  <ofc>1</ofc>
  <rad>vanel</rad>/o
</kap>
<drv mrk="vanel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="vanele.0oj"><tld/>edoj</ref>
      (<trd lng="la">Vanellus</trd>):
      <ekz>la aviadilon trafis svarmo da sturnoj kaj <tld/>oj<fnt><aut>Guido Van Damme</aut>: <vrk>Pli bone unu birdo en la
mano</vrk>, <bib>Monato</bib><lok>jaro 1996a, numero 11a, p. 19a</lok></fnt>.</ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_n;&c_ib;&c_g;&c_a;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_ja;</trd>
  </trdgrp>
  <trd lng="cs">&ccaron;ejka</trd>
  <trd lng="de">Kiebitz</trd>
  <trd lng="en">lapwing</trd>
  <trdgrp lng="es">
    <trd>avefr&iacute;a</trd>,
    <trd>ave fr&iacute;a</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&fa_mim;&fa_re;&fa_qein; &fa_ze;&fa_ye;&fa_be;&fa_alef;</trd>&fa_komo;
    <trd>&fa_ze;&fa_ye;&fa_alef;&fa_kaf;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>vanneau<klr> (oiseau)</klr></trd>
  </trdgrp>
  <trd lng="hu">b&iacute;bic</trd>
  <trd lng="nl">kievit</trd>
  <trd lng="pl">czajka</trd>
  <trdgrp lng="pt">
   <trd>abibe</trd>,
   <trd>abitoninha</trd>,
   <trd>pavoncino</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_i;&c_b;&c_i;&c_s;</trd>
</drv>
</art>
