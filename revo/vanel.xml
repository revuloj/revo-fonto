<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>vanel</rad>/o
</kap>
<drv mrk="vanel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="vanel.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Vanellus</trd>).
    </dif>
  </snc>
  <trd lng="cs">&ccaron;ejka</trd>
  <trd lng="de">Kiebitz</trd>
  <trd lng="en">lapwing</trd>
  <trd lng="hu">bibic</trd>
  <trd lng="pl">czajka</trd>
  <trd lng="ru">&c_ch;&c_i;&c_b;&c_i;&c_s;</trd>
</drv>
<drv mrk="vanel.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super"
      cel="hxarad.0oformaj">&hcirc;aradrioformaj birdoj</ref>
      (<trd lng="la">Vanellidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Kiebitze <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>
