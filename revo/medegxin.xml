<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: medegxin.xml,v 1.5 2022/05/01 12:05:12 afido Exp $">
    <kap>
      <rad>Mede&gcirc;in</rad>/o, <var><kap><rad var="l">Medelin</rad>/o</kap></var>
    </kap>

<drv mrk="medegxin.0o">
  <kap><tld/>o, <var><kap><tld var="l"/>o</kap></var></kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Granda <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      en <ref tip="malprt" cel="kolombi.K0o">Kolombio</ref>, 
      &ccirc;. 250&#8239;km nordokcidente de <ref cel="bogot.0o">Bogoto</ref>:
      <ekz>
        <tld/>o <klr>[&#8230;]</klr> estas la tria plej granda urbo de Kolombio
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Mede%C4%9Dino"
              >Mede&gcirc;ino</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ni realigis la 9-an kongreson en <tld/>o
        <fnt>
          <vrk><url ref="http://luisguillermo.com/9a_KKE/gxenerala.htm"
              >&Gcirc;enerala Raporto pri la 9a Kolombia Kongreso de Esperanto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        persekutado kontra&ubreve; la kokain-karteloj de <tld var="l"/>o kaj Kalio dum la 80aj kaj
        90aj jaroj
        <fnt>
          <vrk><url ref="https://esperanto.co/blogo/per-netflikso/per-netflix-drogsakristoj-narcos/"
              >Per Netflikso: Drog&scirc;akristoj - Narcos</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_M;&c_e;&c_d;&c_e;&c_l;&c_mol;&c_ib;&c_n;</trd>
    <trd lng="de">Medell&iacute;n</trd>
    <trd lng="en">Medell&iacute;n</trd>
    <trd lng="fr">Medell&iacute;n</trd>
    <trd lng="pl">Medell&iacute;n</trd>     
    <trd lng="ro">Medell&iacute;n</trd>
  </snc>
</drv>

  </art>

