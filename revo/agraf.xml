<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>1</ofc>
      <rad>agraf</rad>/o
    </kap>
    <drv mrk="agraf.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Ileto por provizore kunfiksi (<tld/>i) diversajn objektojn,
          tiel ke eblu facile kaj sendifekte malfiksi ilin
          (mal<tld/>i).
        </dif>
        <subsnc mrk="agraf.0o.krocxilo">
          <dif>
            Metalhoketo, kiu eni&gcirc;as en ringeton (<tld/>ingo) por
            fiksi la randojn de vesta&jcirc;o a&ubreve; tuko.
          </dif>
          <trd lng="de">Spange</trd>
          <trd lng="fr">agrafe <klr>(de v&ecirc;tement)</klr></trd>
          <trd lng="nl">haakje</trd>
          <trd lng="pt">colchete</trd>
          <trd lng="ru">&c_z;&c_a;&c_s;&c_t;&#x451;&c_zh;&c_k;&c_a;</trd>
          <trd lng="sv">sp&auml;nne</trd>
        </subsnc>
        <subsnc mrk="agraf.0o.braceleta">
          <dif>
            Fiksilo por <tld/>i la kovrilpa&gcirc;ojn de libro, la
            ekstremojn de koliero a&ubreve; braceleto.
          </dif>
          <trd lng="de">Schlie&szlig;e</trd>
          <trdgrp lng="ru">
            <trd>&c_z;&c_a;&c_s;&c_t;&#x451;&c_zh;&c_k;&c_a;</trd>,
            <trd>&c_z;&c_a;&c_m;&c_o;&c_k;</trd></trdgrp>
        </subsnc>
        <subsnc mrk="agraf.0o.premtenilo">
          <dif>
            Premfiksilo por kunteni plurajn paperfoliojn, la randojn
            de vundo ktp.
          </dif>
          <trd lng="de">Klammer</trd>
          <trdgrp lng="ru">
            <trd>&c_z;&c_a;&c_zh;&c_i;&c_m;</trd>,
            <trd>&c_s;&c_k;&c_r;&c_je;&c_p;&c_k;&c_a;</trd>,
            <trd>&c_p;&c_r;&c_i;&c_shch;&c_je;&c_p;&c_k;&c_a;</trd></trdgrp>
        </subsnc>
      </snc>
    </drv>
    <drv mrk="agraf.0i">
      <kap><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Kunigi, fermi per <tld/>&c_o;:
          <ekz>
            <tld/>i robon, kolieron<fnt>PIV1</fnt>.
          </ekz>
        </dif>
        <refgrp tip="sub">
          <ref cel="buk.0i">buki</ref>,
          <ref cel="buton.0umi">butonumi</ref>
        </refgrp>
      </snc>
      <trd lng="fr">agrafer <klr>(un v&ecirc;tement)</klr></trd>
      <trdgrp lng="pt">
        <trd>acolchetear</trd>,
        <trd>abrochar</trd></trdgrp>
      <trdgrp lng="ru">
        <trd>&c_z;&c_a;&c_s;&c_t;&c_je;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>,
        <trd>&c_r;&c_a;&c_s;&c_sh;&c_i;&c_t;&c_mol;</trd></trdgrp>
    </drv>
    <drv mrk="agraf.mal0i">
      <kap>mal<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Malfermi <tld/>on kaj disigi, malfiksi objektojn
          anta&ubreve;e <tld/>itajn:
          <ekz>
            mal<tld/>i sian jupon<fnt>PIV1</fnt>;
          </ekz><ekz>
            ili mal<tld/>as la &ccirc;arnirojn de siaj flugiloj
            <fnt>
              <aut>L. Frank Baum, tr. D. Broadribb</aut>:
              <vrk>Doroteo kaj la Sorcxisto en Oz</vrk>,
              <lok>A&ubreve;stralio: Bookleaf Publishing, 1996.
                <url
 ref="http://ttt.esperanto.org/esperante/literatur/Baum/doroteo/cxapitro12.html"
                     >&Ccirc;ap. 12.</url></lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="fr">d&eacute;grafer</trd>
      <trdgrp lng="ru">
        <trd>&c_r;&c_a;&c_s;&c_s;&c_t;&c_je;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>,
        <trd>&c_r;&c_a;&c_z;&c_o;&c_m;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>,
        <trd>&c_o;&c_t;&c_k;&c_r;&c_je;&c_p;&c_i;&c_t;&c_mol;</trd>
      </trdgrp>
    </drv>
  </art>
