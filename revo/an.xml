<vortaro>
<art mrk="$Id: an.xml,v 1.53 2024/11/13 18:34:29 revo Exp $">
  <kap>
    <ofc>*</ofc>-<rad>an</rad>/
    <fnt> <bib>UV</bib></fnt>
  </kap>

<subart>

  <drv mrk="an.0">
    <kap>-<tld/></kap>
    <dif>
      Sufikso signifanta membron de:
    </dif>
    <snc mrk="an.0.grupo">
      <dif>
       grupo, kiun la anta&ubreve;metita
       radikalo indikas rekte (la radikalo indikas grupon):
        <ekz>
          akademi<tld/>o, komitat<tld/>o, &scirc;tat<tld/>o, famili<tld/>o,
          parti<tld/>o, polic<tld/>o, samklas<tld/>o, estrar<tld/>o;
        </ekz>
        a&ubreve; nerekte (la radikalo indikas arigan principon):
        <ekz>
          amerik<tld/>o, pariz<tld/>ino, ka&ubreve;kaz<tld/>o,
          step<tld/>o, montar<tld/>o, urb<tld/>o
          <klr>(iu el la lo&gcirc;antaro de loko)</klr>;
        </ekz>
        <ekz>
          krist<tld/>ino, luter<tld/>o, kalvin<tld/>o
          <klr>(membro de konfesio iniciatita de la koncerna
          a&ubreve;toro)</klr>;
        </ekz>
        <ekz>
          movad<tld/>o, samide<tld/>ino
          <klr>(iu el la grupo difinita per komuna celo,
          konvinko)</klr>;
        </ekz>
        <ekz>
          samtemp<tld/>o, samsort<tld/>o,
        </ekz>
        <ekz>
          samgent<tld/>oj
          <fnt>
        <aut>Michel Duc Goninaz</aut>
        <vrk><url ref="&Monato;/2000/006947.html">Eks-Jugoslavio:
          kiu kiun mistifikas?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 8</lok>
        </fnt>;
          <klr>(iu el la grupo difinita per komuna cirkonstanco)</klr>;
        </ekz>
        <ekz>
          <ref cel="sxip.0ano">&scirc;ip<tld/>o</ref>
          <klr>(kp <ref cel="sxip.0estro">&scirc;ipestro</ref>)</klr>,
          lernej<tld/>o, kurs<tld/>o,
          <ref cel="bapt.0anino">bapt<tld/>ino</ref>
          <klr>(iu el la grupo difinita per kuna agado en tio a&ubreve; per tio
            kion indikas la radikalo)</klr>.
        </ekz>
      </dif>
      <rim num="1">
        Ofte, sed ne &ccirc;iam kaj ne tute, la sufiksoj -<tld/> kaj
        -<ref tip="sin" cel="ist.0.adepto">ist</ref> sinonimas
        (vd <ref cel="ist.0.rim2">rim.2 sub -ist</ref>).
      </rim>
      <rim num="2">
        Kelkajn vortojn, pruntitajn kun neesperanta radikfino -an la
        vortaroj tradicie listigas kiel esperantajn deriva&jcirc;ojn:
        <ekz>
          kapel<tld/>o, <ref cel="sakris.0ano">sakristi<tld/>o</ref>
        </ekz>
        ktp.  Tio estas pia memtrompo.
        <aut>Sergio Pokrovskij</aut>
      </rim>
      <trdgrp lng="de">
      <trd><klr>-</klr>aner</trd>,
      <trd><klr>-</klr>er</trd>
      </trdgrp>
    </snc>

    <snc mrk="an.0.lando">
      <dif>
        lando, kies radikalo havas landan signifon. La aldonita sufikso
        -<tld/> formas nomon de la land<tld/>oj, la&ubreve; la sistemo
        <ctl>Lando-land<tld/>o</ctl>:</dif>
        <ekz> en la
        novaj landoj [...], &ccirc;iuj &gcirc;iaj lo&gcirc;antoj uzas por si (per -<tld/>)
        la
        nomon de la lando<fnt><bib>LR</bib>, <lok>Respondo 47, Oficiala
        Gazeto, III, 1911, p. 291</lok></fnt>; Kanado-kanad<tld/>o;
        A&ubreve;stralio-a&ubreve;strali<tld/>o; Al&gcirc;erio-al&gcirc;eri<tld/>o;
        Irlando-irland<tld/>o;
        Sud-Afriko - sud-afrik<tld/>o; Pakistano-pakistan<tld/>o.
        </ekz>
        <ref tip="vid" cel="land.0nomo">landnomo</ref>
        <trd
        lng="th">&#xE41;&#xE2A;&#xE14;&#xE07;&#xE04;&#xE27;&#xE32;&#xE21;&#xE40;&#xE1B;&#xE47;&#xE19;&#xE0A;&#xE32;&#xE27;</trd>
    </snc>
    <trd lng="be">&c_a;&c_f;&c_ib;&c_k;&c_s;, &c_ja;&c_k;&c_ib; &c_v;&c_y;&c_z;&c_n;&c_a;&c_ch;&c_a;&c_je; &c_u;&c_d;&c_z;&c_je;&c_l;&c_mol;&c_n;&c_ib;&c_k;&c_a; &c_g;&c_r;&c_u;&c_p;&c_y;, &c_n;&c_a;&c_s;&c_je;&c_l;&c_mol;&c_n;&c_ib;&c_k;&c_a; &c_k;&c_r;&c_a;&c_ib;&c_n;&c_y;</trd>
    <trdgrp lng="fr">
      <trd>-ien</trd>,
      <trd>-and</trd>,
      <trd>-ais</trd>,
      <trd>-ol</trd>,
      <trd>-ois</trd>,
      <trd>-in</trd>,
      <trd>-ain</trd>,
      <trd>-ard</trd>,
      <trd>-en</trd>,
      <trd>-an</trd>,
      <trd>-ant</trd>,
      <trd>-ier</trd>,
      <trd>-iste</trd>,
    </trdgrp>
    <trd lng="io">-an</trd>
    <trd lng="pl">przyrostek przynale&zdot;no&sacute;ci</trd>
    <trd lng="sgn" kod="M522x557S2ff00482x476S36d00480x520S1e202476x535S20500482x530">D@45</trd>
    <trd
    lng="th">&#xE41;&#xE2A;&#xE14;&#xE07;&#xE04;&#xE27;&#xE32;&#xE21;&#xE40;&#xE1B;&#xE47;&#xE19;&#xE2A;&#xE21;&#xE32;&#xE0A;&#xE34;&#xE01;</trd>
    <trd lng="vo">-an</trd>
  </drv>
</subart>

<subart>
  <dif>
    Samsignifa radiko:
  </dif>

  <drv mrk="an.0o">
    <kap><tld/>o</kap>
    <snc>
      <dif>
        <ref tip="sin" cel="membr.0o.ano">Membro</ref>:
        <ekz>
          <tld/>o de la Berlina Grupo;
        </ekz>
        <ekz>
          <tld/>oj de la katolika Eklezio
          <fnt>
            <bib>Monato</bib>
            <aut>Antonjo Gambuti</aut>
            <vrk><url ref="&Monato;/2001/007305.html">Vatikano
            rebatas (1)</url></vrk>
          </fnt>;
        </ekz>
        <ekz>
          granda diferenco disde la aliaj ekspedicioj, kiuj traktis la
          &scirc;erpojn nur kiel <tld/>ojn de servista klaso
          <fnt>
            <bib>Monato</bib>
            <vrk><url ref="&Monato;/2001/007116.html">Sir Edmund
            Hillary: Vivo dedi&ccirc;e al la &scirc;erpoj</url></vrk>
          </fnt>.
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="be">
        <trd>&c_ch;&c_l;&c_je;&c_n;</trd>,
        <trd>&c_ch;&c_a;&c_l;&c_je;&c_c;</trd>,
        <trd>&c_u;&c_d;&c_z;&c_je;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
      </trdgrp>
    <trdgrp lng="ca">
      <trd>membre</trd>,
      <trd>soci</trd>
    </trdgrp>
    <trdgrp lng="cs">
      <trd>&ccaron;len</trd>,
      <trd>p&rcaron;&iacute;slu&scaron;n&iacute;k</trd>
    </trdgrp>
    <trdgrp lng="de">
        <trd>Mitglied</trd>,
        <trd>Anh&auml;nger</trd>
     </trdgrp>
    <trdgrp lng="el">
        <trd>&my;&epsilon_ton;&lambda;&omikron;&sigma_fina;</trd>
    </trdgrp>
    <trd lng="en">member</trd>
    <trdgrp lng="es">
       <trd>partidario</trd>,
       <trd>miembro<klr tip="amb"> (de un grupo)</klr></trd>
    </trdgrp>
    <trd lng="fr">membre</trd>
    <trd lng="he">&het;&bet;&resh;</trd>
    <trd lng="io">membro</trd>
    <trd lng="ja">&#x4F1A;&#x54E1;  <pr>&#x304B;&#12356;&#12356;&#12435;</pr></trd>
    <trdgrp lng="pl">
      <trd>cz&lstroke;onek</trd>,
      <trd>adept</trd>,
    </trdgrp>
    <trd lng="ru">&c_ch;&c_l;&c_je;&c_n;</trd>
    <trdgrp lng="sk">
      <trd>&ccaron;len</trd>,
      <trd>pr&iacute;slu&scaron;n&iacute;k</trd>,
      <trd>st&uacute;penec</trd>
    </trdgrp>
    <trd lng="th">&#xE2A;&#xE21;&#xE32;&#xE0A;&#xE34;&#xE01;</trd>
    <trd lng="uk">&c_ch;&c_l;&c_je;&c_n;</trd>
</drv>

<drv mrk="an.0i">
  <kap>
    <tld/>i
  </kap>
  <snc mrk="an.0i.POL">
    <dif>
      Esti <tld/>o, <ref tip="vid" cel="membr.0i">membri</ref>:
      <ekz>
        &scirc;i <klr>[&#8230;]</klr> <tld/>is al la bando, kiel speco de morala kaj jura
        konsilisto, avertante &gcirc;is kie ni povis iri sen transpasi la limojn
        <fnt>
          <aut>J. Valano</aut>
          <vrk>&Ccirc;u li bremsis sufi&ccirc;e?</vrk>
          <lok>1978</lok>
        </fnt>;
      </ekz>
      <ekz>
        estis rekonebla nur ke li <tld/>is en la popola milico, &ccirc;ar estis brakbendo sur
        lia mantelo
        <fnt>
          <aut>G. Grass, trad. W. Kirschstein</aut>
          <vrk><url ref="https://esperanto-ondo.ru/Tl-ger03.htm">el: Mia Jarcento</url></vrk>
          <lok>La Ondo de Esperanto, 1999:11 (61)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_d;&c_z;&c_je;&c_l;&c_mol;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_ch;&c_a;&c_l;&c_mol;&c_c;&c_o;&c_m;</ind></trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_s;&c_ja;&c_b;&c_r;&c_a;&c_m;</ind></trd>
  </trdgrp>
  <trd lng="fr">&ecirc;tre membre</trd>
  <trdgrp lng="pl">
    <trd>by&cacute; cz&lstroke;onkiem</trd>,
    <trd>nale&zdot;e&cacute; do</trd>
  </trdgrp>
</drv>

<drv mrk="an.0aro">
    <kap><ofc>*</ofc><tld/>aro <fnt><bib>F</bib>, <lok>Universala Vortaro, <url
    ref="&AdE;/fundamento/universala_vortaro.html#av-4969">an'ar'</url></lok></fnt></kap>
    <snc>
      <dif>
        Grupo, societo:
        <ekz>
          ne mortos jam nia bravega <tld/>aro, &gcirc;in jam ne timigos la
          vento
          <fnt>
            <aut>L. L. Zamenhof</aut>
            <vrk>La Vojo</vrk>
          </fnt>;
        </ekz>
        <ekz>
          la mondo vidas en mi &ccirc;iam la naturan reprezentanton de la
          <tld/>aro esperantista
          <fnt>
            <bib>Paroloj</bib>
            <vrk>Parolado anta&ubreve; la Kvina Kongreso Esperantista en
            Barcelona en la 6a de septembro 1909</vrk>
          </fnt>;
        </ekz>
        <ekz>
          &ccirc;iu vera religiulo, humanisto a&ubreve; framasono,
          kiu el la <tld/>aro de la parolantoj kaj revantoj transiras
          al la <tld/>aro de la efektivigantoj, fari&gcirc;as per tio
          <ctl>homar<tld/>o</ctl>
          <fnt>
            <bib>Homaranismo</bib>
            <vrk>Pri la homaranismo</vrk>
          </fnt>.
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="be">
        <trd>&c_g;&c_r;&c_u;&c_p;&c_a;</trd>,
        <trd>&c_g;&c_r;&c_a;&c_m;&c_a;&c_d;&c_a;</trd>,
        <trd>&c_z;&c_mol;&c_v;&c_ja;&c_z;</trd>
      </trdgrp>
    <trdgrp lng="ca">
            <trd>grup</trd>,
            <trd>col&middot;lectiu</trd>,
            <trd>associaci&oacute;</trd>
          </trdgrp>
    <trdgrp lng="cs">
      <trd>pos&aacute;dka</trd>,
      <trd>st&aacute;do</trd>,
      <trd>&ccaron;lenstvo</trd>
    </trdgrp>
    <trdgrp lng="de">
        <trd>Gruppe</trd>,
        <trd>Verein</trd>
      </trdgrp>
    <trd lng="en">membership</trd>
    <trdgrp lng="es">
       <trd>grupo</trd>,
       <trd>sociedad</trd>
    </trdgrp>
    <trdgrp lng="fr">
        <trd>groupe</trd>,
        <trd>collectif</trd>
    </trdgrp>
    <trd lng="he">&qof;&bet;&vav;&tsadi;&he;</trd>
    <trdgrp lng="io">
      <trd>grupo</trd>,
      <trd>societo</trd>
    </trdgrp>
    <trdgrp lng="ja">
    <trd>&#x96C6;&#x56E3;  <pr>&#12375;&#12421;&#12358;&#12384;&#12435;</pr></trd>,
    <trd>&#x30B5;&#x30FC;&#x30AF;&#x30EB;  <pr>&#12373;&#x30FC;&#x304F;&#x308B;</pr></trd>,
    <trd>&#x4F1A;  <pr>&#x304B;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
      <trd>ekipa</trd>,
      <trd>zesp&oacute;&lstroke;</trd>,
    </trdgrp>
    <trdgrp lng="ro">
      <trd>echipament</trd>,
      <trd>ansamblu</trd>,
    </trdgrp>
    <trd lng="sk">&ccaron;lenstvo</trd>
    <trdgrp lng="th">
      <trd>&#xE01;&#xE25;&#xE38;&#xE48;&#xE21;</trd>,
      <trd>&#xE2D;&#xE07;&#xE04;&#xE4C;&#xE01;&#xE23;</trd>
    </trdgrp>
    <trdgrp lng="uk">
    <trd>&c_g;&c_r;&c_u;&c_p;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_je;&c_k;&c_t;&c_i;&c_v;</trd>
  </trdgrp>
</drv>

<drv mrk="an.0igxi">
    <kap><tld/>i&gcirc;i</kap>
    <snc mrk="an.0igxi.POL">
      <dif>
        <ref tip="super" cel="al.0igxi">Ali&gcirc;i</ref> al
        grupo, societo, organiza&jcirc;o:
        <ekz>
          <tld/>i&gcirc;i al la kongreso;
        </ekz>
        <ekz>
          Hari <tld/>i&gcirc;as en la teamo de Kvidit&ccirc;o.
          <fnt>
            <bib>Viki</bib>
            <vrk><url ref="&Viki;/Harry_Potter_and_the_Philosopher%27s_Stone">Hari
            Poter kaj la &Scirc;tono de la Sa&gcirc;uloj</url></vrk>
          </fnt>;
        </ekz>
        <ekz>
          Petro Baka <tld/>i&gcirc;as al loka grupo kiu apartenas al la landa
          Esperanto-asocio
          <fnt>
            <vrk><url ref="http://autodidactproject.org/my/mariaemba.html">Maria
            kaj la grupo - autodidactproject.org</url></vrk>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="zh">
    <trd>&#20837;&#x4F0D; <pr>r&#xF9;w&#x1D4;</pr></trd>,
    <trd>&#x52A0;&#20837; <pr>ji&amacron;r&#xF9;</pr></trd>
  </trdgrp>
</snc>
    <trd lng="be">&c_u;&c_s;&c_t;&c_u;&c_p;&c_ib;&c_c;&c_mol; <klr>(&c_u; &c_g;&c_r;&c_a;&c_m;&c_a;&c_d;&c_u;, &c_p;&c_a;&c_r;&c_t;&c_y;&c_ju; &c_ib; &c_d;&c_a;
    &c_g;.&c_p;.)</klr></trd>
        <trdgrp lng="ca">
          <trd>adherir-se</trd>,
          <trd>afiliar-se</trd>
        </trdgrp>
    <trdgrp lng="de">
      <trd>Mitglied werden</trd>,
      <trd>eintreten</trd>
    </trdgrp>
    <trd lng="en">join</trd>
    <trdgrp lng="es">
           <trd>adherirse</trd>,
           <trd>ir en grupo</trd>
        </trdgrp>
  <trd lng="fr">adh&eacute;rer</trd>
    <trd lng="he">&lamed;&he;&tsadi;&tet;&resh;&fpe;</trd>
  <trd lng="ja">&#20837;&#x4F1A;&#12377;&#x308B;  <pr>&#x306B;&#12421;&#12358;&#x304B;&#12356;&#12377;&#x308B;</pr></trd>
  <trdgrp lng="pl">
      <trd>zrzesza&cacute; si&eogonek;</trd>,
      <trd>zg&lstroke;asza&cacute; si&eogonek;</trd>,
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_s;&c_t;&c_u;&c_p;&c_i;&c_t;&c_mol; <klr>(&c_v; &c_o;&c_b;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;, &c_p;&c_a;&c_r;&c_t;&c_i;&c_ju;)</klr></trd>,
      <trd>&c_v;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja; <klr>(&c_v; &c_k;&c_o;&c_l;&c_l;&c_je;&c_k;&c_t;&c_i;&c_v;)</klr></trd></trdgrp>
        <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_i;&c_jeu;&c_d;&c_n;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_s;&c_t;&c_u;&c_p;&c_i;&c_t;&c_i; &c_v; &c_s;&c_p;&c_ib;&c_l;&c_k;&c_u;</trd>,
    <trd>&c_o;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_c;&c_ib;&c_ju;</trd>,
    <trd>&c_u;&c_v;&c_ib;&c_j;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_l;&c_i;&c_t;&c_i;&c_s;&c_ja; &c_v; &c_k;&c_o;&c_l;&c_je;&c_k;&c_t;&c_i;&c_v;</trd>
  </trdgrp>
</drv>
