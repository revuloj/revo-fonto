<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: manikur.xml,v 1.10 2022/05/01 12:05:12 revo Exp $">
<kap>
  <rad>manikur</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="manikur.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="manikur.0i.KOS">
    <dif>
      Manflegi:
      <ekz>
        mi ne kredas, ke tia birdo <tld/>us siajn ungojn sola
        <fnt>
          <aut>J. Rejt&odblac;, trad. L. Bal&aacute;zs</aut>,
          <vrk><url ref="">La dekkvar karata a&#468;tomobilo</url></vrk>,
          <lok>2002-03-11</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia patrino &scirc;in vestas, kombas, nutras, <tld/>as kaj banas
        <fnt>
          <aut>&Scirc;ja&ubreve;&scirc;ja&ubreve;</aut>,
          <vrk><url ref="http://www.ebudhano.cn/cn/blog/">E-budhana
Retejo</url></vrk>,
          <lok>2005-09-27</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pedikur.0i.KOS">pedikuri</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
  <ind>&c_m;&c_a;&c_n;&c_ib;&c_k;&c_ju;&c_r;</ind></trd>
  <trd lng="ca">fer la manicura</trd>
  <trdgrp lng="cs">
    <trd>manik&uacute;rovat</trd>,
    <trd>manik&yacute;rovat</trd>,
    <trd>prov&aacute;d&ecaron;t manik&uacute;ru</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>manicure</trd>,
    <trd>give a manicure</trd>
  </trdgrp>
  <trd lng="fr">manucurer</trd>
  <trd lng="nl">manicuren</trd>
  <trd lng="pl">manicure robi&cacute;</trd>
  <trd lng="ro">f&abreve;c&acirc;nd manichiur&abreve;</trd>
  <trd lng="sk">manik&uacute;rova&tcaron;</trd>
  <trd lng="uk">&c_r;&c_o;&c_b;&c_i;&c_t;&c_i; &c_m;&c_a;&c_n;&c_ib;&c_k;&c_ju;&c_r;</trd>
</drv>

<drv mrk="manikur.0o">
  <kap><tld/>o</kap>
  <snc mrk="manikur.0o.KOS">
    <dif>
      Manflego.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_ib;&c_k;&c_ju;&c_r;</trd>
  <trd lng="ca">manicura</trd>
  <trd lng="cs">manik&uacute;ra</trd>
  <trd lng="en">manicure</trd>
  <trd lng="fr">manucure<klr> (activit&eacute;)</klr></trd>
  <trd lng="nl">manicure</trd>
  <trd lng="pl">manicure</trd>
  <trd lng="ro">manichiur&abreve;</trd>
  <trd lng="sk">manik&uacute;ra</trd>
</drv>

<drv mrk="manikur.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="manikur.0isto.KOS">
    <dif>
      Manflegisto:
      <ekz>
        la du viroj estis &leftquot;&Ccirc;efoj de la <tld lit="M"/>istoj en la Re&gcirc;a Palaco&rightquot;
        <fnt>
        <aut>K. Lendon</aut>,
        <vrk>Pri samseksamo en antikva Egiptio</vrk>,
        <lok><url
        ref="&SCE;">soc.culture.esperanto</url>,
        2001-03-19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_j;&c_s;&c_t;&c_a;&c_r;
  <ind>&c_m;&c_a;&c_n;&c_ib;&c_k;&c_ju;&c_r;&c_u;</ind></trd>
  <trd lng="ca">manicur</trd>
  <trdgrp lng="en">
    <trd>manicurist</trd>,
    <trd>nail technician</trd>
  </trdgrp>
  <trd lng="fr">manucure<klr> (personne)</klr></trd>
  <trd lng="nl">manicuurder</trd>
  <trd lng="pl">manikiurzysta</trd>
  <trd lng="ro">manichiurista</trd>
</drv>
</art>

