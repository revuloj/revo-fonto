<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: podi.xml,v 1.19 2016/03/18 23:10:30 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>podi</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="podi.0o">
  <kap><tld/>o</kap>
  <snc mrk="podi.0o.MEBL">
    <dif>
      Planko altigita super la grundo en kunvenejo, spekta&jcirc;ejo,
      prelegejo, sur kiu staras a&ubreve; sidas la gvidantoj, alparolantoj,
      artistoj...;
      <ref tip="sin" cel="estrad.0o">estrado</ref>:
      <ekz>
        Oraj kaj ar&gcirc;entaj benkoj staris sur <tld/>o el verda
        <klr>...</klr> marmoro
        <fnt><bib>MT</bib>, <lok>&Est; 1:6</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sokl.0o">soklo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_o;&c_d;&c_y;&c_ju;&c_m;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_o;&c_s;&c_t;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_m;&c_o;&c_s;&c_t;&c_k;&c_ib;</trd>,
    <trd>&c_e;&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Podium</trd>,
    <trd>Podest</trd>,
    <trd>Estrade</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>podium</trd>,
    <trd>platform</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>estrade</trd>,
    <trd>podium</trd>
  </trdgrp>
  <trd lng="nl">podium</trd>
  <trdgrp lng="pl">
    <trd>podium</trd>,
    <trd>podwy&zdot;szenie</trd>
  </trdgrp>
  <trd lng="pt">p&oacute;dio</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_m;&c_o;&c_s;&c_t;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_m;&c_o;&c_s;&c_t;&c_k;&c_i;</trd>,
    <trd>&c_e;&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_i;&c_u;&c_m;</trd>
  </trdgrp>
  <trd lng="sv">podium</trd>
  <trdgrp lng="sk">
    <trd>p&oacute;dium</trd>,
    <trd>stupienok</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>podium</trd>,
    <trd>stup&iacute;nek</trd>
  </trdgrp>
</drv>
</art>
