<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>podi</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="podi.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Planko altigita super la grundo por loki en kunveno se&gcirc;ojn por
      a&ubreve;toritatojn:
      <ekz>
        oraj benkoj staris sur <tld/>o el verda marmoro<fnt>Z</fnt>,
      </ekz>
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="estrad.0o">estrado</ref>
  </refgrp>
  <refgrp tip="vid">
    <ref cel="sokl.0o">soklo</ref>
  </refgrp>
  <trdgrp lng="de">
    <trd>Podium</trd>,
    <trd>Podest</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>estrade</trd>,
    <trd>podium</trd>
  </trdgrp>
  <trd lng="nl">podium</trd>
  <trd lng="sv">podium</trd>
</drv>
</art>
