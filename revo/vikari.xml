<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vikari</rad>/o
</kap>
<drv mrk="vikari.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KRI</uzo>
  <snc>
    <dif>
      Vicparo&hcirc;estro:
      <ekz>
        solene li enkonduki&gcirc;is en sian novan postenon, fari&gcirc;ante <tld/>o
        &gcirc;uste de sia naski&gcirc;loko<fnt><aut>Ulrich Matthias</aut>:
        <vrk>Profanaj plezuroj</vrk>, <bib>Monato</bib><lok>jaro 1996a,
        numero 3a, p. 20a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_k;&c_a;&c_r;&c_y;&c_j;</trd>
  <trd lng="de">Vikar</trd>
  <trd lng="el">&epsilon;&phi;&eta;&my;&epsilon_ton;&rho;&jota;&omikron;&sigma_fina;</trd>
  <trdgrp lng="fr">
    <trd>vicaire</trd>
  </trdgrp>
  <trd lng="hu">k&aacute;pl&aacute;n</trd>
  <trdgrp lng="nl">
    <trd>onderpastoor</trd>,
    <trd>hulpprediker</trd>
  </trdgrp>
  <trd lng="pt">vig&aacute;rio</trd>
  <trd lng="ru">&c_v;&c_i;&c_k;&c_a;&c_r;&c_i;&c_j;</trd>
</drv>
</art>
