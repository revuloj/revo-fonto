<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vikari.xml,v 1.17 2019/05/05 12:10:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vikari</rad>/o
</kap>
<drv mrk="vikari.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KRI</uzo>
  <snc>
    <dif>
      Vicparo&hcirc;estro:
      <ekz>
        solene li enkonduki&gcirc;is en sian novan postenon,
        fari&gcirc;ante <tld/>o &gcirc;uste de sia naski&gcirc;loko
        <fnt><aut>Ulrich Matthias</aut>:
        <vrk>Profanaj plezuroj</vrk>, <bib>Monato</bib><lok>jaro 1996a,
        numero 3a, p. 20a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_k;&c_a;&c_r;&c_y;&c_j;</trd>
  <trd lng="ca">vicari</trd>
  <trdgrp lng="cs">
    <trd>c&iacute;rkevn&iacute; z&aacute;stupce</trd>,
    <trd>vik&aacute;&rcaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Vikar</trd>,
     <trd><ofc>*</ofc>Stellvertreter</trd>,     
  </trdgrp>
  <trd lng="el">&epsilon;&phi;&eta;&my;&epsilon_ton;&rho;&jota;&omikron;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>curate</trd>,
    <trd>vicar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>vicaire</trd>
  </trdgrp>
  <trd lng="hu">k&aacute;pl&aacute;n</trd>
  <trdgrp lng="nl">
    <trd>onderpastoor</trd>,
    <trd>hulpprediker</trd>
  </trdgrp>
  <trd lng="pl">wikary</trd>
  <trd lng="pt">vig&aacute;rio</trd>
  <trd lng="ru">&c_v;&c_i;&c_k;&c_a;&c_r;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>vik&aacute;r</trd>,
    <trd>z&aacute;stupca cirkevn&eacute;ho hodnost&aacute;ra</trd>
  </trdgrp>
</drv>
</art>
