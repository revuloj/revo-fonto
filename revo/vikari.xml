<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vikari.xml,v 1.21 2023/10/21 16:50:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vikari</rad>/o
</kap>
<drv mrk="vikari.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KRI</uzo>
  <snc>
    <dif>
      Eklezia anstata&ubreve;anto:
      <ekz>
        la&ubreve; &para;1560 de la katolika kate&hcirc;ismo, &ccirc;iu episkopo, estkiel
        <tld/>o de Kristo, pa&scirc;tiste zorgas pri la aparta Eklezio al li
        konfidita;
      </ekz><ekz>
        &leftquot;la <tld/>o de Jesuo Kristo&rightquot; estas unu el la titoloj de papo
        de Romo.
      </ekz>
    </dif>
    <subsnc mrk="vikari.0o.KAT">
      <uzo tip="fak">KAT</uzo>
      <dif>
        Vicparo&hcirc;estro:
        <ekz>
          solene li enkonduki&gcirc;is en sian novan postenon,
          fari&gcirc;ante <tld/>o &gcirc;uste de sia naski&gcirc;loko
          <fnt><aut>Ulrich Matthias</aut>:
          <vrk>Profanaj plezuroj</vrk>, <bib>Monato</bib><lok>jaro 1996a,
          numero 3a, p. 20a</lok></fnt>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="vikari.0o.PROT"><fnt><bib>PIV2</bib></fnt>
      <dif>
        En iuj protestantaj eklezioj: helppredikanto.
      </dif>
    </subsnc>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_k;&c_a;&c_r;&c_y;&c_j;</trd>
  <trd lng="ca">vicari</trd>
  <trdgrp lng="cs">
    <trd>c&iacute;rkevn&iacute; z&aacute;stupce</trd>,
    <trd>vik&aacute;&rcaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Vikar</trd>,
     <trd><ofc>*</ofc>Stellvertreter</trd>,     
  </trdgrp>
  <trd lng="el">&epsilon;&phi;&eta;&my;&epsilon_ton;&rho;&jota;&omikron;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>curate</trd>,
    <trd><ofc>*</ofc>vicar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd><ofc>*</ofc>vicaire</trd>
  </trdgrp>
  <trd lng="hu">k&aacute;pl&aacute;n</trd>
  <trd lng="ja">&#x52A9;&#x4EFB;&#x53F8;&#x796D;  <pr>&#x3059;&#x3051;&#x3068;&#x3046;&#x3057;&#x3055;&#x3044;</pr></trd>
  <trdgrp lng="nl">
    <trd>onderpastoor</trd>,
    <trd>hulpprediker</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd><ofc>*</ofc>zast&eogonek;pca</trd>,
    <trd>wikary</trd>
  </trdgrp>
  <trd lng="pt">vig&aacute;rio</trd>
  <trdgrp lng="ru">
    <trd><ofc>*</ofc>&c_n;&c_a;&c_m;&c_je;&c_s;&c_t;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_v;&c_i;&c_k;&c_a;&c_r;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vik&aacute;r</trd>,
    <trd>z&aacute;stupca cirkevn&eacute;ho hodnost&aacute;ra</trd>
  </trdgrp>
  <trd lng="uk">&c_p;&c_o;&c_m;&c_ib;&c_ch;&c_n;&c_i;&c_k; &c_jeu;&c_p;&c_i;&c_s;&c_k;&c_o;&c_p;&c_a;</trd>
</drv>
</art>
