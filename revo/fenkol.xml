<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fenkol.xml,v 1.27 2023/11/09 19:13:16 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>fenkol</rad>/o
</kap>
<drv mrk="fenkol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">Foeniculum</trd>), aroma herbo;
      unu specio estas kulturata kiel legomo
      kaj kies semojn oni uzas kiel
      <ref tip="lst" cel="spic.0o" lst="voko:spicoj">spico</ref>:
      <ekz>
        jen estas <tld/>o por vi
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        ordinara <tld/>o de kiu semoj kaj la folioj uzatas en 
        la kuirarto, la semoj &ccirc;efe en sa&ubreve;coj, fi&scirc;a&jcirc;oj kaj pano,
        la folioj por salatoj, kuirita legomo, supoj kaj fi&scirc;a&jcirc;oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1293">Fenkolo</url></lok>
        </fnt>.
      </ekz>        
    </dif>
    <bld lok="&WCU;/thumb/f/f0/Foeniculum_vulgare_001.JPG/576px-Foeniculum_vulgare_001.JPG" prm="CC BY-SA 3.0">ordinara <tld/>o (Foeniculum vulgare)
        <fnt>
          <aut>H. Zell, propra verko</aut>
          <lok><url ref="&WCW;/File:Foeniculum_vulgare_001.JPG">&WCW;/File:Foeniculum_vulgare_001.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_f;&c_e;&c_n;&c_h;&c_je;&c_l;&c_mol;</trd>
  <trd lng="bg">&c_r;&c_je;&c_z;&c_je;&c_n;&c_je;</trd>
  <trdgrp lng="ca">
    <trd>fonoll</trd>,
    <trd>g&egrave;nere Foeniculum</trd>
  </trdgrp>
  <trd lng="cs">fenykl obecn&yacute;</trd>
  <trd lng="de">Fenchel</trd>
  <trd lng="en">fennel</trd>
  <trd lng="fa">&a_r;&a_A;&a_z;&f_y;&a_A;&a_n;&a_h;</trd>
  <trd lng="fr">fenouil</trd>
  <trd lng="hu">&eacute;desk&ouml;m&eacute;ny</trd>
  <trd lng="ja">&#x30A6;&#x30A4;&#x30AD;&#x30E7;&#x30A6;</trd>
  <trd lng="nl">venkel</trd>
  <trdgrp lng="pl">
    <trd>koper <klr>w&lstroke;oski</klr></trd>,
    <trd>fenku&lstroke; <klr>w&lstroke;oski</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>funcho</trd>,
    <trd>erva-doce</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_je;&c_n;&c_h;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">fenikel</trd>
  <trd lng="uk">&#x444;&#x435;&#x43D;&#x445;&#x435;&#x43B;&#x44C;</trd>
  <trdgrp lng="zh">
    <trd>&#x5C0F;&#x8334;&#x9999; <pr>xi&#x1CE;ohu&#xED;xi&#x101;ng</pr></trd>,
    <trd>&#x8334; <pr>hu&#xED;</pr></trd>,
    <trd>&#x8334;&#x9999; <pr>hu&#xED;xi&#x101;ng</pr></trd>
  </trdgrp>
</drv>
</art>
