<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fenkol</rad>/o
</kap>
<drv mrk="fenkol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">Foeniculum</trd>). La aroma legomo estas kulturata.   
    </dif>
  </snc>
  <trd lng="de">Fenchel</trd>
  <trd lng="fr">fenouil</trd>
  <trd lng="nl">venkel</trd>
  <trd lng="ru">&c_f;&c_je;&c_n;&c_h;&c_je;&c_l;&c_mol;</trd>
</drv>
</art>
