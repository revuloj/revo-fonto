    <trd lng="be">&c_F;&c_a;&c_j;&c_s;&c_a;&c_l;&c_ja;&c_b;&c_a;&c_d;</trd>
    <trd lng="de">Faisalabad</trd>
    <trd lng="en">Faisalabad</trd>
    <trd lng="fr">Faisalabad</trd>
    <trd lng="ur">&a_f;&f_y;&a_S;&a_l; &a_A_madda;&a_b;&a_A;&a_d;</trd>
  </snc>
</drv>

