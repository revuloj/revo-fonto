<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>jod</rad>/o
</kap>
<drv mrk="jod.0o">
<kap><tld/>o</kap>
<uzo tip="fak">KEM</uzo>
<snc>
<dif>
&Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
atomnumero 53, atompezo 126,9044, simbolo I. Simpla korpo, havanta formon de
folietoj blue grizaj kun metala
brilo, uzata en medicino kaj fotografado: <ekz><tld/>a tinkturo.
</ekz>
</dif>
</snc>
<trd lng="be">&c_jo;&c_d;</trd>     
<trd lng="br">iod</trd>
<trd lng="de">Jod</trd>
<trd lng="el">&jota;&omega_ton;&delta;&jota;&omikron;</trd>
<trd lng="es">yodo</trd>
<trd lng="hu">j&oacute;d</trd>
<trdgrp lng="it">
     <trd>iodio</trd>,
     <trd>iodo</trd>
</trdgrp>
<trdgrp lng="nl">
     <trd>jood<klr tip="ind"> (jodium)</klr></trd>,
     <trd>jodium</trd>
</trdgrp>
<trd lng="pt">iodo</trd>
<trd lng="fr">iode</trd>
<trd lng="ru">&c_j;&c_o;&c_d;</trd>
<trd lng="sv">jod</trd>
</drv>
</art>
