<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: burask.xml,v 1.11 2025/09/11 10:32:57 revo Exp $">
<kap>
  <rad>burask</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="burask.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <uzo tip="fak">MET</uzo>
  <snc mrk="burask.0o.ciklono">
    <dif>
      <ref tip="sin" cel="ciklon.0o">Ciklono</ref>, <ref tip="dif" cel="tempes.0o">tempesto</ref>:
      <ekz>
        efektive, <tld/>oj kaj kontra&ubreve;ciklonoj agas tute sendepende de la veteraj mapoj
        kaj fizikistaj kalkuloj
        <fnt>
          <bib>Kon11</bib>
          <lok>Blogo de eksedzo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Unwetter</trd>
  </snc>
  <snc mrk="burask.0o.ventopusxo">
    <dif>
      <ref tip="dif" cel="pusx.vento0o.MET">Ventopu&scirc;o</ref>:
      <ekz>
        <tld/>o jam forpelis burdojn
        <fnt>
          <aut>Mao Zifu</aut>,
          <vrk>Post Pluvo</vrk>,
          <lok><url ref="http://www.elerno.cn/penseo/pen31.htm">
          Penseo, 1993-01 (31)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        en 1834 aperis lia unua verko &leftquot;<tld lit="B"/>o&rightquot;
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=283568">Sergio
          Aksakov</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <!-- forman maloftan traduku per malofta&jcirc;o se tia ekzistas -->
    <trd lng="cs">pl&iacute;skanice</trd>
    <trd lng="de">Windb&ouml;</trd>
    <trd lng="en">squall</trd>
    <trd lng="es">borrasca</trd>
    <trdgrp lng="fr">
      <trd>rafale</trd>,
      <trd>bourrasque</trd>
    </trdgrp>
    <trd lng="nl">windstoot</trd>
    <trd lng="sk">&ccaron;&lcaron;apkanica</trd>
    <trd lng="uk">&c_s;&c_i;&c_l;&c_mol;&c_n;&c_i;&c_j; &c_v;&c_ib;&c_t;&c_je;&c_r; &c_z; &c_d;&c_o;&c_shch;&c_je;&c_m;
    &c_a;&c_b;&c_o;&nbsp;&c_s;&c_n;&c_ib;&c_g;&c_o;&c_m;</trd>
  </snc>
<trdgrp lng="be">
  <trd>&c_b;&c_u;&c_r;&c_a;</trd>,
  <trd>&c_sh;&c_t;&c_o;&c_r;&c_m;</trd>,
  <trd>&c_c;&c_y;&c_k;&c_l;&c_jo;&c_n;</trd>,
  <trd>&c_b;&c_u;&c_r;&c_a;&c_n;</trd>
</trdgrp>
</drv>
</art>

