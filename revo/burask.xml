<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: burask.xml,v 1.9 2022/05/24 06:39:28 revo Exp $">
<kap>
  <rad>burask</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="burask.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="burask.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      <ref tip="dif" cel="pusx.vento0o.MET">Ventopu&scirc;o</ref>:
      <ekz>
        <tld/>o jam forpelis burdojn
        <fnt>
          <aut>Mao Zifu</aut>,
          <vrk>Post Pluvo</vrk>,
          <lok><url ref="http://www.elerno.cn/penseo/pen31.htm">
          Penseo, 1993-01 (31)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        en 1834 aperis lia unua verko &leftquot;<tld lit="B"/>o&rightquot;
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=283568">Sergio
          Aksakov</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <!-- forman maloftan traduku per malofta&jcirc;o se tia ekzistas -->
  <trd lng="be">&c_b;&c_u;&c_r;&c_a;&c_n;</trd>
  <trd lng="de">Windb&ouml;</trd>
  <trd lng="en">squall</trd>
  <trd lng="es">borrasca</trd>
  <trd lng="fr">rafale</trd>
  <trd lng="nl">windstoot</trd>
  <trd lng="sk">&ccaron;&lcaron;apkanica</trd>
  <trd lng="cs">pl&iacute;skanice</trd>
  <trd lng="uk">&c_s;&c_i;&#x43B;&#x44C;&#x43D;&c_i;&c_j; &c_v;&c_ib;&c_t;&c_je;&c_r; &c_z; &c_d;&#x43E;&c_shch;&c_je;&#x43C;
  &c_a;&c_b;&#x43E;&#xA0;&c_s;&#x43D;&c_ib;&c_g;&#x43E;&#x43C;</trd>
</drv>
</art>

