  </trdgrp>
  <trd lng="uk">&#x43B;&#x43E;&c_g;&c_ib;&#x43A;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x903B;&#x8F91; <pr>lu&#xF3;ji</pr></trd>,
