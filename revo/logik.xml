<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>logik</rad>/o
</kap>
<drv mrk="logik.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Fako de la filozofio studanta la le&gcirc;ojn de rezonado:
      <ekz>
        fakultato de filozofio kaj <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a traktato;
      </ekz>
      <ekz>
        <tld/>a konkludo.
      </ekz>
    </dif>
    <trd lng="en">Logik</trd>
  </snc>
  <snc>
    <dif>
      <ref tip="dif" cel="logik.0eco"><tld lit="L"/>eco</ref>:
      <ekz>
        &ccirc;u ekzistas e&ccirc; guto da senco kaj <tld/>o en tiuj
        &scirc;ercoj?<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="en">Schl&uuml;ssigkeit</trd>
  </snc>
  <trd lng="el">&lambda;&omikron;&gamma;&jota;&kappa;&eta_ton;</trd>
  <trd lng="en">logic</trd>
  <trd lng="es">l&oacute;gica</trd>
  <trd lng="fr">logique<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="nl">logica</trd>
  <trd lng="pt">l&oacute;gica</trd>
  <trd lng="ru">&c_l;&c_o;&c_g;&c_i;&c_k;&c_a;</trd>
</drv>

<drv mrk="logik.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Konforma al la <tld/>o; kapabla <tld/>e rezoni.
    </dif>
  </snc>
  <trd lng="el">&lambda;&omikron;&gamma;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trd lng="fr">logique<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="pt">l&oacute;gico</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_l;&c_o;&c_g;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="logik.0eco">
  <kap>
    <tld/>eco
  </kap>
  <snc>
    <dif>
      Kvalito de io <tld/>a.
    </dif>
  </snc>
  <adm>
  Kiel nomi la econ de IU ~a (kapabla ~e rezoni)? Ankaux ~eco
  mi supozas. <aut>MB</aut>
  </adm>
  <trd lng="fr">logique<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="en">Schl&uuml;ssigkeit</trd>
  <trd lng="nl">logica</trd>
  <trd lng="ru">&c_l;&c_o;&c_g;&c_i;&c_ch;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="logik.matematika0o">
  <kap>
    matematika <tld/>o
  </kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Bran&ccirc;o de <ref tip="malprt" cel="matema.0o">matematiko</ref>,
      studanta la formalajn rilatojn inter propozicioj kaj ilian aplikon
      al la demonstrado de matematikaj teoremoj.
    </dif>
    <ref tip="sin" cel="logistik.0o">logistiko;</ref>
    <refgrp tip="vid">
      <ref cel="propoz.0o.aserto">propozicio</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>mathematische <ind>Logik</ind></trd>,
    <trd>Logistik</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>mathematical <ind>logic</ind></trd>,
    <trd>logistics</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>logique math&eacute;matique</trd>,
    <trd>logistique<klr tip="ind"> (logique math&eacute;matique)</klr></trd>
  </trdgrp>
  <trd lng="pt">l&oacute;gica matem&aacute;tica</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_t;&c_je;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja; <ind>&c_l;&c_o;&c_g;&c_i;&c_k;&c_a;</ind></trd>,
    <trd>&c_l;&c_o;&c_g;&c_i;&c_s;&c_t;&c_i;&c_k;&c_a; <klr>(&c_u;&c_s;&c_t;.)</klr></trd>
  </trdgrp>
</drv>
</art>
