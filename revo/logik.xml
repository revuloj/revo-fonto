<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>logik</rad>/o
</kap>
<drv mrk="logik.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">FIL</uzo>
    <dif>
      Teorio pri valida inferenco.
    </dif>
  </snc>
  <snc>
    <dif>
      Regularo kiu subtenas validajn rezultojn:
      <ekz>
        <tld/>o de la komerco; <tld/>o de la evoluo
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="fak">FIL</uzo>
    <dif>
      Fako de la filozofio studanta la le&gcirc;ojn de rezonado:
      <ekz>
        fakultato de filozofio kaj <tld/>o
        <fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a traktato;
      </ekz>
      <ekz>
        <tld/>a konkludo.
      </ekz>
    </dif>
    <trd lng="en">Logik</trd>
  </snc>
  <snc>
    <dif>
      <ref tip="dif" cel="logik.0eco"><tld lit="L"/>eco
      </ref>:
      <ekz>
        &ccirc;u ekzistas e&ccirc; guto da senco kaj <tld/>o en tiuj
        &scirc;ercoj?<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">Schl&uuml;ssigkeit</trd>
  </snc>
  <trd lng="be">&c_l;&c_jo;&c_g;&c_ib;&c_k;&c_a;
  </trd>
  <trd lng="el">&lambda;&omikron;&gamma;&jota;&kappa;&eta_ton;
  </trd>
  <trd lng="en">logic</trd>
  <trd lng="es">l&oacute;gica</trd>
  <trd lng="fr">logique<klr tip="ind"> (subst.)
  </klr></trd>
  <trd lng="hu">logika</trd>
  <trd lng="nl">logica</trd>
  <trd lng="pl">logika</trd>
  <trd lng="pt">l&oacute;gica</trd>
  <trd lng="ru">&c_l;&c_o;&c_g;&c_i;&c_k;&c_a;
  </trd>
  <trd lng="sk">logika</trd>
  <trd lng="sk">logica</trd>
</drv>

<drv mrk="logik.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Konforma al la <tld/>o; kapabla <tld/>e rezoni.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_g;&c_ib;&c_ch;&c_n;&c_y;
  </trd>
  <trd lng="de">logisch</trd>
  <trd lng="el">
  &lambda;&omikron;&gamma;&jota;&kappa;&omikron_ton;&sigma_fina;
  </trd>
  <trd lng="fr">logique<klr tip="ind"> (adj.)
  </klr></trd>
  <trdgrp lng="hu">
    <trd>logikus</trd>,
    <trd>logikai</trd>
  </trdgrp>
  <trd lng="pl">logiczny</trd>
  <trd lng="pt">l&oacute;gico</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;
    </trd>,
    <trd>&c_l;&c_o;&c_g;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">logick&yacute;</trd>
  <trd lng="it">logico</trd>
</drv>

<drv mrk="logik.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Kvalito de io <tld/>a.
    </dif>
  </snc>
  <adm>
    Kiel nomi la econ de IU ~a (kapabla ~e rezoni)? Ankaux ~eco
    mi supozas. <aut>MB</aut>
  </adm>
  <trd lng="be">
  &c_l;&c_ja;&c_g;&c_ib;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;
  </trd>
  <trd lng="de">Logischsein</trd>
  <trd lng="fr">logique<klr tip="ind"> (subst.)
  </klr></trd>
  <trd lng="en">Schl&uuml;ssigkeit</trd>
  <trd lng="hu">logikuss&aacute;g</trd>
  <trd lng="nl">logica</trd>
  <trd lng="pl">logiczno&sacute;&cacute;</trd>
  <trd lng="ru">&c_l;&c_o;&c_g;&c_i;&c_ch;&c_n;&c_o;&c_s;&c_t;&c_mol;
  </trd>
  <trd lng="sk">logika</trd>
  <trd lng="it">logica</trd>
</drv>

<drv mrk="logik.matematika0o">
  <kap>matematika <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Bran&ccirc;o de <ref tip="malprt" cel="matema.0o">matematiko</ref>,
      studanta la formalajn rilatojn inter propozicioj kaj ilian aplikon
      al la demonstrado de matematikaj teoremoj.
    </dif>
    <ref tip="sin" cel="logistik.0o">logistiko;</ref>
    <ref tip="sin" cel="logik.formala0o">formala <tld/>o;</ref>
    <refgrp tip="vid">
      <ref cel="propoz.0o.aserto">propozicio</ref>.
    </refgrp>
  </snc>
  <trd lng="be">
  &c_m;&c_a;&c_t;&c_e;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_ja;
  <ind>&c_l;&c_jo;&c_g;&c_ib;&c_k;&c_a;</ind>
  </trd>
  <trdgrp lng="de">
    <trd>mathematische <ind>Logik</ind>
    </trd>,
    <trd>Logistik</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>mathematical <ind>logic</ind>
    </trd>,
    <trd>logistics</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>logique math&eacute;matique</trd>,
    <trd>logistique
    <klr tip="ind"> (logique math&eacute;matique)</klr>
    </trd>
  </trdgrp>
  <trd lng="hu">matematikai <ind>logika</ind>
  </trd>
  <trdgrp lng="pl">
    <trd>logika matematyczna</trd>,
    <trd>logistyka<klr> (przest.)</klr></trd>
  </trdgrp>
  <trd lng="pt">l&oacute;gica matem&aacute;tica
  </trd>
  <trdgrp lng="ru">
    <trd>
    &c_m;&c_a;&c_t;&c_je;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja;
    <ind>&c_l;&c_o;&c_g;&c_i;&c_k;&c_a;</ind></trd>,
    <trd>&c_l;&c_o;&c_g;&c_i;&c_s;&c_t;&c_i;&c_k;&c_a;
    <klr tip="ind"> (&c_u;&c_s;&c_t;.)</klr>
    </trd>
  </trdgrp>
  <trd lng="sk">matematick&aacute; <ind>logika</ind>
  </trd>
  <trd lng="it">logica matematica</trd>
</drv>

<drv mrk="logik.formala0o">
  <kap>formala <tld/>o</kap>
  <uzo tip="fak">FIL</uzo>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      Nuntempa maniero de la logiko, kiu anstata&ubreve;igas la normala lingvo
      per alia lingvo (formala) bazita sur simboloj kaj specifaj reguloj, tial
      ke la vortigo estas la&ubreve; la principoj de la rikura generi&gcirc;o.
      <ref tip="sin" cel="logik.matematika0o">matematika <tld/>o</ref>
    </dif>
  </snc>
  <trd lng="it">logica formale</trd>
  <trd lng="pl">logika formalna</trd>
</drv>

<drv mrk="logik.simbola0o">
  <kap>simbola <tld/>o</kap>
  <uzo tip="fak">FIL</uzo>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="logik.formala0o">formala <tld/>o</ref>
    </dif>
  </snc>
  <trd lng="it">logica simbolica</trd>
  <trd lng="pl">logika symboliczna</trd>
</drv>

<drv mrk="logik.deonto0o">
  <kap>deonto<tld/>o</kap>
  <uzo tip="fak">FIL</uzo>
  <snc>
    <dif>
      Netradicia logik-maniero bazita sur valoroj &leftquot;valida&rightquot;
      kaj &leftquot;nevalida&rightquot; anstataa&ubreve;
      &leftquot;vera&rightquot; kaj &leftquot;malvera&rightquot; kaj sur la
      studado de la t.n. &leftquot;praktika silogismo&rightquot;.
    </dif>
  </snc>
  <adm>
    Ne ekzistas radiko "deont".
    <aut>MB</aut>
  </adm>
  <trd lng="it">logica deontica</trd>
  <trd lng="pl">logika deontyczna</trd>
</drv>

<drv mrk="logik.lauxvera0o">
  <kap>la&ubreve;vera <tld/>o</kap>
  <uzo tip="fak">FIL</uzo>
  <snc>
    <dif>
      Tradicia logiko, bazita sur la studado de la asertoj kaj sur neta valoro
      de vereco.
    </dif>
  </snc>
  <trdgrp lng="it">
    <trd>logica aletica</trd>,
    <trd>logica verofunzionale</trd>
  </trdgrp>
</drv>

<drv mrk="logik.svaga0o">
  <kap>svaga <tld/>o</kap>
  <uzo tip="fak">FIL</uzo>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      Logiko bazita sur pluraj valoroj de logika vereco.
    </dif>
  </snc>
  <trd lng="it">logica sfumata</trd>
  <trd lng="en">fuzzy logic</trd>
  <trd lng="pl">logika rozmyta</trd>
</drv>

<drv mrk="logik.neta0o">
  <kap>neta <tld/>o</kap>
  <uzo tip="fak">FIL</uzo>
  <snc>
    <dif>
      Tradiciaj logik-manieroj bazitaj sur neta valoro (ekz.: vero a&ubreve;
      malvero; 0 a&ubreve; 1; valideco a&ubreve; nevalideco).
    </dif>
  </snc>
  <trd lng="it">logica non-sfumata</trd>
</drv>
</art>
