<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tangen.xml,v 1.26 2023/09/04 17:49:39 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>tangent</rad>/o
</kap>
<drv mrk="tangen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 20</lok></fnt>
  <snc mrk="tangen.0o.entriangulo">
    <dif>
      <klr>(de <ref cel="angul.0o.MAT">angulo</ref> <frm>&alfa;</frm>)</klr>
      Kvociento de &gcirc;ia
      <ref cel="sinus.0o">sinuso</ref> per &gcirc;ia
      <ref cel="kosinu.0o">kosinuso</ref>; simb. <frm>tg &alfa;</frm>
      a&ubreve; <frm>tang &alfa;</frm>:
      <ekz>
        la <tld/>o de orto estas nefinia;
      </ekz>
      <ekz>
        la <tld/>o de angulo egalas al la kontra&ubreve;egalo de la <tld/>o de
        &gcirc;ia <ref cel="suplem.0o.angulo">suplemento</ref>;
      </ekz>
      <ekz>
        la <tld/>o de akuta angulo en orta triangulo egalas al la
        kvociento de amba&ubreve; <ref cel="katet.0o">katetoj</ref>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      Ne konfuzu kun:
      <ref cel="tangx.0anto">tan&gcirc;anto</ref>.
    </refgrp>
    <bld lok="../bld/trigo.jpg">
      <!-- la <tld/>o de angulo <frm>&alfa;</frm> egalas al <frm>AT/OM</frm>
      -->
      la <tld/>o de angulo &alfa; egalas al AT/OM 
    </bld>
  </snc>
  <snc mrk="tangen.0o.funkcio">
    <dif>
      Reela <ref tip="super" cel="funkci.0o.MAT">funkcio</ref>, kiu
      &jcirc;etas
      reelon <frm am="x"><k>x</k></frm> al la 
      <tld/>o <sncref ref="tangen.0o.entriangulo"/> de angulo, kies
      mezuro en 
      <ref cel="radian.0o">radianoj</ref> diferencas
      de <frm am="x"><k>x</k></frm> per oblo de <frm am="pi">&pi;</frm>; 
      simb. <frm>tg</frm> a&ubreve; <frm>tang</frm>:
      <ekz>
        <tld/>o estas perioda funkcio, kun
        <ref cel="period.0o.funkcio">periodo <sncref/></ref>
        <frm am="pi">&pi;</frm>;
      </ekz>
      <ekz>
        <tld/>o strebas al nefinio, kiam la argumento strebas al
        <frm am="pi//2">&pi;/2</frm>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_n;&c_g;&c_je;&c_n;&c_s;</trd>
  <trd lng="ca">tangent</trd>
  <trd lng="cs">tangens <klr>(funkce)</klr></trd>
  <trd lng="de">Tangens</trd>
  <trd lng="en">tangent</trd>
  <trd lng="es">tangente<klr tip="amb"> (matem&aacute;ticas)</klr></trd>
  <trd lng="fr">tangente<klr tip="ind"> (trigo.)</klr></trd>
  <trd lng="hu">tangens</trd>
  <trd lng="ja">&#x30BF;&#x30F3;&#x30B8;&#x30A7;&#x30F3;&#x30C8;</trd>
  <trd lng="nl">tangens</trd>
  <trd lng="pl">tangens</trd>
  <trdgrp lng="pt">
   <trd>tangente</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_n;&c_g;&c_je;&c_n;&c_s;</trd>
  <trd lng="sk">tangens <klr>(funkcie)</klr></trd>
  <trd lng="uk">&#x442;&#x430;&#x43D;&#x491;&#x435;&#x43D;&#x441;</trd>
</drv>

<drv mrk="tangen.are0o">
  <kap>are<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <ref tip="dif" cel="tangen.inversa_hiperbola0o">inversa hiperbola
    <tld/>o.</ref>
    <rim>
      Vd rimarkon sub <ref cel="are.0o.argumento">areo</ref>.
    </rim>
  </snc>
</drv>

<drv mrk="tangen.ark0o">
  <kap>ark<tld/>o, <var><kap>arko<tld/>o</kap></var></kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      <ref cel="bild.inversa0igo">Inversa bildigo</ref> de
      la <ref cel="vast.mal0igajxo.bildigo">malvastiga&jcirc;o</ref> de
      <ref tip="super" cel="funkci.0o.MAT">funkcio</ref>
      <ref cel="tangen.0o.funkcio"><tld/>o</ref> al la
      intervalo <frm am="(-pi//2,pi//2)">]-&pi;/2,&pi;/2[</frm>;  simb.
      <frm>arctg</frm>
      a&ubreve; <frm>arktang</frm>:
      <ekz>
        ark<tld/>o de <frm>1</frm> egalas al <frm am="pi//4">&pi;/4</frm>.
      </ekz>
    </dif>
    <rim>
      Vd rimarkon sub <ref cel="ark.0o.angulo">arko</ref>.
    </rim>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_k;&c_t;&c_a;&c_n;&c_g;&c_je;&c_n;&c_s;</trd>
  <trd lng="ca">arctangent</trd>
  <trd lng="cs">arkustangens</trd>
  <trd lng="de">Arkustangens</trd>
  <trd lng="en">arc tangent</trd>
  <trd lng="fr">arc tangente</trd>
  <trd lng="hu">arkusz tangens</trd>
  <trd lng="pl">arcus tangens</trd>
  <trd lng="ru">&c_a;&c_r;&c_k;&c_t;&c_a;&c_n;&c_g;&c_je;&c_n;&c_s;</trd>
  <trd lng="sk">arkus tangens</trd>
</drv>

<drv mrk="tangen.hiperbola0o">
  <kap>hiperbola <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      Reela <ref tip="super" cel="funkci.0o.MAT">funkcio</ref>, kiu
      &jcirc;etas
      reelon <frm am="x"><k>x</k></frm> al 
      <frm am="(sinh x)//(cosh x)">(sinh <k>x</k>)/(cosh
      <k>x</k>)</frm>; simb. <frm>tangh</frm>
      
      a&ubreve; <frm>tgh</frm>.
    </dif>
  </snc>
  <trd lng="ca"><ind>tangent</ind> hiperb&ograve;lica</trd>
  <trdgrp lng="de">
    <trd>Hyperbeltangens</trd>,
    <trd>hyperbolischer <ind>Tangens</ind></trd>
  </trdgrp>
  <trd lng="be">&c_g;&c_ib;&c_p;&c_e;&c_r;&c_b;&c_a;&c_l;&c_ib;&c_ch;&c_n;&c_y; <ind>&c_t;&c_a;&c_n;&c_g;&c_je;&c_n;&c_s;</ind></trd>
  <trd lng="en">hyperbolic <ind>tangent</ind></trd>
  <trd lng="fr">tangente hyperbolique</trd>
  <trd lng="hu">tangens hiperbolikusz</trd>  
  <trd lng="pl">tangens hiperboliczny</trd>
  <trd lng="ru">&c_g;&c_i;&c_p;&c_je;&c_r;&c_b;&c_o;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; <ind>&c_t;&c_a;&c_n;&c_g;&c_je;&c_n;&c_s;</ind></trd>
</drv>

<drv mrk="tangen.inversa_hiperbola0o">
  <kap>inversa hiperbola <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <ref cel="bild.inversa0igo">Inversa bildigo</ref> de
      la <ref cel="vast.mal0igajxo.bildigo">malvastiga&jcirc;o</ref> de
      <ref tip="super" cel="funkci.0o.MAT">funkcio</ref>
      <ref cel="tangen.hiperbola0o">hiperbola <tld/>o</ref> al la
      intervalo <frm am="[0,+oo)">[0,+&#x221E;[</frm>; simb. <frm>artangh</frm>
      
      a&ubreve; <frm>artgh</frm>.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_v;&c_a;&c_r;&c_o;&c_t;&c_n;&c_y; &c_g;&c_ib;&c_p;&c_e;&c_r;&c_b;&c_a;&c_l;&c_ib;&c_ch;&c_n;&c_y; <ind>&c_t;&c_a;&c_n;&c_g;&c_je;&c_n;&c_s;</ind></trd>
  <trd lng="ca"><ind>tangent</ind> hiperb&ograve;lica inversa</trd>
  <trd lng="de">Areatangens</trd>
  <trdgrp lng="en">
    <trd>area-hyperbolic tangent</trd>,
    <trd>inverse hyperbolic tangent</trd></trdgrp> 
  <trdgrp lng="fr">
    <trd>argument de la <ind>tangente</ind> hyperbolique</trd>,
    <trd>tangente hyperbolique inverse</trd>
  </trdgrp>
  <trd lng="hu">arkusz tangens hiperbolikusz</trd>  
  <trd lng="pl">area tangens hiperboliczny</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_je;&c_a;-&c_t;&c_a;&c_n;&c_g;&c_je;&c_n;&c_s; &c_g;&c_i;&c_p;&c_je;&c_r;&c_b;&c_o;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_o;&c_b;&c_r;&c_a;&c_t;&c_n;&c_y;&c_j; &c_g;&c_i;&c_p;&c_je;&c_r;&c_b;&c_o;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; <ind>&c_t;&c_a;&c_n;&c_g;&c_je;&c_n;&c_s;</ind></trd>
  </trdgrp>
</drv>
</art>
