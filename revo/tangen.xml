<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>tangent</rad>/o
</kap>
<drv mrk="tangen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(de <ref cel="angul.0o.MAT">angulo</ref> &alfa;)</klr>
      Kvociento de &gcirc;ia
      <ref cel="sinus.0o">sinuso</ref> per &gcirc;ia 
      <ref cel="kosinu.0o">kosinuso</ref>; simb. tg&#xa0;&alfa; :
      <ekz>
        la <tld/>o de orto estas nefinia;
      </ekz>
      <ekz>
        la <tld/>o de angulo egalas al la negativo de la <tld/>o de &gcirc;ia
        <ref cel="suplem.0o.angulo">suplemento</ref>;
      </ekz>
      <ekz>
        la <tld/>o de akuta angulo en orta triangulo egalas al la
        kvociento de amba&ubreve; <ref cel="katet.0o">katetoj</ref>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      Ne konfuzu kun:
      <ref cel="tangx.0anto">tan&gcirc;anto</ref>.
    </refgrp>
  </snc>
  <bld lok="../bld/trigo.jpg">
    la <tld/>o de angulo &alfa; egalas al AT/OM
  </bld>    
  <trd lng="en">tangent</trd>
  <trd lng="de">Tangens</trd>
  <trd lng="fr">tangente<klr tip="ind"> (trigo.)</klr></trd>
  <trd lng="ru">&c_t;&c_a;&c_n;&c_g;&c_je;&c_n;&c_s;</trd>
</drv>
</art>
