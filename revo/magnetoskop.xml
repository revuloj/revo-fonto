<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>magnetoskop</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="magnetoskop.0o">
  <kap><tld/>o</kap>
  <snc mrk="magnetoskop.0o.KIN">
    <uzo tip="fak">KIN</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="regist.vide0ilo.KIN">Videregistrilo</ref>
      konservanta en magneta formo datumojn de sonoj kaj bildoj;
      videobendregistrilo:
      <ekz>
        
        <fnt>
          <aut></aut>,
          <vrk></vrk>,
          <lok></lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">magn&eacute;toscope</trd>
</drv>
</art>

