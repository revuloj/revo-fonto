<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: magnetoskop.xml,v 1.9 2024/01/25 20:10:33 revo Exp $">
<kap>
  <rad>magnetoskop</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="magnetoskop.0o">
  <kap><tld/>o</kap>
  <snc mrk="magnetoskop.0o.KIN">
    <uzo tip="fak">KIN</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="regist.vide0ilo.KIN">Videregistrilo</ref>
      konservanta en magneta formo datumojn de sonoj kaj bildoj;
      videobendregistrilo:
      <ekz>
        estis &scirc;telitaj du bicikloj <klr>...</klr> la magnetoskopo kaj
        mia
        malnova radiokased<klr>il</klr>o
        <fnt>
          <aut>M. Corobu</aut>,
          <vrk>jen mi</vrk>,
          <lok>soc.culture.esperanto, 2004-08-05</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_d;&c_e;&c_a;&c_m;&c_a;&c_g;&c_n;&c_ib;&c_t;&c_a;&c_f;&c_o;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>magnetoskop</trd>,
    <trd>videorekord&eacute;r</trd>
  </trdgrp>
  <trd lng="de">Videorekorder</trd>
  <trd lng="en">videocassette recorder (VCR)</trd>
  <trd lng="fr">magn&eacute;toscope</trd>
  <trdgrp lng="pl">
    <trd>magnetowid</trd>,
    <trd>magnetoskop</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>&icirc;nregistrare</trd>,
    <trd>magnetoscoape</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>magnetoskop</trd>,
    <trd>videorekord&eacute;r</trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_ib;&c_d;&c_je;&c_o;&c_m;&c_a;&c_g;&c_n;&c_ib;&c_t;&c_o;&c_f;&c_o;&c_n;</trd>
  <trdgrp lng="zh">
    <trd>&#x5F55;&#x50CF;&#x673A; <pr>l&#xF9;xi&#xE0;ngj&#x12B;</pr></trd>,
    <trd>&#x9304;&#x50CF;&#x6A5F; <pr>l&#xF9;xi&#xE0;ngj&#x12B;</pr></trd>
  </trdgrp>
</drv>
</art>

