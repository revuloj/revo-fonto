<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: endos.xml,v 1.11 2023/10/14 15:49:04 revo Exp $">
    <kap><rad>endos</rad>/i <fnt><bib>PIV2</bib></fnt></kap>

    <drv mrk="endos.0i">
      <kap><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
      <uzo tip="fak">EKON</uzo>
        <dif>
          Fari surskribon sur (la dorsa flanko de) bilo, kambio a&ubreve;
          &ccirc;eko, per kiu oni cedas &gcirc;in kaj &gcirc;ian valoron al indikita
          persono a&ubreve; al &gcirc;ia prezentonto:
          <ekz>
            <tld/>i kambion al iu.
          </ekz>
        </dif>
      </snc>
  <trd lng="bg">&c_i;&c_n;&c_d;&c_o;&c_s;&c_i;&c_r;&c_a;&c_m;</trd>
  <trd lng="cs">indosovat</trd>
  <trd lng="de">indossieren</trd>
  <trdgrp lng="en">
        <trd>endorse</trd>,
        <trd>indorse</trd>
      </trdgrp>
  <trd lng="fr">endosser
        <klr tip="ind"> (un papier-valeur &agrave; l'ordre de qn)</klr></trd>
  <trd lng="hu">forgat<klr tip="ind"> (v&aacute;lt&oacute;t)</klr></trd>
  <trd lng="it">girare <klr tip="ind"> (di assegno)</klr></trd>
  <trd lng="ja">&#x88CF;&#x66F8;&#x3059;&#x308B;  <pr>&#x3046;&#x3089;&#x304C;&#x304D;&#x3059;&#x308B;</pr></trd>
  <trd lng="nl">endosseren</trd>
  <trd lng="pt">endossar</trd>
  <trdgrp lng="ru">
        <trd>&c_i;&c_n;&c_d;&c_o;&c_s;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; <ind>&c_p;&c_je;&c_r;&c_je;&c_d;&c_a;&c_t;&c_o;&c_ch;&c_n;&c_u;&c_ju; &c_n;&c_a;&c_d;&c_p;&c_i;&c_s;&c_mol;</ind></trd>
      </trdgrp>
  <trd lng="sk">indosova&tcaron;</trd>
  <trd lng="uk">&#x456;&#x43D;&#x434;&#x43E;&#x441;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
</drv>

    <drv mrk="endos.0o">
      <kap><tld/>o</kap>
      <snc>
      <uzo tip="fak">EKON</uzo>
        <dif>
          Ago <tld/>i.
        </dif>
      </snc>
  <trd lng="cs">indosament .</trd>
  <trd lng="en">endorsement <klr>(not approval)</klr></trd>
  <trd lng="fr">endossement</trd>
  <trd lng="he">&#x5D4;&#x5B2;&#x5E1;&#x5B8;&#x5D1;&#x5B8;&#x5D4;</trd>
  <trd lng="ru">&c_i;&c_n;&c_d;&c_o;&c_s;&c_s;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>
  <trd lng="sk">indosament .</trd>
</drv>

    <drv mrk="endos.0ajxo">
      <kap><tld/>a&jcirc;o</kap>
      <snc>
      <uzo tip="fak">EKON</uzo>
        <dif>
          La transdona surskriba&jcirc;o pri <tld/>o.
        </dif>
      </snc>
      <trd lng="en">endorsement <klr>(signature)</klr></trd>
      <trd lng="fr">endos</trd>
      <trdgrp lng="hu">
        <trd>h&aacute;tirat<klr tip="ind"> (v&aacute;lt&oacute;n)</klr></trd>,
        <trd>forgatm&aacute;ny</trd></trdgrp>
      <trd lng="nl">endossement</trd>
      <trd lng="pt">endosso</trd>
      <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_d;&c_a;&c_t;&c_o;&c_ch;&c_n;&c_a;&c_ja; &c_n;&c_a;&c_d;&c_p;&c_i;&c_s;&c_mol;</trd>
    </drv>

    <drv mrk="endos.0anto">
      <kap><tld/>anto</kap>
      <snc>
      <uzo tip="fak">EKON</uzo>
        <dif>
          Tiu, kiu <tld/>as, transdonas sian rajton per <tld/>a&jcirc;o.
        </dif>
      </snc>
  <trd lng="cs">indosant</trd>
  <trdgrp lng="en">
        <trd>endorser</trd></trdgrp>
  <trdgrp lng="fr">
        <trd>endosseur</trd>
      </trdgrp>
  <trdgrp lng="hu">
        <trd>rendelv&eacute;nyes</trd>,
        <trd>forgat&oacute;</trd></trdgrp>
  <trd lng="it">girante<klr tip="ind"> (finanz.)</klr></trd>
  <trd lng="ru">&c_i;&c_n;&c_d;&c_o;&c_s;&c_s;&c_a;&c_n;&c_t;</trd>
  <trd lng="sk">indosant</trd>
</drv>

  </art>

