<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: brecx.xml,v 1.20 2021/03/05 17:02:56 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>bre&ccirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="brecx.0o">
  <kap><tld/>o</kap>
  <snc mrk="brecx.0o.KOMUNE">
    <dif>
      Trarompa&jcirc;o, tra<ref tip="vid" cel="fend.0o">fendo</ref>,
      mankloko en muro, roko, plektobarilo:
      <ekz>
        ripari la <tld/>on de &scirc;ipo;
      </ekz>
      <ekz>
        fermi la <tld/>ojn en la murego<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;topi la <tld/>on en la bu&gcirc;eto.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_b;&c_o;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_l;&c_o;&c_m;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_r;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">breixa</trd>
  <trdgrp lng="de">
    <trd>Bresche</trd>,
    <trd>L&uuml;cke</trd>,
    <trd>Riss</trd>
  </trdgrp>
  <trd lng="en">breach</trd>
  <trd lng="es">brecha</trd>
  <trd lng="fr">br&egrave;che</trd>
  <trdgrp lng="he">
    <trd>&#x5E4;&#x5E8;&#x5E6;&#x5D4;</trd>,
    <trd>&#x5E1;&#x5D3;&#x5E7;</trd>
  </trdgrp>
  <trd lng="hu">r&eacute;s</trd>
  <trdgrp lng="pl">
    <trd>luka</trd>,
    <trd>wy&lstroke;om</trd>,
    <trd>wyrwa</trd>
  </trdgrp>
  <trd lng="ro">ruptur&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_je;&c_sh;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_b;&c_o;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_r;&c_y;&c_v;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_l;&c_o;&c_m;</trd>
  </trdgrp>
</drv>

<drv mrk="brecx.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="brecx.0eto.KOMUNE">
    <dif>
      Randrompa&jcirc;o, rompdifekto, skrapdenteto &ccirc;e la rando de
      vazo, kru&ccirc;o, telero, glaso, tran&ccirc;ilo ks.
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_ch;&c_a;&c_r;&c_b;&c_ib;&c_n;&c_a;</trd>
  <trd lng="de">Ecke</trd>
  <trd lng="hu">csorba</trd>
  <trdgrp lng="fr">
    <trd>br&egrave;che</trd>,
    <trd>entaille</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>szczerba</trd>,
    <trd>wyszczerbienie</trd>,
    <trd>szczelina</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_z;&c_u;&c_b;&c_r;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_shch;&c_je;&c_r;&c_b;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="brecx.0igi">
  <kap><tld/>igi, <var><kap><tld/>i</kap></var></kap>
  <snc mrk="brecx.0igi.KOMUNE">
    <dif>
      Fari <tld/>on:
      <ekz>
        <tld/>itaj &scirc;tonegoj
        <fnt><bib>Far3</bib>, <lok>&ccirc;ap. 8</lok></fnt>;
      </ekz>
      <ekz>
        niaj vicoj balda&ubreve; <tld/>i&gcirc;is.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_l;&c_a;&c_m;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;br&eacute;cher</trd>,
    <trd>entamer <klr tip="ind">(faire une br&egrave;che)</klr></trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5E1;&#x5D3;&#x5D5;&#x5E7;</trd>
  <trd lng="hu">r&eacute;st &uuml;t</trd>
  <trdgrp lng="pl">
    <trd>zrobi&cacute; <ind>luk&eogonek;</ind></trd>,
    <trd>zrobi&cacute; <ind>wy&lstroke;om</ind></trd>,
    <trd>zrobi&cacute; <ind>wyrw&eogonek;</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_b;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_l;&c_o;&c_m;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="brecx.0etigi">
  <kap><tld/>etigi, <var><kap><tld/>eti</kap></var></kap>
  <snc mrk="brecx.0etigi.KOMUNE">
    <dif>
      Fari <tld/>eton:
      <ekz>
        reakrigi <tld/>etitan tran&ccirc;ilon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>&eacute;br&eacute;cher</trd>,
    <trd>entailler</trd>
  </trdgrp>
  <trd lng="hu">kicsorb&iacute;t</trd>
  <trd lng="pl">wyszczerbi&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_z;&c_u;&c_b;&c_r;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_shch;&c_je;&c_r;&c_b;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
