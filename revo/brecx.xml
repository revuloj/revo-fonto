<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>bre&ccirc;</rad>/o
</kap>
<drv mrk="brecx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Trarompa&jcirc;o, trafendego, mankloko en muro, roko, plektobarilo:
      <ekz>
        ripari la <tld/>on de &scirc;ipo;
      </ekz>
      <ekz>
        fermi la <tld/>ojn en la murego<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;topi la <tld/>on en la bu&gcirc;eto.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Bresche</trd>,
    <trd>L&uuml;cke</trd>,
    <trd>Ri&szlig;</trd>
  </trdgrp>
  <trd lng="fr">br&egrave;che</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_je;&c_sh;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_b;&c_o;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_r;&c_y;&c_v;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_l;&c_o;&c_m;</trd>
  </trdgrp>
</drv>

<drv mrk="brecx.0eto">
  <kap><tld/>eto</kap>
  <snc>
    <dif>
      Randrompa&jcirc;o, rompdifekto, skrapdenteto &ccirc;e la rando de
      vazo, kru&ccirc;o, telero, glaso, tran&ccirc;ilo ks.
    </dif>
  </snc>
  <trd lng="de">Ecke</trd>
  <trdgrp lng="fr">
    <trd>br&egrave;che</trd>,
    <trd>entaille</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_z;&c_u;&c_b;&c_r;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_shch;&c_je;&c_r;&c_b;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="brecx.0igi">
  <kap><tld/>(ig)i</kap>
  <snc>
    <dif>
      Fari <tld/>on:
      <ekz>
        <tld/>itaj &scirc;tonegoj
        <fnt>(<aut>Kabe</aut>: <vrk>La Faraono, 3a vol.</vrk>,
        <lok>&ccirc;ap. 8</lok>)</fnt>;
      </ekz>
      <ekz>
        niaj vicoj balda&ubreve; <tld/>i&gcirc;is.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>&eacute;br&eacute;cher</trd>,
    <trd>entamer <klr tip="ind">(faire une br&egrave;che)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="brecx.0etigi">
  <kap><tld/>et(ig)i</kap>
  <snc>
    <dif>
      Fari <tld/>eton:
      <ekz>
        reakrigi <tld/>etitan tran&ccirc;ilon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>&eacute;br&eacute;cher</trd>,
    <trd>entailler</trd>
  </trdgrp>
</drv>
</art>
