<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>bre&ccirc;</rad>/o
</kap>
<drv mrk="brecx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Trarompa&jcirc;o, trafendego, mankloko en muro, roko, plektobarilo:
      <ekz>
        ripari la <tld/>on de &scirc;ipo;
      </ekz>
      <ekz>
        fermi la <tld/>ojn en la murego<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <klr>(f)</klr> &scirc;topi la <tld/>on en la bud&gcirc;eto.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Bresche</trd>,
    <trd>L&uuml;cke</trd>,
    <trd>Ri&szlig;</trd>
  </trdgrp>
</drv>
<drv mrk="brecx.0eto">
  <kap><tld/>eto</kap>
  <snc>
    <dif>
      Randrompa&jcirc;o, rompdifekto, skrapdenteto &ccirc;e la rando de
      vazo, kru&ccirc;o, telero, glaso, tran&ccirc;ilo ks.
    </dif>
  </snc>
  <trd lng="de">Ecke</trd>
</drv>
<drv mrk="brecx.0igi">
  <kap><tld/>(ig)i</kap>
  <snc>
    <dif>
      Fari <tld/>on: 
      <ekz>
        <tld/>itaj &scirc;tonegoj
        <fnt>(<aut>Kabe</aut>: <vrk>La Faraono, 3a vol.</vrk>,
        <lok>&ccirc;ap. 8</lok>)</fnt>;
      </ekz>
      <ekz>
        niaj vicoj balda&ubreve; <tld/>i&gcirc;is.
      </ekz>
    </dif>
  </snc>
</drv>
<drv mrk="brecx.0etigi">
  <kap><tld/>et(ig)i</kap>
  <snc>
    <dif>
      Fari <tld/>eton:
      <ekz>
        reakrigi <tld/>etitan tran&ccirc;ilon.
      </ekz>
    </dif>
  </snc>
</drv>
</art>
