<art mrk="$Id: sad.xml,v 1.5 2024/03/24 19:59:58 revo Exp $">
<kap>
  <rad>Sad</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="sad.0ismo">
  <kap><tld lit="s"/>ismo<fnt><bib>SPV</bib></fnt></kap>
  <snc mrk="sad.0ismo.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      &Gcirc;uado de suferigo trudata al aliaj, ofte pere de seksaj agoj:
      <ekz>
        mi sentis ion detruan, <tld lit="s"/>isman en li
        <fnt>
          <aut>C. Piron </aut>,
          <vrk><url ref="http://esperanto.org.uk/eldonoj/piron/tekstoj/Cxu_vi_kuiras_cxine.pdf">
          &Ccirc;u vi kuiras &ccirc;ine?</url></vrk>,
          <lok>1976</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_d;&c_y;&c_z;&c_m;</trd>
  <trd lng="de">Sadismus</trd>
  <trd lng="en">sadism</trd>
  <trd lng="fr">sadisme</trd>
  <trd lng="it">sadismo</trd>
  <trd lng="pl">sadyzm</trd>
  <trdgrp lng="zh">
    <trd>&#x8650;&#x5F85;&#x72C2; <pr>nu:&#xE8;d&#xE0;iku&#xE1;ng</pr></trd>,
    <trd>&#x65BD;&#x8650;&#x7656; <pr>sh&#x12B;nu:&#xE8;p&#x1D0;</pr></trd>,
    <trd>&#x65BD;&#x8650;&#x72C2; <pr>sh&#x12B;nu:&#xE8;ku&#xE1;ng</pr></trd>
  </trdgrp>
</drv>
</art>

