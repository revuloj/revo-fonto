<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kicx.xml,v 1.13 2023/12/03 09:58:49 revo Exp $">
<kap>
  <rad>ki&ccirc;</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="kicx.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="kicx.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      <ref tip="super" cel="stil.0o">Stilo</ref> imitanta arton, 
      kutime aplikata al malaltkostaj produktoj por amaskonsumo:
      <ekz>
        <nom>Salvador Dal&iacute;</nom> <klr>[&#x2026;]</klr> en la ornamado de plafonoj kaj vandoj de siaj
        domoj, li atingis la limon inter <tld/>o kaj arto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=20095">Salvador Dal&iacute;</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_ch; <klr>(&c_s;&c_t;&c_y;&c_l;&c_mol;)</klr></trd>
  <trd lng="bg">&c_k;&c_i;&c_ch;</trd>
  <trd lng="de">Kitsch<klr tip="amb"> (Stil)</klr></trd>
  <trd lng="fr">kitsch<klr> (subst.)</klr></trd>
  <trd lng="ja">&#x30AD;&#x30C3;&#x30C1;&#x30E5;</trd>
  <trd lng="nl">kitsch</trd>
  <trd lng="pl">kicz</trd>
  <trd lng="ru">&c_k;&c_i;&c_ch;</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_ib;&c_ch;</trd>,
    <trd>&c_k;&c_ib;&c_t;&c_ch;</trd>
  </trdgrp>
</drv>

<drv mrk="kicx.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="kicx.0a.BELA">
    <dif>
      Konforma al <ref tip="vid" cel="kicx.0o.BELA"><tld/>o</ref>:
      <ekz>
        <tld/>a cervo-pentra&jcirc;o
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 10</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj kajeraj amromanoj
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 5 (55)</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu parto de nia mondo entuziasme kaj senkritike <klr>[estas akceptema]</klr> al
        la plej <ref cel="banal.0a">banala</ref>j, <ref cel="trivia.0a">triviala</ref>j, 
        <ref cel="stras.0a">strasa</ref>j, pucaj, <tld/>aj kaj malutilaj elementoj
        de tiu iam esperplena nova mondo
        <fnt>
          <aut>Garbhan MacAoidh</aut>
            <vrk><url ref="&Monato;/2000/007076.php">Ho, tempoj! Ho, moroj!</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 19</lok>
          </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tamen (e&ccirc; se &scirc;ajnas <tld/>a) mi donus mian vivon
        por dek tieaj lokoj, kelkaj uloj, havenoj, pinarbaroj
        <fnt>
          <aut>J. E. Pacheco, trad. J. J. Schmitter</aut>,
          <vrk>&Scirc;tatperfido</vrk>,
          <lok>Flor' kaj kanto. - Meksika Esperanto-Federacio,
          1998</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &jcirc;urnalisto priskribis la konkurson kiel &leftquot;tiel 
        sen&ccirc;arma kaj <tld/>a kiel Esperanto&rightquot;
        <fnt>
        <aut>Eriketo</aut>,
        <vrk>sen&ccirc;arma kaj ki&ccirc;a?</vrk>,
        <lok><url ref="http://eriketo.blogspot.com">[blogo]</url>,
        2006-05-19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_ch;&c_a;&c_v;&c_y;</trd>
  <trd lng="de">kitschig</trd>
  <trd lng="fr">kitsch<klr> (adj.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x4FD7;&#x60AA;&#x306A;  <pr>&#x305E;&#x304F;&#x3042;&#x304F;&#x306A;</pr></trd>,
    <trd>&#x307E;&#x304C;&#x3044;&#x7269;&#x306E;  <pr>&#x307E;&#x304C;&#x3044;&#x3082;&#x306E;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">kitscherig</trd>
  <trd lng="pl">kiczowaty</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_t;&c_ch;&c_je;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_v;&c_k;&c_u;&c_s;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="zh">&#x602F; <pr>qi&#xE8;</pr></trd>
</drv>

<drv mrk="kicx.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="kicx.0ajxo.BELA">
    <dif>
      <ref tip="super" cel="art.0ajxo">Arta&jcirc;o</ref>, <ref tip="super" cel="produk.0o.produktajxo">produkto</ref> 
      <tld/>a:
      <ekz>
        tio pri <tld/>a&jcirc;oj <klr>[&#x2026;]</klr> ne povas ne pensigi
        anka&ubreve; pri verdaj steloj, verdaj standardoj, kaj verdstelaj standardoj
        <fnt>
          <aut>Ken Miner</aut>,
          <vrk><url ref="http://www.sunflower.com/~miner/TAGLIBRO_package/taglibro.html">taglibro</url></vrk>,
          <lok>2003-10-05</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="jxujxu.0o">&jcirc;u&jcirc;uo</ref>,
      <ref cel="suvenir.0o">suveniro</ref>,
      <ref cel="um.0ilo">umilo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_ch; <klr>(&c_r;&c_e;&c_ch;)</klr></trd>,
    <trd>&c_b;&c_je;&c_z;&c_g;&c_u;&c_s;&c_t;&c_o;&c_w;&c_sh;&c_ch;&c_y;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kitsch<klr tip="amb"> (Objekt)</klr></trd>,
    <trd>Tand</trd>,
    <trd>Schnickschnack</trd>
  </trdgrp>
  <trd lng="fr">objet <ind>kitsch</ind></trd>
  <trd lng="nl">kitsch<klr tip="ind"> (object)</klr></trd>
  <trd lng="pl">kicz</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_ch;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_v;&c_k;&c_u;&c_s;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
</drv>
</art>

