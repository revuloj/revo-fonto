<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gonore.xml,v 1.12 2023/11/11 11:36:36 revo Exp $">
<kap>
  <rad>gonore</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="gonore.0o">
  <kap><tld/>o</kap>
  <snc mrk="gonore.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Venera <ref tip="lst" lst="voko:malsanoj" cel="san.mal0o.MED">malsano</ref>
      ka&ubreve;zata de <ref tip="prt" cel="gonoko.0o">gonokokoj</ref>:
      <ekz>
        ni ricevis de la blankuloj alkoholon, <tld/>on kaj sifilison
        <fnt><bib>SatirRak</bib>, <lok>la barbaro</lok></fnt>;
      </ekz>
      <ekz>
        koito donas okazon por du korpaj konsekvencoj, gravedi&gcirc;oj kaj
        seksaj malsanoj
        (aidoso, <tld/>o, sifiliso, ktp)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=34740">Amoro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="blenor.0o.MED">blenoragio</ref>
    <trd lng="de">Tripper</trd>
    <trdgrp lng="fr">
      <trd>gonorrh&eacute;e</trd>,
      <trd>blennorragie</trd>
    </trdgrp>
    <trd lng="zh">&#x6DCB;&#x75C5; <pr>l&#xED;nb&#xEC;ng</pr></trd>
</snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_u;&c_r;&c_z;&c_p;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_g;&c_a;&c_n;&c_a;&c_r;&c_e;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>gonorea</trd>,
    <trd>kapavka</trd>
  </trdgrp>
  <trd lng="ja">&#x6DCB;&#x75C5;  <pr>&#x308A;&#x3093;&#x3073;&#x3087;&#x3046;</pr></trd>
  <trdgrp lng="sk">
    <trd>gonorea</trd>,
    <trd>kvapavka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&#x43E;&#x43D;&#x43E;&c_r;&c_je;&#x44F;</trd>,
    <trd>&c_t;&c_r;&c_i;&#x43F;&c_je;&c_r;</trd>
  </trdgrp>
</drv>
</art>

