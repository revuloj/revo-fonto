<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: epifiz.xml,v 1.17 2022/03/17 13:47:18 revo Exp $">
<kap><rad>epifiz</rad>/o</kap>
<drv mrk="epifiz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Dika fino de longa <ref tip="malprt" cel="ost.0o">osto</ref>:
      <ekz>
        femurosto <klr>[&#x2026;]</klr> konsistas el tri partoj: unu centra parto (diafizo) kaj
        du ekstrema&jcirc;oj (<tld/>oj)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=32775">Femurosto</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="apofiz.0o">apofizo</ref>,
      <ref cel="diafiz.0o">diafizo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_p;&c_ib;&c_f;&c_ib;&c_z;</trd>
  <trdgrp lng="cs">
    <trd>epif&yacute;za</trd>,
    <trd>&scaron;i&scaron;inka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Epiphyse</trd>,
    <trd>Epiphysis</trd>,
    <trd>Knochenendst&uuml;ck</trd>,
    <trd>R&ouml;hrenknochenendst&uuml;ck</trd>
  </trdgrp>
  <trd lng="fr">&eacute;piphyse</trd>
  <trdgrp lng="hu">
    <trd>epifizis</trd>,
    <trd>iz&uuml;leti v&eacute;g<klr tip="ind"> (csonton)</klr></trd>
  </trdgrp>
  <trd lng="it">epifisi<klr> (ossea)</klr></trd>
  <trd lng="nl">epifyse</trd>
  <trd lng="pl">epifiza</trd>
  <trd lng="pt">ep&iacute;fise</trd>
  <trd lng="ru">&c_e;&c_p;&c_i;&c_f;&c_i;&c_z;</trd>
  <trdgrp lng="sk">
    <trd>epif&yacute;za</trd>,
    <trd>&scaron;i&scaron;inka</trd>
  </trdgrp>
  <trd lng="uk">&#x435;&#x43F;&#x456;&#x444;&#x456;&#x437;&#x430;</trd>
</drv>
</art>
