<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>reklam</rad>/o
</kap>
<drv mrk="reklam.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Alvoko al la publiko per &ccirc;iuspecaj rimedoj por la&ubreve;di kaj rekomendi
      komerca&jcirc;on, entreprenon a&ubreve; ian aferon.
    </dif>
    <refgrp tip="sin">
      <ref cel="admon">admono</ref>,
      <ref cel="sxild.0o">&scirc;ildo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Werbung</trd>,
    <trd>Reklame</trd>
  </trdgrp>
  <trd lng="en">advertisement</trd>
  <trd lng="nl">reclame</trd>
  <trd lng="ru">&c_r;&c_je;&c_k;&c_l;&c_a;&c_m;&c_a;</trd>
  <trd lng="sv">reklam</trd>
</drv>

<drv mrk="reklam.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Anonci kaj rekomendi per <tld/>o:
      <ekz>
        <tld/>i ion, pri, por io.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="public.0i">publici</ref>,
      <ref cel="admon.0i">admoni</ref>,
      <ref cel="trumpe.dis0i">distrumpeti</ref>,
      <ref cel="tamtam.0i">tamtami</ref>,
      <ref cel="fam.0igi">disfamigi</ref>.
    </refgrp>
  </snc>
  <trd lng="ru">&c_r;&c_je;&c_k;&c_l;&c_a;&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

</art>
