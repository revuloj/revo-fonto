<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cicer.xml,v 1.16 2021/03/06 06:20:00 revo Exp $">
<kap>
  <rad>cicer</rad>/o
</kap>
<drv mrk="cicer.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TIP</uzo>
  <snc>
    <dif>
      <ref tip="malprt" cel="tip.pres0o">Prestipo</ref>,
      kies grandeco estas dekdupunkta, kaj servas kiel unuo
      de tipografia mezuro:
      <ekz>
        en 1737 <nom>Pierre Fournier</nom> difinis punkton kiel 1/12 de la grando de la franca
        tiparo <tld lit="C"/>o, a&ubreve; 0,34882&#x202F;mm
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=31785">Punkto (tipografio)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_c;&c_y;&c_c;&c_e;&c_r;&c_a;</trd>
    <trd lng="ca">c&iacute;cero</trd>
    <trd lng="de">Cicero</trd>
    <trd lng="es">c&iacute;cero</trd>
    <trd lng="fr">cic&eacute;ro</trd>
    <trd lng="hu">cicer&oacute;</trd>
    <trd lng="it">cicero</trd>
    <trd lng="nl">cicero</trd>
    <trd lng="pl">cycero</trd>
    <trd lng="ru">&c_c;&c_i;&c_c;&c_je;&c_r;&c_o;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>cicero</trd>,
    <trd>onze</trd>
  </trdgrp>
  <trd lng="he">&#x5E7;&#x5D9;&#x5E7;&#x5E8;&#x5D5;</trd>
  <trd lng="sk">cicero</trd>
</drv>
</art>
