<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>madagaskar</rad>/o
</kap>

<drv mrk="madagas.0o">
  <kap>
    <tld lit="M"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="madagas.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o.POL">&Scirc;tato</ref> en la samnoma 
      insulo trovi&gcirc;anta en Hinda Oceano oriente 
      de suda parto de <ref tip="malprt" cel="afrik.A0o">Afriko</ref> 
      kun &ccirc;efurbo Antanarivo.
      Teritorio de Respubliko Madagaskaro.
      ISO-3166 kodo: MG.
    </dif>
    <trd lng="bg">&c_M;&c_a;&c_d;&c_a;&c_g;&c_a;&c_s;&c_k;&c_a;&c_r;</trd>
    <trd lng="de">Madagaskar</trd>
    <trd lng="en">Madagascar</trd>
    <trd lng="nl">Madagascar</trd>
    <trdgrp lng="pl">
      <trd>Madagaskar</trd>,
      <trd>Republika <ind>Madagaskaru</ind></trd>,
      <trd>Republika <ind>Malgaska</ind><klr> (przest.)</klr></trd>
    </trdgrp>
  </snc>
</drv>

</art>

