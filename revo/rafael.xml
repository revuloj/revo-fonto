<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rafael.xml,v 1.12 2023/09/03 07:53:09 revo Exp $">
<kap>
  <rad>Rafael</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="rafael.0o">
  <kap><tld/>o</kap>
  <snc mrk="rafael.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> hebredevena.
    </dif>
  </snc>
  <snc mrk="rafael.0o.PERS">
    <uzo tip="fak">PERS</uzo>
    <dif>
      Itala <ref tip="lst" cel="pentr.0isto" lst="voko:historiaj_personoj">pentristo</ref> kaj
      arkitekto (1483
      &ndash; 1520), plennome 
      <tld/>o Sanzio:
      <ekz>
        &leftquot;la&ubreve; mia opinio&rightquot;, respondis Bazarov, &leftquot;<tld/>o ne valoras
        unu 
        kupran gro&scirc;on&rightquot;
        <fnt><bib>PatrojFiloj</bib>, <lok>&ccirc;apitro 10a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_R;&c_a;&c_f;&c_a;&c_e;&c_l;&c_mol;</trd>,
    <trd>&c_R;&c_a;&c_f;&c_a;&c_e;&c_l;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_R;&c_a;&c_f;&c_a;&c_je;&c_l;&c_o;</trd>
  <trdgrp lng="cs">
    <trd>Raffael</trd>,
    <trd>Raffaello Santi</trd>
  </trdgrp>
  <trd lng="de">Raffael</trd>
  <trd lng="fr">Rapha&euml;l</trd>
  <trd lng="it">Raffaello</trd>
  <trdgrp lng="ja">
    <trd>&#x30E9;&#x30D5;&#x30A1;&#x30A8;&#x30ED;</trd>,
    <trd>&#x30E9;&#x30D5;&#x30A1;&#x30A8;&#x30EB;</trd>
  </trdgrp>
  <trd lng="nl">Rafael</trd>
  <trdgrp lng="pl">
    <trd>Rafael</trd>,
    <trd>Rafael Santi</trd>
  </trdgrp>
  <trd lng="pt">Rafael</trd>
  <trd lng="ro">Rafael</trd>
  <trd lng="sk">Raffaello Santi</trd>
  <trd lng="uk">&c_R;&c_a;&c_f;&c_a;&c_je;&#x43B;&#x44C; (&c_h;&c_u;&c_d;&#x43E;&c_zh;&#x43D;&c_i;&#x43A;)</trd>
</drv>
</art>

