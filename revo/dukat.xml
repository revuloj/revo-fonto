<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
	<ofc>1</ofc>
	<rad>dukat</rad>/o
</kap>
<drv mrk="dukat.0o">
	<kap><tld/>o</kap>
	<uzo tip="fak">EKON</uzo>
	<uzo tip="fak">HIS</uzo>
	<snc>
	<dif>
		Malnova ora <ref tip="super" cel="mon.0ero">monero</ref>, 
		komence emisiita de Venecio.
	</dif>
	</snc>
	<trd lng="be">&c_d;&c_u;&c_k;&c_a;&c_t;</trd>
	<trd lng="ca">ducat</trd>
	<trdgrp lng="de">
	   <trd>Dukat</trd>,
	   <trd>Dukaten</trd>
	</trdgrp>
	<trd lng="es">ducado</trd>
	<trd lng="fr">ducat</trd>
	<trd lng="hu">duk&aacute;t</trd>
    <trd lng="pl">dukat</trd>
    <trd lng="pt">ducado <klr tip="ind">(moeda)</klr></trd>
	<trd lng="ru">&c_d;&c_u;&c_k;&c_a;&c_t;</trd>
</drv>
</art>
