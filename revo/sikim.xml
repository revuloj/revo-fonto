<vortaro>
<art mrk="$Id: sikim.xml,v 1.3 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>Sikim</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sikim.0o">
  <kap><tld/>o</kap>
  <snc mrk="sikim.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Gubernio en la orienta parto de
      <ref tip="malprt" cel="hind.H0ujo.POL">Hindujo</ref>:
      <ekz>
        <tld/>o estis sendependa &scirc;tato, kiu estis regata de 
        la Chogyal-monarkio &gcirc;is 1975
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Sikimo">
          Sikimo</url>, 2014-03-17</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">Sikkim</trd>
  <trd lng="fr">Sikkim</trd>
  <trd lng="sk">Sikkim</trd>
</drv>
</art>

