  <trd lng="uk">&c_S;&c_ib;&c_k;&c_k;&c_ib;&c_m; (&c_sh;&c_t;&c_a;&c_t;)</trd>
  <trdgrp lng="zh">
    <trd>&#x932B;&#x91D1; <pr>X&imacron;j&imacron;n</pr></trd>,
    <trd>&#x9521;&#x91D1; <pr>X&imacron;j&imacron;n</pr></trd>,
    <trd>&#x932B;&#x91D1;&#x90A6; <pr>x&iacute;j&imacron;nb&amacron;ng</pr></trd>,
    <trd>&#x9521;&#x91D1;&#x90A6; <pr>x&iacute;j&imacron;nb&amacron;ng</pr></trd>
  </trdgrp>
</drv>
