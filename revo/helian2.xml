<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: helian2.xml,v 1.18 2023/08/31 15:46:15 revo Exp $">
<kap>
  <rad>heliantem</rad>/o
</kap>
<drv mrk="helian2.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref>
      el familio <ref tip="malprt" cel="cist.0oj">cistacoj</ref>,
      (<trd lng="la">Helianthemum</trd>), 
      <ref tip="lst" cel="arbust.0o" lst="voko:malvoparencoj">duonarbusto</ref>
      multspecie kreskanta en Mediteraneo,
      kun simplaj kontra&ubreve;sidaj folioj, belaj, kvinnombraj 
      kaj multstamenaj, ofte orflavaj floroj direkti&gcirc;antaj al la suno,  
      kaj kun kapsulaj fruktoj:
      <ekz>
        <tld/>o a&ubreve; sunrozo, estas genro de proksimume 110 specioj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=417748">Heliantemo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        montaj kaj mediteraneaj specioj kunlo&gcirc;as: festuko, karekso, bromuso stara,
        <tld/>o blanka , sileno itala, rubio <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=466286">Roko Solutre</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/a/af/Helianthemum_linii_02.JPG/365px-Helianthemum_linii_02.JPG" prm="CC     BY-SA 3.0">floro de <tld/>o, La Palma, Hispanujo
        <fnt>
          <aut>H. Zell, propra verko</aut>
          <lok><url ref="&WCW;/File:Helianthemum_linii_02.JPG">&WCW;/File:Helianthemum_linii_02.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_s;&c_o;&c_n;&c_c;&c_a;&c_c;&c_mol;&c_v;&c_je;&c_t;</trd>
  <trd lng="de">Sonnenr&ouml;schen</trd>
  <trd lng="fr">h&eacute;lianth&egrave;me</trd>
  <trdgrp lng="hu">
    <trd>napvir&aacute;g</trd>,
    <trd>tetemold&oacute;</trd>,
    <trd>tetemtold&oacute;</trd>
  </trdgrp>
  <trd lng="ja">&#x30CF;&#x30F3;&#x30CB;&#x30C1;&#x30D0;&#x30CA;</trd>
  <trd lng="nl">zonneroosje</trd>
  <trd lng="pt">heli&acirc;ntemo</trd>
  <trd lng="ru">&c_s;&c_o;&c_l;&c_n;&c_c;&c_je;&c_c;&c_v;&c_je;&c_t;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43E;&#x43D;&c_c;&c_je;&c_c;&c_v;&c_ib;&c_t;</trd>,
    <trd>&c_s;&#x43E;&#x43D;&#x44F;&#x43D;&#x43A;&c_a;</trd>
  </trdgrp>
</drv>
</art>
