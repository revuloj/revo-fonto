<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: batik.xml,v 1.13 2023/10/29 19:11:00 revo Exp $">
<kap>
  <rad>batik</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="batik.0o">
  <kap><tld/>o</kap>
  <snc mrk="batik.0o.TEKS_precedo">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Indonezidevena procedo por tinkturo de &scirc;tofoj
      rezervanta lokojn per vakso:
      <ekz>
        konkursantoj partoprenos en 14 laborkampoj <klr>...</klr>
        ceramiko, kro&ccirc;etado, <tld/>o, pentrado ktp.
        <fnt>
        <vrk>La 6a internacia abilimpiada konkurso...</vrk>,
        <lok><url ref="http://www.volny.cz/toutf8.cs/aeh-ikeh/informilo/INFORMILO_2003_11.htm">Informilo,
        2003-11</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fa">
      <trd>&f_ch;&a_A;&f_p; <ind>&f_k;&a_l;&a_A;&a_q;&a_h;&#x200C;&a_A;&f_y;</ind></trd>,
      <trd>&a_b;&a_A;&a_t;&f_y;&f_k;</trd>
    </trdgrp>
    <trd lng="pl">batik</trd>
    <trd lng="zh">&#x8721;&#x67D3; <pr>l&#xE0;r&#x1CE;n</pr></trd>
</snc>
  <snc mrk="batik.0o.TEKS_sxtofo">
    <dif>
      <tld lit="B"/>a&jcirc;o, &scirc;tofo <tld/>e kolorigita:
      <ekz>
        verk' sur <tld/>o / anta&ubreve; kaj post tinkturo
        <fnt>
        <aut>Mao Zifu</aut>,
        <vrk><url ref="http://www.elerno.net/originala/orig31.htm">ekspliko de
        son&gcirc;oj</url></vrk>,
        <lok>La Kancerkliniko, 1997-01, numero 81a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="pl">batik</trd>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_t;&c_y;&c_k;</trd>
  <trd lng="ca">b&agrave;tik</trd>
  <trd lng="cs">batika <klr>(technika barven&#xED; l&#xE1;tky)</klr></trd>
  <trd lng="de">Batik</trd>
  <trd lng="en">Batik</trd>
  <trd lng="fr">batik</trd>
  <trd lng="id">batik</trd>
  <trd lng="nl">batik</trd>
  <trd lng="pt">batique</trd>
  <trd lng="sk">batika</trd>
  <trd lng="uk">&c_b;&c_a;&c_t;&c_i;&#x43A;</trd>
</drv>
</art>

