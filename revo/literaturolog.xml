<drv mrk="literaturolog.0o">
  <kap><tld/>o</kap>
  <snc mrk="literaturolog.0o.BELE">
    <uzo tip="fak">BELE</uzo>
    <dif>
      Studanto de 
      <ref tip="vid" cel="literaturologi.0o.BELE">literaturologio</ref>:
      <ekz>
        &ccirc;i tiu eseo <klr>...</klr> nur celas rakonti al la 
        &gcirc;enerala esperantista legantaro (ne al 
        <tld/>oj kaj kritikistoj) pri la &ccirc;efaj vivo&scirc;tupoj 
        de Fjodor Dostojevskij
        <fnt>
          <aut>A. Kor&jcirc;enkov</aut>,
          <vrk><url
          ref="&LOdE;Libroj/V_bibl08.pdf">
          Fjodor Dostojevskij en Esperanto</url></vrk>,
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_t;&c_a;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;&c_z;&c_n;&c_a;&c_w;&c_c;&c_a;</trd>
  <trd lng="fr">litt&eacute;raturologue</trd>
  <trd lng="pl">literaturoznawca</trd>     
  <trd lng="ro">specialist &icirc;n literatur&abreve;</trd>
</drv>
</art>

