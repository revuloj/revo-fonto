<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>patos</rad>/o
</kap>
<drv mrk="patos.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Emocia, solena a&ubreve; entuziasma esprimo de altaj pensoj a&ubreve; intensaj
      sentoj:
      <ekz>
        la <tld/>o de Pindaro, V. Hugo.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="emfaz.0o">emfazo</ref>,
      <ref cel="bombas.0o">bombasto</ref>,
      <ref cel="verv.0o">vervo</ref>,
      <ref cel="tusx">kortu&scirc;e</ref>,
      <ref cel="drast">draste</ref>,
      <ref cel="tragik">tragike</ref>,
      <ref cel="akcent">akcente</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_t;&c_a;&c_s;</trd>
  <trd lng="bg">&c_p;&c_a;&c_t;&c_o;&c_s;</trd>
  <trd lng="de">Pathos</trd>
  <trdgrp lng="en">
    <trd>passion</trd>,
    <trd>feeling</trd>,
    <trd>bombast</trd>,
    <trd>rant</trd>,
    <trd>fervor</trd>,
    <trd>expression</trd>
  </trdgrp>
  <trd lng="fr">path&eacute;tique<klr> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>p&aacute;tosz</trd>,
    <trd>emelkedetts&eacute;g</trd>
  </trdgrp>
  <trd lng="nl">pathos</trd>
  <trd lng="pt">patos</trd>
  <trd lng="ru">&c_p;&c_a;&c_f;&c_o;&c_s;</trd>
</drv>
</art>
