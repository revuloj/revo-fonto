<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ekzegez</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="ekzege.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BIB</uzo>
  <uzo tip="fak">LIN</uzo>
  <snc>
    <dif>
      Klariga metoda interpreto de teksto (historia, jura, teologia ktp).
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_z;&c_e;&c_g;&c_je;&c_z;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Textauslegung</trd>,
    <trd>Bibelauslegung</trd>,
    <trd>Exegese</trd>
  </trdgrp>
  <trd lng="fr">ex&eacute;g&egrave;se</trd>
  <trdgrp lng="hu">
    <trd>exeg&eacute;zis</trd>,
    <trd>sz&ouml;vegmagyar&aacute;zat</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_o;&c_l;&c_k;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="ekzege.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">BIB</uzo>
  <snc>
    <dif>
      Fakulo pri <tld/>o, speciale pri <tld/>o de la Biblio.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Interpret</trd>,
    <trd>Textausleger</trd>,
    <trd>Bibelausleger</trd>,
    <trd>Exeget</trd>
  </trdgrp>
  <trd lng="fr">ex&eacute;g&egrave;te</trd>
  <trdgrp lng="hu">
    <trd>exeg&eacute;ta</trd>,
    <trd>bibliamagyar&aacute;z&oacute;</trd>,
    <trd>sz&ouml;vegmagyar&aacute;z&oacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_k;&c_z;&c_je;&c_g;&c_je;&c_t;</trd>,
    <trd>&c_t;&c_o;&c_l;&c_k;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_i;&c_n;&c_t;&c_je;&c_r;&c_p;&c_r;&c_je;&c_t;&c_a;&c_t;&c_o;&c_r;</trd>
  </trdgrp>
</drv>
</art>
