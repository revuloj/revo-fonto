<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gemologi</rad>/o
</kap>

<drv mrk="gemologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="gemologi.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Scienco pri <ref tip="vid" cel="sxton.juvel0o.MIN">juvel&scirc;tonoj</ref>
      a&ubreve; gemoj, kiu helpas taksi ilian valoron kaj distingi ilin de
      imita&jcirc;oj:
      <ekz>
        <tld/>o estas scienco pri valoraj &scirc;tonoj
        <fnt>
        <aut>Galina Beliakova</aut>,
        <vrk><url ref="http://www.linabel.de/Gem/Eo/Gemmologie800_eo.html">gemologio</url></vrk>,
        <lok>[vidita je 2006-02-17]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Gemmologie</trd>,
    <trd>Edelsteinkunde</trd>
  </trdgrp>
  <trd lng="fr">gemmologie</trd>
  <trd lng="hu">gemol&oacute;gia</trd>
  <trd lng="pt">gemologia</trd>
</drv>
</art>

