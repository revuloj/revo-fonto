<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gemologi.xml,v 1.11 2021/10/04 12:46:12 revo Exp $">
<kap>
  <rad>gemologi</rad>/o <fnt><bib>Viki</bib></fnt>
</kap>

<drv mrk="gemologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="gemologi.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Scienco pri <ref tip="vid" cel="gem.0o.MIN">gemoj</ref>,
      kiu helpas taksi ilian valoron kaj distingi ilin de
      imita&jcirc;oj:
      <ekz>
        <tld/>o estas scienco pri valoraj &scirc;tonoj
        <fnt>
          <aut>G. Beliakova</aut>
          <vrk><url
          ref="http://www.linabel.de/Gem/Eo/Gemmologie800_eo.html">Gemologio</url></vrk>
          <lok>[vidita en 2006]</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="gemolog.0o.MIN">gemologo</ref>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_m;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_g;&c_je;&c_m;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="ca">gemmologia</trd>
  <trdgrp lng="de">
    <trd>Gemmologie</trd>,
    <trd>Edelsteinkunde</trd>
  </trdgrp>
  <trd lng="en">gemology</trd>
  <trd lng="fr">gemmologie</trd>
  <trd lng="es">gemolog&iacute;a</trd>
  <trd lng="hu">gemol&oacute;gia</trd>
  <trdgrp lng="nl">
    <trd>edelsteenkunde</trd>,
    <trd>gemmologie</trd>
  </trdgrp>
  <trd lng="pl">gemmologia</trd>
  <trd lng="pt">gemologia</trd>
  <trd lng="ru">&c_g;&c_je;&c_m;&c_m;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
</drv>
</art>

