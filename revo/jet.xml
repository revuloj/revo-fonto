<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: jet.xml,v 1.13 2022/04/11 20:38:23 revo Exp $">
<kap>
  <rad>jet</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="jet.0o">
  <kap><tld/>o</kap>
  <snc mrk="jet.0o.reakcio">
    <dif>
      La movo a&ubreve; forto estigata per el&jcirc;eto de flua&jcirc;o 
      (precipe
      en <ref cel="raket.0o.AVI">raketo</ref> a&ubreve; <tld/>avio).
    </dif>
    <rim>
      La&ubreve; la vortaro de Minnaja estas pli bone uzi la vorton 
      &leftquot;jetpu&scirc;o&rightquot; por la movo a&ubreve; la forto kaj 
      &leftquot;jeto&rightquot; por la flama eljeta&jcirc;o mem.
    </rim>
    <trd lng="en">jet propulsion</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_je;&c_a;&c_k;&c_t;&c_i;&c_v;&c_n;&c_a;&c_ja; &c_t;&c_ja;&c_g;&c_a;</trd>,
      <trd>&c_r;&c_je;&c_a;&c_k;&c_t;&c_i;&c_v;&c_n;&c_o;&c_je; &c_d;&c_v;&c_i;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="it">
    <trd>getto</trd>,
    <trd>aviogetto</trd>
  </trdgrp>
  </snc>
  <snc mrk="jet.0o.avio">
    <dif>
      <ref tip="dif" cel="jet.0avio"><tld lit="J"/>avio</ref>:
      <ekz>
        do forfalis la renovigo de la ekssovetiaj <tld/>oj
        <em>MIG</em>, la plej bonaj ma&scirc;inoj, sed kun a&ccirc;aj
        elektronika&jcirc;oj
        <fnt>
          <aut>K. Juh&aacute;sz</aut>:
          <vrk>Hungario: Sveda impeto!</vrk>,
          <bib>Monato</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
  <trd>jet</trd>,
  <trd>tryskov&eacute; letadlo</trd>,
  <trd>tryskov&yacute; motor</trd>
  </trdgrp>
  
  <trd lng="pt">jato</trd>
  <trd lng="sk">reakt&iacute;vne lietadlo</trd>
  <trd lng="uk">&c_r;&c_je;&c_a;&#x43A;&c_t;&c_i;&c_v;&#x43D;&c_i;&c_j; &#x43B;&c_ib;&c_t;&c_a;&#x43A;</trd>
</drv>


<drv mrk="jet.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Sin movi per el&jcirc;eto de flua&jcirc;o (gaso a&ubreve; likvo) en
      la direkto
      kontra&ubreve;a al la estigata movo; sin movi per <tld/>avio:
      <ekz>
        la malamika piloto <tld/>as rekte al li, &scirc;ajne pro kolero
        pri tio, kio okazis al lia amiko
        <fnt>
          <aut>&Ubreve;. &Gcirc;. &Ubreve;iljamz</aut>,
          <vrk>Dratizitaj</vrk>,
          <lok><url ref="http://esperanto.us/dratizitaj/Dratizitaj_3.html">
          &Ccirc;ap. 3a</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">jetten</trd>
  <trd lng="en"><klr>to </klr>jet</trd>
  <trd lng="ru">&c_d;&c_v;&c_i;&c_g;&c_a;&c_t;&c_mol;&c_s;&c_ja; &c_n;&c_a; &c_r;&c_je;&c_a;&c_k;&c_t;&c_i;&c_v;&c_n;&c_o;&c_j; &c_t;&c_ja;&c_g;&c_je;</trd>
 <trd lng="it">propellere<klr> (di jet)</klr></trd>
</drv>


<drv mrk="jet.0avio">
  <kap><tld/>avio,
  <var><kap><tld/>aviadilo</kap></var></kap>
  <snc>
    <dif>
      <ref tip="super" cel="avi.0o">Avio</ref> pelata per
      <tld/>motoro:
      <ekz>
        la Boeing 747 <klr>[...]</klr> (elefanta <tld/>aviadilo) estas
        usona pasa&gcirc;eraviadilo
        <klr>[...]</klr> longtempe la plej granda
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=137091">Boeing
          747</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="aviad.reakci0ilo">reakcia aviadilo</ref>,
    <ref cel="jet.0o.avio"><tld/>o<sncref/></ref>
  </refgrp>
  <trdgrp lng="de">
    <trd>Strahlflugzeug</trd>,
    <trd>D&uuml;senflugzeug</trd>
    <trd>Jet</trd>,
    <trd>D&uuml;senjet</trd>
  </trdgrp>
  <trd lng="en">jet plane</trd>
  <trd lng="fr">jet<klr> (avion)</klr></trd>
  <trdgrp lng="pt">
     <trd>jato</trd>
     <trd>avi&atilde;o a jato</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_a;&c_k;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j; &c_s;&c_a;&c_m;&c_o;&c_l;&c_jo;&c_t;</trd>
  <trdgrp lng="sv">
    <trd>jetflygplan</trd>,
    <trd>reaplan</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>jet</trd>,
    <trd>aereo a reazione</trd>
  </trdgrp>
  <trd lng="sk">pr&uacute;dov&eacute; lietadlo</trd>
<trdgrp lng="cs">
<trd>proudnicov&yacute; letoun</trd>,
<trd>tryskov&eacute; letadlo</trd>
</trdgrp>
</drv>

<drv mrk="jet.0motoro">
  <kap><tld/>motoro</kap>
  <snc>
  <uzo tip="fak">TEK</uzo>
    <dif>
      Motoro a&ubreve; movilo, estiganta la pelforton per el&jcirc;eto de
      flua&jcirc;o:
      <ekz>
        en pli kaj pli perfekta formo ili <klr>(la helicoj)</klr>
        regis duonjarcenton, &gcirc;is momento, kiam estis
        anstata&ubreve;igitaj per <tld/>motoroj
        <fnt>
          <aut>J. Kubat</aut>,
          <vrk>Nova erao de helicoj &#8208; &ccirc;u helic-feno a&ubreve;
          fen-helico?</vrk>,
          <vrk><url ref="http://www.fw.hu/eventoj/steb/ingxenierado/ceteraj/aviado/saest82-15.htm">
          SAEST-82</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
  <trd>proudnicov&yacute; motor</trd>,
  <trd>proudov&yacute; motor</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Strahltriebwerk</trd>,
      <trd>D&uuml;sentriebwerk</trd>
  </trdgrp>
  <trd lng="en">jet engine</trd>
  <trd lng="fr">r&eacute;acteur<klr> (moteur)</klr></trd>
  <trdgrp lng="ru">
      <trd>&c_r;&c_je;&c_a;&c_k;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j; &c_d;&c_v;&c_i;&c_g;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
      <trd>&c_d;&c_v;&c_i;&c_g;&c_a;&c_t;&c_je;&c_l;&c_mol; &c_p;&c_r;&c_ja;&c_m;&c_o;&c_j; &c_r;&c_je;&c_a;&c_k;&c_c;&c_i;&c_i;</trd>
  </trdgrp>
  <trd lng="pt">motor a jato</trd>
  <trd lng="sk">pr&uacute;dov&yacute; motor</trd>
  <trdgrp lng="sv">
      <trd>jetmotor</trd>,
      <trd>reamotor</trd>
  </trdgrp>
</drv>
</art>

