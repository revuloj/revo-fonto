<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&ccirc;evaled</rad>/oj
</kap>
<drv mrk="cxeval1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo
      <ref tip="super" cel="huf.nepar0uloj">neparhufuloj</ref>
      (<trd lng="la">Equidae</trd>), kun nefenditaj hufoj.
    </dif>
    <ref tip="sin" cel="huf.unu0uloj">unuhufuloj</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Pferde</trd>,
    <trd>Einhufer</trd>
  </trdgrp>
  <trd lng="fr">&eacute;quid&eacute;s</trd>
  <trd lng="hu">l&oacute;f&eacute;l&eacute;k</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_sh;&c_a;&c_d;&c_i;&c_n;&c_y;&c_je;</trd>,
    <trd>&c_l;&c_o;&c_sh;&c_a;&c_d;&c_i; (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</trd>
  </trdgrp>
</drv>
</art>
