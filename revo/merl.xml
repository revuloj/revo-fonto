<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>merl</rad>/o
</kap>
<drv mrk="merl.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="turd.0o">turdo</ref>
      (<trd lng="la">Turdus merula</trd>), nigra kun flava
      beko kaj lerte fajfanta.
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_o;&c_r;&c_n;&c_y; <ind>&c_d;&c_r;&c_o;&c_z;&c_d;</ind></trd>
  <trd lng="br">moualc'h</trd>
  <trd lng="cs">kos &ccaron;ern&yacute;</trd>
  <trd lng="de">Amsel</trd>
  <trd lng="en">blackbird</trd>
  <trd lng="fr">merle</trd>
  <trd lng="hu">feketerig&oacute;</trd>
  <trd lng="nl">merel</trd>
  <trd lng="pl">kos</trd>
  <trd lng="ru">&c_ch;&c_jo;&c_r;&c_n;&c_y;&c_j; <ind>&c_d;&c_r;&c_o;&c_z;&c_d;</ind></trd>
</drv>
</art>
