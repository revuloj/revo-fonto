<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>benin</rad>/o
</kap>

<drv mrk="benin.B0o">
  <kap>
    <tld lit="B"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="benin.B0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la centra Afriko kun &ccirc;efurbo Porto Novo.
      Teritorio de Respubliko Benino.
      &Gcirc;is 1975 nomata Dahomeo.
      ISO-3166 kodo: BJ.
      &Gcirc;i limas kun
      <ref tip="vid" cel="burkin.B0o.GEOG">Burkino</ref>,
      <ref tip="vid" cel="nigxer.N0o.GEOG">Ni&gcirc;ero</ref>,
      <ref tip="vid" cel="nigxeri.N0o.GEOG">Ni&gcirc;erio</ref>,
      <ref tip="vid" cel="togo.T0olando.GEOG">Togolando</ref>
      kaj <ref tip="vid" cel="atlant.A0o.GEOG">Atlantiko</ref> (Gvinea 
      Golfo).
    </dif>
    <trd lng="bg">&c_B;&c_je;&c_n;&c_i;&c_n;</trd>
    <trd lng="de">Benin</trd>
    <trd lng="en">Benin</trd>
    <trdgrp lng="pl">
      <trd>Benin</trd>,
      <trd>Republika <ind>Beninu</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

