<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
   <kap>
    <rad>oligopsoni</rad>/o
   </kap>
   <drv mrk="oligops.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">EKON</uzo>
    <snc>
    <dif>
      Merkata situacio, en kiu malmultaj klientoj regas la a&ccirc;etan
flankon
      kaj tiel povas dikti la prezojn a&ubreve; liverkondi&ccirc;ojn al la
      vendantoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="oligopol.0o">oligopolo</ref>,
      <ref cel="monops.0o">monopsono</ref>,
      <ref cel="duops.0o">duopsonio</ref>,
      <ref cel="polips.0o">polipsonio</ref>.
    </refgrp>
    </snc>
    <trd lng="de">Oligopson</trd>
    <trdgrp lng="en">
       <trd>oligopsony</trd>,
       <trd>buyer's oligopoly</trd>
    </trdgrp>
   </drv>
  </art>


<!--
$Log$
-->
</vortaro>