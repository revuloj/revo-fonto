<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: televizi.xml,v 1.10 2023/04/18 12:46:18 revo Exp $">
<kap>
<rad>televizi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="televizi.0o">
  <kap><tld/>o</kap>
  <snc mrk="televizi.0o.televido">
    <uzo tip="fak">TEL</uzo>
    <dif>
      <ref tip="dif" cel="televid.0o.TEL">Televido<sncref/></ref>:
      <ekz>
        mi preska&ubreve; neniam spektas la usonan profitcelan <tld/>on
        <fnt>
          <aut>Georgo</aut>,
          <vrk>Babilono 5</vrk>,
          <lok><url ref="&SCE;">
          soc.culture.esperanto</url>, 1998-03-23</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi provis do agadi <klr>[&#8230;]</klr>, kreante servojn profesiajn en
        la sektoroj de &jcirc;urnalismo, komunikado, interreto, <tld/>o
        <fnt>
          <aut>Flavio Rebelo</aut>,
          <vrk><url ref="http://www.gxangalo.com/modules/kolumnoj/article.php?artID=56">
          Realo kaj Fantazio - Unua parto</url></vrk>,
          <lok>&Gcirc;angalo, &Ccirc;efartikoloj, 2006-03-06</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#8230;]</klr> ne deziras spekti televizion, nek a&ubreve;skulti radion, nek legi
        <fnt>
          <bib>Monato</bib>
          <aut>Julian Modest</aut>
          <vrk><url ref="&Monato;/2012/010807.php">La memmortigo de Tanja</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &jcirc;urnalistoj de usona <tld/>o NBC raportis pri 345 miskomprenoj, kra&scirc;oj kaj preska&ubreve;-kra&scirc;oj
        pro lingvaj problemoj
        <fnt>
          <bib>Monato</bib>
          <aut>S. Maul</aut>
        </fnt>;
      </ekz>
      <ekz>
        dum <tld/>a konversacio, oni reelsendis kronika&jcirc;on el la jaro 1993
        <fnt><aut>Bardhyl Selimi</aut>: 
          <vrk><url ref="&Monato;/2001/007205.html">Voja&gcirc;impresoj, somero 2000</url></vrk>,
          <bib>Monato</bib>
          <lok>2001/01, p. 26</lok>
        </fnt>;
      </ekz>
      <ekz>
        la konkurso de e&ubreve;ropaj <tld/>oj pri kanzonoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2006/008965.php">Lingvaj problemoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la 53a <ind>E&ubreve;ro<tld/>a Kanto-konkurso</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Dimitrije Jani&ccaron;i&cacute;</aut>
          <vrk><url ref="&Monato;/2008/009469.php">Not-inda konkurso</url></vrk>
        </fnt>.
        <trdgrp lng="de">
          <trd>Eurovision Song Contest</trd>,
          <trd>ESC</trd>
        </trdgrp>
      </ekz>
    </dif>
    <trd lng="de">Fernsehen</trd>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_l;&c_je;&c_v;&c_ib;&c_z;&c_ib;&c_ja;</trd>
  <trd lng="es">televisi&oacute;n<klr> (como tecnolog&iacute;a)</klr></trd>
  <trd lng="fr">t&eacute;l&eacute;vision<klr> (technique)</klr></trd>
  <trd lng="pl">telewizja<klr> (jako medium lub technika)</klr></trd>
</drv>
</art>
