<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc> <rad>&scirc;ra&ubreve;b</rad>/o</kap>
<drv mrk="sxraux.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Ilo el metalo a&ubreve; ligno, esence konsistanta el cilindra a&ubreve; konusforma
      akso helice kanelita. &Gcirc;ia formo, grando kaj uzo tre varias; &gcirc;i servas
      ordinare por kunigi a&ubreve; fiksi pli firme ol per najlo, sed anka&ubreve; por
      premi, levi, fermi, bori, movon transigi ktp.
    </dif>
  </snc>
  <trd lng="de">Schraube</trd>
  <trd lng="en">screw</trd>
  <trd lng="fr">vis</trd>
  <trd lng="nl">schroef</trd>
  <trd lng="ru">&c_v;&c_i;&c_n;&c_t;</trd>
</drv>

<drv mrk="sxraux.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Turni <tld/>on.
    </dif>
  </snc>
  <snc>
    <dif>
      Fiksi per <tld/>o.
    </dif>
  </snc>
  <trd lng="de">schrauben</trd>
  <trd lng="fr">visser</trd>
  <trd lng="nl">schroeven</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_v;&c_i;&c_n;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_v;&c_i;&c_n;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_v;&c_i;&c_n;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="sxraux.0igi">
  <kap><tld/>igi</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Fari helicajn kanelojn &ccirc;e cilindro a&ubreve; konuso.
    </dif>
  </snc>
  <trd lng="fr">fileter</trd>
  <trd lng="nl">schroefdraad maken</trd>
  <trd lng="ru">&c_n;&c_a;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol; &c_r;&c_je;&c_z;&c_mol;&c_b;&c_u;</trd>
</drv>

<drv mrk="sxraux.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Ilo por turni kaj enigi <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Schraubenzieher</trd>,
    <trd>Schraubendreher</trd>
  </trdgrp>
  <trd lng="en">screwdriver</trd>
  <trd lng="fr">tournevis</trd>
  <trd lng="nl">schroevendraaier</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_v;&c_jo;&c_r;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_g;&c_a;&c_je;&c_ch;&c_n;&c_y;&c_j; &c_k;&c_l;&c_ju;&c_ch;</trd>
    &c_i; &c_t;.&c_p;.
  </trdgrp>
</drv>

<drv mrk="sxraux.0igilo">
  <kap><tld/>igilo</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Ilo por fari helicajn kanelojn &ccirc;e <tld/>o.
    </dif>
  </snc>
  <trd lng="nl">draadtrekker</trd>
  <trd lng="fr">fili&egrave;re</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_sh;&c_k;&c_a; <klr>(&c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t; &c_d;&c_l;&c_ja; &c_n;&c_a;&c_r;&c_je;&c_z;&c_a;&c_n;&c_i;&c_ja;
  &c_n;&c_a;&c_r;&c_u;&c_zh;&c_n;&c_o;&c_j; &c_r;&c_je;&c_z;&c_mol;&c_b;&c_y;)</klr></trd>
</drv>

<drv mrk="sxraux.0ingo">
  <kap><tld/>ingo</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Helice interne kanelita ringo, en kiun oni turnante enigas <tld/>on.
    </dif>
  </snc>
  <trd lng="nl">moer</trd>
  <trd lng="fr">&eacute;crou</trd>
  <trd lng="ru">&c_g;&c_a;&c_j;&c_k;&c_a;</trd>
</drv>

<drv mrk="sxraux.0borilo">
  <kap><tld/>borilo</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Ilo por fari helicajn kanelojn interne de <tld/>ingo.
    </dif>
  </snc>
  <trd lng="nl">boor<klr tip="ind"> (om moeren te draaien)</klr></trd>
  <trd lng="ru">&c_m;&c_je;&c_t;&c_ch;&c_i;&c_k; <klr>(&c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t; &c_d;&c_l;&c_ja; &c_n;&c_a;&c_r;&c_je;&c_z;&c_a;&c_n;&c_i;&c_ja;
  &c_v;&c_n;&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_je;&c_j; &c_r;&c_je;&c_z;&c_mol;&c_b;&c_y;)</klr></trd>
</drv>

<drv mrk="sxraux.0forma">
  <kap><tld/>forma</kap>
  <snc>
    <dif>
      Similanta <tld/>on:
      <ekz>
        <tld/>forma &scirc;tuparo
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="spiral">spirala</ref>,
      <ref cel="tord.0a">torda</ref>,
      <ref cel="serpen.0a">serpenta</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="nl">
    <trd>schroefvormig</trd>,
    <trd>wentel-</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_i;&c_n;&c_t;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="sxraux.0kanelo">
  <kap><tld/>kanelo</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Elstara kanelo, kiu helice &ccirc;irka&ubreve;as la kernon de <tld/>o.
    </dif>
  </snc>
  <trd lng="ru">&c_r;&c_je;&c_z;&c_mol;&c_b;&c_a;</trd>
</drv>

<drv mrk="sxraux.0pasxo">
  <kap><tld/>pa&scirc;o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Distanco inter du homologaj punktoj de la kanelo de <tld/>o,
      mezurata paralele al la akso.
    </dif>
  </snc>
  <trd lng="ru">&c_sh;&c_a;&c_g; &c_r;&c_je;&c_z;&c_mol;&c_b;&c_y;</trd>
</drv>

<drv mrk="sxraux.0sxlosilo">
  <kap><tld/>&scirc;losilo</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Speco de <ref tip="super" cel="sxraux.0ilo"><tld/>ilo</ref>,
      kiu servas por <tld/>i la <tld/>ojn,
      <tld/>ingojn a&ubreve; <ref tip="vid" cel="bolt.0o">boltojn</ref>
      kun poligona kapo:
      <ekz>
        <ind>angla <tld/>&scirc;losilo</ind>
        <klr>(kies beko estas lar&gcirc;igebla)</klr>.
        <trd lng="ru">&c_r;&c_a;&c_z;&c_v;&c_o;&c_d;&c_n;&c_o;&c_j; &c_k;&c_l;&c_ju;&c_ch;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="ru">&c_g;&c_a;&c_je;&c_ch;&c_n;&c_y;&c_j; &c_k;&c_l;&c_ju;&c_ch;</trd>
</drv>

<drv mrk="sxraux.0turnilo">
  <kap><tld/>turnilo</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Speco de <ref tip="super" cel="sxraux.0ilo"><tld/>ilo</ref>,
      kiu servas por <tld/>i la <tld/>ojn kun enkapa fendo.
    </dif>
  </snc>
  <trd lng="ru">&c_o;&c_t;&c_v;&c_jo;&c_r;&c_t;&c_k;&c_a;</trd>
</drv>
</art>
