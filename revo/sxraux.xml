  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_r;&c_a;&c_z;&c_a;&c_c;&c_mol; &c_r;&c_a;&c_z;&c_mol;&c_b;&c_u;</trd>,
    <trd>&c_sh;&c_r;&c_u;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>turn a <ind>screw</ind></trd>,
