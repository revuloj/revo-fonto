<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxraux.xml,v 1.28 2024/03/25 07:28:39 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;ra&ubreve;b</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sxraux.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sxraux.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Ilo el metalo a&ubreve; ligno konsistanta el cilindra
      a&ubreve; konusforma akso helice kanelita, kiu servas
      ordinare por kunigi a&ubreve; fiksi pli firme ol per najlo, sed
      anka&ubreve; por premi, levi, fermi, bori, movon transigi ktp.
    </dif>
    <ref tip="sub" cel="bolt.0o">bolto</ref>
  </snc>
  <trd lng="be">&c_sh;&c_r;&c_u;&c_b;&c_a;</trd>
  <trd lng="cs">vrut</trd>
  <trd lng="de">Schraube</trd>
  <trd lng="el">&beta;&jota_ton;&delta;&alfa;</trd>
  <trd lng="en">screw</trd>
  <trd lng="es">tornillo</trd>
  <trd lng="fr">vis</trd>
  <trd lng="hu">csavar</trd>
  <trd lng="it">vite</trd>
  <trd lng="nl">schroef</trd>
  <trdgrp lng="pl">
    <trd>wkr&eogonek;t</trd>,
    <trd>&sacute;ruba</trd>
  </trdgrp>
  <trd lng="pt">parafuso</trd>
  <trd lng="ru">&c_v;&c_i;&c_n;&c_t;</trd>
  <trd lng="sv">skruv</trd>
  <trd lng="uk">&c_gu;&c_v;&c_i;&#x43D;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#x87BA;&#x4E1D; <pr>lu&#xF3;s&#x12B;</pr></trd>,
    <trd>&#x87BA;&#x7D72; <pr>lu&#xF3;s&#x12B;</pr></trd>,
    <trd>&#x87BA;&#x6746; <pr>lu&#xF3;g&#x1CE;n</pr></trd>,
    <trd>&#x87BA;&#x687F; <pr>lu&#xF3;g&#x1CE;n</pr></trd>,
    <trd>&#x87BA;&#x91D8; <pr>lu&#xF3;d&#x12B;ng</pr></trd>,
    <trd>&#x87BA;&#x9489; <pr>lu&#xF3;d&#x12B;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxraux.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxraux.0i.TEK_turni">
    <dif>
      Turni <tld/>on.
    </dif>
    <trd lng="hu">csavar</trd>
  </snc>
  <snc mrk="sxraux.0i.TEK_fiksi">
    <dif>
      Fiksi per <tld/>o.
    </dif>
    <trd lng="hu">csavaroz</trd>
  </snc>
  <trd lng="be">&c_sh;&c_r;&c_u;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>&scaron;roubovat</trd>,
    <trd>za&scaron;roubovat</trd>
  </trdgrp>
  <trd lng="de">schrauben</trd>
  <trd lng="el">&beta;&jota;&delta;&omega_ton;&ny;&omega;</trd>
  <trd lng="es">atornillar</trd>
  <trd lng="fr">visser</trd>
  <trd lng="it">avvitare</trd>
  <trd lng="nl">schroeven</trd>
  <trdgrp lng="pl">
    <trd>przykr&eogonek;ca&cacute;</trd>,
    <trd>po&lstroke;&aogonek;czy&cacute; <ind>wkr&eogonek;tem</ind></trd>,
    <trd>skr&eogonek;ci&cacute; <ind>wkr&eogonek;tem</ind></trd>,
    <trd>ze&sacute;rubowa&cacute;</trd>,
    <trd>przy&sacute;rubowa&cacute;</trd>,
    <trd>po&lstroke;&aogonek;czy&cacute; <ind>&sacute;rub&aogonek;</ind></trd>,
    <trd>skr&eogonek;ci&cacute; <ind>&sacute;rub&aogonek;</ind></trd>
  </trdgrp>
  <trd lng="pt">aparafusar</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_v;&c_i;&c_n;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_i;&c_n;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_v;&c_i;&c_n;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_v;&c_i;&c_n;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_v;&c_i;&c_n;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_v;&c_i;&c_n;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">skruva</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_gu;&c_v;&c_i;&#x43D;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_r;&c_i;&c_gu;&c_v;&c_i;&#x43D;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_u;&c_gu;&c_v;&c_i;&#x43D;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x62E7; <pr>n&#x1D0;ng</pr></trd>,
    <trd>&#x64F0; <pr>n&#x1D0;ng</pr></trd>,
    <trd>&#x62E7;&#x7D27;&#x87BA;&#x4E1D; <pr>n&#xED;ngj&#x1D0;nlu&#xF3;s&#x12B;</pr></trd>,
    <trd>&#x64F0;&#x7DCA;&#x87BA;&#x7D72; <pr>n&#xED;ngj&#x1D0;nlu&#xF3;s&#x12B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxraux.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="sxraux.0igi.Tek">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Fari helicajn kanelojn &ccirc;e cilindro a&ubreve; konuso.
    </dif>
  </snc>
  <trd lng="es">terrajar</trd>
  <trd lng="fr">fileter</trd>
  <trd lng="hu">menetet v&aacute;g</trd>
  <trd lng="it">filettare</trd>
  <trd lng="nl">schroefdraad maken</trd>
  <trdgrp lng="pl">
    <trd>gwintowa&cacute;</trd>,
    <trd>nacina&cacute; <ind>gwint</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol; &c_r;&c_je;&c_z;&c_mol;&c_b;&c_u;</trd>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_a;&c_r;&c_ib;&c_z;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&#x43E;&c_b;&c_i;&c_t;&c_i; &c_gu;&c_v;&c_i;&#x43D;&c_t;&#x43E;&c_v;&c_u; &#x43D;&c_a;&c_r;&c_ib;&c_z;&#x43A;&c_u;</trd>
  </trdgrp>
</drv>

<drv mrk="sxraux.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="sxraux.0ilo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Ilo por turni kaj enigi <tld/>on.
    </dif>
  </snc>
  <trd lng="cs">&scaron;roubov&aacute;k</trd>
  <trdgrp lng="de">
    <trd>Schraubenzieher</trd>,
    <trd>Schraubendreher</trd>
  </trdgrp>
  <trd lng="el">&kappa;&alfa;&tau;&sigma;&alfa;&beta;&jota_ton;&delta;&jota;</trd>
  <trd lng="en">screwdriver</trd>
  <trd lng="es">desarmador</trd>
  <trd lng="fr">tournevis</trd>
  <trd lng="hu">csavaroz&oacute;</trd>
  <trdgrp lng="it">
    <trd>cacciavite</trd>,
    <trd>giravite</trd>,
    <trd>avvitatore</trd>
  </trdgrp>
  <trd lng="nl">schroevendraaier</trd>
  <trdgrp lng="pl">
    <trd>wkr&eogonek;tak</trd>,
    <trd>&sacute;rubokr&eogonek;t</trd>,
    <trd>klucz<klr> (do &sacute;rub)</klr></trd>
  </trdgrp>
  <trd lng="pt">chave de parafuso</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_v;&c_jo;&c_r;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_g;&c_a;&c_je;&c_ch;&c_n;&c_y;&c_j; &c_k;&c_l;&c_ju;&c_ch;</trd>
    &c_i; &c_t;.&c_p;.
  </trdgrp>
  <trd lng="sv">skruvmejsel</trd>
  <trd lng="uk">&c_v;&c_i;&#x43A;&c_r;&c_u;&c_t;&#x43A;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x87BA;&#x4E1D;&#x5200; <pr>lu&#xF3;s&#x12B;d&#x101;o</pr></trd>,
    <trd>&#x87BA;&#x7D72;&#x5200; <pr>lu&#xF3;s&#x12B;d&#x101;o</pr></trd>,
    <trd>&#x87BA;&#x4E1D;&#x8D77;&#x5B50; <pr>lu&#xF3;s&#x12B;q&#x1D0;zi</pr></trd>,
    <trd>&#x87BA;&#x7D72;&#x8D77;&#x5B50; <pr>lu&#xF3;s&#x12B;q&#x1D0;zi</pr></trd>,
    <trd>&#x8D77;&#x5B50; <pr>q&#x1D0;z&#x1D0;</pr></trd>,
    <trd>&#x6539;&#x9310; <pr>g&#x1CE;izhu&#x12B;</pr></trd>,
    <trd>&#x6539;&#x9525; <pr>g&#x1CE;izhu&#x12B;</pr></trd>,
    <trd>&#x87BA;&#x4E1D;&#x6279; <pr>lu&#xF3;s&#x12B;p&#x12B;</pr></trd>,
    <trd>&#x87BA;&#x7D72;&#x6279; <pr>lu&#xF3;s&#x12B;p&#x12B;</pr></trd>,
    <trd>&#x93CD; <pr>lu&#xF3;</pr></trd>,
    <trd>&#x9559; <pr>lu&#xF3;</pr></trd>,
    <trd>&#x6539;&#x5200; <pr>g&#x1CE;id&#x101;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxraux.0igilo">
  <kap><tld/>igilo</kap>
  <snc mrk="sxraux.0igilo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Ilo por fari helicajn kanelojn &ccirc;e <tld/>o.
    </dif>
  </snc>
  <trd lng="es">terraja</trd>
  <trd lng="hu">menetv&aacute;g&oacute;</trd>
  <trd lng="nl">draadtrekker</trd>
  <trd lng="fr">fili&egrave;re</trd>
  <trd lng="it">filiera<klr> (filettatrice)</klr></trd>
  <trd lng="pl">narzynka</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_sh;&c_k;&c_a;
  <klr>(&c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t; &c_d;&c_l;&c_ja; &c_n;&c_a;&c_r;&c_je;&c_z;&c_a;&c_n;&c_i;&c_ja; &c_n;&c_a;&c_r;&c_u;&c_zh;&c_n;&c_o;&c_j; &c_r;&c_je;&c_z;&c_mol;&c_b;&c_y;)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43B;&c_u;&#x43F;</trd>,
    <trd>&c_gu;&c_v;&c_i;&#x43D;&c_t;&#x43E;&c_r;&c_ib;&c_z;</trd>
  </trdgrp>
</drv>

<drv mrk="sxraux.0ingo">
  <kap><tld/>ingo</kap>
  <snc mrk="sxraux.0ingo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Helice interne kanelita ringo, en kiun oni turnante enigas
      <tld/>on.
    </dif>
    <rim>
      -ingo estas ta&ubreve;ga anka&ubreve; por indiki la truon,
      kie estas la &scirc;ra&ubreve;bo... Ni povus difini tion nur kiel &leftquot;kanelita
      ringo&rightquot;, se la vorto estus <tld/>ringo.
      <aut>A. Codazzi</aut>
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_j;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_u;&c_t;&c_e;&c_r;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>matice<klr> (&scaron;roubu)</klr></trd>,
    <trd>matka<klr> (&scaron;roubu)</klr></trd>
  </trdgrp>
  <trd lng="de">Mutter</trd>
  <trd lng="el">&pi;&alfa;&xi;&jota;&my;&alfa_ton;&delta;&jota;</trd>
  <trd lng="es">tuerca</trd>
  <trd lng="fr">&eacute;crou</trd>
  <trd lng="hu">anya<klr tip="ind"> (csavar&eacute;)</klr></trd>
  <trdgrp lng="it">
   <trd>dado<klr> (di bullone)</klr></trd>,
   <trd>madrevite</trd>
  </trdgrp>
  <trd lng="nl">moer</trd>
  <trd lng="pl">nakr&eogonek;tka</trd>
  <trd lng="pt">porca</trd>
  <trd lng="ru">&c_g;&c_a;&c_j;&c_k;&c_a;</trd>
  <trd lng="sv">mutter</trd>
  <trd lng="uk">&c_g;&c_a;&c_j;&#x43A;&c_a;</trd>
  <trd lng="zh">&#x87BA;&#x6BCD; <pr>lu&#xF3;m&#x1D4;</pr></trd>
</drv>

<drv mrk="sxraux.0borilo">
  <kap><tld/>borilo</kap>
  <snc mrk="sxraux.0borilo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Ilo por fari helicajn kanelojn interne de <tld/>ingo.
    </dif>
  </snc>
  <trd lng="de">Gewindebohrer</trd>
  <trd lng="es">machuelo</trd>
  <trd lng="fr">taraud</trd>
  <trd lng="hu">menetf&uacute;r&oacute;</trd>
  <trd lng="it">maschio<klr> (filettatore)</klr></trd>
  <trd lng="nl">boor<klr tip="ind"> (om moeren te draaien)</klr></trd>
  <trd lng="pl">gwintownik</trd>
  <trd lng="ru">&c_m;&c_je;&c_t;&c_ch;&c_i;&c_k;
  <klr>(&c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;
  &c_d;&c_l;&c_ja; &c_n;&c_a;&c_r;&c_je;&c_z;&c_a;&c_n;&c_i;&c_ja;
  &c_v;&c_n;&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_je;&c_j;
  &c_r;&c_je;&c_z;&c_mol;&c_b;&c_y;)</klr></trd>
  <trdgrp lng="zh">
    <trd>&#x4E1D;&#x653B;&#x548C;&#x4E1D;&#x677F; <pr>s&#x12B;g&#x14D;ngh&#xE9;s&#x12B;b&#x1CE;n</pr></trd>,
    <trd>&#x7D72;&#x653B;&#x548C;&#x7D72;&#x677F; <pr>s&#x12B;g&#x14D;ngh&#xE9;s&#x12B;b&#x1CE;n</pr></trd>,
    <trd>&#x87BA;&#x4E1D;&#x653B;&#x4E1D;&#x9525; <pr>lu&#xF3;s&#x12B;g&#x14D;ngs&#x12B;zhu&#x12B;</pr></trd>,
    <trd>&#x87BA;&#x7D72;&#x653B;&#x7D72;&#x9310; <pr>lu&#xF3;s&#x12B;g&#x14D;ngs&#x12B;zhu&#x12B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxraux.0forma">
  <kap><tld/>forma</kap>
  <snc mrk="sxraux.0forma.KOMUNE">
    <dif>
      Similanta <tld/>on:
      <ekz>
        <ind><tld/>forma &scirc;tuparo</ind>
        <trd lng="it">scala a chiocciola</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="serpen.0a">serpenta</ref>,
      <ref cel="spiral">spirala</ref>,
      <ref cel="tord.0a">torda</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_r;&c_u;&c_b;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;</trd>
  <trd lng="fr">h&eacute;lico&iuml;dal</trd>
  <trdgrp lng="hu">
    <trd>csavarforma</trd>,
    <trd>csavaros</trd>,
    <trd>csavarszer&udblac;</trd>
  </trdgrp>
  <trdgrp lng="it">
   <trd>a <ind>vite</ind></trd>,
   <trd>elicoidale<klr> (a vite)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schroefvormig</trd>,
    <trd>wentel-</trd>
  </trdgrp>
  <trd lng="pl">&sacute;rubowy</trd>
  <trd lng="ru">&c_v;&c_i;&c_n;&c_t;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="sxraux.0kanelo">
  <kap><tld/>kanelo</kap>
  <snc mrk="sxraux.0kanelo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Elstara kanelo, kiu helice &ccirc;irka&ubreve;as la kernon de
      <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_mol;&c_b;&c_a; <klr>(&c_sh;&c_r;&c_u;&c_b;&c_y;)</klr></trd>
  <trd lng="cs">z&aacute;vit</trd>
  <trd lng="es">cuerda<klr> (de tornillo)</klr></trd>
  <trd lng="fr">filetage</trd>
  <trd lng="hu">csavarmenet</trd>
  <trdgrp lng="it">
   <trd>filetto</trd>,
   <trd>filettatura<klr> (filetto)</klr></trd>
  </trdgrp>
  <trd lng="pl">gwint</trd>
  <trd lng="ru">&c_r;&c_je;&c_z;&c_mol;&c_b;&c_a;</trd>
  <trd lng="uk">&c_z;&#x43E;&c_v;&#x43D;&c_ib;&c_sh;&#x43D;&#x44F; &c_r;&c_ib;&c_z;&#x44C;&c_b;&c_a;</trd>
</drv>

<drv mrk="sxraux.0pasxo">
  <kap><tld/>pa&scirc;o</kap>
  <snc mrk="sxraux.0pasxo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Distanco inter du homologaj punktoj de la kanelo de <tld/>o,
      mezurata paralele al la akso.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_o;&c_k; <klr>(&c_sh;&c_r;&c_u;&c_b;&c_y;, &c_r;&c_a;&c_z;&c_mol;&c_b;&c_y;)</klr></trd>
  <trd lng="es">paso<klr> (de tornillo)</klr></trd>
  <trd lng="fr">pas de vis</trd>
  <trd lng="hu">menetemelked&eacute;s</trd>
  <trd lng="it">passo<klr> (di filettatura)</klr></trd>
  <trd lng="pl">skok <ind>gwintu</ind></trd>
  <trd lng="ru">&c_sh;&c_a;&c_g; &c_r;&c_je;&c_z;&c_mol;&c_b;&c_y;</trd>
  <trd lng="uk">&#x43A;&c_r;&#x43E;&#x43A; (&c_u; &c_r;&c_ib;&c_z;&#x44C;&c_b;&c_ib;)</trd>
</drv>

<drv mrk="sxraux.0sxlosilo">
  <kap><tld/>&scirc;losilo</kap>
  <snc mrk="sxraux.0sxlosilo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Speco de <ref tip="super" cel="sxraux.0ilo"><tld/>ilo</ref>,
      kiu servas por <tld/>i la <tld/>ojn,
      <tld/>ingojn a&ubreve; <ref tip="vid" cel="bolt.0o">boltojn</ref>
      kun poligona kapo:
      <ekz>
        <ind>angla <tld/>&scirc;losilo</ind>
        <klr>(kies beko estas lar&gcirc;igebla)</klr>.
        <trd lng="es">llave inglesa</trd>
        <trd lng="hu">franciakulcs</trd>
        <trd lng="it">chiave inglese</trd>
        <trd lng="pt">chave inglesa</trd>
        <trd lng="ru">&c_r;&c_a;&c_z;&c_v;&c_o;&c_d;&c_n;&c_o;&c_j; &c_k;&c_l;&c_ju;&c_ch;</trd>
      </ekz>
    </dif>
  </snc>
