    <trd>&c_n;&c_a;&c_r;&c_e;&c_z;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_m;&c_je;&c_t;&c_ch;&c_y;&c_k; <klr tip="amb">(&c_p;&c_r;&c_y;&c_l;&c_a;&c_d;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="de">Gewindebohrer</trd>
  <trd lng="en">tap</trd>
  <trd lng="es">machuelo</trd>
  <trd lng="fr">taraud</trd>
  <trd lng="hu">menetf&uacute;r&oacute;</trd>
  <trd lng="it">maschio<klr> (filettatore)</klr></trd>
  <trd lng="nl">boor<klr tip="ind"> (om moeren te draaien)</klr></trd>
  <trd lng="pl">gwintownik</trd>
  <trd lng="ru">&c_m;&c_je;&c_t;&c_ch;&c_i;&c_k;
