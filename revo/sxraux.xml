<vortaro>
<art mrk="$Id: sxraux.xml,v 1.35 2026/02/16 19:01:12 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;ra&ubreve;b</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="sxraux.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sxraux.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Ilo el cilindra
      a&ubreve; konusforma akso helice kanelita, kiu servas
      ordinare por kunigi a&ubreve; fiksi pli firme ol per najlo
      (sed anka&ubreve; por premi, levi, fermi, bori, movon transigi ktp):
      <ekz>
        <tld/>o kun ronda, kun konusa, sen kapo;
      </ekz>
      <ekz>
        la <tld/>oj cedis, kaj la pordo lar&gcirc;e malfermi&gcirc;is
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekkvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vapor&scirc;ipa <tld/>o <klr>(vd <ref cel="helic.0o.TEK">helico</ref>)</klr> komencas labori,
        <klr>[&#x2026;]</klr> blankaj tukoj, movi&gcirc;ante,
        donis al ni la lastan amikan adia&ubreve;on
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
          <lok>El Odeso &gcirc;is Konstantinopolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &gcirc;is la lasta <tld/>o <klr>(profunde)</klr> la truston ni purigos,
        &ccirc;iujn mankojn forigos, <klr>[&#x2026;]</klr> la tutan aparaton rekonstruos
        <fnt>
          <bib>Metrop</bib>
        </fnt>!
      </ekz>
      <ekz>
        <klr>[ili]</klr> premligis kiel pied<tld/>oj <klr>(turmentilo)</klr>
        kaj tenis kiel feraj &ccirc;enoj
        <fnt>
          <bib>Fab4</bib>
          <lok>Knabino, kiu pa&scirc;is sur panon</lok>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; ne <tld/>oj por poleksoj estus povintaj eltorturi el ni tion, kion ni vidis
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 33</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="bolt.0o">bolto</ref>
  </snc>
  <trd lng="be">&c_sh;&c_r;&c_u;&c_b;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>caragol</trd>,
    <trd>cargol</trd>,
    <trd>vis</trd>,
    <trd>gramp&oacute;</trd>
  </trdgrp>
  <trd lng="cs">vrut</trd>
  <trd lng="de">Schraube</trd>
  <trd lng="el">&beta;&jota_ton;&delta;&alfa;</trd>
  <trd lng="en">screw</trd>
  <trd lng="es">tornillo</trd>
  <trd lng="fr">vis</trd>
  <trd lng="hu">csavar</trd>
  <trd lng="it">vite</trd>
  <trd lng="nl">schroef</trd>
  <trdgrp lng="pl">
    <trd>wkr&eogonek;t</trd>,
    <trd>&sacute;ruba</trd>
  </trdgrp>
  <trd lng="pt">parafuso</trd>
  <trd lng="ru">&c_v;&c_i;&c_n;&c_t;</trd>
  <trd lng="sv">skruv</trd>
  <trd lng="uk">&c_gu;&c_v;&c_i;&c_n;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#x87BA;&#x4E1D; <pr>lu&oacute;s&imacron;</pr></trd>,
    <trd>&#x87BA;&#x7D72; <pr>lu&oacute;s&imacron;</pr></trd>,
    <trd>&#x87BA;&#x6746; <pr>lu&oacute;g&#x1CE;n</pr></trd>,
    <trd>&#x87BA;&#x687F; <pr>lu&oacute;g&#x1CE;n</pr></trd>,
    <trd>&#x87BA;&#x91D8; <pr>lu&oacute;d&imacron;ng</pr></trd>,
    <trd>&#x87BA;&#x9489; <pr>lu&oacute;d&imacron;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxraux.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxraux.0i.TEK_turni">
    <dif>
      Turni <tld/>on:
      <ekz>
        mi <tld/>is hokon al la subo de la tablo
        <fnt>
          <bib>KrM</bib>
          <lok>27. Dekstra mano ne sciu kion faras la maldekstra.</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is je la akvokranoj
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 27</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">csavar</trd>
  </snc>
  <snc mrk="sxraux.0i.TEK_fiksi">
    <dif>
      Fiksi per <tld/>o:
      <ekz>
        tragediverkisto vin <tld/>as sur altajn irbastonojn kaj tiras vin per
        metalfadenoj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">anschrauben</trd>
    <trd lng="hu">csavaroz</trd>
  </snc>
  <trd lng="be">&c_sh;&c_r;&c_u;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>caragolar</trd>,
    <trd>acaragolar</trd>,
    <trd>cargolar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>&scaron;roubovat</trd>,
    <trd>za&scaron;roubovat</trd>
  </trdgrp>
  <trd lng="de">schrauben</trd>
  <trd lng="el">&beta;&jota;&delta;&omega_ton;&ny;&omega;</trd>
  <trd lng="en">screw</trd>
  <trd lng="es">atornillar</trd>
  <trd lng="fr">visser</trd>
  <trd lng="it">avvitare</trd>
  <trd lng="nl">schroeven</trd>
  <trdgrp lng="pl">
    <trd>przykr&eogonek;ca&cacute;</trd>,
    <trd>po&lstroke;&aogonek;czy&cacute; <ind>wkr&eogonek;tem</ind></trd>,
    <trd>skr&eogonek;ci&cacute; <ind>wkr&eogonek;tem</ind></trd>,
    <trd>ze&sacute;rubowa&cacute;</trd>,
    <trd>przy&sacute;rubowa&cacute;</trd>,
    <trd>po&lstroke;&aogonek;czy&cacute; <ind>&sacute;rub&aogonek;</ind></trd>,
    <trd>skr&eogonek;ci&cacute; <ind>&sacute;rub&aogonek;</ind></trd>
  </trdgrp>
  <trd lng="pt">aparafusar</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_v;&c_i;&c_n;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_i;&c_n;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_v;&c_i;&c_n;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_v;&c_i;&c_n;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_v;&c_i;&c_n;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_v;&c_i;&c_n;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">skruva</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_gu;&c_v;&c_i;&c_n;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_gu;&c_v;&c_i;&c_n;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_u;&c_gu;&c_v;&c_i;&c_n;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x62E7; <pr>n&#x1D0;ng</pr></trd>,
    <trd>&#x64F0; <pr>n&#x1D0;ng</pr></trd>,
    <trd>&#x62E7;&#x7D27;&#x87BA;&#x4E1D; <pr>n&iacute;ngj&#x1D0;nlu&oacute;s&imacron;</pr></trd>,
    <trd>&#x64F0;&#x7DCA;&#x87BA;&#x7D72; <pr>n&iacute;ngj&#x1D0;nlu&oacute;s&imacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxraux.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="sxraux.0igi.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Fari helicajn kanelojn &ccirc;e cilindro a&ubreve; konuso.
