  <trd lng="be">&c_p;&c_l;&c_a;&c_sh;&c_k;&c_a; <klr tip="amb">(&c_d;&c_l;&c_ja; &c_r;&c_a;&c_z;&c_mol;&c_b;&c_y;)</klr></trd>
