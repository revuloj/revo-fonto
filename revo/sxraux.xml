<drv mrk="sxraux.0sxlosilo">
  <kap><tld/>&scirc;losilo</kap>
  <snc mrk="sxraux.0sxlosilo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Speco de <ref tip="super" cel="sxraux.0ilo"><tld/>ilo</ref>,
      kiu servas por <tld/>i la <tld/>ojn,
      <tld/>ingojn a&ubreve; <ref tip="vid" cel="bolt.0o">boltojn</ref>
      kun poligona kapo:
      <ekz>
        <ind>angla <tld/>&scirc;losilo</ind>
        <klr>(kies beko estas lar&gcirc;igebla)</klr>;
        <trd lng="ca">clau anglesa</trd>
        <trdgrp lng="de">
          <trd>Engl&auml;nder</trd>,
          <trd>Universalschraubenschl&uuml;ssel</trd>
        </trdgrp>
        <trd lng="es">llave inglesa</trd>
        <trd lng="hu">franciakulcs</trd>
        <trd lng="it">chiave inglese</trd>
        <trd lng="pt">chave inglesa</trd>
        <trd lng="ru">&c_r;&c_a;&c_z;&c_v;&c_o;&c_d;&c_n;&c_o;&c_j; &c_k;&c_l;&c_ju;&c_ch;</trd>
      </ekz>
      <ekz>
        in&gcirc;eniero envenis kaj intencis mortbati la hejtiston per <tld/>&scirc;losilo, &ccirc;ar la&ubreve;
        lia diro la vaporpremo ne sufi&ccirc;is
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 31</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/0/05/2008-04-14_Chrome-Vanadium_Wrenches.jpg/640px-2008-04-14_Chrome-Vanadium_Wrenches.jpg"
      prm="CC BY-SA 3.0"
      ><tld/>&scirc;losilaro
        <fnt>
          <aut>Ildar Sagdejev (Specious), propra verko</aut>
          <lok><url ref="&WCW;/File:2008-04-14_Chrome-Vanadium_Wrenches.jpg"
              >https://commons.wikimedia.org/wiki/File:2008-04-14_Chrome-Vanadium_Wrenches.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_ju;&c_ch; <klr>(&c_g;&c_a;&c_je;&c_ch;&c_n;&c_y;)</klr></trd>
  <trd lng="ca">clau</trd>
  <trd lng="cs">kl&iacute;&ccaron;<klr> (na &scaron;roubov&aacute;n&iacute;)</klr></trd>
  <trd lng="de">Schraubenschl&uuml;ssel</trd>
  <trdgrp lng="en">
    <trd>wrench</trd>,
    <trd>spanner</trd>
  </trdgrp>
  <trd lng="es">llave<klr> (para tuercas)</klr></trd>
  <trd lng="fr">cl&eacute; de serrage</trd>
  <trd lng="hu">csavarkulcs</trd>
  <trd lng="it">chiave
     <klr> (spaccata, ad anello, inglese, a tubo, ecc..)</klr>
  </trd>
  <trd lng="pl">klucz <klr> (do &sacute;rub np. maszynowy, oczkowy, itd.)</klr></trd>
  <trd lng="pt">chave de boca</trd>
  <trd lng="ru">&c_g;&c_a;&c_je;&c_ch;&c_n;&c_y;&c_j; &c_k;&c_l;&c_ju;&c_ch;</trd>
  <trdgrp lng="zh">
    <trd>&#x6273;&#x624B; <pr>b&amacron;nsh&#x1D2;u</pr></trd>,
    <trd>&#x87BA;&#x4E1D;&#x94B3; <pr>lu&oacute;s&imacron;qi&aacute;n</pr></trd>,
    <trd>&#x87BA;&#x7D72;&#x9257; <pr>lu&oacute;s&imacron;qi&aacute;n</pr></trd>
  </trdgrp>
</drv>

