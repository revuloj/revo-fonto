<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: briz.xml,v 1.14 2022/03/13 11:53:50 revo Exp $">
<kap>
  <rad>briz</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="briz.0o">
  <kap><tld/>o</kap>
  <snc mrk="briz.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Mezforta <ref tip="super" cel="vent.0o.MET">vento</ref> okazanta
      pro varmodiferencoj inter lokoj, ekzemple inter maro kaj tero:
      <ekz>
        kelkajn tagojn ni &scirc;ipveturis kun modera <tld/>o
        <fnt>
          <aut>Lukiano, trad. G. Berveling</aut>,
          <vrk><url ref="http://www.esperanto.gr/lit/els/rakontoj.pdf">veraj
          rakontoj</url>. - 2005</vrk>
        </fnt>.
      </ekz>
      <ekz>
        kie blovas <tld/>o, ondadas fragmitoj kaj kantas birdoj
        <fnt>
          <aut>Ren Leiping</aut>,
          <vrk><url ref="http://www.espero.com.cn/z06-08h/08h-1/08-1-4.htm">Birdoprotektanto...</url></vrk>,
          <lok>[vidita en 2006-10]</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="vent.0o">vento</ref>
    <ref tip="vid" cel="zefir.0o">zefiro</ref>
    <trd lng="bg">&c_b;&c_r;&c_i;&c_z;</trd>
    <trd lng="ca">brisa</trd>
    <trd lng="de">Brise</trd>
    <trd lng="en">breeze</trd>
    <trd lng="fa">&a_n;&a_s;&f_y;&a_m;</trd>
    <trd lng="fr">brise</trd>
    <trd lng="pl">bryza</trd>
    <trd lng="ru">&c_b;&c_r;&c_i;&c_z;</trd>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_y;&c_z;</trd>
  <trd lng="bo">&#xF62;&#xFB3;&#xF74;&#xF44;&#xF0B;&#xF56;&#xF66;&#xF72;&#xF63;&#xF0B;&#xF54;&#xF7C;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>br&#xED;za</trd>,
    <trd>p&rcaron;&#xED;mo&rcaron;sk&#xFD; v&#xED;tr</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&#x5E8;&#x5D5;&#x5BC;&#x5D7;&#x5B7; &#x5E7;&#x5DC;&#x5D4;</trd>,
    <trd>&#x5D1;&#x5E8;&#x5D9;&#x5D6;&#x5D4;</trd>
  </trdgrp>
  <trd lng="sk">v&#xE1;nok</trd>
  <trd lng="uk">&#x431;&#x440;&#x438;&#x437;</trd>
</drv>
</art>

