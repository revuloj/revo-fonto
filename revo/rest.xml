<drv mrk="rest.post0i">
  <kap>post<tld/>i</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="rest.post0i.KOMUNE">
    <dif>
      <tld lit="R"/>i post iu okaza&jcirc;o, post la foriro de aliaj:
      <ekz>
        ili do kolektis <klr>[la fragmentojn el]</klr> la kvin
        hordeaj panoj, kiuj post<tld/>is al la man&gcirc;intoj
        <fnt><bib>NT</bib>, <lok>&Joh; 6:13</lok></fnt>;
      </ekz><ekz>
        <klr>[la princo eliris kaj]</klr> &ccirc;iuj aliaj personoj en
        la &ccirc;ambrego post<tld/>is
        <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 11a</lok></fnt>;
      </ekz><ekz>
        pri tiu punkto e&ccirc; ne grajno da suspekto postrestu
        <fnt><bib>UrdHadda</bib><lok>p. 8a</lok></fnt>;
      </ekz><ekz>
        <uzo tip="fak">AGR</uzo>
        la <ind>post<tld/>a&jcirc;o</ind>n de via rikolto ne kolektu
        <fnt><bib>MT</bib>, <lok>&Lev; 23:22</lok></fnt>.
        <trdgrp lng="es">
          <trd>restante</trd>,
          <trd>resto</trd>,
          <trd>residuo</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>restant<klr tip="ind"> (ce qui reste)</klr></trd>,
          <trd>reliquat<klr tip="ind"> (ce qui reste)</klr></trd>,
          <trd>r&eacute;sidu<klr tip="ind"> (ce qui reste)</klr></trd>,
          <trd>glanure</trd>
        </trdgrp>
        <trdgrp lng="pl">
          <trd>spu&sacute;cizna</trd>,
          <trd>pozosta&lstroke;o&sacute;&cacute;</trd>
        </trdgrp>
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_a;&c_s;&c_t;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;<klr tip="amb">
        (&c_p;&c_a;&c_s;&c_mol;&c_l;&c_ja; &c_k;&c_a;&c_g;&c_o;&c_s;&c_mol;&c_c;&c_ib;, &c_ch;&c_a;&c_g;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
    <trdgrp lng="cs">
      <trd>p&rcaron;eb&yacute;vat</trd>,
      <trd>setrvat</trd>,
      <trd>zb&yacute;t</trd>,
      <trd>zb&yacute;vat</trd>,
      <trd>zdr&zcaron;et se</trd>,
      <trd>z&uring;stat</trd>
    </trdgrp>
    <trd lng="es">ser dejado</trd>
    <trdgrp lng="fr">
      <trd>&ecirc;tre en <ind>reste</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>megmarad</trd>,
      <trd>elmarad</trd>
    </trdgrp>
    <trd lng="pl">pozosta&cacute;</trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_s;&c_t;&c_a;&c_t;&c_mol;&c_s;&c_ja;<klr tip="ind">
          (&c_p;&c_o;&c_s;&c_l;&c_je; &c_ch;&c_je;&c_g;&c_o;-&c_l;., &c_k;&c_o;&c_g;&c_o;-&c_l;.)</klr></trd>,
      <trd>&c_o;&c_s;&c_t;&c_a;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;<klr tip="amb">
          (&c_p;&c_o;&c_s;&c_l;&c_je; &c_ch;&c_je;&c_g;&c_o;-&c_l;., &c_k;&c_o;&c_g;&c_o;-&c_l;.)</klr></trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>osta&tcaron;</trd>,
      <trd>zosta&tcaron;</trd>,
      <trd>zv&yacute;&scaron;i&tcaron;</trd>
    </trdgrp>
  </snc>
  <snc mrk="rest.post0i.maldevanci">
    <dif>
      <ref tip="dif" cel="devanc.mal0i">Maldevanci</ref>:
      <ekz>
        Kolombo iomete post<tld/>is kaj sekvis &scirc;in, &scirc;telumante de
        palmo al palmo
  	    <fnt>
  	      <aut>Jen&odblac; Rejt&odblac;, tr. L&aacute;szl&oacute; Bal&aacute;zs</aut>:
  	      <vrk><url ref="http://www.freeweb.hu/verkoj/02.htm">La anta&ubreve;en&scirc;ovita garnizono</url></vrk>
  	    </fnt>;
      </ekz><ekz>
        la vorto <ctl>evolulando</ctl> estas e&ubreve;femismo por la landoj
        post<tld/>antaj
        <fnt>
          <aut>Nikolao Gudskov</aut>:
          <vrk>Kondi&ccirc;oj por evoluo de evolulandoj</vrk>.
          <bib>Monato</bib>
        </fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        anka&ubreve; Podro ne post<tld/>is en meritado de la&ubreve;roj por garni
        sin kaj Mageltonon
        <fnt>
          <aut>Ch. Dickens, tr. W. Auld</aut>:
          <vrk><url ref="&LOdE;counter/counter.php?libro=21">La
          postlasitaj aferoj de la Klubo Pikvika</url></vrk>.
          <lok>&Ccirc;ap. 7a.</lok>
        </fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        la teatrodirektoro ne post<tld/>is koncerne elegantecon
        <fnt><bib>UrdHadda</bib><lok>p. 10a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="fru.mal0i">malfrui</ref>
    <trdgrp lng="be">
      <trd>&c_a;&c_d;&c_s;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_u; &c_r;&c_u;&c_h;&c_u;)</klr></trd>,
      <trd>&c_z;&c_a;&c_t;&c_r;&c_y;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_c;&c_a; <klr tip="amb">(&c_u; &c_r;&c_a;&c_z;&c_mol;&c_v;&c_ib;&c_c;&c_mol;&c_c;&c_ib;)</klr></trd>
    </trdgrp>
    <trd lng="de">zur&uuml;ckbleiben</trd>
    <trdgrp lng="es">
      <trd>retrasarse</trd>,
      <trd>quedarse atr&aacute;s</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>retarder<klr tip="ind"> (sur son &eacute;poque, rester en
          arri&egrave;re)</klr></trd>,
      <trd>rester en arri&egrave;re</trd>
    </trdgrp>
    <trd lng="pl">pozosta&cacute;</trd>
    <trdgrp lng="ru">
      <trd> &c_o;&c_t;&c_s;&c_t;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_s;&c_t;&c_a;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x5F8C;&#x306B;&#x6B8B;&#x308B;  <pr>&#12354;&#12392;&#x306B;&#x306E;&#12371;&#x308B;</pr></trd>,
    <trd>&#x53D6;&#x308A;&#x6B8B;&#12373;&#x308C;&#x308B;
    <pr>&#12392;&#x308A;&#x306E;&#12371;&#12373;&#x308C;&#x308B;</pr></trd>,
    <trd>&#x843D;&#x5F8C;&#12377;&#x308B;  <pr>&#12425;&#x304F;&#12372;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_s;&c_t;&c_a;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_l;&c_i;&c_sh;&c_a;&c_t;&c_i;&c_s;&c_ja; &c_p;&c_o;&c_z;&c_a;&c_d;&c_u;</trd>,
    <trd>&c_z;&c_a;&c_l;&c_i;&c_sh;&c_a;&c_t;&c_i;&c_s;&c_ja; (&c_p;&c_o; &c_k;&c_o;&c_m;&c_u;-&c_n;.</trd>,
    <trd>&c_ch;&c_o;&c_m;&c_u;-&c_n;.)</trd>
  </trdgrp>
</drv>
