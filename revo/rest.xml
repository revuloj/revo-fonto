    <trd>estada</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>me&scaron;k&aacute;n&iacute;</trd>,
    <trd>pobyt</trd>
  </trdgrp>
  <trd lng="de">Aufenthalt</trd>
  <trdgrp lng="es">
    <trd>permanencia</trd>,
    <trd>estancia</trd>
  </trdgrp>
