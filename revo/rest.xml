<drv mrk="rest.0adi">
  <kap><tld/>adi</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="rest.0adi.KOMUNE">
    <dif>
      Longe <tld/>i:
      <ekz>
        li <tld/>adu kun vi por &ccirc;iam
        <fnt>
          <bib>NT</bib>
          <lok>Johano 14:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam ili petis lin <tld/>adi pli longe, li ne konsentis
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 18:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>adu en mia amo
        <fnt>
          <bib>NT</bib>
          <lok>Johano 15:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili ne <tld/>ados en sia nekredemeco
        <fnt>
          <bib>NT</bib>
          <lok>Romanoj 11:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi restos kaj apud<tld/>ados kun vi &ccirc;iuj
        <fnt>
          <bib>NT</bib>
          <lok>Filipianoj 1:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        matene &scirc;i volonte <tld/>adis pli longe en la lito
        <fnt>
          <bib>Fab4</bib>
          <lok>&leftquot;Bela&rightquot;</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="logx.0i">lo&gcirc;i</ref>,
      <ref cel="nest.0i">nesti</ref>,
      <ref cel="okup.0i">okupi</ref>,
      <ref cel="instal">sin instali</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_n;&c_a;&c_h;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trd lng="de">sich aufhalten</trd>
  <trdgrp lng="es">
    <trd>permanecer</trd>,
  </trdgrp>
  <trd lng="fr">s&eacute;journer</trd>
  <trdgrp lng="hu">
    <trd>tart&oacute;zkodik <klr tip="ind">(vhol)</klr></trd>,
    <trd>id&odblac;zik</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_b;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="uk">&#x43F;&#x435;&#x440;&#x435;&#x431;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
</drv>

<drv mrk="rest.0ajxo">
