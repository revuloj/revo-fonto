<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>2</ofc>
<rad>disciplin</rad>/o</kap>
<drv mrk="discip.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Deviga regularo kaj lernita obeo necesa por la bonordo de iu
      anaro:
      <ekz>
        militista, mona&hcirc;a <tld/>o;
      </ekz>
      <ekz>
        mi al &ccirc;iuj instruis <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a cedo de la malplimulto<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ne deklini&gcirc;i de la <tld/>a vojo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e aligi siajn fortojn al la komuna armeo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Zucht</trd>,
      <trd>Ordnung</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&fa_alef;&fa_nun;&fa_zat;&fa_be;&fa_alef;&fa_ta;</trd>
      <trd>&fa_nun;&fa_za;&fa_mim;</trd>
      <trd>&fa_dal;&fa_ye;&fa_sin;&fa_ye;&fa_pe;&fa_lam;&fa_ye;&fa_nun;</trd>
    </trdgrp>
    <trd lng="nl">tucht</trd>
    <trdgrp lng="pl">
      <trd>dyscyplina</trd>,
      <trd>porz&aogonek;dek</trd>,
      <trd>karno&sacute;&cacute;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Reguloj rekomendindaj por certigi la bonan funkciadon de la homaj
      kapabloj:
      <ekz>
        intelekta, scienca <tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Disziplin</trd>,
      <trd>Zweig</trd>
    </trdgrp>
    <trd lng="nl">tak<klr tip="ind"> (v.wetenschap bv.)</klr></trd>
    <trd lng="pl">dyscyplina</trd>
  </snc>
  <trd
  lng="be">&c_d;&c_y;&c_s;&c_c;&c_y;&c_p;&c_l;&c_ib;&c_n;&c_a;</trd>
  <trd lng="en">discipline</trd>
  <trd lng="es">disciplina</trd>
  <trd lng="fa">&fa_re;&fa_sxin;&fa_te;&fa_heh;<klr tip="amb">
  (&fa_ein;&fa_lam;&fa_mim;&fa_ye; &fa_ye;&fa_alef;
  &fa_te;&fa_hxe;&fa_sat;&fa_sat;&fa_ye;)</klr></trd>
  <trd lng="fr">discipline</trd>
  <trd lng="hu">fegyelem</trd>  
  <trd lng="nl">discipline</trd>
  <trd lng="pt">disciplina</trd>
  <trd lng="ru">&c_d;&c_i;&c_s;&c_c;&c_i;&c_p;&c_l;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">discipl&iacute;na</trd>
</drv>

<drv mrk="discip.0emo">
  <kap><tld/>emo</kap>
  <snc>
    <dif>
      Volonta absoluta obeo al iu regularo:
      <ekz>
        <tld/>emo estas nepre necesa en nia movado.
      </ekz>
    </dif>
  </snc>
  <trd
  lng="be">&c_d;&c_y;&c_s;&c_c;&c_y;&c_p;&c_l;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>Disziplin</trd>,
    <trd>Unterordnung</trd>
  </trdgrp>
  <trd lng="en">obedience</trd>
  <trdgrp lng="fa">
    <trd>&fa_fe;&fa_re;&fa_mim;&fa_alef;&fa_nun;&fa_be;&fa_re;&fa_dal;&fa_alef;&fa_re;&fa_ye;</trd>
    <trd>&fa_alef;&fa_ta;&fa_alef;&fa_ein;&fa_te;</trd>
  </trdgrp>
  <trd lng="hu">fegyelmezetts&eacute;g</trd>  
  <trd lng="nl">discipline<klr tip="ind"> (onderdanigheid)</klr></trd>
  <trdgrp lng="pl">
    <trd>zdycyplinowanie</trd>,
    <trd>karno&sacute;&cacute;</trd>,
    <trd>podporz&aogonek;dkowanie</trd>
  </trdgrp>
  <trd
  lng="ru">&c_d;&c_i;&c_s;&c_c;&c_i;&c_p;&c_l;&c_i;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">disciplinovanos&tcaron;</trd>
</drv>

<drv mrk="discip.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Alkutimigi iun obei al <tld/>o:
      <ekz>
        li balda&ubreve; <tld/>is la lernantojn;
      </ekz>
      <ekz>
        <tld/>i siajn impulsojn.
      </ekz>
    </dif>
  </snc>
  <trd
  lng="be">&c_d;&c_y;&c_s;&c_c;&c_y;&c_p;&c_l;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>disziplinieren</trd>,
    <trd>z&auml;hmen</trd>,
    <trd>b&auml;ndigen</trd>,
    <trd>in Zucht und Ordnung halten</trd>
  </trdgrp>
  <trd lng="en">discipline</trd>
  <trd lng="es">disciplinar</trd>
  <trdgrp lng="fa">
    <trd>&fa_mim;&fa_nun;&fa_zat;&fa_be;&fa_ta;
    &fa_kaf;&fa_re;&fa_dal;&fa_nun;</trd>
    <trd>&fa_be;&fa_heh; <ind>&fa_nun;&fa_za;&fa_mim;</ind>
    &fa_ein;&fa_alef;&fa_dal;&fa_te;
    &fa_dal;&fa_alef;&fa_dal;&fa_nun;</trd>
  </trdgrp>
  <trd lng="fr">discipliner</trd>
  <trd lng="hu">(meg)<ind>fegyelmez</ind></trd>  
  <trd lng="nl">disciplineren</trd>
  <trd lng="pl">dyscyplinowa&cacute;</trd>
  <trd
  lng="ru">&c_d;&c_i;&c_s;&c_c;&c_i;&c_p;&c_l;&c_i;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">disciplinova&tcaron;</trd>
</drv>
</art>
