<vortaro>
<art mrk="$Id: discip.xml,v 1.35 2023/11/04 14:29:02 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>disciplin</rad>/o
</kap>
<drv mrk="discip.0o">
  <kap><tld/>o</kap>
  <snc mrk="discip.0o.MOR">
    <dif>
      Lernita submeti&gcirc;o, <ref tip="super" cel="obe.0ado">obeado</ref> al reguloj,
      necesa por la bonordo de iu anaro:
      <ekz>
        li enkondukis severan <tld/>on inter la pastroj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tiu]</klr> skriba&jcirc;o <klr>[&#x2026;]</klr> estas anka&ubreve; utila por <klr>[&#x2026;]</klr>
        <tld/>o en justeco
        <fnt>
          <bib>NT</bib>
          <lok>II. Timoteo 3:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili venkos, &ccirc;ar ilia militistaro estas glora pro sia <tld/>o
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        nia armeo perdos la <tld/>on, kiam <klr>[&#x2026;]</klr> en &gcirc;ian fronton englitis
        perfiduloj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi al <klr>[&#x2026;]</klr> &ccirc;iuj instruis <tld/>on
        <fnt><bib>Revizoro</bib></fnt>;
      </ekz>
      <ekz>
        inflacio ne estos pli granda obstaklo ol financa <tld/>o
        <fnt>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2011/010398.php">E&ubreve;ro da&ubreve;re allogas</url></vrk>
          <bib>Monato</bib>
          <lok>2011/10, p. 20</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; 50 jaroj mal<tld/>o estis, se ni ka&scirc;e babilis dum leciono, ridis a&ubreve; sendis
        letereton al alia
        <fnt>
          <aut>Lenke Sz&aacute;sz</aut>
          <vrk><url ref="&Monato;/2015/011475.php">La klasa fototabulo</url></vrk>
          <bib>Monato</bib>
          <lok>2015/08, p. 30</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Zucht</trd>,
      <trd>Ordnung</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_A;&a_n;&a_D;&a_b;&a_A;&a_T;</trd>
      <trd>&a_n;&a_Z;&a_m;</trd>
      <trd>&a_d;&f_y;&a_s;&f_y;&f_p;&a_l;&f_y;&a_n;</trd>
    </trdgrp>
    <trd lng="nl">tucht</trd>
    <trdgrp lng="pl">
      <trd>dyscyplina</trd>,
      <trd>porz&aogonek;dek</trd>,
      <trd>karno&sacute;&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="discip.0o.metodo">
    <dif>
      <ref tip="super" cel="regul.0aro">Regularo</ref> obeenda de anaro, por certigi
      ties bonan funkciadon:
      <ekz>
        ni akceptas la <tld/>on de niaj klasaj organizoj
        <fnt>
          <bib>Lanti</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi ne konas tian paragrafon en la soldata <tld/>o, kiu malpermesus tion
        <fnt>
          <bib>SatirRak</bib>
        </fnt>;
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Regelwerk</trd>,
      <trd>Vorgaben</trd>,
    </trdgrp>
    <trd lng="nl">tak<klr tip="ind"> (v.wetenschap bv.)</klr></trd>
    <trd lng="pl">dyscyplina</trd>
  </snc>
  <snc mrk="discip.0o.brancxo">
    <dif>
      <ref tip="dif" cel="brancx.0o.FIG">Bran&ccirc;o</ref> de scienco, instruado, sporto, kun aparta
      regularo:
      <ekz>
        lia faka verkado ampleksis anka&ubreve; historion, filozofion, sociologion kaj aliajn
        <tld/>ojn
        <fnt>
          <aut>Bo Sandelin</aut>
          <vrk><url ref="&Monato;/2001/pdf/monato200105.pdf#page=12">La 19a jarcento: krisolo
          de ekonomikaj skoloj, Marksismo</url></vrk>
           <bib>Monato</bib>
          <lok>2001/05, p. 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        necesas turni sin al la sub-<tld/>o psikokrokodilologio
        <fnt>
          <bib>GhR</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi faru lozajn rimarkojn pri diverstipaj soci-intelektaj kondutoj, miaj rimarkoj
        ne pretendas al scienca <tld/>o, sed estas frukto de <klr>[libera
        pensado]</klr>
        <fnt>
          <bib>LOdE</bib>
          <aut>Walter Zelazny </aut>
          <vrk>Pri la rilatoj inter inteligencio kaj plebo en la movado</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Zweig</trd>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_s;&c_c;&c_y;&c_p;&c_l;&c_ib;&c_n;&c_a;</trd>
  <trd lng="ca">disciplina</trd>
  <trdgrp lng="cs">
    <trd>discipl&iacute;na</trd>,
    <trd>k&aacute;ze&ncaron;</trd>,
    <trd>&rcaron;&aacute;d</trd>
  </trdgrp>
  <trd lng="de">Disziplin</trd>
  <trd lng="en">discipline</trd>
  <trd lng="es">disciplina</trd>
  <trd lng="fa">&a_r;&a_sh;&a_t;&a_h;<klr tip="amb">
  (&a_ayn;&a_l;&a_m;&f_y; &f_y;&a_A;
  &a_t;&a_kh;&a_S;&a_S;&f_y;)</klr></trd>
  <trd lng="fr">discipline</trd>
  <trd lng="hu">fegyelem</trd>
  <trdgrp lng="ja">
    <trd>&#x898F;&#x5F8B;  <pr>&#x304D;&#x308A;&#x3064;</pr></trd>,
    <trd>&#x98A8;&#x7D00;  <pr>&#x3075;&#x3046;&#x304D;</pr></trd>,
    <trd>&#x8A13;&#x7DF4;  <pr>&#x304F;&#x3093;&#x308C;&#x3093;</pr></trd>,
    <trd>&#x3057;&#x3064;&#x3051;</trd>,
    <trd>&#x6212;&#x5F8B;  <pr>&#x304B;&#x3044;&#x308A;&#x3064;</pr></trd>
  </trdgrp>
  <trd lng="nl">discipline</trd>
  <trd lng="pt">disciplina</trd>
  <trd lng="ru">&c_d;&c_i;&c_s;&c_c;&c_i;&c_p;&c_l;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>discipl&iacute;na</trd>,
    <trd>poriadok</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_i;&c_s;&c_c;&c_i;&#x43F;&#x43B;&c_ib;&#x43D;&c_a;</trd>,
    <trd>&c_d;&c_i;&c_s;&c_c;&c_i;&#x43F;&#x43B;&c_ib;&#x43D;&c_a;</trd>,
    <trd>&#x43F;&c_r;&c_je;&c_d;&#x43C;&c_je;&c_t; (&#x43D;&c_a;&c_v;&c_ch;&c_a;&#x43D;&#x43D;&#x44F;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7D00;&#x5F8B; <pr>j&#xEC;l&#xFC;</pr></trd>,
    <trd>&#x8A13;&#x80B2; <pr>x&#xF9;ny&#xF9;</pr></trd>,
    <trd>&#x5206;&#x7C7B;&#x65B9;&#x6CD5; <pr>f&#x113;nl&#xE8;if&#x101;ngf&#x1CE;</pr></trd>,
    <trd>&#x7D00; <pr>j&#xEC;</pr></trd>,
    <trd>&#x81EA;&#x6211;&#x4FEE;&#x517B; <pr>z&#xEC;w&#x1D2;xi&#x16B;y&#x1CE;ng</pr></trd>,
    <trd>&#x81EA;&#x6211;&#x8A13;&#x7DF4; <pr>z&#xEC;w&#x1D2;x&#xF9;nli&#xE0;n</pr></trd>,
    <trd>&#x81EA;&#x5F8B; <pr>z&#xEC;l&#xFC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="discip.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Montranta <tld/>on, kun <tld/>o:
      <ekz>
        <tld/>a cedo de la malplimulto
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Oka Kongreso Esperantista en Krakow en la 11a de a&ubreve;gusto 1912</vrk>
        </fnt>;
      </ekz>
      <ekz>
        ni <klr>[ne]</klr> deklini&gcirc;u de nia <tld/>a vojo
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado en la Guildhall (Londono) en la 21a de a&ubreve;gusto 1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        ili balda&ubreve; <tld/>e aligos siajn fortojn al tiu komuna granda armeo
        <fnt>
          <bib>OV</bib>
          <lok>p. 448</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="sever.0a.iu">severa<sncref/></ref>,
       <ref cel="rigid.0a.necedeme">rigida<sncref/></ref>,
       <ref cel="rigor.0a">rigora</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_y;&c_s;&c_c;&c_y;&c_p;&c_l;&c_ib;&c_n;&c_a;&c_r;&c_n;&c_y;</trd>,
    <trd>&c_d;&c_y;&c_s;&c_c;&c_y;&c_p;&c_l;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">diszipliniert</trd>
  <trd lng="ja">&#x898F;&#x5F8B;&#x4E0A;&#x306E;  <pr>&#x304D;&#x308A;&#x3064;&#x3058;&#x3087;&#x3046;&#x306E;</pr></trd>
  <trd lng="uk">&c_d;&c_i;&c_s;&c_c;&c_i;&#x43F;&#x43B;&c_ib;&#x43D;&c_a;&c_r;&#x43D;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x9075;&#x5B88;&#x7D00;&#x5F8B; <pr>z&#x16B;nsh&#x1D2;uj&#xEC;l&#xFC;</pr></trd>,
    <trd>&#x6709;&#x7D44;&#x7E54; <pr>y&#x1D2;uz&#x1D4;zh&#x12B;</pr></trd>,
    <trd>&#x53D7;&#x8FC7;&#x8BAD;&#x7EC3; <pr>sh&#xF2;ugu&#xF2;x&#xF9;nli&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="discip.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="discip.0i.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      Alkutimigi iun obei al <tld/>o;
      <ref tip="vid" cel="obe.0igi">obeigi</ref>:
      <ekz>
        la militista rigoro, vere, <tld/>is por ia tempo lian memvolon
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la grekaj regimentoj ne estas sufi&ccirc;e <tld/>itaj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la debato estis fervora, <klr>[&#x2026;]</klr> sed <tld/>ita kaj demokrata
        <fnt>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2007/009291.php">La mallonga politika kariero de Hortensia
          Kvinke</url></vrk>
          <bib>Monato</bib>
          <lok>2007/10, p. 8</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dres.0i">dresi</ref>,
      <ref cel="eduk.0i.infanon">eduki<sncref/></ref>,
      <ref cel="met.sub0i">submeti</ref>,
      <ref cel="trejn.0i">trejni</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_s;&c_c;&c_y;&c_p;&c_l;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">disciplinar</trd>
  <trdgrp lng="cs">
    <trd>b&yacute;t uk&aacute;zn&ecaron;n&yacute;</trd>,
    <trd>dodr&zcaron;ovat disciplinu</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>disziplinieren</trd>,
    <trd>z&auml;hmen</trd>,
    <trd>b&auml;ndigen</trd>,
    <trd>in Zucht und Ordnung halten</trd>
  </trdgrp>
  <trd lng="en">discipline</trd>
  <trd lng="es">disciplinar</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&a_n;&a_D;&a_b;&a_T;
    &f_k;&a_r;&a_d;&a_n;</trd>
    <trd>&a_b;&a_h; <ind>&a_n;&a_Z;&a_m;</ind>
    &a_ayn;&a_A;&a_d;&a_t;
    &a_d;&a_A;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>discipliner</trd>,
    <trd>mettre au pas</trd>
  </trdgrp>
  <trd lng="he">&lamed;&he;&resh;&gimel;&yod;&lamed; &lamed;&mem;&shin;&mem;&ayin;&tav;</trd>
  <trd lng="hu">(meg)<ind>fegyelmez</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x898F;&#x5F8B;&#x3065;&#x3051;&#x308B;  <pr>&#x304D;&#x308A;&#x3064;&#x3065;&#x3051;&#x308B;</pr></trd>,
    <trd>&#x935B;&#x7DF4;&#x3059;&#x308B;  <pr>&#x305F;&#x3093;&#x308C;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x3057;&#x3064;&#x3051;&#x3092;&#x3059;&#x308B;</trd>
  </trdgrp>
  <trd lng="nl">disciplineren</trd>
  <trd lng="pl">dyscyplinowa&cacute;</trd>
  <trd lng="ru">&c_d;&c_i;&c_s;&c_c;&c_i;&c_p;&c_l;&c_i;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>disciplinova&tcaron;</trd>,
    <trd>dodr&zcaron;iava&tcaron; discipl&iacute;nu</trd>
  </trdgrp>
  <trd lng="uk">&c_d;&c_i;&c_s;&c_c;&c_i;&#x43F;&#x43B;&c_ib;&#x43D;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x60E9; <pr>ch&#xE9;ng</pr></trd>,
    <trd>&#x7BA1;&#x6559; <pr>gu&#x1CE;nji&#xE0;o</pr></trd>,
    <trd>&#x99B4; <pr>x&#xFA;n</pr></trd>,
    <trd>&#x60E9;&#x5904; <pr>ch&#xE9;ngch&#xF9;</pr></trd>
  </trdgrp>
</drv>


<drv mrk="discip.0emo">
