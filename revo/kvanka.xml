<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kvanka.xml,v 1.26 2020/01/14 17:10:17 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>kvankam</rad> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kvanka.0">
   <kap>
     <ofc>*</ofc><tld/>
   </kap>
   <gra><vspec>konjunkcio</vspec></gra>
   <snc mrk="kvanka.0.KOMUNE">
     <dif>
       Ne &gcirc;enate de la malhelpo ke...,
       <ref tip="sin" cel="malgra.0ke">malgra&ubreve; ke</ref>,
       <ref tip="vid" cel="spit.0eke">spite al tio ke</ref>:
      <ekz>
        ricevinte de vi leteron internacian kaj kompreninte &gcirc;in, <tld/>
        li la lingvon ne lernis <klr>[&#8230;]</klr>
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni nomas &gcirc;in tola&jcirc;o, <tld/> ne &ccirc;io estas farita el tolo
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
         <klr>[mi vidas]</klr> nenion, <tld/> &ccirc;io estas luma
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
         pre&gcirc;adi mi ne povas, <tld/> forte mi volus 
         pre&gcirc;i
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        spitante &ccirc;iujn &leftquot;<tld/>ojn&#8221; kaj &leftquot;ne-&ccirc;iamojn&#8221;
        <fnt>
          <bib>Monato</bib>
          <aut>Grigorij Arosev</aut>
          <vrk><url
          ref="https://www.monato.be/2003/008100.html">Grigorij
          Arosev</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
    <rim>
       La verbo <em>esti</em> kelkfoje estas subkomprenita:
       <ekz>
         ni voja&gcirc;is kune, <tld/> malamikoj.
       </ekz>
    </rim>
  <trdgrp lng="be">
     <trd>&c_h;&c_o;&c_c;&c_mol;</trd>,
     <trd>&c_h;&c_a;&c_c;&c_ja;</trd>
  </trdgrp>
  <trd lng="bg">&c_v;&c_malmol;&c_p;&c_r;&c_je;&c_k;&c_i; &c_ch;&c_je;</trd>
  <trd lng="cs">a&ccaron;koliv</trd>
  <trdgrp lng="de">
    <trd>obwohl</trd>,
    <trd>obgleich</trd>,
    <trd>obschon</trd>,
    <trd>obzwar</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>although</trd>,
    <trd>while</trd>
  </trdgrp>
  <trd lng="es">aunque</trd>
  <trd lng="fi">vaikka</trd>
  <trd lng="fr">bien que</trd>
  <trdgrp lng="hu">
     <trd>noha</trd>,
     <trd>hab&aacute;r</trd>,
     <trd>j&oacute;llehet</trd>
  </trdgrp>
  <trd lng="io">quankam</trd>
  <trdgrp lng="nl">
     <trd>alhoewel</trd>,
     <trd>hoewel</trd>,
     <trd>ofschoon</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>b&aogonek;d&zacute;</trd>,
    <trd>chocia&zdot;</trd>,
    <trd>cho&cacute;</trd>,
    <trd>jakkolwiek</trd>,
    <trd>wprawdzie</trd>
  </trdgrp> 
  <trdgrp lng="pt">
     <trd>embora <klr tip="amb">(conj.)</klr></trd>,
     <trd>ainda que</trd>,
     <trd>conquanto</trd>,
     <trd>posto que</trd>,
     <trd>apesar de</trd>,
     <trd>se <ind>bem</ind> que</trd>,
     <trd>n&atilde;o <ind>obstante</ind></trd>,
     <trd>em <ind>que</ind></trd>
  </trdgrp>
  <trd lng="ro">de&scedil;i</trd>
  <trd lng="ru">&c_h;&c_o;&c_t;&c_ja;</trd>
  <trd lng="sv">fast&auml;n</trd>
  <trd lng="vo">do</trd>
</drv>

<drv mrk="kvanka.0_tamen">
  <kap>
    <tld/>... tamen...
  </kap>
  <snc mrk="kvanka.0_tamen.KOMUNE">
    <dif>
      <tld lit="K"/> <klr>(emfaze)</klr>:
      <ekz>
        <tld/> mi havus grandan deziron vin ofte ankora&ubreve; tikli 
        per la akra tran&ccirc;ilo <klr>[&#8230;]</klr>, tamen malgra&ubreve; &ccirc;io mi 
        volas malligi vian &scirc;nuron kaj <klr>[liberigi vin]</klr>
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
    <rim>
      <fnt><bib>PV</bib></fnt>
      Kelkaj uzas en tiu senco <em>kvankam... sed...</em>, kiun oni devas severe
      eviti:
      <ekz>
        <tld/> li estas preska&ubreve; almozulo, sed li alportas al la urbo
        re&gcirc;an donacon
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        nun la po&scirc;ta&jcirc;oj estas <tld/> jam specigitaj la&ubreve; la germanaj
        kursoj, sed ili &ccirc;iam ankora&ubreve; estas amerika propra&jcirc;o
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </rim>
  <trd lng="fr">bien que... cependant...</trd>
</drv>
</art>
