<art mrk="$Id: valeri.xml,v 1.7 2022/05/06 12:17:21 revo Exp $">
<kap>
  <rad>Valeri</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="valeri.0o">
  <kap><tld/>o</kap>
  <snc mrk="valeri.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        <tld/>o Evangelisti <klr>...</klr> estas itala historiisto kaj 
        unu el plej popularaj italaj verkistoj de scienc-fikcio  
        <fnt>
          <bib>Viki</bib>, <lok><url ref="&Viki;/Valerio_Evangelisti">
          Valerio Evangelisti</url>, 2008-10-22</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o kaj Andrea estas verkantaj pluajn kantojn en Esperanto, 
        cele al realigo de disko
        <fnt>
          <aut>A. Fontana</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/ancxjo.pac-horano/81750">
          La muzikgrupo "R&ecirc;verie"...</url></vrk>,
          <lok>2008-11-14</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_V;&c_a;&c_l;&c_je;&c_r;</trd>,
      <trd>&c_V;&c_a;&c_l;&c_je;&c_r;&c_y;&c_j;</trd>
    </trdgrp>
    <trd lng="ca">Valeri</trd>
    <trdgrp lng="fr">
      <trd>Val&egrave;re</trd>,
      <trd>Valerius</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>Walery</trd>,
      <trd>Waleriusz</trd>
    </trdgrp>
  </snc>
  <trd lng="uk">&#x412;&#x430;&#x43B;&#x435;&#x440;&#x456;&#x439;</trd>
</drv>
</art>

