<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: dal.xml,v 1.8 2019/07/06 09:10:24 revo Exp $">
<kap>
  <rad>dal</rad>/o
</kap>
<drv mrk="dal.0o">
  <kap><tld/>o</kap>
  <snc mrk="dal.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Sekaj gu&scirc;ograjnoj kiaj pizo, kikero kaj similaj... pecigitaj a&ubreve;
      preparitaj diversmaniere en la hinda kuirarto:
      <ekz>
        patrino en sario makulita miksadas okrokoloran <tld/>on
        <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>man&gcirc;i rubman&gcirc;a&jcirc;on, p.
        11a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">dal</trd>
  <trdgrp lng="en">
     <trd>dhal</trd>,
     <trd>dahl</trd>,
     <trd>dal</trd>,
     <trd>daal</trd>
   </trdgrp>
  <trdgrp lng="fr">
    <trd>dhal</trd>,
    <trd>dal</trd>
  </trdgrp>
  <trd lng="hi">&#2338;&#2354; [<ind>&#7693;hal</ind>]</trd>
  <trdgrp lng="pt">
    <trd>dal</trd>,
    <trd>dhal</trd>
  </trdgrp>
</drv>
</art>

