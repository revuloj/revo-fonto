<vortaro>
<art mrk="$Id: ejnsxtejn.xml,v 1.14 2019/06/30 18:19:02 revo Exp $">
<kap>
  <rad>Ejn&scirc;tejn</rad>/o,
  <var><kap><rad>Ejnstejn</rad>/o</kap></var>
</kap>
<drv mrk="ejnsxtejn.0o">
  <kap><tld/>o,
    <var><kap>Ejnstejno</kap></var>
  </kap>
  <uzo tip="fak">PERS</uzo>
  <snc mrk="ejnsxtejn.0o.PERS">
    <dif>
      Germane <trd lng="de">Einstein<klr tip="ind">
      (Albert)</klr></trd>;
      mondfama <ref tip="lst" lst="voko:historiaj_personoj"
      cel="fizik.0isto">fizikisto</ref>-teoriisto (1879-1955),
      nobelpremiito (1921), unu el la kreintoj de la moderna
      <ref tip="vid" cel="fizik.0o">fiziko</ref>, a&ubreve;toro de la
      teorio de relativeco:
      <ekz>
        mi sentis min kvaza&ubreve; elementa instruisto pri matematiko, al kiu oni petas
        komentarii <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Petro De Smet'</aut>
          <vrk><url ref="&Monato;/2011/010160.php"
              >Belaj figoj post Pasko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kvantum.0o">kvantumo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_E;&c_j;&c_n;&c_sh;&c_t;&c_e;&c_j;&c_n;</trd>
  <trd lng="ca">Einstein</trd>
  <trd lng="en">Einstein</trd>
  <trd lng="fa">&a_A;&f_y;&a_n;&a_sh;&a_t;&f_y;&a_n;<klr tip="ind">
  (&a_A_madda;&a_l;&a_b;&a_r;&a_t;)</klr></trd>
  <trd lng="fr">Einstein<klr tip="ind"> (Albert)</klr></trd>
  <trd lng="es">Einstein</trd>
  <trd lng="hu">Einstein</trd>
  <trd lng="it">Einstein</trd>
  <trd lng="nl">Einstein<klr tip="ind"> (Albert)</klr></trd>
  <trd lng="pl">Albert <ind>Einstein</ind></trd>
  <trd lng="pt">Einstein</trd>
  <trd lng="ru">&c_E;&c_j;&c_n;&c_sh;&c_t;&c_je;&c_j;&c_n;</trd>
</drv>
</art>

