<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>konsci</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="konsci.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="konsci.0i.PSI">
    <dif>
      Senpere scikoni sian propran sentadon a&ubreve; penson:
      <ekz>
        <tld/>i ion<fnt>Z</fnt>,
      </ekz>
      <ekz>
        pri io;
      </ekz>
      <ekz>
        <tld/>i sian fortecon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ni <tld/>u bone la tutan gravecon de la hodia&ubreve;a tago<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la malsanulo ne plu <tld/>as.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_s;&c_mol;&c_v;&c_je;&c_d;&c_a;&c_m;&c_l;&c_ja;&c_c;&c_mol;</trd>
  <trd lng="de">sich einer Sache <ind>bewusst</ind> sein</trd>
  <trd lng="en">be <ind>conscious</ind></trd>
  <trd lng="fr">&ecirc;tre <ind>conscient</ind> de</trd>
  <trdgrp lng="hu">
    <trd>tudat&aacute;n&aacute;l van</trd>,
    <trd>tudat&aacute;ban van</trd>
  </trdgrp>
  <trd lng="nl">zich <ind>bewust</ind> zijn</trd>
  <trdgrp lng="pt">
    <trd>ter <ind>consci&ecirc;ncia</ind> de </trd>,
    <trd>ser <ind>consciente</ind> de</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_z;&c_n;&c_a;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="konsci.0o">
  <kap><tld/>o</kap>
  <snc mrk="konsci.0o.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      Senpera scikono, kiun la menso havas pri siaj statoj kaj agoj:
      <ekz>
        &scirc;i svenis, &scirc;i perdis la <tld/>on
        <fnt><bib>Gerda</bib>, <lok>&ccirc;apitro 9a</lok></fnt>;
      </ekz>
      <ekz>
        kun <tld/>o de sia senkapableco<fnt>Z</fnt>;
      </ekz>
      <ekz>
        denove ricevi la <tld/>on<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_s;&c_mol;&c_v;&c_je;&c_d;&c_a;&c_m;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_s;&c_mol;&c_v;&c_ja;&c_d;&c_o;&c_m;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_t;&c_o;&c_m;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">v&ecaron;dom&iacute;</trd>
  <trdgrp lng="de">
    <trd>Bewusstsein</trd>,
    <trd>Besinnung</trd>
  </trdgrp>
  <trd lng="fr">conscience</trd>
  <trdgrp lng="hu">
    <trd>tudat</trd>,
    <trd>&ouml;ntudat</trd>
  </trdgrp>
  <trd lng="nl">bewustzijn</trd>
  <trd lng="ru">&c_s;&c_o;&c_z;&c_n;&c_a;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="konsci.0a">
  <kap><tld/>a</kap>
  <snc mrk="konsci.0a.PSI">
    <dif>
      Karakterizata per <tld/>o:
      <ekz>
        <tld/>a ago;
      </ekz>
      <ekz>
        <tld/>a pri kulpo;
      </ekz>
      <ekz>
        li parolis kun mem<tld/>a modesteco;
      </ekz>
      <ekz>
        <tld/>e mensogi<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ne blinde, sed <tld/>e akcepti reformojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind>sen<tld/>e</ind> vi agis, <tld/>e vi pagos.
        <trdgrp lng="de">
          <trd>unbewusst</trd>
        </trdgrp>
        <trd lng="hu">&ouml;ntudatlanul</trd>
        <trd lng="ru">&c_b;&c_je;&c_s;&c_s;&c_o;&c_z;&c_n;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;</trd><fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_v;&c_ja;&c_d;&c_o;&c_m;&c_y;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_t;&c_o;&c_m;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>bewusst</trd>
  </trdgrp>
  <trd lng="fr">conscient</trd>
  <trdgrp lng="hu">
    <trd>tudatos</trd>,
    <trd>&ouml;ntudatos</trd>
  </trdgrp>
  <trd lng="nl">bewust</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_z;&c_n;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_o;&c_z;&c_n;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="konsci.sen0igxi">
  <kap>sen<tld/>i&gcirc;i</kap>
  <snc mrk="konsci.sen0igxi.PSI">
    <dif>
      Perdi la <tld/>on; <ref tip="sin" cel="sven.0i">sveni</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_mol;&c_n;&c_je;&c_p;&c_r;&c_y;&c_t;&c_o;&c_m;&c_n;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_l;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>bewusstlos werden</trd>,
    <trd>besinnungslos werden</trd>,
    <trd>das <ind>Bewusstsein</ind> verlieren</trd>,
    <trd>die <ind>Besinnung</ind> verlieren</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>perdre conscience</trd>,
    <trd>perdre connaissance</trd>,
    <trd>s'<ind>&eacute;vanouir</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>elvesz&iacute;ti az <ind>&ouml;ntudat&aacute;t</ind></trd>,
    <trd>&ouml;ntudatlann&aacute; v&aacute;lik</trd>
  </trdgrp>
  <trd lng="nl">bewusteloos worden</trd>
  <trd lng="ru">&c_p;&c_o;&c_t;&c_je;&c_r;&c_ja;&c_t;&c_mol; &c_s;&c_o;&c_z;&c_n;&c_a;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="konsci.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="konsci.sen0a.PSI">
    <dif>
      Karakterizata per manko de <tld/>o:
      <ekz>
        sen<tld/>a frenezulo;
      </ekz>
      <ekz>
        por peko sen<tld/>a puno nenia<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        sen<tld/>a imitado de la ekzistantaj lingvoj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_p;&c_r;&c_y;&c_t;&c_o;&c_m;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_mol;&c_v;&c_ja;&c_d;&c_o;&c_m;&c_y;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_mol;&c_s;&c_mol;&c_v;&c_ja;&c_d;&c_o;&c_m;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>bewusstlos</trd>,
    <trd>besinnungslos</trd>,
    <trd>ohne <ind>Bewusstsein</ind></trd>,
    <trd>ohne <ind>Besinnung</ind></trd>
  </trdgrp>
  <trd lng="fr">inconscient<klr> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>&ouml;ntudatlan</trd>,
    <trd>tudat alatti</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bewusteloos</trd>,
    <trd>onbewust</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_s;&c_s;&c_o;&c_z;&c_n;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="konsci.sub0o">
  <kap>sub<tld/>o</kap>
  <snc mrk="konsci.sub0o.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      Parto de la menso, kiu ne estas <tld/>a:
      <ekz>
        dum &scirc;ia dormo &scirc;ia sub<tld/>o bone de&jcirc;oris por filtri kaj enlasi 
        nur tiujn bruojn, kiuj rilatas rekte al &scirc;ia profesio
        <fnt><bib>KielAkvo</bib>, <lok>1:1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">subconscient<klr> (subst.)</klr></trd>
</drv>

<drv mrk="konsci.re0igxi">
  <kap>re<tld/>i&gcirc;i<fnt>Z</fnt></kap>
  <snc mrk="konsci.re0igxi.PSI">
    <dif>
      Reakiri la <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_p;&c_r;&c_y;&c_t;&c_o;&c_m;&c_n;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_ch;&c_u;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wieder zu sich kommen</trd>,
    <trd>wieder zu <ind>Bewusstsein</ind> kommen</trd>,
    <trd>wieder zu <ind>Besinnung</ind> kommen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>reprendre connaissance</trd>,
    <trd>revenir &agrave; soi</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>felocs&uacute;dik</trd>,
    <trd>visszanyeri az <ind>&ouml;ntudat&aacute;t</ind></trd>
  </trdgrp>
  <trd lng="nl">tot <ind>bewustzijn</ind> komen</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_ch;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_j;&c_t;&c_i; &c_v; &c_s;&c_je;&c_b;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
