<kap>
  <ofc>2</ofc>
  <rad>mimoz</rad>/o
</kap>
<drv mrk="mimoz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="mimoza.0oj"><tld/>acoj</ref>
      (<trd lng="la">Mimosa</trd>), unu specio (<trd lng="la">Mimosa
      pudica</trd>) kunvolvas la folietojn &ccirc;e tu&scirc;o.
    </dif>
    <trd lng="en">sensitive plant</trd>
  </snc>
  <trd lng="de">Mimose</trd>
  <trd lng="en">mimosa</trd>
  <trd lng="nl">mimosa</trd>
  <trd lng="ru">&c_m;&c_i;&c_m;&c_o;&c_z;&c_a;</trd>
</drv>
</art>
