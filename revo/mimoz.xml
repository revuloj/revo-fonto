<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>mimoz</rad>/o
</kap>
<drv mrk="mimoz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="mimoza.0oj"><tld/>acoj</ref>
      (<trd lng="la">Mimosa</trd>), unu specio (<trd lng="la">Mimosa
      pudica</trd>) kunvolvas la folietojn &ccirc;e tu&scirc;o.
    </dif>
  </snc>
  <trd lng="de">Mimose</trd>
</drv>

</art>
