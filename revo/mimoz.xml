<kap>
  <ofc>2</ofc>
  <rad>mimoz</rad>/o
</kap>
<drv mrk="mimoz.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref>
      el familio <ref tip="malprt" cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Mimosa</trd>), unu specio (<trd lng="la">Mimosa
      pudica</trd>) kunvolvas la folietojn &ccirc;e tu&scirc;o:
      <ekz>
        &ccirc;efaj produktoj de la urbeto estas <tld/>o-arbetoj kaj balailoj
        <fnt>
       <aut>Thomas Eccardt</aut>
          <vrk><url ref="&Monato;/2000/006859.php">Urbeto a&ubreve; sendependa lando?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/05, p. 8</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/6/6e/Mimosa_pudica_2.jpg/320px-Mimosa_pudica_2.jpg" prm="CC BY-SA     4.0">floranta bran&ccirc;o de Sentema <tld/>o (Mimosa pudica)
        <fnt>
          <aut>Eric Hunt, propra verko</aut>
          <lok><url ref="&WCW;/File:Mimosa_pudica_2.jpg">&WCW;/File:Mimosa_pudica_2.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_m;&c_o;&c_z;&c_a;</trd>
  <trd lng="ca">sensitiva</trd><trd lng="ca">mimosa</trd>
  <trd lng="cs">mim&oacute;za</trd>
  <trd lng="de">Mimose</trd>
  <trd lng="en">sensitive plant</trd><trd lng="en">mimosa</trd>
  <trd lng="fr">mimosa</trd>
  <trdgrp lng="hu">
    <trd>mim&oacute;za</trd>,
    <trd>&eacute;rz&odblac;ke</trd></trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30DF;&#x30E2;&#x30B6;</trd>,
    <trd>&#x30AA;&#x30B8;&#x30AE;&#x30BD;&#x30A6;</trd>
  </trdgrp>
  <trd lng="nl">mimosa</trd>
  <trd lng="pt">mimosa</trd>
  <trd lng="ru">&c_m;&c_i;&c_m;&c_o;&c_z;&c_a;</trd>
  <trd lng="sk">mim&oacute;za</trd>
  <trd lng="uk">&#x43C;&c_ib;&#x43C;&#x43E;&c_z;&c_a;</trd>
  <trd lng="zh">&#x542B;&#x7F9E;&#x8349; <pr>h&#xE1;nxi&#x16B;c&#x1CE;o</pr></trd>
</drv>
</art>
