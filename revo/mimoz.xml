<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>mimoz</rad>/o
</kap>
<drv mrk="mimoz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="mimoz.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Mimosa</trd>), unu specio (<trd lng="la">Mimosa           
      pudica</trd>) kunvolvas la folietojn &ccirc;e tu&scirc;o.
    </dif>
  </snc>
  <trd lng="de">Mimose</trd>
</drv>
<drv mrk="mimoz.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Mimosaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Mimosengew&auml;chse</trd>
</drv>
</art>
