<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>milion</rad>/o
</kap>
<drv mrk="milion.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Mil miloj. Matematika simbolo 1000000:
      <ekz>tiu &ccirc;i urbo havas <tld/>n da lo&gcirc;antoj
      <fnt><bib>F</bib>, <lok>&FE; 14</lok></fnt>;
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_l;&c_mol;&c_jo;&c_n;</trd>
  <trd lng="de">Million</trd>
  <trd lng="en">million</trd>
  <trd lng="es">mill&oacute;n</trd>
  <trd lng="fr">million</trd>
  <trd lng="hu">milli&oacute;</trd>
  <trd lng="nl">miljoen</trd>
  <trd lng="ru">&c_m;&c_i;&c_l;&c_l;&c_i;&c_o;&c_n;</trd>
  <trd lng="tr">milyon</trd>
  <trd lng="vo">balion</trd>
</drv>

<drv mrk="milion.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Ri&ccirc;ulo posedanta almena&ubreve; unu <tld/>on da valuto
      (da e&ubreve;roj, da dolaroj...):
      <ekz>
        &scirc;i versperman&gcirc;is kun amanto <tld/>ula kelkajn horojn anta&ubreve; sia
        morto
        <fnt>
          <aut>Paul Gubbins</aut>:
          <vrk>Diana... &ccirc;u diino?</vrk>,
          <bib>Monato</bib><lok>jaro 1997a, numero 9a, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_l;&c_mol;&c_ja;&c_n;&c_e;&c_r;</trd>
  <trd lng="de">Million&auml;r</trd>
  <trd lng="en">millionaire</trd>
  <trd lng="es">millonario</trd>
  <trd lng="fr">millionnaire</trd>
  <trd lng="hu">milliomos</trd>
  <trd lng="nl">miljonair</trd>
  <trd lng="ru">&c_m;&c_i;&c_l;&c_l;&c_i;&c_o;&c_n;&c_je;&c_r;</trd>
  <trd lng="tr">milyoner</trd>
</drv>
</art>
