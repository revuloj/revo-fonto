<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: milion.xml,v 1.32 2021/05/30 08:26:08 revo Exp $">
<kap>
  <ofc>*</ofc><ofc>2</ofc><ofc>7</ofc>
  <rad>milion</rad>/o
</kap>
<drv mrk="milion.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Mil miloj, matematika simbolo 1000000:
      <ekz>tiu &ccirc;i urbo havas <tld/>on da lo&gcirc;antoj
      <fnt><bib>F</bib>, <lok>&FE; 14</lok></fnt>;
      </ekz>
    </dif>
     <ref tip="lst" cel="nombr.0o.MAT" lst="voko:nombroj">nombro</ref>
  </snc>
  <trd lng="af">miljoen</trd>
  <trd lng="be">&c_m;&c_ib;&c_l;&c_mol;&c_jo;&c_n;</trd>
  <trd lng="bo">&#xF66;&#xF0B;&#xF61;</trd>
  <trd lng="br">milion</trd>
  <trd lng="ca">mili&oacute;</trd>
  <trd lng="cs">mili&oacute;n</trd>
  <trd lng="de">Million</trd>
  <trd lng="en">million</trd>
  <trd lng="es">mill&oacute;n</trd>
  <trd lng="fa">&a_m;&f_y;&a_l;&f_y;&a_w;&a_n;</trd>
  <trd lng="fr">million</trd>
  <trd lng="fy">miljoen</trd>
  <trd lng="hu">milli&oacute;</trd>
  <trd lng="id">juta</trd>
  <trd lng="io">miliono</trd>
  <trd lng="ku">milyon</trd>
  <trd lng="ne">&#x932;&#x93E;&#x916;</trd>
  <trd lng="nl">miljoen</trd>
  <trd lng="no">million</trd>
  <trd lng="pl">milion</trd>
  <trd lng="pt">milh&atilde;o <klr>(1.000.000)</klr></trd>
  <trd lng="ru">&c_m;&c_i;&c_l;&c_l;&c_i;&c_o;&c_n;</trd>
  <trd lng="sk">mili&oacute;n</trd>
  <trd lng="sq">milion</trd>
  <trd lng="tr">milyon</trd>
  <trd lng="vo">balion</trd>
</drv>

<drv mrk="milion.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Ri&ccirc;ulo posedanta almena&ubreve; unu <tld/>on da valuto
      (da e&ubreve;roj, da dolaroj...):
      <ekz>
        &scirc;i versperman&gcirc;is kun amanto <tld/>ula kelkajn horojn
        anta&ubreve;
        sia
        morto
        <fnt>
          <aut>Paul Gubbins</aut>:
          <vrk>Diana... &ccirc;u diino?</vrk>,
          <bib>Monato</bib><lok>jaro 1997a, numero 9a, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_l;&c_mol;&c_ja;&c_n;&c_e;&c_r;</trd>
  <trd lng="br">milioner</trd>
  <trd lng="ca">milionari</trd>
  <trd lng="cs">milion&aacute;&rcaron;</trd>
  <trd lng="de">Million&auml;r</trd>
  <trd lng="en">millionaire</trd>
  <trd lng="es">millonario</trd>
  <trd lng="fa">&a_m;&f_y;&a_l;&f_y;&a_w;&a_n;&a_r;</trd>
  <trd lng="fr">millionnaire</trd>
  <trd lng="fy">miljon&ecirc;r</trd>
  <trd lng="hu">milliomos</trd>
  <trdgrp lng="id">
    <trd>miliarder</trd>,
    <trd>miliuner</trd>
  </trdgrp>
  <trd lng="nl">miljonair</trd>
  <trd lng="no">million&aelig;r</trd>
  <trd lng="pl">milioner</trd>
  <trd lng="ru">&c_m;&c_i;&c_l;&c_l;&c_i;&c_o;&c_n;&c_je;&c_r;</trd>
  <trd lng="sk">milion&aacute;r</trd>
  <trd lng="tr">milyoner</trd>
  <trd lng="vo">balionan</trd>
</drv>
</art>
