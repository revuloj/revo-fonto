<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>milion</rad>/o
</kap>
<drv mrk="milion.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Mil miloj.
    </dif>
  </snc>
  <trd lng="es">mill&oacute;n</trd>
  <trd lng="de">Million</trd>
  <trd lng="nl">miljoen</trd>
  <trd lng="ru">&c_m;&c_i;&c_l;&c_l;&c_i;&c_o;&c_n;</trd>
  <trd lng="tr">milyon</trd>
</drv>

<drv mrk="milion.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Ri&ccirc;ulo posedanta almena&ubreve; unu <tld/>on da frankoj, funtoj,
      markoj, dolaroj ks.
    </dif>
  </snc>
  <trd lng="es">millonario</trd>
  <trd lng="de">Million&auml;r</trd>
  <trd lng="nl">miljonair</trd>
  <trd lng="ru">&c_m;&c_i;&c_l;&c_l;&c_i;&c_o;&c_n;&c_je;&c_r;</trd>
  <trd lng="tr">milyoner</trd>
</drv>
</art>
