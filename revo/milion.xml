        tiu &ccirc;i urbo havas <tld/>on da lo&gcirc;antoj
        <fnt><bib>F</bib>, <lok>&FE; 14</lok></fnt>;
      </ekz>
      <ekz>
        duono da <tld/>o
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj da stelformaj flokoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>funta pezo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> faris tion por la <tld/>a fojo
        <fnt>
          <bib>IdO</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj <tld/>oj konsistas el milonoj
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
