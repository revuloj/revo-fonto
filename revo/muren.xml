<art mrk="$Id: muren.xml,v 1.25 2024/01/26 19:05:54 revo Exp $">
<kap>
  <rad>muren</rad>/o
</kap>
<drv mrk="muren.0o">
  <kap><tld/>o</kap>
   <snc>
   <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda, angiloforma, senskvama, venena
      <ref tip="lst" cel="fisx.mar0o" lst="voko:fi&scirc;oj">marfi&scirc;o</ref>;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio <ref tip="malprt" cel="murene.0oj"><tld/>edoj</ref>
      (<trd lng="la">Muraena</trd>):
      <ekz>
        <tld/>oj <klr>[en]</klr>lo&gcirc;as rokajn kava&jcirc;ojn kaj gvate &ccirc;asas aliajn bestojn per
        tre fajna flarsenso, ili ne havas skvamojn kaj la ha&ubreve;to <klr>[&#x2026;]</klr> eligas
        protektan mukon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=131721">Mureno</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/a/a5/Muraena_helena_-_Linnaeus%2C_1758.jpg/320px-Muraena_helena_-_Linnaeus%2C_1758.jpg" prm="CC BY-SA 3.0"><tld/>o elrigardas sian kaverneton
        <fnt>
          <aut>Waielbi, propra verko</aut>
          <lok><url ref="&WCW;/File:Muraena_helena_-_Linnaeus,_1758.jpg">&WCW;/File:Muraena_helena_-_Linnaeus,_1758.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_r;&c_e;&c_n;&c_a;</trd>
  <trd lng="cs">mur&eacute;na</trd>
  <trd lng="de">Mur&auml;ne</trd>
  <trd lng="es">morena</trd>
  <trd lng="fr">mur&egrave;ne</trd>
  <trd lng="hu">mur&eacute;na</trd>
  <trdgrp lng="ja">
    <trd>&#x30C8;&#x30E9;&#x30A6;&#x30C4;&#x30DC;</trd>,
    <trd>&#x30A6;&#x30C4;&#x30DC;</trd>
  </trdgrp>
  <trd lng="nl">murena</trd>
  <trd lng="pl">murena</trd>
  <trd lng="pt">mor&eacute;ia</trd>
  <trd lng="ru">&c_m;&c_u;&c_r;&c_je;&c_n;&c_a;</trd>
  <trd lng="sk">mur&eacute;na</trd>
  <trd lng="uk">&c_m;&c_u;&c_r;&c_je;&c_n;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x6D77;&#x9C54; <pr>h&#x1CE;ish&#xE0;n</pr></trd>,
    <trd>&#x6D77;&#x9CDD; <pr>h&#x1CE;ish&#xE0;n</pr></trd>
  </trdgrp>
</drv>
</art>
