<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: safran.xml,v 1.28 2024/03/24 19:59:58 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>safran</rad>/o
</kap>

<drv mrk="safran.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="safran.0o.floro">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Specio de <ref tip="super" cel="krokus.0o">krokuso</ref>
      (<trd lng="la">Crocus sativus</trd>):
      <ekz>
        ili dancas kun hiacintoj kaj <tld/>oj
        <fnt><bib>Fab1</bib>, <lok>Floroj de la malgranda
        Ida</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <snc mrk="safran.0o.spico">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="spic.0o">Spico</ref> ekstraktata el
      la floroj de <ref tip="malprt" cel="safran.0o.floro"><tld/>o<sncref/></ref>.
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_a;&c_f;&c_r;&c_a;&c_n;</trd>
  <trd lng="bg">&c_sh;&c_a;&c_f;&c_r;&c_a;&c_n;</trd>
  <trd lng="ca">safr&agrave;</trd>
  <trd lng="cs">&#x161;afr&#xE1;n set&#xFD;</trd>
  <trd lng="de">Safran</trd>
  <trd lng="es">azafr&aacute;n</trd>
  <trd lng="fr">safran</trd>
  <trdgrp lng="he">
    <trd>&zayin;&ayin;&pe;&resh;&fnun;</trd>,
    <trd>&kaf;&resh;&kaf;&vav;&fmem;</trd>
  </trdgrp>
  <trd lng="hu">s&aacute;fr&aacute;ny</trd>
  <trd lng="it">zafferano</trd>
  <trdgrp lng="ja">
    <trd>&#x30B5;&#x30D5;&#x30E9;&#x30F3;</trd>,
    <trd>&#x30B5;&#x30D5;&#x30E9;&#x30F3;&#x7C89;  <pr>&#x30B5;&#x30D5;&#x30E9;&#x30F3;&#x3053;&#x306A;</pr></trd>
  </trdgrp>
  <trd lng="nl">saffraan</trd>
  <trd lng="pl">szafran</trd>
  <trdgrp lng="pt">
   <trd>a&ccedil;afr&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_a;&c_f;&c_r;&c_a;&c_n;</trd>
  <trd lng="sk">&#x161;afran siaty</trd>
  <trd lng="uk">&#x448;&#x430;&#x444;&#x440;&#x430;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x8543;&#x7D05;&#x82B1; <pr>f&#xE1;nh&#xF3;nghu&#x101;</pr></trd>,
    <trd>&#x8543;&#x7EA2;&#x82B1; <pr>f&#xE1;nh&#xF3;nghu&#x101;</pr></trd>
  </trdgrp>
</drv>
</art>
