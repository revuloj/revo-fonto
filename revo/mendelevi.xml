<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mendelevi.xml,v 1.22 2022/03/23 14:01:45 revo Exp $">
<kap>
  <rad>mendelevi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="mendelevi.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Md</mlg>
  <snc mrk="mendelevi.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="101">elemento</ref>,
      <ref tip="super" cel="urani.trans0o">transuranio</ref> kun
      atomnumero 101, atompezo 258,0986;
      kies &ccirc;iuj <ref tip="super" cel="izotop.0o">izotopoj</ref> 
      estas radioaktivaj; aktinoido:
      <ekz>
        <tld/>o estas transurania radioaktivega metala elemento, 
        kiun oni povas fari bombardante ejn&scirc;tejnion per alfaradioj,
        &gcirc;i estis nomita oma&gcirc;e al la kemiisto <nom>Dmitrij Mendelejev</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=28876">Mendelevio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_n;&c_mol;&c_d;&c_z;&c_ja;&c_l;&c_je;&c_v;&c_ib;&c_j;</trd>
  <trd lng="bg">&c_m;&c_je;&c_n;&c_d;&c_je;&c_l;&c_je;&c_v;&c_i;&c_j;</trd>
  <trd lng="br">mendeleviom</trd>
  <trd lng="ca">Mendelevi</trd>
  <trd lng="cs">mend&ecaron;levium</trd>
  <trd lng="de">Mendelevium</trd>
  <trd lng="en">mendelevium</trd>
  <trd lng="es">mendelevio</trd>
  <trd lng="fr">mend&eacute;l&eacute;vium</trd>
  <trd lng="hu">mendel&eacute;vium</trd>
  <trd lng="id">mendelevium</trd>
  <trd lng="it">mendelevio</trd>
  <trd lng="la">Mendelevium</trd>
  <trd lng="nl">mendelevium</trd>
  <trd lng="pl">Mendelew</trd>
  <trd lng="pt">mendel&eacute;vio</trd>
  <trd lng="ru">&c_m;&c_je;&c_n;&c_d;&c_je;&c_l;&c_je;&c_v;&c_i;&c_j;</trd>
  <trd lng="sk">mendelevium</trd>
  <trd lng="sv">mendelevium</trd>
  <trd lng="uk">&#x43C;&#x435;&#x43D;&#x434;&#x435;&#x43B;&#x435;&#x432;&#x456;&#x439;</trd>
</drv>
</art>
