<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>kariofil</rad>/o
</kap>
<drv mrk="kariof.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Najloforma sekigita florbur&gcirc;ono de <tld/>ujo, 
      uzata kiel spica&jcirc;o.
    </dif>
  </snc>
  <trd lng="de">Nelke</trd>
  <trd lng="fr">clou de girofle</trd>
</drv>
<drv mrk="kariof.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de herboj (<trd lng="la">Caryophyllaceae</trd>):
      <refgrp tip="sub">
        <ref cel="stelari.0o">stelario</ref>,
        <ref cel="diant.0o">dianto</ref>
      </refgrp>, sed ne <tld/>ujo.
    </dif>
  </snc>
  <trd lng="de">Nelkengew&auml;chse</trd>
</drv>
<drv mrk="kariof.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <dif>
      Arbo el familio <ref tip="super" cel="mirt.0acoj">mirtacoj</ref>
      (<trd lng="la">Syzygium aromaticum</trd>).
    </dif>
  </snc>
  <trd lng="de">Gew&uuml;rznelkenbaum</trd>
</drv>
</art>
