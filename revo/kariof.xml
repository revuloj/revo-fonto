<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>kariofil</rad>/o
</kap>
<drv mrk="kariof.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Najloforma sekigita florbur&gcirc;ono de
      <ref tip="malprt" cel="kariof.0ujo"><tld/>ujo</ref>,
      uzata kiel spica&jcirc;o.
    </dif>
    <trd lng="en">clove</trd>
  </snc>
  <trd lng="de">Nelke</trd>
  <trd lng="fr">clou de girofle</trd>
  <trd lng="nl">kruidnagel</trd>
  <trd lng="pl">go&zacute;dzik</trd>
</drv>

<drv mrk="kariof.0ujo">
  <kap><tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Arbo el familio <ref tip="super" cel="mirtac.0oj">mirtacoj</ref>
      (<trd lng="la">Syzygium aromaticum</trd>).
    </dif>
  </snc>
  <trd lng="de">Gew&uuml;rznelkenbaum</trd>
  <trdgrp lng="en">
      <trd>clove</trd>,
      <trd>clove tree</trd>
    </trdgrp>
  <trd lng="nl">kruidnagelboom</trd>
</drv>
</art>
