<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kariof.xml,v 1.30 2023/12/03 08:00:23 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kariofil</rad>/o
</kap>
<drv mrk="kariof.0o">
  <kap><tld/>o</kap>
  <snc mrk="kariof.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Najloforma sekigita florbur&gcirc;ono de
      <ref tip="malprt" cel="kariof.0ujo"><tld/>ujo</ref>,
      uzata kiel <ref tip="lst" cel="spic.0o" lst="voko:spicoj">spico</ref>:
      <ekz>
        dispremu l'ajlon kaj la <tld/>ojn
        <fnt>
          <aut>rubulo</aut>,
          <vrk><url ref="http://www.rubulo.net/">
          Fotostrioj en Esperanto</url></vrk>,
          <lok>2006-11-26</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_v;&c_a;&c_z;&c_mol;&c_d;&c_z;&c_ib;&c_k;&c_a; <klr>(&c_p;&c_r;&c_y;&c_p;&c_r;&c_a;&c_v;&c_a;)</klr></trd>
  <trd lng="br">tach-jenofl</trd>
  <trdgrp lng="ca">
    <trd>clavell</trd>,
    <trd>clau d'esp&egrave;cia</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hvozd&#xED;k</trd>,
    <trd>kartouzek</trd>
  </trdgrp>
  <trd lng="de">Nelke</trd>
  <trd lng="en">clove</trd>
  <trd lng="es">clavo<klr> (especia)</klr></trd>
  <trd lng="fr">clou de girofle</trd>
  <trd lng="hu">szegf&udblac;szeg</trd>
  <trdgrp lng="ja">
    <trd>&#x30C1;&#x30E7;&#x30A6;&#x30B8;</trd>,
    <trd>&#x30AF;&#x30ED;&#x30FC;&#x30D6;</trd>
  </trdgrp>
  <trd lng="nl">kruidnagel</trd>
  <trd lng="pl">go&zacute;dzik</trd>
  <trd lng="ru">&c_g;&c_v;&c_o;&c_z;&c_d;&c_i;&c_k;&c_a;
  <klr>(&c_p;&c_r;&c_ja;&c_n;&c_o;&c_s;&c_t;&c_mol;)</klr></trd>
  <trd lng="sk">klin&#x10D;eky <klr>(korenie)</klr></trd>
  <trd lng="tr">karanfil tanesi</trd>
  <trd lng="uk">&c_g;&c_v;&#x43E;&c_z;&c_d;&c_i;&#x43A;&c_a; (&#x43F;&c_r;&#x44F;&#x43D;&c_ib;&c_s;&c_t;&#x44C;)</trd>
  <trd lng="zh">&#x5EB7;&#x4E43;&#x99A8; <pr>k&#x101;ngn&#x1CE;ix&#x12B;n</pr></trd>
</drv>

<drv mrk="kariof.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="kariof.0ujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbo, <ref tip="lst" cel="speci.0o.BIO" lst="voko:botanikaj_specioj">specio</ref> el familio
      <ref tip="malprt" cel="mirtac.0oj">mirtacoj</ref>
      (<trd lng="la">Syzygium aromaticum</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_v;&c_a;&c_z;&c_mol;&c_d;&c_z;&c_ib;&c_ch;&c_n;&c_a;&c_je; <ind>&c_d;&c_r;&c_e;&c_v;&c_a;</ind></trd>
  <trdgrp lng="br">
    <trd>gwezenn-jenofl</trd>,
    <trd>jenoflenn</trd>
  </trdgrp>
  <trd lng="ca">claveller</trd>
  <trd lng="de">Gew&uuml;rznelkenbaum</trd>
  <trdgrp lng="en">
    <trd>clove</trd>,
    <trd>clove tree</trd>
  </trdgrp>
  <trd lng="fr">giroflier</trd>
  <trd lng="hu">szegf&udblac;szegfa</trd>
  <trd lng="nl">kruidnagelboom</trd>
  <trd lng="ru">&c_g;&c_v;&c_o;&c_z;&c_d;&c_i;&c_ch;&c_n;&c_o;&c_je;
  &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  <trd lng="zh">&#x4E01;&#x9999;&#x82B1; <pr>d&#x12B;ngxi&#x101;nghu&#x101;</pr></trd>
</drv>
</art>
