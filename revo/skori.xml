<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>skori</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="skori.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Speco de vitreca materio, kiu per fandado aparti&gcirc;as de metalmina&jcirc;oj
      = metal&scirc;a&ubreve;mo.
    </dif>
    <trd lng="nl">metaalschuim</trd>
    <trdgrp lng="ru">
      <trd>&c_sh;&c_l;&c_a;&c_k;</trd>,
      <trd>&c_o;&c_k;&c_a;&c_l;&c_i;&c_n;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      For&jcirc;etinda&jcirc;o, senvalora&jcirc;o:
      <ekz>
        via ar&gcirc;ento fari&gcirc;is <tld/>o<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="bala">forbalaa&jcirc;o</ref>,
      <ref cel="vent">ventuma&jcirc;o</ref>,
      <ref cel="rekrem.0o">rekremento</ref>,
      <ref cel="fecx.0o">fe&ccirc;o</ref>.
    </refgrp>
    <trdgrp lng="ru">
      <trd>&c_sh;&c_l;&c_a;&c_k;</trd>,
      <trd>&c_o;&c_t;&c_b;&c_r;&c_o;&c_s;&c_y;</trd>
    </trdgrp>
  </snc>
  <trd lng="de">Schlacke</trd>
</drv>
</art>
