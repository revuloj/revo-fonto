    <trdgrp lng="be">
      <trd>&c_sh;&c_l;&c_a;&c_k;</trd>, 
      <trd>&c_a;&c_k;&c_a;&c_l;&c_ib;&c_n;&c_a;</trd>
    </trdgrp>
    <trd lng="bg">&c_sh;&c_l;&c_a;&c_k;&c_a;</trd> 
    <trdgrp lng="cs">
      <trd>p&rcaron;en. brak</trd>,
      <trd>struska</trd>,
      <trd>&scaron;kv&aacute;ra</trd>
    </trdgrp>
    <trd lng="de">Schlacke</trd>
    <trdgrp lng="en">
      <trd>cinder</trd>,
      <trd>cinders</trd>,
      <trd>clinker</trd>,
      <trd>dross</trd>,
      <trd>scalia</trd>,
      <trd>slag</trd>
