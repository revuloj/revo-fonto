<art mrk="$Id: skori.xml,v 1.20 2022/03/27 17:06:35 revo Exp $">
<kap>
  <rad>skori</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="skori.0o">
  <kap><tld/>o</kap>
  <snc mrk="skori.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref cel="vitr.0o.substanco">Vitr</ref>eca materio, 
      kiu per fandado aparti&gcirc;as de  metalmina&jcirc;oj:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        via ar&gcirc;ento fari&gcirc;is <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 1:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        dikaj tavoloj de malnova, altforna skorio
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>;
      </ekz>
      <ekz>
        dum la fabrikado de &scirc;talo el gisfero, formi&gcirc;as,
        kiel rezulto de puriga procezo, <tld/>o, kiu
        konsistas &ccirc;efe el kompleksaj kombina&jcirc;oj de kalcio
        kun fosforo
        <fnt>
          <aut>W. F. Rolt</aut>,
          <vrk>El la Polvo de la Tero</vrk>,
          <lok>1967</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="skori.0o.cindra">
    <dif>
      Porohava, krusta <ref tip="super" cel="cindr.0o.KOMUNE">cindro</ref> kunfandi&gcirc;inta pro alta 
      temperaturo; unuopa peco de tiu cindra&jcirc;o:
      <ekz>
        la hejtisto rompis per peza longa ferstango la <tld/>ojn de sur la kradoj, la
        <tld/>oj sidis firme
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="sin" cel="sxlak.0o.TEK">&scirc;lako</ref>
