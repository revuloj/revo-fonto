<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>pa&ubreve;lovni</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="pauxlovni.0o">
  <kap><tld/>o</kap>
  <snc mrk="pauxlovni.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro de arboj el la familio <ref tip="super"
      cel="skrofu1.0oj">skrofulariacoj</ref>,
      el kiu unu japandevena specio (<trd lng="la">Paulownia
      tomentosa</trd>)
      proksimume 10 metrojn alta, kun lar&gcirc;aj folioj kaj printempaj
      flor-panikloj, ornamas &gcirc;ardenojn
      kaj provizas malpezan lignon ta&ubreve;gan por mebloj kaj aliaj a&jcirc;oj:
      <ekz>
        ligna bovlo estas farita el arboradiko a&ubreve; granda tubero de <tld/>o
        <fnt>
        <aut>&Ccirc;ina Radio Internacia</aut>,
        <vrk><url
        ref="http://es.chinabroadcast.cn/1/2005/01/24/1@20229.htm">Popolaj
        metia&jcirc;oj de Tibeto (2)</url></vrk>,
        <lok>2005-01-24</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Paulownie</trd>
  <trdgrp lng="fr">
    <trd>paulownia</trd>
  </trdgrp>
  <trd lng="hu">cs&aacute;sz&aacute;rfa</trd>
  <trd lng="pt">paul&ocirc;nia-imperial</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_d;&c_a;&c_m;&c_o;&c_v;&c_o; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_l;&c_o;&c_v;&c_n;&c_i;&c_ja; &c_v;&c_o;&c_j;&c_l;&c_o;&c_ch;&c_n;&c_a;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
