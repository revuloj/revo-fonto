<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>pa&ubreve;lovni</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="pauxlovni.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro de arboj el la familio <ref tip="super"
      cel="skrofu1.0oj">skrofulariacoj</ref>,
      el kiu unu japandevena specio (<trd lng="la">Paulownia
      tomentosa</trd>)
      proksimume 10 m alta, kun lar&gcirc;aj folioj kaj printempaj
      flor-panikloj 5 &gcirc;is 8 cm longaj, uzatas en plezur-&gcirc;ardenoj,
      kaj kies malpeza ligno ta&ubreve;gas por mebloj, muzikiloj kaj 
      aliaj a&jcirc;oj.
    </dif>
  </snc>
  <trd lng="de">Paulownie</trd>
  <trdgrp lng="fr">
    <trd>paulownia</trd>
  </trdgrp>
  <trd lng="hu">cs&aacute;sz&aacute;rfa</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_d;&c_a;&c_m;&c_o;&c_v;&c_o; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_l;&c_o;&c_v;&c_n;&c_i;&c_ja; &c_v;&c_o;&c_j;&c_l;&c_o;&c_ch;&c_n;&c_a;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
