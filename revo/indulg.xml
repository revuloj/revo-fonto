<kap>
  <ofc>*</ofc>
  <rad>indulg</rad>/i
</kap>
<drv mrk="indulg.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Malsevere trakti ies kulpon a&ubreve; pekon, esti pardonema kontra&ubreve;
      iu erarinta:
      <ekz>
        <tld/>i pekintan filon;
      </ekz>
      <ekz>
        <tld/>i al, kontra&ubreve; iu;
      </ekz>
      <ekz>
        <tld/>i kulpon, pri kulpo;
      </ekz>
      <ekz>
        <tld/>u lin pro lia pento;
      </ekz>
      <ekz>
        forta kolero devas esti punata, &ccirc;ar se vi &gcirc;in <tld/>os,
        &gcirc;i fari&gcirc;os ankora&ubreve; pli granda<fnt>Z</fnt>;
      </ekz>
      <ekz>
        esti <tld/>ata<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kompat.0i">kompati</ref>,
      <ref cel="pardon.0i">pardoni</ref>.
    </refgrp>
    <trd lng="be">&c_p;&c_a;&c_b;&c_l;&c_a;&c_zh;&c_l;&c_ib;&c_v;&c_a;
&c_s;&c_t;&c_a;&c_v;&c_ib;&c_c;&c_c;&c_a;</trd>
    <trdgrp lng="hu">
      <trd>eln&eacute;z<klr tip="amb"> (megbocs&aacute;t)</klr></trd>,
      <trd>irgalmaz</trd>,
      <trd>megkegyelmez</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>pob&lstroke;a&zdot;a&cacute;</trd>,
      <trd>by&cacute; <ind>pob&lstroke;a&zdot;liwym</ind></trd>,
      <trd>by&cacute; <ind>wyrozumia&lstroke;ym</ind></trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_t;&c_n;&c_o;&c_s;&c_i;&c_t;&c_mol;&c_s;&c_ja;
&c_s;&c_n;&c_i;&c_s;&c_h;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;</trd>
  </snc>
  <snc>
    <dif>
      Milde agi kontra&ubreve; iu, ne ofendi, montri konsideron al,
      doma&gcirc;i:
      <ekz>
        oni devas <tld/>i la orelojn de la infanoj<fnt>B</fnt>;
      </ekz>
      <ekz>
        la kalumnio <tld/>os eble vian puran nomon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li ne <tld/>as vian rangon nek oficon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i venkitan malamikon;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la krizo <tld/>is nian landon.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_sh;&c_k;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_sh;&c_k;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_d;&c_a;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="hu">megk&iacute;m&eacute;l</trd>
    <trdgrp lng="ru">
      <trd>&c_shch;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_o;&c_shch;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>oszcz&eogonek;dza&cacute; <klr>(kogo&sacute;)</klr></trd>,
      <trd>przebacza&cacute;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>b&yacute;t shov&iacute;vav&yacute;</trd>,
    <trd>poshov&ecaron;t</trd>,
    <trd>sn&aacute;&scaron;et</trd>,
    <trd>&scaron;et&rcaron;it <klr>(n&ecaron;koho)</klr></trd>
  </trdgrp>
  <trd lng="de">nachsichtig sein</trd>
  <trd lng="en">be lenient</trd>
  <trdgrp lng="fr">
    <trd>&eacute;pargner<klr tip="ind"> (m&eacute;nager)</klr></trd>,
    <trd>m&eacute;nager<klr tip="ind"> (&eacute;pargner)</klr></trd>,
    <trd>se montrer <ind>cl&eacute;ment</ind> (pour)</trd>,
    <trd>se montrer <ind>compr&eacute;hensif</ind> (pour)</trd>,
    <trd>se montrer <ind>indulgent</ind> (pour)</trd>,
    <trd>avoir de l'<ind>indulgence</ind> (pour)</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5BB9;&#x8D66;&#x3059;&#x308B;  [<ind>&#x3088;&#x3046;&#x3057;&#x3083;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x5927;&#x76EE;&#x306B;&#x898B;&#x308B;  [<ind>&#x304A;&#x304A;&#x3081;&#x306B;&#x307F;&#x308B;</ind>]</trd>,
    <trd>&#x5BDB;&#x5927;&#x306B;&#x6271;&#x3046;  [<ind>&#x304B;&#x3093;&#x3060;&#x3044;&#x306B;&#x3042;&#x3064;&#x304B;&#x3046;</ind>]</trd>,
    <trd>&#x3044;&#x305F;&#x308F;&#x308B;</trd>,
    <trd>&#x52A9;&#x547D;&#x3059;&#x308B;  [<ind>&#x3058;&#x3087;&#x3081;&#x3044;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">ontzien</trd>
  <trd lng="pt">ser <ind>indulgente</ind> para</trd>
  <trdgrp lng="sk">
    <trd>by&tcaron; zhovievav&yacute;</trd>,
    <trd>omilosti&tcaron;</trd>,
    <trd>&scaron;etri&tcaron; niekoho</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x431;&#x443;&#x442;&#x438; &#x43F;&#x43E;&#x431;&#x43B;&#x430;&#x436;&#x43B;&#x438;&#x432;&#x438;&#x43C;</trd>,
    <trd>&#x43C;&#x438;&#x43B;&#x43E;&#x441;&#x442;&#x438;&#x432;&#x438;&#x43C;</trd>,
    <trd>&#x449;&#x430;&#x434;&#x438;&#x442;&#x438;</trd>,
    <trd>&#x431;&#x435;&#x440;&#x435;&#x433;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x440;&#x43E;&#x449;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="indulg.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Malsevereco, pardonemo:
      <ekz>
        peti <tld/>on;
      </ekz>
      <ekz>
        meriti <tld/>on de iu.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_b;&c_l;&c_a;&c_zh;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    <trdgrp lng="hu">
      <trd>eln&eacute;z&eacute;s<klr tip="amb">
(megbocs&aacute;t&aacute;s)</klr></trd>,
      <trd>irgalom</trd>,
      <trd>kegyelem</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_n;&c_i;&c_s;&c_h;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Respekta konsidero, modereco kontra&ubreve; iu a&ubreve; io:
      <ekz>
        havi <tld/>on kontra&ubreve; malsanulo;
      </ekz>
      <ekz>
        oni devas lernigi la infanojn montri <tld/>on al la maljunuloj.
      </ekz>
    </dif>
    <trd lng="be">&c_l;&c_ib;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    <trd lng="hu">k&iacute;m&eacute;let</trd>
    <trd lng="ru">&c_p;&c_o;&c_shch;&c_a;&c_d;&c_a;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>milosrdnost</trd>,
    <trd>ohled</trd>,
    <trd>shov&iacute;vavost</trd>,
    <trd>smilov&aacute;n&iacute;</trd>,
    <trd>strp&ecaron;n&iacute;</trd>,
    <trd>&scaron;&iacute;&rcaron;en&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Nachsichtig</trd>,
      <trd>Weitherzigkeit</trd>,
      <trd>Gro&szlig;z&uuml;gigkeit</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>indulgency</trd>,
    <trd>clemency</trd>,
    <trd>forbearance</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>indulgence</trd>,
    <trd>bienveillance</trd>,
    <trd>cl&eacute;mence</trd>,
    <trd>compr&eacute;hension<klr tip="ind"> (bienveillance)</klr></trd>,
    <trd>&eacute;gards</trd>,
    <trd>mansu&eacute;tude</trd>,
    <trd>m&eacute;nagement</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5BB9;&#x8D66;  [<ind>&#x3088;&#x3046;&#x3057;&#x3083;</ind>]</trd>,
    <trd>&#x5BDB;&#x5927;  [<ind>&#x304B;&#x3093;&#x3060;&#x3044;</ind>]</trd>,
    <trd>&#x5BDB;&#x5BB9;  [<ind>&#x304B;&#x3093;&#x3088;&#x3046;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">toegeeflijkheid</trd>
  <trdgrp lng="pl">
    <trd>pob&lstroke;a&zdot;anie</trd>,
    <trd>pob&lstroke;a&zdot;liwo&sacute;&cacute;</trd>,
    <trd>wyrozumia&lstroke;o&sacute;&cacute;</trd>
  </trdgrp>
  <trd lng="pt">indulg&ecirc;ncia</trd>
  <trdgrp lng="sk">
    <trd>milosrdenstvo</trd>,
    <trd>zhovievavos&tcaron;</trd>,
    <trd>zmilovanie</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x440;&#x43E;&#x441;&#x438;&#x442;&#x438; &#x43F;&#x43E;&#x449;&#x430;&#x434;&#x438;</trd>
</drv>

<drv mrk="indulg.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <dif>
      Inklina al <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_b;&c_l;&c_a;&c_zh;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_l;&c_ib;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_m;&c_ib;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>blahosklonn&yacute;</trd>,
    <trd>shov&iacute;vav&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>nachsichtig</trd>,
      <trd>weitherzig</trd>,
      <trd>gro&szlig;z&uuml;gig</trd>
  </trdgrp>
  <trdgrp lng="en">
      <trd>indulgent</trd>,
      <trd>clement</trd>,
      <trd>lenient</trd>,
      <trd>magnanimous</trd>,
      <trd>merciful</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>indulgent</trd>,
    <trd>bienveillant</trd>,
    <trd>bon<klr tip="ind"> (compr&eacute;hensif)</klr></trd>,
    <trd>cl&eacute;ment</trd>,
    <trd>compr&eacute;hensif</trd>,
    <trd>tol&eacute;rant</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>eln&eacute;z&odblac;</trd>,
    <trd>irgalmas</trd>,
    <trd>megbocs&aacute;t&oacute;</trd>,
    <trd>k&iacute;m&eacute;letes</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5BDB;&#x5BB9;&#x306A;  [<ind>&#x304B;&#x3093;&#x3088;&#x3046;&#x306A;</ind>]</trd>,
    <trd>&#x7518;&#x3044;  [<ind>&#x3046;&#x307E;&#x3044;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">toegeeflijk</trd>
  <trdgrp lng="pl">
    <trd>pob&lstroke;a&zdot;liwy</trd>,
    <trd>wyrozumia&lstroke;y</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>indulgente</trd>,
   <trd>clemente</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_n;&c_i;&c_s;&c_h;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_m;&c_i;&c_l;&c_o;&c_s;&c_t;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">zhovievav&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x438;&#x43B;&#x43E;&#x441;&#x442;&#x438;&#x432;&#x438;&#x439;</trd>,
    <trd>&#x43C;&#x438;&#x43B;&#x43E;&#x441;&#x435;&#x440;&#x434;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x43F;&#x43E;&#x431;&#x43B;&#x430;&#x436;&#x43B;&#x438;&#x432;&#x438;&#x439;</trd>,
    <trd>&#x432;&#x438;&#x440;&#x43E;&#x437;&#x443;&#x43C;&#x456;&#x43B;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="indulg.sen0a">
  <kap>sen<tld/>a</kap>
  <snc>
    <dif>
      Severa, ne pardonema.
    </dif>
    <refgrp tip="vid">
      <ref cel="fer.0a">fera</ref>,
      <ref cel="kruel.0a">kruela</ref>,
      <ref cel="rigid.0a">rigida</ref>,
      <ref cel="kompat.sen0a">senkompata</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>unnachsichtig</trd>,
      <trd>schonungslos</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>exacting</trd>,
      <trd>hard</trd>,
      <trd>implacable</trd>,
      <trd>pitiless</trd>,
      <trd>severe</trd>,
      <trd>stern</trd>,
      <trd>unsparing</trd>, <trd>nonindulgent</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>k&ouml;ny&ouml;rtelen</trd>,
      <trd>irgalmatlan</trd>,
      <trd>kegyetlen</trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_je;&c_s;&c_p;&c_o;&c_shch;&c_a;&c_d;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc>
    <dif>
      Senrespekta, senkonsidera.
    </dif>
    <refgrp tip="vid">
      <ref cel="krud.0a">kruda</ref>,
      <ref cel="gxenti.mal0a">mal&gcirc;entila</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ja;&c_z;&c_mol;&c_l;&c_ib;&c_t;&c_a;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_g;&c_r;&c_u;&c_b;&c_y;</trd>,
    <trd>&c_zh;&c_o;&c_r;&c_s;&c_t;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="cs">neoblomn&yacute;</trd>
  <trd lng="en">inconsiderate</trd>
  <trdgrp lng="fr">
    <trd>sans <ind>indulgence</ind></trd>,
    <trd>sans <ind>cl&eacute;mence</ind></trd>,
    <trd>cruel</trd>,
    <trd>sans <ind>&eacute;gard</ind></trd>,
    <trd>inflexible</trd>,
    <trd>insensible<klr tip="ind"> (s&eacute;v&egrave;re)</klr></trd>,
    <trd>sans <ind>mansu&eacute;tude</ind></trd>,
    <trd>s&eacute;v&egrave;re</trd>
  </trdgrp>
  <trd lng="hu">k&iacute;m&eacute;letlen</trd>
  <trdgrp lng="ja">
    <trd>&#x5BB9;&#x8D66;&#x306E;&#x306A;&#x3044;  [<ind>&#x3088;&#x3046;&#x3057;&#x3083;&#x306E;&#x306A;&#x3044;</ind>]</trd>,
    <trd>&#x4E0D;&#x5BDB;&#x5BB9;&#x306A;  [<ind>&#x3075;&#x304B;&#x3093;&#x3088;&#x3046;&#x306A;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">meedogenloos</trd>
 <trdgrp lng="pl">
    <trd>bezwgl&eogonek;dny</trd>,
    <trd>surowy</trd>,
    <trd>stanowczy</trd>
  </trdgrp>
  <trd lng="pt">severo</trd>
  <trd lng="sk">neoblomn&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x443;&#x432;&#x43E;&#x440;&#x438;&#x439;</trd>,
    <trd>&#x43D;&#x435;&#x449;&#x430;&#x434;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x431;&#x435;&#x437;&#x436;&#x430;&#x43B;&#x456;&#x441;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>
