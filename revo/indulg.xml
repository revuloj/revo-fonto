<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>indulg</rad>/i
</kap>
<drv mrk="indulg.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Malsevere trakti ies kulpon a&ubreve; pekon, esti pardonema kontra&ubreve;
      iu erarinta:
      <ekz>
        <tld/>i pekintan filon;
      </ekz>
      <ekz>
        <tld/>i al, kontra&ubreve; iu;
      </ekz>
      <ekz>
        <tld/>i kulpon, pri kulpo;
      </ekz>
      <ekz>
        <tld/>u lin pro lia pento;
      </ekz>
      <ekz>
        forta kolero devas esti punata, &ccirc;ar se vi &gcirc;in <tld/>os,
        &gcirc;i fari&gcirc;os ankora&ubreve; pli granda<fnt>Z</fnt>;
      </ekz>
      <ekz>
        esti <tld/>ata<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kompat.0i">kompati</ref>,
      <ref cel="pardon.0i">pardoni</ref>.
    </refgrp>
    <trd lng="ru">&c_o;&c_t;&c_n;&c_o;&c_s;&c_i;&c_t;&c_mol;&c_s;&c_ja; &c_s;&c_n;&c_i;&c_s;&c_h;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;</trd>
  </snc>
  <snc>
    <dif>
      Milde agi kontra&ubreve; iu, ne ofendi, montri konsideron al,
      doma&gcirc;i:
      <ekz>
        oni devas <tld/>i la orelojn de la infanoj<fnt>B</fnt>;
      </ekz>
      <ekz>
        la kalumnio <tld/>os eble vian puran nomon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li ne <tld/>as vian rangon nek oficon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i venkitan malamikon;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la krizo <tld/>is nian landon.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_shch;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_o;&c_shch;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="de">nachsichtig sein</trd>
  <trdgrp lng="fr">
    <trd>&eacute;pargner<klr tip="ind"> (m&eacute;nager)</klr></trd>,
    <trd>m&eacute;nager<klr tip="ind"> (&eacute;pargner)</klr></trd>
  </trdgrp>
  <trd lng="en">lenient</trd>
  <trd lng="nl">ontzien</trd>
</drv>

<drv mrk="indulg.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Malsevereco, pardonemo:
      <ekz>
        peti <tld/>on;
      </ekz>
      <ekz>
        meriti <tld/>on de iu.
      </ekz>
    </dif>
    <trd lng="de">Nachsicht</trd>
    <trd lng="ru">&c_s;&c_n;&c_i;&c_s;&c_h;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Respekta konsidero, modereco kontra&ubreve; iu a&ubreve; io:
      <ekz>
        havi <tld/>on kontra&ubreve; malsanulo;
      </ekz>
      <ekz>
        oni devas lernigi la infanojn montri <tld/>on al la maljunuloj.
      </ekz>
    </dif>
    <trd lng="ru">&c_p;&c_o;&c_shch;&c_a;&c_d;&c_a;</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>indulgence</trd>,
    <trd>bienveillance</trd>,
    <trd>cl&eacute;mence</trd>,
    <trd>compr&eacute;hension<klr tip="ind"> (bienveillance)</klr></trd>,
    <trd>&eacute;gards</trd>,
    <trd>m&eacute;nagement</trd>
  </trdgrp>
  <trd lng="nl">toegeeflijkheid</trd>
</drv>

<drv mrk="indulg.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <dif>
      Inklina al <tld/>o.
    </dif>
  </snc>
  <trd lng="de">nachsichtig</trd>
  <trdgrp lng="fr">
    <trd>indulgent</trd>,
    <trd>bienveillant</trd>,
    <trd>cl&eacute;ment</trd>,
    <trd>compr&eacute;hensif</trd>,
    <trd>tol&eacute;rant</trd>
  </trdgrp>
  <trd lng="nl">toegeeflijk</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_n;&c_i;&c_s;&c_h;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_m;&c_i;&c_l;&c_o;&c_s;&c_t;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="indulg.sen0a">
  <kap>sen<tld/>a</kap>
  <snc>
    <dif>
      Severa, ne pardonema.
    </dif>
    <refgrp tip="vid">
      <ref cel="rigid.0a">rigida</ref>,
      <ref cel="fer.0a">fera</ref>,
      <ref cel="kruel.0a">kruela</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>unnachsichtig</trd>,
      <trd>schonungslos</trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_je;&c_s;&c_p;&c_o;&c_shch;&c_a;&c_d;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc>
    <dif>
      Senrespekta, senkonsidera.
    </dif>
    <refgrp tip="vid">
      <ref cel="krud.0a">kruda</ref>,
      <ref cel="gxenti.mal0a">mal&gcirc;entila</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>sans <ind>indulgence</ind></trd>,
    <trd>sans <ind>&eacute;gard</ind></trd>,
    <trd>inflexible</trd>,
    <trd>insensible<klr tip="ind"> (s&eacute;v&egrave;re)</klr></trd>,
    <trd>s&eacute;v&egrave;re</trd>
  </trdgrp>
  <trd lng="nl">meedogenloos</trd>
</drv>
</art>
