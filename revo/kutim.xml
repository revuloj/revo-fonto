<drv mrk="kutim.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kutim.0i.KOMUNE">
    <dif>
      Forte inklini al ia ago, pro ofta ripetado:
      <ekz>
        li <tld/>as fumi pipon post tagman&gcirc;o;
      </ekz>
      <ekz>
        tiu infano <tld/>as je mensogado;
      </ekz>
      <ekz>
        la Asiriano <klr>[&#x2026;]</klr> <tld/>is al la orienta malrapideco en la voja&gcirc;oj kaj aferoj
        <fnt>
          <bib>Far2</bib>
          <lok>&ccirc;apitro 10a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lo&gcirc;antoj de <nom>Vallegrande</nom> <tld/>is traveturadojn de la militistoj
        <fnt>
          <aut>R. Gott, trad. V. Lutermano</aut>
          <vrk><url ref="https://eo.mondediplo.com/article1012.html">La korpo de la Che</url></vrk>
          <lok>Le Monde diplomatique, 2005-07-01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_c;&c_mol; <ind>&c_z;&c_v;&c_y;&c_ch;&c_k;&c_u;</ind></trd>
  <trdgrp lng="cs">
    <trd>b&yacute;t zvykl&yacute;</trd>,
    <trd>b&yacute;t zvykl&yacute;</trd>,
    <trd>m&iacute;t ve zvyku</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>gew&ouml;hnt sein</trd>,
     <trd>gewohnt sein</trd>,
     <trd>gew&ouml;hnlich tun</trd>,
     <trd>pflegen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>accustom</trd>,
    <trd><klr>be </klr>accustomed</trd>,
    <trd><klr>be </klr>used<klr> to</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
     <trd>tener la costumbre de</trd>,
     <trd>acostumbrarse a</trd>
  </trdgrp>
  <trd lng="fr">avoir l'<ind>habitude</ind> de</trd>
  <trdgrp lng="hu">
     <trd>szokott</trd>,
     <trd>az a <ind>szok&aacute;sa</ind></trd>
  </trdgrp>
  <trd lng="nl">gewoon zijn</trd>
  <trdgrp lng="pl">
    <trd>zwyczaj mie&cacute;</trd>,
    <trd>przyzwyczai&cacute; si&eogonek;</trd>,
    <trd>przyzwyczajonym by&cacute;</trd>
  </trdgrp>
  <trd lng="pt">ter o <ind>costume</ind> de</trd>
  <trdgrp lng="ro">
    <trd>au obiceiul</trd>,
    <trd>deprinde</trd>,
    <trd>sa se obisnuiasca</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_m;&c_je;&c_t;&c_mol; &c_p;&c_r;&c_i;&c_v;&c_y;&c_ch;&c_k;&c_u;</trd>
  <trdgrp lng="sk">
    <trd>ma&tcaron; vo zvyku</trd>,
    <trd>zvyka&tcaron;</trd>,
    <trd>zvykn&uacute;&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x430;&#x442;&#x438; &#x437;&#x432;&#x438;&#x447;&#x43A;&#x443;</trd>,
    <trd>&#x437;&#x432;&#x438;&#x447;&#x430;&#x439;</trd>
  </trdgrp>
</drv>

