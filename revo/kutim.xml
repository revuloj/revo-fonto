    <trd>habitude</trd>,
    <trd>us</trd>,
    <trd>usage<klr> (coutume)</klr></trd>
  </trdgrp>
  <trd lng="hu">szok&aacute;s</trd>
  <trd lng="nl">gewoonte</trd>
  <trdgrp lng="pl">
    <trd>przyzwyczajenie</trd>,
    <trd>zwyczaj</trd>,
    <trd>obyczaj</trd>
  </trdgrp>   
  <trdgrp lng="ro">
