  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_v;&c_u;&c_ch;&c_y;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_a;&c_d;&c_v;&c_y;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_z;&c_v;&c_y;&c_ch;&c_a;&c_ib;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>sich <ind>abgew&ouml;hnen</ind></trd>
  </trdgrp>
  <trd lng="en">break a habit</trd>
  <trdgrp lng="es">
    <trd>desacostumbrarse</trd>,
    <trd>deshabituarse</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>d&eacute;saccoutumer</ind></trd>,
    <trd>se <ind>d&eacute;shabituer</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
