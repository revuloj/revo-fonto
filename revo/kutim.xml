  </trdgrp>
  <trdgrp lng="es">
     <trd>tener la costumbre de</trd>,
     <trd>acostumbrarse a</trd>
  </trdgrp>
  <trd lng="fr">avoir l'<ind>habitude</ind> de</trd>
  <trdgrp lng="hu">
     <trd>szokott</trd>,
     <trd>az a <ind>szok&aacute;sa</ind></trd>
  </trdgrp>
  <trd lng="nl">gewoon zijn</trd>
  <trdgrp lng="pl">
    <trd>zwyczaj mie&cacute;</trd>,
    <trd>przyzwyczai&cacute; si&eogonek;</trd>,
    <trd>przyzwyczajonym by&cacute;</trd>
  </trdgrp>
  <trd lng="pt">ter o <ind>costume</ind> de</trd>
  <trdgrp lng="ro">
    <trd>au obiceiul</trd>,
    <trd>deprinde</trd>,
    <trd>sa se obisnuiasca</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_m;&c_je;&c_t;&c_mol; &c_p;&c_r;&c_i;&c_v;&c_y;&c_ch;&c_k;&c_u;</trd>
  <trdgrp lng="sk">
    <trd>ma&tcaron; vo zvyku</trd>,
    <trd>zvyka&tcaron;</trd>,
    <trd>zvykn&uacute;&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_m;&c_a;&c_t;&c_i; &c_z;&c_v;&c_i;&c_ch;&c_k;&c_u;</trd>,
    <trd>&c_z;&c_v;&c_i;&c_ch;&c_a;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="kutim.0a">
  <kap><tld/>a</kap>
  <snc mrk="kutim.0a.io">
    <dif>
      <klr>(io)</klr> Bazita sur <tld/>o;
      <ref cel="ordina.0a">ordinara</ref>:
      <ekz>
        &scirc;iaj veluraj okuloj ree rigardis kun la <tld/>a dol&ccirc;a melankolio
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        vespere en la <tld/>a horo li ku&scirc;i&gcirc;is
        <fnt>
          <bib>ElektFab</bib>
          <lok>La kura&gcirc;a tajloreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Panglos</nom> estis pendigata, kvankam tio estis ne <tld/>a
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        prezentante la <tld/>an raporton
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve; la <tld/>a maniero
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur &scirc;ia <tld/>a loko sidis sinjorino
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        defendu min per ciaj <tld/>aj talento kaj elokventeco
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="banal.0a">banala</ref>,
      <ref cel="tag.cxiu0a">&ccirc;iutaga</ref>,
      <ref cel="normal.0a.KOMUNE">normala<sncref/></ref>,
      <ref cel="trivia.0a">triviala</ref>,
      <ref cel="vulgar.0a.popola">vulgara<sncref/></ref>
