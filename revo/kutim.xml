    <trd>elszokik</trd>
  </trdgrp>
  <trd lng="ja">&#32722;&#24931;&#12434;&#12394;&#12367;&#12377;  <pr>&#12375;&#12421;&#12358;&#12363;&#12435;&#12434;&#12394;&#12367;&#12377;</pr></trd>
  <trdgrp lng="nl">
