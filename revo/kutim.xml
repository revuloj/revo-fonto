<drv mrk="kutim.de0igi">
  <kap>de<tld/>igi, <var><kap>mal<tld/>igi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kutim.de0igi.KOMUNE">
    <dif>
      Senigi je iu <tld/>o,
      <ref tip="vid" cel="instru.mal0i.PED">malinstrui</ref>:
      <ekz>
        li jam de<tld/>igis &scirc;in de similaj eksplodoj, kaj li kunpremis la dentojn
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi de<tld/>igis lin, ke li plu ne ardu sur min kaj ne rigarda&ccirc;u min
        <fnt>
          <bib>Fab4</bib>
          <lok>Ne&gcirc;ulo</lok>
        </fnt>!
      </ekz>
      <ekz>
        <nom>Lidia</nom> volas lin de<tld/>igi de la mamnutrado
        <fnt>
          <bib>VRA</bib>
          <lok>Moa Martinson</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi de<tld/>igis al mi la lekadon de tran&ccirc;iloj,
        el tio mi faros poemon
        <fnt>
          <aut>Cezar</aut>,
          <vrk><url ref="http://groups.yahoo.com/group/kreado/message/1478?var=1">
          Dekutimigo de la lekado de tran&ccirc;iloj</url></vrk>,
          <lok>2004</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_v;&c_u;&c_ch;&c_a;&c_c;&c_mol; <klr>(&c_a;&c_d; &c_z;&c_v;&c_y;&c_ch;&c_k;&c_ib;)</klr></trd>
  <trdgrp lng="cs">
    <trd>odnau&ccaron;it</trd>,
    <trd>odvyknout n&ecaron;koho od n&ecaron;&ccaron;eho</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>entw&ouml;hnen</trd>,
    <trd>abgew&ouml;hnen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>dehabituate</trd>,
    <trd>dishabituate</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;shabituer</trd>
  <trdgrp lng="pl">
    <trd>odzwyczai&cacute;</trd>,
    <trd>oduczy&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>dezobi&scomma;nuim</trd>,
    <trd>dezv&abreve;&tcomma;a</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odnau&ccaron;i&tcaron;</trd>,
    <trd>odvyka&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_u;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_z;&c_v;&c_i;&c_ch;&c_a;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="kutim.de0igxi">
  <kap>de<tld/>i&gcirc;i,
  <var><kap>mal<tld/>i&gcirc;i</kap></var></kap>
  <snc mrk="kutim.de0igxi.KOMUNE">
    <dif>
      Perdi <tld/>on:
      <ekz>
        mi tute de<tld/>i&gcirc;is de fumado
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        Jam de tre longe la militistaro &parsl; De<tld/>i&gcirc;is de l&#8217; oferoj sangaj
        <fnt>
          <bib>Ifigenio</bib>
          <lok>Ifigenio en Ta&ubreve;rido</lok>
        </fnt>;
      </ekz>
      <ekz>
        de <klr>[diservoj]</klr>, &scirc;ajnus, la
        Francoj estas jam longe de<tld/>i&gcirc;intaj
        <fnt>
         <aut>L. N. Tolstoj, trad. I. Ma&ccirc;ernis</aut>,
         <vrk><url ref="http://cindymckee.com/librejo/TOLSTOJ_Kristanismo_kaj_patriotismo.pdf">
         Kristanismo kaj Patriotismo</url></vrk>,
         <lok>1931</lok>
        </fnt>.
