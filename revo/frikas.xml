    <kap>
      <rad>frikas</rad>/i
      <fnt><bib>PIV</bib></fnt>
    </kap>

<drv mrk="frikas.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="stuf.0i">Stufi</ref> pecigitan
      helan viandon en sa&ubreve;co:
      <ekz>
        &ccirc;io tiel alloge &ccirc;emane ku&scirc;is en ladujoj <klr>[&#8230;]</klr> kokinetaj <ind><tld/>a&jcirc;o</ind>j,
        paste&ccirc;oj, <klr>[&#8230;]</klr> diversaj konfita&jcirc;oj, biskvitoj
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 47</lok>
        </fnt>.
        <trd lng="de">Frikassee</trd>
      </ekz>
    </dif>
    <trd lng="de">frikassieren</trd>
    <trd lng="en">fricassee <klr>(verb)</klr></trd>
    <trd lng="fr">fricasser</trd>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol; <ind>&c_f;&c_r;&c_y;&c_k;&c_a;&c_s;&c_e;</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x30D5;&#x30EA;&#x30AB;&#x30C3;&#x30BB;&#x306B;&#12377;&#x308B;</trd>,
    <trd>&#x30DB;&#x30EF;&#x30A4;&#x30C8;&#x30BD;&#x30FC;&#x30B9;&#12391;&#x716E;&#x8FBC;&#12416; 
    <pr>&#x30DB;&#x30EF;&#x30A4;&#x30C8;&#x30BD;&#x30FC;&#x30B9;&#12391;&#x306B;&#12371;&#12416;</pr></trd>
  </trdgrp>
  <trd lng="zh">&#21057;&#32905; <pr>du&#xF2;r&#xF2;u</pr></trd>
</drv>

  </art>

