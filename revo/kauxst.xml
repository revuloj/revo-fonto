<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <rad>ka&ubreve;stik</rad>/a
</kap>
<drv mrk="kauxst.0a">
   <kap><tld/>a</kap>
   <uzo tip="fak">KEM</uzo>
   <uzo tip="fak">MED</uzo>
   <snc>
     <uzo tip="klr">(pri substanco)</uzo>
     <dif>
       Brulanta, ka&ubreve;teriza, mordanta:
       <ekz>
         <tld/>aj rimedoj.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="fr">
     <trd>caustique<klr tip="ind"> (adj.)</klr></trd>,
     <trd>corrosif</trd>
   </trdgrp>
   <trdgrp lng="be">
     <trd>&c_k;&c_a;&c_w;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>,
     <trd>&c_je;&c_d;&c_k;&c_ib;</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>kaustisch</trd>,
     <trd>&auml;tzend</trd>,
     <trd>brennend</trd>,
     <trd>bei&szlig;end</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>caustic</trd>,
     <trd>burning</trd>,
     <trd>corrosive</trd>
   </trdgrp>
   <trdgrp lng="es">
     <trd>ca&uacute;stico</trd>,
     <trd>corrosivo</trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>mar&oacute;</trd>,
     <trd>&eacute;get&odblac;</trd>,
     <trd>kausztikus</trd>
   </trdgrp>
   <trdgrp lng="nl">
     <trd>bijtend</trd>,
     <trd>brandend</trd>,
     <trd>corrosief</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_je;&c_d;&c_k;&c_i;&c_j;</trd>,
 
<trd>&c_k;&c_a;&c_u;&c_s;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
   </trdgrp>
</drv>

<drv mrk="kauxst.0ajxo">
   <kap><tld/>a&jcirc;o</kap>
   <snc>
     <dif>
       <tld lit="K"/>a substanco.
     </dif>
   </snc>
   <trdgrp lng="de">
     <trd>Kaustikum</trd>,
     <trd>&auml;tzender Stoff</trd>,
     <trd>&Auml;tzmittel</trd>,
     <trd>Beize</trd>
   </trdgrp>
   <trd lng="en">caustic (substance)</trd>
   <trd lng="fr">caustique<klr tip="ind"> (substance)</klr></trd>
   <trdgrp lng="hu">
     <trd>mar&oacute; anyag</trd>,
     <trd>m&aacute;r&oacute;szer</trd>,
     <trd>kausztikum</trd>
   </trdgrp>
   <trd lng="nl">bijtende stof</trd>
   <trd lng="ru">&c_k;&c_a;&c_u;&c_s;&c_t;&c_i;&c_k;</trd>
</drv>

<drv mrk="kauxst.0eco">
   <kap><tld/>eco</kap>
   <snc>
     <dif>
       Eco de tio, kio estas <tld/>a:
       <ekz>
         la <tld/>eco de acidoj.
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="brul.0i">bruli</ref>,
       <ref cel="frot.0i">froti</ref>,
       <ref cel="kauxte.0i">ka&ubreve;terizi</ref>,
       <ref cel="korod.0i">korodi</ref>,
       <ref cel="mord.0i">mordi</ref>,
       <ref cel="rod1.0i">rodi</ref>.
     </refgrp>
   </snc>
   <trdgrp lng="de">
     <trd>Kaustizit&auml;t</trd>,
     <trd>&Auml;tzkraft</trd>,
     <trd>Brennverm&ouml;gen</trd>,
     <trd>Beizst&auml;rke</trd>
   </trdgrp>
   <trd lng="en">causticity</trd>
   <trd lng="fr">causticit&eacute;</trd>
   <trdgrp lng="hu">
     <trd>mar&oacute; hat&aacute;s</trd>,
     <trd>&eacute;get&odblac; hat&aacute;s</trd>,
     <trd>kausztikuss&aacute;g</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_je;&c_d;&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>,
     <trd>&c_zh;&c_g;&c_u;&c_ch;&c_je;&c_s;&c_t;&c_mol;</trd>
   </trdgrp>
</drv>
</art>
