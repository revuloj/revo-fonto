<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kauxst.xml,v 1.21 2023/12/03 08:00:25 revo Exp $">
<kap>
   <rad>ka&ubreve;stik</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kauxst.0a">
   <kap><tld/>a</kap>
   <snc mrk="kauxst.0a.KEM">
     <uzo tip="fak">KEM</uzo>
     <uzo tip="fak">MED</uzo>
     <uzo tip="klr">(pri substanco)</uzo>
     <dif>
       Brulanta, ka&ubreve;teriza, mordanta:
       <ekz>
         <tld/>aj rimedoj.
       </ekz>
     </dif>
   </snc>
   <snc mrk="kauxst.0a.FIG">
     <uzo tip="stl">RAR</uzo>
     <uzo tip="stl">FIG</uzo>
     <dif>
       Morda, ironia, akre mokanta:
       <ekz>
         pri la nigruloj de tiu &ccirc;i kontinento li skribas <tld/>e:
         <ctl>En la lingvoj de la a&ubreve;straliaj indi&gcirc;enoj ne ekzistas
         vortoj, kiuj tradukus justeco, peko, kulpo</ctl>
        <fnt><aut>T. Steele</aut>, <vrk>Flugi kun kakatuoj</vrk>, 
        <lok>2010</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_k;&c_a;&c_w;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>,
     <trd>&c_je;&c_d;&c_k;&c_ib;</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>kaustisch</trd>,
     <trd>&auml;tzend</trd>,
     <trd>brennend</trd>,
     <trd>bei&szlig;end</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>caustic</trd>,
     <trd>burning</trd>,
     <trd>corrosive</trd>
   </trdgrp>
   <trdgrp lng="es">
     <trd>ca&uacute;stico</trd>,
     <trd>corrosivo</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>caustique<klr tip="ind"> (adj.)</klr></trd>,
     <trd>corrosif</trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>mar&oacute;</trd>,
     <trd>&eacute;get&odblac;</trd>,
     <trd>kausztikus</trd>
   </trdgrp>
   <trdgrp lng="nl">
     <trd>bijtend</trd>,
     <trd>brandend</trd>,
     <trd>corrosief</trd>,
     <trd>agressief</trd>
   </trdgrp>
   <trdgrp lng="pl">
    <trd>gryz&aogonek;cy</trd>,
    <trd>kaustyczny</trd>,
    <trd>&zdot;r&aogonek;cy</trd>
  </trdgrp>   
<trdgrp lng="ro">
    <trd>acru</trd>,
    <trd>caustic</trd>,
    <trd>coroziv</trd>
  </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_je;&c_d;&c_k;&c_i;&c_j;</trd>,
     <trd>&c_k;&c_a;&c_u;&c_s;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
   </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x457;&#x434;&#x43A;&#x438;&#x439;</trd>,
    <trd>&#x43A;&#x430;&#x443;&#x441;&#x442;&#x438;&#x447;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
  <trd lng="zh">&#x82DB;&#x6027; <pr>k&#x113;x&#xEC;ng</pr></trd>
</drv>

<drv mrk="kauxst.0ajxo">
   <kap><tld/>a&jcirc;o</kap>
   <snc mrk="kauxst.0ajxo.KEM">
     <uzo tip="fak">KEM</uzo>
     <dif>
       <tld lit="K"/>a substanco.
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_k;&c_a;&c_w;&c_s;&c_t;&c_y;&c_k;</trd>,
     <trd>&c_je;&c_d;&c_k;&c_a;&c_je; &c_r;&c_e;&c_ch;&c_y;&c_v;&c_a;</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>Kaustikum</trd>,
     <trd>&auml;tzender Stoff</trd>,
     <trd>&Auml;tzmittel</trd>,
     <trd>Beize</trd>
   </trdgrp>
   <trd lng="en">caustic (substance)</trd>
   <trd lng="fr">caustique<klr tip="ind"> (substance)</klr></trd>
   <trdgrp lng="hu">
     <trd>mar&oacute; anyag</trd>,
     <trd>m&aacute;r&oacute;szer</trd>,
     <trd>kausztikum</trd>
   </trdgrp>
   <trdgrp lng="nl">
     <trd>bijtende stof</trd>,
     <trd>agressieve stof</trd>,
     <trd>corrosieve stof</trd>
   </trdgrp>
   <trdgrp lng="pl">
     <trd>&zdot;r&aogonek;ca substancja</trd>,
     <trd>kaustyk</trd>
   </trdgrp>   
   <trdgrp lng="ro">
     <trd>substan&#x21B;&abreve; corosiv&abreve;</trd>,
     <trd>caustic</trd>
   </trdgrp>
   <trd lng="ru">&c_k;&c_a;&c_u;&c_s;&c_t;&c_i;&c_k;</trd>
  <trd lng="uk">&#x43A;&#x430;&#x443;&#x441;&#x442;&#x438;&#x43A;</trd>
</drv>

<drv mrk="kauxst.0eco">
   <kap><tld/>eco</kap>
   <snc mrk="kauxst.0eco.KEM">
     <uzo tip="fak">KEM</uzo>
     <dif>
       Eco de tio, kio estas <tld/>a:
       <ekz>
         la <tld/>eco de acidoj.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="brul.0i">bruli</ref>,
       <ref cel="kauxte.0i">ka&ubreve;terizi</ref>,
       <ref cel="korod.0i">korodi</ref>,
       <ref cel="mord.0i">mordi</ref>,
       <ref cel="rongx.0i">rodi</ref>
     </refgrp>
   </snc>
   <trd lng="be">&c_je;&c_d;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
   <trdgrp lng="de">
     <trd>Kaustizit&auml;t</trd>,
     <trd>&Auml;tzkraft</trd>,
     <trd>Brennverm&ouml;gen</trd>,
     <trd>Beizst&auml;rke</trd>
   </trdgrp>
   <trd lng="en">causticity</trd>
   <trd lng="fr">causticit&eacute;</trd>
   <trdgrp lng="hu">
     <trd>mar&oacute; hat&aacute;s</trd>,
     <trd>&eacute;get&odblac; hat&aacute;s</trd>,
     <trd>kausztikuss&aacute;g</trd>
   </trdgrp>
   <trd lng="nl">agressiviteit<klr tip="ind"> (v.stoffen)</klr></trd>
   <trd lng="pl">kaustyczno&sacute;&cacute;</trd>
   <trdgrp lng="ru">
     <trd>&c_je;&c_d;&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>,
     <trd>&c_zh;&c_g;&c_u;&c_ch;&c_je;&c_s;&c_t;&c_mol;</trd>
   </trdgrp>
  <trd lng="uk">&#x43A;&#x430;&#x443;&#x441;&#x442;&#x438;&#x447;&#x43D;&#x456;&#x441;&#x442;&#x44C;</trd>
</drv>
</art>
