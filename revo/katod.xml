<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>katod</rad>/o
</kap>
<drv mrk="katod.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      Negativa
      <ref tip="super" cel="elektr3.0o">elektrodo</ref> de
      <ref tip="malprt" cel="pil.0o">pilo</ref>,
      <ref tip="malprt" cel="valv.0o.ELE">valvo</ref> a&ubreve; alia
      elektra aparato.
    </dif>
    <refgrp tip="ant">
      <ref cel="anod.0o">anodo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Minuspol</trd>,
    <trd>Kathode</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cathode</trd>,
    <trd>negative electrode</trd>
  </trdgrp>
  <trd lng="nl">kathode</trd>
  <trd lng="ru">&c_k;&c_a;&c_t;&c_o;&c_d;</trd>
</drv>
</art>
