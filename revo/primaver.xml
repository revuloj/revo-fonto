<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: primaver.xml,v 1.13 2019/10/20 17:10:17 revo Exp $">
<kap>
  <rad>primaver</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="primaver.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">POE</uzo>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="primaver.0o.printempo">
    <dif>
      <ref tip="dif" cel="printe.0o">Printempo</ref>:
      <ekz>
        festantoj eltrinku po tri tasojn adia&ubreve;e al la <tld/>o
        <fnt><bib>RugxDom</bib> <lok>capitro 63a, p. 365a</lok></fnt>;
      </ekz>
      <ekz>
        la Primavero estas Primavero, Sezono la plej dol&ccirc;a de la Jaro
        <fnt>
          <bib>Monato</bib>
          <aut>Geraldo Mattos</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006863.html"
              >Kio estas Amo?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_s;&c_n;&c_a;</trd>
  <trd lng="bg">&c_p;&c_r;&c_o;&c_l;&c_je;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>jaro</trd>,
    <trd>vesna</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Lenz</trd>
  </trdgrp>
  <trd lng="en">primavera</trd>
  <trd lng="es">primavera</trd>
  <trdgrp lng="fr">
    <trd>printemps</trd>
  </trdgrp>
  <trd lng="hu">kikelet</trd>
  <trd lng="nl">lente<klr tip="ind"> (ontr.)</klr></trd>
  <trd lng="pt">primavera</trd>
  <trdgrp lng="sk">
    <trd>jar</trd>,
    <trd>vesna</trd>
  </trdgrp>
  <trd lng="vo">flolatim</trd>
</drv>
</art>

