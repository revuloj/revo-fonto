<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ni&gcirc;er</rad>/o
</kap>

<drv mrk="nigxer.0o">
  <kap>
    <tld lit="N"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="nigxer.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la okcidenta Afriko kun &ccirc;efurbo Niameo.
      Teritorio de <em>Respubliko Ni&gcirc;ero</em>.
      ISO-3166-1 kodo: NE.
      &Gcirc;i limas kun
      <ref tip="vid" cel="algxeri.0o.GEOG">Al&gcirc;erio</ref>,
      <ref tip="vid" cel="benin.0o.GEOG">Benino</ref>,
      <ref tip="vid" cel="burkin.0o.GEOG">Burkino</ref>,
      <ref tip="vid" cel="cxad.0o.GEOG">&Ccirc;ado</ref>,
      <ref tip="vid" cel="mali.0o.GEOG">Malio</ref> kaj
      <ref tip="vid" cel="nigxeri.0o.GEOG">Ni&gcirc;erio</ref>.
    </dif>
    <trd lng="bg">&c_N;&c_i;&c_g;&c_je;&c_r;</trd>
    <trd lng="de">Niger</trd>
    <trd lng="en">Niger</trd>    
    <trd lng="it">nigeria</trd>
    <trdgrp lng="pl">
      <trd>Niger</trd>,
      <trd>Republika <ind>Nigru</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

