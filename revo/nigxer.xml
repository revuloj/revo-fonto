<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nigxer.xml,v 1.17 2024/01/29 15:59:10 revo Exp $">
<kap>
  <rad>Ni&gcirc;er</rad>/o
</kap>

<drv mrk="nigxer.N0o">
  <kap>
    <tld lit="N"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="nigxer.N0o.lando">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <mlg kod="ISO-3166">NE</mlg>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o.POL">&Scirc;tato</ref>
      en la okcidenta <ref tip="malprt" cel="afrik.A0o.mondparto">Afriko</ref>
      kun &ccirc;efurbo Niameo; teritorio de Respubliko <tld/>o,
      limanta kun
      <ref tip="vid" cel="algxeri.A0o.GEOG">Al&gcirc;erio</ref>,
      <ref tip="vid" cel="benin.B0o.GEOG">Benino</ref>,
      <ref tip="vid" cel="burkin.B0o.GEOG">Burkino</ref>,
      <ref tip="vid" cel="cxad.CX0o.GEOG">&Ccirc;ado</ref>,
      <ref tip="vid" cel="mali.M0o.GEOG">Malio</ref> kaj
      <ref tip="vid" cel="nigxeri.N0o.GEOG">Ni&gcirc;erio</ref>:
      <ekz>
        unika vizo ebligas al eksterlandanoj restadi en la komuna spaco,
        konsistanta al
        Benino, Burkino, Ebur-Bordo, <tld/>o kaj Togolo
        <fnt>
          <bib>Monato</bib>
          <aut>Christian Fabossou</aut>
          <vrk><url ref="&Monato;/2003/007931.html">Komuna vizo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        duono de la 11 milionoj da lo&gcirc;antoj de <tld/>o vivas en granda
        malri&ccirc;eco
        <fnt>
          <bib>Monato</bib>
          <aut>Thimbiano Mardia Oumarou/pg</aut>
          <vrk><url ref="&Monato;/2005/008706.php">Surstrata mizero</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        plago de migrantaj akridoj neniigis 4 milionojn da hektaroj en
        Ma&ubreve;ritanio,
        Senegalo, Malio, <tld/>o kaj &Ccirc;ado
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2006/008814.php">E&ubreve;ropo plialtigas sian drataron</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_N;&c_ib;&c_g;&c_je;&c_r; <klr>(&c_d;&c_z;&c_ja;&c_r;&c_zh;&c_a;&c_v;&c_a;)</klr></trd>
    <trd lng="bg">&c_N;&c_i;&c_g;&c_je;&c_r;</trd>
    <trd lng="de">Niger</trd>
    <trd lng="en">Niger</trd>
    <trdgrp lng="fr">
      <trd>Niger<klr> (pays)</klr></trd>,
      <trd>R&eacute;publique du Niger</trd>
    </trdgrp>    
    <trd lng="id">Niger</trd>
    <trd lng="it">Nigeria</trd>
    <trd lng="nl">Niger</trd>
    <trdgrp lng="pl">
      <trd>Niger</trd>,
      <trd>Republika <ind>Nigru</ind></trd>
    </trdgrp>
    <trd lng="pt">N&iacute;ger</trd>
    <trdgrp lng="zh">
    <trd>&#x5C3C;&#x65E5; <pr>N&#xED;r&#xEC;</pr></trd>,
    <trd>&#x5C3C;&#x65E5;&#x5C14; <pr>N&#xED;r&#xEC;&#x11B;r</pr></trd>,
    <trd>&#x5C3C;&#x65E5;&#x723E; <pr>N&#xED;r&#xEC;&#x11B;r</pr></trd>
  </trdgrp>
</snc>

  <snc mrk="nigxer.N0o.rivero">
  <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" lst="voko:riveroj" cel="river.0o.GEOG">Rivero</ref>
      trafluanta Gvineon, Malion, <ref cel="nigxer.N0o.lando">Ni&gcirc;eron</ref>
      kaj Ni&gcirc;erion, la&ubreve; kiu la lando estas nomata:
      <ekz>
        la unua e&ubreve;ropano atinginta la riveron <tld/>o &ndash; pereis en 1806,
        la skoto
        <nom>D. Livingstone</nom> &ndash; kiu unuafoje trairis la afrikan
        kontinenton de okcidento &gcirc;is
        oriento
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2004/008160.html">Afriko kaj aidoso</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_N;&c_ib;&c_g;&c_je;&c_r; <klr>(&c_r;&c_a;&c_k;&c_a;)</klr></trd>
    <trd lng="de">Niger <klr>(Fluss)</klr></trd>
    <trdgrp lng="fr">
      <trd>Niger<klr> (fleuve)</klr></trd>,
      <trd>Djoliba</trd>
    </trdgrp>
  </snc>
  <trd lng="ja">&#x30CB;&#x30B8;&#x30A7;&#x30FC;&#x30EB;&#x5DDD;  <pr>&#x30CB;&#x30B8;&#x30A7;&#x30FC;&#x30EB;&#x304B;&#x308F;</pr></trd>
  <trd lng="uk">&#x41D;&c_ib;&c_gu;&c_je;&c_r;</trd>
</drv>

</art>

