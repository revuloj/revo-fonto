<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
   <art mrk="$Id$">
     <kap>
       <ofc>*</ofc>
       <rad>nek</rad>
     </kap>

     <drv mrk="nek.0">
       <kap><ofc>*</ofc><tld/></kap>
       <snc mrk="nek.0.kroma">
         <dif>
           Nega <ref tip="super" cel="konjun.0o.GRA">konjunkcio</ref>,
           enkondukanta kroman negatan propozicion a&ubreve; parton;
           samsignifa kun &leftquot;kaj ne&rightquot;:
           <ekz>
             mi ne renkontis lin <tld/> lian fraton
             <fnt><bib>F</bib>, <lok>&FE;28.</lok></fnt>;
           </ekz><ekz>
             la viro ne ridis <tld/> ek&gcirc;emis<fnt>Z</fnt>;
           </ekz><ekz>
             neniam, <tld/> en la tagoj de bonstato, <tld/> en
             la momento de la mizeri&gcirc;o, <tld/> en la tempo, kiam
             &scirc;i provis diversajn vojojn kaj de &ccirc;iuj devis
             retiri&gcirc;i post kelkaj faritaj pasoj, neniam
             Marta prezentis al si, ke &scirc;iaj fortoj estas
             tiel malgrandaj <klr>...</klr>
             <fnt><bib>Marta</bib></fnt>.
           </ekz>
         </dif>
         <rim>
           Post <em>sen</em> oni uzas prefere <em>kaj</em> ol
           <em>nek</em>:
           <ekz>
             ili kreskas sen plugo kaj sem'<fnt>Z</fnt>;
           </ekz><ekz>
             strangaj kreska&jcirc;oj sen tero kaj bazo<fnt>B</fnt>.
           </ekz>
         </rim>
       </snc>
       <trd lng="be">&c_n;&c_ib;</trd>
       <trd lng="de">auch <ind>nicht</ind></trd>
       <trd lng="el">&omikron;&ypsilon_ton;&tau;&epsilon;</trd>
       <trd lng="en">nor</trd>
       <trdgrp lng="es">
         <trd>ni</trd>,
         <trd>tampoco</trd>
       </trdgrp>
       <trd lng="fi">eik&auml; <klr> (enk&auml;, etk&auml;...)</klr></trd>
       <trd lng="fr">ni</trd>
       <trd lng="he">&vav;&lamed;&alef;</trd>
       <trd lng="hu">sem</trd>
       <trd lng="nl">noch</trd>
       <trd lng="pt">nem</trd>
       <trd lng="ru">&c_n;&c_i;</trd>
     </drv>

     <drv mrk="nek.0nek">
       <kap><ofc>*</ofc><tld/> ... <tld/> ...</kap>
       <snc>
         <dif>
           Nega <ref tip="super" cel="konjun.0o.GRA">konjunkcio</ref>,
           uzata anta&ubreve; &ccirc;iu ano de listo da negataj aferoj
(anka&ubreve; anta&ubreve;
           la unua):
           <ekz>
             mi renkontis <tld/> lin <tld/> lian fraton;
             <fnt><bib>F</bib>, <lok>&FE;28.</lok></fnt>;
           </ekz><ekz>
             restas <tld/> konsilo <tld/> konsolo
             <fnt><bib>PrV</bib></fnt>;
           </ekz><ekz>
             &scirc;an&gcirc;oj estos farataj neniaj, <tld/> en la gramatiko
             <tld/> en la vortaro<fnt>Z</fnt>;
           </ekz><ekz>
             <tld/> en tiu tago <tld/> en la sekvanta venis la vikingo
             <fnt><bib>Fab4</bib>,
               <lok>Filino de la mar&ccirc;a re&gcirc;o</lok></fnt>;
           </ekz><ekz>
             personoj, kiuj havas <tld/> ian a&ubreve;toritaton, <tld/> ian
             forton<fnt>Z</fnt>.
           </ekz>
         </dif>
       </snc>
       <trd lng="de">weder ... noch</trd>
       <trd lng="en">neither ... nor</trd>
       <trd lng="es">ni</trd>
       <trd lng="fi">ei ... eik&auml;</trd>
       <trd lng="nl">noch ... noch</trd>
       <trd lng="ru">&c_n;&c_i; ... &c_n;&c_i;</trd>
     </drv>

   </art>
