<vortaro>
  <art mrk="$Id: suv.xml,v 1.3 2022/05/05 14:43:49 afido Exp $">
    <kap>
      <rad>Suv</rad>/o
    </kap>

<drv mrk="suv.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      La <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&ccirc;efurbo</ref> de <ref tip="malprt" cel="figxi.0o">Fi&gcirc;io</ref>:
      <ekz>
        la Templo de <tld/>o
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Templo_de_Suvo">Templo de Suvo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        forveturo al <tld/>o, promenado tra la urbo
        <fnt>
          <vrk><url ref="https://e.orientalista.pl/cirkau-la-mondo-kronologio/">&Ccirc;irka&ubreve; la Mondo. Kronologio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Suva</trd>
    <trd lng="en">Suva</trd>
    <trd lng="fr">Suva</trd>
  </snc>
  <trd lng="uk">&#x421;&#x443;&#x432;&#x430; (&#x441;&#x442;&#x43E;&#x43B;&#x438;&#x446;&#x44F;)</trd>
</drv>

  </art>

