<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: suv.xml,v 1.7 2024/03/25 07:28:38 afido Exp $">
    <kap>
      <rad>Suv</rad>/o
    </kap>

<drv mrk="suv.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      La <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&ccirc;efurbo</ref> de <ref tip="malprt" cel="figxi.0o">Fi&gcirc;io</ref>:
      <ekz>
        la Templo de <tld/>o
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Templo_de_Suvo">Templo de Suvo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        forveturo al <tld/>o, promenado tra la urbo
        <fnt>
          <vrk><url ref="https://e.orientalista.pl/cirkau-la-mondo-kronologio/">&Ccirc;irka&ubreve; la Mondo.
          Kronologio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_S;&c_u;&c_v;&c_a;</trd>
    <trd lng="de">Suva</trd>
    <trd lng="en">Suva</trd>
    <trd lng="fr">Suva</trd>
  </snc>
  <trd lng="ja">&#x30B9;&#x30D0;</trd>
  <trd lng="pl">Suva</trd>
  <trd lng="uk">&c_S;&c_u;&c_v;&c_a; (&c_s;&c_t;&#x43E;&#x43B;&c_i;&c_c;&#x44F;)</trd>
  <trdgrp lng="zh">
    <trd>&#x82CF;&#x74E6; <pr>S&#x16B;w&#x1CE;</pr></trd>,
    <trd>&#x8607;&#x74E6; <pr>S&#x16B;w&#x1CE;</pr></trd>
  </trdgrp>
</drv>

  </art>

