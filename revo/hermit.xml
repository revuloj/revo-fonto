<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>Hermit</rad>/o
</kap>
<drv mrk="hermit.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Franclingve: Charles <trd lng="fr">Hermite</trd>, 1822-1901. 
      Franca matematikisto.
    </dif>
  </snc>
  <trd lng="de">Hermite</trd>
  <trd lng="en">Hermite</trd>
  <trd lng="hu">Hermite</trd>
  <trd lng="ru">&c_E;&c_r;&c_m;&c_i;&c_t;</trd>
</drv>
<drv mrk="hermit.h0a">
  <kap><tld lit="h"/>a</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <refgrp tip="vid">
      <ref cel="form.hermita0o"><tld lit="h"/>a formo</ref>,
      <ref cel="produt.hermita_skalara0o"><tld lit="h"/>a skalara
produto</ref>,
      <ref cel="spac.hermita0o"><tld lit="h"/>a spaco</ref>.
    </refgrp>
  </snc>
  <snc mrk="hermit.0a.matrico">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <dif>
      <klr>(p.p. <ref cel="matric.n_p0o">(n,n)-matrico</ref> super la korpo de
      <ref cel="komple.0o.MAT">kompleksoj</ref>)</klr>
      Tia, ke &ccirc;iu &gcirc;ia elemento kun indico (i,j) egalas
      al la <ref cel="konjug.0ito.MAT">konjugito</ref> de tiu kun indico (j,i).
    </dif>
  </snc>
  <trd lng="de">Hermitesch</trd>
  <trd lng="en">Hermitian</trd>
  <trdgrp lng="fr">
    <trd>hermitien</trd>,
    <trd>hermitique</trd></trdgrp>
  <trdgrp lng="hu">
    <trd>Hermite-f&eacute;le</trd>,
    <trd>hermitikus</trd></trdgrp>  
  <trd lng="ru">&c_e;&c_r;&c_m;&c_i;&c_t;&c_o;&c_v;</trd>
</drv>
</art>

