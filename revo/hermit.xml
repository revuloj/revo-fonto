<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Hermit</rad>/o
</kap>
<drv mrk="hermit.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <dif>
      Franclingve: Charles <trd lng="fr">Hermite</trd>, 1822-1901.
      Franca matematikisto.
    </dif>
  </snc>
  <trd lng="be">&c_E;&c_r;&c_m;&c_ib;&c_t;</trd>
  <trd lng="de">Hermite</trd>
  <trd lng="en">Hermite</trd>
  <trd lng="hu">Hermite</trd>
  <trd lng="nl">Charles <ind>Hermite</ind></trd>
  <trd lng="pl">Hermite</trd>
  <trd lng="ru">&c_E;&c_r;&c_m;&c_i;&c_t;</trd>
</drv>

<drv mrk="hermit.h0a">
  <kap><tld lit="h"/>a</kap>
  <snc mrk="hermit.h0a.matrico">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      <klr>(p.p. <ref cel="matric.n_p0o">(n,n)-matrico</ref> super la korpo de
      <ref cel="komple.0o.MAT">kompleksoj</ref>)</klr>
      Tia, ke &ccirc;iu &gcirc;ia elemento kun indico
      <frm>(<k>i</k>,<k>j</k>)</frm> egalas al la
      <ref cel="konjug.0ito.komplekso">konjugito</ref>
      de tiu kun indico <frm>(<k>j</k>,<k>i</k>)</frm>.
    </dif>
    <trdgrp lng="be">
      <trd>&c_e;&c_r;&c_m;&c_ib;&c_t;&c_a;&c_v;&c_a; <klr tip="amb">(&c_m;&c_a;&c_t;&c_r;&c_y;&c_c;&c_a;)</klr></trd>,
      <trd>&c_s;&c_a;&c_m;&c_a;&c_s;&c_p;&c_a;&c_l;&c_u;&c_ch;&c_a;&c_n;&c_a;&c_ja; <klr tip="amb">(&c_m;&c_a;&c_t;&c_r;&c_y;&c_c;&c_a;)</klr></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Hermitesch<klr tip="ind"> (-e Matrix)</klr></trd>,
      <trd>selbstadjungiert<klr tip="ind"> (-e Matrix)</klr></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>hermitian<klr tip="ind"> (matrix)</klr></trd>,
      <trd>self-adjoint<klr tip="ind"> (matrix)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd><klr tip="amb">(matrice) </klr>hermitienne</trd>,
      <trd><klr tip="amb">(matrice) </klr>autoadjointe</trd>
    </trdgrp>
    <trd lng="pl"><klr tip="amb">(macierz) </klr>hermitowska</trd>
    <trdgrp lng="ru">
      <trd>&c_e;&c_r;&c_m;&c_i;&c_t;&c_o;&c_v;&c_a;<klr tip="amb"> (&c_m;&c_a;&c_t;&c_r;&c_i;&c_c;&c_a;)</klr></trd>,
      <trd>&c_s;&c_a;&c_m;&c_o;&c_s;&c_o;&c_p;&c_r;&c_ja;&c_zh;&c_jo;&c_n;&c_n;&c_a;&c_ja;<klr tip="amb"> (&c_m;&c_a;&c_t;&c_r;&c_i;&c_c;&c_a;)</klr></trd>
    </trdgrp>
  </snc>
  <snc>
    <refgrp tip="vid">
      <ref cel="form.hermita0o"><tld lit="h"/>a formo</ref>,
      <ref cel="produt.hermita_skalara0o"><tld lit="h"/>a skalara
      produto</ref>,
      <ref cel="spac.hermita0o"><tld lit="h"/>a spaco</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_E;&c_r;&c_m;&c_ib;&c_t;&c_a;</trd>,
    <trd>&c_e;&c_r;&c_m;&c_ib;&c_t;&c_a;&c_w;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>von <ind>Hermite</ind></trd>,
    <trd>Hermitesch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Hermite['s]</trd>,
    <trd>Hermitian</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d'<ind>Hermite</ind></trd>,
    <trd>hermitien</trd>,
    <trd>hermitique</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>Hermite-f&eacute;le</trd>,
    <trd>hermitikus</trd>
  </trdgrp>
  <trd lng="nl">Hermite-</trd>
  <trdgrp lng="pl">
    <trd>Hermite'a</trd>,
    <trd>hermitowski</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_E;&c_r;&c_m;&c_i;&c_t;&c_a;</trd>,
    <trd>&c_e;&c_r;&c_m;&c_i;&c_t;&c_o;&c_v;</trd>
  </trdgrp>
</drv>
</art>
