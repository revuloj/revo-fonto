<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>Hermit</rad>/o
</kap>
<drv mrk="hermit.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Franclingve: Charles <trd lng="fr">Hermite</trd>, 1596-1650. 
      Franca matematikisto.
    </dif>
  </snc>
  <trd lng="de">Hermite</trd>
  <trd lng="en">Hermite</trd>
  <trd lng="ru">&c_E;&c_r;&c_m;&c_i;&c_t;</trd>
</drv>
<drv mrk="hermit.h0a">
  <kap><tld lit="h"/>a</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <refgrp tip="vid">
      <ref cel="form.hermita0o"><tld lit="h"/>a formo</ref>,
      <ref cel="produt.hermita_skalara0o"><tld lit="h"/>a skalara produto</ref>,
      <ref cel="spac.hermita0o"><tld lit="h"/>a spaco</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Hermitesch</trd>
  <trd lng="en">Hermitian</trd>
  <trdgrp lng="fr">
    <trd>hermitien</trd>,
    <trd>hermitique</trd></trdgrp> 
  <trd lng="ru">&c_e;&c_r;&c_m;&c_i;&c_t;&c_o;&c_v;</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>