<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>klor</rad>/o
</kap>
<mlg>Cl</mlg>
<drv mrk="klor.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 17, atompezo 35,453, simbolo <frm>Cl</frm>.
      <ref tip="super" cel="gas.0o">Gaso</ref> verdete flava, 
      kun forta sufoka odoro, uzata por blankigo, 
      seninfektigo ktp. <ref tip="super" 
      cel="haloge.0o">Halogeno</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_l;&c_jo;&c_r;</trd> 
  <trd lng="br">klor</trd>
  <trd lng="de">Chlor</trd>
  <trd lng="en">chlorine</trd>
  <trd lng="es">cloro</trd> 
  <trd lng="fr">chlore</trd>
  <trd lng="hu">kl&oacute;r</trd>
  <trd lng="it">cloro</trd>
  <trd lng="la">Chlorum</trd>
  <trd lng="nl">chloor</trd>
  <trd lng="pl">chlor</trd>
  <trd lng="pt">cloro</trd>
  <trd lng="ru">&c_h;&c_l;&c_o;&c_r;</trd>
</drv>
</art>
