<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>tram</rad>/o
</kap>
<drv mrk="tram.0o">
  <kap><tld/>o</kap>
  <snc mrk="tram.0o.FER">
    <uzo tip="fak">FER</uzo>
    <dif>
      Eta <ref tip="super" cel="vagon.0aro.FER">vagonaro</ref> por porto
      de homoj sur urbaj stratoj:
      <ekz>
        sur la vitra kesto ekhelis la skriba&jcirc;o &leftquot;Atentu <tld/>on!&rightquot;
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 3a, p. 38a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_r;&c_a;&c_m;&c_v;&c_a;&c_j;</trd>
    <trdgrp lng="de">
      <trd>Stra&szlig;enbahn</trd>,
      <trd>Trambahn [esp. DE-sude, CH, AT]</trd>,
      <trd>Tram [esp. DE-sude, CH, AT]</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>street car <klr> (US)</klr></trd>,
      <trd>tram <klr> (BR)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>tram</trd>,
      <trd>tramway</trd>
    </trdgrp>
    <trd lng="hu">villamos</trd>
    <trd lng="nl">tram</trd>
    <trd lng="es">tranv&iacute;a</trd>
    <trd lng="pl">tramwaj</trd>
    <trd lng="pt">bonde</trd>
    <trd lng="ru">&c_t;&c_r;&c_a;&c_m;&c_v;&c_a;&c_j;</trd>
    <trd lng="sv">sp&aring;rvagn</trd>
  </snc>
  <snc mrk="tram.0o.relo">
    <uzo tip="stl">ARK</uzo>
    <dif>
      Kava relo por omnibuso; <tld/>relo.
    </dif>
    <trd lng="be">&c_t;&c_r;&c_a;&c_m;&c_v;&c_a;&c_j;&c_n;&c_a;&c_ja; &c_r;&c_e;&c_j;&c_k;&c_a;</trd>
    <trd lng="de">Stra&szlig;enbahnschiene</trd>
    <trdgrp lng="en">
      <trd>street car rail <klr> (US)</klr></trd>,
      <trd>tram rail <klr> (BR)</klr></trd>
    </trdgrp>
    <trd lng="fr">rail de tramway</trd>
    <trd lng="nl">tramspoor</trd>
    <trd lng="hu">villamoss&iacute;n</trd>
    <trd lng="ru">&c_t;&c_r;&c_a;&c_m;&c_v;&c_a;&c_j;&c_n;&c_y;&c_j; <ind>&c_r;&c_je;&c_l;&c_mol;&c_s;</ind></trd>
    <trd lng="sv">sp&aring;rvagnsr&auml;ls</trd>
  </snc>
</drv>

<drv mrk="tram.kablo0o">
  <kap>kablo<tld/>o</kap>
  <snc mrk="tram.kablo0o.FER">
    <uzo tip="fak">FER</uzo>
    <dif>
      <ref tip="super" cel="tram.0o.FER"><tld lit="T"/>o</ref>, kiu
      veturas supren kaj malsupren la&ubreve; kruta&jcirc;o, tirate de kablo:
      <ekz>
        per kablo-<tld/>o <klr>...</klr> ili supreniras
        <fnt>
        <aut>Saliko</aut>,
        <vrk>Vizito al la nacia parko Vanoise</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2005a, numero 2a, p. 22a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">funiculaire</trd>
  <trd lng="ru">&c_f;&c_u;&c_n;&c_i;&c_k;&c_u;&c_l;&c_jo;&c_r;</trd>
</drv>

<drv mrk="tram.0vojo">
  <kap><tld/>vojo</kap>
  <snc mrk="tram.0vojo.FER">
    <uzo tip="fak">FER</uzo>
    <dif>
      Fervojo provizita per kavaj reloj, sur kiuj iras <tld/>oj.
    </dif>
    <trd lng="be">&c_t;&c_r;&c_a;&c_m;&c_v;&c_a;&c_j;&c_n;&c_y; &c_sh;&c_l;&c_ja;&c_h;</trd>
    <trdgrp lng="en">
      <trd>street car tracks <klr> (US)</klr></trd>,
      <trd>tramway <klr> (BR)</klr></trd>
    </trdgrp>
    <trd lng="fr">voie de tram</trd>
    <trd lng="hu">villamosp&aacute;lya</trd>
    <trd lng="ru">&c_t;&c_r;&c_a;&c_m;&c_v;&c_a;&c_j;&c_n;&c_a;&c_ja; &c_d;&c_o;&c_r;&c_o;&c_g;&c_a;</trd>
    <trd lng="sv">sp&aring;rvagnssp&aring;r</trd>
  </snc>
</drv>
</art>
