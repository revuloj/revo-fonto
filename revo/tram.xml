<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tram.xml,v 1.43 2023/10/21 14:37:42 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>tram</rad>/o
</kap>

<drv mrk="tram.0o">
  <kap><tld/>o</kap>
  <snc mrk="tram.0o.FER">
    <uzo tip="fak">FER</uzo>
    <dif>
      Eta <ref tip="super" cel="vagon.0aro.FER">vagonaro</ref> por porto
      de homoj sur urbaj stratoj:
      <ekz>
        de malproksime oni povas a&ubreve;di la knaran siblon de la <tld/>o
        <fnt>
          <bib>LGA</bib>
          <lok>II.</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o ne kapablas solvi la transportan krizon,
        sed &ccirc;iuj ma&ccirc;is la malnovan
        temon, &ccirc;u konstrui, a&ubreve; ne konstrui la metropolitenon
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        sur la vitra kesto ekhelis la skriba&jcirc;o &leftquot;Atentu <tld/>on!&rightquot;
        <fnt><bib>MkM</bib> <lok>&ccirc;apitro 3a, p. 38a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_r;&c_a;&c_m;&c_v;&c_a;&c_j;</trd>
    <trd lng="bg">&c_t;&c_r;&c_a;&c_m;&c_v;&c_a;&c_j;</trd>
    <trdgrp lng="de">
      <trd>Stra&szlig;enbahn</trd>,
      <trd>Trambahn <klr>(esp. DE-sude, CH, AT)</klr></trd>,
      <trd>Tram esp. <klr>(DE-sude, CH, AT)</klr></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>street car <klr> (US)</klr></trd>,
      <trd>tram <klr> (BR)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>tram</trd>,
      <trd>tramway</trd>
    </trdgrp>
    <trd lng="hu">villamos</trd>
    <trd lng="nl">tram</trd>
    <trd lng="es">tranv&iacute;a</trd>
    <trd lng="pl">tramwaj</trd>
    <trd lng="pt">bonde</trd>
    <trd lng="ru">&c_t;&c_r;&c_a;&c_m;&c_v;&c_a;&c_j;</trd>
    <trd lng="sv">sp&aring;rvagn</trd>
  </snc>
  <snc mrk="tram.0o.relo">
    <uzo tip="stl">ARK</uzo>
    <dif>
      Kava relo por omnibuso; <tld/>relo.
    </dif>
    <trd lng="be">&c_t;&c_r;&c_a;&c_m;&c_v;&c_a;&c_j;&c_n;&c_a;&c_ja; &c_r;&c_e;&c_j;&c_k;&c_a;</trd>
    <trd lng="de">Stra&szlig;enbahnschiene</trd>
    <trdgrp lng="en">
      <trd>street car rail <klr> (US)</klr></trd>,
      <trd>tram rail <klr> (BR)</klr></trd>
    </trdgrp>
    <trd lng="fr">rail de tramway</trd>
    <trd lng="nl">tramspoor</trd>
    <trd lng="hu">villamoss&iacute;n</trd>
    <trd lng="pl"><ind>szyna</ind> tramwajowa</trd>
    <trd lng="pt">trilho cavado</trd>
    <trd lng="ru">&c_t;&c_r;&c_a;&c_m;&c_v;&c_a;&c_j;&c_n;&c_y;&c_j; <ind>&c_r;&c_je;&c_l;&c_mol;&c_s;</ind></trd>
    <trd lng="sv">sp&aring;rvagnsr&auml;ls</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>elektrika</trd>,
    <trd>tramvaj</trd>,
    <trd>v&uring;z pouli&ccaron;n&iacute; dr&aacute;hy</trd>,
    <trd>&scaron;alina</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8DEF;&#x9762;&#x96FB;&#x8ECA;  <pr>&#x308D;&#x3081;&#x3093;&#x3067;&#x3093;&#x3057;&#x3083;</pr></trd>,
    <trd>&#x5E02;&#x8857;&#x96FB;&#x8ECA;  <pr>&#x3057;&#x304C;&#x3044;&#x3067;&#x3093;&#x3057;&#x3083;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>1.elektri&ccaron;ka</trd>,
    <trd>2.&zcaron;liabkov&aacute; ko&lcaron;ajnica</trd>
  </trdgrp>
  <trd lng="uk">&c_t;&c_r;&c_a;&c_m;&c_v;&c_a;&c_j;</trd>
</drv>

<drv mrk="tram.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="tram.0i.TRA">
    <dif>
      <ref tip="super" cel="vetur.0i.TRA">Veturi<sncref/></ref> per <tld/>o:
      <ekz>
        la homoj iras laborejen kiel la aliajn tagojn, ili <tld/>as, a&ubreve;tobusas
        <fnt>
          <aut>&Eacute;. Vuillard, trad. J.-C. Roy</aut>
          <vrk><url ref="https://z4editions.fr/tagordo-2/">Tagordo</url></vrk>
          <lok>2018</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_je;&c_h;&c_a;&c_c;&c_mol; &c_n;&c_a; <ind>&c_t;&c_r;&c_a;&c_m;&c_v;&c_a;&c_ib;</ind></trd>
  <trd lng="de"><ind>Stra&szlig;enbahn</ind> fahren</trd>
  <trd lng="fr">prendre le <ind>tram</ind></trd>
</drv>

<drv mrk="tram.kablo0o">
  <kap>kablo<tld/>o</kap>
  <snc mrk="tram.kablo0o.FER">
    <uzo tip="fak">FER</uzo>
    <dif>
      <ref tip="super" cel="tram.0o.FER"><tld lit="T"/>o</ref>, kiu
      veturas supren kaj malsupren la&ubreve; kruta&jcirc;o, tirate de kablo:
      <ekz>
        per kablo-<tld/>o <klr>[&#x2026;]</klr> ili supreniras
        <fnt>
        <aut>Saliko</aut>
        <vrk>Vizito al la nacia parko Vanoise</vrk>
        <bib>Monato</bib>
        <lok>2005:2, p. 22a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="funikular.0o">funikularo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_n;&c_a;&c_t;&c_n;&c_y; <ind>&c_t;&c_r;&c_a;&c_m;&c_v;&c_a;&c_j;</ind></trd>,
    <trd>&c_f;&c_u;&c_n;&c_ib;&c_k;&c_u;&c_l;&c_jo;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>lanovka pozemn&iacute;</trd>,
    <trd>v&uring;z pozemn&iacute; lanovky</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kabelstra&szlig;enbahn</trd>,
    <trd>Kabelbahn</trd>
  </trdgrp>
  <trd lng="en">funicular</trd>
  <trd lng="es">telef&eacute;rico</trd>
  <trd lng="fr">funiculaire</trd>
  <trd lng="hu">sikl&oacute; <klr tip="amb">(j&aacute;rm&udblac;)</klr></trd>
  <trd lng="ja">&#x30B1;&#x30FC;&#x30D6;&#x30EB;&#x30AB;&#x30FC;</trd>
  <trd lng="nl">kabeltram</trd>
  <trd lng="pl">tramwaj linowy</trd>
  <trd lng="pt">funicular</trd>
  <trd lng="ru">&c_f;&c_u;&c_n;&c_i;&c_k;&c_u;&c_l;&c_jo;&c_r;</trd>
  <trd lng="sk">pozemn&aacute; lanovka</trd>
  <trd lng="uk">&c_f;&c_u;&c_n;&c_ib;&c_k;&c_u;&c_l;&c_je;&c_r;</trd>
</drv>

<drv mrk="tram.0vojo">
  <kap><tld/>vojo</kap>
  <snc mrk="tram.0vojo.FER">
    <uzo tip="fak">FER</uzo>
    <dif>
      Fervojo provizita per kavaj reloj, sur kiuj iras <tld/>oj.
    </dif>
    <trd lng="be">&c_t;&c_r;&c_a;&c_m;&c_v;&c_a;&c_j;&c_n;&c_y; &c_sh;&c_l;&c_ja;&c_h;</trd>
    <trdgrp lng="de">
      <trd>Stra&szlig;enbahngleise</trd>,
      <trd>Trambahngleise</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>street car tracks <klr> (US)</klr></trd>,
      <trd>tramway <klr> (BR)</klr></trd>
    </trdgrp>
    <trd lng="es">ra&iacute;l de tranv&iacute;a</trd>
    <trd lng="fr">voie de tram</trd>
    <trd lng="hu">villamosp&aacute;lya</trd>
    <trd lng="nl">tramspoorweg</trd>
    <trd lng="pl">tor tramwajowy</trd>
    <trdgrp lng="pt">
      <trd>tramway</trd>,
      <trd>tr&acirc;muei</trd>
    </trdgrp>
    <trd lng="ru">&c_t;&c_r;&c_a;&c_m;&c_v;&c_a;&c_j;&c_n;&c_a;&c_ja; &c_d;&c_o;&c_r;&c_o;&c_g;&c_a;</trd>
    <trd lng="sv">sp&aring;rvagnssp&aring;r</trd>
  </snc>
  <trd lng="cs">tramvajov&aacute; dr&aacute;ha</trd>
  <trdgrp lng="ja">
    <trd>&#x8DEF;&#x9762;&#x96FB;&#x8ECA;&#x8ECC;&#x9053;  <pr>&#x308D;&#x3081;&#x3093;&#x3067;&#x3093;&#x3057;&#x3083;&#x304D;&#x3069;&#x3046;</pr></trd>,
    <trd>&#x96FB;&#x8ECA;&#x9053;  <pr>&#x3067;&#x3093;&#x3057;&#x3083;&#x307F;&#x3061;</pr></trd>,
    <trd>&#x8DEF;&#x9762;&#x96FB;&#x8ECA;  <pr>&#x308D;&#x3081;&#x3093;&#x3067;&#x3093;&#x3057;&#x3083;</pr></trd>
  </trdgrp>
  <trd lng="sk">elektri&ccaron;kov&aacute; tra&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_r;&c_a;&c_m;&c_v;&c_a;&c_j;&c_n;&c_je; &c_s;&c_p;&c_o;&c_l;&c_u;&c_ch;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_t;&c_r;&c_a;&c_m;&c_v;&c_a;&c_j;&c_n;&c_a; &c_k;&c_o;&c_l;&c_ib;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
