<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>tram</rad>/o
</kap>
<drv mrk="tram.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Kava relo por omnibuso; <tld/>relo.
    </dif>
    <trd lng="de">Stra&szlig;enbahnschiene</trd>
    <trdgrp lng="en">
      <trd>street car rail <ind>(Usone)</ind></trd>,
      <trd>tram rail <ind>(Brite)</ind></trd>
    </trdgrp>
    <trd lng="nl">tramspoor</trd>
    <trd lng="ru">&c_t;&c_r;&c_a;&c_m;&c_v;&c_a;&c_j;&c_n;&c_y;&c_j; <ind>&c_r;&c_je;&c_l;&c_mol;&c_s;</ind></trd>
    <trd lng="sv">sp&aring;rvagnsr&auml;ls</trd>
  </snc>
  <snc>
    <dif>
      Omnibuso destinita por iri sur tiaj reloj; <tld/>veturilo.
    </dif>
    <trdgrp lng="de">
      <trd>Stra&szlig;enbahn</trd>,
      <trd>Trambahn [esp. DE-sude, CH, AT]</trd>,
      <trd>Tram [esp. DE-sude, CH, AT]</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>street car <klr>(Usone)</klr></trd>,
      <trd>tram <klr>(Brite)</klr></trd>
    </trdgrp>
    <trd lng="nl">tram</trd>
    <trd lng="es">tranv&iacute;a</trd>
    <trd lng="pl">tramwaj</trd>
    <trd lng="ru">&c_t;&c_r;&c_a;&c_m;&c_v;&c_a;&c_j;</trd>
    <trd lng="sv">sp&aring;rvagn</trd>
  </snc>
</drv>

<drv mrk="tram.0vojo">
  <kap><tld/>vojo</kap>
  <snc>
    <dif>
      Fervojo provizita per kavaj reloj, sur kiuj iras omnibusoj.
    </dif>
    <trdgrp lng="en">
      <trd>street car tracks <klr>(Usone)</klr></trd>,
      <trd>tramway <klr>(Brite)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_t;&c_r;&c_a;&c_m;&c_v;&c_a;&c_j;&c_n;&c_a;&c_ja; &c_d;&c_o;&c_r;&c_o;&c_g;&c_a;</trd>
    <trd lng="sv">sp&aring;rvagnssp&aring;r</trd>
  </snc>
</drv>
</art>
