<art mrk="$Id: aliment.xml,v 1.11 2021/02/28 15:34:26 revo Exp $">
<kap>
  <rad>aliment</rad>/o <fnt>Z</fnt> <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="aliment.0o">
  <kap><tld/>o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <fnt><bib>IKEV</bib></fnt>
  <snc mrk="aliment.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Devo liveri al parenco a&ubreve; boparenco la vivrimedojn
      necesajn al ties situacio, &gcirc;enerale en formo de
      <ref tip="super" cel="help.mon0o.KOMUNE">monhelpo</ref>:
      <ekz>
        Mary malsukcese petis &jcirc;uristan apartigon kaj <tld/>on
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Elias_Ashmole">
          Elias Ashmole</url>, 2012-08-06</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_ib;&c_m;&c_e;&c_n;&c_t;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>alimenty</trd>,
    <trd>v&#xFD;&#x17E;ivn&#xE9;</trd>,
    <trd>&#xFA;hrada osobn&#xED;ch pot&#x159;eb</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Unterhalt</trd>,
    <trd>Unterhaltsbetrag</trd>
  </trdgrp>
  <trdgrp lng="en">
      <trd>alimony</trd>,
      <trd>maintenance allowance</trd>
   </trdgrp>
  <trd lng="es">alimentos</trd>
  <trdgrp lng="fr">
    <trd>obligation alimentaire</trd>,
    <trd>pension alimentaire</trd>
  </trdgrp>
  <trd lng="he">&#x5DE;&#x5D6;&#x5D5;&#x5DF;</trd>
  <trdgrp lng="hu">
    <trd>eltart&aacute;s</trd>,
    <trd>tart&aacute;sd&iacute;j</trd>
  </trdgrp>
  <trd lng="pl">alimenty</trd>
  <trd lng="ru">&c_a;&c_l;&c_i;&c_m;&c_je;&c_n;&c_t;&c_y;</trd>
  <trdgrp lng="sk">
    <trd>v&yacute;&zcaron;ivn&eacute;</trd>,
    <trd>alimenty</trd>
  </trdgrp>
</drv>
</art>

