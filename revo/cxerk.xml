<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>&ccirc;erk</rad>/o
</kap>
<drv mrk="cxerk.0o">
<kap><ofc>*</ofc><tld/>o</kap>
<snc>
<dif>
Kesto en kiun oni metas la korpon de mortinto. 
</dif>
<refgrp tip="sin">
<ref cel="tomb.0o">tombo</ref>,
<ref cel="sarkof.0o">sarkofago</ref>.
</refgrp>
</snc>
<trd lng="en">coffin</trd>
<trd lng="ru">&c_g;&c_r;&c_o;&c_b;</trd>
</drv>
<drv mrk="cxerk.0a">
<kap><tld/>a</kap>
<snc num="1">
<dif>
De <tld/>o. 
</dif>
</snc>
<snc num="2">
<uzo tip="klr">(f)</uzo><dif>
Funebra, mal&gcirc;ojiga: <ekz><tld/>a tono.
</ekz>
</dif>
</snc>
<trd lng="ru">&c_g;&c_r;&c_o;&c_b;&c_o;&c_v;&c_o;&c_j;</trd>
</drv>
<drv mrk="cxerk.0veturilo">
<kap><tld/>veturilo</kap>
<snc>
<dif>
Veturilo por transporti la mortintojn.  
</dif>
</snc>
<trdgrp lng="ru">
  <trd>&c_p;&c_o;&c_g;&c_r;&c_je;&c_b;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_l;&c_je;&c_s;&c_n;&c_i;&c_c;&c_a;</trd>,
  <trd>&c_k;&c_a;&c_t;&c_a;&c_f;&c_a;&c_l;&c_k;</trd>
</trdgrp>
</drv>
</art>
<!--
$Log$
-->
</vortaro>
