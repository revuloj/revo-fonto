<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cxerk.xml,v 1.36 2023/11/29 09:27:12 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&ccirc;erk</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="cxerk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="cxerk.0o.MOR">
    <dif>
      Kesto, en kiun oni metas la korpon de mortinto.
    </dif>
    <refgrp tip="vid">
      <ref cel="sarkof.0o">sarkofago</ref>,
      <ref cel="tomb.0o">tombo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_u;&c_n;&c_a;</trd>,
    <trd>&c_d;&c_a;&c_m;&c_a;&c_v;&c_ib;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_k;&c_o;&c_v;&c_ch;&c_je;&c_g;</trd>
  <trd lng="ca">ta&uuml;t</trd>
  <trd lng="de">Sarg</trd>
  <trdgrp lng="el">
    <trd>&phi;&epsilon_ton;&rho;&epsilon;&tau;&rho;&omikron;&sigma_fina;</trd>,
    <trd>&sigma;&omikron;&rho;&omikron_ton;&sigma_fina;</trd>
  </trdgrp>
  <trd lng="en">coffin</trd>
  <trd lng="es">ata&uacute;d</trd>
  <trd lng="fa">&a_t;&a_A;&a_b;&a_w;&a_t;</trd>
  <trd lng="fr">cercueil</trd>
  <trd lng="he">&#x5D0;&#x5E8;&#x5D5;&#x5DF; &#x5DE;&#x5EA;&#x5D9;&#x5DD;</trd>
  <trd lng="hu">kopors&oacute;</trd>
  <trd lng="it">bara</trd>
  <trdgrp lng="ja">
    <trd>&#x68FA;  <pr>&#x304B;&#12435;</pr></trd>,
    <trd>&#x67E9;  <pr>&#x304B;&#12435;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>lijkkist</trd>,
    <trd>kist<klr tip="ind"> (lijkkist)</klr></trd>
  </trdgrp>
  <trd lng="pl">trumna</trd>
  <trdgrp lng="pt">
    <trd>caix&atilde;o</trd>,
    <trd>esquife</trd>,
    <trd>f&eacute;retro</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_o;&c_b;</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&#x43E;&#x43C;&#x43E;&c_v;&c_i;&#x43D;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_u;&#x43D;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x58FD;&#26448; <pr>sh&#xF2;uc&#xE1;i</pr></trd>,
    <trd>&#x68FA; <pr>gu&amacron;n</pr></trd>,
    <trd>&#x68FA;&#26408; <pr>gu&amacron;nm&#xF9;</pr></trd>,
    <trd>&#x69E5; <pr>hu&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="cxerk.0a">
  <kap><tld/>a</kap>
  <snc mrk="cxerk.0a.MOR">
    <dif>
      De <tld/>o.
    </dif>
    <trd lng="be">&c_t;&c_r;&c_u;&c_n;&c_n;&c_y;</trd>
    <trd lng="hu">kopors&oacute;-</trd>
    <trd lng="pl">trumienny</trd>
  </snc>
  <snc mrk="cxerk.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Funebra, mal&gcirc;ojiga:
      <ekz>
        <tld/>a tono.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_m;&c_a;&c_g;&c_ib;&c_l;&c_mol;&c_n;&c_y;</trd>,
      <trd>&c_z;&c_a;&c_m;&c_a;&c_g;&c_ib;&c_l;&c_mol;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Sarg-</trd>,
      <trd>Beerdigungs-</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>funereal</trd>,
      <trd>sepulchral</trd>
    </trdgrp>
    <trd lng="fr">s&eacute;pulcral<klr tip="ind"> (fig.)</klr></trd>
    <trd lng="hu">s&iacute;ri</trd>
    <trdgrp lng="it">
      <trd>funebre</trd>,
      <trd>funereo</trd>,
      <trd>sepolcrale</trd>
    </trdgrp>
    <trd lng="pl">pogrzebowy</trd>
    <trd lng="pt">f&uacute;nebre</trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#12402;&#12388;&#x304E;&#x306E;</trd>,
    <trd>&#38512;&#x6C17;&#x306A;  <pr>&#12356;&#12435;&#x304D;&#x306A;</pr></trd>,
    <trd>&#38512;&#x9B31;&#x306A;  <pr>&#x304B;&#12370;&#12358;&#12388;&#x306A;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_o;&c_b;&c_o;&c_v;&c_o;&c_j;</trd>
</drv>

<drv mrk="cxerk.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="cxerk.0isto.FUN">
    <dif>
      Metiisto faranta a&ubreve; komercisto vendanta <tld/>ojn:
      <ekz>
        la <tld/>isto invitis la &scirc;uiston sidi&gcirc;i kaj tetrinki, kaj dank' al la aperta
        karaktero de <nom>Gottlieb Schulz</nom>, ili balda&ubreve; konversaciis amike
        <fnt>
           <aut>A. Pu&scirc;kin, trad. N. Lozga&ccirc;ev</aut>
          <vrk><url ref="https://esperanto-ondo.ru/Rn-pusk1.htm">
          &Ccirc;erkisto</url></vrk>
          <lok>La Ondo de Esperanto, 1999:6 (56)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="funebr.0isto.FUN">funebristo</ref>,
    <ref cel="tomb.0isto.FUN">tombisto</ref>
  </refgrp>
  <trd lng="be">&c_t;&c_r;&c_u;&c_n;&c_a;&c_r;</trd>
  <trdgrp lng="fr">
    <trd>fabricant de cercueils</trd>,
    <trd>marchand de cercueils</trd>
  </trdgrp>
</drv>

<drv mrk="cxerk.0ujo">
  <kap><tld/>ujo <fnt><bib>PIV1</bib></fnt></kap>
  <snc>
    <dif>
      Aparta konstrua&jcirc;o a&ubreve; fermita loko (ekz-e en la kelo de pre&gcirc;ejo)
      kien oni formetas &ccirc;erko(j)n kun la entombigata(j) mortinto(j):
      <ekz>
        Kaj la <tld/>ujo, kien mi trankvile
        Vin metis, kial &gcirc;i malfermis nun
        La pezan sian bu&scirc;on de marmoro,
        Por vin el&jcirc;eti?
        <fnt><aut>&Scirc;ekspiro, tr. &Z;</aut>:
          <vrk>Hamleto, Re&gcirc;ido de Danujo</vrk>. <lok>1:3</lok>.
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kript.0o">kripto</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_y;&c_p;&c_t;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_g;&c_ib;&c_l;&c_mol;&c_n;&c_ja;</trd>,
    <trd>&c_s;&c_k;&c_l;&c_je;&c_p;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>sepulchre</trd>,
    <trd>crypt</trd>,
    <trd>vault</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>caveau</trd>,
    <trd>s&eacute;pulcre</trd>
  </trdgrp>
  <trd lng="he">&#x5D0;&#x5E8;&#x5D5;&#x5DF; &#x5DE;&#x5EA;&#x5D9;&#x5DD;</trd>
  <trd lng="ja">&#x7D0D;&#x68FA;&#x5BA4;  <pr>&#x306E;&#12358;&#x304B;&#12435;&#12375;&#12388;</pr></trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_l;&c_je;&c_p;</trd>,
    <trd>&c_g;&c_r;&c_o;&c_b;&c_n;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="cxerk.0veturilo">
  <kap><tld/>veturilo</kap>
  <snc>
    <dif>
      <ref tip="super" cel="vetur.0ilo">Veturilo</ref> por
      transporti la mortintojn.
    </dif>
    <refgrp tip="vid">
      <ref cel="cxar1.0o">&ccirc;aro</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_a;&c_f;&c_a;&c_l;&c_k;</trd>
  <trd lng="bg">&c_p;&c_o;&c_g;&c_r;&c_je;&c_b;&c_a;&c_l;&c_n;&c_a; &c_k;&c_o;&c_l;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Leichenwagen</trd>,
    <trd>Bestattungswagen</trd>
  </trdgrp>
  <trd lng="el">&ny;&epsilon;&kappa;&rho;&omikron;&phi;&omikron_ton;&rho;&alfa;</trd>
  <trd lng="en">hearse</trd>
  <trd lng="es">coche <ind>f&uacute;nebre</ind></trd>
  <trd lng="fa">&a_n;&a_ayn;&a_sh;&#x200C;&f_k;&a_sh;</trd>
  <trd lng="fr">corbillard</trd>
  <trdgrp lng="hu">
    <trd>halottaskocsi</trd>,
    <trd>gy&aacute;szkocsi</trd>
  </trdgrp>
  <trd lng="it"><ind>carro</ind> funebre</trd>
  <trd lng="ja">&#x970A;&#x67E9;&#x8ECA;  <pr>&#x308C;&#12356;&#x304D;&#12421;&#12358;&#12375;&#12419;</pr></trd>
  <trd lng="nl">lijkwagen</trd>
  <trd lng="pl">karawan</trd>
  <trdgrp lng="pt">
    <trd>carro f&uacute;nebre</trd>,
    <trd>rabec&atilde;o<klr tip="ind"> (komune)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_g;&c_r;&c_je;&c_b;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_l;&c_je;&c_s;&c_n;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_t;&c_a;&c_f;&c_a;&c_l;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8F34; <pr>ch&#x16B;n</pr></trd>,
    <trd>&#x8F00; <pr>&#xE9;r</pr></trd>,
    <trd>&#x8F40; <pr>w&emacron;n</pr></trd>,
    <trd>&#x67E9;&#x8ECA; <pr>ji&#xF9;ch&emacron;</pr></trd>
  </trdgrp>
</drv>
</art>
