<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kokard</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kokard.0o">
  <kap><tld/>o</kap>
  <snc mrk="kokard.0o.TEKS_insigna">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="insign.0o.signo">Insigno</ref>
      en formo de disketo a&ubreve; banteto, kiun oni portas
      &ccirc;e la &ccirc;apelo.
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_u;&c_k;&c_a;&c_r;&c_d;&c_a;</trd>,
      <trd>&c_c;&c_e;&c_sh;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="de">Kokarde</trd>
    <trd lng="en">cockade</trd>
    <trd lng="es">escarapela</trd>
    <trd lng="hu">kok&aacute;rda</trd>
  <trdgrp lng="pt">
    <trd>cocar</trd>,
    <trd>tope <klr tip="amb">(no chap&eacute;u)</klr></trd>
  </trdgrp>
    <trd lng="ru">&c_k;&c_o;&c_k;&c_a;&c_r;&c_d;&c_a;</trd>
  </snc>
  <snc mrk="kokard.0o.TEKS_ornama">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Banto el rubando, uzata de virinoj kiel ornamo.  
    </dif>
    <trd lng="be">&c_b;&c_a;&c_n;&c_t;</trd>
    <trd lng="de">Bandschleife</trd>
    <trd lng="en">bow</trd>
  </snc>
  <trd lng="fr">cocarde</trd>
  <trd lng="hu">szalagcsokor</trd>
  <trd lng="nl">kokarde</trd>
  <trd lng="pl">kokarda</trd>
  <trd lng="pt">la&ccedil;o <klr tip="amb">(de fita)</klr></trd>
</drv>
</art>
