<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kokard.xml,v 1.23 2023/10/18 17:23:24 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>kokard</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kokard.0o">
  <kap><tld/>o</kap>
  <snc mrk="kokard.0o.TEKS_insigna">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="insign.0o.signo">Insigno</ref>
      en formo de disketo a&ubreve; banteto, kiun oni portas
      &ccirc;e la &ccirc;apelo:
      <ekz>
        la publiko <klr>...</klr> de supraj eta&gcirc;oj &jcirc;etas
        tutajn nubojn da diverskoloraj <tld/>oj
        <fnt>
         <aut>L. N. Tolstoj, trad. I. Ma&ccirc;ernis</aut>,
         <vrk><url ref="http://cindymckee.com/librejo/TOLSTOJ_Kristanismo_kaj_patriotismo.pdf">
         Kristanismo kaj Patriotismo</url></vrk>,
         <lok>1931</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_u;&c_k;&c_a;&c_r;&c_d;&c_a;</trd>,
      <trd>&c_c;&c_e;&c_sh;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="de">Kokarde</trd>
    <trd lng="en">cockade</trd>
    <trd lng="es">escarapela</trd>
    <trd lng="hu">kok&aacute;rda</trd>
  <trdgrp lng="pt">
    <trd>cocar</trd>,
    <trd>tope <klr tip="amb">(no chap&eacute;u)</klr></trd>
  </trdgrp>
    <trd lng="ru">&c_k;&c_o;&c_k;&c_a;&c_r;&c_d;&c_a;</trd>
  </snc>
  <snc mrk="kokard.0o.TEKS_ornama">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Banto el rubando, uzata de virinoj kiel ornamo.  
    </dif>
    <trd lng="be">&c_b;&c_a;&c_n;&c_t;</trd>
    <trd lng="de">Bandschleife</trd>
    <trd lng="en">bow</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>kokarda</trd>,
    <trd>r&#x16F;&#x17E;ice listov&#xE1;</trd>
  </trdgrp>
  <trd lng="fr">cocarde</trd>
  <trd lng="hu">szalagcsokor</trd>
  <trdgrp lng="ja">
    <trd>&#x82B1;&#x5F62;&#x5E3D;&#x7AE0;  <pr>&#x306F;&#x306A;&#x304C;&#x305F;&#x307C;&#x3046;&#x3057;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x8A18;&#x7AE0;  <pr>&#x304D;&#x3057;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x82B1;&#x7D50;&#x3073;  <pr>&#x306F;&#x306A;&#x3080;&#x3059;&#x3073;</pr></trd>
  </trdgrp>
  <trd lng="nl">kokarde</trd>
  <trd lng="pl">kokarda</trd>
  <trd lng="pt">la&ccedil;o <klr tip="amb">(de fita)</klr></trd>
  <trd lng="sk">kokarda</trd>
  <trd lng="uk">&#x43A;&#x43E;&#x43A;&#x430;&#x440;&#x434;&#x430;</trd>
</drv>
</art>
