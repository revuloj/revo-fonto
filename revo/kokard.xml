<drv mrk="kokard.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Insigno en formo de disketo a&ubreve; banteto, kiun oni portas &ccirc;e la &ccirc;apelo.
    </dif>
    <trd lng="en">cockade</trd>
    <trd lng="ru">&c_k;&c_o;&c_k;&c_a;&c_r;&c_d;&c_a;</trd>
  </snc>
  <snc num="2">
    <dif>
      Banto el rubando, uzata de virinoj kiel ornamo.  
    </dif>
    <trd lng="en">bow</trd>
  </snc>
</drv>
</art>
