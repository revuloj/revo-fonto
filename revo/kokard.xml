<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kokard</rad>/o
</kap>
<drv mrk="kokard.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Insigno en formo de disketo a&ubreve; banteto, kiun oni portas &ccirc;e la &ccirc;apelo.
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_u;&c_k;&c_a;&c_r;&c_d;&c_a;</trd>,
      <trd>&c_c;&c_e;&c_sh;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="de">Kokarde</trd>
    <trd lng="en">cockade</trd>
    <trd lng="nl">kokarde</trd>
    <trd lng="ru">&c_k;&c_o;&c_k;&c_a;&c_r;&c_d;&c_a;</trd>
  </snc>
  <snc num="2">
    <dif>
      Banto el rubando, uzata de virinoj kiel ornamo.  
    </dif>
    <trd lng="be">&c_b;&c_a;&c_n;&c_t;</trd>
    <trd lng="de">Bandschleife</trd>
    <trd lng="en">bow</trd>
  </snc>
</drv>
</art>
