<art mrk="$Id: litani.xml,v 1.18 2023/09/01 19:11:00 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>litani</rad>/o
</kap>
<drv mrk="litani.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <uzo tip="fak">KAT</uzo>
    <dif>
      Pre&gcirc;o konsistanta el longa serio da mallongaj alvokoj al
      Dio, Dipatrino a&ubreve; sanktuloj.
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Longa kaj teda elnomado, elnombrado:
      <ekz>
        senfina <tld/>o da plendoj, ripro&ccirc;oj.
      </ekz>
    </dif>
    <trd lng="de">eint&ouml;nige Aufz&auml;hlung</trd>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_t;&c_a;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="de">Litanei</trd>
  <trd lng="el">&lambda;&jota;&tau;&alfa;&ny;&epsilon;&jota_ton;&alfa;</trd>
  <trd lng="en">litany</trd>
  <trd lng="es">letan&iacute;a</trd>
  <trd lng="hu">lit&aacute;nia</trd>
  <trd lng="it">litania</trd>
  <trdgrp lng="ja">
    <trd>&#x9023;&#x7977;</trd>,
    <trd>&#x9577;&#x305F;&#x3089;&#x3057;&#x3044;&#x8A71;  [<ind>&#x306A;&#x304C;&#x305F;&#x3089;&#x3057;&#x3044;&#x306F;&#x306A;&#x3057;</ind>]</trd>,
    <trd>&#x7E70;&#x308A;&#x8A00;  [<ind>&#x304F;&#x308A;&#x3054;&#x3068;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">litanie</trd>
  <trd lng="pl">litania</trd>
  <trdgrp lng="pt">
    <trd>ladainha</trd>,
    <trd>litania</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_i;&c_t;&c_a;&c_n;&c_i;&c_ja;</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x456;&#x442;&#x430;&#x43D;&#x456;&#x44F;</trd>,
    <trd>&#x43C;&#x43E;&#x43B;&#x435;&#x431;&#x435;&#x43D;&#x44C;</trd>
  </trdgrp>
</drv>
</art>
