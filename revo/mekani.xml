<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>mekanik</rad>/o
</kap>
<drv mrk="mekani.0o">
  <kap><tld/>o <fnt><lok>Akademio de Esperanto, <url
  ref="http://www.akademio-de-esperanto.org/aktoj/aktoj2/8oa.html">8a
  Oficiala Aldono</url>, II.A.1</lok></fnt></kap>
  <snc>
      <ref tip="dif" cel="mehxan.0o">me&hcirc;aniko</ref>
      <ekz>
        pri <tld/>o mi scias nenion
        <fnt><bib>Gerda</bib>, <lok>&ccirc;apitro 11a</lok></fnt>.
      </ekz>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_h;&c_a;&c_n;&c_ib;&c_k;&c_a;</trd>
  <trd lng="ca">mec&agrave;nica</trd>
