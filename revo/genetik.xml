<vortaro>
<art mrk="$Id$">
<kap>
<rad>genetik</rad>/o
</kap>
<drv mrk="genetik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BIO</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="scienc.0o.fako">Scienco</ref> pri la heredeco, 
      &gcirc;ia kemia fundamento kaj &gcirc;ia efiko sur disvolvi&gcirc;on de 
      la individuo kaj la evoluon de la specio.
    </dif>
  </snc>
  <trdgrp lng="de">
   <trd>Genetik</trd>,
   <trd>Vererbungslehre</trd></trdgrp>
  <trd lng="en">genetics</trd>
  <trd lng="fr">g&eacute;n&eacute;tique<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="nl">
    <trd>erfelijkheidsleer</trd>,
    <trd>genetiek</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_je;&c_n;&c_je;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="sv">genetik</trd>
</drv>
</art>

