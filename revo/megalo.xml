<vortaro>
<art mrk="$Id: megalo.xml,v 1.18 2020/07/26 16:10:35 revo Exp $">
<kap>
  <rad>megalosa&ubreve;r</rad>/o
</kap>
<drv mrk="megalo.0o">
  <kap><tld/>o</kap>
  <snc mrk="megalo.0o.ZOO">
    <uzo tip="fak">PAL</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">Genro</ref>
      de <ref tip="lst" cel="dinosa.0o" lst="voko:dinosa&ubreve;roj">dinosa&ubreve;roj</ref>
      (<trd lng="la">Megalosaurus</trd>),
      vivinta en <ref tip="malprt" cel="jxuras.0o">&jcirc;uraso</ref>
      kaj <ref tip="malprt" cel="kretac.0o">kretaceo</ref>:
      <ekz>
        <nom>William Buckland</nom> <klr>[&#8230;]</klr> verkis la unuan sciencan priskribon de
        dinosa&ubreve;ro, pri <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=276219"
              >William Buckland</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        dum la malfrua &jcirc;uraso estis ne malpli ol kvar apartaj teropodaj evolulinioj &#8208;
        ceratosa&ubreve;ruloj, <tld/>oj, karnosa&ubreve;roj, kaj celurosa&ubreve;roj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=202263"
              >Teropodoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/b/bb/Megalosaurus_dinosaur.png/320px-Megalosaurus_dinosaur.png"
      prm="PD"
      >desegno de <tld/>o
        <fnt>
          <aut>LadyofHats Mariana Ruiz prilaborita de FunkMonk and Steveoc 86</aut>
          <lok><url ref="&WCW;/File:Megalosaurus_dinosaur.png"
              >https://commons.wikimedia.org/wiki/File:Megalosaurus_dinosaur.png</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_g;&c_a;&c_l;&c_a;&c_z;&c_a;&c_w;&c_r;</trd>
  <trd lng="ca">megalosaure</trd>
  <trd lng="de">Megalosaurus</trd>
  <trd lng="fr">m&eacute;galosaure</trd>
  <trd lng="hu">Megalosaurus</trd>
  <trd lng="nl">megalosaurus</trd>
  <trd lng="pl">megalozaur</trd>
  <trd lng="pt">megalossauro</trd>
  <trd lng="ro">megalosaurus</trd>
  <trd lng="ru">&c_m;&c_je;&c_g;&c_a;&c_l;&c_o;&c_z;&c_a;&c_v;&c_r;</trd>
  <trd lng="sk">megalosaurus</trd>
</drv>
</art>
