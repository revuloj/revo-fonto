<art mrk="$Id: fabel.xml,v 1.36 2023/11/09 19:13:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fabel</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fabel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fabel.0o.BELE">
    <uzo tip="fak">BELE</uzo>
    <dif>
      Fantazia <ref tip="super" cel="rakont.0o">rakonto</ref> pri mirigaj 
      <ref tip="prt" cel="aventu.0o">aventuro</ref>j:
      <ekz>
        <klr>[t]</klr>ie lo&gcirc;as la homo, kiu povoscias rakonti <tld/>ojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia patrino amas a&ubreve;di fabelojn moralajn kaj bonmorajn
        <fnt>
          <bib>Fab1</bib>
          <lok>Fluganta kofro</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj <tld/>oj estas <ref tip="prt" cel="alegor.0o">alegorio</ref>j pri 
        homaj interrilatoj
        <fnt>
          <bib>Monato</bib>
          <aut>Angela Tellier</aut>
          <vrk><url ref="&Monato;/2004/008306.html">La&ubreve;proverbaj fabeloj</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        forrifuzu profanajn kaj avinajn <tld/>ojn kaj ekzercu vin al pieco
        <fnt>
          <bib>NT</bib>
          <lok>I. Timoteo 4:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili pensas, ke mi timos iliajn <tld/>ojn pri la sorto
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni trovas <tld/>ojn jam &ccirc;e la antikvaj popoloj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Anta&ubreve;parolo de la tradukinto</lok>
        </fnt>;
      </ekz>
      <ekz>
        en Francujo en la fino de la deksepa jarcento aperis la tutmonde konataj
        <tld/>oj de <nom>Perrault</nom> kaj la traduko <klr>[&#x2026;]</klr> de Mil kaj unu noktoj &dash; la
        fama kolekto de arabaj <tld/>oj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Anta&ubreve;parolo de la tradukinto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne sole la infanoj legas kun plezuro la <nom>Grimm</nom>'ajn <tld/>ojn, anka&ubreve; la maturaj
        homoj &scirc;atas ilin
        <fnt>
          <bib>ElektFab</bib>
          <lok>Anta&ubreve;parolo de la tradukinto</lok>
        </fnt>;
      </ekz>
      <ekz>
        bela celo por <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>  
    <refgrp tip="vid">
      <ref cel="fabl.0o">fablo</ref>,
      <ref cel="legend.0o">legendo</ref>,
      <ref cel="rakont.mir0o">mirrakonto</ref>,
      <ref cel="mit.0o">mito</ref>
    </refgrp>
    <trdgrp lng="br">
      <trd>kontadenn</trd>,
      <trd>mojenn<klr tip="ind"> (kontadenn)</klr></trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>ap&ograve;leg</trd>,
      <trd>par&agrave;bola</trd>
    </trdgrp>
    <trd lng="en">fairy tale</trd>
    <trdgrp lng="pl">
      <trd>bajka</trd>,
      <trd>ba&sacute;&nacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="fabel.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="prt" cel="pens.el0ajxo">Elpensa&jcirc;o</ref>, mensoga rakonto ks:
      <ekz>
        anka&ubreve; tio estas <tld/>o... senhonta <tld/>o!... &mdash;
        ekkriis <nom>Tutmozis</nom>
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 3a</lok></fnt>
      </ekz>
      <ekz>
        a&ubreve;skultu nin, kaj konvinki&gcirc;u, ke ni ne estas <tld/>o
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista en Washington en la 15a de a&ubreve;gusto
          1910</vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi diros, ke tio estas <tld/>oj por enluli&gcirc;i, ke mi bone scias, kion mi scias
        <fnt>
          <bib>GDn</bib>
          <lok>Akto Unua</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="br">
      <trd>brozenn</trd>,
      <trd>bidenno&ugrave;<klr> [pl.]</klr></trd>,
      <trd>mojenn<klr tip="ind"> (gaou)</klr></trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>invenci&oacute;</trd>,
      <trd>farsa</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>fairy tale</trd>,
      <trd>bedtime <ind>story</ind></trd>
    </trdgrp>
    <trd lng="pl">bajka</trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_z;&c_k;&c_a;</trd>
  <trd lng="bg">&c_p;&c_r;&c_i;&c_k;&c_a;&c_z;&c_k;&c_a;</trd>
  <trd lng="ca">faula</trd>
  <trd lng="cs">poh&aacute;dka</trd>
  <trdgrp lng="de">
    <trd>M&auml;rchen</trd>,
    <trd>Sage</trd>
  </trdgrp>
  <trd lng="es">f&aacute;bula</trd>
  <trdgrp lng="fr">
    <trd>conte</trd>,
    <trd>fable<klr tip="ind"> (l&eacute;gende, mensonge)</klr></trd>,
    <trd>l&eacute;gende</trd>
  </trdgrp>
  <trd lng="hu">mese</trd>
  <trd lng="id">dongeng</trd>
  <trdgrp lng="ja">
    <trd>&#x7AE5;&#x8A71;  <pr>&#x3069;&#x3046;&#x308F;</pr></trd>,
    <trd>&#x304A;&#x3068;&#x304E;&#x8A71;  <pr>&#x304A;&#x3068;&#x304E;&#x3070;&#x306A;&#x3057;</pr></trd>,
    <trd>&#x4F5C;&#x308A;&#x8A71;  <pr>&#x3064;&#x304F;&#x308A;&#x3070;&#x306A;&#x3057;</pr></trd>
  </trdgrp>
  <trd lng="nl">sprookje</trd>
  <trd lng="pt">f&aacute;bula</trd>
  <trd lng="ru">&c_s;&c_k;&c_a;&c_z;&c_k;&c_a;</trd>
  <trd lng="sv">saga</trd>
  <trd lng="sk">rozpr&aacute;vka</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_z;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_i;&c_g;&c_a;&c_d;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x795E;&#x8A71;&#x6545;&#x4E8B; <pr>sh&#xE9;nhu&#xE0;g&#xF9;sh&#xEC;</pr></trd>,
    <trd>&#x9B54;&#x5E7B;&#x6545;&#x4E8B; <pr>m&#xF3;hu&#xE0;ng&#xF9;sh&#xEC;</pr></trd>,
    <trd>&#x7AE5;&#x8A71; <pr>t&#xF3;nghu&#xE0;</pr></trd>,
    <trd>&#x7AE5;&#x8A71;&#x6545;&#x4E8B; <pr>t&#xF3;nghu&#xE0;g&#xF9;sh&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fabel.0a">
  <kap><tld/>a</kap>
  <snc mrk="fabel.0a.BELE">
    <dif>
      Apartenanta al fabelo: 
      <ekz>
        la arbaro <klr>[&#x2026;]</klr> estis kiel <tld/>a regno de silento kaj verdeflava
        krepusko
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la elektro <klr>[&#x2026;]</klr> &ccirc;esis esti iu sekreto, &scirc;losita en la <tld/>a kofro
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        vivecaj recitoj, en poezia formo, pri <tld/>aj heroa&jcirc;oj de <nom>Gilgame&scirc;</nom>
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve; <tld/>a historio poloj kaj rusoj devenas de du fratoj: <nom>Lech</nom> kaj
        <nom>Rus</nom>
        <fnt>
          <bib>SrT</bib>
          <lok>Libro XI.</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mistik.0a.neracia">mistika<sncref/></ref>,
      <ref cel="mit.0a">mita</ref>
    </refgrp>
    <trd lng="br">mojennel</trd>
    <trd lng="en">fabulous</trd>
    <trd lng="hu">mesebeli</trd>
    <trdgrp lng="pl">
      <trd>bajkowy</trd>,
      <trd>ba&sacute;niowy</trd>
    </trdgrp>
    <trd lng="sv">sago-</trd>
  </snc>
  <snc mrk="fabel.0a.FIG">
    <dif>
      <ref cel="ordina.ekster0a">Eksterordinara</ref>, <ref cel="kred.ne0ebla">nekredebla</ref>:
      <ekz>
        grandaj floroj kun <tld/>a lar&gcirc;a foliaro
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        li faras al Kastilujo kaj Leonujo preska&ubreve; <tld/>an proponon &mdash; ri&ccirc;i&gcirc;i per
        <klr>[&#x2026;]</klr> efektivigo de lia projekto
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiuj niaj samideanoj <klr>[&#x2026;]</klr> kies vortoj pri ia nova popolo eble &scirc;ajnis
        al vi tro <tld/>aj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista en Washington en la 15a de a&ubreve;gusto
          1910</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mirakl">mirakla</ref>,
      <ref cel="mir">miriga</ref>
    </refgrp>
    <trd lng="br">marzhus</trd>
    <trd lng="en">fabulous</trd>
    <trd lng="hu">mes&eacute;s</trd>
    <trd lng="pl">bajeczny</trd>
    <trd lng="sv">sagolik</trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_z;&c_a;&c_ch;&c_n;&c_y;</trd>
  <trd lng="bg">&c_p;&c_r;&c_i;&c_k;&c_a;&c_z;&c_je;&c_n;</trd>
  <trd lng="ca">fabul&oacute;s</trd>
  <trdgrp lng="cs">
    <trd>b&aacute;je&ccaron;n&yacute; <klr>(skv&ecaron;l&yacute;)</klr></trd>,
    <trd>poh&aacute;dkov&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>m&auml;rchenhaft</trd>,
    <trd>sagenhaft</trd>,
    <trd>fabelhaft</trd>
  </trdgrp>
  <trd lng="es">fabuloso</trd>
  <trdgrp lng="fr">
    <trd>fabuleux</trd>,
    <trd>l&eacute;gendaire</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x304A;&#x3068;&#x304E;&#x8A71;&#x306E;  <pr>&#x304A;&#x3068;&#x304E;&#x3070;&#x306A;&#x3057;&#x306E;</pr></trd>,
    <trd>&#x67B6;&#x7A7A;&#x306E;  <pr>&#x304B;&#x304F;&#x3046;&#x306E;</pr></trd>,
    <trd>&#x4FE1;&#x3058;&#x3089;&#x308C;&#x306A;&#x3044;&#x3088;&#x3046;&#x306A;  <pr>&#x3057;&#x3093;&#x3058;&#x3089;&#x308C;&#x306A;&#x3044;&#x3088;&#x3046;&#x306A;</pr></trd>,
    <trd>&#x9A5A;&#x304F;&#x307B;&#x3069;&#x306E;  <pr>&#x304A;&#x3069;&#x308D;&#x304F;&#x307B;&#x3069;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">fabelachtig</trd>
  <trd lng="ru">&c_s;&c_k;&c_a;&c_z;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">rozpr&aacute;vkov&yacute;</trd>
  <trd lng="uk">&c_k;&c_a;&c_z;&c_k;&c_o;&c_v;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x6781;&#x597D; <pr>j&#xED;h&#x1CE;o</pr></trd>,
    <trd>&#x7F8E;&#x5982;&#x7AE5;&#x8A71; <pr>m&#x11B;ir&#xFA;t&#xF3;nghu&#xE0;</pr></trd>,
    <trd>&#x5947;&#x5999; <pr>q&#xED;mi&#xE0;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fabel.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc>
    <dif>
      <ref cel="fantaz.0o.fantaziajxo">Fantazio</ref>, 
      <ref tip="prt" cel="fabel.0o.FIG"><tld/>o<sncref/></ref>:
      <ekz>
        <tld/>a&jcirc;oj, <tld/>a&jcirc;oj! &ccirc;u vi ne hontas
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>?
      </ekz>
      <ekz>
        konstruigi al mi domon <klr>[&#x2026;]</klr> por mi tio estas <tld/>a&jcirc;o
        <fnt>
          <bib>ChM</bib>
          <lok>7</lok>
        </fnt>;
      </ekz>
      <ekz>
        li &ccirc;iam povus ensor&ccirc;i &scirc;in per <tld/>a&jcirc;oj pri la soleco de homo
        <fnt><aut>Alberto Garc&iacute;a Fumero</aut>,
        <vrk><url ref="&Monato;/2000/006950.php">Alfa ondo</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/08, p. 23</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>M&auml;rchen <klr>(Unm&ouml;gliches)</klr></trd>,
    <trd>Fantasie <klr>(Unm&ouml;gliches)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x4F5C;&#x308A;&#x4E8B;  <pr>&#x3064;&#x304F;&#x308A;&#x3054;&#x3068;</pr></trd>
</drv>









<drv mrk="fabel.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="fabel.0aro.BELE">
    <uzo tip="fak">BELE</uzo>
    <dif>
      <ref tip="super" cel="kolekt.0o.libr">Kolekto</ref> de <tld/>oj:
      <ekz>
        &ccirc;i tiu libro donas al ni entute 19 ekzemplerojn el la
        <tld/>aro de <nom>Miyazawa</nom>
        <fnt>
          <aut>Don Harlow</aut>,
          <vrk><url ref="http://www.esperanto-usa.org/en/node/826">
          Recenzo: Goo&scirc; la '&ccirc;elisto</url></vrk>,
          <lok>2007-04-18</lok>
        </fnt>;
      </ekz>
      <ekz>
        la flutistoj estas kaj restas nevideblaj kaj &scirc;ajnas sin ka&scirc;i tie supre kiel
        koboldoj el <tld/>aro
        <fnt>
          <bib>IdO</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">recull de faules</trd>
  <trdgrp lng="cs">
    <trd>kniha poh&aacute;dek</trd>,
    <trd>sb&iacute;rka poh&aacute;dek</trd>
  </trdgrp>
  <trd lng="de">M&auml;rchensammlung</trd>
  <trd lng="fr">recueil de contes</trd>
  <trd lng="sk">zbierka rozpr&aacute;vok</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_z;&c_k;&c_i;</trd>,
    <trd>&c_z;&c_b;&c_ib;&c_r;&c_k;&c_a; &c_k;&c_a;&c_z;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="zh">&#x6545;&#x4E8B;&#x4E66; <pr>g&#xF9;shish&#x16B;</pr></trd>
</drv>
