  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_a;&c_z;&c_i;&c_k;</trd>,
    <trd>&c_v;&c_a;&c_n;&c_n;&c_o;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#30402; <pr>y&uacute;</pr></trd>,
    <trd>&#33080;&#30406; <pr>li&#462;np&eacute;n</pr></trd>,
    <trd>&#33225;&#30406; <pr>li&#462;np&eacute;n</pr></trd>,
    <trd>&#38754;&#30406; <pr>mi&agrave;np&eacute;n</pr></trd>,
    <trd>&#22823;&#32592; <pr>d&agrave;igu&agrave;n</pr></trd>,
