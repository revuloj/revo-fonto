<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kuv</rad>/o
</kap>
<drv mrk="kuv.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Granda nekovrita ujo, &gcirc;enerale rondforma, el ligno, metalo a&ubreve; &scirc;tono:     
      <ekz>
        <ind>lav<tld/>o</ind>,          
        <trd lng="br">gwalc'heris</trd>
        <trd lng="fr">lavabo</trd>
        <trd lng="hu">mosogat&oacute;</trd>
      <trdgrp lng="pt">
        <trd>tina</trd>,
        <trd>cuba</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind>ban<tld/>o</ind>,
        <trd lng="br">kibell</trd>       
        <trd lng="fr">baignoire</trd>
        <trd lng="hu">f&uuml;rd&odblac;k&aacute;d</trd>
        <trd lng="pt">banheira</trd>
      </ekz>
      <ekz>
        <ind>bapto<tld/></ind>o.
        <trd lng="hu">keresztel&odblac;medence</trd>
        <trd lng="pt">pia batismal</trd>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="tin.0o">tino</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_z;</trd>,
    <trd>&c_n;&c_a;&c_ch;&c_o;&c_w;&c_k;&c_ib;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_y;&c_t;&c_a;</trd>,
    <trd>&c_v;&c_a;&c_n;&c_n;&c_a;</trd>
  </trdgrp>                    
  <trd lng="br">barazh</trd>
  <trdgrp lng="de">
     <trd>Wanne</trd>,
     <trd>Bottich</trd>,
     <trd>Zuber</trd>,
     <trd>Kufe</trd>
  </trdgrp>          
  <trdgrp lng="en">
    <trd>tub</trd>,
    <trd>bath</trd>
  </trdgrp>
  <trd lng="es">cubo</trd>
  <trdgrp lng="fr">
    <trd>baquet</trd>,
    <trd>cuve</trd>
  </trdgrp>
  <trd lng="hu">k&aacute;d</trd>  
  <trdgrp lng="nl">
    <trd>kuip</trd>,
    <trd>tobbe</trd>,
    <trd>teil</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_a;&c_z;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_y;&c_t;&c_o;</trd>,
    <trd>&c_k;&c_u;&c_p;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_v;&c_a;&c_n;&c_n;&c_a;</trd>
  </trdgrp>
</drv>
</art>
