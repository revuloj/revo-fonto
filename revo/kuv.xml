<drv mrk="kuv.bapto0o">
  <kap>bapto<tld/>o</kap>
  <snc mrk="kuv.bapto0o.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      <tld lit="K"/>o en kiu okazas baptaj akvumoj;
      <ref tip="sin" cel="bapt.0ujo">baptujo</ref>:
      <ekz>
        maldekstre estas origina malfrua gotika bapto<tld/>o de komenco
        de 16-a jarcento
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=134356">Pre&gcirc;ejo
          de sankta Katarino (Bansk&aacute; &Scaron;tiavnica)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_p;&c_je;&c_l;&c_mol;</trd>
  <trdgrp lng="br">
    <trd>maen-badez</trd>,
    <trd>maen-font</trd>
  </trdgrp>
  <trd lng="cs">k&rcaron;titelnice</trd>
  <trd lng="de">Taufbecken</trd>
  <trd lng="en"><ind>baptism</ind>al bath</trd>
  <trd lng="fr">fonts baptismaux</trd>
  <trd lng="hu">keresztel&odblac;medence</trd>
  <trdgrp lng="id">
    <trd><ind>bak</ind> pembaptisan</trd>,
    <trd><ind>kolam</ind> baptis</trd>
  </trdgrp>
  <trd lng="ja">&#27927;&#31036;&#30436;  <pr>&#12379;&#12435;&#12428;&#12356;&#12400;&#12435;</pr></trd>
  <trdgrp lng="nl">
    <trd>doopvont</trd>,
    <trd>doopbekken</trd>,
    <trd>vont</trd>
  </trdgrp>
  <trd lng="pl">chrzcielnica</trd>
  <trd lng="pt">pia batismal</trd>
  <trd lng="ro">cristelni&#539;&abreve;</trd>
  <trd lng="ru">&c_k;&c_u;&c_p;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">krstite&lcaron;nica</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_u;&c_p;&c_ib;&c_l;&c_mol;</trd>,
    <trd>&c_h;&c_r;&c_je;&c_s;&c_t;&c_i;&c_l;&c_mol;&c_n;&c_i;&c_c;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="kuv.lavo0o">
  <kap>lavo<tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolumo</lok>
        </fnt>,
        <var><kap>lav<tld/>o</kap></var>
  </kap>
  <snc mrk="kuv.lavo0o.HEJM">
    <dif>
      <tld lit="K"/>o ordinare provizita per
      krano kaj elfluejo destinita al permana
      lavado:
      <ekz>
        kiam ni ankora&ubreve; estis tre malgrandaj, la vazaro jam ne plu estis lavata en la
        blu&scirc;tona lavo<tld/>o, sed en emajlita pelvo
        <fnt>
          <aut>H. De Cooman</aut>
          <vrk><url ref="https://de-cooman.be/wp/eo/tag/purigado/">Vazarlavado</url></vrk>
          <lok>2018</lok>
        </fnt>;
      </ekz>
      <ekz>
        en &ccirc;iu &ccirc;ambro estas lav<tld/>o, sed &scirc;ranko nur en kelkaj &ccirc;ambroj
        <fnt>
          <aut>B. Schumann</aut>
          <vrk><url ref="http://bertosch.free.fr/iik2008/">39a Internacia Infana
          Kongreseto</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="lav.0ujo.KOMUNE">lavujo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_u;&c_k;&c_a;&c_m;&c_y;&c_j;&c_n;&c_ib;&c_c;&c_a;</trd>,
    <trd>&c_u;&c_m;&c_y;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="br">gwalc'heris</trd>
  <trdgrp lng="cs">
    <trd>d&rcaron;ez</trd>,
    <trd>necky</trd>,
    <trd>n&aacute;dr&zcaron; na pran&iacute;</trd>,
    <trd>vana na koup&aacute;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Waschsch&uuml;ssel</trd>,
    <trd>Zuber</trd>,
    <trd>B&uuml;tte</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>sink</trd>,
    <trd>basin <klr>(UK)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;vier</trd>,
    <trd>lavabo</trd>
  </trdgrp>
  <trd lng="hu">mosogat&oacute;</trd>
  <trdgrp lng="id">
    <trd><ind>bak</ind> cuci</trd>,
    <trd>wastafel</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#27927;&#28655;&#26742;  <pr>&#12379;&#12435;&#12383;&#12367;&#12362;&#12369;</pr></trd>,
    <trd>&#12383;&#12425;&#12356;</trd>,
    <trd>&#27969;&#12375;  <pr>&#12394;&#12364;&#12375;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>wasbak</trd>,
    <trd>wastafel</trd>
  </trdgrp>
  <trd lng="pl">umywalka</trd>
  <trdgrp lng="pt">
    <trd>tina</trd>,
    <trd>cuba</trd>
  </trdgrp>
  <trd lng="ro">chiuvet&abreve;</trd>
  <trdgrp lng="sk">
    <trd>drez</trd>,
    <trd>um&yacute;vadlo</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#38754;&#30406; <pr>mi&agrave;np&eacute;n</pr></trd>,
    <trd>&#32025;&#28479;&#27744; <pr>zh&#464;ji&amacron;ngch&iacute;</pr></trd>,
    <trd>&#32440;&#27974;&#27744; <pr>zh&#464;ji&amacron;ngch&iacute;</pr></trd>,
    <trd>&#27927;&#33080;&#30406; <pr>x&#464;li&#462;np&eacute;n</pr></trd>,
    <trd>&#27927;&#33225;&#30406; <pr>x&#464;li&#462;np&eacute;n</pr></trd>
  </trdgrp>
</drv>
