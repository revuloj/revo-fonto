<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kuv</rad>/o
</kap>
<drv mrk="kuv.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Granda nekovrita ujo, &gcirc;enerale rondforma, el ligno, metalo a&ubreve; &scirc;tono:
      <ekz>
        lav<tld/>o, ban<tld/>o, bapto<tld/>o.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="tin.0o">tino</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Wanne</trd>,
     <trd>Bottich</trd>,
     <trd>Zuber</trd>,
     <trd>Kufe</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>tub</trd>,
    <trd>bath</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kuip</trd>,
    <trd>tobbe</trd>,
    <trd>teil</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_a;&c_z;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_y;&c_t;&c_o;</trd>,
    <trd>&c_k;&c_u;&c_p;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_v;&c_a;&c_n;&c_n;&c_a;</trd>
  </trdgrp>
</drv>
</art>
