<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: enant.xml,v 1.21 2023/08/31 11:49:29 revo Exp $">
<kap>
<rad>enant</rad>/o
</kap>
<drv mrk="enant.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">Oenanthe</trd>), kies unu specio estas
      <ref tip="sub" cel="feland.0o">felandrio</ref>:
      <ekz>
        la plej multaj specioj de <tld/>o kreskas sur malseka grundo, en mar&ccirc;oj a&ubreve; e&ccirc; en akvo;
      </ekz>
      <ekz>
        sciencistoj eltrovis, ke la Safranflava <tld/>o estis uzata de antikvaj sardoj por venenigi
        maljunulojn kaj krimulojn 
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="cikut.0o">cikuto</ref>,
       <ref cel="koni.0o">konio</ref>,
       <ref cel="sardon.0a_rido">sardona rido</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_je;&c_zh;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="ca">
    <trd>c&ograve;lits</trd>,
    <trd>g&egrave;nere Oenanthe</trd>
  </trdgrp>
  <trd lng="cs">chaluha</trd>
  <trdgrp lng="de">
    <trd>Pferdesaat</trd>,
    <trd>Wasserfenchel</trd>
  </trdgrp>
  <trd lng="en">water dropworts</trd>
  <trd lng="fa">&a_A_madda;&a_b;&zwnj;&f_ch;&f_k;&a_A;&a_n;</trd>
  <trd lng="fr">&oelig;nanthe</trd>
  <trd lng="hu">borgy&ouml;k&eacute;r</trd>
  <trdgrp lng="ja">
    <trd>&#x30BB;&#x30EA;</trd>,
    <trd>&#x30CF;&#x30B7;&#x30B0;&#x30ED;&#x30D2;&#x30BF;&#x30AD;</trd>
  </trdgrp>
  <trd lng="pl">kropid&lstroke;o</trd>
  <trdgrp lng="pt">
    <trd>enante</trd>,
    <trd>enanto</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_m;&c_je;&c_zh;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">chaluha</trd>
  <trd lng="uk">&#x43E;&#x43C;&c_je;&c_g;</trd>
</drv>
</art>
