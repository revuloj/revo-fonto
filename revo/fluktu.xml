<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>fluktu</rad>/i
</kap>
<drv mrk="fluktu.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc num="1">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Al- k reflui: <ekz>ulceroj <tld/>as.</ekz>
    </dif>
    <trd lng="de">fluktuieren</trd>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Alterne varii:
      <ekz><tld/>ado de la opinioj, de la borskurzoj</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="sxveb.0i">&scirc;vebi 5</ref>.
    </refgrp>
    <trdgrp lng="de">
     <trd>schwanken</trd>,
     <trd>wechseln</trd></trdgrp>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_f;&c_l;&c_ju;&c_k;&c_t;&c_u;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_je;&c_b;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
