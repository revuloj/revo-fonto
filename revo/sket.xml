<vortaro>
  <art mrk="$Id: sket.xml,v 1.3 2024/09/10 21:27:16 afido Exp $">
    <kap>
      <rad>sket</rad>/i
    </kap>

<drv mrk="sket.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">SPO</uzo>
  <snc mrk="sket.0i.glitiri">
    <dif>
      <ref tip="dif" cel="glit.0i.antauxenigxi">Glitiri</ref> per glit&scirc;uoj,
      rul&scirc;uoj, rultabulo ks:
      <ekz>
        la vintroj estis malvarmaj kun multa ne&gcirc;o, <klr>[&#8230;]</klr> ni povis multe sledi,
        skii kaj <tld/>i
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan MAUL</aut>
          <vrk><url ref="&Monato;/2003/008190.html"
              >Varmego suferiga</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>antoj <klr>[&#8230;]</klr> no&ccirc;as glacion
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="&Monato;/2010/009883.php"
              >Trancporto</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ejo staras je dispono de sportemuloj
        <fnt>
          <bib>Monato</bib>
          <aut>Mireille Grosjean</aut>
          <vrk><url ref="&Monato;/2015/011395.php"
              >Urbo kun forta studemo</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni povas anka&ubreve; lui <klr>[&#8230;]</klr> <tld/>ilojn
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2009/009812.php"
              >&Gcirc;ui simplajn &scirc;atokupojn sub&ccirc;iele</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>skaten</trd>,
      <trd>eislaufen</trd>
    </trdgrp>
    <trd lng="en">skate</trd>
    <trd lng="fr">faire du skate</trd>
    <trd lng="pl">&sacute;lizga&cacute; si&eogonek;  <klr>(&lstroke;y&zdot;wy, rolki, deskorolka)</klr></trd>
  </snc>
  <trdgrp lng="be">
     <trd>&c_s;&c_mol;&c_l;&c_ib;&c_z;&c_g;&c_a;&c_c;&c_c;&c_a;</trd>,
     <trd>&c_k;&c_o;&c_w;&c_z;&c_a;&c_c;&c_c;&c_a;</trd>,
     <trd>&c_k;&c_a;&c_t;&c_a;&c_c;&c_c;&c_a; <klr>(&c_n;&c_a; &c_k;&c_a;&c_n;&c_mol;&c_k;&c_o;&c_h;)</klr></trd>
   </trdgrp>
</drv>
  </art>

