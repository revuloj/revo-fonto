<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>oka</rad>/o
</kap>

<drv mrk="oka1.0o">
  <kap><tld/>o</kap>
  <snc mrk="oka1.0o.BOT">
    <uzo tip="fak">AGR</uzo>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Sudamerika <ref tip="super" cel="veget.0ajxo">vegeta&jcirc;o</ref> el 
      la Andoj, kun man&gcirc;eblaj radikotuberetoj de
      diversaj koloroj la&ubreve; la specoj, specio el la genro <ref
      tip="super"
      cel="oksali.0o.BOT">oksalido</ref> (<trd lng="la">Oxalis
      tuberosa</trd>):
      <ekz>
        sur la terasoj <klr>...</klr> kreskis <klr>...</klr> kukurbo,
        tomato, ternukso kaj <tld/>o
        <fnt>
        <bib>Viki</bib>,
        <lok>inkaa imperio, 2005-07-09</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>knolliger Sauerklee</trd>,
    <trd>Oka</trd>,
    <trd>Yam</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>oca du P&eacute;rou</trd>,
    <trd>truffette acide</trd>,
    <trd>urelle tub&eacute;reuse</trd>
  </trdgrp>
  <trd lng="nl">oca</trd>
  <trd lng="pt">oca <klr tip="amb">(bot.)'</klr></trd>
</drv>
</art>

