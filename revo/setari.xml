<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>setari</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="setari.0o">
  <kap><tld/>o</kap>
  <snc mrk="setari.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Neda&ubreve;ra herbo kun rigidaj haroj, genro
      el familio <ref tip="super" cel="poac.0oj.BOT">poacoj</ref>:
      <ekz>
        &ccirc;iutage &scirc;i man&gcirc;is riza&jcirc;on kun <tld/>o
        <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>&leftquot;iun tagon por mortigi
        tiun
        memoron&rightquot;, p. 236a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>millet<klr tip="amb"> (&agrave; grappes, d'italie...)</klr></trd>,
    <trd>s&eacute;taire</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>muhar</trd>,
    <trd>mohar</trd>
  </trdgrp>
  <trd lng="it">setaria</trd>
</drv>
</art>

