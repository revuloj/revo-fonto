	<trd lng="be">&c_d;&c_y;&c_jo;&c_p;&c_t;&c_r;&c_y;&c_ja;</trd>
	<trdgrp lng="de">
		<trd>Dioptrie</trd>,
		<trd>Linsenbrechwert</trd>
	</trdgrp>
	<trd lng="es">dioptr&iacute;a</trd>
	<trd lng="fr">dioptrie</trd>
	<trd lng="hu">dioptria</trd>
	<trd lng="nl">dioptrie</trd>
	<trd lng="ru">&c_d;&c_i;&c_o;&c_p;&c_t;&c_r;&c_i;&c_ja;</trd>
</drv>
</art>
