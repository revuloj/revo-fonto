<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: etilen.xml,v 1.17 2019/06/30 18:19:02 revo Exp $">
<kap>
<rad>etilen</rad>/o
</kap>
<drv mrk="etilen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      <frm>C<sub>2</sub>H<sub>4</sub></frm>, senkolora <ref tip="super" cel="gas.0o.FIZ">gaso</ref>,
      kiun oni ricevas per distilado de karbo a&ubreve; nafto:
      <ekz>
        oni produktas <tld/>on el nafto, el etileno polietilenon kaj el tiu la diversajn
        objektojn el plasto
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2014/011143.php"
              >Plasto planta</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_t;&c_y;&c_l;&c_je;&c_n;</trd>
  <trd lng="cs">etylen</trd>
  <trdgrp lng="de">
     <trd>&Auml;thylen</trd>,
     <trd>&Auml;then</trd>
  </trdgrp>
  <trd lng="en">ethylene</trd>
  <trd lng="es">etileno</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_t;&a_n;</trd>,
    <trd>&a_A;&a_t;&f_y;&a_l;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">&eacute;thyl&egrave;ne</trd>
  <trd lng="hu">etil&eacute;n</trd>
  <trd lng="pl">etylen</trd>
  <trd lng="pt">etileno</trd>
  <trd lng="ru">&c_e;&c_t;&c_i;&c_l;&c_je;&c_n;</trd>
  <trd lng="sk">etyl&eacute;n</trd>
</drv>
</art>
