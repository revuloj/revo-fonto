<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: loteri.xml,v 1.25 2025/11/15 20:29:34 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>loteri</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="loteri.0o">
  <kap><tld/>o</kap>
  <snc mrk="loteri.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="super" cel="lud.hazard0o">Hazardludo</ref>
      la&ubreve; kiu la partoprenantoj ricevas numeritajn biletojn,
      kiuj rajtigas ilin al &scirc;ancoj pri gajno montrota de la sorto.
      <ekz>
        50000 frankojn <klr>[mi&#x2026;]</klr> gajnis en la <tld/>o
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>!
      </ekz>
      <ekz>
        mi a&ccirc;etis bileton de <tld/>o, kaj sur &gcirc;in falis la granda premio
        <fnt>
          <bib>IdO</bib>
          <lok>Kvara Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Gewinnspiel</trd>,
      <trd>Verlosung</trd>
    </trdgrp>
  </snc>
  <snc mrk="loteri.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Afero de <ref cel="hazard.0o">hazardo</ref>:
      <ekz>
        edzi&gcirc;o estas nura <tld/>o;
      </ekz>
      <ekz>
        <klr>[la]</klr> diverskolora <tld/>o de la vivo, en kiu multaj metas en la
        ludon sian senkulpecon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum kelkaj semajnoj voja&gcirc;i per trajno fari&gcirc;is <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk>Politiko elreligita</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la minado <klr>[de bitmono]</klr> estas <tld/>o
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Gl&uuml;cksspiel</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_t;&c_a;&c_r;&c_e;&c_ja;</trd>
  <trd lng="cs">loterie</trd>
  <trdgrp lng="de">
    <trd>Lotterie</trd>
  </trdgrp>
  <trd lng="en">lottery</trd>
  <trd lng="es">loter&iacute;a</trd>
  <trd lng="fr">loterie</trd>
  <trd lng="hu">lott&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x5B9D;&#x304F;&#x3058;  <pr>&#x305F;&#x304B;&#x3089;&#x304F;&#x3058;</pr></trd>,
    <trd>&#x5BCC;&#x304F;&#x3058;  <pr>&#x3068;&#x307F;&#x304F;&#x3058;</pr></trd>,
    <trd>&#x904B;  <pr>&#x3046;&#x3093;</pr></trd>,
    <trd>&#x5DE1;&#x308A;&#x5408;&#x308F;&#x305B;  <pr>&#x3081;&#x3050;&#x308A;&#x3042;&#x308F;&#x305B;</pr></trd>
  </trdgrp>
  <trd lng="nl">loterij</trd>
  <trd lng="pl">loteria</trd>
  <trd lng="pt">loteria</trd>
  <trd lng="ro">loterie</trd>
  <trd lng="ru">&c_l;&c_o;&c_t;&c_je;&c_r;&c_je;&c_ja;</trd>
  <trd lng="sk">lot&eacute;ria</trd>
  <trd lng="uk">&c_l;&c_o;&c_t;&c_je;&c_r;&c_je;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#x6478;&#x5F69; <pr>m&omacron;c&#x1CE;i</pr></trd>,
    <trd>&#x6416;&#x734E; <pr>y&aacute;oji&#x1CE;ng</pr></trd>,
    <trd>&#x6447;&#x5956; <pr>y&aacute;oji&#x1CE;ng</pr></trd>,
    <trd>&#x62BD;&#x5956; <pr>ch&omacron;uji&#x1CE;ng</pr></trd>,
    <trd>&#x62BD;&#x734E; <pr>ch&omacron;uji&#x1CE;ng</pr></trd>,
    <trd>&#x535A;&#x5F69; <pr>b&oacute;c&#x1CE;i</pr></trd>,
    <trd>&#x5E78;&#x8FD0;&#x62BD;&#x5956; <pr>x&igrave;ngy&ugrave;nch&omacron;uji&#x1CE;ng</pr></trd>,
    <trd>&#x5E78;&#x904B;&#x62BD;&#x734E; <pr>x&igrave;ngy&ugrave;nch&omacron;uji&#x1CE;ng</pr></trd>,
    <trd>&#x6416;&#x865F; <pr>y&aacute;oh&agrave;o</pr></trd>,
    <trd>&#x6447;&#x53F7; <pr>y&aacute;oh&agrave;o</pr></trd>,
    <trd>&#x4E50;&#x900F; <pr>l&egrave;t&ograve;u</pr></trd>,
    <trd>&#x6A02;&#x900F; <pr>l&egrave;t&ograve;u</pr></trd>
  </trdgrp>
</drv>
</art>
