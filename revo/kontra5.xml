<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kontra5.xml,v 1.17 2023/09/01 15:46:43 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>kontralt</rad>/o <fnt><bib>FK</bib></fnt>
</kap>
<drv mrk="kontra5.0o">
  <kap><tld/>o</kap>
  <snc mrk="kontra5.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      La plej malalta el la virinaj vo&ccirc;oj:
      <ekz>
        <nom>Maria Radner</nom> <klr>[&#x2026;]</klr> estis Germana <tld/>o kaj operkantistino
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=520305">Maria Radner</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Martha</nom>, la edzino de <nom>Bertrand</nom>, la nutristino de <nom>Iolanta</nom> (kontralto)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=65677">Iolanta</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_t;&c_r;&c_a;&c_l;&c_mol;&c_t;&c_a;</trd>
  <trd lng="ca">contralt</trd>
  <trdgrp lng="de">
    <trd>Kontraalt</trd>,
    <trd>tiefer <ind>Alt</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>alto</trd>,
    <trd>contralto</trd>
  </trdgrp>
  <trd lng="es">contralto</trd>
  <trd lng="fr">contralto</trd>
  <trd lng="hu">alt</trd>
  <trd lng="ja">&#x30B3;&#x30F3;&#x30C8;&#x30E9;&#x30EB;&#x30C8;</trd>
  <trdgrp lng="nl">
    <trd>contra-alt</trd>,
    <trd>lage <ind>alt</ind></trd>
  </trdgrp>
  <trd lng="pl">kontralt</trd>
  <trd lng="pt">contralto</trd>
  <trd lng="ro">contralto</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_t;&c_r;&c_a;&c_l;&c_mol;&c_t;&c_o;</trd>
</drv>
</art>
