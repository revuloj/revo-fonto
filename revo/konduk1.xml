<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>konduktanc</rad>/o
</kap>
<drv mrk="konduk1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      Propreco konduki la elektron inverse al la elektra
      <ref tip="ant" cel="rezist1.0o">rezistanco</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_v;&c_o;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="de">Leitwert</trd>
  <trd lng="en">conductance</trd>
  <trdgrp lng="hu">
    <trd>vezet&odblac;k&eacute;pess&eacute;g</trd>,
    <trd>konduktancia</trd></trdgrp>
  <trd lng="nl">conductantie</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;&c_i;&c_m;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>
</art>
