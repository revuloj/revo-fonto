<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: konduk1.xml,v 1.20 2023/12/03 09:58:55 revo Exp $">
<kap>
  <rad>konduktanc</rad>/o
</kap>
<drv mrk="konduk1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="grand.fizika0o">Fizika grando</ref>
      inversa al la  <ref cel="rezist1.0o">rezistanco</ref>
      de elektra cirkvito:
      <ekz>
        granda <tld/>o de metaloj estas pro tio, ke en ili estas grandega kvanto da
        liberaj kurenteroj &dash; elektronoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=45028">Elektra rezistanco</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">
  &c_p;&c_r;&c_a;&c_v;&c_o;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="ca">conduct&agrave;ncia</trd>
  <trdgrp lng="cs">
    <trd>konduktance</trd>,
    <trd>vodivost</trd>
  </trdgrp>
  <trd lng="de">Leitwert</trd>
  <trd lng="en">conductance</trd>
  <trd lng="es">conductancia</trd>
  <trd lng="fa">&a_r;&a_s;&a_A;&a_n;&a_A;&f_y;&f_y;<klr tip="ind"> (&a_b;&a_r;&a_q;)</klr></trd>
  <trd lng="fr">conductance</trd>
  <trdgrp lng="hu">
    <trd>vezet&odblac;k&eacute;pess&eacute;g</trd>,
    <trd>konduktancia</trd></trdgrp>
  <trd lng="ja">&#x30B3;&#x30F3;&#x30C0;&#x30AF;&#x30BF;&#x30F3;&#x30B9;</trd>
  <trd lng="nl">conductantie</trd>
  <trdgrp lng="pl">
    <trd>konduktancja</trd>,
    <trd>przewodno&sacute;&cacute;<klr> elektryczna</klr></trd>
  </trdgrp>
  <trd lng="pt">condut&acirc;ncia</trd>
  <trd lng="ru">
  &c_p;&c_r;&c_o;&c_v;&c_o;&c_d;&c_i;&c_m;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">vodivos&tcaron;</trd>
  <trd lng="zh">&#x7535;&#x5BFC; <pr>di&#xE0;nd&#x1CE;o</pr></trd>
</drv>
</art>
