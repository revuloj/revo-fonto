<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>detal</rad>/o
</kap>
<drv mrk="detal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Malgranda parto, konsiderata  kontraste al la tuto:
      <ekz>
        zorge raportu pri &ccirc;iuj <tld/>oj;
      </ekz>
      <ekz>
        mi forgesis multajn <tld/>ojn de la afero.
      </ekz>
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Malgrava&jcirc;o, akcesora&jcirc;o:
      <ekz>
        droni en <tld/>oj
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
      </ekz>
      <ekz>
        bagatelaj eksteraj <tld/>a&jcirc;oj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la &ccirc;iutagaj <tld/>a&jcirc;oj.
      </ekz>
    </dif>
    <trd lng="ru">&c_m;&c_je;&c_l;&c_o;&c_ch;&c_mol;</trd>
  </snc>
  <trdgrp lng="de">
    <trd>Einzelheit</trd>,
    <trd>Detail</trd>
  </trdgrp>
  <trd lng="en">detail</trd>
  <trd lng="fr">d&eacute;tail</trd>
  <trd lng="nl">detail</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_t;&c_a;&c_l;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_r;&c_o;&c_b;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="detal.0ado">
  <kap><tld/>ado</kap>
  <snc num="1">
    <dif>
      Preciza kaj aparta konsiderado pri &ccirc;iuj partoj de tuto:
      <ekz>
        fari la <tld/>adon de konto.
      </ekz>
    </dif>
    <trd lng="ru">&c_d;&c_je;&c_t;&c_a;&c_l;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;</trd>
  </snc>
  <snc num="2">
    <dif>
      Pomalgranda vendado 
    </dif>
    <refgrp tip="sin">
      <ref cel="debit.0o">debito</ref>.
    </refgrp>
  </snc>
</drv>
<drv mrk="detal.0a">
  <kap><tld/>a</kap>
  <snc num="1">
    <dif>
      Poparta, iompostioma:
      <ekz>
        repreni <tld/>e, kian oni donis mal<tld/>e.
      </ekz>
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Enhavanta &ccirc;iujn <tld/>ojn, preciza k kompleta:
      <ekz>
        <tld/>a analizo de verko<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>a fakturo, rakonto.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_d;&c_je;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_p;&c_o;&c_d;&c_r;&c_o;&c_b;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc num="3">
    <dif>
      Vendanta pomalgrande:
      <ekz>
        <tld/>a komerco.
      </ekz>
    </dif>
    <trd lng="ru">&c_r;&c_o;&c_z;&c_n;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc num="4">
    <dif>
      Akcesora, malgrava:
      <ekz>
        <tld/>a eraro.
      </ekz>
    </dif>
  </snc>
</drv>
<drv mrk="detal.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      <tld/>vendisto. 
    </dif>
  </snc>
</drv>
<drv mrk="detal.mal0a">
  <kap>mal<tld/>a</kap>
  <snc num="1">
    <dif>
      &Gcirc;enerala, sen<tld/>a:
      <ekz>
        rakonti la&ubreve; mal<tld/>aj trajtoj.
      </ekz>
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Pogranda.  
    </dif>
    <trd lng="ru">&c_o;&c_p;&c_t;&c_o;&c_v;&c_y;&c_j;</trd>
  </snc>
</drv>
<drv mrk="detal.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1">
    <dif>
      Komplete fari, konsiderante aparte &ccirc;iujn partojn:
      <ekz>
        li <tld/>is la historion de esp-o de 1887 &gcirc;is 1900.
      </ekz>
    </dif>
    <trd lng="ru">&c_d;&c_je;&c_t;&c_a;&c_l;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc num="2">
    <dif>
      Pomalgrande vendi:
      <ekz>
        tie &ccirc;i oni ne <tld/>as la &scirc;tofpecojn.
      </ekz>
    </dif>
  </snc>
</drv>
</art>
