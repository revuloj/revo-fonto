</drv>

<drv mrk="detal.po0a">
  <kap>
    po<tld/>a
  </kap>
  <snc mrk="detal.po0a.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref cel="detal.0a.EKON">Detala<sncref/></ref>:
      <ekz>
        <ind>po<tld/>a vendo</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2009/009796.php">Nova ekologia vila&gcirc;o</url></vrk>
          <lok>2009</lok>
        </fnt>;
        <trd lng="de">Einzelhandel</trd>
