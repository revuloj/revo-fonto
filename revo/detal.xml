      </ekz>
    </dif>
    <trd lng="pl">hurtowo</trd>
  <trdgrp lng="be">
    <trd>&c_g;&c_u;&c_r;&c_t;&c_o;&c_m;</trd>,
    <trd>&c_o;&c_p;&c_t;&c_a;&c_m;</trd>
  </trdgrp>
  </snc>
  <trd lng="cs">hrub&ecaron;</trd>
