<drv mrk="detal.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="detal.0ajxo.ARK">
    <dif>
      <ref tip="dif" cel="detal.0o.malgravajxo"><tld lit="D"/>o<sncref/></ref>:
      <ekz>
        koncerne la <tld/>a&jcirc;ojn oni jam tiam
        vidis tion, kion oni vidas nun
        <fnt><bib>Fab4</bib>,
        <lok>Filino de la re&gcirc;a mar&ccirc;o</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i estis <klr>[&#x2026;]</klr> tro nekompetenta
        pri tiu svarmo da &ccirc;iutagaj <tld/>a&jcirc;oj
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        mi demandos pri kelkaj neeviteblaj <tld/>a&jcirc;oj
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> demandis nur pri <tld/>a&jcirc;oj
        <fnt><bib>Metrop</bib>, <lok>&ccirc;apitro 7</lok></fnt>.
      </ekz>
    </dif>
    <rim>
      &Scirc;ajnas, ke mankas ekzemploj kun la
      senco <ref cel="detal.0o.precizajxo"><tld/>o <sncref/></ref>,
      sed povas esti, ke la unua citita ekzemplo celas &gcirc;in.
      La la&ubreve;devena adjektiva karaktero de la radiko,
      ver&scirc;ajne instigis al uzo de <em><tld/>a&jcirc;o</em>
      en pli vasta senco, ol &gcirc;i havas hodia&ubreve;.
    </rim>
    <trdgrp lng="pl">
      <trd>detal</trd>,
      <trd>drobiazg</trd>,
      <trd>szczeg&oacute;&lstroke;</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_o;&c_d;&c_r;&c_o;&c_b;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_r;&c_a;&c_b;&c_ja;&c_z;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="he">&pe;&resh;&tet;</trd>
</drv>

