<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>al&hcirc;emi</rad>/o
</kap>
<drv mrk="alhxem.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Okulta scienco, kiu per &hcirc;emiaj procedoj kaj magiaj formuloj 
      ser&ccirc;is la &scirc;tonon de la sa&gcirc;uloj kaj aliajn &hcirc;imerojn.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Alchemie</trd>,
    <trd>Alchimie</trd>,
    <trd>Goldmacherkunst</trd>
  </trdgrp>
  <trd lng="fr">alchimie</trd>
  <trdgrp lng="nl">
    <trd>alchemie</trd>,
    <trd>alchimie</trd>
  </trdgrp>
  <trd lng="pt">alquimia</trd>
  <trd lng="ru">&c_a;&c_l;&c_h;&c_i;&c_m;&c_i;&c_ja;</trd>
</drv>

<drv mrk="alhxem.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Persono kiu praktikas <tld/>on.
    </dif>
  </snc>
  <trd lng="de">Alchimist</trd>
  <trdgrp lng="nl">
    <trd>alchemist</trd>,
    <trd>alchimist</trd>
  </trdgrp>
  <trd lng="pt">alquimista</trd>
</drv>
</art>
