<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: bomben.xml,v 1.2 2025/03/03 12:53:26 afido Exp $">
    <kap>
      <rad>bomben</rad>/o
      <fnt><bib>PIV2</bib></fnt>,
      <var><kap><rad var="ator">bombinator</rad>/o <fnt><bib>JED</bib></fnt></kap></var>
    </kap>

<drv mrk="bomben.0o">
  <kap><tld/>o, <var><kap><tld var="ator"/>o</kap></var></kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">Genro</ref> de malgrandaj
      <ref tip="malprt" cel="anur.0oj">anuroj</ref> kun avertkolora ventro (<trd
      lng="la">Bombina</trd>):
      <ekz>
        flavventra <tld/>o precipe vivas en malsekaj herbejoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=542203"
              >Malsekherbejo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        Okul&#8217; de <tld/>o <klr>[&#8230;]</klr> &#11005; Tiel flosas mi sur la ondo
        <fnt>
          <aut>Oda &Scirc;efer</aut>
          <vrk><url
          ref="https://www.reddit.com/r/Esperanto/comments/1fsdk1a/tradukita_versa%C4%B5o_oda_%C5%9Defer_la_ensor%C4%89itino_oda/"
              >La ensor&ccirc;itino</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/4/4f/Bombina_bombina_1_(Marek_Szczepanek)_tight_crop.jpg/320px-Bombina_bombina_1_(Marek_Szczepanek)_tight_crop.jpg"
      prm="CC BY-SA 3.0"
      ><tld/>o (Bombina bombina)
        <fnt>
          <aut>Marek Szczepanek, File:Bombina_bombina_1_(Marek_Szczepanek).jpg</aut>
          <lok><url ref="&WCW;/File:Bombina_bombina_1_(Marek_Szczepanek)_tight_crop.jpg"
              >https://commons.wikimedia.org/wiki/File:Bombina_bombina_1_(Marek_Szczepanek)_tight_crop.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Unke</trd>
    <trd lng="en">fire-bellied toad</trd>
    <trd lng="ru">&c_zh;&c_je;&c_r;&c_l;&c_ja;&c_n;&c_k;&c_i; <klr>(&c_r;&c_o;&c_d;)</klr></trd>
  </snc>
<trd lng="be">&c_zh;&c_a;&c_r;&c_l;&c_ja;&c_n;&c_k;&c_a;</trd>
</drv>

  </art>

