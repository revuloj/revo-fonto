<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: akropol.xml,v 1.10 2015/07/04 08:37:30 revo Exp $">
<kap>
  <rad>akropol</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="akropol.0o">
  <kap><tld/>o</kap>
  <snc mrk="akropol.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Pli alta, fortika parto de antikvaj mediteraneaj kaj okcidentaziaj
      <ref tip="malprt" cel="urb.0o.ARKI">urboj</ref>, kun sanktejoj:
      <ekz>
        tiu estis popolo, kiu vere kapablis krei la <tld lit="A"/>on
        <fnt><bib>SatirRak</bib>, <lok>Logos</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_r;&c_o;&c_p;&c_a;&c_l;&c_mol;</trd>
  <trd lng="ca">acr&ograve;poli</trd>
  <trdgrp lng="cs">
    <trd>akropole</trd>,
    <trd>akropolis</trd>,
    <trd>posvátný vrch</trd>
  </trdgrp>
  <trd lng="de">Akropolis</trd>
  <trd lng="es">acr&oacute;polis</trd>
  <trd lng="fr">acropole</trd>
  <trd lng="hu">akropolisz</trd>
  <trd lng="nl">acropolis</trd>
  <trdgrp lng="pt">
   <trd>acr&oacute;pole</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_k;&c_r;&c_o;&c_p;&c_o;&c_l;&c_mol;</trd>
  <trd lng="sk">akropola</trd>
</drv>
</art>

