<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>akropol</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="akropol.0o">
  <kap><tld/>o</kap>
  <snc mrk="akropol.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Pli alta, fortika parto de antikvaj mediteraneaj kaj okcidentaziaj
      <ref tip="malprt" cel="urb.0o.ARKI">urboj</ref>, kun sanktejoj:
      <ekz>
        tiu estis popolo, kiu vere kapablis krei la <tld lit="A"/>on
        <fnt><bib>SatirRak</bib>, <lok>Logos</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_r;&c_o;&c_p;&c_a;&c_l;&c_mol;</trd>
  <trd lng="ca">acr&ograve;poli</trd>
  <trd lng="es">acr&oacute;polis</trd>
  <trd lng="fr">acropole</trd>
</drv>
</art>

