<drv mrk="ole.fisx0o">
  <kap><ofc>*</ofc>fi&scirc;<tld/>o
  <fnt><bib>UV</bib>, <lok><url ref="&AdE;/fundamento/universala_vortaro.html#av-5025">fi&scirc;'ole'o</url></lok></fnt>
  </kap>
  <snc mrk="ole.fisx0o.KOMUNE">
    <dif>
      <ref tip="super" cel="ole.0o.KOMUNE">Oleo</ref> el
      <ref tip="vid" cel="fisx.0o">fi&scirc;oj</ref>, uzata por diversaj
      celoj:
      <ekz>
        ili &scirc;miris al si la angulojn de la bu&scirc;o per fi&scirc;<tld/>o, por ke ili fari&gcirc;u pli
        flekseblaj
        <fnt>
          <bib>Fab3</bib>
          <lok>Johan&ccirc;jo-malsa&gcirc;ulo</lok>
        </fnt>;
      </ekz>
      <ekz>
        briletis la fi&scirc;<tld/>a lumigilo sur la librobreto de la avino
        <fnt>
          <aut>H. Laxness, trad. B. Ragnarsson</aut>
          <vrk>Sendependaj homoj</vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        fi&scirc;<tld/>a kuir-uzino
        <fnt>
          <bib>VRA</bib>
          <lok>Eyvind Johnson</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_y;&c_b;&c_ib;&c_n; <ind>&c_t;&c_l;&c_u;&c_sh;&c_ch;</ind></trd>
  <trd lng="cv">&c_p;&c_u;&c_l;&c_a; &#x4AB;&#x4D1;&c_v;&#x4D7;<baz>&#x4AB;&c_u;</baz></trd>
  <trd lng="fr">huile de poisson</trd>
  <trd lng="id"><ind>minyak</ind> ikan</trd>
  <trd lng="ja">&#x9B5A;&#x6CB9;  <pr>&#x304E;&#x3087;&#x3086;</pr></trd>
  <trd lng="pl">olej rybny</trd>
  <trd lng="uk">&c_r;&c_i;&c_b;&#x2019;&c_ja;&c_ch;&c_i;&c_j; &c_zh;&c_i;&c_r;</trd>
</drv>

<drv mrk="ole.gas0o">
  <kap>gas<tld/>o, <var><kap>dizel<tld/>o</kap></var></kap>
  <snc mrk="ole.gas0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Naftoderiva&jcirc;o ta&ubreve;ga kiel brula&jcirc;o por
      <ref tip="vid" cel="dizel.0o">dizeloj</ref>;
      <ref tip="sin" cel="gasojl.0o.KEM">gasojlo</ref>:
      <ekz>
        evidenti&gcirc;is, ke estas akvo anka&ubreve; en la gas<tld/>o
        <fnt><aut>E. de Zilah</aut>, <vrk>Vivi sur barko</vrk>,
        <lok>1994</lok></fnt>;
      </ekz>
      <ekz>
        a&ubreve;tomobiloj en &Ccirc;inio &ccirc;iujare konsumas <klr>[&#x2026;]</klr>
        20&#x202F;% de dizel<tld/>o de la lando
        <fnt>
          <aut>Zuo Yan</aut>,
          <vrk><url ref="http://esperanto.china.org.cn/ekonomio/2007-06/13/content_8380897.htm">
          Uzado de novaj energioj por a&ubreve;tomobiloj en &Ccirc;inio</url></vrk>,
          <lok>2007-06-13</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum longa tempo oni vendas malmultekoste gas<tld/>on por hejti lo&gcirc;ejojn
        <fnt>
          <bib>Monato</bib>
          <vrk>Brulaj problemoj</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">diesel fuel</trd>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_z;&c_o;&c_j;&c_l;&c_mol;</trd>,
    <trd>&c_d;&c_y;&c_z;&c_e;&c_l;&c_mol;&c_n;&c_a;&c_je; &c_p;&c_a;&c_l;&c_ib;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>gazolin</trd>,
    <trd>petrolether</trd>,
    <trd>plynov&yacute; olej</trd>
  </trdgrp>
  <trd lng="de">Diesel</trd>
  <trdgrp lng="fr">
    <trd>diesel</trd>,
    <trd>gazole</trd>
  </trdgrp>
  <trd lng="id">diesel</trd>
  <trd lng="ja">&#x30C7;&#x30A3;&#x30FC;&#x30BC;&#x30EB;&#x6CB9;
  <pr>&#x3067;&#x3043;&#x30FC;&#x305C;&#x308B;&#x3042;&#x3076;&#x3089;</pr></trd>
  <trdgrp lng="pl">
    <trd>olej nap&eogonek;dowy</trd>,
    <trd>diesel <klr>(pot.)</klr></trd>,
    <trd>ropa <klr>(pot.)</klr></trd>
  </trdgrp>
  <trd lng="pt">&oacute;leo diesel</trd>
  <trd lng="ro">ulei de motor</trd>
  <trd lng="sk">plynov&yacute; olej</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_a;&c_z;&c_o;&c_j;&c_l;&c_mol;</trd>,
    <trd>&c_d;&c_i;&c_z;&c_je;&c_l;&c_mol;&c_n;&c_je; &c_p;&c_a;&c_l;&c_i;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="zh">&#x67F4;&#x6CB9; <pr>ch&aacute;iy&oacute;u</pr></trd>
</drv>

<drv mrk="ole.oliv0o">
  <kap>oliv<tld/>o</kap>
  <snc mrk="ole.oliv0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="ole.0o.KOMUNE"><tld lit="O"/>o</ref> el
      <ref tip="vid" cel="oliv.0o">olivoj</ref>:
      <ekz>
        <klr>[li]</klr> luis unu el tiuj malgrandaj butiketoj, <klr>[&#x2026;]</klr> por
        vendadi tie oliv<tld/>on, fabon, senfe&ccirc;ajn kukojn
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la oliv<tld/>o en mia lanterno elbrulis
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        manoj trempitaj en aroma oliv<tld/>o komencis froti lian belforman korpon
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;efe la poli-nesaturitaj a&ubreve; plurloke nesaturitaj, troveblaj ene de oliv<tld/>o,
        avokado kaj migdaloj, reale protektas la koron
        <fnt>
          <bib>Monato</bib>
          <aut>Lenio Marobin</aut>
          <vrk><url ref="&Monato;/2014/011298.php"
              >Ela&ccirc;etas sin (kelkaj) grasoj</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_je;&c_j; &c_a;&c_l;&c_ib;&c_w;&c_k;&c_a;&c_v;&c_y;</trd>
  <trd lng="bg">&c_z;&c_je;&c_h;&c_t;&c_i;&c_n;</trd>
  <trd lng="cs">olivov&yacute; olej</trd>
  <trd lng="de">Oliven&ouml;l</trd>
  <trd lng="en">olive oil</trd>
  <trd lng="fr">huile d'olive</trd>
  <trd lng="id"><ind>minyak</ind> zaitun</trd>
  <trd lng="ja">&#x30AA;&#x30EA;&#x30FC;&#x30D6;&#x6CB9;
  <pr>&#x30AA;&#x30EA;&#x30FC;&#x30D6;&#x3086;</pr></trd>
  <trd lng="pl">oliwa z oliwek</trd>
  <trd lng="sk">olivov&yacute; olej</trd>
  <trdgrp lng="zh">
    <trd>&#x6A44;&#x6984;&#x6CB9; <pr>g&#x1CE;nl&#x1CE;ny&oacute;u</pr></trd>,
    <trd>&#x6A44;&#x6B16;&#x6CB9; <pr>g&#x1CE;nl&#x1CE;ny&oacute;u</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ole.palm0o">
  <kap>palm<tld/>o</kap>
  <snc mrk="ole.palm0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="ole.0o.KOMUNE">Oleo</ref> el la fruktoj de
      olepalmo (<nac>Elaeis guineensis</nac>), uzata por kuiro, en sapo, en
      me&hcirc;anika&jcirc;oj...:
      <ekz>
        tian damnan laboron mi havas, sinjoro: fari, kvaza&ubreve; mi
        a&ccirc;etas palm<tld/>on, kaj dume ser&ccirc;i novajn trovejojn de
        perlokonkoj
        <fnt><bib>Salamandroj</bib>, <lok>libro 1a, &ccirc;apitro
        1a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_l;&c_je;&c_j; &c_p;&c_a;&c_l;&c_mol;&c_m;&c_a;&c_v;&c_y;</trd>
    <trd lng="bg">&c_p;&c_a;&c_l;&c_m;&c_o;&c_v;&c_o; &c_m;&c_a;&c_s;&c_l;&c_o;</trd>
    <trd lng="de">Palm&ouml;l</trd>
    <trd lng="en">palm oil</trd>
    <trd lng="fr">huile de palme</trd>
    <trd lng="id"><ind>minyak</ind> sawit</trd>
    <trd lng="pl">olej palmowy</trd>
    <trd lng="pt">azeite de dend&ecirc;</trd>    <trdgrp lng="zh">
    <trd>&#x68D5;&#x6988;&#x6CB9; <pr>z&omacron;ngl&uacute;:y&oacute;u</pr></trd>,
    <trd>&#x68D5;&#x6ADA;&#x6CB9; <pr>z&omacron;ngl&uacute;:y&oacute;u</pr></trd>,
    <trd>&#x68D5;&#x6988;&#x6CB9; <pr>z&omacron;ngl&#x1D8;y&oacute;u</pr></trd>,
    <trd>&#x68D5;&#x6ADA;&#x6CB9; <pr>z&omacron;ngl&#x1D8;y&oacute;u</pr></trd>
  </trdgrp>
</snc>
  <trd lng="cs">palmov&yacute; olej</trd>
  <trd lng="cv">&c_p;&c_a;&c_l;&c_mol;&c_m;&c_a; &#x4AB;&#x4D1;&c_v;&#x4D7;<baz>&#x4AB;&c_u;</baz></trd>
  <trd lng="ja">&#x30E4;&#x30B7;&#x6CB9;  <pr>&#x30E4;&#x30B7;&#x3086;</pr></trd>
  <trd lng="sk">palmov&yacute; olej</trd>
</drv>

<drv mrk="ole.sankt0i">
  <kap>sankt<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ole.sankt0i.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Konsekri, &scirc;mirante per <tld/>o sanktigita:
      <ekz>
        ili sankt<tld/>is Davidon re&gcirc;o super <nom>Izrael</nom>
        <fnt><bib>MT</bib>, <lok>&Sa2; 5:3</lok></fnt>;
      </ekz>
      <ekz>
        sankt<tld/>ita pastro
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 4:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lasta sankt<tld/>ado
        <klr>(unu el la katolikaj sakramentoj)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
  <ind>&c_p;&c_a;&c_m;&c_a;&c_z;&c_a;&c_n;&c_mol;&c_n;&c_je;</ind>
  <klr>(&c_c;&c_a;&c_r;&c_k;.)</klr></trd>
  <trd lng="en">to anoint</trd>
  <trd lng="br">olevi&ntilde;</trd>
  <trd lng="de">salben</trd>
  <trd lng="es">dar los santos <ind>&oacute;leos</ind></trd>
  <trdgrp lng="fr">
    <trd>consacrer<klr> (par onction)</klr></trd>,
    <trd>oindre</trd>
  </trdgrp>
  <trd lng="hu">felken <klr>(szentelt olajjal</klr></trd>
  <trdgrp lng="id">
    <trd><baz>minyak</baz>meminyaki</trd>,
    <trd><baz>oles</baz>mengolesi minyak urapan</trd>,
    <trd><baz>urap</baz>mengurapi</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8056;&#x6CB9;&#x3092;&#x5857;&#x308B;  <pr>&#x3072;&#x3058;&#x308A;&#x3042;&#x3076;&#x3089;&#x3092;&#x306C;&#x308B;</pr></trd>,
    <trd>&#x8056;&#x5225;&#x3059;&#x308B;  <pr>&#x3072;&#x3058;&#x308A;&#x3079;&#x3064;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">zalven</trd>
  <trd lng="pl">namaszcza&cacute;<klr> olejkiem</klr></trd>
  <trd lng="ru">&c_s;&c_o;&c_v;&c_je;&c_r;&c_sh;&c_a;&c_t;&c_mol; &c_o;&c_b;&c_r;&c_ja;&c_d;
  <ind>&c_p;&c_o;&c_m;&c_a;&c_z;&c_a;&c_n;&c_i;&c_ja;</ind></trd>
  <trdgrp lng="uk">
    <trd>&c_jeu;&c_l;&c_je;&c_o;&c_s;&c_v;&c_ja;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_o;&c_b;&c_o;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x62B9;&#x6CB9;&#x818F; <pr>m&#x1D2;y&oacute;ug&amacron;o</pr></trd>
</drv>

