    <trd>&c_o;&c_m;&c_a;&c_s;&c_l;&c_ja;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_i;&c_m;&c_p;&c_r;&c_je;&c_g;&c_n;&c_i;&c_r;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="br">eoulia&ntilde;</trd>
  <trd lng="cs">naolejovat</trd>
  <trdgrp lng="de">
    <trd>&ouml;len</trd>,
    <trd>ein&ouml;len</trd>
  </trdgrp>
  <trd lng="en">to oil</trd>
  <trd lng="es">aceitar</trd>
  <trd lng="fr">huiler</trd>
  <trdgrp lng="hu">
    <trd>beolajoz</trd>,
    <trd>megolajoz</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>lumas</baz>melumasi</trd>,
    <trd><baz>minyak</baz>meminyaki</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6CB9;&#x3092;&#x5857;&#x308B;  <pr>&#x3042;&#x3076;&#x3089;&#x3092;&#x306C;&#x308B;</pr></trd>,
    <trd>&#x6CB9;&#x3092;&#x3055;&#x3059;  <pr>&#x3042;&#x3076;&#x3089;&#x3092;&#x3055;&#x3059;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>smeren</trd>,
    <trd>inoli&euml;n</trd>,
    <trd>oli&euml;n</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>smarowa&cacute; <ind>olejem</ind></trd>,
    <trd>olei&cacute;</trd>
  </trdgrp>
  <trd lng="pt">olear</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_m;&c_a;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;
