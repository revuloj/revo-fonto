<drv mrk="ole.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Enhavanta <tld/>on:
      <ekz>
        <tld/>a pentrado;
      </ekz>
      <ekz>
        <tld/>aj haroj, ha&ubreve;to.
      </ekz>
    </dif>
    <trd lng="br">eoulek</trd>
    <trd lng="de">&ouml;lhaltig</trd>
    <trdgrp lng="en">
      <trd>oily</trd>,
      <trd>oil</trd>, <trd>greasy</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>olaj-</trd>,
      <trd>olajos</trd>
    </trdgrp>
    <trd lng="nl">olie-</trd>
    <trdgrp lng="pl">
    <trd>olejny</trd>,
    <trd>oleisty</trd>,
    <trd>t&lstroke;usty</trd>
    </trdgrp>
    <trd lng="ru">&c_m;&c_a;&c_s;&c_l;&c_ja;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc>
    <dif>
      Havanta la naturon de <tld/>o:
      <ekz>
        <tld/>a karaktero<fnt>Z</fnt>
        <klr>(cedema.
        <refgrp tip="vid">
          <ref cel="or.0o">oro</ref>,
          <ref cel="vaks.0o">vakso</ref>
        </refgrp>)</klr>.
      </ekz>
    </dif>
    <trd lng="de">&ouml;lig</trd>
    <trd lng="nl">olieachtig</trd>
    <trd lng="pl">mi&eogonek;kki</trd>
    <trd lng="pt">oleoso</trd>
