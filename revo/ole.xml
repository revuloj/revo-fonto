<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ole</rad>/o
</kap>
<drv mrk="ole.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Grasa flua&jcirc;o el animala (baleno, foko), vegeta&jcirc;a (lino, nukso) a&ubreve;
      minerala (nafto, skisto) origino, uzata por nutrado, medicino,
      lumigado, arto, industrio kaj religiaj ceremonioj:
      <ekz>
        oliva, nuksa <tld/>o;
      </ekz>
      <ekz>
        sanktaj <tld/>oj
        <klr>(uzataj por sakramentoj)</klr>;
      </ekz>
      <ekz>
        glata kiel <tld/>o;
      </ekz>
      <ekz>
        enver&scirc;i <tld/>on en la fajron<fnt>Z</fnt>
        <klr>(inciti ardon de disputantoj)</klr>;
      </ekz>
      <ekz>
        <tld/>havaj, <tld/>donaj vegeta&jcirc;oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">&Ouml;l</trd>
  <trd lng="en">oil</trd>
  <trd lng="hu">olaj</trd>
  <trd lng="nl">olie</trd>
  <trd lng="ru">&c_m;&c_a;&c_s;&c_l;&c_o; <klr>(&c_zh;&c_i;&c_d;&c_k;&c_o;&c_je;: &c_r;&c_a;&c_s;&c_t;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je;, &c_m;&c_i;&c_n;&c_je;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_je; &c_i; &c_t;.&c_p;.)</klr></trd>
  <trd lng="sv">olja</trd>
</drv>

<drv mrk="ole.0a">
  <kap><tld/>(ec)a</kap>
  <snc>
    <dif>
      Enhavanta <tld/>on:
      <ekz>
        <tld/>a pentrado;
      </ekz>
      <ekz>
        <tld/>aj haroj, ha&ubreve;to.
      </ekz>
    </dif>
    <trd lng="de">&ouml;lhaltig</trd>
    <trdgrp lng="hu">
      <trd>olaj-</trd>,
      <trd>olajos</trd>
    </trdgrp>
    <trd lng="nl">olie-</trd>
    <trd lng="ru">&c_m;&c_a;&c_s;&c_l;&c_ja;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc>
    <dif>
      Havanta la naturon de <tld/>o:
      <ekz>
        <tld/>a karaktero<fnt>Z</fnt>
        <klr>(cedema.
        <refgrp tip="sin">
          <ref cel="or.0o">oro</ref>,
          <ref cel="vaks.0o">vakso</ref>
        </refgrp>)</klr>.
      </ekz>
      <trd lng="ru">&c_m;&c_a;&c_s;&c_l;&c_ja;&c_n;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>
    </dif>
    <trd lng="de">&ouml;lig</trd>
    <trd lng="nl">olieachtig</trd>
  </snc>
</drv>

<drv mrk="ole.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;miri per <tld/>o:
      <ekz>
        <tld/>i ma&scirc;inon, &scirc;losilon, la hokojn de la pordo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>ita papero.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>&ouml;len</trd>,
    <trd>ein&ouml;len</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>beolajoz</trd>,
    <trd>megolajoz</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>smeren</trd>,
    <trd>inoli&euml;n</trd>,
    <trd>oli&euml;n</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_m;&c_a;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol; &c_m;&c_a;&c_s;&c_l;&c_o;&c_m;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_m;&c_a;&c_s;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>sm&ouml;rja</trd>,
    <trd>olja in</trd>
  </trdgrp>
</drv>

<drv mrk="ole.sankt0i">
  <kap>sankt<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Konsekri, &scirc;mirante per <tld/>o sanktigita:
      <ekz>
        sankt<tld/>i iun re&gcirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la Sankt<tld/>ito
        <klr>(Mesio)</klr>;
      </ekz>
      <ekz>
        la lasta sankt<tld/>ado
        <klr>(unu el la katolikaj sakramentoj)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">salben</trd>
  <trd lng="hu">felken <klr>(szentelt olajjal</klr></trd>
  <trd lng="nl">zalven</trd>
  <trd lng="ru">&c_s;&c_o;&c_v;&c_je;&c_r;&c_sh;&c_a;&c_t;&c_mol; &c_o;&c_b;&c_r;&c_ja;&c_d; <ind>&c_p;&c_o;&c_m;&c_a;&c_z;&c_a;&c_n;&c_i;&c_ja;</ind></trd>
</drv>

<drv mrk="ole.sxton0o">
  <kap>&scirc;ton<tld/>o</kap>
  <snc>
    <dif>
      Petrolo.
    </dif>
  </snc>
</drv>
</art>
