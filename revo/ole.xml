<drv mrk="ole.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Enhavanta <tld/>on; funkcianta per <tld/>o:
      <ekz>
        <tld/>aj pentra&jcirc;oj
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj kukoj
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 11:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        malhela <tld/>a lampeto brulis sur la kameno
        <fnt>
          <bib>ElektFab</bib>
          <lok>Cindrulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a lubrika&jcirc;o
        <fnt>
          <bib>Kon11</bib>
          <lok>La Blaga Blogo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i havis pe&ccirc;e nigran, <tld/>an, buklan hararon
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 10</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="br">eoulek</trd>
    <trd lng="de">&ouml;lhaltig</trd>
    <trdgrp lng="en">
      <trd>oily</trd>,
      <trd>oil</trd>, <trd>greasy</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>olaj-</trd>,
      <trd>olajos</trd>
    </trdgrp>
    <trd lng="nl">olie-</trd>
    <trdgrp lng="pl">
    <trd>olejny</trd>,
    <trd>oleisty</trd>,
    <trd>t&lstroke;usty</trd>
    </trdgrp>
    <trd lng="ru">&c_m;&c_a;&c_s;&c_l;&c_ja;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Glita kiel <tld/>o:
      <ekz>
        karaktero <tld/>a
        <klr>(<ref cel="flat.0a">flata</ref> kaj <ref cel="evit.0ema">evitema</ref>,
        <ref cel="glit.0a.evitema">glita<sncref/></ref>,
        <ref cel="gxenti.0acxa">&gcirc;entila&ccirc;a</ref>)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">&ouml;lig</trd>
    <trd lng="nl">olieachtig</trd>
    <trd lng="pl">mi&eogonek;kki</trd>
    <trd lng="pt">oleoso</trd>
    <trd lng="ru">&c_m;&c_a;&c_s;&c_l;&c_ja;&c_n;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_je;&c_j;&c_n;&c_y;</trd>
  <trdgrp lng="bg">
    <trd>&c_m;&c_a;&c_s;&c_l;&c_je;&c_n;</trd>,
    <trd>&c_p;&c_je;&c_t;&c_r;&c_o;&c_l;&c_je;&c_n;</trd>,
    <trd>&c_n;&c_je;&c_f;&c_t;&c_je;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>olejnat&yacute;</trd>,
    <trd>olejov&yacute;</trd>
  </trdgrp>
  <trd lng="fr">huileux</trd>
  <trd lng="id"><baz>minyak</baz>berminyak</trd>
  <trdgrp lng="ja">
    <trd>&#x6CB9;&#x306E;  <pr>&#x3042;&#x3076;&#x3089;&#x306E;</pr></trd>,
    <trd>&#x6CB9;&#x6027;&#x306E;  <pr>&#x3086;&#x305B;&#x3044;&#x306E;</pr></trd>,
    <trd>&#x6CB9;&#x3092;&#x542B;&#x3093;&#x3060;  <pr>&#x3042;&#x3076;&#x3089;&#x3092;&#x3075;&#x304F;&#x3093;&#x3060;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>olejnat&yacute;</trd>,
    <trd>olejov&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_o;&c_l;&c_ib;&c_j;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_zh;&c_i;&c_r;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_m;&c_a;&c_s;&c_l;&c_ja;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6CB9; <pr>y&oacute;u</pr></trd>,
    <trd>&#x6CB9;&#x4E4E;&#x4E4E; <pr>y&oacute;uh&umacron;h&umacron;</pr></trd>,
    <trd>&#x542B;&#x6CB9; <pr>h&aacute;ny&oacute;u</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ole.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ole.0i.TEK">
    <dif>
      <ref cel="sxmir.0i.surmeti">&Scirc;miri</ref> per <tld/>o:
      <ekz>
        lavu do vin kaj <tld/>u vin kaj vestu vin per viaj plej bonaj vestoj
        <fnt>
          <bib>MT</bib>
          <lok>Rut 3:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        sin <tld/>i per eksterordinaraj parfumoj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni <tld/>as la hokojn de la pordoj
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ita papero;
      </ekz>
      <ekz>
        <tld/>i la ma&scirc;inon
        <fnt>
          <bib>ViV</bib>
          <lok>I.</lok>
        </fnt>;
      </ekz>
      <ekz>
        li mem estas pacama, <klr>[&#x2026;]</klr> e&ccirc; neniam <tld/>is la antikvan avan armilon
        <fnt>
          <bib>ChR</bib>
          <lok>Kiu pafis al Pipulo?</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_l;&c_je;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_m;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol;
    <ind>&c_a;&c_l;&c_je;&c_je;&c_m;</ind></trd>
