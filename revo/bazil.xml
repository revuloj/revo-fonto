<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Bazil</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="bazil.0o">
  <kap><tld/>o</kap>
  <snc mrk="bazil.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira anta&ubreve;nomo grekdevena:
      <ekz>
        li alveturis en la tago de <tld/>o Egiptano
        <fnt><bib>Revizoro</bib>, <lok>akto 1a, sceno 3a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_V;&c_a;&c_s;&c_ib;&c_l;&c_mol;</trd>,
    <trd>&c_B;&c_a;&c_z;&c_y;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="fr">Basile</trd>
  <trdgrp lng="ru">
    <trd>&c_V;&c_a;&c_s;&c_i;&c_l;&c_i;&c_j;</trd>,
    <trd>&c_B;&c_a;&c_z;&c_i;&c_l;&c_mol;<klr> (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>
  </trdgrp>
</drv>
</art>

