<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bazil.xml,v 1.9 2022/03/12 20:16:57 revo Exp $">
<kap>
  <rad>Bazil</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="bazil.0o">
  <kap><tld/>o</kap>
  <snc mrk="bazil.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> grekdevena:
      <ekz>
        li alveturis en la tago de <tld/>o Egiptano
        <fnt><bib>Revizoro</bib>, <lok>akto 1a, sceno 3a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_V;&c_a;&c_s;&c_ib;&c_l;&c_mol;</trd>,
    <trd>&c_B;&c_a;&c_z;&c_y;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">Basil</trd>
  <trd lng="fr">Basile</trd>
  <trd lng="hu">Vazul</trd>
  <trd lng="nl">Baziel</trd>
  <trd lng="pl">Bazyli</trd>
  <trdgrp lng="ru">
    <trd>&c_V;&c_a;&c_s;&c_i;&c_l;&c_i;&c_j;</trd>,
    <trd>&c_B;&c_a;&c_z;&c_i;&c_l;&c_mol;<klr>
    (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x412;&#x430;&#x441;&#x438;&#x43B;&#x44C;</trd>,
    <trd>&#x411;&#x430;&#x437;&#x456;&#x43B;&#x44C;&#xA0;(&#x456;&#x43C;&#x435;&#x43D;&#x430;)</trd>
  </trdgrp>
</drv>
</art>

