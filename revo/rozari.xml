<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rozari.xml,v 1.24 2024/04/14 05:47:07 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>rozari</rad>/o
</kap>
<drv mrk="rozari.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="rozari.0o.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      <ref cel="bid.0aro.JUV">Bidaro</ref>
      servanta por kalkuli la diratajn <ref tip="vid" cel="pregx.0o">pre&gcirc;ojn</ref>:
      <ekz>
        &scirc;i recitis la pre&gcirc;ojn de la <tld/>o, longe tenante &ccirc;iun eron kaj kun senlima amo
        murmuretante &ccirc;iun vorton
        <fnt>
          <bib>Lasta</bib>
          <lok>La Lasta</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Emma Demartino</nom> me&hcirc;anike eltiris <tld/>on kaj komencis pre&gcirc;i
        <fnt>
          <bib>IKr</bib>
          <lok>Neniam Plu!</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_u;&c_zh;&c_a;&c_n;&c_je;&c_c;</trd>
    <trd lng="ca">rosari</trd>
    <trd lng="de">Rosenkranz</trd>
    <trd lng="en">rosary</trd>
    <trd lng="es">rosario</trd>
    <trd lng="fa">&a_t;&a_s;&a_b;&f_y;&a_H;</trd>
    <trdgrp lng="fr">
      <trd>rosaire</trd>,
      <trd>chapelet</trd>
    </trdgrp>
    <trd lng="hu">r&oacute;zsaf&uuml;z&eacute;r</trd>    
    <trd lng="nl">rozenkrans</trd>
    <trd lng="pl">r&oacute;&zdot;aniec</trd>
    <trd lng="ru">&c_ch;&c_jo;&c_t;&c_k;&c_i;</trd>
    <trdgrp lng="zh">
    <trd>&#x5FF5;&#x73E0; <pr>ni&#xE0;nzh&#x16B;</pr></trd>,
    <trd>&#x73AB;&#x7470;&#x5FF5;&#x73E0; <pr>m&#xE9;iguini&#xE0;nzh&#x16B;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="rozari.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Longa <ref tip="super" cel="vic.0o">vico</ref> da ne intime ligitaj a&jcirc;oj:
      <ekz>
        tuta <tld/>o da rememora&jcirc;oj, da insultoj;
      </ekz>
      <ekz>
        &ccirc;irka&ubreve; la bosko pendis punktitaj elektraj lampoj kiel <tld/>o de voluptulino
        <fnt>
          <bib>VRA</bib>
          <lok>Lars Ahlin</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="cxen.0o">&ccirc;eno</ref>,
      <ref cel="litani.0o">litanio</ref>,
      <ref cel="girlan.0o">girlando</ref>,
      <ref cel="ruband.0o">rubando</ref>
    </refgrp>
    <trd lng="be">&c_l;&c_a;&c_n;&c_c;&c_u;&c_zh;&c_o;&c_k;</trd>
    <trd lng="en">litany</trd>
    <trdgrp lng="hu">
      <trd>l&aacute;nc</trd>,
      <trd>f&uuml;z&eacute;r</trd>
    </trdgrp>
    <trd lng="pl">litania</trd>
    <trd lng="ru">&c_c;&c_je;&c_p;&c_o;&c_ch;&c_k;&c_a; <klr>(f)</klr></trd>
  </snc>
  <trd lng="bo">&t_-a;&t_pha;&t_rata;&t_drengbu;&t_nga;&t_tsheg;&t_ba;&t_tsheg;</trd>
  <trd lng="cs">r&uring;&zcaron;enec</trd>
  <trdgrp lng="ja">
    <trd>&#x30ED;&#x30B6;&#x30EA;&#x30AA;</trd>,
    <trd>&#x6570;&#x73E0;  <pr>&#x3058;&#x3085;&#x305A;</pr></trd>,
    <trd>&#x6570;&#x73E0;&#x3064;&#x306A;&#x304E;&#x306B;&#x306A;&#x3063;&#x305F;&#x3082;&#x306E;  <pr>&#x3058;&#x3085;&#x305A;&#x3064;&#x306A;&#x304E;&#x306B;&#x306A;&#x3063;&#x305F;&#x3082;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>rist</trd>,
    <trd>reeks</trd>
  </trdgrp>
  <trd lng="sk">ru&zcaron;enec</trd>
  <trdgrp lng="uk">
    <trd>&c_ch;&c_o;&c_t;&c_k;&c_i;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_v;&c_je;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
