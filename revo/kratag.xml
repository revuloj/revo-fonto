<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kratag</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kratag.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kratag.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbeto de mezvarmaj landoj kun fruprintempaj blankaj floroj,
      genro el familio <ref tip="super" cel="rozac.0oj">rozacoj</ref>
      (<trd lng="la">Crataegus</trd>):
      <ekz>
        &ccirc;ie videblis florantaj <tld/>oj, &scirc;afoj disvasti&gcirc;intaj en
        herbejoj
        <fnt><aut>R. L. Stevenson, trad. A. Goodheir</aut>,
        <vrk>Forkaptita</vrk>, <lok>Kardo, 1976</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/kratag.jpg">
    floroj kaj folioj de <ind>dustilusa <tld/>o</ind>
    <trd lng="en">midland hawthorn</trd>
    <trd lng="la">Crataegus laevigata</trd>
    <trdgrp lng="hu">
      <trd>cseregalagonya</trd>,
      <trd>k&eacute;tbib&eacute;s <ind>galagonya</ind></trd>
    </trdgrp>
    <trd lng="nl">tweestijlige <ind>meidoorn</ind></trd>
  </bld>
  <trd lng="be">&c_g;&c_l;&c_o;&c_g;</trd>
  <trd lng="de">Wei&szlig;dorn</trd>
  <trd lng="en">hawthorn</trd>
  <trd lng="fr">aub&eacute;pine</trd>
  <trd lng="hu">galagonya</trd>
  <trd lng="nl">meidoorn</trd>
  <trd lng="pt">cratego</trd>
  <trd lng="ru">&c_b;&c_o;&c_ja;&c_r;&c_y;&c_sh;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
