<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>kratag</rad>/o
</kap>
<drv mrk="kratag.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="sub" cel="roz">rozacoj</ref> 
      (<trd lng="la">Crataegus</trd>), 
      arbedo kun blankaj floroj.
    </dif>
  </snc>
  <bld lok="../bld/kratag.jpg">floroj kaj folioj de dustilusa <tld/>o
    (<trd lng="la">Crataegus laevigata</trd>)</bld>
  <trd lng="de">Wei&szlig;dorn</trd>
</drv>
</art>
