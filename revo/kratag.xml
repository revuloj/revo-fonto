<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kratag.xml,v 1.27 2023/12/03 09:59:00 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kratag</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kratag.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kratag.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbeto de mezvarmaj landoj kun fruprintempaj blankaj floroj,
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref> el familio <ref tip="malprt" cel="rozac.0oj">rozacoj</ref>
      (<trd lng="la">Crataegus</trd>):
      <ekz>
        la formoj &leftquot;<tld/>o&rightquot; kaj &leftquot;vipuro&rightquot; ne estas preseraroj
        <fnt>
          <bib>LR</bib>
          <lok>2. Vortuzo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ie videblis florantaj <tld/>oj, &scirc;afoj disvasti&gcirc;intaj en
        herbejoj
        <fnt><aut>R. L. Stevenson, trad. A. Goodheir</aut>,
        <vrk>Forkaptita</vrk>, <lok>Kardo, 1976</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="&WCU;/thumb/d/d8/Crataegus-oxyacantha-flowers.JPG/640px-Crataegus-oxyacantha-flowers.JPG" prm="CC BY-SA 3.0">floroj kaj folioj de <ind>dustilusa <tld/>o</ind> (Crataegus laevigata), Malling, Jutlando
       <fnt>
         <aut>Sten, propra foto</aut>
         <lok><url ref="&WCW;/File:Crataegus-oxyacantha-flowers.JPG">&WCW;/File:Crataegus-oxyacantha-flowers.JPG</url></lok>
       </fnt>
    <trd lng="en">midland hawthorn</trd>
    <trd lng="la">Crataegus laevigata</trd>
    <trdgrp lng="hu">
      <trd>cseregalagonya</trd>,
      <trd>k&eacute;tbib&eacute;s <ind>galagonya</ind></trd>
    </trdgrp>
    <trd lng="nl">tweestijlige <ind>meidoorn</ind></trd>
  </bld>
  <trd lng="be">&c_g;&c_l;&c_o;&c_g;</trd>
  <trdgrp lng="ca">
    <trd>ar&ccedil; blanc</trd>,
    <trd>espinalb</trd>
  </trdgrp>
  <trd lng="de">Wei&szlig;dorn</trd>
  <trd lng="en">hawthorn</trd>
  <trd lng="fr">aub&eacute;pine</trd>
  <trd lng="hu">galagonya</trd>
  <trd lng="ja">&#x30B5;&#x30F3;&#x30B6;&#x30B7;</trd>
  <trd lng="nl">meidoorn</trd>
  <trd lng="pl">g&lstroke;&oacute;g</trd>
  <trd lng="pt">cratego</trd>
  <trdgrp lng="ro">
    <trd>p&abreve;ducel</trd>,
    <trd>gherghin</trd>
  </trdgrp>    
  <trd lng="ru">&c_b;&c_o;&c_ja;&c_r;&c_y;&c_sh;&c_n;&c_i;&c_k;</trd>
  <trd lng="sv">hagtorn</trd>
  <trd lng="sk">hloh</trd>
  <trd lng="cs">hloh</trd>
  <trd lng="uk">&#x433;&#x43B;&#x456;&#x434;</trd>
  <trdgrp lng="zh">
    <trd>&#x67E4; <pr>zh&#x101;</pr></trd>,
    <trd>&#x5C71;&#x6942; <pr>sh&#x101;nzh&#x101;</pr></trd>,
    <trd>&#x6942; <pr>zh&#x101;</pr></trd>
  </trdgrp>
</drv>
</art>
