<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>konkav</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="konkav.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Prezentanta kavan randon, kiel interno de cirklo a&ubreve; sfero:
      <ekz>
        la <tld/>a supra&jcirc;o de okulvitro por miopulo.
      </ekz>
    </dif>
    <trd lng="en">concave</trd>
    <trd lng="fr">concave</trd>
    <trdgrp lng="nl">
      <trd>concaaf</trd>,
      <trd>hol</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="konkav.0a.subaro">
      <fnt><bib>PIV1</bib></fnt>
      <dif>
        <klr>(p.p. <ref cel="figur.geometria0o">geometria figuro</ref>,
        subaro de <ref cel="spac.afina0o">afina spaco</ref>, a&ubreve; de
        <ref cel="spac.vektora0o">vektora spaco</ref>)</klr>
        Ne <ref cel="konvek.0a.subaro">konveksa</ref>:
        <ekz>
          <tld/>a <ref cel="angul.0o.figuro">angulo</ref> <klr>(pli
          granda ol <ref cel="strecx.0ita.angulo">stre&ccirc;ita</ref>
          angulo)</klr>,
          <ref cel="later.plur0o">plurlatero</ref>,
          <ref cel="edr.plur0o">pluredro</ref>.
        </ekz>
      </dif>
      <trd lng="de">konkave<klr tip="amb"> (Untermenge)</klr></trd>
      <trd lng="en">concave<klr tip="amb"> (subset)</klr></trd>
      <trd lng="fr"><klr tip="amb">(sous-ensemble)
      </klr><ind>concave</ind></trd>
      <trd lng="ru">&c_v;&c_o;&c_g;&c_n;&c_u;&c_t;&c_o;&c_je; <klr
      tip="amb">(&c_p;&c_o;&c_d;&c_m;&c_n;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
    </subsnc>
    <subsnc mrk="konkav.0a.bildigo">
      <dif>
        <klr>(p.p. <ref cel="bild.0igo">bildigo</ref> f de
        la aro de <ref cel="reel.0o.MAT">reeloj</ref> al si mem)</klr>
        Tia, ke &gcirc;ia negativo estas
        <ref cel="konvek.0a.bildigo">konveksa</ref>.
      </dif>
      <trd lng="de">konkave<klr tip="amb"> (Funktion)</klr></trd>
      <trd lng="en">concave<klr tip="amb"> (function)</klr></trd>
      <trd lng="fr"><klr tip="amb">(fonction) </klr><ind>concave</ind></trd>
      <trd lng="ru">&c_v;&c_o;&c_g;&c_n;&c_u;&c_t;&c_a;&c_ja;<klr tip="amb">
      (&c_f;&c_u;&c_n;&c_k;&c_c;&c_i;&c_ja;)</klr></trd>
    </subsnc>
  </snc>
  <refgrp tip="ant">
    <ref cel="konvek.0a">konveksa</ref>.
  </refgrp>
  <trd lng="ru">&c_v;&c_o;&c_g;&c_n;&c_u;&c_t;&c_y;&c_j;</trd>
</drv>
</art>
