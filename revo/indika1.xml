<art mrk="$Id: indika1.xml,v 1.17 2024/09/18 15:30:59 revo Exp $">
<kap>
<rad>indikator</rad>/o
</kap>
<drv mrk="indika1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Aparato, kiu servas al indiko de tensio, premo, laboro ktp de
      ma&scirc;ino:
      <ekz>
        la vorto <tld/>o estas &ccirc;efe rezervata por <tld/>oj de fazo,
        kurento a&ubreve; de frekvenco, por aliaj mezuraparatoj oni diras
        &gcirc;enerale <ref cel="indik.0ilo">indikilo</ref>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_d;&c_y;&c_k;&c_a;&c_t;&c_a;&c_r;</trd>
  <trd lng="bg">&c_i;&c_n;&c_d;&c_i;&c_k;&c_a;&c_t;&c_o;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>indik&#xE1;tor</trd>,
    <trd>ukazatel</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Anzeiger</trd>,
     <trd>Anzeigeger&auml;t</trd>,
     <trd>Anzeigeinstrument</trd>,
     <trd>Messger&auml;t</trd>,
     <trd>Messinstrument</trd>
  </trdgrp>
  <trd lng="en">indicator</trd>
  <trd lng="es">medidor</trd>
  <trd lng="fr">indicateur<klr tip="ind"> (appareil)</klr></trd>
  <trdgrp lng="hu">
     <trd>kijelz&odblac;</trd>,
     <trd>jelz&odblac;m&udblac;szer</trd>
  </trdgrp>
  <trd lng="nl">meter</trd>
  <trd lng="pt">medidor</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_k;&c_a;&c_z;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_i;&c_n;&c_d;&c_i;&c_k;&c_a;&c_t;&c_o;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>indik&#xE1;tor</trd>,
    <trd>ukazovate&#x13E;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ib;&#x43D;&c_d;&c_i;&#x43A;&c_a;&c_t;&#x43E;&c_r;</trd>,
    <trd>&#x43F;&#x43E;&#x43A;&c_a;&c_zh;&c_ch;&c_i;&#x43A;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#25351;&#x793A;&#x7B26; <pr>zh&#x1D0;sh&#xEC;f&#xFA;</pr></trd>,
    <trd>&#25351;&#x793A;&#32773; <pr>zh&#x1D0;sh&#xEC;zh&#x11B;</pr></trd>,
    <trd>&#x516C;&#x793A; <pr>g&#x14D;ngsh&#xEC;</pr></trd>,
    <trd>&#25351;&#x793A;&#29289; <pr>zh&#x1D0;sh&#xEC;w&#xF9;</pr></trd>,
    <trd>&#25351;&#x5F81; <pr>zh&#x1D0;zh&emacron;ng</pr></trd>,
    <trd>&#x706F;&#x53F7; <pr>d&emacron;ngh&#xE0;o</pr></trd>,
    <trd>&#20839;&#x5BB9;&#25351;&#x793A;&#22120; <pr>n&#xE8;ir&#xF3;ngzh&#x1D0;sh&#xEC;q&#xEC;</pr></trd>
  </trdgrp>
</drv>
