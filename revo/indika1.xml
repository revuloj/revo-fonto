<kap>
<rad>indikator</rad>/o
</kap>
<drv mrk="indika1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Aparato, kiu servas al indiko de tensio, premo, laboro ktp de
      ma&scirc;ino:
      <ekz>
        la vorto <tld/>o estas &ccirc;efe rezervata por <tld/>oj de fazo,
        kurento a&ubreve; de frekvenco, por aliaj mezuraparatoj oni diras
        &gcirc;enerale <ref cel="indik.0ilo">indikilo</ref>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_i;&c_n;&c_d;&c_i;&c_k;&c_a;&c_t;&c_o;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>indik&#xE1;tor</trd>,
    <trd>ukazatel</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Anzeiger</trd>,
     <trd>Anzeigeger&auml;t</trd>,
     <trd>Anzeigeinstrument</trd>,
     <trd>Messger&auml;t</trd>,
     <trd>Messinstrument</trd>
  </trdgrp>
  <trd lng="en">indicator</trd>
  <trd lng="es">medidor</trd>
  <trd lng="fr">indicateur<klr tip="ind"> (appareil)</klr></trd>
  <trdgrp lng="hu">
     <trd>kijelz&odblac;</trd>,
     <trd>jelz&odblac;m&udblac;szer</trd>
  </trdgrp>
  <trd lng="nl">meter</trd>
  <trd lng="pt">medidor</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_k;&c_a;&c_z;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_i;&c_n;&c_d;&c_i;&c_k;&c_a;&c_t;&c_o;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>indik&#xE1;tor</trd>,
    <trd>ukazovate&#x13E;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x456;&#x43D;&#x434;&#x438;&#x43A;&#x430;&#x442;&#x43E;&#x440;</trd>,
    <trd>&#x43F;&#x43E;&#x43A;&#x430;&#x436;&#x447;&#x438;&#x43A;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6307;&#x793A;&#x7B26; <pr>zh&#x1D0;sh&#xEC;f&#xFA;</pr></trd>,
    <trd>&#x6307;&#x793A;&#x8005; <pr>zh&#x1D0;sh&#xEC;zh&#x11B;</pr></trd>,
    <trd>&#x516C;&#x793A; <pr>g&#x14D;ngsh&#xEC;</pr></trd>,
    <trd>&#x6307;&#x793A;&#x7269; <pr>zh&#x1D0;sh&#xEC;w&#xF9;</pr></trd>,
    <trd>&#x6307;&#x5F81; <pr>zh&#x1D0;zh&#x113;ng</pr></trd>,
    <trd>&#x706F;&#x53F7; <pr>d&#x113;ngh&#xE0;o</pr></trd>,
    <trd>&#x5167;&#x5BB9;&#x6307;&#x793A;&#x5668; <pr>n&#xE8;ir&#xF3;ngzh&#x1D0;sh&#xEC;q&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>
