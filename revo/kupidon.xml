<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: kupidon.xml,v 1.3 2022/09/08 12:58:27 revo Exp $">
    <kap>
      <rad>Kupidon</rad>/o
    </kap>

<drv mrk="kupidon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc mrk="kupidon.0o.amoro">
    <dif>
      <ref tip="dif" cel="amor.A0o">Amoro</ref>:
      <ekz>
        &scirc;ajnas al mi, ke vi deziris <tld/>on kun Venero
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        lia debuta verko estis &leftquot;Jupitero karesanta Kupidonon&rightquot;, en novklasikeca stilo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=124720">Michelangelo Grigoletti</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la statuo <tld/>o prenante papilion kiu estas en la muzeo de Luvro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=360670">Parko Thabor</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_K;&c_u;&c_p;&c_ib;&c_d;&c_o;&c_n;</trd>
    <trd lng="de">Cupido</trd>
    <trd lng="en">Cupid</trd>
    <trd lng="fr">Cupidon</trd>
  </snc>
  <trd lng="uk">&#x41A;&c_u;&#x43F;&c_ib;&c_d;&#x43E;&#x43D;</trd>
</drv>

  </art>

