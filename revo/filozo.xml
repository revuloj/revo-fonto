<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>filozof</rad>/o
</kap>
<drv mrk="filozo.0o">
  <kap><tld/>o</kap>
  <snc mrk="filozo.0o.scienculo">
    <dif>
      Scienculo, kiu esploras k pritraktas la 
      <ref cel="filozo1.0o">filozofion</ref>. 
    </dif>
    <ref tip="vid" cel="pens.0isto0ulo">pensulo.</ref>
    <trd lng="de">Philosoph</trd>
  </snc>
  <snc mrk="filozo.0o.pacienculo">
    <dif>
      Praktike sa&gcirc;a k trankvilanima homo, kiu pacience toleras
      la malfeli&ccirc;a&jcirc;ojn. 
    </dif>
    <trd lng="de">Denker</trd>
  </snc>
  <trd lng="en">philosopher</trd>
  <trd lng="fr">philosophe</trd>
  <trd lng="ru">&c_f;&c_i;&c_l;&c_o;&c_s;&c_o;&c_f;</trd>
</drv>
<drv mrk="filozo.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="filozo.0i.science">
    <ref cel="filozo1.0i">filozofii.</ref> 
    <trd lng="de">philosophieren</trd>
    <trd lng="fr">philosopher</trd>
  </snc>
  <snc mrk="filozo.0i.pacience">
    <dif>
      Montri&gcirc;i <tld/>o <sncref ref="filozo.0o.pacienculo"/>; sa&gcirc;e k trankvile vivi tolerante kun pacienco la malfeli&ccirc;a&jcirc;ojn.  
    </dif>
    <trd lng="fr">rester <ind>philosophe</ind></trd>
  </snc>
</drv>
</art>
