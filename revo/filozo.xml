<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>filozof</rad>/o
</kap>
<drv mrk="filozo.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Scienculo, kiu esploras k pritraktas la filozofion. 
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Praktike sa&gcirc;a k trankvilanima homo, kiu pacience toleras
      la malfeli&ccirc;a&jcirc;ojn. 
    </dif>
  </snc>
  <trd lng="en">philosopher</trd>
  <trd lng="ru">&c_f;&c_i;&c_l;&c_o;&c_s;&c_o;&c_f;</trd>
</drv>
<drv mrk="filozo.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc num="1">
    <dif>
      Esplori k pritrakti la filozofion. 
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Sa&gcirc;e k trankvile vivi tolerante kun pacienco la malfeli&ccirc;a&jcirc;ojn.  
    </dif>
  </snc>
</drv>
</art>
