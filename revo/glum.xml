<art mrk="$Id: glum.xml,v 1.20 2024/04/08 08:28:03 revo Exp $">
<kap><rad>glum</rad>/o</kap>
<drv mrk="glum.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      &Ccirc;e la <ref tip="malprt" cel="poac.0oj">poacoj</ref>,
      &ccirc;iu el la du <ref tip="super" cel="brakte.0o">brakteoj</ref>,
      kovrantaj la spiketon:
      <ekz>
        interna kaj ekstera <tld/>oj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lodikl.0o">lodiklo</ref>,
      <ref cel="pale.0o">paleo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_u;&c_s;&c_k;&c_a; <klr tip="amb">(&c_k;&c_a;&c_l;&c_o;&c_s;&c_n;&c_a;&c_ja;)</klr></trd>
  <trd lng="bg">&c_p;&c_l;&c_ja;&c_v;&c_a;</trd>
  <trd lng="ca">gluma</trd>
  <trd lng="cs">pleva</trd>
  <trdgrp lng="de">
    <trd>H&uuml;llspelze</trd>,
    <trd>Spelze <klr tip="ind">(H&uuml;ll-)</klr></trd>
  </trdgrp>
  <trd lng="fr">glume</trd>
  <trd lng="hu">pelyva</trd>
  <trd lng="ja">&#21253;&#x7A4E;  <pr>&#x304F;&#x308B;&#x7A4E;</pr></trd>
  <trdgrp lng="nl">
    <trd>kaf</trd>,
    <trd>vruchtwand</trd>,
    <trd>zaadhuid</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>gluma</trd>,
   <trd>espat&eacute;lia</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a; &c_z;&c_je;&c_r;&c_n;&c_a;</trd>,
    <trd>&c_k;&c_r;&c_o;&c_ju;&c_shch;&c_a;&c_ja; &c_ch;&c_je;&c_sh;&c_u;&c_j;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_v;&c_o;&c_r;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">pleva</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43B;&c_ib;&c_v;&#x43A;&c_a;</trd>,
    <trd>&#x43A;&#x43E;&#x43B;&#x43E;&c_s;&#x43A;&#x43E;&c_v;&c_a; &#x43B;&c_u;&c_s;&#x43A;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#x7A4E; <pr>y&#x1D0;ng</pr></trd>
</drv>
</art>
