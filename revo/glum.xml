<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: glum.xml,v 1.21 2024/08/14 18:32:30 revo Exp $">
<kap>
  <rad>glum</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="glum.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      &Ccirc;e la <ref tip="malprt" cel="poac.0oj">poacoj</ref>,
      &ccirc;iu el la du <ref tip="super" cel="brakte.0o">brakteoj</ref>,
      kovrantaj la spiketon:
      <ekz>
        interna kaj ekstera <tld/>oj;
      </ekz>
      <ekz>
        vi povas vidi blankecajn stamenojn eli&gcirc;intaj el helverdaj <tld/>oj, florado de
        rizo da&ubreve;ras nur 90 minutojn
        <fnt>
          <aut>Vastalto</aut>
          <vrk><url ref="https://www.vastalto.com/blog/?paged=106"
              >Rizfloroj</url></vrk>
          <lok>2007-08-24</lok>
        </fnt>;
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lodikl.0o">lodiklo</ref>,
      <ref cel="pale.0o">paleo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_u;&c_s;&c_k;&c_a; <klr tip="amb">(&c_k;&c_a;&c_l;&c_o;&c_s;&c_n;&c_a;&c_ja;)</klr></trd>
  <trd lng="bg">&c_p;&c_l;&c_ja;&c_v;&c_a;</trd>
  <trd lng="ca">gluma</trd>
  <trd lng="cs">pleva</trd>
  <trdgrp lng="de">
    <trd>H&uuml;llspelze</trd>,
    <trd>Spelze <klr tip="ind">(H&uuml;ll-)</klr></trd>
  </trdgrp>
  <trd lng="fr">glume</trd>
  <trd lng="hu">pelyva</trd>
  <trd lng="ja">&#21253;&#31310; <pr>&#12388;&#12388;&#12415;&#12360;&#12356;</pr></trd>
  <trdgrp lng="nl">
    <trd>kaf</trd>,
    <trd>vruchtwand</trd>,
    <trd>zaadhuid</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>gluma</trd>,
   <trd>espat&eacute;lia</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a; &c_z;&c_je;&c_r;&c_n;&c_a;</trd>,
    <trd>&c_k;&c_r;&c_o;&c_ju;&c_shch;&c_a;&c_ja; &c_ch;&c_je;&c_sh;&c_u;&c_j;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_v;&c_o;&c_r;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">pleva</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_l;&c_ib;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_o;&c_s;&c_k;&c_o;&c_v;&c_a; &c_l;&c_u;&c_s;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#31310; <pr>y&#464;ng</pr></trd>
</drv>
</art>
