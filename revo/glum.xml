<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>glum</rad>/o</kap>
<drv mrk="glum.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      &Ccirc;e la <ref tip="malprt" cel="poac.0oj">poacoj</ref>,
      &ccirc;iu el la du <ref tip="super" cel="brakte.0o">brakteoj</ref>,
      kovrantaj la spiketon:
      <ekz>
        interna kaj ekstera <tld/>oj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lodikl.0o">lodiklo</ref>,
      <ref cel="pale.0o">paleo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>H&uuml;llspelze</trd>,
    <trd>Spelze <klr tip="ind">(H&uuml;ll-)</klr></trd>
  </trdgrp>
  <trd lng="fr">glume</trd>
  <trd lng="hu">pelyva</trd>
  <trdgrp lng="nl">
    <trd>kaf</trd>, 
    <trd>vruchtwand</trd>,
    <trd>zaadhuid</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>gluma</trd>, 
   <trd>espat&eacute;lia</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a; &c_z;&c_je;&c_r;&c_n;&c_a;</trd>,
    <trd>&c_k;&c_r;&c_o;&c_ju;&c_shch;&c_a;&c_ja; &c_ch;&c_je;&c_sh;&c_u;&c_j;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_v;&c_o;&c_r;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
