<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tapet.xml,v 1.28 2023/10/21 13:02:26 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>tapet</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tapet.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tapet.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Papero, teksa&jcirc;o, ledo a&ubreve; io simila, per kiu oni tegas
      la murojn de &ccirc;ambro:
      <ekz>
        ili ne povis sufi&ccirc;e admiri la nombron kaj belecon de
        l&#x2019; <tld/>oj, litoj, sofoj, tabletoj, tabloj kaj speguloj
        <fnt>
        <aut>C. Perrault, trad. P. Sarpy</aut>,
        <vrk>La Blua-Barbo</vrk>, 
        <lok>
        <url ref="http://eo.wikibooks.org/wiki/Rakontoj_pri_Feinoj">
        Rakontoj pri feinoj</url>, Hachette, 1904</lok></fnt>. 
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_p;&c_a;&c_l;&c_je;&c_r;&c_y;</trd>
  <trd lng="ca">empaperat</trd>
  <trdgrp lng="cs">
    <trd>pokr&#xFD;vka</trd>,
    <trd>vrstva vl&#xE1;ken z corpus callosum</trd>
  </trdgrp>
  <trd lng="de">Tapete</trd>
  <trdgrp lng="en">
    <trd>wall covering</trd>,
    <trd>wallpaper</trd>,
    <trd>tapestry</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>tapisserie</trd>,
    <trd>tenture</trd>,
    <trd>papier peint</trd>
  </trdgrp>
  <trd lng="hu">tap&eacute;ta</trd>
  <trdgrp lng="ja">
    <trd>&#x58C1;&#x7D19;  <pr>&#x304B;&#x3079;&#x304C;&#x307F;</pr></trd>,
    <trd>&#x58C1;&#x5E03;  <pr>&#x304B;&#x3079;&#x306C;&#x306E;</pr></trd>,
    <trd>&#x58C1;&#x639B;  <pr>&#x304B;&#x3079;&#x304B;&#x3051;</pr></trd>
  </trdgrp>
  <trd lng="nl">behang</trd>
  <trd lng="pl">tapeta</trd>
  <trdgrp lng="pt">
   <trd>tapete</trd>,
   <trd>forro</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_o;&c_i;</trd>,
    <trd>&c_o;&c_b;&c_i;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>tapeta</trd>,
    <trd>&#x10D;al&#xFA;n</trd>
  </trdgrp>
  <trd lng="sv">tapet</trd>
  <trdgrp lng="uk">
    <trd>&#x448;&#x43F;&#x430;&#x43B;&#x435;&#x440;&#x430;</trd>,
    <trd>&#x43E;&#x431;&#x431;&#x438;&#x432;&#x43D;&#x438;&#x439; &#x43C;&#x430;&#x442;&#x435;&#x440;&#x456;&#x430;&#x43B;</trd>,
    <trd>&#x43E;&#x431;&#x431;&#x438;&#x432;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>

<drv mrk="tapet.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="tapet.0i.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Tegi per <tld/>o:
      <ekz>
        per blua &scirc;alo <tld/>ita korbo<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la fasado estis <tld/>ita per vinberujo.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="super">
    <ref cel="kovr.0i">kovri</ref>,
    <ref cel="ornam.0i">ornami</ref>
  </refgrp>
  <trd lng="be">&c_a;&c_b;&c_k;&c_l;&c_je;&c_j;&c_v;&c_a;&c_c;&c_mol;
    <ind>&c_sh;&c_p;&c_a;&c_l;&c_je;&c_r;&c_a;&c_m;&c_ib;</ind></trd>
  <trd lng="ca">empaperar</trd>
  <trd lng="de">tapezieren</trd>
  <trd lng="fr">tapisser</trd>
  <trd lng="hu">tap&eacute;t&aacute;z</trd>
  <trd lng="ja">&#x58C1;&#x7D19;&#x30FB;&#x58C1;&#x5E03;&#x3092;&#x5F35;&#x308B;  <pr>&#x304B;&#x3079;&#x304C;&#x307F;&#x30FB;&#x304B;&#x3079;&#x306C;&#x306E;&#x3092;&#x306F;&#x308B;</pr></trd>
  <trd lng="nl">behangen</trd>
  <trdgrp lng="pl">
    <trd>tapetowa&cacute;</trd>,
    <trd>okleja&cacute; <ind>tapet&aogonek;</ind></trd>
  </trdgrp>
  <trd lng="sv">tapetsera</trd>
  <trd lng="sk">tapetova&tcaron;</trd>
  <trd lng="pt">forrar</trd>
  <trd lng="ru">&c_o;&c_k;&c_l;&c_je;&c_i;&c_v;&c_a;&c_t;&c_mol;
  <ind>&c_o;&c_b;&c_o;&c_ja;&c_m;&c_i;</ind></trd>
  <trdgrp lng="sk">
    <trd>pokr&#xFD;va&#x165;</trd>,
    <trd>&#x10D;al&#xFA;ni&#x165;</trd>
  </trdgrp>
  <trd lng="cs">tapetovat</trd>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x431;&#x43A;&#x43B;&#x435;&#x44E;&#x432;&#x430;&#x442;&#x438; &#x448;&#x43F;&#x430;&#x43B;&#x435;&#x440;&#x430;&#x43C;&#x438;</trd>,
    <trd>&#x43E;&#x431;&#x431;&#x438;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>
</art>
