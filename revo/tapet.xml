<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tapet.xml,v 1.31 2025/12/06 16:35:38 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>tapet</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="tapet.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tapet.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Papero, teksa&jcirc;o, ledo a&ubreve; io simila, per kiu oni tegas
      la murojn de &ccirc;ambro:
      <ekz>
        porkledaj <tld/>oj
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i donis pli da atento ol al la flordesegna&jcirc;o de la <tld/>oj
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili ne povis sufi&ccirc;e admiri la nombron kaj belecon de
        l&#x2019; <tld/>oj, litoj, sofoj, tabletoj, tabloj kaj speguloj
        <fnt>
        <aut>C. Perrault, trad. P. Sarpy</aut>,
        <vrk>La Blua-Barbo</vrk>,
        <lok>
        <url ref="http://eo.wikibooks.org/wiki/Rakontoj_pri_Feinoj">
        Rakontoj pri feinoj</url>, Hachette, 1904</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_p;&c_a;&c_l;&c_je;&c_r;&c_y;</trd>
  <trd lng="ca">empaperat</trd>
  <trdgrp lng="cs">
    <trd>pokr&yacute;vka</trd>,
    <trd>vrstva vl&aacute;ken z corpus callosum</trd>
  </trdgrp>
  <trd lng="de">Tapete</trd>
  <trdgrp lng="en">
    <trd>wall covering</trd>,
    <trd>wallpaper</trd>,
    <trd>tapestry</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>tapisserie</trd>,
    <trd>tenture</trd>,
    <trd>papier peint</trd>
  </trdgrp>
  <trd lng="hu">tap&eacute;ta</trd>
  <trdgrp lng="ja">
    <trd>&#x58C1;&#x7D19;  <pr>&#x304B;&#x3079;&#x304C;&#x307F;</pr></trd>,
    <trd>&#x58C1;&#x5E03;  <pr>&#x304B;&#x3079;&#x306C;&#x306E;</pr></trd>,
    <trd>&#x58C1;&#x639B;  <pr>&#x304B;&#x3079;&#x304B;&#x3051;</pr></trd>
  </trdgrp>
  <trd lng="nl">behang</trd>
  <trd lng="pl">tapeta</trd>
  <trdgrp lng="pt">
   <trd>tapete</trd>,
   <trd>forro</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_o;&c_i;</trd>,
    <trd>&c_o;&c_b;&c_i;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>tapeta</trd>,
    <trd>&ccaron;al&uacute;n</trd>
  </trdgrp>
  <trd lng="sv">tapet</trd>
  <trdgrp lng="uk">
    <trd>&c_sh;&c_p;&c_a;&c_l;&c_je;&c_r;&c_a;</trd>,
    <trd>&c_o;&c_b;&c_b;&c_i;&c_v;&c_n;&c_i;&c_j; &c_m;&c_a;&c_t;&c_je;&c_r;&c_ib;&c_a;&c_l;</trd>,
    <trd>&c_o;&c_b;&c_b;&c_i;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5899;&#x7EB8; <pr>qi&aacute;ngzh&#x1D0;</pr></trd>,
    <trd>&#x7246;&#x7D19; <pr>qi&aacute;ngzh&#x1D0;</pr></trd>,
    <trd>&#x58C1;&#x7D19; <pr>b&igrave;zh&#x1D0;</pr></trd>,
    <trd>&#x58C1;&#x7EB8; <pr>b&igrave;zh&#x1D0;</pr></trd>,
    <trd>&#x7CCA;&#x5899;&#x7EB8; <pr>h&uacute;qi&aacute;ngzh&#x1D0;</pr></trd>,
    <trd>&#x7CCA;&#x7246;&#x7D19; <pr>h&uacute;qi&aacute;ngzh&#x1D0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="tapet.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="tapet.0i.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="teg.0i.alglui">Tegi</ref> per <tld/>o:
      <ekz>
        blua papero ru&gcirc;e punktita <tld/>is la murojn
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        via &ccirc;ambro estas fre&scirc;e <tld/>ita
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>III</lok>
        </fnt>;
      </ekz>
      <ekz>
        muroj <klr>[&#x2026;]</klr> <tld/>itaj per brik-imita papero
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 9</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;tatoficisto kun pasia intereso pri pentrado, <tld/>os la murojn de la urbeto
        per centoj da larmeltiraj nekrologoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2011/010281.php"
              >Mankas kapro en lia kapo</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="super">
      <ref cel="kovr.0i">kovri</ref>,
      <ref cel="ornam.0i">ornami</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_k;&c_l;&c_je;&c_j;&c_v;&c_a;&c_c;&c_mol;
    <ind>&c_sh;&c_p;&c_a;&c_l;&c_je;&c_r;&c_a;&c_m;&c_ib;</ind></trd>
  <trd lng="ca">empaperar</trd>
  <trd lng="de">tapezieren</trd>
  <trd lng="fr">tapisser</trd>
  <trd lng="hu">tap&eacute;t&aacute;z</trd>
  <trd lng="ja">&#x58C1;&#x7D19;&#x30FB;&#x58C1;&#x5E03;&#x3092;&#x5F35;&#x308B;  <pr>&#x304B;&#x3079;&#x304C;&#x307F;&#x30FB;&#x304B;&#x3079;&#x306C;&#x306E;&#x3092;&#x306F;&#x308B;</pr></trd>
  <trd lng="nl">behangen</trd>
  <trdgrp lng="pl">
    <trd>tapetowa&cacute;</trd>,
    <trd>okleja&cacute; <ind>tapet&aogonek;</ind></trd>
  </trdgrp>
  <trd lng="sv">tapetsera</trd>
  <trd lng="sk">tapetova&tcaron;</trd>
  <trd lng="pt">forrar</trd>
  <trd lng="ru">&c_o;&c_k;&c_l;&c_je;&c_i;&c_v;&c_a;&c_t;&c_mol;
  <ind>&c_o;&c_b;&c_o;&c_ja;&c_m;&c_i;</ind></trd>
  <trdgrp lng="sk">
    <trd>pokr&yacute;va&tcaron;</trd>,
    <trd>&ccaron;al&uacute;ni&tcaron;</trd>
  </trdgrp>
  <trd lng="cs">tapetovat</trd>
  <trdgrp lng="uk">
    <trd>&c_o;&c_b;&c_k;&c_l;&c_je;&c_ju;&c_v;&c_a;&c_t;&c_i; &c_sh;&c_p;&c_a;&c_l;&c_je;&c_r;&c_a;&c_m;&c_i;</trd>,
    <trd>&c_o;&c_b;&c_b;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8CBC;&#x7246;&#x7D19; <pr>ti&emacron;qi&aacute;ngzh&#x1D0;</pr></trd>,
    <trd>&#x8D34;&#x5899;&#x7EB8; <pr>ti&emacron;qi&aacute;ngzh&#x1D0;</pr></trd>
  </trdgrp>
</drv>
</art>
