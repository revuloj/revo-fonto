<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>tapet</rad>/o
</kap>
<drv mrk="tapet.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Papero, teksa&jcirc;o, ledo a&ubreve; io simila, per kiu oni tegas la
      murojn de
      &ccirc;ambro.
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_p;&c_a;&c_l;&c_je;&c_r;&c_y;</trd>
  <trd lng="de">Tapete</trd>
  <trd lng="nl">behang</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_o;&c_i;</trd>,
    <trd>&c_o;&c_b;&c_i;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sv">tapet</trd>
  <trd lng="sk">tapeta</trd>
</drv>

<drv mrk="tapet.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Tegi per <tld/>o:
      <ekz>
        per blua &scirc;alo <tld/>ita korbo<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la fasado estis <tld/>ita per vinberujo.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="super">
    <ref cel="ornam.0i">ornami</ref>,
    <ref cel="kovr.0i">kovri</ref>.
  </refgrp>
  <trd lng="be">&c_a;&c_b;&c_k;&c_l;&c_je;&c_j;&c_v;&c_a;&c_c;&c_mol; 
    <ind>&c_sh;&c_p;&c_a;&c_l;&c_je;&c_r;&c_a;&c_m;&c_ib;</ind></trd>
  <trd lng="de">tapezieren</trd>
  <trd lng="nl">behangen</trd>
  <trd lng="sv">tapetsera</trd>
  <trd lng="sv">tapetova&tcaron;</trd>

</drv>
</art>
