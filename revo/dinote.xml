<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dinote.xml,v 1.19 2023/11/04 14:29:02 revo Exp $">
<kap>
  <rad>dinoteri</rad>/o
</kap>
<drv mrk="dinote.0o">
  <kap><tld/>o</kap>
  <snc mrk="dinote.0o.ZOO">
    <uzo tip="fak">PAL</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">Genro</ref>
      de fosiliaj <ref tip="malprt" cel="mam.0uloj">mamuloj</ref>
      simila al <ref cel="elefan.0o">elefanto</ref>, sed iom pli granda.
      Diference de la elefanto la dentegoj estas en la suba makzelo kaj
      kurbi&gcirc;as malsupren:
      <ekz>
        <tld/>oj vivis de la <ref tip="malprt" cel="miocen.0o">mioceno</ref> 
        anta&ubreve; 22 jarmilionoj &gcirc;is <ref tip="malprt" cel="plejst.0o">plejstoceno</ref>
        anta&ubreve; 1 jarmiliono;
      </ekz>
      <ekz>
        oni jam en la 17a jc trovis ostojn de <tld/>o &ccirc;e Liono
        <fnt>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="mamut.0o">mamuto</ref>
    <bld lok="&WCU;/thumb/2/2b/Dinotherium.jpg/640px-Dinotherium.jpg" prm="PD">desegno de <tld/>o de Heinrich Harder (oni nenion scias pri oreloj, lipoj kaj rostron)
        <fnt>
          <aut>Necrophorus</aut>
          <lok><url ref="&WCW;/File:Dinotherium.jpg">&WCW;/File:Dinotherium.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_n;&c_a;&c_t;&c_e;&c_r;&c_y;&c_j;</trd>
  <trd lng="bg">&c_d;&c_je;&c_j;&c_n;&c_o;&c_t;&c_je;&c_r;&c_i;&c_u;&c_m;</trd>
  <trd lng="de">Deinotherium</trd>
  <trd lng="en">deinotherium</trd>
  <trd lng="es">dinoterio</trd>
  <trd lng="fr">dinoth&eacute;rium</trd>
  <trdgrp lng="hu">
    <trd>deinotherium</trd>,
    <trd>dinotherium</trd>
  </trdgrp>
  <trd lng="ja">&#x30C7;&#x30A3;&#x30CE;&#x30C6;&#x30EA;&#x30A6;&#x30E0;</trd>
  <trd lng="la">deinotherium</trd>
  <trd lng="nl">deinotherium</trd>
  <trd lng="pl">deinotherium</trd>
  <trd lng="pt">dinot&eacute;rio</trd>
  <trd lng="ru">&c_d;&c_i;&c_n;&c_o;&c_t;&c_je;&c_r;&c_i;&c_j;</trd>
  <trd lng="uk">&#x434;&#x438;&#x43D;&#x43E;&#x442;&#x435;&#x440;&#x456;&#x439;</trd>
  <trd lng="zh">&#x6050;&#x8C61; <pr>k&#x1D2;ngxi&#xE0;ng</pr></trd>
</drv>
</art>
