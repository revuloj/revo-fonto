<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>Neptun</rad>/o</kap>
<drv mrk="neptun.0o">
  <kap><tld/>o</kap>
  <snc mrk="neptun.0o.mita">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Romia <ref tip="super" cel="di.0o.politeisma">dio</ref>
      de fontoj, riveroj kaj maroj. Analogo de helena
      <ref tip="vid" cel="pozidon.0o">Pozidono</ref>
    </dif>
  </snc>
  <snc mrk="neptun.0o.planedo">
    <uzo tip="fak">AST</uzo>
    <dif>
      La oka (la&ubreve; distanco de la <ref tip="vid"
      cel="sun.0o.nia">Suno</ref>)
      <ref tip="super" cel="planed.0o">planedo</ref>
      de la <ref tip="malprt" cel="sun.0sistemo">sunsistemo</ref>.
    </dif>
    <trd lng="zh">&#28023;&#29579;&#26143;<klr> [H&#462;iw&aacute;ngx&#299;ng]</klr></trd>
  </snc>
  <trd lng="be">&c_N;&c_e;&c_p;&c_t;&c_u;&c_n;</trd>
  <trd lng="de">Neptun</trd>
  <trd lng="el">&Pi;&omikron;&sigma;&epsilon;&jota;&delta;&omega_acute;&ny;&alfa;&sigma_fina;</trd>
  <trd lng="en">Neptune</trd>
  <trd lng="es">Neptuno</trd>
  <trd lng="fr">Neptune</trd>
  <trd lng="it">Nettuno</trd>
  <trd lng="ru">&c_N;&c_je;&c_p;&c_t;&c_u;&c_n;</trd>
</drv>
</art>

