<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: neptun.xml,v 1.17 2019/06/30 18:21:48 revo Exp $">
<kap>
  <rad>Neptun</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="neptun.0o">
  <kap><tld/>o</kap>
  <snc mrk="neptun.0o.mita">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Romia <ref tip="super" cel="di.0o.politeisma">dio</ref>
      de fontoj, riveroj kaj maroj, analogo de helena
      <ref tip="vid" cel="pozidon.0o">Pozidono</ref>:
      <ekz>
        <tld/>o estis anka&ubreve; dio de tertremoj, lia festotago en la antikva Romo estis la
        23-a de julio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=876"
              >Neptuno (dio)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">Neptunus</trd>
  </snc>
  <snc mrk="neptun.0o.AST">
    <uzo tip="fak">AST</uzo>
    <mlg>&#9798;</mlg>
    <dif>
      La oka (la&ubreve; distanco de la <ref tip="vid"
      cel="sun.0o.nia">Suno</ref>)
      <ref tip="lst" cel="planed.0o" lst="voko:planedoj">planedo</ref>
      de la <ref tip="malprt" cel="sun.0sistemo">sunsistemo</ref>:
      <ekz>
        la astrologoj atentas nur la poziciojn de la suno, luno kaj kvin planedoj kaj
        tute neglektas la poste malkovritajn Uranon, <tld/>on, Plutonon
        <fnt>
          <bib>Monato</bib>
          <aut>Last</aut>
          <vrk><url ref="&Monato;/2007/008953.php"
              >Astrologoj progresas</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">Neptunusz</trd>
    <trd lng="zh">&#28023;&#29579;&#26143;<klr>
    [H&#462;iw&aacute;ngx&#299;ng]</klr></trd>
  </snc>
  <trd lng="be">&c_N;&c_e;&c_p;&c_t;&c_u;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>Neptun</trd>,
    <trd>Neptunus</trd>
  </trdgrp>
  <trd lng="de">Neptun</trd>
  <trd lng="el">&Pi;&omikron;&sigma;&epsilon;&jota;&delta;&omega_acute;&ny;&alfa;&sigma_fina;</trd>
  <trd lng="en">Neptune</trd>
  <trd lng="es">Neptuno</trd>
  <trd lng="fa">&a_n;&f_p;&a_t;&a_w;&a_n;</trd>
  <trd lng="fr">Neptune</trd>
  <trd lng="id">Neptunus</trd>
  <trd lng="it">Nettuno</trd>
  <trd lng="nl">Neptunus</trd>
  <trd lng="pl">Neptun</trd>
  <trd lng="pt">Netuno</trd>
  <trd lng="ru">&c_N;&c_je;&c_p;&c_t;&c_u;&c_n;</trd>
  <trd lng="sk">Nept&uacute;n</trd>
</drv>
</art>
