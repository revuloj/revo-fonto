<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: empetr.xml,v 1.7 2023/11/09 18:28:45 revo Exp $">
<kap>
  <rad>empetr</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="empetr.0o">
  <kap><tld/>o</kap>
  <snc mrk="empetr.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Malalta ligna vegeta&jcirc;o, <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">genro</ref>
      el familio erikacoj (<trd lng="la">Empetrum</trd>) kun skvamaj folioj,
      trovi&gcirc;anta precipe en malvarmaj landoj, en tundro, torfejoj, malsekaj arbaroj,
      kun man&gcirc;eblaj berosimilaj drupetoj:
      <ekz>
        la &ccirc;efaj vaskulaj plantoj renkontataj estas la nana saliko, <klr>[&#8230;]</klr> la
        nigra <tld/>o, la ru&gcirc;a vakcinio, la mirtelo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=394811"
              >Nacia parko Fulufj&auml;llet</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="empetr.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="E"/>a drupo.
    </dif>
  </snc>
    <bld
      lok="&WCU;/thumb/a/a0/Empetrum_rubrum_(Terre_de_Feu).jpg/320px-Empetrum_rubrum_(Terre_de_Feu).jpg"
      prm="CC BY-SA 3.0"
      >ru&gcirc;a <tld/>o
        <fnt>
          <aut>Serge Ouach&eacute;e, propra verko</aut>
          <lok><url ref="&WCW;/File:Empetrum_rubrum_(Terre_de_Feu).jpg"
              >https://commons.wikimedia.org/wiki/File:Empetrum_rubrum_(Terre_de_Feu).jpg</url></lok>
        </fnt>
    </bld>
  <trd lng="be">&c_b;&c_a;&c_g;&c_n;&c_o;&c_w;&c_k;&c_a;</trd>
  <trd lng="cs">&scaron;&iacute;cha &ccaron;ern&aacute;</trd>
  <trd lng="de">Kr&auml;henbeere</trd>
  <trd lng="fr">caramine</trd>
  <trd lng="pl">ba&zdot;yna</trd>
  <trd lng="sk">&scaron;ucha &ccaron;ierna</trd>
  <trd lng="uk">&c_v;&c_o;&c_d;&c_ja;&c_n;&c_k;&c_a;</trd>
</drv>

<drv mrk="empetr.nigra0o">
  <kap>nigra <tld/>o</kap>
  <snc mrk="empetr.nigra0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Specio de <tld/>o kun nigraj drupetoj
      (<trd lng="la">Enpetrum nigrum</trd>):
      <ekz>
        vera franda&jcirc;o estis la deserto, kie nigraj <tld/>oj
        estis miksitaj kun sukero kaj fi&scirc;a graso
        <fnt>
          <aut>J. Lindstedt</aut>,
          <vrk><url ref="http://lingvatereno.blogspot.fr/2014/11/orienta-taglibro-8.html">
          Orienta Taglibro (8)</url></vrk>,
          <lok>2014-08-14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
    <bld
      lok="&WCU;/thumb/5/5c/Empetrum_nigrum_01.JPG/640px-Empetrum_nigrum_01.JPG"
      prm="CC BY-SA 3.0"
      >nigra <tld/>o, Svislando
        <fnt>
          <aut>H. Zell, propra verko</aut>
          <lok><url ref="&WCW;/File:Empetrum_nigrum_01.JPG"
              >https://commons.wikimedia.org/wiki/File:Empetrum_nigrum_01.JPG</url></lok>
        </fnt>
    </bld>
  <trd lng="be">&c_b;&c_a;&c_g;&c_n;&c_o;&c_w;&c_k;&c_a; &c_ch;&c_o;&c_r;&c_n;&c_a;&c_ja;</trd>
  <trd lng="de">Schwarze <ind>Kr&auml;henbeere</ind> </trd>
  <trd lng="fr">caramine noire</trd>
  <trd lng="pl">ba&zdot;yna czarna</trd>
</drv>
</art>

