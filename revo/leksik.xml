<drv mrk="leksik.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Scienco pri la vortoj studataj la&ubreve; ilia etimologio, signifo kaj
      uzado:
      <ekz>
        internacia simpozio pri lingva planado kaj <tld/>o
        <fnt>
        <aut>H. Gorecka</aut>,
        <vrk>La Ondo de Esperanto</vrk>,
        <lok>soc.culture.esperanto, 2001-03-26</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_je;&c_k;&c_s;&c_ib;&c_k;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="el">&lambda;&epsilon;&xi;&jota;&kappa;&omikron;&lambda;&omikron;&gamma;&jota_ton;&alfa;</trd>
  <trdgrp lng="de">
    <trd>Lexikologie</trd>,
    <trd>Wortschatzerforschung</trd>
  </trdgrp>
  <trd lng="en">lexicology</trd>
  <trd lng="es">lexicolog&iacute;a</trd>
  <trd lng="fr">lexicologie</trd>
  <trdgrp lng="hu">
    <trd>lexikol&oacute;gia</trd>,
    <trd>sz&oacute;k&eacute;szlettan</trd>,
    <trd>sz&oacute;kincstan</trd>
  </trdgrp>
  <trd lng="nl">lexicologie</trd>
  <trd lng="pt">lexicologia</trd>
  <trd lng="ru">&c_l;&c_je;&c_k;&c_s;&c_i;&c_k;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
</drv>
</art>
