        kriis<fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro 41a, p.
        312a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">simitarra</trd>
  <trd lng="es">cimitarra</trd>
  <trd lng="fr">cimeterre</trd>
</drv>
