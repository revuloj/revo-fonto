<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tinus</rad>/o
  <fnt>PIV1</fnt>
</kap>

<drv mrk="tinus.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="skombr1.0oj">skombredoj</ref>
      (<trd lng="la">Thunnus</trd>), granda marfi&scirc;o kun firma, bongusta
      karno iom similanta la bovidan viandon.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_n;&c_je;&c_c;</trd>
  <trd lng="bg">&c_t;&c_o;&c_n;</trd>
  <trd lng="de">Thunfisch</trd>
  <trd lng="en">tuna</trd>
  <trd lng="es">at&uacute;n</trd>
  <trd lng="fr">thon</trd>
  <trd lng="hu">tonhal</trd>
  <trd lng="it">tonno</trd>
  <trd lng="nl">tonijn</trd>
  <trdgrp lng="pt">
    <trd>atum</trd>,
    <trd>albacora</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_u;&c_n;&c_je;&c_c;</trd>
</drv>
</art>
