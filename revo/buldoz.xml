<art mrk="$Id: buldoz.xml,v 1.17 2023/08/31 08:48:54 revo Exp $">
<kap>
  <rad>buldoz</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="buldoz.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Fortika kaj peza ra&ubreve;p<ref cel="vetur.0ilo">veturilo</ref>
      destinita por senradikigi kaj niveli grundon.
      <ekz>
        per elektraj segiloj oni distran&ccirc;is la palisarojn &ccirc;irka&ubreve; la domoj, por ke <tld/>oj pli facile povu forigi la teron el la &gcirc;ardenoj
        <fnt>
          <bib>Monato</bib>
          <aut>Thierry Salomon</aut>
          <vrk><url ref="&Monato;/2010/010202.php">Ru&gcirc;&scirc;lima vulkano</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en la jaro 1938 diversaj konstruma&scirc;inoj jam aperis en la mondo, tamen estis neniu <tld/>o dum la konstruado de la &scirc;oseo
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="&Monato;/2011/010328.php">Fragmentoj de la dua mondmilito</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_l;&c_mol;&c_d;&c_o;&c_z;&c_e;&c_r;</trd>
  <trd lng="bg">&c_b;&c_u;&c_l;&c_d;&c_o;&c_z;&c_je;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>buldozer</trd>,
    <trd>&ccaron;eln&iacute; shrnova&ccaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Planierraupe</trd>,
    <trd>Schubraupe</trd>,
    <trd>Bulldozer</trd>,
    <trd>Flachbagger</trd>
  </trdgrp>
  <trd lng="en">bulldozer</trd>
  <trd lng="es">bulldozer</trd>
  <trdgrp lng="fr">
    <trd>bulldozer</trd>,
    <trd>bouteur</trd>
  </trdgrp>
  <trd lng="he">&#x5D3;&#x5B7;&#x5D7;&#x5E4;&#x5BC;&#x5D5;&#x5B9;&#x5E8;</trd>
  <trd lng="hu">buld&oacute;zer</trd>
  <trd lng="ja">&#x30D6;&#x30EB;&#x30C9;&#x30FC;&#x30B6;&#x30FC;</trd>
  <trdgrp lng="pl">
    <trd>buldo&zdot;er</trd>,
    <trd>spychacz</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_u;&c_l;&c_mol;&c_d;&c_o;&c_z;&c_je;&c_r;</trd>
  <trd lng="sk">buldoz&eacute;r</trd>
  <trd lng="uk">&#x431;&#x443;&#x43B;&#x44C;&#x434;&#x43E;&#x437;&#x435;&#x440;</trd>
</drv>
</art>
