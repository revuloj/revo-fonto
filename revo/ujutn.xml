<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ujutn.xml,v 1.6 2023/12/05 08:31:40 revo Exp $">
<kap>
  <rad>ujutn</rad>/a
</kap>
<drv mrk="ujutn.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Bonetosa, <ref cel="komfor.0a">komforta</ref>, <ref cel="hejm.0eca">hejmeca</ref>:
      <ekz>
        bonegaj man&gcirc;oj, <tld/>aj, amuzaj programeroj kaj ege interesa
        ekskurso
        <fnt>
          <vrk>Sukcesaj kulturtagoj en Sarpsborg</vrk>
          <lok>Internacia fervojisto, 1994-04</lok>
        </fnt>;
      </ekz>
      <ekz>
        gemuta, <tld/>a situacio a&ubreve; kunteksto
        <fnt>
          <aut>Federico Gobbo</aut>
          <vrk>Amikaro</vrk>
          <lok>jaro 2019a, p. 69a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="agrabl.0a">agrabla</ref>,
      <ref cel="distr.0a">distra</ref>
    </refgrp>
    <trd lng="be">&c_u;&c_t;&c_u;&c_l;&c_mol;&c_n;&c_y;</trd>
    <trd lng="af">gesellig</trd>
    <trd lng="da">hyggelig</trd>
    <trdgrp lng="de">
      <trd>gem&uuml;tlich</trd>,
      <trd>behaglich</trd>,
      <trd>beschaulich</trd>,
      <trd>kuschelig</trd>,
      <trd>gesellig</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>acogedor</trd>,
      <trd>confortable</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>cosy</trd>,
      <trd>cozy</trd>
    </trdgrp>
    <trd lng="fy">gesellich</trd>
    <trd lng="nl">gezellig</trd>
    <trd lng="pt">aconchegante</trd>
    <trd lng="ru">&c_u;&c_ju;&c_t;&c_n;&c_y;&c_j;</trd>
  </snc>
</drv>
</art>

