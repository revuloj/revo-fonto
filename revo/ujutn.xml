<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ujutn.xml,v 1.4 2020/03/20 13:45:22 revo Exp $">
<kap>
  <rad>ujutn</rad>/a
</kap>
<drv mrk="ujutn.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Bonetosa, komforta, hejmeca:
      <ekz>
        bonegaj man&gcirc;oj, <tld/>aj, amuzaj programeroj kaj ege interesa
        ekskurso
        <fnt>
          <vrk>Sukcesaj kulturtagoj en Sarpsborg</vrk>
          <lok>Internacia fervojisto, 1994-04</lok>
        </fnt>;
      </ekz>
      <ekz>
        gemuta, <tld/>a situacio a&ubreve; kunteksto
        <fnt>
          <aut>Federico Gobbo</aut>
          <vrk>Amikaro</vrk>
          <lok>jaro 2019a, p. 69a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="af">gesellig</trd>
    <trd lng="da">hyggelig</trd>
    <trd lng="de">gem&uuml;tlich</trd>
    <trdgrp lng="es">
      <trd>acogedor</trd>,
      <trd>confortable</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>cosy</trd>,
      <trd>cozy</trd>
    </trdgrp>
    <trd lng="fy">gesellich</trd>
    <trd lng="nl">gezellig</trd>
    <trd lng="pt">aconchegante</trd>
    <trd lng="ru">&c_u;&c_ju;&c_t;&c_n;&c_y;&c_j;</trd>
  </snc>
</drv>
</art>

