<kap>
  <rad>old</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="old.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="old.0a.maljuna">
    <dif>
      <ref tip="dif" cel="jun.mal0a">maljuna</ref>
      <ekz>
        kelkaj grandaj &ccirc;aroj entenis <tld/>ajn kaj junajn servistinojn
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 59a, volumo 2a, p.
        291a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">bejahrt</trd>
    <trdgrp lng="fa">
      <trd>&f_p;&f_y;&a_r;</trd>&a_komo;
      <trd>&a_s;&a_A;&a_l;&a_m;&a_n;&a_d;</trd>&a_komo;
      <trd>&f_k;&a_h;&a_n;&a_s;&a_A;&a_l;</trd>
    </trdgrp>
    <trd lng="hu">&ouml;reg</trd>
    <trd lng="io">olda</trd>
  </snc>
  <snc mrk="old.0a.malnova">
    <dif>
      <ref tip="dif" cel="nov.mal0a">malnova</ref>
      <ekz>
        Somere li kutimis ripozi en la silenta kaj friska korto de la
        <tld/>a gepatra domo
        <fnt>
        <aut>Julian Modest</aut>,
        <vrk>La ikono</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2001a, numero 12a, p. 23a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">alt</trd>
    <trdgrp lng="fa">
      <trd>&f_k;&a_h;&a_n;</trd>&a_komo;
      <trd>&a_b;&a_A;&a_s;&a_t;&a_A;&a_n;&f_y;</trd>&a_komo;
      <trd>&a_q;&a_d;&f_y;&a_m;&f_y;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>ancien<klr> (adj.)</klr></trd>
    </trdgrp>
    <trd lng="hu">r&eacute;gi</trd>
    <trd lng="io">anciena</trd>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_r;&c_y;</trd>
  <trd lng="cs">star&yacute; <klr>(tvor, v&ecaron;c)</klr></trd>
  <trd lng="en">old</trd>
  <trdgrp lng="fr">
    <trd>vieux<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="fy">&acirc;ld</trd>
  <trd lng="nl">oud<klr tip="ind"> (ontr.)</klr></trd>
  <trdgrp lng="pl">
    <trd>leciwy</trd>,
    <trd>s&eogonek;dziwy</trd>,   
    <trd>stary</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>velho <klr tip="amb">(adj.)</klr></trd>,
    <trd>anci&atilde;o <klr tip="amb">(adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>b&abreve;tr&acirc;n</trd>,
    <trd>b&abreve;tr&icirc;n</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_r;&c_y;&c_j;</trd>
  <trd lng="sk">star&yacute;</trd>
  <trd lng="uk">&#x441;&#x442;&#x430;&#x440;&#x438;&#x439;</trd>
</drv>
