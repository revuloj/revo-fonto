<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>old</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="old.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">POE</uzo>
  <snc mrk="old.0a.maljuna">
    <dif>
      <ref tip="dif" cel="jun.mal0a">maljuna</ref>
      <ekz>
        kelkaj grandaj &ccirc;aroj entenis <tld/>ajn kaj junajn servistinojn
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 59a, p. 291a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">bejahrt</trd>
    <trd lng="hu">&ouml;reg</trd>
  </snc>
  <snc mrk="old.0a.malnova">
    <dif>
      <ref tip="dif" cel="nov.mal0a">malnova</ref>
      <ekz>
        Somere li kutimis ripozi en la silenta kaj friska korto de la
        <tld/>a gepatra domo
        <fnt><aut>Julian Modest</aut>: <vrk>La ikono</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 12a, p. 23a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>ancien<klr> (adj.)</klr></trd>
    </trdgrp>
    <trd lng="hu">r&eacute;gi</trd>
  </snc>
  <trd lng="de">alt</trd>
  <trdgrp lng="fr">
    <trd>vieux<klr> (adj.)</klr></trd>
  </trdgrp>
</drv>
</art>

