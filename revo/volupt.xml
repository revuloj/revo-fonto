    <trd>&c_u;&c_c;&c_je;&c_h;&c_a;</trd>,
    <trd>&c_ju;&c_r;</trd>
  </trdgrp>
  <trd lng="ca">voluptuositat</trd>
  <trd lng="de">Wollust</trd>
  <trd lng="el">&eta;&delta;&omikron;&ny;&eta_ton;</trd>
  <trd lng="en">voluptuous</trd>
  <trd lng="es">voluptuosidad</trd>
  <trdgrp lng="hu">
    <trd>k&eacute;j</trd>,
    <trd>gy&ouml;ny&ouml;r</trd>
  </trdgrp>
  <trd lng="nl">wellust</trd>
  <trd lng="pt">vol&uacute;pia</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_a;&c_d;&c_o;&c_s;&c_t;&c_r;&c_a;&c_s;&c_t;&c_i;&c_je;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_l;&c_a;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
