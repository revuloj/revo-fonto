<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>volupt</rad>/o
</kap>
<drv mrk="volupt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Plezuro de la sentumoj ka&ubreve;zanta tre vivan &gcirc;uon:
      <ekz>
        &gcirc;ui <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li ser&ccirc;igis lin &ccirc;e &ccirc;iuj <tld/>vendistinoj;
      </ekz>
      <ekz>
        <tld/>ama temperamento.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Wollust</trd>
  <trd lng="en">voluptuous</trd>
  <trd lng="nl">wellust</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_a;&c_d;&c_o;&c_s;&c_t;&c_r;&c_a;&c_s;&c_t;&c_i;&c_je;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_l;&c_a;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
