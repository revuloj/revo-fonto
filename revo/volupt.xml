<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>volupt</rad>/o
</kap>
<drv mrk="volupt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Plezuro de la sentumoj ka&ubreve;zanta tre vivan &gcirc;uon:
      <ekz>
        &gcirc;ui <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li ser&ccirc;igis lin &ccirc;e &ccirc;iuj <tld/>vendistinoj;
      </ekz>
      <ekz>
        <tld/>ama temperamento.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_l;&c_a;&c_d;&c_a;&c_s;&c_t;&c_r;&c_a;&c_s;&c_n;&c_a;&c_s;&c_c;&c_mol;</trd>,
