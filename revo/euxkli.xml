<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>E&ubreve;klid</rad>/o
</kap>
<drv mrk="euxkli.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Greklingve:
      <trd lng="grc">&Epsilon;&ypsilon;&kappa;&lambda;&epsilon;&jota_acute;&delta;&eta;&sigma_fina;</trd>.
      Greka matematikisto el Aleksandrio, 3-a jc a.K.
    </dif>
  </snc>
  <trd lng="en">Euclid</trd>
  <trd lng="fr">Euclide</trd>
  <trdgrp lng="ru">
    <trd>&c_Je;&c_v;&c_k;&c_l;&c_i;&c_d;</trd>,
    <trd>&c_E;&c_v;&c_k;&c_l;&c_i;&c_d;</trd>
  </trdgrp>
  <trd lng="sv">Euklides</trd>
</drv>
<drv mrk="euxkli.e0a">
  <kap><tld lit="e"/>a</kap>
  <snc>
    <refgrp tip="vid">
      <ref cel="algor.euxklida0o"><tld lit="e"/>a algoritmo</ref>,
      <ref cel="divid.euxklida0o"><tld lit="e"/>a divido</ref>,
      <ref cel="ring.euxklida0o"><tld lit="e"/>a ringo</ref>,
      <ref cel="spac.euxklida0o"><tld lit="e"/>a spaco</ref>.
    </refgrp>
  </snc>
  <trd lng="en">Euclidean</trd>
  <trd lng="fr">euclidien</trd>
  <trd lng="ru">&c_Je;&c_v;&c_k;&c_l;&c_i;&c_d;&c_o;&c_v;</trd>
  <trd lng="sv">euklidisk</trd>
</drv>
</art>
