<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>E&ubreve;klid</rad>/o
</kap>
<drv mrk="euxkli.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Greklingve:
      <trd lng="grc">&Epsilon;&ypsilon;&kappa;&lambda;&epsilon;&jota_acute;&delta;&eta;&sigma_fina;</trd>.
      Greka matematikisto el Aleksandrio, 3-a jc a.K.
    </dif>
  </snc>
  <trd lng="de">Euklid</trd>
  <trd lng="en">Euclid</trd>
  <trd lng="fr">Euclide</trd>
  <trd lng="hu">Eukleid&eacute;sz</trd>
  <trd lng="pl">Euklides</trd>
  <trdgrp lng="ru">
    <trd>&c_Je;&c_v;&c_k;&c_l;&c_i;&c_d;</trd>,
    <trd>&c_E;&c_v;&c_k;&c_l;&c_i;&c_d;</trd>
  </trdgrp>
  <trd lng="sv">Euklides</trd>
</drv>

<drv mrk="euxkli.e0a">
  <kap><tld lit="e"/>a</kap>
  <snc>
    <refgrp tip="vid">
      <ref cel="algor.euxklida0o"><tld lit="e"/>a algoritmo</ref>,
      <ref cel="divid.euxklida0o"><tld lit="e"/>a divido</ref>,
      <ref cel="ring.euxklida0o"><tld lit="e"/>a ringo</ref>,
      <ref cel="spac.euxklida0o"><tld lit="e"/>a spaco</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>von <ind>Euklid</ind></trd>,
    <trd>Euklids</trd>,
    <trd>euklidisch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Euclid['s]</trd>,
    <trd>Euclidean</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d'<ind>Euclide</ind></trd>,
    <trd>euclidien</trd>
  </trdgrp>
  <trd lng="hu">euklideszi</trd>
  <trdgrp lng="pl">
    <trd>Euklidesa</trd>,
    <trd>euklidesowy</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_Je;&c_v;&c_k;&c_l;&c_i;&c_d;&c_a;</trd>,
    <trd>&c_je;&c_v;&c_k;&c_l;&c_i;&c_d;&c_o;&c_v;</trd>
  </trdgrp>
  <trd lng="sv">euklidisk</trd>
</drv>
</art>
