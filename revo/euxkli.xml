<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: euxkli.xml,v 1.21 2023/11/07 20:34:12 revo Exp $">
<kap>
  <rad>E&ubreve;klid</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="euxkli.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Greklingve: <trd lng="grc">&Epsilon;&ypsilon;&kappa;&lambda;&epsilon;&jota_ton;&delta;&eta;&sigma_fina;</trd>,
      greka <ref tip="lst" lst="voko:historiaj_personoj" cel="matema.0isto">matematikisto</ref> el
      <ref tip="malprt" cel="aleksandri.A0o">Aleksandrio</ref>, 3-a jc a.K.:
      <ekz>
        <tld/>o pruvis anka&ubreve; la tiel nomatan Duan teoremon de <tld/>o: &leftquot;la nombro de
        primoj estas senfina&rightquot;
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1046">E&ubreve;klido</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>                      
  <trd lng="be">&c_E;&c_w;&c_k;&c_l;&c_ib;&c_d;</trd>
  <trd lng="br">Euklides</trd>
  <trd lng="de">Euklid</trd>
  <trd lng="en">Euclid</trd>
  <trd lng="fr">Euclide</trd>
  <trd lng="hu">Eukleid&eacute;sz</trd>
  <trd lng="pl">Euklides</trd>
  <trd lng="pt">Euclides</trd>
  <trd lng="es">Euclides</trd>
  <trdgrp lng="ru">
    <trd>&c_Je;&c_v;&c_k;&c_l;&c_i;&c_d;</trd>,
    <trd>&c_E;&c_v;&c_k;&c_l;&c_i;&c_d;</trd>
  </trdgrp>
  <trd lng="sv">Euklides</trd>
  <trd lng="uk">&#x415;&#x432;&#x43A;&#x43B;&#x456;&#x434;</trd>
  <trd lng="zh">&#x6B27;&#x51E0;&#x91CC;&#x5FB7; <pr>&#x14D;uj&#x12B;l&#x1D0;d&#xE9;</pr></trd>
</drv>

<drv mrk="euxkli.e0a">
  <kap><tld lit="e"/>a</kap>
  <snc>
    <refgrp tip="vid">
      <ref cel="algor.euxklida0o"><tld lit="e"/>a algoritmo</ref>,
      <ref cel="divid.euxklida0o"><tld lit="e"/>a divido</ref>,
      <ref cel="ring.euxklida0o"><tld lit="e"/>a ringo</ref>,
      <ref cel="spac.euxklida0o"><tld lit="e"/>a spaco</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_w;&c_k;&c_l;&c_ib;&c_d;&c_a;&c_w;</trd>    
  <trdgrp lng="br">
    <trd>euklidel</trd>,
    <trd>euklidian</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>von <ind>Euklid</ind></trd>,
    <trd>Euklids</trd>,
    <trd>euklidisch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Euclid['s]</trd>,
    <trd>Euclidean</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d'<ind>Euclide</ind></trd>,
    <trd>euclidien</trd>
  </trdgrp>
  <trd lng="hu">euklideszi</trd>
  <trdgrp lng="pl">
    <trd>Euklidesa</trd>,
    <trd>euklidesowy</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_Je;&c_v;&c_k;&c_l;&c_i;&c_d;&c_a;</trd>,
    <trd>&c_je;&c_v;&c_k;&c_l;&c_i;&c_d;&c_o;&c_v;</trd>
  </trdgrp>
  <trd lng="sv">euklidisk</trd>
  <trd lng="es">euclideano</trd>
</drv>
</art>
