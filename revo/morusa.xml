<art mrk="$Id: morusa.xml,v 1.17 2022/03/23 14:02:26 revo Exp $">
<kap>
  <rad>morusac</rad>/oj
</kap>
<drv mrk="morusa.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="morusa.0oj.BOT">
  <uzo tip="fak">BOT</uzo>
    <dif>
       <ref tip="lst" lst="voko:botanikaj_familioj" cel="famili.0o.BIO">Familio</ref> de arboj  kaj arbedoj 
      el la ordo de rozoparencaj
      <ref tip="malprt" cel="flor.0oplant">floroplantoj</ref>
      (<trd lng="la">Moraceae</trd>) kun:
      lakta suko;
      folioj nedividitaj a&ubreve; lobaj;
      tirsoj;
      floroj unuseksaj;
      karnaj grupfruktoj:
      <ekz>
        naturk&ubreve;&ccirc;uko estas elasta gumo <klr>[...]</klr> el preparita suko
        de <klr>[...]</klr>
        lianoj el e&ubreve;forbiacoj, Fiscus elastica el <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=20149">Ka&ubreve;&ccirc;uko</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la <ref tip="prt" cel="arb.pan0o">panarbo</ref> <klr>[...]</klr>
        
        el la familio de la <tld lit="M"/>oj
        estas arbo origine de Orient-Hindio kaj Pacifikaj insuloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=41583">Panarbo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="morus.0o">moruso</ref>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_t;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_ch;&c_je;&c_r;&c_n;&c_i;&c_ch;&c_je;&c_v;&c_i;</trd>
  <trd lng="cs">moru&scaron;ovn&iacute;kovit&eacute;</trd>
  <trd lng="de">Maulbeergew&auml;chse</trd>
  <trd lng="fr">Morac&eacute;es</trd>
  <trd lng="hu">eperfaf&eacute;l&eacute;k</trd>
  <trd lng="nl">moerbeienfamilie</trd>
  <trd lng="pl">morwowate</trd>     
  <trd lng="ro">moraceae</trd>
  <trd lng="ru">&c_t;&c_u;&c_t;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">moru&scaron;ovit&eacute;</trd>
  <trd lng="uk">&#x448;&#x43E;&#x432;&#x43A;&#x43E;&#x432;&#x438;&#x446;&#x435;&#x432;&#x456;</trd>
</drv>
</art>
