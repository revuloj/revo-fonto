    <trd>&#x898B;&#x305B;&#x304B;&#x3051;&#x308B;  [<ind>&#x307F;&#x305B;&#x304B;&#x3051;&#x308B;</ind>]</trd>,
    <trd>&#x3075;&#x308A;&#x3092;&#x3059;&#x308B;</trd>
  </trdgrp>
  <trd lng="nl">z. <ind>aanstellen</ind></trd>
  <trdgrp lng="pt">
        <trd>afetar</trd>,
        <trd>fingir</trd>
      </trdgrp>
  <trdgrp lng="ru">
        <trd>&c_p;&c_r;&c_i;&c_t;&c_v;&c_o;&c_r;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
        <trd>&c_l;&c_o;&c_m;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
        <trd>&c_r;&c_i;&c_s;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
        <trd>&c_zh;&c_je;&c_m;&c_a;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
      </trdgrp>
  <trdgrp lng="sk">
    <trd>afektova&tcaron;</trd>,
    <trd>neprirodzene sa spr&#xE1;va&#x165;</trd>,
    <trd>strojene</trd>
  </trdgrp>
  <trdgrp lng="uk">
