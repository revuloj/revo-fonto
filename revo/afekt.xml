        <trd>stumma&ntilde;<klr tip="ind"> (un dremm)</klr></trd>
      </trdgrp>
  <trd lng="cs">tv&#xE1;&#x159;it se</trd>
  <trdgrp lng="en"><trd>feign</trd>, <trd>pretend</trd></trdgrp>
  <trd lng="es">afectar</trd>
  <trd lng="he">&#x5DC;&#x5D4;&#x5E9;&#x5E4;&#x5D9;&#x5E2;</trd>
  <trdgrp lng="it">
