<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>afekt</rad>/i
    </kap>
    <drv mrk="afekt.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          &Scirc;ajnigi ion per nenaturaj, nesimplaj manieroj:
          <ekz>
            <tld/>i solenan 
            sintenadon, gravan mienon;
          </ekz><ekz>
            &scirc;i <tld/>adis  anta&ubreve; la spegulo;
          </ekz><ekz>
            hipokritulo &scirc;ajnigas por ka&scirc;i, <tld/>ulo por paradi.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="koket.0i">koketi</ref>,
          <ref cel="poz.0i">pozi</ref>,
          <ref cel="dand.0o">dando</ref>.
        </refgrp>
      </snc>
      <trdgrp lng="be">
        <trd>&c_p;&c_r;&c_y;&c_t;&c_v;&c_a;&c_r;&c_a;&c_c;&c_c;&c_a;</trd>, 
        <trd>&c_p;&c_r;&c_y;&c_k;&c_ib;&c_d;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>, 
        <trd>&c_v;&c_y;&c_s;&c_t;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>, 
        <trd>&c_m;&c_a;&c_n;&c_je;&c_r;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;
        </trd>
      </trdgrp>
      <trdgrp lng="br">
        <trd>ober (an) <ind>neuz</ind> (da)</trd>, 
        <trd>stumma&ntilde;<klr tip="ind">(un dremm)</klr></trd>
      </trdgrp>
      <trd lng="en">affect</trd>
      <trd lng="es">afectar</trd>
      <trdgrp lng="fr">
        <trd>affecter</trd>,
        <trd>feindre</trd></trdgrp>
      <trdgrp lng="it">
        <trd>affettare</trd>,
        <trd>far mostra di</trd>
      </trdgrp>
      <trd lng="nl">z. <ind>aanstellen</ind></trd>
      <trdgrp lng="pt">
        <trd>afetar</trd>,
        <trd>fingir</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_p;&c_r;&c_i;&c_t;&c_v;&c_o;&c_r;&c_ja;&c_t;&c_mol;&c_s;&c_ja;
        </trd>,
        <trd>&c_l;&c_o;&c_m;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
        <trd>&c_r;&c_i;&c_s;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
        <trd>&c_zh;&c_je;&c_m;&c_a;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
      </trdgrp>
    <trd lng="sk">afektova&tcaron;</trd>
    </drv>
  </art>
