        <trd>&c_p;&c_o;&c_z;&c_jo;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>
      </trdgrp>
  <trdgrp lng="sk">
    <trd>afekt</trd>,
    <trd>strojenos&#x165;</trd>
  </trdgrp>
</drv>

    <drv mrk="afekt.0a">
      <kap><tld/>a</kap>
      <snc mrk="afekt.0a.0anta">
        <dif>
          <tld lit="A"/>anta:
          <ekz>
            Seabra evidenti&gcirc;as ne <tld/>a, sed a&ubreve;tenta, maldekstrulo
            <fnt>
              <aut>J. Camacho</aut>
              <vrk>Soluloj kaj putinoj</vrk>
              <lok><url ref="http://esperanto.net/literaturo/novel/paneobskur.html">Originala
                  literaturo Esperanta</url></lok>
            </fnt>;
          </ekz>
          <ekz>
            la a&ubreve;toro <klr>[verkas]</klr> en la vortumema frazado
            de humila kaj <tld/>a duonklera farmbienisto
            <fnt>
              <aut>R. Platteau</aut>
              <vrk>Ri&ccirc;iga, agrabla, nepre leginda libro</vrk>
              <lok><url ref="&LOdE;R-steel2.htm">LOdE
                  2003:11</url></lok>
            </fnt>;
          </ekz>
          <ekz>
            subite la geamantoj sin liberigis reciproke, el preno kaj
            teno,
            kaj <tld/>e senkulpaj ili dissidi&gcirc;is
            <fnt><bib>UrdHadda</bib>, <lok>p. 24</lok></fnt>;
          </ekz>
          <ekz>
            &Scirc;ol starigis la klasikan demandon en
            <ind>dol&ccirc;<tld/>a</ind> tono
            <klr>(tono <tld/>anta dol&ccirc;econ)</klr>
            <fnt><bib>UrdHadda</bib>, <lok>p. 7</lok></fnt>;
            <trdgrp lng="fr">
              <trd>doucereux</trd>,
              <trd>mielleux</trd>
            </trdgrp>
          </ekz>
          <ekz>
            <klr>[mi estas ludilo]</klr> de ilia <ind>mild<tld/>a</ind>
            malico
            <fnt><bib>Revadoj</bib>, <lok>Oka promenado</lok></fnt>.
            <trdgrp lng="fr">
              <trd>doucereux</trd>,
              <trd>mielleux</trd>
            </trdgrp>
          </ekz>
        </dif>
        <ref tip="vid" cel="afekt.0ema"><tld/>ema</ref>
        <trdgrp lng="en"><trd>affected</trd>, <trd>artificial</trd>,
          <trd>assumed</trd></trdgrp>
        <trdgrp lng="fr">
          <trd>qui <ind>affecte</ind></trd>,
          <trd>affect&eacute;<klr tip="ind"> (qui affecte)</klr></trd>,
          <trd>appr&ecirc;t&eacute;</trd>,
          <trd>artificiel<klr tip="ind"> (qui affecte)</klr></trd>,
          <trd>chichiteux</trd>,
          <trd>mani&eacute;r&eacute;</trd>,
          <trd>minaudier</trd>,
          <trd>peu <ind>naturel</ind></trd>,
          <trd>pr&eacute;cieux<klr tip="ind"> (affect&eacute;)</klr></trd>,
        </trdgrp>
      </snc>
      <snc mrk="afekt.0a.montranta_0on">
        <dif>
          Montranta ies <tld/>on:
          <ekz>
            <tld/>a vo&ccirc;o, parolmaniero, rideto;
          </ekz>
          <ekz>
            kiam oni ludas en <tld/>a maniero, lia muziko estas
            abomeninda
            <fnt>
              <vrk>Granda konanto</vrk>
              <lok><url ref="http://es1.chinabroadcast.cn/1/2004/08/14/1@13721.htm">&Ccirc;ina Radio Internacia</url></lok>
            </fnt>;
          </ekz>
          <ekz>
            Stanjo tiel kondutis kun mi, ke mi ne povis ekskludi
            <tld/>an strebon vundi min
            <fnt>
              <aut>V. Rybakov, trad. J. Finkel</aut>
              <vrk>Gravitavio &leftquot;Carido&rightquot;</vrk>
              <lok><url ref="http://lib.ru/RYBAKOW/graviletesp.txt">http://lib.ru/RYBAKOW/graviletesp.txt</url></lok>
            </fnt>;
          </ekz>
          <ekz>
            Rita <tld/>e levis al li admone minacan fingron
            <fnt><bib>UrdHadda</bib>, <lok>p. 12</lok></fnt>.
          </ekz>
        </dif>
        <trdgrp lng="fr">
          <trd>affect&eacute;<klr tip="ind"> (qui trahit de
              l'affectation)</klr></trd>,
          <trd>appr&ecirc;t&eacute;</trd>,
          <trd>artificiel<klr tip="ind"> (qui trahit de
          l'affectation)</klr></trd>,
          <trd>chichiteux</trd>,
          <trd>de <ind>commande</ind><klr tip="ind">
          (comportement)</klr></trd>,
          <trd>mani&eacute;r&eacute;</trd>,
          <trd>peu <ind>naturel</ind></trd>,
        </trdgrp>
      </snc>
      <snc mrk="afekt.0a.0ita">
        <dif>
          <tld lit="A"/>ita:
          <ekz>
            ne turmentu min kiel anta&ubreve;e per vanaj duboj kaj
            <tld/>a malvarmo
            <fnt>
              <aut>M. Lermontov, trad. N. Danovskij</aut>
              <vrk>Princidino Mary</vrk>
              <lok><url ref="http://www.sezonoj.itgo.com/Mary-01.htm">Rusa
                  literaturo en Esperanto</url></lok>
            </fnt>;
          </ekz>
          <ekz>
            li donis al &scirc;i la kajeron kun la brulnigri&gcirc;inta rando,
            la sekan rozon, la foton kaj, kun <tld/>a zorgemo, la
            &scirc;parkasan
            libreton
            <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 24</lok></fnt>.
          </ekz>
        </dif>
        <trdgrp lng="fr">
          <trd>affect&eacute;<klr tip="ind"> (feint)</klr></trd>,
          <trd>artificiel<klr tip="ind"> (feint)</klr></trd>,
          <trd>de <ind>commande</ind><klr tip="ind">
          (comportement)</klr></trd>,
          <trd>feint<klr tip="ind"> (artificiel)</klr></trd>,
          <trd>forc&eacute;<klr tip="ind"> (artificiel)</klr></trd>,
          <trd>peu <ind>naturel</ind></trd>
        </trdgrp>
        <trdgrp lng="it">
        <trd>affettato<klr> (artificiale)</klr></trd>,
        <trd>artificioso</trd>,
        <trd>manierato</trd>,
        <trd>ricercato<klr> (atteggiamento)</klr></trd>
      </trdgrp>
      </snc>
  <trdgrp lng="cs">
    <trd>afektovan&#xFD;</trd>,
    <trd>afektov&#xFD;</trd>,
    <trd>nep&#x159;irozen&#xFD;</trd>,
    <trd>strojen&#xFD; chov&#xE1;n&#xED;m</trd>
  </trdgrp>
  <trd lng="he">&#x5DE;&#x5D5;&#x5E9;&#x5E4;&#x5E2;</trd>
  <trdgrp lng="ja">
    <trd>&#x6C17;&#x53D6;&#x3063;&#x305F;  [<ind>&#x304D;&#x3069;&#x3063;&#x305F;</ind>]</trd>,
    <trd>&#x898B;&#x305B;&#x304B;&#x3051;&#x306E;  [<ind>&#x307F;&#x305B;&#x304B;&#x3051;&#x306E;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>predstieran&#xFD;</trd>,
    <trd>strojen&#xFD;</trd>
  </trdgrp>
</drv>

    <drv mrk="afekt.0ajxo">
      <kap><tld/>a&jcirc;o</kap>
      <snc>
        <dif>
          Konkreta elemento de <tld/>a konduto:
          <ekz>
            la ideo pri nacieco facile povas tiel etendi&gcirc;i,
            ke &gcirc;i fari&gcirc;os <tld/>a&jcirc;o
            <fnt><bib>Fab3</bib>, <lok>Io</lok></fnt>;
          </ekz>
          <ekz>
            <klr>[en &scirc;ia konduto]</klr> mi trovis ian formon de
            &gcirc;entileco, <klr>[...]</klr> kiu miaopinie ja valoras
            tiun de la <tld/>a&jcirc;oj
            <fnt><bib>Revadoj</bib>, <lok>Na&ubreve;a promenado</lok></fnt>;
          </ekz>
          <ekz>
            <klr>[vidante la malordemon de mia kunlo&gcirc;anto]</klr>
            mi ekhavigis al mi virtajn <tld/>a&jcirc;ojn
            <fnt><aut>A. Conan Doyle, trad. D. Booton</aut>
              <vrk><url ref="http://members.tripod.com/~booton/musgravo/musgravo.html">La musgrava rito</url></vrk>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="en"><trd>affectation</trd>,
      <trd>mannerism</trd></trdgrp>
      <trdgrp lng="fr">
        <trd>chichi<klr tip="ind"> (minauderie)</klr></trd>,
        <trd>fa&ccedil;on<klr tip="ind"> (chichi)</klr></trd>,
        <trd>mani&egrave;re<klr tip="ind"> (chichi)</klr></trd>,
        <trd>minauderie</trd>,
        <trd>mine<klr tip="ind"> (minauderie)</klr></trd>,
        <trd>simagr&eacute;e</trd>,
        <trd>singerie<klr tip="ind"> (minauderie)</klr></trd>
      </trdgrp>
      <trdgrp lng="it">
        <trd>smanceria</trd>,
        <trd>leziosaggine</trd>
      </trdgrp>
      <trdgrp lng="ja">
    <trd>&#x6C17;&#x53D6;&#x3063;&#x305F;&#x614B;&#x5EA6;  [<ind>&#x304D;&#x3069;&#x3063;&#x305F;&#x305F;&#x3044;&#x3069;</ind>]</trd>,
    <trd>&#x4F53;&#x88C1;  [<ind>&#x3066;&#x3044;&#x3055;&#x3044;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="ru">
        <trd>&c_zh;&c_je;&c_m;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>,
        <trd>&c_r;&c_i;&c_s;&c_o;&c_v;&c_k;&c_a;</trd>,
        <trd>&c_p;&c_o;&c_z;&c_a;</trd>
      </trdgrp>
    </drv>

    <drv mrk="afekt.0ema">
      <kap><tld/>ema</kap>
      <fnt><bib>VdE</bib></fnt>
      <snc>
