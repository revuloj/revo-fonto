<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: biksxu.xml,v 1.11 2023/10/29 19:11:01 revo Exp $">
<kap>
  <rad>bik&scirc;u</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="biksxu.0o">
  <kap><tld/>o</kap>
  <snc mrk="biksxu.0o.BUD">
    <uzo tip="fak">BUD</uzo>
    <dif>
      Budaisma <ref tip="super" cel="monahx.0o.REL">mona&hcirc;o</ref>:
      <ekz>
        la <tld/>o kuris tra la bordo de la rivero, eniris en la akvon
        kaj prenis la besteton per sia mano
        <fnt>
          <aut>trad. R. de Carvalho Mendon&ccedil;a</aut>,
          <vrk><url ref="http://blog-da-lela.blogspot.com/">seokoblog-da-lela</url></vrk>,
          <lok>2006-08-27</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x548C;&#x5C1A; <pr>h&#xE9;sh&#xE0;ng</pr></trd>
</snc>
  <trd lng="be">&c_b;&c_g;&c_ib;&c_k;&c_sh;&c_u;</trd>
  <trd lng="de">Bhikkhu</trd>
  <trd lng="fr">bikshu</trd>
  <trd lng="hu">biksu</trd>
  <trd lng="ja">&#x6BD4;&#x4E18; <pr>&#x3073;&#x304F;</pr></trd>
  <trd lng="pl">bikszu</trd>
  <trd lng="uk">&c_b;&c_h;&c_ib;&#x43A;&c_sh;&c_u;&#xA0;(&c_b;&c_u;&c_d;&c_d;&c_i;&c_s;&c_t;&c_s;&#x44C;&#x43A;&c_i;&c_j; &#x43C;&#x43E;&#x43D;&c_a;&c_h;)</trd>
</drv>

<drv mrk="biksxu.0ino">
  <kap><tld/>ino</kap>
  <snc mrk="biksxu.0ino.BUD">
    <uzo tip="fak">BUD</uzo>
    <dif>
      Budaisma <ref tip="super" cel="monahx.0ino.REL">mona&hcirc;ino</ref>:
      <ekz>
        <tld/>oj, <tld/>inoj, &ccirc;ieluloj, drakoj, demonoj kaj
        gandarvoj rigardante unu la aliaj<klr>n</klr>
        hezitas demandi
        <fnt>
        <aut>trad. Asano San&ccirc;i</aut>,
        <vrk><url ref="http://www001.upp.so-net.ne.jp/jble/sutro/sps02.html">
        Saddharma-Pundarika Sutro, 2</url></vrk>,
        <lok>La Japana Budhano, 1979-80, numero 187-190</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_g;&c_ib;&c_k;&c_sh;&c_u; <klr>(&c_zh;&c_a;&c_n;&c_ch;&c_y;&c_n;&c_a;)</klr></trd>
    <trd lng="de">Bhikkhuni</trd>
    <trd lng="hu">biksun&odblac;</trd>
  </snc>
</drv>
</art>

