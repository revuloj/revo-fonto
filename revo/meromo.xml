<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: meromo.xml,v 1.10 2024/01/25 20:10:36 revo Exp $">
  <kap>
    <rad>meromorf</rad>/a
  </kap>
<drv mrk="meromo.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 21</lok></fnt>
  <snc>
    <dif>
      <klr>(p.p. kompleksa <ref cel="funkci.0o.MAT">funkcio</ref> en
      iu subaro de &gcirc;ia fonto-aro)</klr>
      Tia, ke &gcirc;i estas <ref cel="holomo.0a.cxie">holomorfa</ref> &ccirc;e
      &ccirc;iu punkto de la koncerna subaro, krom &ccirc;e iuj punktoj,
      kiuj estas <ref cel="polus.0o.de_funkcio">polusoj</ref> de &gcirc;i:
      <ekz>
        &ccirc;iu <ind><tld/>a funkcio</ind> identi&gcirc;as kun la kvociento de
        du holomorfaj funkcioj.
        <trd lng="ca">funci&oacute; meromorfa</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_e;&c_r;&c_a;&c_m;&c_o;&c_r;&c_f;&c_n;&c_y;</trd>
    <trd lng="ca">meromorf</trd>
    <trd lng="de">meromorph</trd>
    <trd lng="en">meromorphic</trd>
    <trd lng="fa">&a_b;&a_r;&a_kh;&a_h;&zwnj;&a_r;&f_y;&a_kh;&a_t;</trd>
    <trd lng="fr">m&eacute;romorphe</trd>
    <trd lng="hu">meromorf</trd>
    <trd lng="nl">meromorf</trd>
    <trd lng="pl">meromorficzny</trd>
    <trd lng="pt">merom&oacute;rfico</trd>
    <trd lng="ru">&c_m;&c_je;&c_r;&c_o;&c_m;&c_o;&c_r;&c_f;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trd lng="uk">&#x43C;&#x435;&#x440;&#x43E;&#x43C;&#x43E;&#x440;&#x444;&#x43D;&#x438;&#x439;</trd>
  <trdgrp lng="zh">
    <trd>&#x4E9A;&#x7EAF; <pr>y&#xE0;ch&#xFA;n</pr></trd>,
    <trd>&#x4E9E;&#x7D14; <pr>y&#xE0;ch&#xFA;n</pr></trd>
  </trdgrp>
</drv>
</art>

