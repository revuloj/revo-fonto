<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>dilu</rad>/i
</kap>

<drv mrk="dilu.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="koncen.mal0i">Malkoncentri</ref>
      solva&jcirc;on per solvilo:
      <ekz>
        la analizenda materialo <klr>(urino)</klr> estis malnova kaj
        tre <tld/>ita
        <fnt><bib>UrdHadda</bib>, <lok>p. 27</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_b;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;
  </trd>
  <trdgrp lng="de">
    <trd>verd&uuml;nnen</trd>,
    <trd>verw&auml;ssern</trd>
  </trdgrp>
  <trd lng="es">diluir</trd>
  <trdgrp lng="fr">
    <trd>diluer</trd>,
    <trd>allonger<klr tip="ind"> (diluer)</klr></trd>,
    <trd>couper<klr tip="ind"> (diluer)</klr></trd>,
    <trd>d&eacute;layer</trd>,
    <trd>&eacute;tendre<klr tip="ind"> (diluer)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rozcie&nacute;cza&cacute;</trd>,
    <trd>rozrzedza&cacute;</trd>,
    <trd>rozwadnia&cacute;</trd>
  </trdgrp>
  <trd lng="pt">diluir</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_b;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_zh;&c_i;&c_zh;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="dilu.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <ref tip="ant" cel="dilu.0i"><tld/>i</ref>
    <ref tip="dif" cel="koncen.0i.KEM">koncentri <sncref/></ref>
  </snc>
  <trdgrp lng="de">
    <trd>eindicken</trd>,
    <trd>konzentieren</trd>,
    <trd>reduzieren</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>concentrer<klr tip="ind"> (chim.)</klr></trd>,
    <trd>r&eacute;duire<klr tip="ind"> (tr., cuis.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>
&c_k;&c_o;&c_n;&c_c;&c_je;&c_n;&c_t;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;
&c_mol;
    </trd>,
    <trd>&c_s;&c_g;&c_u;&c_shch;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
