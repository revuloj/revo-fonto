<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>arkipelag</rad>/o
</kap>
<drv mrk="arkipe.0o">
  <kap><tld/>o</kap>
  <snc mrk="arkipe.0o.insularo">
    <uzo tip="fak">GEOG</uzo>
    <ref tip="dif" cel="insul.0aro">insularo</ref>
    <ekz>
      senlaboreco atakas la <tld/>on<fnt><aut>Hori Yasuo</aut>:
      <vrk>Senlaboreco atakas la arkipelagon</vrk>,
      <bib>Monato</bib><lok>jaro 2002a, numero 4a, p.
      10-11a</lok></fnt>.
    </ekz>
    <trdgrp lng="de">
      <trd>Inselgruppe</trd>,
      <trd>Archipel</trd>
    </trdgrp>
    <trd lng="en">archipelago</trd>
    <trd lng="es">archipielago</trd>
    <trd lng="fr">archipel</trd>
    <trdgrp lng="hu">
      <trd>szigetvil&aacute;g</trd>,
      <trd>szigettenger</trd>,
      <trd>archipelagus</trd>
    </trdgrp>
    <trd lng="ru">&c_a;&c_r;&c_h;&c_i;&c_p;&c_je;&c_l;&c_a;&c_g;</trd>
    <trdgrp lng="sv">
      <trd>sk&auml;rg&aring;rd</trd>,
      <trd>&ouml;v&auml;rld</trd>
    </trdgrp>
  </snc>
  <snc mrk="arkipe.0o.egeamaro">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Multinsula maro, parto de <ref tip="prt"
      cel="mediterane.0o">Mediteraneo</ref>, inter Grekujo kaj Turkujo.
    </dif>
    <trdgrp lng="de">
      <trd>&Auml;g&auml;is</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>mer <ind>&Eacute;g&eacute;e</ind></trd>
    </trdgrp>
    <trd lng="hu">g&ouml;r&ouml;g szigetvil&aacute;g</trd>
  </snc>
</drv>
</art>
