<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: arkipe.xml,v 1.38 2023/10/28 08:33:08 revo Exp $">
<kap>
  <ofc>3</ofc><rad>arkipelag</rad>/o,
  <var><kap><ofc>3</ofc><rad var="&hcirc;">ar&hcirc;ipelag</rad>/o</kap></var>
</kap>
<drv mrk="arkipe.0o">
  <kap><tld/>o, <var><kap><tld var="&hcirc;"/>o</kap></var></kap>
  <snc mrk="arkipe.0o.insularo">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="dif" cel="insul.0aro">Insularo</ref>:
      <ekz>
        Japanio konsistas el vulkana <tld/>o kun 86 aktivaj vulkanoj
        <fnt><aut>HORI Jasuo</aut>: 
          <vrk><url ref="&Monato;/2000/007147.php">Erupcias maltrankvilo</url></vrk>,
          <bib>Monato</bib>
          <lok>2000/12, p. 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiaj ceremonioj fakte okazis e&ccirc; publike sur la Samoa-<tld/>o
        <fnt>
	<aut>Rudiger Hauger</aut>
	<vrk><url ref="&Monato;/2001/007315.php">De&ccirc;ifrado de la paskinsula skribo</url></vrk>
	<bib>Monato</bib>
	<lok>2001/02, p. 23</lok>
        </fnt>;
      </ekz>
      <ekz>
        granda &scirc;ildo <klr>[&#x2026;]</klr> bonvenigas &scirc;ipojn al tiu anta&ubreve;e skandinava <tld/>o
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2004/008323.html">Le&gcirc;oj landon prosperigas</url></vrk>
          <bib>Monato</bib>
          <lok>2004/07, p. 27</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la atlantikaj <tld var="&hcirc;"/>oj
        <fnt><aut>Juan R&eacute;gulo P&eacute;rez</aut>, <vrk>dracenoj &ccirc;e pentristoj de
          la 15a jarcento</vrk>, <lok>norda prismo, 1957, numero 3a, p.
          123-128a</lok>
         </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_r;&c_h;&c_ib;&c_p;&c_e;&c_l;&c_ja;&c_g;</trd>
    <trd lng="ca">arxip&egrave;lag</trd>
    <trdgrp lng="de">
      <trd>Inselgruppe</trd>,
      <trd>Archipel</trd>
    </trdgrp>
    <trd lng="en">archipelago</trd>
    <trd lng="es">archipi&eacute;lago</trd>
    <trdgrp lng="fa">
      <trd>&a_j;&a_z;&f_y;&a_r;&f_g;&a_A;&a_n;</trd>,
      <trd>&a_m;&a_j;&a_m;&a_ayn;&#x200C;&a_A;&a_l;&a_j;&a_z;&a_A;&f_y;&a_r;</trd>
    </trdgrp>
    <trd lng="fr">archipel</trd>
    <trd lng="nl">archipel</trd>
    <trdgrp lng="hu">
      <trd>szigetvil&aacute;g</trd>,
      <trd>szigettenger</trd>,
      <trd>archipelagus</trd>
    </trdgrp>
    <trd lng="it">arcipelago</trd>
    <trd lng="ru">&c_a;&c_r;&c_h;&c_i;&c_p;&c_je;&c_l;&c_a;&c_g;</trd>
    <trdgrp lng="sv">
      <trd>sk&auml;rg&aring;rd</trd>,
      <trd>&ouml;v&auml;rld</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x5217;&#x5C9B; <pr>li&#xE8;d&#x1CE;o</pr></trd>,
    <trd>&#x5217;&#x5CF6; <pr>li&#xE8;d&#x1CE;o</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="arkipe.0o.egeamaro">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Multinsula maro, parto de <ref tip="prt" cel="mediterane.M0o">Mediteraneo</ref>, inter Grekujo
      kaj Turkujo.
    </dif>
    <trd lng="be">&c_E;&c_g;&c_je;&c_j;&c_s;&c_k;&c_a;&c_je; &c_m;&c_o;&c_r;&c_a;</trd>
    <trd lng="ca">Arxip&egrave;lag</trd>
    <trdgrp lng="de">
      <trd>&Auml;g&auml;isches Meer</trd>,
      <trd>&Auml;g&auml;is</trd>
    </trdgrp>
    <trd lng="fa">&a_d;&a_r;&f_y;&a_A;&f_y; <ind>&a_A;&f_zh;&a_h;</ind></trd>
    <trdgrp lng="fr">
      <trd>mer <ind>&Eacute;g&eacute;e</ind></trd>
    </trdgrp>
    <trd lng="nl">Ege&iuml;sche Zee</trd>
    <trd lng="hu">g&ouml;r&ouml;g szigetvil&aacute;g</trd>
    <trd lng="ru">&c_E;&c_g;&c_je;&c_j;&c_s;&c_k;&c_o;&c_je; &c_m;&c_o;&c_r;&c_je;</trd>
    <trdgrp lng="zh">
    <trd>&#x611B;&#x7434;&#x6D77; <pr>&#xE0;iq&#xED;nh&#x1CE;i</pr></trd>,
    <trd>&#x7231;&#x7434;&#x6D77; <pr>&#xE0;iq&#xED;nh&#x1CE;i</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="cs">
    <trd>archipelag</trd>,
    <trd>skupina ostrov&uring;</trd>,
    <trd>souostrov&iacute;</trd>
  </trdgrp>
  <trd lng="he">&#x5D0;&#x5B7;&#x5E8;&#x5DB;&#x5B4;&#x5D9;&#x5E4;&#x5BC;&#x5B5;&#x5DC;&#x5B8;&#x5D2;</trd>
  <trdgrp lng="ja">
    <trd>&#x7FA4;&#x5CF6;  <pr>&#x3050;&#x3093;&#x3068;&#x3046;</pr></trd>,
    <trd>&#x8AF8;&#x5CF6;  <pr>&#x3057;&#x3087;&#x3068;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="sk">s&uacute;ostrovie</trd>
  <trd lng="uk">&c_a;&c_r;&c_h;&c_ib;&#x43F;&c_je;&#x43B;&c_a;&c_g;</trd>
</drv>
</art>
