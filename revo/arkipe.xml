<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>arkipelag</rad>/o
</kap>
<drv mrk="arkipe.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      La maro, kun multaj insuloj, inter Grekujo kaj Turkujo.
    </dif>
  </snc>
  <snc num="2">
    <dif>
      <ref tip="sin" cel="insul.0aro">Insularo</ref> prezentanta
      geografian unuon.
    </dif>
    <trd lng="fr">archipel</trd>
    <trd lng="ru">&c_a;&c_r;&c_h;&c_i;&c_p;&c_je;&c_l;&c_a;&c_g;</trd>
  </snc>
</drv>
</art>
