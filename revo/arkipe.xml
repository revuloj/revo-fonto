<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>arkipelag</rad>/o
</kap>
<drv mrk="arkipe.0o">
  <kap><tld/>o</kap>
  <snc num="1">
  <uzo tip="fak">GEOG</uzo>
    <dif>
      La maro, kun multaj insuloj, inter Grekujo kaj Turkujo.
    </dif>
  </snc>
  <snc num="2">
  <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="sin" cel="insul.0aro">Insularo</ref> prezentanta
      geografian unuon.
    </dif>
    <trd lng="fr">archipel</trd>
    <trd lng="ru">&c_a;&c_r;&c_h;&c_i;&c_p;&c_je;&c_l;&c_a;&c_g;</trd>
    <trdgrp lng="sv">
       <trd>sk&auml;rg&aring;rd</trd>,
       <trd>&ouml;v&auml;rld</trd>
    </trdgrp>
  </snc>
</drv>
</art>
