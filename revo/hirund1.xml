<drv mrk="hirund1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo
      <ref tip="super" cel="paser.0oformaj">paseroformaj birdoj</ref>,
      kun pintaj flugiloj kaj plata beko
      (<trd lng="la">Hirundinidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Schwalben<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="fr">hirundinid&eacute;s</trd>
  <trd lng="nl">zwaluwen<klr tip="ind"> (fam.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_s;&c_t;&c_o;&c_ch;&c_k;&c_o;&c_v;&c_y;&c_je;</trd>,
    <trd>&c_l;&c_a;&c_s;&c_t;&c_o;&c_ch;&c_k;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
</drv>
</art>
