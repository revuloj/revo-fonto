<vortaro>
  <art mrk="$Id: impote.xml,v 1.25 2023/11/19 10:22:12 revo Exp $">
    <kap>
      <rad>impotent</rad>/a
      <fnt>Z</fnt>
    </kap>

    <drv mrk="impote.0a">
      <kap><tld/>a</kap>
      <snc mrk="impote.0a.MED">
        <uzo tip="fak">MED</uzo>
        <dif>
          (Pri viro:) da&ubreve;re nekapabla je seksumo pro manko de
          peniserekto a&ubreve; de spermel&jcirc;eto:
          <ekz>
            <ctl>Espereble li ne estas <tld/>a</ctl>, &scirc;i diris al
            si, <ctl>estus tro doma&gcirc;e &ccirc;e tia bela besto.</ctl>
            <fnt>
              <aut>Joh&aacute;n BALANO</aut>: <vrk>&Ccirc;u &scirc;i mortu tra-fike?</vrk>
            </fnt>
          </ekz>
        </dif>
      </snc>
      <snc mrk="impote.0a.FIG">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Nekapabla ion krei, senfrukta:
          <ekz>
            la luksa universitato ne instruas junularon, kiel superi
            mizeron, radikala intelektularo estas <tld/>a <klr>...</klr>
            <fnt>
              <aut>I. Ilf, E. Petrov, tr. V. Okc</aut>:
              <vrk>Unueta&gcirc;a Usono</vrk>.
              <lok><url ref="http://www.esperanto.mv.ru/Usono/truu17.html">&Ccirc;ap. 17a</url></lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_ib;&c_m;&c_p;&c_a;&c_t;&c_e;&c_n;&c_t;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>impotentn&iacute;</trd>,
    <trd>neschopn&yacute;</trd>
  </trdgrp>
  <trd lng="de">impotent</trd>
  <trd lng="en">impotent</trd>
  <trd lng="es">impotente</trd>
  <trd lng="fr">impuissant<klr tip="ind"> (sexuellement)</klr></trd>
  <trd lng="hu">impotens</trd>
  <trd lng="id">impoten</trd>
  <trdgrp lng="ja">
    <trd>&#x6027;&#x7684;&#x4E0D;&#x80FD;&#x306E;  <pr>&#x305B;&#x3044;&#x3066;&#x304D;&#x3075;&#x306E;&#x3046;&#x306E;</pr></trd>,
    <trd>&#x30A4;&#x30F3;&#x30DD;&#x30C6;&#x30F3;&#x30C4;&#x306E;</trd>
  </trdgrp>
  <trd lng="nl">impotent</trd>
  <trd lng="pt">impotente<klr tip="ind"> (sexualmente)</klr></trd>
  <trd lng="ru">&c_i;&c_m;&c_p;&c_o;&c_t;&c_je;&c_n;&c_t;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>impotentn&yacute;</trd>,
    <trd>neschopn&yacute;</trd>
  </trdgrp>
  <trd lng="sv">impotent</trd>
  <trd lng="uk">&#x456;&#x43C;&#x43F;&#x43E;&#x442;&#x435;&#x43D;&#x442;&#x43D;&#x438;&#x439;</trd>
</drv>

    <drv mrk="impote.0eco">
      <kap><tld/>eco</kap>
      <uzo tip="fak">MED</uzo>
      <snc>
        <dif>
          La seksa misfunkcio &ccirc;e viro <tld/>a:
          <ekz>
            la <tld/>eco estas motivo por eksedzi&gcirc;o.
          </ekz>
        </dif>
      </snc>
      <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>
      <trd lng="be">&c_ib;&c_m;&c_p;&c_a;&c_t;&c_e;&c_n;&c_c;&c_y;&c_ja;</trd>
      <trd lng="fr">impuissance<klr> (sexuelle)</klr></trd>
      <trd lng="id">impotensi</trd>
      <trdgrp lng="ja">
    <trd>&#x6027;&#x7684;&#x4E0D;&#x80FD;  <pr>&#x305B;&#x3044;&#x3066;&#x304D;&#x3075;&#x306E;&#x3046;</pr></trd>,
    <trd>&#x30A4;&#x30F3;&#x30DD;&#x30C6;&#x30F3;&#x30C4;</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_m;&c_p;&c_o;&c_t;&c_je;&c_n;&c_c;&c_i;&c_ja;</trd>
      <trdgrp lng="sk">
    <trd>nemoh&uacute;cnos&tcaron;</trd>,
    <trd>neschopnos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>impotence</trd>,
    <trd>impotentnost</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x456;&#x43C;&#x43F;&#x43E;&#x442;&#x435;&#x43D;&#x446;&#x456;&#x44F;</trd>,
    <trd>&#x441;&#x442;&#x430;&#x442;&#x435;&#x432;&#x435; &#x431;&#x435;&#x437;&#x441;&#x438;&#x43B;&#x43B;&#x44F;</trd>
  </trdgrp>
  <trd lng="zh">&#x4E0D;&#x4E3E; <pr>b&#xF9;j&#x1D4;</pr></trd>
</drv>

    <drv mrk="impote.0ulo">
      <kap><tld/>ulo</kap>
      <uzo tip="fak">MED</uzo>
      <snc>
        <dif>
          Viro <tld/>a:
          <ekz>
            viro, kiu tion ne komprenas, estas a&ubreve; nepra stultulo a&ubreve;
            nenionkapabla <tld/>ulo
            <fnt>
              <aut>Vina&rcaron;</aut>: <vrk>La skandalo pro Jozefo</vrk>.
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_ib;&c_m;&c_p;&c_a;&c_t;&c_e;&c_n;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>impotent</trd>,
    <trd>neplodn&yacute; &ccaron;l.</trd>
  </trdgrp>
  <trd lng="fr">impuissant<klr> (subst.)</klr></trd>
  <trd lng="ru">&c_i;&c_m;&c_p;&c_o;&c_t;&c_je;&c_n;&c_t;</trd>
  <trd lng="sk">impotent</trd>
  <trd lng="uk">&#x456;&#x43C;&#x43F;&#x43E;&#x442;&#x435;&#x43D;&#x442;</trd>
</drv> 
