<kap>
  <rad>&ccirc;ak</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="cxak.0o">
  <kap><tld/>o</kap>
  <snc mrk="cxak.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Alta rigida <ref tip="lst" lst="voko:vesta&jcirc;oj" cel="vest.kap0o.TEKS">vesta&jcirc;o</ref>, 
      uzata kiel parto de <ref tip="malprt" cel="unifor.0o">uniformo</ref>:
      <ekz>
        bonan vesperon! diris la soldato kaj ektu&scirc;is per la mano sian <tld/>on
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        polica gardisto <klr>[&#x2026;]</klr> kun grandega pot-forma <tld/>o
        sur la kapo
        <fnt>
          <aut>Ivan Turgenev, trad. A. Kor&jcirc;enkov k.a.</aut>,
          <vrk><url ref="&LOdE;Rn-turg1.htm">
          La unua amo</url></vrk>,
          <lok>Jekaterinburg: Sezonoj, 1993</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kasked.0o">kaskedo</ref>,
      <ref cel="kep.0o">kepo</ref>,
      <ref cel="fez.0o">fezo</ref>
    </refgrp>
    <bld lok="&WCU;/thumb/e/ed/Preusischer_Tschako.jpg/320px-Preusischer_Tschako.jpg" prm="CC-BY-SA-3.0">prusa <tld/>o
        <fnt>
          <aut>Stahlkocher, propra foto</aut>
          <lok><url ref="&WCW;/File:Preusischer_Tschako.jpg">&WCW;/File:Preusischer_Tschako.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_v;&c_e;&c_r;</trd>
  <trd lng="ca">xac&oacute;</trd>
  <trd lng="de">Tschako</trd>
  <trd lng="en">shako</trd>
  <trd lng="es">chac&oacute;</trd>
  <trd lng="fr">shako</trd>
  <trd lng="hu">cs&aacute;k&oacute;</trd>
  <trd lng="it">sciacc&ograve;</trd>
  <trdgrp lng="ja">
    <trd>&#x30B7;&#x30E3;&#x30B3;&#x30FC;</trd>,
    <trd>&#x7B52;&#x5F62;&#x8ECD;&#x5E3D;  <pr>&#x3064;&#x3064;&#x304B;&#x305F;&#x3050;&#x3093;&#x307C;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">sjako</trd>
  <trd lng="pl">czako</trd>
  <trd lng="pt">barretina</trd>
  <trd lng="ru">&c_k;&c_i;&c_v;&c_je;&c_r;</trd>
  <trdgrp lng="uk">
    <trd>&#x432;&#x456;&#x439;&#x441;&#x44C;&#x43A;.</trd>,
    <trd>&#x456;&#x441;&#x442;. &#x43A;&#x456;&#x432;&#x435;&#x440;</trd>
