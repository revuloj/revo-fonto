<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: semina.xml,v 1.22 2024/03/24 20:00:00 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>seminari</rad>/o
</kap>
<drv mrk="semina.0o">
  <kap><tld/>o</kap>
  <snc mrk="semina.0o.loko">
    <dif>
      <ref tip="super" cel="lern.0ejo.loko">Lernejo <sncref/></ref>, kie
      oni
      instruas kandidatojn al pastreco a&ubreve; al instruisteco:
      <ekz>
        la infano forlasis la hejmon kaj nur dek-tri-jara komencis lerni
        en la <tld/>o de Stoutenburg<fnt><aut>Julian Modest</aut>: <vrk>
        Sincere kaj honeste pri si mem (Gerrit Berveling: La unuaj 25
        jaroj
        de mia memoro)</vrk>, <bib>Monato</bib><lok>jaro 1995a, numero
        7a,
        p. 22a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_e;&c_m;&c_ib;&c_n;&c_a;&c_r;&c_y;&c_ja;</trd>
    <trd lng="en">seminary</trd>
    <trdgrp lng="nl">
      <trd>seminarie</trd>,
      <trd>kweekschool</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_je;&c_m;&c_i;&c_n;&c_a;&c_r;&c_i;&c_ja;</trd>
  </snc>
  <snc mrk="semina.0o.kurso">
    <dif>
      Praktika <ref tip="super" cel="kurs.0o.lerna">kurso
      <sncref/></ref>
      pri iu scienco en universitato:
      <ekz>
        mia ministrejo igis min partopreni en dulingva, tutsemajna
        <tld/>o
        por lerni kiel bone negoci &leftquot;bruselajn&rightquot;
        aferojn<fnt><aut>Ami&ccirc;jo</aut>: <vrk>E&ubreve;ropaj negocoj en
        Mastri&hcirc;to</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 12a,
        p.
        8a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_e;&c_m;&c_ib;&c_n;&c_a;&c_r;</trd>
    <trd lng="de">Seminar</trd>
    <trd lng="en">seminar</trd>
    <trdgrp lng="nl">
      <trd>seminar</trd>,
      <trd>seminarie</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_je;&c_m;&c_i;&c_n;&c_a;&c_r;</trd>
  </snc>
  <trd lng="ca">seminari</trd>
  <trd lng="cs">semin&#xE1;&rcaron; <klr>(kurz i kn&#x11B;&#x17E;sk&#xE1; &scaron;kola)</klr></trd>
  <trdgrp lng="fr">
    <trd>s&eacute;minaire</trd>
  </trdgrp>
  <trd lng="hu">szemin&aacute;rium</trd>
  <trd lng="it">seminario</trd>
  <trdgrp lng="ja">
    <trd>&#x30BC;&#x30DF;&#x30CA;&#x30FC;&#x30EB;</trd>,
    <trd>&#x30BB;&#x30DF;&#x30CA;&#x30FC;  <pr>&#x305B;&#x307F;&#x306A;&#x30FC;</pr></trd>,
    <trd>&#x7814;&#x7A76;&#x4F1A;  <pr>&#x3051;&#x3093;&#x304D;&#x3085;&#x3046;&#x304B;&#x3044;</pr></trd>,
    <trd>&#x795E;&#x5B66;&#x6821;  <pr>&#x3057;&#x3093;&#x304C;&#x3063;&#x3053;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="pl">seminarium</trd>
  <trd lng="sk">semin&#xE1;r</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_je;&#x43C;&c_ib;&#x43D;&c_a;&c_r;&c_ib;&#x44F;</trd>,
    <trd>&c_s;&c_je;&#x43C;&c_ib;&#x43D;&c_a;&c_r;(&c_ib;&c_j;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7814;&#x8A0E;&#x6703; <pr>y&#xE1;nt&#x1CE;ohu&#xEC;</pr></trd>,
    <trd>&#x7814;&#x8BA8;&#x4F1A; <pr>y&#xE1;nt&#x1CE;ohu&#xEC;</pr></trd>,
    <trd>&#x8A0E;&#x8AD6;&#x6703; <pr>t&#x1CE;ol&#xF9;nhu&#xEC;</pr></trd>,
    <trd>&#x8BA8;&#x8BBA;&#x4F1A; <pr>t&#x1CE;ol&#xF9;nhu&#xEC;</pr></trd>,
    <trd>&#x8A0E;&#x8AD6;&#x73ED; <pr>t&#x1CE;ol&#xF9;nb&#x101;n</pr></trd>,
    <trd>&#x8BA8;&#x8BBA;&#x73ED; <pr>t&#x1CE;ol&#xF9;nb&#x101;n</pr></trd>
  </trdgrp>
</drv>
</art>
