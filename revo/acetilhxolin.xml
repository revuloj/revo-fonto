<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: acetilhxolin.xml,v 1.8 2023/10/27 13:42:09 revo Exp $">
<kap>
  <rad>acetil&hcirc;olin</rad>/o, 
  <var><kap><rad var="k">acetilkolin</rad>/o <fnt><bib>PIV2</bib></fnt></kap></var>
</kap>
<drv mrk="acetilhxolin.0o">
  <kap><tld/>o, <var><kap><tld var="k"/>o</kap></var></kap>
  <snc mrk="acetilhxolin.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      <frm>C<sub>7</sub>H<sub>16</sub>NO<sub>2</sub></frm>,
      <ref tip="super" cel="trans.nervo0igilo.FIZL">nervotransigilo</ref>
      aktiva en la centra kaj en la malcentra nervaro:
      <ekz>
        bazo de la kuracado estas meza dozo de kortikoidoj 
        <klr>[&#x2026;]</klr> kun <tld/>o en funkcio kiel antioksida aganto
        <fnt>
          <aut>J. Hradil</aut>,
          <vrk>Idiopatia pulma fibrozo</vrk>,
          <lok>en: <url ref="&STEB;medicino/med-mallonge.htm">
          Mallongaj artikoloj</url>, [vidita en 2008]</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[fagopiro]</klr> estas grava fonto de &hcirc;olino, kiun homa korpo bezonas por
        krei <tld/>on
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=253842">Soba</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld var="k"/>o estas acetata estero de kolino    
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Acetilkolino">
          acetilkolino</url>, 2013-04-05</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Acetylcholin</trd>
    <trdgrp lng="zh">
    <trd>&#x4E59;&#x9170;&#x80C6;&#x78B1; <pr>y&#x1D0;xi&#x101;nd&#x1CE;nji&#x1CE;n</pr></trd>,
    <trd>&#x4E59;&#x9170;&#x81BD;&#x9E7C; <pr>y&#x1D0;xi&#x101;nd&#x1CE;nji&#x1CE;n</pr></trd>,
    <trd>&#x4E59;&#x91AF;&#x80C6;&#x7877; <pr>y&#x1D0;x&#x12B;d&#x1CE;nji&#x1CE;n</pr></trd>,
    <trd>&#x4E59;&#x91AF;&#x81BD;&#x9E7C; <pr>y&#x1D0;x&#x12B;d&#x1CE;nji&#x1CE;n</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_c;&c_e;&c_t;&c_y;&c_l;&c_h;&c_a;&c_l;&c_ib;&c_n;</trd>
  <trd lng="en">acetylcholine</trd>
  <trd lng="es">acetilcolina</trd>
  <trd lng="fr">ac&eacute;tylcholine</trd>
</drv>
</art>

