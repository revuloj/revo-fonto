<drv mrk="frot.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="frot.0i.MOV">
    <dif>
      Plurfoje tien kaj reen prem&scirc;ovi ion sur io:
      <ekz>
        li estas <tld/>ita kaj polurita
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i per salo
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 16:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ita per lardo
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li forman&gcirc;is sekan kukon <tld/>itan per ajlo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj dis&ccirc;iploj de&scirc;iris la spikojn, kaj man&gcirc;is, <tld/>ante ilin en la manoj
        <fnt>
          <bib>NT</bib>
          <lok>Luko 6:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>is Elizon per suko de juglando, tiel ke &scirc;i fari&gcirc;is tute mallume bruna
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>is denove unu alumeton je la muro
        <fnt>
          <bib>Fab2</bib>
          <lok>Knabineto kun alumetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la botelo kantadis, kiam oni &gcirc;in <tld/>is per la korko
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ek&gcirc;emis, <tld/>is la okulojn, kvaza&ubreve; homo vekita
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        incensa&jcirc;o <klr>[&#x2026;]</klr> bone <tld/>miksita
        <klr>(kp <ref cel="pist.0i">pisti</ref>)</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 30:35</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <ind><tld/>purigi</ind>taj 
        <klr>(kp <ref cel="polur.0i">poluri</ref>)</klr>
        stanaj teleroj brilis
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
        <trd lng="be">&c_v;&c_y;&c_c;&c_ib;&c_r;&c_a;&c_c;&c_mol; <klr>(&c_ch;&c_y;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;)</klr></trd>
        <trd lng="fr">r&eacute;curer</trd>
      </ekz>
      <ekz>
        <tld/>purigita pargeta planko
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="grat.0i">grati</ref>,
      <ref cel="polur.0i">poluri</ref>,
      <ref cel="skrap.0i">skrapi</ref>,
      <ref cel="visx.0i">vi&scirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="af">vryf</trd>
  <trd lng="am">&#x121B;&#x123B;&#x1238;&#x1275;</trd>
  <trd lng="ar">&#x641;&#x631;&#x643;</trd>
  <trd lng="az">ovu&#x15F;durmaq</trd>
  <trdgrp lng="be">
    <trd>&c_c;&c_je;&c_r;&c_c;&c_ib;</trd>,
    <trd>&c_n;&c_a;&c_c;&c_ib;&c_r;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_r;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bn">&#x9AC;&#x9BF;&#x9AA;&#x9A4;&#x9CD;&#x9A4;&#x9BF;</trd>
  <trd lng="bo">&#xF60;&#xF55;&#xF74;&#xF62;&#xF0B;&#xF60;&#xF55;&#xF74;&#xF62;&#xF0B;&#xF42;&#xF4F;&#xF7C;&#xF44;&#xF0B;</trd>
  <trd lng="ca">fregar</trd>
  <trdgrp lng="cs">
    <trd>mnout</trd>,
    <trd>prot&rcaron;&iacute;t</trd>,
    <trd>t&rcaron;&iacute;t</trd>
  </trdgrp>
  <trd lng="da">gnide</trd>
  <trd lng="de">reiben</trd>
  <trd lng="en">rub</trd>
  <trd lng="es">frotar</trd>
  <trd lng="et">h&#xF5;&#xF5;ruda</trd>
  <trd lng="fr">frotter</trd>
  <trd lng="fy">rub</trd>
  <trd lng="gl">fregar</trd>
  <trd lng="gu">&#xA98;&#xAB8;&#xAB5;&#xAC1;&#xA82;</trd>
  <trd lng="hi">&#x930;&#x917;&#x921;&#x93C;</trd>
  <trd lng="hr">trljati</trd>
  <trd lng="hy">&#x577;&#x583;&#x578;&#x582;&#x574;</trd>
  <trdgrp lng="id">
    <trd>meng<ind>gosok</ind></trd>,
    <trd>meng<ind>gesek</ind></trd>
  </trdgrp>
  <trd lng="is">nudda</trd>
  <trd lng="ja">&#x3053;&#x3059;&#x308B;</trd>
  <trd lng="jw">samubarang kang angel</trd>
  <trd lng="ka">&#x10E0;&#x10E3;&#x10D1;&#x10DA;&#x10E1; &#x10E8;&#x10D4;&#x10D0;&#x10D3;&#x10D2;&#x10D4;&#x10DC;&#x10E1;</trd>
  <trd lng="km">&#x179B;&#x17BB;&#x1794;</trd>
  <trd lng="ko">&#xBB38;&#xC9C0;</trd>
  <trd lng="lo">&#xE96;&#xEB9;</trd>
  <trd lng="ml">&#xD24;&#xD1F;&#xD35;&#xD41;&#xD15;</trd>
  <trd lng="mr">&#x918;&#x93E;&#x938;&#x923;&#x947;</trd>
  <trd lng="my">&#x1015;&#x103D;&#x1010;&#x103A;&#x1010;&#x102D;&#x102F;&#x1000;&#x103A;</trd>
  <trd lng="ne">&#x926;&#x932;&#x94D;&#x928;&#x941;</trd>
  <trd lng="nl">wrijven</trd>
  <trd lng="ny">pakani</trd>
  <trd lng="pa">&#xA30;&#xA71;&#xA2C; &#xA26;&#xA40;</trd>
  <trd lng="ps">&#x648;&#x645;&#x648;&#x69A;&#x626;</trd>
  <trdgrp lng="pt">
    <trd>esfregar</trd>,
    <trd>friccionar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_je;&c_r;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_t;&c_i;&c_r;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_t;&c_i;&c_r;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sd">&#x6AA;&#x64A;&#x646;&#x633;&#x644;</trd>
