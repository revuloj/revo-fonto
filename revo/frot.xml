<drv mrk="frot.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="frot.0i.MOV">
    <dif>
      Plurfoje tien kaj reen prem&scirc;ovi ion sur io:
      <ekz>
        li estas <tld/>ita kaj polurita
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i per salo
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 16:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ita per lardo
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li forman&gcirc;is sekan kukon <tld/>itan per ajlo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj dis&ccirc;iploj de&scirc;iris la spikojn, kaj man&gcirc;is, <tld/>ante ilin en la manoj
        <fnt>
          <bib>NT</bib>
          <lok>Luko 6:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>is Elizon per suko de juglando, tiel ke &scirc;i fari&gcirc;is tute mallume bruna
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>is denove unu alumeton je la muro
        <fnt>
          <bib>Fab2</bib>
          <lok>Knabineto kun alumetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la botelo kantadis, kiam oni &gcirc;in <tld/>is per la korko
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ek&gcirc;emis, <tld/>is la okulojn, kvaza&ubreve; homo vekita
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        incensa&jcirc;o <klr>[&#8230;]</klr> bone <tld/>miksita
        <klr>(kp <ref cel="pist.0i">pisti</ref>)</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 30:35</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <ind><tld/>purigi</ind>taj
        <klr>(kp <ref cel="polur.0i">poluri</ref>)</klr>
        stanaj teleroj brilis
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
        <trd lng="be">&c_v;&c_y;&c_c;&c_ib;&c_r;&c_a;&c_c;&c_mol; <klr>(&c_ch;&c_y;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;)</klr></trd>
        <trd lng="fr">r&eacute;curer</trd>
      </ekz>
      <ekz>
        <tld/>purigita pargeta planko
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
