<drv mrk="frot.0a">
  <kap><tld/>a</kap>
  <snc mrk="frot.0a.rilata">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        per tio redukti&gcirc;as la <tld/>a surfaco kaj plii&gcirc;as la
        rapideco
        <fnt>
          <aut>Tibor Ujlaky-Nagy</aut>:
          <vrk>La sporta lingvo en Esperanto</vrk>.
          <lok><url ref="http://www.freeweb.hu/eventoj/steb/sportalingvo.rtf">Hungara
          Esperanto-Asocio, 1972</url></lok>
        </fnt>;
      </ekz><ekz>
        la kupla&jcirc;o estas <tld/>a kupla&jcirc;o, a&ubreve; &ccirc;e la pli novaj
        &scirc;altiloj dentrada
        <fnt>
          <aut>Ir. J. R. G. Isbr&uuml;cker</aut>:
          <vrk><url ref="&STEB;ingxenierado/telekomuniko/telefoniado1928/telefoniado1928.htm">Evoluo
          de telefonio</url></vrk>.
          <lok>Biblioteko Tutmonda, n-roj 21&ndash;22, eldonejo Rudolf
            Mosse: Berlino, 1928</lok>
        </fnt>;
      </ekz><ekz>
        <ref cel="frot.0sono.FON"><tld/>a konsonanto</ref>
        <fnt><bib>PMEG</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_r;&c_y;&c_k;&c_c;&c_y;&c_j;&c_n;&c_y;</trd>,
    <trd>&c_f;&c_r;&c_y;&c_k;&c_a;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_ja;&c_k;&c_ib; <ind>&c_t;&c_r;&c_e;&c_c;&c_c;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="pl">&sacute;cierny</trd>
  <trdgrp lng="ru">
     <trd>&c_t;&c_r;&c_u;&c_shch;&c_i;&c_j;&c_s;&c_ja;</trd>,
     <trd>&c_f;&c_r;&c_i;&c_k;&c_c;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>,
     <trd>&c_f;&c_r;&c_i;&c_k;&c_a;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
   </trdgrp>
</drv>

<drv mrk="frot.0o">
  <kap><tld/>o</kap>
  <snc mrk="frot.0o.MOV">
    <dif>
      Ago <tld/>i.
    </dif>
    <trd lng="de">Reiben</trd>
    <trd lng="es">frotaci&oacute;n</trd>
  </snc>
  <snc mrk="frot.0o.FIZ">
    <dif>
      Rezisto de du objektoj, kiuj &scirc;ovi&gcirc;as unu sur alia;
      inter<tld/>i&gcirc;o:
      <ekz>
        tio klarigis bremsotempon, <tld/>on kaj aliajn komplikajn konceptojn
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2015/011597.php">&Scirc;ton&jcirc;eto sen cenzuro</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind><tld/>punkto</ind> <klr>(malfacila&jcirc;o)</klr>.
        <trd lng="fr">point de friction<klr tip="ind">
        (d&eacute;saccord)</klr></trd>
      </ekz>
    </dif>
    <trd lng="es">fricci&oacute;n</trd>
    <trd lng="de">Reibung</trd>
    <trdgrp lng="zh">
    <trd>&#30952;&#25830; <pr>m&oacute;c&amacron;</pr></trd>,
    <trd>&#20105;&#25191; <pr>zh&emacron;ngzh&iacute;</pr></trd>,
    <trd>&#25705;&#25830;&#21147; <pr>m&oacute;c&amacron;l&igrave;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_t;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="ca">fregament</trd>
  <trdgrp lng="cs">
    <trd>t&rcaron;en&iacute;</trd>,
    <trd>&scaron;krtnut&iacute;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>frottement</trd>,
    <trd>friction<klr tip="ind"> (frottement)</klr></trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>gosok</ind>an</trd>,
    <trd><ind>gesek</ind>an</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#25705;&#25830;  <pr>&#12414;&#12373;&#12388;</pr></trd>,
    <trd>&#12371;&#12377;&#12427;&#12371;&#12392;</trd>
  </trdgrp>
  <trd lng="nl">wrijving</trd>
  <trdgrp lng="pl">
    <trd>tarcie</trd>,
    <trd>froterowanie</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>mas&aacute;&zcaron;</trd>,
    <trd>trenie</trd>
  </trdgrp>
  <trd lng="uk">&c_t;&c_je;&c_r;&c_t;&c_ja;</trd>
</drv>

<drv mrk="frot.de0i">
  <kap>de<tld/>i, <var><kap>for<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="frot.de0i.forfroti">
    <dif>
      <tld lit="F"/>ante forigi:
      <ekz>
        oni povas tre bone vidi, ke tio estas tomba &scirc;tono <klr>[&#8230;]</klr>, tamen la
        surskribo <klr>[&#8230;]</klr> preska&ubreve; tute de<tld/>i&gcirc;is
        <fnt>
          <bib>Fab3</bib>
          <lok>Malnova tomba &scirc;tono</lok>
        </fnt>.
      </ekz>
      <ekz>
        surbruste <klr>[&#8230;]</klr> ikoneto kun duone for<tld/>ita bildo de nekonata
        sanktulo
        <fnt>
          <bib>MkM</bib>
          <lok>5</lok>
        </fnt>;
      </ekz>
      <ekz>
        maldikaj kotonvesta&jcirc;oj, ki<klr>[es]</klr> flormotivoj estis for<tld/>itaj de
        koroda perua &scirc;mirsapo
        <fnt>
          <bib>VRA</bib>
          <lok>Harry Martinson</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>esborrar</trd>,
      <trd>raspar</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>abreiben</trd>,
      <trd>wegreiben</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>effacer<klr tip="ind"> (en frottant)</klr></trd>,
      <trd lng="fr">r&eacute;curer</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_t;&c_je;&c_r;&c_je;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_t;&c_je;&c_r;&c_je;&c_t;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>zotrie&tcaron;</trd>,
      <trd>vytiera&tcaron;</trd>
      </trdgrp>
  </snc>
  <snc mrk="frot.de0i.skrapi">
    <dif>
      Supra&jcirc;e skrapdifekti:
      <ekz>
        &ccirc;iuj kapoj senhari&gcirc;is kaj &ccirc;iuj &scirc;ultroj de<tld/>i&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 29:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ha&ubreve;to estis tiom for<tld/>ita, ke sangogutoj trapenetris &gcirc;in
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">ratllar</trd>
    <trd lng="de">absch&uuml;rfen</trd>
    <trdgrp lng="fr">
      <trd>rayer</trd>,
      <trd>&eacute;rafler</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_c;&c_ib;&c_r;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_c;&c_ib;&c_r;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">set&rcaron;&iacute;t</trd>
  <trd lng="es">fregar</trd>
  <trdgrp lng="ja">
    <trd>&#12371;&#12377;&#12426;&#33853;&#12392;&#12377;  <pr>&#12371;&#12377;&#12426;&#12362;&#12392;&#12377;</pr></trd>,
    <trd>&#12363;&#12377;&#12426;&#20663;&#12434;&#12388;&#12369;&#12427;  <pr>&#12363;&#12377;&#12426;&#12365;&#12378;&#12434;&#12388;&#12369;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>wegkrabben</trd>,
    <trd>afwrijven</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&sacute;ciera&cacute;</trd>,
    <trd>wyciera&cacute;</trd>
  </trdgrp>
  <trd lng="sk">o&scaron;&uacute;pa&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_t;&c_i;&c_r;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_t;&c_i;&c_r;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="frot.dis0i">
  <kap>dis<tld/>i, <var><kap>tra<tld/>i</kap></var></kap>
  <snc mrk="frot.dis0i.KOMUNE">
    <dif>
      Per <tld/>o diserigi,
      <ref tip="vid" cel="uz.el0i.foruzi">eluzi</ref> materialon:
      <ekz>
        miksi &gcirc;in, bone dis<tld/>ante, kaj ricevinte specon de nedensa pasto
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Mi dis<tld/>as ilin simile al polvo de la tero
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 22:43</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;nurego estis tro dika, mi ne povis tra<tld/>i &gcirc;in kaj mi ne povis elturni
        miajn membrojn el &gcirc;i
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 48</lok>
        </fnt>;
      </ekz>
      <ekz>
        akvofaloj kaj torentoj <klr>[&#8230;]</klr> jarojn post
        jaroj dis<tld/>as la rokojn, sur kiuj ili fluas
        <fnt>
          <aut>W. F. Rolt</aut>,
          <vrk>El la Polvo de la Tero</vrk>,
          <lok>1967</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_r;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">erosionar</trd>
  <trd lng="cs">rozet&rcaron;&iacute;t</trd>
  <trdgrp lng="de">
    <trd>zerreiben</trd>,
    <trd>aufreiben</trd>,
    <trd>zermalmen</trd>
  </trdgrp>
  <trd lng="fr">effriter</trd>
  <trd lng="ja">&#12377;&#12426;&#12388;&#12406;&#12377;</trd>
  <trd lng="pl">rozciera&cacute;</trd>
  <trd lng="sk">rozotrie&tcaron;</trd>
  <trd lng="uk">&c_r;&c_o;&c_z;&c_t;&c_i;&c_r;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#30740; <pr>y&aacute;n</pr></trd>,
    <trd>&#28040;&#30952; <pr>xi&amacron;om&oacute;</pr></trd>,
    <trd>&#30952;&#30862; <pr>m&ograve;su&igrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="frot.el0i">
  <kap>
    el<tld/>i
  </kap>
  <snc>
    <dif>
      <tld lit="F"/>e forigi ion (ekz-e malpura&jcirc;on) el io:
      <ekz>
        pureco estas duono de vivo! &scirc;i diris kaj el<tld/>is la kaldronon
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="skrap.el0i">elskrapi</ref>,
      <ref cel="pur.frot0igi">frotpurigi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_c;&c_ib;&c_r;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>ausreiben</trd>,
    <trd>scheuern</trd>
  </trdgrp>
  <trd lng="en">rub out</trd>
  <trd lng="fr">effacer</trd>
  <trd lng="ja">&#30952;&#12365;&#12354;&#12370;&#12427;  <pr>&#12415;&#12364;&#12365;&#12354;&#12370;&#12427;</pr></trd>
  <trdgrp lng="pl">
    <trd>wytrze&cacute;</trd>,
    <trd>zetrze&cacute;</trd>,
    <trd>pucowa&cacute;</trd>
  </trdgrp>
</drv>

<drv mrk="frot.en0i">
  <kap>
    en<tld/>i
  </kap>
  <snc>
    <dif>
      <tld lit="F"/>i substancon sur io (ekz-e ha&ubreve;to) &gcirc;is &gcirc;i enprofundi&gcirc;as:
      <ekz>
        li en<tld/>is al &scirc;i la &scirc;mira&jcirc;on en la piedon
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi devis en<tld/>i al mi poste bonan porcion da tabako <klr>[en la nazon]</klr>
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        per la manplato &scirc;i komencis en<tld/>i la pomadon sur la frunto kaj vangoj
        <fnt>
          <bib>MkM</bib>
          <lok>20</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="sxmir.0i.surmeti">&scirc;miri<sncref/></ref>
  </snc>
  <trd lng="be">&c_u;&c_c;&c_ib;&c_r;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">einreiben</trd>
  <trd lng="en">rub in</trd>
  <trd lng="fr">insister sur</trd>
  <trdgrp lng="pl">
    <trd>natrze&cacute;</trd>,
    <trd>wciera&cacute;</trd>
  </trdgrp>
  <trd lng="zh">&#25186; <pr>g&#468;</pr></trd>
</drv>

<drv mrk="frot.inter0i">
  <kap>
    inter<tld/>i,
    <var><kap>kun<tld/>i</kap></var>
  </kap>
  <snc>
    <dif>
      <tld lit="F"/>i unu sur la alia, ekz-e por <tld/>varmigi:
      <ekz>
        per mallonga movo li inter<tld/>is la manojn
        <fnt>
          <bib>MkM</bib>
          <lok>25</lok>
        </fnt>;
      </ekz>
      <ekz>
        se ni kun<tld/>os ilin kvaza&ubreve; fajrobastonetojn, ni ekflamigos <klr>[fajron]</klr>
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Kvar</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_je;&c_r;&c_c;&c_ib; <klr>(&c_a;&c_d;&c_n;&c_o; &c_a;&c_b; &c_a;&c_d;&c_n;&c_o;)</klr></trd>
  <trdgrp lng="de">
    <trd>zusammen<ind>reiben</ind></trd>,
    <trd>scheuern <klr>(aneinander reiben)</klr></trd>
  </trdgrp>
  <trd lng="en">rub together</trd>
  <trd lng="pl">trze&cacute; si&eogonek; wzajemnie</trd>
</drv>

<drv mrk="frot.tra0i">
  <kap>
    tra<tld/>i
  </kap>
  <snc>
    <dif>
      <tld lit="F"/>i &gcirc;is ekestas truo, dis&scirc;iro:
      <ekz>
        la &scirc;nurego estis tro dika, mi ne povis tra<tld/>i &gcirc;in kaj mi ne povis elturni
        miajn membrojn
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 48</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la tablo ku&scirc;is verda vakstolo, kiu je la anguloj kaj e&gcirc;oj estis tiom
        tra<tld/>ita, ke oni povis meti siajn manojn en la truojn
        <fnt>
          <aut>B.Traven, trad. Dorothea &amp; Hans-Georg Kaiser</aut>
          <vrk><url ref="https://www.karapaco.de/u/B.Traven/btraven_mahagontrunko.pdf"
              >Mahagontrunko</url></vrk>
          <lok>&ccirc;ap. 4, p. 152</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="uz.el0i.foruzi">eluzi<sncref/></ref>,
      <ref cel="triv.0i">trivi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_c;&c_ib;&c_r;&c_a;&c_c;&c_mol; <klr>(&c_n;&c_a;&c_s;&c_k;&c_r;&c_o;&c_z;&c_mol;)</klr></trd>
  <trdgrp lng="de">
    <trd>durchscheuern</trd>,
    <trd>zerschlei&szlig;en</trd>
  </trdgrp>
</drv>

<drv mrk="frot.0sono">
  <kap><tld/>sono</kap>
  <snc mrk="frot.0sono.KOMUNE">
    <dif>
      Bruo produktata de <tld/>ado:
      <ekz>
        grinci: eligi malagrablan <tld/>sonon
        <fnt>
          <aut>William Auld</aut>:
          <vrk>Pa&scirc;oj al plena posedo</vrk>.
          <lok><url
          ref="http://www.elerno.cn/elibro/pasoj%20al%20plena%20posedo/dudeksesa.htm">Leciono
          26&#170;.</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_g;&c_u;&c_k; &c_t;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_ja;</trd>
  </snc>
  <snc mrk="frot.0sono.FON">
    <uzo tip="fak">FON</uzo>
    <dif>
      Da&ubreve;ra <ref tip="super" cel="konson1.0o">konsonanto</ref>,
      produktata per malvastigo de la bu&scirc;kanalo,
      tra kiu la spirblovo eliras naskante <tld/>an bruon; en
      Esperanto la sonvaloroj de <em>f, v, s, z, &scirc;, &jcirc;, &hcirc;, h</em>:
      <ekz>
        ekzemplo de aktuale okazanta &scirc;an&gcirc;o en la angla prononco
        estas la evoluo de la dentaj <tld/>sonoj, [&theta;] kiel en
        &leftquot;<nac>mouth</nac>&rightquot; kaj [&#240;] kiel en &leftquot;<nac>smooth</nac>&rightquot;, kiuj
        iom post iom fari&gcirc;as lipo-dentaj [f], respektive [v]
        <fnt>
          <aut>John Wells</aut>:
          <vrk><url ref="http://esperanto.org.uk/lbe/arkivo/965/07.html">La Liverpola
          prononco</url></vrk>.
          <lok>La Brita Esperantisto, a&ubreve;tuno 2008.</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="bru.0anto.FON">bruanto</ref>
    <trd lng="be">&c_f;&c_r;&c_y;&c_k;&c_a;&c_t;&c_y;&c_w;&c_n;&c_y; &c_g;&c_u;&c_k;</trd>
    <trd lng="ca">consonant <ind>fricativa</ind></trd>
    <trdgrp lng="de">
      <trd>Frikativ</trd>,
      <trd>Engelaut</trd>,
      <trd>Reibelaut</trd>
    </trdgrp>
    <trd lng="en">fricative</trd>
    <trd lng="es">fricativa</trd>
    <trd lng="fa">&a_h;&a_m;&a_kh;&a_w;&a_A;&a_n;
      <ind>&a_s;&a_A;&f_y;&a_sh;&f_y;</ind></trd>
    <trdgrp lng="fr">
      <trd>fricative<klr> (subst.)</klr></trd>,
      <trd>spirante<klr> (subst.)</klr></trd>
    </trdgrp>
    <trd lng="nl">fricatief</trd>
    <trd lng="pl">sp&oacute;&lstroke;g&lstroke;oska szczelinowa</trd>
    <trdgrp lng="pt">
      <trd>consoante <ind>constritiva fricativa</ind></trd>,
      <trd>fricativa</trd>,
      <trd>consoante <ind>constritiva</ind></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_f;&c_r;&c_i;&c_k;&c_a;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j; &c_z;&c_v;&c_u;&c_k;</trd>,
      <trd>&c_s;&c_p;&c_i;&c_r;&c_a;&c_n;&c_t;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="frot.ek0akonsonanto">
  <kap>ek<tld/>a konsonanto <fnt><bib>PMEG</bib></fnt></kap>
  <uzo tip="fak">FON</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="konson1.0o">Konsonanto</ref>
      kombinanta parolsonojn
      <ref tip="prt" cel="eksplo.0akonsonanto">eksplodan</ref> kaj
      <ref tip="prt" cel="frot.0sono.FON">frotan</ref>, en kiu
      la eksplodan altenon sekvas frotsona forteno de la lango
      (en Esperanto: <em>c, &ccirc;, &gcirc;</em>):
      <ekz>
        la silabolimo situas anta&ubreve; la konsonanto <klr>(...)</klr>
        anka&ubreve;, se la konsonanto estas ek<tld/>a (/<esc>pro-c&eacute;-so</esc>/,
        /<esc>fe-l&iacute;-&ccirc;a</esc>/, /<esc>p&aacute;-&gcirc;o</esc>/)
        <fnt>
          <aut>Marc Bavant</aut>: <vrk>Silabo kaj silabado</vrk>.
          <lok><url ref="http://lingvakritiko.com/2006/12/20/silabo-kaj-silabado/">Lingva
          kritiko</url></lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="super" cel="bru.0anto.FON">bruanto</ref>
  <trd lng="be">&c_a;&c_f;&c_r;&c_y;&c_k;&c_a;&c_t;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Affrikat</trd>,
    <trd>Affrikata</trd>,
    <trd>Affrikate</trd>
  </trdgrp>
  <trd lng="ca">africat</trd>
  <trd lng="en">affricative</trd>
  <trd lng="es">africada</trd>
  <trd lng="fa">&a_h;&a_m;&a_kh;&a_w;&a_A;&a_n; &a_m;&a_r;&f_k;&a_b;</trd>
  <trd lng="fr">affriqu&eacute;e<klr> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>z&aacute;r-r&eacute;shang</trd>,
    <trd>affrik&aacute;ta</trd>
  </trdgrp>
  <trd lng="it">affricata</trd>
  <trdgrp lng="pl">
    <trd>afrykata</trd>,
    <trd>sp&oacute;&lstroke;g&lstroke;oska zwarto-szczelinowa</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>africado</trd>,
    <trd>explosivo-fricativo</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_f;&c_f;&c_r;&c_i;&c_k;&c_a;&c_t;&c_a;</trd>
</drv>

