<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>frot</rad>/i
</kap>
<drv mrk="frot.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Plurfoje tien k reen prem&scirc;ovi ion sur io:
      <ekz><tld/>i la ha&ubreve;ton per harmufo post bano;</ekz>
      <ekz><tld/>i pargeton<fnt>B</fnt>;</ekz>
      <ekz>&ccirc;iuj vazoj brilis spegulsimile <tld/>purigitaj<fnt>Z</fnt>;</ekz>
      <ekz><tld/>lavi tukojn.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="grat.0i">grati</ref>,
      <ref cel="skrap.0i">skrapi</ref>,
      <ref cel="visx.0i">vi&scirc;i</ref>,
      <ref cel="polur.0i">poluri</ref>.
    </refgrp>
  </snc>
  <trd lng="de">reiben</trd>
  <trd lng="en">rub</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_je;&c_r;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_t;&c_i;&c_r;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_t;&c_i;&c_r;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="frot.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Ago <tld/>i. 
    </dif>
    <trd lng="de">Reiben</trd>
  </snc>
  <snc num="2">
    <dif>
      Rezisto de du objektoj, kiuj &scirc;ovi&gcirc;as unu sur alia; inter<tld/>i&gcirc;o:
      <ekz>
        <tld/>punkto <klr>(f: malfacila&jcirc;o)</klr>.
      </ekz> 
    </dif>
    <trd lng="de">Reibung</trd>
  </snc>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>
<drv mrk="frot.de0i">
  <kap>de<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1">
    <dif>
      <tld lit="F"/>ante forigi:
      <ekz>
        la surskribo sur la tomba &scirc;tono preska&ubreve; tute de<tld/>i&gcirc;is<fnt>Z</fnt>.
      </ekz> 
    </dif>
    <trd lng="de">abreiben</trd>
    <trd lng="ru">&c_s;&c_t;&c_je;&c_r;&c_je;&c_t;&c_mol;</trd>
  </snc>
  <snc num="2">
    <dif>
      Supra&jcirc;e skrapdifekti.  
    </dif>
    <trd lng="de">absch&uuml;rfen</trd>
  </snc>
</drv>
</art>
