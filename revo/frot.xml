<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>frot</rad>/i</kap>
<drv mrk="frot.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Plurfoje tien kaj reen prem&scirc;ovi ion sur io:
      <ekz>
        <tld/>i la ha&ubreve;ton per harmufo post bano;
      </ekz>
      <ekz>
        <tld/>i pargeton<fnt>B</fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj vazoj brilis spegulsimile
        <ind><tld/>purigi</ind>taj<fnt>Z</fnt>;
        <trd lng="be">&c_v;&c_y;&c_c;&c_ib;&c_r;&c_a;&c_c;&c_mol; <klr>(&c_ch;&c_y;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;)</klr></trd>
        <trd lng="fr">r&eacute;curer</trd>
      </ekz>
      <ekz>
        <tld/>lavi tukojn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="grat.0i">grati</ref>,
      <ref cel="skrap.0i">skrapi</ref>,
      <ref cel="visx.0i">vi&scirc;i</ref>,
      <ref cel="polur.0i">poluri</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_je;&c_r;&c_c;&c_ib;</trd>,
    <trd>&c_n;&c_a;&c_c;&c_ib;&c_r;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_r;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">reiben</trd>
  <trd lng="en">rub</trd>
  <trd lng="es">frotar</trd>
  <trd lng="fr">frotter</trd>
  <trd lng="nl">wrijven</trd>
  <trdgrp lng="pt">
    <trd>esfregar</trd>,
    <trd>friccionar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_je;&c_r;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_t;&c_i;&c_r;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_t;&c_i;&c_r;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">trie&tcaron;</trd>
</drv>

<drv mrk="frot.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i.
    </dif>
    <trd lng="de">Reiben</trd>
    <trd lng="es">frotaci&oacute;n</trd>
  </snc>
  <snc>
    <dif>
      Rezisto de du objektoj, kiuj &scirc;ovi&gcirc;as unu sur alia; inter<tld/>i&gcirc;o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind><tld/>punkto</ind> <klr>(malfacila&jcirc;o)</klr>.
        <trd lng="fr">point de friction<klr tip="ind">
        (d&eacute;saccord)</klr></trd>
      </ekz>
    </dif>
    <trd lng="es">fricci&oacute;n</trd>
    <trd lng="de">Reibung</trd>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="fr">
    <trd>frottement</trd>,
    <trd>friction<klr tip="ind"> (frottement)</klr></trd>
  </trdgrp>
  <trd lng="nl">wrijving</trd>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">trenie</trd>
</drv>

<drv mrk="frot.de0i">
  <kap>de<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="F"/>ante forigi:
      <ekz>
        la surskribo sur la tomba &scirc;tono preska&ubreve; tute
        de<tld/>i&gcirc;is<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">abreiben</trd>
    <trd lng="fr">effacer<klr tip="ind"> (en frottant)</klr></trd>
    <trd lng="ru">&c_s;&c_t;&c_je;&c_r;&c_je;&c_t;&c_mol;</trd>
    <trd lng="sk">zotrie&tcaron;</trd>
  </snc>
  <snc>
    <dif>
      Supra&jcirc;e skrapdifekti.
    </dif>
    <trd lng="de">absch&uuml;rfen</trd>
    <trdgrp lng="fr">
      <trd>rayer</trd>,
      <trd>&eacute;rafler</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_c;&c_ib;&c_r;&c_a;&c_c;&c_mol;</trd>
  <trd lng="nl">afwrijven</trd>
</drv>
</art>
