<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>urtik</rad>/o
</kap>
<drv mrk="urtik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="urtika.0oj"><tld/>acoj</ref>
      (<trd lng="la">Urtica</trd>),
      kies folioj kaj trunketo pikas sentigante brulumon:
      <ekz><tld/>on frosto ne difektas
        <klr>(malbonuloj &ccirc;iam estas plej fortikaj)</klr>
<fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
  </snc>
  <bld lok="../bld/urtik.jpg">
    <ind>granda <tld/>o</ind>
    <trd lng="la">Urtica dioica</trd>
  </bld>
  <trd lng="de">Brennnessel</trd>
  <trd lng="es">ortiga</trd>
  <trd lng="fr">ortie</trd>
  <trd lng="nl">brandnetel</trd>
  <trd lng="ru">&c_k;&c_r;&c_a;&c_p;&c_i;&c_v;&c_a;</trd>
</drv>

</art>
