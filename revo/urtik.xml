<art mrk="$Id: urtik.xml,v 1.32 2024/09/11 06:11:12 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>urtik</rad>/o
</kap>
<drv mrk="urtik.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
       tip="malprt" cel="urtika.0oj"><tld/>acoj</ref>
       (<trd lng="la">Urtica</trd>),
       kies folioj kaj trunketo pikas sentigante brulumon:
       <ekz><tld/>on frosto ne difektas
         <klr>(malbonuloj &ccirc;iam estas plej fortikaj)</klr>
         <fnt><bib>PrV</bib></fnt>.</ekz>
     </dif>
   </snc>
   <bld lok="../bld/urtik.jpg">
     <ind>granda <tld/>o</ind>
     <trd lng="ca">ortiga gran</trd>
     <trd lng="br">linad bras</trd>
     <trd lng="hu">nagy <ind>csal&aacute;n</ind></trd>
     <trd lng="la">Urtica dioica</trd>
     <trd lng="pl">pokrzywa zwyczajna</trd>
   </bld>
   <trd lng="be">&c_k;&c_r;&c_a;&c_p;&c_ib;&c_v;&c_a;</trd>
   <trd lng="bg">&c_k;&c_o;&c_p;&c_r;&c_i;&c_v;&c_a;</trd>
   <trd lng="br">linad(enn)</trd>
   <trd lng="ca">ortiga</trd>
   <trd lng="de">Brennnessel</trd>
   <trd lng="en">nettle</trd>
   <trd lng="es">ortiga</trd>
   <trd lng="fa">&f_g;&a_z;&a_n;&a_h;</trd>
   <trd lng="fi">nokkonen</trd>
   <trd lng="fr">ortie</trd>
   <trd lng="hu">csal&aacute;n</trd>
   <trd lng="it">ortica</trd>
   <trd lng="ja">&#12452;&#12521;&#12463;&#12469;</trd>
  <trd lng="nl">brandnetel</trd>
   <trd lng="pl">pokrzywa</trd>
   <trd lng="pt">urtiga</trd>
   <trd lng="ru">&c_k;&c_r;&c_a;&c_p;&c_i;&c_v;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>p&racute;h&lcaron;ava</trd>,
    <trd>&zcaron;ih&lcaron;ava</trd>
  </trdgrp>
  <trd lng="cs">kop&rcaron;iva</trd>
  <trd lng="uk">&c_k;&c_r;&c_o;&c_p;&c_i;&c_v;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#24322;&#26666;&#33640;&#40635; <pr>y&igrave;zh&#363;qi&aacute;nm&aacute;</pr></trd>,
    <trd>&#30064;&#26666;&#34113;&#40635; <pr>y&igrave;zh&#363;qi&aacute;nm&aacute;</pr></trd>,
    <trd>&#17773; <pr>qi&aacute;n</pr></trd>,
    <trd>&#33640; <pr>x&uacute;n</pr></trd>,
    <trd>&#34113; <pr>x&uacute;n</pr></trd>
  </trdgrp>
</drv>

</art>
