<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: baf.xml,v 1.4 2012/08/24 19:10:23 revo Exp $">
<kap>
  <rad>baf</rad> <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="baf.0">
  <kap><tld/></kap>
  <gra><vspec>ekkrio</vspec></gra>
  <snc mrk="baf.0.KOMUNE">
    <dif>
      Malgrave, neinterese, elrevige, indiferente:
      <ekz>
        kaj nun, <tld/>, mi ne scias kiel mi miksi&gcirc;is kun vi
        <fnt>
          <aut>M. de Seabra</aut>,
          <vrk>Deely Bobbers por Lucy</vrk>,
          <lok>Promeso en obskuro, 1997</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>, &scirc;ajnas tede, sed mi rigardos
        <fnt>
          <aut>C. Smith</aut>,
          <vrk><url
          ref="http://www.liberafolio.org/2011/vikipedio-en-esperanto-baldau-dekjara">
          Vikipedio en Esperanto - balda&ubreve; dekjara </url></vrk>,
          <lok>libera folio, 2011-01-16</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">bah!</trd>
  <trdgrp lng="fr">
    <trd>bah !</trd>,
    <trd>bof !</trd>
  </trdgrp>
 <trdgrp lng="hu">
   <trd>&aacute;h</trd>,
   <trd>l&aacute;ri-f&aacute;ri</trd>
 </trdgrp> 
 <trdgrp lng="it">
    <trd>mah</trd>,
    <trd>boh</trd>,
    <trd>uffa</trd>
  </trdgrp>
</drv>
</art>

