<art mrk="$Id: oraci.xml,v 1.21 2024/01/30 16:09:26 revo Exp $">
<kap>
  <rad>oraci</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="oraci.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="oraci.0o.BELE">
    <uzo tip="fak">BELE</uzo>
    <dif>
      Literatura verko, destinita por esti dirata anta&ubreve; publiko:
      <ekz>
        la funebraj <tld/>oj de Bossuet;
      </ekz>
      <ekz>
        reprezentanto de la Irlandaj Naciistoj <klr>...</klr> faris 
        patosan <tld/>on
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="diskur.0o">diskurso</ref>,
      <ref cel="parol.0ado">parolado</ref>,
      <ref cel="orator.0ajxo">oratora&jcirc;o</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>obrann&aacute; &rcaron;e&ccaron;</trd>,
    <trd>orace</trd>,
    <trd>promluva</trd>,
    <trd>smute&ccaron;n&iacute; &rcaron;e&ccaron;</trd>
  </trdgrp>
  <trd lng="de">Rede</trd>
  <trd lng="en">oration</trd>
  <trdgrp lng="fr">
    <trd>discours</trd>,
    <trd>harangue</trd>,
    <trd>oraison</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>besz&eacute;d</trd>,
    <trd>sz&oacute;noklat</trd>,
    <trd>or&aacute;ci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5F14;&#x8F9E;  <pr>&#x3061;&#x3087;&#x3046;&#x3058;</pr></trd>,
    <trd>&#x8FFD;&#x60BC;&#x6F14;&#x8AAC;  <pr>&#x3064;&#x3044;&#x3068;&#x3046;&#x3048;&#x3093;&#x305C;&#x3064;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>redevoering</trd>,
    <trd>oratie</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>oracja</trd>,
    <trd>mowa</trd>
  </trdgrp>
  <trd lng="pt">ora&ccedil;&atilde;o<klr tip="ind"> (ret&oacute;rica)</klr></trd>
  <trdgrp lng="ro">
    <trd>discurs</trd>,
    <trd>vorbire</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_ch;&c_mol;<klr tip="amb"> (&c_k;&c_a;&c_k; &c_l;&c_i;&c_t;&c_je;&c_r;&c_a;&c_t;&c_u;&c_r;&c_n;&c_o;&c_je;
  &c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trdgrp lng="sk">
    <trd>predn&aacute;&scaron;ka; prejav</trd>,
    <trd>re&ccaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x445;&#x432;&#x430;&#x43B;&#x44C;&#x43D;&#x430; &#x43F;&#x440;&#x43E;&#x43C;&#x43E;&#x432;&#x430; &#x43D;&#x430; &#x447;&#x435;&#x441;&#x442;&#x44C; &#x43D;&#x435;&#x431;&#x456;&#x436;&#x447;&#x438;&#x43A;&#x430;</trd>,
    <trd>&#x43D;&#x430;&#x434;&#x433;&#x440;&#x43E;&#x431;&#x43D;&#x430; &#x43F;&#x440;&#x43E;&#x43C;&#x43E;&#x432;&#x430;</trd>,
    <trd>&#x43F;&#x440;&#x43E;&#x449;&#x430;&#x43B;&#x44C;&#x43D;&#x435; &#x441;&#x43B;&#x43E;&#x432;&#x43E;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x81F4;&#x8F9E; <pr>zh&#xEC;c&#xED;</pr></trd>,
    <trd>&#x81F4;&#x8FAD; <pr>zh&#xEC;c&#xED;</pr></trd>,
    <trd>&#x6F14;&#x8AAA; <pr>y&#x1CE;nshu&#x14D;</pr></trd>,
    <trd>&#x6F14;&#x8BF4; <pr>y&#x1CE;nshu&#x14D;</pr></trd>
  </trdgrp>
</drv>
