<drv mrk="labor.0isto">
  <kap><tld/>isto, <tld/>ulo</kap>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Homo, kiu <tld/>as:
      <ekz>
        la <tld/>istoj de la penso.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_r;&c_a;&c_c;&c_a;&c_w;&c_n;&c_ib;&c_k;</trd>
    <trd lng="de">Arbeitender</trd>
    <trd lng="hu">dolgoz&oacute;</trd>    
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_b;&c_o;&c_t;&c_n;&c_i;&c_k;</trd>,
      <trd>&c_t;&c_r;&c_u;&c_zh;&c_je;&c_n;&c_i;&c_k;</trd>
    </trdgrp>
  </snc>
  <snc mrk="labor.0isto0ulo.proleto">
    <dif>
      Homo, kiu metie kaj korpe laboras por akiri vivrimedojn:
      <ekz>
        kampo<tld/>isto;
      </ekz>
      <ekz>
        la <ind><tld/>ista klaso</ind>.
        <trd lng="br">renkad al labourerien</trd>
        <trd lng="fr">classe ouvri&egrave;re</trd>
        <trd lng="hu">munk&aacute;soszt&aacute;ly</trd>        
        <trd lng="ru">&c_r;&c_a;&c_b;&c_o;&c_ch;&c_i;&c_j;
        &c_k;&c_l;&c_a;&c_s;&c_s;</trd>
      </ekz>
    </dif>
    <ref tip="vid" cel="prolet.0o">proleto</ref>
    <trd lng="be">&c_p;&c_r;&c_a;&c_c;&c_o;&c_w;&c_n;&c_y;
    <klr>(&c_n;&c_a;&c_z;.)</klr></trd>
    <trd lng="de">Arbeiter</trd>
    <trdgrp lng="en">
      <trd>laborer</trd>,
      <trd>working man</trd>,
    </trdgrp>
    <trd lng="hu">munk&aacute;s</trd>        
    <trd lng="ru">&c_r;&c_a;&c_b;&c_o;&c_ch;&c_i;&c_j;
    <klr>(&c_s;&c_u;&c_shch;.)</klr></trd>
  </snc>
  <trdgrp lng="br">
    <trd>labourer</trd>,
    <trd>micherour</trd>
  </trdgrp>
  <trd lng="el">&epsilon;&rho;&gamma;&alfa;&zeta;&omikron_ton;&my;&epsilon;&ny;&omikron;&sigma_fina;</trd>
  <trd lng="es">trabajador</trd>
  <trdgrp lng="fr">
    <trd>ouvrier</trd>,
    <trd>travailleur<klr tip="ind"> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="nl">arbeider</trd>
  <trd lng="pt">trabalhador</trd>
</drv>

