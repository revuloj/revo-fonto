<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>labor</rad>/i
</kap>
<drv mrk="labor.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Korpe a&ubreve; spirite peni por produkti a&jcirc;on, plenumi taskon,
      rezultigi efikon:
      <ekz>
        <tld/>i en fabriko, oficejo, pri terkulturo;
      </ekz>
      <ekz>
        <tld/>i super malfacila tasko, super verko, romano,
        teksilo<fnt>Z</fnt>,
        super la stablo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la homaro, por kies bono ni <tld/>as<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiuj, kiuj volas <tld/>i super la lingvo internacia<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <adm>
    &Scirc;ajnus al mi pli logike ne kovri &ccirc;iujn sencojn de ~i
    per la fako EKON kaj konservi &gcirc;in nur por speciala faka
    senco (sub ~i a&ubreve; ~o). <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_c;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">labourat</trd>
  <trd lng="cs">pracovat</trd>
  <trd lng="de">arbeiten</trd>
  <trdgrp lng="el">
    <trd>&delta;&omikron;&ypsilon;&lambda;&epsilon;&ypsilon_ton;&omega;</trd>,
    <trd>&epsilon;&rho;&gamma;&alfa_ton;&zeta;&omikron;&my;&alfa;&jota;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>work</trd>,
    <trd>labor</trd>
  </trdgrp>
  <trd lng="es">trabajar</trd>
  <trd lng="fr">travailler<klr tip="ind"> (intr.)</klr></trd>
  <trdgrp lng="hu">
    <trd>dolgozik</trd>,
    <trd>m&udblac;k&ouml;dik</trd>
  </trdgrp>  
  <trd lng="it">lavorare</trd>
  <trd lng="nl">werken</trd>
  <trd lng="pt">trabalhar</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_b;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_t;&c_r;&c_u;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="vo">v&ouml;lig&ouml;n</trd>
</drv>

<drv mrk="labor.0o">
  <kap><tld/>o</kap>
  <snc mrk="labor.0o.KOMUNE">
    <dif>
      Ago <tld/>i, rezulto de tia ago:
      <ekz>
        <tld/>o kondukas al honoro kaj oro<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_r;&c_a;&c_c;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Arbeit</trd>,
      <trd>Arbeiten</trd>,
      <trd>Werken</trd>
    </trdgrp>
    <trd lng="ru">&c_t;&c_r;&c_u;&c_d;</trd>
  </snc>
  <snc mrk="labor.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      <ref tip="super" cel="grand.fizika0o">Fizika grando</ref>,
      egala al la integralo de skalara produto de
      <ref cel="fort.0o.FIZ">forto</ref>, aplikita al korpo, per la
      infinitezima movo, kiun &gcirc;ia efiko produktas sur la korpo:
      <ekz>
        <tld/>o de konstanta forto, aplikita al korpo, kiu movi&gcirc;as
        la&ubreve; la direkto de la forto, egalas al produto de la forto
        per la trairata distanco;
      </ekz>
      <ekz>
        <tld/>o estas unu el la formoj de
        <ref tip="super" cel="energi.0o.FIZ">energio</ref>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="jxul.jx0o">&jcirc;ulo</ref>,
      <ref cel="pov.0o.FIZ">povo</ref>.
    </refgrp>
    <trd lng="be">&c_r;&c_a;&c_b;&c_o;&c_t;&c_a;</trd>
    <trd lng="de">Arbeit</trd>
  </snc>
  <trd lng="br">labour</trd>
  <trdgrp lng="el">
    <trd>&delta;&omikron;&ypsilon;&lambda;&epsilon;&jota;&alfa_ton;</trd>,
    <trd>&epsilon;&rho;&gamma;&alfa;&sigma;&jota_ton;&alfa;</trd>
  </trdgrp>
  <trd lng="fr">travail</trd>
  <trdgrp lng="hu">
    <trd>munka</trd>,
    <trd>dolog</trd>,
    <trd>m&udblac;k&ouml;d&eacute;s</trd>
  </trdgrp>  
  <trdgrp lng="nl">
    <trd>werk</trd>,
    <trd>arbeid</trd>
  </trdgrp>
  <trd lng="pt">trabalho</trd>
  <trd lng="ru">&c_r;&c_a;&c_b;&c_o;&c_t;&c_a;</trd>
  <trd lng="vo">v&ouml;lig</trd>
</drv>

<drv mrk="labor.0o.ILO">
  <kap>Internacia <tld lit="L"/>-Organiza&jcirc;o</kap>
  <mlg>ILO</mlg>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>ManPol</bib></fnt>
  <snc>
    <dif>
      Speciala <ref tip="super" cel="organi2.0ajxo">organiza&jcirc;o</ref> de
      <ref tip="malprt" cel="naci.Unuigxintaj_0oj">Unui&gcirc;intaj Nacioj</ref>,
      kiu celas akceli la socialan justecon per plibonigoj de la viv- kaj
      <tld/>-kondi&ccirc;oj:
      <ekz>
        spertuloj de la Internacia <tld lit="L"/>-Organizajo (ILO)
        asertas, ke la nova jarmilo komenci&gcirc;is sen solvo por unu el la
        plej gravaj problemoj de la anta&ubreve;a: la sen&ccirc;esa kresko de
        senlaboreco.
        <fnt><aut>Evgeni GEORGIEV</aut>:
        <vrk><url ref="&Monato;/007398.html">Komputika teknologio helpos al la
        senlaboruloj</url></vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 05/2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_M;&c_ib;&c_zh;&c_n;&c_a;&c_r;&c_o;&c_d;&c_n;&c_a;&c_ja;
    &c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ja;
    <ind>&c_p;&c_r;&c_a;&c_c;&c_y;</ind></trd>,
    <trd>&c_M;&c_A;&c_P;</trd>
  </trdgrp>
  <trd lng="br">Aozadur Etrebroadel al <ind>Labour</ind></trd>
  <trdgrp lng="de">
    <trd>Internationale <ind>Arbeitsorganisation</ind></trd>,
    <trd>IAO</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>International <ind>Labour</ind> Organisation</trd>,
    <trd>ILO</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>Organizaci&oacute;n Internacional del <ind>Trabajo</ind></trd>,
    <trd>OIT</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Organisation Internationale du <ind>Travail</ind></trd>,
    <trd>OIT</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>Nemzetk&ouml;zi <ind>Munka&uuml;gyi Szervezet</ind></trd>,
    <trd>ILO</trd>
  </trdgrp>  
  <trdgrp lng="nl">
    <trd>Internationale <ind>Arbeidsorganisatie</ind></trd>,
    <trd>IAO</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>Organiza&ccedil;&atilde;o Internacional do <ind>Trabalho</ind></trd>,
    <trd>OIT</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_M;&c_je;&c_zh;&c_d;&c_u;&c_n;&c_a;&c_r;&c_o;&c_d;&c_n;&c_a;&c_ja;
    &c_O;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;
    <ind>&c_T;&c_r;&c_u;&c_d;&c_a;</ind></trd>,
    <trd>&c_M;&c_O;&c_T;</trd>
  </trdgrp>
</drv>

