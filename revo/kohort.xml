<art mrk="$Id$">
<kap>
  <rad>kohort</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kohort.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Dekono de la romana legio.
    </dif>
    <trd lng="de">Kohorte</trd>
    <trd lng="en">cohort</trd>
  </snc>
  <snc>
    <dif>
      Multnombra aro da homoj:
      <ekz>
        kontra&ubreve; tuta <tld/>o e&ccirc; Herkulo estas
        malforta<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Haufe</trd>
    <trd lng="en">cohort</trd>
  </snc>
  <trdgrp lng="nl">
    <trd>cohort</trd>,
    <trd>cohorte</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_g;&c_o;&c_r;&c_t;&c_a;</trd>
</drv>
</art>
