<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kohort.xml,v 1.23 2023/12/03 09:58:51 revo Exp $">
<kap>
  <rad>kohort</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kohort.0o">
  <kap><tld/>o</kap>
  <snc mrk="kohort.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Dekono de romia
      <ref tip="malprt" cel="legi.0o">legio</ref>:
      <ekz>
        la <tld/>o kaj la milestro kaj la oficistoj de la Judoj
        kaptis do Jesuon kaj ligis lin
        <fnt><bib>NT</bib><lok>&Joh; 18:12</lok></fnt>.
      </ekz>
      <ekz>
        en la posto de la palaco, kie, veninte al <nom>Jer&scirc;alaim</nom> kun la prokuratoro,
        kvartiris la Unua <tld/>o de la XII Fulma legio
        <fnt>
          <bib>MkM</bib>
          <lok>2</lok>
        </fnt>;
      </ekz>
      <ekz>
        kontra&ubreve; tuta <tld/>o e&ccirc; Herkulo estas malforta
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_g;&c_o;&c_r;&c_t;&c_a;</trd>
    <trd lng="bg">&c_k;&c_o;&c_h;&c_o;&c_r;&c_t;&c_a;</trd>
    <trd lng="de">Kohorte</trd>
    <trd lng="en">cohort</trd>
    <trd lng="fr">cohorte<klr tip="ind"> (romaine)</klr></trd>
    <trd lng="it">cohorte</trd>
    <trd lng="zh">&#x961F;&#x5217; <pr>du&#xEC;li&#xE8;</pr></trd>
</snc>
  <snc mrk="kohort.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Multnombra <ref tip="super" cel="ar.0o">aro</ref> da kunbatalantoj, subtenantoj, samideanoj:
      <ekz>
        kaj tiujn <klr>[reformistojn]</klr> sekvis kaj sekvas
        la <tld/>o de la sen&ccirc;apeligistoj
        <fnt>
          <bib>Voja&gcirc;o</bib>
          <aut>G. Waringhien</aut>
          <vrk>Kaj tamen ni &gcirc;in atingis!</vrk>
          <lok>leciono 26a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="band.0o.samideanoj">bando<sncref/></ref>,
      <ref cel="rot.0o.aro_de_kunirantoj">roto<sncref/></ref>
    </refgrp>
    <trd lng="be">&c_k;&c_a;&c_g;&c_o;&c_r;&c_t;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Haufen <klr>(Menschen)</klr></trd>,
      <trd>Schar <klr>(Menschen)</klr></trd>
    </trdgrp>
    <trd lng="en">cohort</trd>
    <trdgrp lng="fr">
      <trd>cohorte<klr tip="ind"> (fig.)</klr></trd>,
      <trd>cort&egrave;ge<klr tip="ind"> (fig.)</klr></trd>,
      <trd>foule</trd>,
      <trd>troupe</trd>
    </trdgrp>
    <trd lng="zh">&#x7FA4;&#x7D44; <pr>q&#xFA;nz&#x1D4;</pr></trd>
</snc>
  <trd lng="ca">cohort</trd>
  <trdgrp lng="cs">
    <trd>kohortace</trd>,
    <trd>napomenut&iacute;</trd>,
    <trd>pok&aacute;r&aacute;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6B69;&#x5175;&#x968A;  <pr>&#x307B;&#x3078;&#x3044;&#x305F;&#x3044;</pr></trd>,
    <trd>&#x4E00;&#x56E3;  <pr>&#x3044;&#x3061;&#x3060;&#x3093;</pr></trd>,
    <trd>&#x4E00;&#x7FA4;  <pr>&#x3044;&#x3061;&#x3050;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>cohort</trd>,
    <trd>cohorte</trd>
  </trdgrp>
  <trd lng="pl">kohorta</trd>     
  <trd lng="ro">cohort&abreve;</trd>
  <trd lng="ru">&c_k;&c_o;&c_g;&c_o;&c_r;&c_t;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>kohort&aacute;cia</trd>,
    <trd>napomenutie</trd>,
    <trd>pokarhanie</trd>
  </trdgrp>
  <trd lng="uk">&c_k;&c_o;&c_g;&c_o;&c_r;&c_t;&c_a;</trd>
</drv>
</art>
