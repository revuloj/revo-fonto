<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ensemb.xml,v 1.19 2023/08/31 11:49:29 revo Exp $">
<kap>
  <rad>ensembl</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="ensemb.0o">
  <kap><tld/>o</kap>
  <snc mrk="ensemb.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="grup.0o.KOMUNE">Grupo</ref> de kunakordaj,
      harmoniaj aferoj:
    </dif>
    <subsnc mrk="ensemb.0o.MUZ.TEA">
      <uzo tip="fak">MUZ</uzo>
      <uzo tip="fak">TEA</uzo>
      <dif>
        <ref tip="super" cel="grup.0o.personoj">Grupo</ref> de
        muzikistoj
        a&ubreve; aktoroj kune ludantaj:
        <ekz>
          mondfama urba balet<tld/>o
          <fnt>
          <aut>J. Horvath</aut>,
          <vrk>Gy&odblac;r a&ubreve; kie ekkokerikis ventokoko</vrk>,
          <bib>Monato</bib>,
          <lok>2002:8, p. 22a</lok>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="be">
        <trd>&c_g;&c_u;&c_r;&c_t;</trd>,
        <trd>&c_a;&c_n;&c_s;&c_a;&c_m;&c_b;&c_l;&c_mol;</trd>,
        <trd>&c_t;&c_r;&c_u;&c_p;&c_a;</trd>
      </trdgrp>
      <trdgrp lng="bg">
        <trd>&c_a;&c_n;&c_s;&c_a;&c_m;&c_b;&c_malmol;&c_l;</trd>,
        <trd>&c_t;&c_r;&c_u;&c_p;&c_a;</trd>
      </trdgrp>
      <trd lng="de">Theatertruppe</trd>
      <trd lng="es">ensamble</trd>
      <trdgrp lng="pl">
        <trd>zesp&oacute;&lstroke;</trd>,
        <trd>ensambl</trd>,
        <trd>trupa</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="ensemb.0o.ARKI">
      <uzo tip="fak">ARKI</uzo>
      <dif>
        Aro da <ref tip="prt" cel="konstr.0ajxo.domo">konstrua&jcirc;oj</ref> kunfunkciaj, en
        akorda stilo:
        <ekz>
          <nom>Rocamadour</nom>, unu el la plej gravaj pilgrimlokoj dum la mezepoko
          ankora&ubreve; havas arkitekturan <tld/>on eksterordinaran
          <fnt><bib>Viki</bib> <lok>Lot,
          2005-09-10</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="pl">zesp&oacute;&lstroke;<klr> (np.
      pa&lstroke;acowy)</klr></trd>
    </subsnc>
    <subsnc mrk="ensemb.0o.BELA">
      <uzo tip="fak">BELA</uzo>
      <dif>
        Tuto de la dekoracio, de la mebloj kaj de la ornama&jcirc;oj
        de unu &ccirc;ambro, harmonie elektitaj kaj aran&gcirc;itaj.
      </dif>
      <refgrp tip="vid">
        <ref cel="mebl.0aro">meblaro</ref>
      </refgrp>
      <trd lng="be">&c_a;&c_n;&c_s;&c_a;&c_m;&c_b;&c_l;&c_mol;</trd>
      <trdgrp lng="de">
        <trd>Ganzes</trd>,
        <trd>Gesamtheit</trd>
      </trdgrp>
      <trd lng="pl">wystr&oacute;j</trd>
    </subsnc>
    <subsnc mrk="ensemb.0o.TEKS">
      <uzo tip="fak">TEKS</uzo>
      <dif>
        <ref tip="dif" cel="komple2.0o.TEKS">Kompleto<sncref/></ref>:
        <ekz>
          <nom>Claude</nom> portis du grandajn orajn krucojn sur la refaldoj de sia <tld/>o
          <fnt>
          <aut>T. Steele</aut>
          <vrk>Por savi nigran animon</vrk>
          <lok>en: Australia Felix, 1999</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_s;&c_t;&c_r;&c_o;&c_j;</trd>
      <trd lng="de">Zusammenstellung</trd>
      <trdgrp lng="pl">
        <trd>komplet</trd>,
        <trd>zestaw</trd>
      </trdgrp>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="garnit.0o.KOMUNE">garnituro</ref>,
      <ref cel="komple2.0o.KOMUNE">kompleto</ref>,
      <ref cel="servic.0o.KUI">servico</ref>,
      <ref cel="sortim.0o.KOMUNE">sortimento</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>sbor</trd>,
    <trd>soubor <klr>(nap&rcaron;. divadeln&iacute;)</klr></trd>
  </trdgrp>
  <trd lng="de">Ensemble</trd>
  <trd lng="en">ensemble</trd>
  <trd lng="fr">ensemble<klr tip="ind"> (musique, archi.,
  v&ecirc;tement, troupe  d'artistes)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x3072;&#x3068;&#x305D;&#x308D;&#x3044;</trd>,
    <trd>&#x30A2;&#x30F3;&#x30B5;&#x30F3;&#x30D6;&#x30EB;</trd>,
    <trd>&#x5C0F;&#x697D;&#x56E3;  [<ind>&#x3057;&#x3087;&#x3046;&#x304C;&#x304F;&#x3060;&#x3093;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">ensemble</trd>
  <trdgrp lng="pt">
    <trd>conjunto</trd>,
    <trd>ensemble</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_n;&c_s;&c_a;&c_m;&c_b;&c_l;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>s&uacute;bor</trd>,
    <trd>zbor</trd>,
    <trd>zostava</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&c_n;&c_s;&c_a;&c_m;&c_b;&c_l;&c_mol;</trd>,
    <trd>&c_k;&c_o;&c_m;&c_p;&c_l;&c_je;&c_k;&c_t;</trd>,
    <trd>&c_n;&c_a;&c_b;&c_ib;&c_r;</trd>
  </trdgrp>
</drv>
</art>
