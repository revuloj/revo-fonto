<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>ensembl</rad>/o
</kap>
<drv mrk="ensemb.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <uzo tip="fak">MUZ</uzo>
    <uzo tip="fak">TEA</uzo>
    <dif>
      Trupo da muzikistoj a&ubreve; aktoroj kune ludantaj.
    </dif>
    <trd lng="es">ensamble</trd>
  </snc>
  <snc num="2">
    <dif>
      Tuto de la dekoracio, de la mebloj kaj de la ornama&jcirc;oj
      de unu &ccirc;ambro, harmonie elektitaj kaj aran&gcirc;itaj.
    </dif>
  </snc>
  <snc num="3">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Tuto de la diversaj pecoj de kostumo, harmonie alfaritaj.
    </dif>
  </snc>
  <trd lng="fr">ensemble<klr tip="ind"> (harmonie, v&ecirc;tement, troupe d'artistes)</klr></trd>
  <trd lng="nl">ensemble</trd>
  <trd lng="ru">&c_a;&c_n;&c_s;&c_a;&c_m;&c_b;&c_l;&c_mol;</trd>
</drv>
</art>

