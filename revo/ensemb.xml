<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ensembl</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="ensemb.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="grup.0o.KOMUNE">Grupo</ref> de kunakordaj,
      harmoniaj aferoj:
    </dif>
    <subsnc mrk="ensemb.0o.MUZ.TEA">
      <uzo tip="fak">MUZ</uzo>
      <uzo tip="fak">TEA</uzo>
      <dif>
        <ref tip="super" cel="grup.0o.personoj">Grupo</ref> de muzikistoj
        a&ubreve; aktoroj kune ludantaj:
        <ekz>
          mondfama urba balet<tld/>o
          <fnt>
          <aut>Jozefo Horvath</aut>,
          <vrk>Gy&odblac;r a&ubreve; kie ekkokerikis ventokoko</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2002a, numero 8a, p. 22a</lok>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="be">
        <trd>&c_g;&c_u;&c_r;&c_t;</trd>,
        <trd>&c_a;&c_n;&c_s;&c_a;&c_m;&c_b;&c_l;&c_mol;</trd>,
        <trd>&c_t;&c_r;&c_u;&c_p;&c_a;</trd>
      </trdgrp>
      <trd lng="de">Theatertruppe</trd>
      <trd lng="es">ensamble</trd>
    </subsnc>
    <subsnc mrk="ensemb.0o.ARKI">
      <uzo tip="fak">ARKI</uzo>
      <dif>
        Aro da <ref tip="prt" cel="konstr.0ajxo.domo">konstrua&jcirc;oj</ref> kunfunkciaj, en akorda stilo:
        <ekz>
          Rocamadour, unu el la plej gravaj pilgrimlokoj dum la mezepoko
          ankora&ubreve; havas arkitekturan <tld/>on eksterordinaran (pre&gcirc;ejoj,
          kapeloj, kastelo, mezepokaj domoj)
          <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;Lot&rightquot;, 2005-09-10</lok></fnt>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="ensemb.0o.BELA">
      <uzo tip="fak">BELA</uzo>
      <dif>
        Tuto de la dekoracio, de la mebloj kaj de la ornama&jcirc;oj
        de unu &ccirc;ambro, harmonie elektitaj kaj aran&gcirc;itaj.
      </dif>
      <refgrp tip="vid">
        <ref cel="mebl.0aro">meblaro</ref>
      </refgrp>
      <trd lng="be">&c_a;&c_n;&c_s;&c_a;&c_m;&c_b;&c_l;&c_mol;</trd>
      <trdgrp lng="de">
        <trd>Ganzes</trd>,
        <trd>Gesamtheit</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="ensemb.0o.TEKS">
      <uzo tip="fak">TEKS</uzo>
      <dif>
        Sortimento de harmonie alfaritaj vestoj.
      </dif>
      <refgrp tip="vid">
        <ref cel="kostum.0o">kostumo</ref>
      </refgrp>
      <trd lng="be">&c_s;&c_t;&c_r;&c_o;&c_j;</trd>
      <trd lng="de">Zusammenstellung</trd>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="garnit.0o">garnituro</ref>,
      <ref cel="sortim.0o">sortimento</ref>
    </refgrp>
  </snc>
  <trd lng="de">Ensemble</trd>
  <trd lng="fr">ensemble<klr tip="ind"> (musique, archi., v&ecirc;tement, troupe  d'artistes)</klr></trd>
  <trd lng="nl">ensemble</trd>
  <trdgrp lng="pt">
    <trd>conjunto</trd>,
    <trd>ensemble</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_n;&c_s;&c_a;&c_m;&c_b;&c_l;&c_mol;</trd>
</drv>
</art>
