<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stotink.xml,v 1.8 2022/06/28 14:28:37 revo Exp $">
<kap>
  <rad>stotink</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="stotink.0o">
  <kap><tld/>o</kap>
  <snc mrk="stotink.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="lst" cel="unu.monasub0o" lst="voko:monunuoj">Mona
      subunuo</ref>, egala al
      centono de <ref tip="malprt" cel="lev1.0o.EKON">levo</ref> kaj
      aliaj
      monunuoj; tiomvalora <ref tip="super" cel="mon.0ero">monero</ref>:
      <ekz>
        ni &scirc;paris <tld/>ojn por konjako
        <fnt>
        <aut>Pavel Stan&ccirc;ev, trad. Vedi</aut>,
        <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Noveloj/kolora_knabino.html">La
        kolora knabino</url></vrk>,
        <lok>Paco, 1989-2, p. 8a-9a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_c;&c_ib;&c_n;&c_k;&c_a;</trd>
  <trd lng="cs">stotinka</trd>
  <trd lng="fr">stotink<klr tip="amb"> (monnaie)</klr></trd>
  <trd lng="pl">stotinka</trd>
  <trd lng="pt">stotink</trd>
  <trd lng="sk">stotinka</trd>
  <trd lng="uk">&c_s;&c_t;&#x43E;&c_t;&c_i;&#x43D;&#x43A;&c_a; (&c_r;&#x43E;&c_z;&#x43C;&c_ib;&#x43D;&#x43D;&c_a;
  &#x43C;&#x43E;&#x43D;&c_je;&c_t;&c_a;)</trd>
</drv>
</art>

