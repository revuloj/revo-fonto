  <art mrk="$Id: karol.xml,v 1.13 2023/12/03 08:00:24 revo Exp $">
    <kap>
      <rad>karol</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>

    <drv mrk="karol.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">KRI</uzo>
      <uzo tip="fak">MUZ</uzo>
      <snc>
        <dif>
          Popola <ref tip="super" cel="kant.0ajxo">kanta&jcirc;o</ref> por la
          <ref cel="krist.0nasko">kristnaska festo</ref>:
          <ekz>
            Cris&oacute;stomo faris tre belajn koplojn, e&ccirc; la <tld/>ojn por
            la
            Kristnasko
            <fnt><bib>DonKihxoto</bib>,
              <lok>&ccirc;apitro 12a, p. 88a</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <ref tip="hom" cel="karol1.0o">Karolo</ref>
      <trd lng="be">&c_k;&c_a;&c_l;&c_ja;&c_d;&c_k;&c_a;</trd>
      <trd lng="cs">koleda</trd>
      <trd lng="en">carol</trd>
      <trd lng="de">Weihnachtslied</trd>
      <trd lng="fr">chant de No&euml;l</trd>
      <trd lng="hu">&uuml;nnepk&ouml;sz&ouml;nt&odblac; <ind>&eacute;nek</ind></trd>  
      <trd lng="nl">kerstlied</trd>
      <trd lng="pl">kol&eogonek;da</trd>
      <trd lng="pt">dan&ccedil;a carola</trd>
      <trd lng="ro">colind&abreve;</trd>
      <trdgrp lng="ru">
        <trd>&c_k;&c_o;&c_l;&c_ja;&c_d;&c_a;</trd>,
        <trd>&c_k;&c_o;&c_l;&c_ja;&c_d;&c_k;&c_a;</trd>,
        <trd>&c_h;&c_r;&c_i;&c_s;&c_t;&c_o;&c_s;&c_l;&c_a;&c_v;&c_i;&c_je;</trd>
      </trdgrp>
      <trd lng="sk">koleda</trd>      
  <trdgrp lng="uk">
    <trd>&#x432;&#x435;&#x441;&#x435;&#x43B;&#x430; &#x43E;&#x431;&#x440;&#x44F;&#x434;&#x43E;&#x432;&#x430; &#x43F;&#x456;&#x441;&#x43D;&#x44F;</trd>,
    <trd>&#x445;&#x43E;&#x440;&#x43E;&#x432;&#x43E;&#x434;&#x43D;&#x430;</trd>,
    <trd>&#x437;&#x430;&#x441;&#x442;&#x456;&#x43B;&#x44C;&#x43D;&#x430; &#x43F;&#x456;&#x441;&#x43D;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x980C;&#x6B4C; <pr>s&#xF2;ngg&#x113;</pr></trd>,
    <trd>&#x5723;&#x8BDE;&#x6B4C; <pr>sh&#xE8;ngd&#xE0;ng&#x113;</pr></trd>
  </trdgrp>
</drv>
