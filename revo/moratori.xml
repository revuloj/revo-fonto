<art mrk="$Id: moratori.xml,v 1.11 2024/01/26 19:05:53 revo Exp $">
<kap>
  <rad>moratori</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="moratori.0o">
  <kap><tld/>o</kap>
  <snc mrk="moratori.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Pago<ref tip="super" cel="prokra.0o">prokrasto</ref> konsentita al &scirc;uldanto:
      <ekz>
        necesas ne <tld/>o, sed pura kaj simpla forstreko de la &scirc;uldo
        <fnt>
          <aut>I. Ramonet, trad. V. Lutermano, trad. P. Beaudonnet</aut>,
          <vrk><url ref="&Mondediplo;article969.html">
          Post la cunamo</url></vrk>,
          <lok>le Monde diplomatique, 2005-01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="moratori.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Portempa le&gcirc;a malpermeso a&ubreve; &ccirc;esigo de iu agado:
      <ekz>
        <tld/>o por la konstruado de atomcentraloj
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2005/008577.php">Venta energio superas la atoman</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>George Ryan</nom>, guberniestro de la &scirc;tato <nom>Illinois</nom> <klr>[&#x2026;]</klr> 
        dekretis <tld/>on pri ekzekutoj
        <fnt>
          <aut>R. Picard</aut>,
          <vrk><url ref="http://rozenn.picard.free.fr/timesp.htm">
          Tim, mortkondamnita...</url></vrk>,
          <lok>2000-03</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="interm.0o">intermito</ref>,
       <ref cel="romp.inter0i.cxesigi">interrompi<sncref/></ref>,
       <ref cel="pauxz.0o">pa&ubreve;zo</ref>,
       <ref cel="ultima.0o">ultimato</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_a;&c_t;&c_o;&c_r;&c_y;&c_j;</trd>
  <trd lng="cs">moratorium</trd>
  <trd lng="de">Moratorium</trd>
  <trd lng="fr">moratoire</trd>
  <trdgrp lng="ja">
    <trd>&#x30E2;&#x30E9;&#x30C8;&#x30EA;&#x30A2;&#x30E0;</trd>,
    <trd>&#x652F;&#x6255;&#x7336;&#x4E88;  <pr>&#x3057;&#x306F;&#x3089;&#x3044;&#x3086;&#x3046;&#x3088;</pr></trd>
  </trdgrp>
  <trd lng="pl">moratorium</trd>
  <trd lng="ro">moratoriu</trd>
  <trdgrp lng="sk">
    <trd>morat&oacute;rium</trd>,
    <trd>pr&iacute;ro&ccaron;ie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x43E;&#x440;&#x430;&#x442;&#x43E;&#x440;&#x456;&#x439;</trd>,
    <trd>&#x432;&#x456;&#x434;&#x441;&#x442;&#x440;&#x43E;&#x447;&#x43A;&#x430; &#x43F;&#x43B;&#x430;&#x442;&#x435;&#x436;&#x456;&#x432;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5EF6;&#x671F;&#x507F;&#x8FD8; <pr>y&#xE1;nq&#x12B;ch&#xE1;nghu&#xE1;n</pr></trd>,
    <trd>&#x5EF6;&#x671F;&#x511F;&#x9084; <pr>y&#xE1;nq&#x12B;ch&#xE1;nghu&#xE1;n</pr></trd>
  </trdgrp>
</drv>
