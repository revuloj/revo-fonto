<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>3</ofc>
   <rad>eklips</rad>/o
</kap>
<drv mrk="eklips.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">AST</uzo>
   <snc>
     <dif>
       Neda&ubreve;ra &scirc;ajna malapero de astro:
       <ekz>
         parta, plena <tld/>o de l' suno.
       </ekz>
     </dif>
   </snc>
  <trd
   lng="be">&c_z;&c_a;&c_c;&c_mol;&c_m;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="ca">eclipsi</trd>
  <trd lng="cs">zatmění</trd>
  <trdgrp lng="de">
     <trd>Finsternis</trd>,
     <trd>Verfinsterung</trd>,
     <trd>Verdunkelung</trd>,
     <trd>Verdeckung</trd>
   </trdgrp>
  <trd lng="en">eclipse</trd>
  <trd lng="es">eclipse</trd>
  <trd lng="fa">&f_g;&a_r;&a_f;&a_t;</trd>
  <trd lng="fr">&eacute;clipse</trd>
  <trdgrp lng="hu">
     <trd>els&ouml;t&eacute;t&uuml;l&eacute;s</trd>,
     <trd>fogyatkoz&aacute;s</trd>,
     <trd>eklipszis</trd>
   </trdgrp>
  <trd lng="id">gerhana</trd>
  <trd lng="pl">za&cacute;mienie</trd>
  <trd lng="pt">eclipse</trd>
  <trd lng="ru">&c_z;&c_a;&c_t;&c_m;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">zatmenie</trd>
  <trd lng="sv">f&ouml;rm&ouml;rkelse</trd>
</drv>

<drv mrk="eklips.0a">
   <kap><tld/>a</kap>
   <snc>
     <dif>
       Rilata al <tld/>o.
     </dif>
     <trd
     lng="be">&c_z;&c_a;&c_c;&c_mol;&c_m;&c_je;&c_n;&c_je;&c_v;&c_y;</trd>
     <trd lng="ca">ecl&iacute;ptic</trd>
     <trd lng="pl">za&cacute;mieniowy</trd>
     <trd lng="ru">&c_z;&c_a;&c_t;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
   </snc>
   <snc>
     <dif>
       Prezentanta serion da mallongaj lumbriloj:
       <ekz>
         <uzo tip="fak">MAR</uzo>
         <tld/>a lumturo.
       </ekz>
     </dif>
     <trd
     lng="be">&c_p;&c_r;&c_o;&c_b;&c_l;&c_ib;&c_s;&c_k;&c_a;&c_v;&c_y;</trd>
     <trd lng="ca">intermitent</trd>
     <trd lng="pl">b&lstroke;yskowy</trd>
     <trd
     lng="ru">&c_p;&c_r;&c_o;&c_b;&c_l;&c_je;&c_s;&c_k;&c_o;&c_v;&c_y;&c_j;</trd>
     <trd lng="fr">&eacute;cliptique</trd>
   </snc>
</drv>

<drv mrk="eklips.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc mrk="eklips.0i.sxirmi">
     <uzo tip="fak">AST</uzo>
     <dif>
       Neda&ubreve;re ka&scirc;i
       <ref tip="super" cel="sxirm.0i.kovri">&scirc;irmante</ref>
       la vidon:
       <ekz>
         la Luno <tld/>is la Sunon.
       </ekz>
     </dif>
     <trd lng="de">verfinstern</trd>
     <trdgrp lng="hu">
       <trd>eltakar</trd>,
       <trd>&aacute;rny&eacute;kba bor&iacute;t</trd>
     </trdgrp>
     <trdgrp lng="pl">
       <trd>za&cacute;miewa&cacute;</trd>,
       <trd>przes&lstroke;ania&cacute;</trd>
     </trdgrp>
   </snc>
   <snc mrk="eklips.0i.FIG">
     <uzo tip="stl">FIG</uzo>
     <dif>
       &Scirc;ajnigi malpli rimarkinda la superecon de iu pro
       sia propra supereco:
       <ekz>
         li <tld/>as &ccirc;iujn konkurantojn.
       </ekz>
     </dif>
     <trd lng="de">in den <ind>Schatten stellen</ind></trd>
     <trd lng="en">surpass</trd>
     <trd lng="hu">a <ind>h&aacute;tt&eacute;rbe
     szor&iacute;t</ind></trd>
     <trdgrp lng="pl">
       <trd>za&cacute;miewa&cacute;</trd>,
       <trd>przy&cacute;miewa&cacute;</trd>
     </trdgrp>
   </snc>
  <trd lng="be">&c_z;&c_a;&c_c;&c_mol;&c_m;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="ca">eclipsar</trd>
  <trdgrp lng="cs">
    <trd>zatemnit</trd>,
    <trd>zatemňovat</trd>
  </trdgrp>
  <trd lng="en">eclipse</trd>
  <trd lng="es">eclipsar</trd>
  <trd lng="fr">&eacute;clipser</trd>
  <trdgrp lng="ru">
     <trd>&c_z;&c_a;&c_t;&c_m;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_z;&c_a;&c_t;&c_m;&c_i;&c_t;&c_mol;</trd>
   </trdgrp>
  <trd lng="sk">zatemniť</trd>
</drv>
<adm>
   ~o: &ccirc;u ne ekzistas figura senco (&scirc;ia populareco suferis 
~ojn)?
   ~i FIG: &ccirc;u io ne povas ~i ion?
   <aut>MB</aut>
</adm>
</art>
