<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>elast</rad>/a
</kap>
<drv mrk="elast.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="elast.0a.FIZ">
    <dif>
      Facile kuntiri&gcirc;anta, etendi&gcirc;anta a&ubreve; aliformi&gcirc;anta pro prema a&ubreve; tira
      forto, kaj reprenanta sian anta&ubreve;an formon, kiam tiu forto &ccirc;esas:
      <ekz>
        la gasoj estas tre <tld/>aj;
      </ekz>
      <ekz>
        <tld/>a vergo, &scirc;elko, risorto;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>aj pa&scirc;oj<fnt>B</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kauxcx.0o">ka&ubreve;&ccirc;uko</ref>,
      <ref cel="risort.0o">risorto</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>elastisch</trd>,
      <trd>dehnbar</trd>,
      <trd>verformbar</trd>,
      <trd>federnd</trd>,
      <trd>geschmeidig</trd>
    </trdgrp>
  </snc>
  <snc mrk="elast.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Facile fleksebla kaj adaptebla al la cirkonstancoj:
      <ekz>
        la signifo de la afiksoj devas resti <tld/>a;
      </ekz>
      <ekz>
        la lingvo iom post iom pli<tld/>i&gcirc;as<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>dehnbar</trd>,
      <trd>anpassbar</trd>
    </trdgrp>
  </snc>
  <refgrp tip="vid">
    <ref cel="adapt">adapti&gcirc;i</ref>,
    <ref cel="fleks.0ebla">fleksebla</ref>,
    <ref cel="rigid.mal0a">malrigida</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_e;&c_l;&c_ja;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_g;&c_n;&c_u;&c_t;&c_k;&c_ib;</trd>,
    <trd>&c_p;&c_r;&c_u;&c_g;&c_k;&c_ib;</trd>,
    <trd>&c_p;&c_r;&c_u;&c_t;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;lastique</trd>,
    <trd>compressible</trd>,
    <trd>souple</trd>
  </trdgrp>
  <trd lng="es">el&aacute;stico</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_l;&c_a;&c_s;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_g;&c_i;&c_b;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_u;&c_p;&c_r;&c_u;&c_g;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
