  </trdgrp>
  <trd lng="sv">oskyldig</trd>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_je;&c_v;&c_i;&#x43D;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_v;&c_i;&#x43D;&#x43D;&c_i;&c_j;</trd>,
