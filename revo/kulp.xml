<drv mrk="kulp.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="kulp.sen0a.JUR">
    <dif>
      Ne <tld/>a:
      <ekz>
        mi tamen kredas, ke li estas sen<tld/>a
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        kovri sen<tld/>ajn persekutatojn
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        ver&scirc;i sen<tld/>an sangon
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 59:7</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="kulp.sen0a.senpeka">
    <dif>
      Sen &ccirc;ia <tld/>o, neniam <tld/>inta, morale sendifekta:
      <ekz>
        io ekpikis la koron de Doroteo, kvaza&ubreve; la rigardo de la sen<tld/>a infano
        trabruligus lin
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiu sen<tld/>a &gcirc;ojo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>!
      </ekz>
      <ekz>
        &scirc;i per sia sen<tld/>a kaj tu&scirc;anta vo&ccirc;o ekkriis
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        sen<tld/>a amo perdas sian rozon
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i estis afero tute sen<tld/>a, &ccirc;ar &gcirc;i prezentis nur konsilon
        <fnt>
          <bib>LR</bib>
          <lok>14. Gramatikoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="naiv.0a">naiva</ref>,
      <ref cel="nobl.0a.iu">nobla<sncref/></ref>,
      <ref cel="pek.sen0a">senpeka</ref>
    </refgrp>    
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_v;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_t;&c_y;</trd>,
    <trd>&c_b;&c_ja;&c_z;&c_mol;&c_v;&c_ib;&c_n;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">innocent</trd>
  <trdgrp lng="cs">
    <trd>benign&iacute;</trd>,
    <trd>nevinn&yacute;</trd>,
    <trd>ne&scaron;kodn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>unschuldig</trd>,
    <trd>frei von <ind>Schuld</ind></trd>
  </trdgrp>
  <trd lng="en">innocent</trd>
  <trd lng="fr">innocent<klr> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>v&eacute;tlen</trd>,
    <trd>&aacute;rtatlan</trd>
  </trdgrp>
  <trd lng="nl">onschuldig</trd>
  <trd lng="pl">niewinny</trd>     
  <trd lng="ro">nevinovat</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_v;&c_i;&c_n;&c_o;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_v;&c_i;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
