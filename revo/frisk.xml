<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>frisk</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="frisk.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="frisk.0a.KOMUNE">
    <ref tip="dif" cel="varm.mal0eta.KOMUNE">malvarmeta</ref>
      <ekz>
        <tld/>a posttagmezo de Aprilo
        <fnt>
          <aut>trad. J. Sarasua</aut>,
          <vrk><url ref="http://filmoj.net/la-pedalauxto">
          La Pedala&ubreve;to</url></vrk>,
          <lok>2009</lok>
        </fnt>.
      </ekz>
  </snc>
  <trdgrp lng="cs">
    <trd>osvěžující</trd>,
    <trd>příjem.chladivý</trd>,
    <trd>studený</trd>,
    <trd>vlažný</trd>
  </trdgrp>
  <trd lng="fr">frisquet</trd>
  <trdgrp lng="sk">
    <trd>chladivý</trd>,
    <trd>osviežujúci</trd>
  </trdgrp>
</drv>
</art>

