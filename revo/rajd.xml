  <trd lng="et">ride</trd>
  <trdgrp lng="fr">
    <trd>aller<klr tip="ind"> (&agrave; dos d'animal, &agrave;
    v&eacute;lo...)</klr></trd>,
    <trd>chevaucher</trd>,
    <trd>monter<klr tip="ind"> (un animal, un v&eacute;lo...)</klr></trd>
