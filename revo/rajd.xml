<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rajd</rad>/i
</kap>
<drv mrk="rajd.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Iri sidante sur dorso de besto:
      <ekz>
        <tld/>i &ccirc;evalon<fnt>B</fnt>,
      </ekz>
      <ekz>
        sur &ccirc;evalo, kamelo;
      </ekz>
      <ekz>
        <ind><tld/>anto</ind>;
        <trd lng="fr">cavalier<klr tip="ind"> (subst.)</klr></trd>
      </ekz>
      <ekz>
        <ind><tld/>arto</ind>;
        <trd lng="fr">&eacute;quitation</trd>
        <trd lng="hu">lovagl&oacute;m&udblac;v&eacute;szet</trd>
        <trdgrp lng="ru">
          <trd>&c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_o; &c_v;&c_je;&c_r;&c_h;&c_o;&c_v;&c_o;&c_j; &c_je;&c_z;&c_d;&c_y;</trd>,
          <trd>&c_v;&c_y;&c_je;&c_z;&c_d;&c_k;&c_a;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        malfeli&ccirc;o venas <ind><tld/>e</ind> <klr>(rapide)</klr>,
        foriras piede <klr>(malrapide)</klr>
        <trd lng="be">&c_k;&c_o;&c_n;&c_n;&c_a;</trd>
        <trd lng="fr">&agrave; <ind>cheval</ind></trd>
        <trdgrp lng="hu">
          <trd>lovon</trd>,
          <trd>lovagolva</trd>
        </trdgrp>
        <trd lng="ru">&c_v;&c_je;&c_r;&c_h;&c_o;&c_m;</trd>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Veturi per speciala ilo, sidante kvaza&ubreve; sur &ccirc;evalo:
      <ekz>
        <tld/>i sur, per bicikleto;
      </ekz>
      <ekz>
        sor&ccirc;istino <tld/>anta sur balailo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_je;&c_h;&c_a;&c_c;&c_mol; <ind>&c_k;&c_o;&c_n;&c_n;&c_a;</ind></trd>
  <trd lng="de">reiten</trd>
  <trd lng="en">ride</trd>
  <trdgrp lng="fr">
    <trd>aller<klr tip="ind"> (&agrave; dos d'animal, &agrave; v&eacute;lo...)</klr></trd>,
    <trd>chevaucher</trd>,
    <trd>monter<klr tip="ind"> (un animal, un v&eacute;lo...)</klr></trd>
  </trdgrp>
  <trd lng="hu">lovagol</trd>  
  <trd lng="nl">rijden <klr tip="ind">(op een dier)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_je;&c_h;&c_a;&c_t;&c_mol; <ind>&c_v;&c_je;&c_r;&c_h;&c_o;&c_m;</ind></trd>,
    <trd>&c_je;&c_z;&c_d;&c_i;&c_t;&c_mol; <ind>&c_v;&c_je;&c_r;&c_h;&c_o;&c_m;</ind></trd>
  </trdgrp>
  <trd lng="sv">rida</trd>
</drv>

<drv mrk="rajd.0antaro">
  <kap><tld/>antaro</kap>
  <snc>
    <dif>
      Grupo da <tld/>antoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="kavale.0o">kavalerio</ref>,
      <ref cel="kavalk.0o">kavalkado</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_k;&c_a;&c_d;&c_a;</trd>
  <trdgrp lng="fr">
    <trd>groupe de <ind>cavaliers</ind></trd>,
    <trd>troupe<klr tip="ind"> (de cavaliers)</klr></trd>
  </trdgrp>
  <trd lng="hu">lovascsapat</trd>  
  <trd lng="ru">&c_k;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_k;&c_a;&c_d;&c_a;</trd>
</drv>

<drv mrk="rajd.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Ccirc;irka&ubreve;iri <tld/>ante,
      <tld/>i &ccirc;irka&ubreve;:
      <ekz>
        el la fonto fari&gcirc;is ne fluanta rivereto, sed staranta
        mar&ccirc;o, kiun oni anka&ubreve; devis &ccirc;irka&ubreve;<tld/>i
        <fnt><bib>Fab4</bib><lok>Filino de la mar&ccirc;a re&gcirc;o</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="hu">k&ouml;rbelovagol</trd>  
</drv>

<drv mrk="rajd.post0i">
  <kap>post<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref cel="persek.0i.postkuri">Persekuti</ref> <tld/>ante:
      <ekz>
        galopante <klr>[Midaso]</klr> post<tld/>is Dionizon kaj
        rimarkis plena de hororo, ke anka&ubreve; la brido en sia mano
        &scirc;an&gcirc;i&gcirc;is en oron
        <fnt>
        <aut>E. Peti&scaron;ka trad. Cezar</aut>
        <vrk>Midaso</vrk>
        <lok><url ref="http://www.marktplatz-deutschland.de/rsabg/pic2/sat-kulturo/literaturo/midaso.html">http://www.marktplatz-deutschland.de/rsabg/pic2/sat-kulturo/literaturo/midaso.html</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      La la&ubreve;dire biblia ekzemplo citita en PIV &scirc;ajnas
      ne ekzisti.
    </rim>
  </snc>
  <trd lng="fr">pourchasser<klr tip="ind"> (&agrave; cheval)</klr></trd>
  <trd lng="hu">ut&aacute;nalovagol</trd>  
</drv>
</art>
