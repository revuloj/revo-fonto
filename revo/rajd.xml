<drv mrk="rajd.0e">
  <kap><tld/>e</kap>
  <snc mrk="rajd.0e.RAJ">
    <dif>
      Veturante a&ubreve; sintenante kiel <tld/>anto: side, kun flanke pendaj
      gamboj:
      <ekz>
        li sidi&gcirc;is <tld/>e sur la kapro, frapis &gcirc;iajn flankojn per siaj kalkanoj kaj
        forgalopis
        <fnt>
          <bib>Fab3</bib>
          <lok>Johan&ccirc;jo-malsa&gcirc;ulo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi tuj sidi&gcirc;is <tld/>e sur la sledeto kaj lasis &gcirc;in gliti
        malsupren
        <fnt><bib>Kiso</bib>, <lok>la punkto de Arkimedo</lok></fnt>;
      </ekz>
      <ekz>
        malfeli&ccirc;o venas <tld/>e, foriras piede
        <klr>(malfeli&ccirc;o aperas rapide kaj fori&gcirc;as malrapide)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_n;&c_n;&c_a;</trd>
  <trd lng="cs">na zv&iacute;&rcaron;eti <klr>(j&iacute;zdou)</klr></trd>
  <trd lng="de">reitend</trd>
  <trdgrp lng="en">
    <trd>straddlingly</trd>,
    <trd>straddlewise</trd>,
    <trd>astride</trd>
  </trdgrp>
  <trd lng="fr">&agrave; <ind>cheval</ind></trd>
  <trdgrp lng="hu">
    <trd>lovon</trd>,
    <trd>lovagolva</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>dengan menunggang</trd>,
    <trd>dengan menunggangi</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x99AC;&#x306B;&#x4E57;&#x3063;&#x3066;  <pr>&#x3046;&#x307E;&#x306B;&#x306E;&#x3063;&#x3066;</pr></trd>,
    <trd>&#x99AC;&#x4E57;&#x308A;&#x306B;  <pr>&#x3046;&#x307E;&#x306E;&#x308A;&#x306B;</pr></trd>
  </trdgrp>
  <trd lng="nl">te <ind>paard</ind><klr> (of op een ander
  dier)</klr></trd>
  <trdgrp lng="pl">
    <trd>konno</trd>,
    <trd>okrakiem</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_je;&c_r;&c_h;&c_o;&c_m;</trd>
  <trd lng="sk">na zviera&tcaron; <klr>(jazdou)</klr></trd>
  <trd lng="uk">&c_v;&c_je;&c_r;&c_h;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x99AC;&#x80CC; <pr>m&#x1CE;b&egrave;i</pr></trd>,
    <trd>&#x9A6C;&#x80CC; <pr>m&#x1CE;b&egrave;i</pr></trd>,
    <trd>&#x8DE8;&#x7740; <pr>ku&agrave;zhu&oacute;</pr></trd>,
    <trd>&#x8DE8;&#x8457; <pr>ku&agrave;zhu&oacute;</pr></trd>
  </trdgrp>
</drv>

