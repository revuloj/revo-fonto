      <ekz>
        kiu kura&gcirc;as <tld/>i sur leono
        <fnt><bib>F</bib>, <lok>&FE; 7</lok></fnt>?
      </ekz>
      <ekz>
        <ind><tld/>arto</ind>;
        <trd lng="fr">&eacute;quitation</trd>
        <trd lng="hu">lovagl&oacute;m&udblac;v&eacute;szet</trd>
        <trd lng="pt">equita&ccedil;&atilde;o</trd>
