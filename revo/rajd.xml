<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rajd</rad>/i
</kap>
<drv mrk="rajd.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Iri sidante sur dorso de besto:
      <ekz>
        <tld/>i &ccirc;evalon<fnt>B</fnt>,
      </ekz>
      <ekz>
        sur &ccirc;evalo, kamelo;
      </ekz>
      <ekz>
        <ind><tld/>arto</ind>;
        <trdgrp lng="ru">
          <trd>&c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_o; &c_v;&c_je;&c_r;&c_h;&c_o;&c_v;&c_o;&c_j; &c_je;&c_z;&c_d;&c_y;</trd>,
          <trd>&c_v;&c_y;&c_je;&c_z;&c_d;&c_k;&c_a;</trd>
        </trdgrp>
      </ekz>
      <ekz><uzo tip="stl">FIG</uzo>
        malfeli&ccirc;o venas <ind><tld/>e</ind> <klr>(rapide)</klr>,
        foriras piede <klr>(malrapide)</klr>.
        <trd lng="ru">&c_v;&c_je;&c_r;&c_h;&c_o;&c_m;</trd>
<fnt><bib>PrV</bib></fnt></ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Veturi per speciala ilo sidante kvaza&ubreve; sur &ccirc;evalo:
      <ekz>
        <tld/>i sur, per bicikleto;
      </ekz>
      <ekz>
        sor&ccirc;istino <tld/>anta sur balailo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">reiten</trd>
  <trd lng="en">ride</trd>
  <trd lng="nl">rijden <klr tip="ind">(op een dier)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_je;&c_h;&c_a;&c_t;&c_mol; <ind>&c_v;&c_je;&c_r;&c_h;&c_o;&c_m;</ind></trd>,
      <trd>&c_je;&c_z;&c_d;&c_i;&c_t;&c_mol; <ind>&c_v;&c_je;&c_r;&c_h;&c_o;&c_m;</ind></trd>
    </trdgrp>
  <trd lng="sv">rida</trd>
</drv>

<drv mrk="rajd.0antaro">
  <kap><tld/>antaro</kap>
  <snc>
    <dif>
      Grupo da <tld/>antoj.
    </dif>
    <refgrp tip="sin">
      <ref cel="kavalk.0o">kavalkado</ref>.
    </refgrp>
  </snc>
  <trd lng="ru">&c_k;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_k;&c_a;&c_d;&c_a;</trd>
</drv>
</art>
