<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>4</ofc>
<rad>bis</rad></kap>
<drv mrk="bis.0">
<kap><tld/></kap>
<snc>
<dif>
Ekkrio por peti ripetadon (= refoje! Ankora&ubreve; unu fojon!)
</dif>
</snc>
 <trdgrp lng="de">
    <trd>Zugabe</trd>,
    <trd>da capo<klr> [ark]</klr></trd>
 </trdgrp>
<trd lng="fr">bis!</trd>
<trd lng="hu">&uacute;jra!</trd>
<trd lng="ru">&c_b;&c_i;&c_s;!</trd>
</drv>

    <drv mrk="bis.0i">
      <kap><tld/>i</kap>
      <snc>
        <dif>
          Krie peti pri ripeto:
          <ekz>
            <tld/>i tenorulon, arion, oratoron.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="de">
         <trd>Zugabe! <ind>schreien</ind></trd>,
         <trd><klr>nach </klr>Zugabe <ind>verlangen</ind></trd>
      </trdgrp>
      <trd lng="fr">bisser</trd>
      <trd lng="hu">&uacute;jr&aacute;zik<klr tip="ind"> (&uacute;jr&aacute;t ki&aacute;lt)</klr></trd>
    </drv>

</art>
