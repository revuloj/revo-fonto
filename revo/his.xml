<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: his.xml,v 1.19 2023/08/31 15:46:18 revo Exp $">
<kap>
  <rad>his</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="his.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="his.0i.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Suprentiri per &scirc;nuro:
      <ekz>
        <tld/>i signalon, velon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_y;&c_m;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_z;&c_d;&c_y;&c_m;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">hissar</trd>
  <trd lng="cs">vzty&#x10D;it <klr>(vlajku)</klr></trd>
  <trdgrp lng="de">
    <trd>hissen</trd>,
    <trd>aufziehen</trd>
  </trdgrp>
  <trd lng="el">&ypsilon;&psi;&omega_ton;&ny;&omega;</trd>
  <trd lng="fr">hisser</trd>
  <trdgrp lng="hu">
    <trd>felvon<klr tip="ind"> (z&aacute;szl&oacute;t)</klr></trd>,
    <trd>felh&uacute;z<klr tip="ind"> (zsin&oacute;rral)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>issare<klr> (mar.)</klr></trd>,
    <trd>inalberare<klr> (issare)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3042;&#x3052;&#x308B;</trd>,
    <trd>&#x63B2;&#x3052;&#x308B;  [<ind>&#x304B;&#x304B;&#x3052;&#x308B;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>hijsen</trd>,
    <trd>optrekken</trd>
  </trdgrp>
  <trd lng="pl">wci&aogonek;ga&cacute;</trd>
  <trd lng="pt">i&ccedil;ar</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_n;&c_ja;&c_t;&c_mol; <klr>(&c_p;&c_a;&c_r;&c_u;&c_s;, &c_f;&c_l;&c_a;&c_g; &c_i;
  &c_t;.&c_p;.)</klr></trd>
  <trd lng="sk">vytiahnu&#x165; <klr>(&#x161;n&#xFA;rkou vlajku)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x456;&#x434;&#x43D;&#x456;&#x43C;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x456;&#x434;&#x456;&#x439;&#x43C;&#x430;&#x442;&#x438; (&#x432;&#x456;&#x442;&#x440;&#x438;&#x43B;&#x430;</trd>,
    <trd>&#x43F;&#x440;&#x430;&#x43F;&#x43E;&#x440;)</trd>
  </trdgrp>
</drv>

<drv mrk="his.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="his.mal0i.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Malsuprentiri per &scirc;nuro:
      <ekz>
        mi mal<tld/>is la flagon kaj kapitulacis
        <fnt>
          <aut>M. Twain, trad. A. de Ruyter kaj W. Kirschstein</aut>,
          <vrk>Malfacilas esti kandidato</vrk>,
          <bib>LOdE</bib>,
          <lok>2000:4 (66)</lok>
        </fnt>        
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">arriar</trd>
  <trd lng="cs">spou&#x161;t&#x11B;t vlajku</trd>
  <trd lng="fr">amener<klr> (voile, drapeau...)</klr></trd>
  <trd lng="it">ammainare</trd>
  <trd lng="ja">&#x304A;&#x308D;&#x3059;</trd>
  <trd lng="sk">sp&#xFA;&#x161;&#x165;a&#x165; vlajku</trd>
  <trd lng="uk">&#x43E;&#x43F;&#x443;&#x441;&#x43A;&#x430;&#x442;&#x438;</trd>
</drv>
</art>
