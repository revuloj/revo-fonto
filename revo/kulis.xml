<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>kulis</rad>/o
</kap>
<drv mrk="kulis.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <dif>
      Parto de teatro malanta&ubreve; la sceno, &ccirc;e kiu oni aran&gcirc;as la
      dekoracia&jcirc;ojn:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        post la <tld/>oj<fnt>B</fnt>
        <klr>(nepublike, sekrete, ka&scirc;e)</klr>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>slip</trd>,
    <trd>wing</trd>
  </trdgrp>
  <trd lng="nl">coulisse</trd>
  <trd lng="ru">&c_k;&c_u;&c_l;&c_i;&c_s;&c_a;</trd>
</drv>
</art>
