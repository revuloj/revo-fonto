<drv mrk="toler.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="toler.0ema.KOMUNE">
    <dif>
      Inklina <tld/>i:
      <ekz>
        li estas <tld/>ema al la diversaj opinioj;
      </ekz>
      <ekz>
        esti afabla al &ccirc;iuj, instruema, <tld/>ema
        <fnt>
          <bib>NT</bib>
          <lok>II. Timoteo 2:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        neniu <tld/>ema homo permesus al si publike insulti la anojn de
        alia religio
        <fnt>
          <aut>Anna L&ouml;wenstein</aut>
            <vrk><url ref="&Monato;/2000/006800.php">Funebro sen dio</url></vrk>
            <bib>Monato</bib>
            <lok>2000/02, p. 4</lok>
          </fnt>;
      </ekz>
      <ekz>
        kun &ccirc;ia humileco kaj mildeco, kun <tld/>emeco, paciencante unu
        al alia en amo
        <fnt>
          <bib>NT</bib>
          <lok>Efesanoj 4:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kiso kompensu viajn fidelan atendon kaj sinceran  <tld/>emon
         <fnt>
          <aut>Shi Chengtai</aut>
          <vrk><url ref="&Monato;/1995/A950601.php">La kompensa kiso</url></vrk>
          <bib>Monato</bib>
          <lok>1995/06, p. 27</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_ja;&c_r;&c_p;&c_l;&c_ib;&c_v;&c_y;</trd>
  <trd lng="ca">tolerant</trd>
  <trdgrp lng="cs">
    <trd>sn&aacute;&scaron;enliv&yacute;</trd>,
    <trd>tolerantn&iacute;</trd>
  </trdgrp>
  <trd lng="de">tolerant</trd>
  <trd lng="en">tolerant</trd>
  <trd lng="es">tolerante</trd>
  <trd lng="fr">tol&eacute;rant</trd>
  <trdgrp lng="hu">
    <trd>t&uuml;relmes</trd>,
    <trd>toler&aacute;ns</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5BDB;&#x5927;&#x306A;  <pr>&#x304B;&#x3093;&#x3060;&#x3044;&#x306A;</pr></trd>,
    <trd>&#x5BDB;&#x5BB9;&#x306A;  <pr>&#x304B;&#x3093;&#x3088;&#x3046;&#x306A;</pr></trd>,
    <trd>&#x6211;&#x6162;&#x5F37;&#x3044;  <pr>&#x304C;&#x307E;&#x3093;&#x3065;&#x3088;&#x3044;</pr></trd>,
    <trd>&#x8F9B;&#x62B1;&#x5F37;&#x3044;  <pr>&#x3057;&#x3093;&#x307C;&#x3046;&#x3065;&#x3088;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">verdraagzaam</trd>
  <trdgrp lng="pl">
