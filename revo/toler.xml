  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6211;&#x6162;&#x3059;&#x308B;  <pr>&#x304C;&#x307E;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x8F9B;&#x62B1;&#x3059;&#x308B;  <pr>&#x3057;&#x3093;&#x307C;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x8010;&#x3048;&#x5FCD;&#x3076;  <pr>&#x305F;&#x3048;&#x3057;&#x306E;&#x3076;</pr></trd>,
    <trd>&#x8A31;&#x5BB9;&#x3059;&#x308B;  <pr>&#x304D;&#x3087;&#x3088;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5927;&#x76EE;&#x306B;&#x898B;&#x308B;  <pr>&#x304A;&#x304A;&#x3081;&#x306B;&#x307F;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
