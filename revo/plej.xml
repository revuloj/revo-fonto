<drv mrk="plej.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="P"/> granda atingebla kvanto a&ubreve; grado; <ref tip="sin" cel="maksim1.0o.KOMUNE">maksimumo</ref>:
      <ekz>
        li faras sian <tld/>on
        <fnt>
          <bib>ChB</bib>
          <lok>9</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu lasta volumo primeditas malpli akrajn problemojn, sed &gcirc;uste en &gcirc;i la stilo
        de <nom>Waringhien</nom> atingas sian <tld/>on
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#8470;6</lok>
        </fnt>;
      </ekz>
      <ekz>
        min vi &ccirc;irka&ubreve;braki emas &parsl; per la <tld/>o de la jun&#8217;
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470;6</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">das <ind>Meiste</ind></trd>
  <trd lng="en">the <ind>most</ind></trd>
</drv>

<drv mrk="plej.mal0">
  <kap>mal<tld/></kap>
  <gra><vspec>adverbo</vspec></gra>
  <snc mrk="plej.mal0.KOMUNE">
    <dif>
      Kvante a&ubreve; kvalite sub &ccirc;iuj
      aliaj:
      <ekz>
        el &ccirc;iuj siaj fratoj Antono estas la mal<tld/> sa&gcirc;a
        <fnt>
          <bib>F</bib>
          <lok>&FE; 10</lok>
        </fnt>;
      </ekz>
      <ekz>
        la mal<tld/> granda el &ccirc;iuj sanktuloj
        <fnt>
          <bib>NT</bib>
          <lok>Efesanoj 3:8</lok>
        </fnt>;
