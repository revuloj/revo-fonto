  <trd lng="yi">&#x5DE;&#x5E2;&#x5E8;&#x5E1;&#x5D8;</trd>
  <trd lng="zu">iningi</trd>
</drv>

<drv mrk="plej.0a">
  <kap><tld/>a</kap>
  <snc mrk="plej.0a.KOMUNE">
    <dif>
      Tiom granda, forta..., ke pli
      granda, forta ne estas;
      <ref tip="vid" cel="maksim1.0a.KOMUNE">maksimuma</ref>:
      <ekz>
        la&ubreve; mia <tld/>a kapablo
        <fnt><bib>Kiso</bib></fnt>;
      </ekz>
      <ekz>
        la <tld/>a fripono en la urbo
        <fnt>
          <aut>Czin &Scirc;entanj (= Jin Shengtan), trad. P. Mo&jcirc;ajev</aut>
          <vrk><url ref="http://esperantio.narod.ru/ekster/19ghojo.htm">Dek na&ubreve; &gcirc;ojoj de la vivo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="plej.mal0a.KOMUNE">malpleja</ref>
    </refgrp>
  </snc>
  <trd lng="de">maximal</trd>
  <trd lng="en">maximal</trd>
  <trd lng="fr">maximal</trd>
  <trdgrp lng="hu">
    <trd>maxim&aacute;lis</trd>,
    <trd>maximum-</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>massimo<klr> (agg.)</klr></trd>,
    <trd>massimale<klr> (agg.)</klr></trd>
  </trdgrp>
  <trd lng="nl">maximaal</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_k;&c_s;&c_i;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_a;&c_i;&c_b;&c_o;&c_l;&c_mol;&c_sh;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="plej.0e">
   <kap><tld/>e</kap>
   <snc mrk="plej.0e.KOMUNE">
     <dif>
       En la <tld/> alta grado, maksimume:
       <ekz>
         &ccirc;iu laboristo <tld/>e 50-jara rajtas ripozi.
       </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>&c_n;&c_a;&c_j;&c_b;&c_o;&c_l;&c_je;&c_j;</trd>,
       <trd>&c_n;&c_a;&c_j;&c_b;&c_o;&c_l;&c_mol;&c_sh;</trd>,
       <trd>&c_b;&c_o;&c_l;&c_mol;&c_sh; &c_z;&c_a; &c_w;&c_s;&c_jo;</trd>
     </trdgrp>
     <trd lng="de">h&ouml;chstens</trd>
     <trdgrp lng="en">
       <trd>at most</trd>,
       <trd>maximally</trd>
     </trdgrp>
     <trdgrp lng="fi">
       <trd>eniten</trd>
     </trdgrp>
     <trd lng="fr">au maximum</trd>
     <trd lng="hu">legfeljebb</trd>
     <trd lng="nl">hoogstens</trd>
     <trd lng="ru">&c_b;&c_o;&c_l;&c_mol;&c_sh;&c_je; &c_v;&c_s;&c_je;&c_g;&c_o;</trd>
   </snc>
   <snc mrk="plej.0e.plejofte">
     <dif>
       <tld lit="P"/> ofte:
       <ekz>
         oni povis nur malofte vidi &scirc;ian &ccirc;apelon sur tiu loko, kiun
         tiu &ccirc;i vesta&jcirc;o <tld/>e okupas &ccirc;e la aliaj homoj<fnt>Z</fnt>;
       </ekz><ekz>
         <tld/>e okazas, ke...;
       </ekz><ekz>
         en nia lingvo la vortoj <tld/>e estas pli naturaj kaj pli
         bonsonaj se ili estas ne kunligataj<fnt>Z</fnt>;
       </ekz><ekz>
         la infanoj estas trankvilaj <tld/>e tiam, kiam oni enlitigis
         ilin<fnt>Z</fnt>.
       </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>&c_n;&c_a;&c_j;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_je;&c_j;</trd>,
       <trd>&c_z;&c_v;&c_y;&c_ch;&c_a;&c_j;&c_n;&c_a;</trd>
     </trdgrp>
     <trd lng="de">meistens</trd>
     <trdgrp lng="en">
       <trd>most often</trd>
     </trdgrp>
     <trdgrp lng="fi">
       <trd>enimm&auml;kseen</trd>
     </trdgrp>
     <trdgrp lng="fr">
       <trd>dans la plupart des cas</trd>,
       <trd>le plus souvent</trd>
     </trdgrp>
     <trdgrp lng="hu">
       <trd>legink&aacute;bb</trd>,
       <trd>leggyakrabban</trd>
     </trdgrp>
     <trd lng="nl">meestal</trd>
     <trdgrp lng="ru">
       <trd>&c_ch;&c_a;&c_shch;&c_je; &c_v;&c_s;&c_je;&c_g;&c_o;</trd>,
       <trd>&c_p;&c_o; &c_b;&c_o;&c_l;&c_mol;&c_sh;&c_je;&c_j; &c_ch;&c_a;&c_s;&c_t;&c_i;</trd>,
       <trd>&c_o;&c_b;&c_y;&c_ch;&c_n;&c_o;</trd>
     </trdgrp>
   </snc>
  <trd lng="cs">nejv&iacute;ce</trd>
  <trdgrp lng="sk">
    <trd>najviac nadov&scaron;etko</trd>,
    <trd>najv&auml;&ccaron;&scaron;mi</trd>
  </trdgrp>
</drv>

<drv mrk="plej.mal0">
   <kap>mal<tld/></kap>
   <gra><vspec>adverbo</vspec></gra>
   <snc mrk="plej.mal0.KOMUNE">
     <dif>
       Kvante a&ubreve; kvalite malsuper &ccirc;iuj
       aliaj:
       <ekz>
         el &ccirc;iuj lernantoj jen la mal<tld/> laborema.
       </ekz>
     </dif>
   </snc>
  <trdgrp lng="be">
     <trd>&c_n;&c_a;&c_j;&c_m;&c_je;&c_n;&c_je;&c_j;</trd>,
     <trd>&c_n;&c_a;&c_j;&c_m;&c_je;&c_n;&c_sh;</trd>
   </trdgrp>
  <trd lng="cs">nejm&eacute;n&ecaron;</trd>
  <trd lng="de">wenigst</trd>
  <trdgrp lng="en">
       <trd>least</trd>,
     </trdgrp>
  <trd lng="fi">v&auml;hiten</trd>
  <trd lng="fr">le moins</trd>
  <trd lng="hu">legkev&eacute;sb&eacute;</trd>
  <trd lng="nl">minst</trd>
  <trd lng="ru">&c_n;&c_a;&c_i;&c_m;&c_je;&c_n;&c_je;&c_je;</trd>
  <trd lng="sk">najmenej</trd>
  <trd lng="sv">minst</trd>
</drv>

<drv mrk="plej.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="plej.mal0a.KOMUNE">
    <dif>
      Tiom malgranda, malforta..., ke pli
      malgranda, malforta ne estas;
      <ref tip="vid" cel="minimu.0a.KOMUNE">minimuma</ref>:
      <ekz>
        neniu sentis e&ccirc; mal<tld/>an efikon
        <fnt><bib>MkM</bib></fnt>
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="plej.0a.KOMUNE">pleja</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_n;&c_ib;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trdgrp lng="en">
    <trd>minimal</trd>,
    <trd>minimum</trd>
  </trdgrp>
  <trd lng="es">m&iacute;nimo <klr>(adjetivo)</klr></trd>
  <trdgrp lng="fr">
    <trd>minimal</trd>,
    <trd>le <ind>moindre</ind></trd>
  </trdgrp>
  <trd lng="pl">minimalny</trd>
  <trd lng="pt">m&iacute;nimo</trd>
  <trdgrp lng="hu">
    <trd>minim&aacute;lis</trd>,
    <trd>legkevesebb</trd>,
    <trd>legkisebb</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_i;&c_n;&c_i;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="plej.mal0e">
   <kap>mal<tld/>e</kap>
   <snc mrk="plej.mal0e.KOMUNE">
     <dif>
       Minimume, almena&ubreve;.
     </dif>
     <trdgrp lng="be">
       <trd>&c_n;&c_a;&c_j;&c_m;&c_je;&c_n;&c_je;&c_j;</trd>,
       <trd>&c_n;&c_a;&c_j;&c_m;&c_je;&c_n;&c_sh;</trd>,
       <trd>&c_m;&c_je;&c_n;&c_sh; &c_z;&c_a; &c_w;&c_s;&c_jo;</trd>
     </trdgrp>
     <trd lng="de">mindestens</trd>
     <trdgrp lng="en">
       <trd>at least</trd>,
       <trd>minimally</trd>
     </trdgrp>
     <trd lng="fr">au minimum</trd>
     <trd lng="hu">legal&aacute;bb</trd>
     <trd lng="nl">minstens</trd>
     <trd lng="ru">&c_m;&c_je;&c_n;&c_mol;&c_sh;&c_je; &c_v;&c_s;&c_je;&c_g;&c_o;</trd>
   </snc>
   <snc mrk="plej.mal0e.plejmalofte">
     <dif>
       <tld lit="P"/> malofte.
     </dif>
     <trd lng="be">&c_n;&c_a;&c_j;&c_r;&c_a;&c_d;&c_z;&c_je;&c_j;</trd>
     <trd lng="de">wenigstens</trd>
     <trdgrp lng="en">
       <trd>least often</trd>
     </trdgrp>
     <trd lng="fr">le moins souvent</trd>
     <trdgrp lng="hu">
       <trd>legkev&eacute;sb&eacute;</trd>,
       <trd>legritk&aacute;bban</trd>
     </trdgrp>
     <trd lng="nl">zelden</trd>
     <trd lng="ru">&c_r;&c_je;&c_zh;&c_je; &c_v;&c_s;&c_je;&c_g;&c_o;</trd>
   </snc>
  <trd lng="cs">nejm&eacute;n&ecaron; <klr>(adv.)</klr></trd>
  <trdgrp lng="sk">
    <trd>aspo&ncaron;</trd>,
    <trd>minim&aacute;lne</trd>,
    <trd>prinajmenej</trd>,
    <trd>prinajmen&scaron;om</trd>,
    <trd>&ccaron;o len</trd>
  </trdgrp>
</drv>
