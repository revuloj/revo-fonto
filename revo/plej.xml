<drv mrk="plej.0e">
  <kap><tld/>e</kap>
  <snc mrk="plej.0e.KOMUNE">
    <dif>
      En la <tld/> alta grado, <ref tip="sin" cel="maksim1.0e">maksimume</ref>:
      <ekz>
        vin mi amas <tld/>e
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        por kia okupo vi sentas vin <tld/>e kapabla
        <fnt>
          <bib>Marta</bib>
        </fnt>?
      </ekz>
      <ekz>
        klopodi pri &ccirc;ies favoro estas <tld/>e malsa&gcirc;a laboro
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>&c_n;&c_a;&c_j;&c_b;&c_o;&c_l;&c_je;&c_j;</trd>,
       <trd>&c_n;&c_a;&c_j;&c_b;&c_o;&c_l;&c_mol;&c_sh;</trd>,
       <trd>&c_b;&c_o;&c_l;&c_mol;&c_sh; &c_z;&c_a; &c_w;&c_s;&c_jo;</trd>
     </trdgrp>
     <trd lng="de">h&ouml;chstens</trd>
     <trdgrp lng="en">
       <trd>at most</trd>,
       <trd>maximally</trd>
     </trdgrp>
     <trdgrp lng="fi">
       <trd>eniten</trd>
     </trdgrp>
     <trd lng="fr">au maximum</trd>
     <trd lng="hu">legfeljebb</trd>
     <trd lng="nl">hoogstens</trd>
     <trdgrp lng="pl">
       <trd>przewa&zdot;nie</trd>,
       <trd>zw&lstroke;aszcza</trd>
     </trdgrp>
     <trdgrp lng="ro">
       <trd>mai ales</trd>,
       <trd>&icirc;n special</trd>,
       <trd>mai cu seam&abreve;</trd>
     </trdgrp>
     <trd lng="ru">&c_b;&c_o;&c_l;&c_mol;&c_sh;&c_je; &c_v;&c_s;&c_je;&c_g;&c_o;</trd>
     <trdgrp lng="zh">
    <trd>&#26368;&#22810; <pr>zu&igrave;du&omacron;</pr></trd>,
    <trd>&#33267;&#22810; <pr>zh&igrave;du&omacron;</pr></trd>,
    <trd>&#20805;&#20854;&#37327; <pr>ch&omacron;ngq&iacute;li&agrave;ng</pr></trd>,
    <trd>&#33267;&#39640; <pr>zh&igrave;g&amacron;o</pr></trd>,
    <trd>&#22823;&#19981;&#36807; <pr>d&agrave;b&ugrave;gu&ograve;</pr></trd>,
    <trd>&#22823;&#19981;&#36942; <pr>d&agrave;b&ugrave;gu&ograve;</pr></trd>
  </trdgrp>
</snc>
   <snc mrk="plej.0e.plejofte">
     <dif>
       <tld lit="P"/> ofte:
      <ekz>
        oni <klr>[&scirc;ian &ccirc;apelon]</klr> nur malofte povis vidi sur tiu loko, kiun tiu &ccirc;i
        vesta&jcirc;o <tld/>e okupas &ccirc;e aliaj homoj
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz><ekz>
         en nia lingvo la vortoj <tld/>e estas pli naturaj kaj pli
         bonsonaj se ili estas ne kunligataj
         <fnt>
          <bib>LR</bib>
          <lok>Unua Parto &ndash; Leksikologio</lok>
        </fnt>;
       </ekz>
      <ekz>
        li nur postulas, ke ili estu trankvilaj, kaj tiaj ili estas <tld/>e tiam, kiam
        oni ilin enlitigis
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>.
      </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>&c_n;&c_a;&c_j;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_je;&c_j;</trd>,
       <trd>&c_z;&c_v;&c_y;&c_ch;&c_a;&c_j;&c_n;&c_a;</trd>
     </trdgrp>
     <trd lng="de">meistens</trd>
     <trdgrp lng="en">
       <trd>most often</trd>
     </trdgrp>
     <trdgrp lng="fi">
       <trd>enimm&auml;kseen</trd>
     </trdgrp>
     <trdgrp lng="fr">
       <trd>dans la plupart des cas</trd>,
       <trd>le plus souvent</trd>
     </trdgrp>
     <trdgrp lng="hu">
       <trd>legink&aacute;bb</trd>,
       <trd>leggyakrabban</trd>
     </trdgrp>
     <trd lng="nl">meestal</trd>
     <trdgrp lng="ru">
       <trd>&c_ch;&c_a;&c_shch;&c_je; &c_v;&c_s;&c_je;&c_g;&c_o;</trd>,
       <trd>&c_p;&c_o; &c_b;&c_o;&c_l;&c_mol;&c_sh;&c_je;&c_j; &c_ch;&c_a;&c_s;&c_t;&c_i;</trd>,
       <trd>&c_o;&c_b;&c_y;&c_ch;&c_n;&c_o;</trd>
     </trdgrp>
     <trd lng="zh">&#22823;&#22810; <pr>d&agrave;du&omacron;</pr></trd>
</snc>
  <trd lng="cs">nejv&iacute;ce</trd>
  <trdgrp lng="ja">
    <trd>&#26368;&#22823;&#12391;  <pr>&#12373;&#12356;&#12384;&#12356;&#12391;</pr></trd>,
