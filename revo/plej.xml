<drv mrk="plej.0a">
  <kap><tld/>a</kap>
  <snc mrk="plej.0a.KOMUNE">
    <dif>
      Tiom granda, forta..., ke pli
      granda, forta ne estas;
      <ref tip="vid" cel="maksim1.0a.KOMUNE">maksimuma</ref>:
      <ekz>
        la&ubreve; mia <tld/>a kapablo
        <fnt><bib>Kiso</bib></fnt>;
      </ekz>
      <ekz>
        la <tld/>a fripono en la urbo
        <fnt>
          <aut>Czin &Scirc;entanj (= Jin Shengtan), trad. P. Mo&jcirc;ajev</aut>
          <vrk><url ref="http://esperantio.narod.ru/ekster/19ghojo.htm">Dek na&ubreve; &gcirc;ojoj de la
          vivo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="plej.mal0a.KOMUNE">malpleja</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_j;&c_b;&c_o;&c_l;&c_mol;&c_sh;&c_y;</trd>,
    <trd>&c_n;&c_a;&c_j;&c_m;&c_a;&c_c;&c_n;&c_je;&c_j;&c_sh;&c_y;</trd>,
    <trd>&c_m;&c_a;&c_k;&c_s;&c_y;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>meiste</trd>,
    <trd>maximale</trd>
  </trdgrp>
  <trd lng="en">maximal</trd>
  <trd lng="fr">maximal</trd>
  <trdgrp lng="hu">
    <trd>maxim&aacute;lis</trd>,
    <trd>maximum-</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>massimo<klr> (agg.)</klr></trd>,
    <trd>massimale<klr> (agg.)</klr></trd>
  </trdgrp>
  <trd lng="nl">maximaal</trd>
  <trd lng="pl">maksymalny</trd>
