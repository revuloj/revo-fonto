<drv mrk="plej.0">
   <kap><ofc>*</ofc><tld/></kap>
   <gra><vspec>adverbo</vspec></gra>
   <snc mrk="plej.0.KOMUNE">
     <dif>
       Kvante a&ubreve; grade super &ccirc;iuj aliaj:
       <ekz>
         &scirc;i estis unu el la <tld/> belaj knabinoj, kiujn oni povis
         trovi
         <fnt><bib>F</bib>, <lok>Ekzercaro</lok></fnt>;
       </ekz><ekz>
         tion oni povas fari <ind>sen la <tld/> malgranda risko</ind>
         <trd lng="fi">ilman v&auml;h&auml;isint&auml;k&auml;&auml;n riski&auml;</trd>;
       </ekz><ekz>
         la <tld/> Supra
         <klr>(Dio)</klr>;
       </ekz><ekz>
         <ind>la <tld/> inteligenta el &ccirc;iuj</ind>
         <trd lng="fi">kaikkein &auml;lykk&auml;in</trd>;
       </ekz><ekz>
         la scion &scirc;atu pli, ol <tld/> puran oron
	 <fnt><bib>MT</bib>, <lok>&Sen; 6:10</lok></fnt>;
       </ekz><ekz>
         <ind>&ccirc;io iras <tld/> bone</ind>
         <trd lng="fi">kaikki menee oikein hyvin (t. mit&auml;
         parhaiten)</trd>.
       </ekz>
     </dif>
   </snc>
   <trd lng="af">mees</trd>
  <trd lng="am">&#x1260;&#x1323;&#x121D;</trd>
  <trd lng="ar">&#x623;&#x643;&#x62B;&#x631;</trd>
  <trd lng="az">&#x259;n</trd>
  <trdgrp lng="be">
     <trd>&c_n;&c_a;&c_j;&c_b;&c_o;&c_l;&c_je;&c_j;</trd>,
     <trd>&c_n;&c_a;&c_j;&c_b;&c_o;&c_l;&c_mol;&c_sh;</trd>,
     <trd>&c_s;&c_a;&c_m;&c_y;</trd>,
     <trd>&c_n;&c_a;&c_j;-</trd>
   </trdgrp>
   <trdgrp lng="bg">
     <trd>&c_n;&c_a;&c_j;</trd>
     <trd>&c_n;&c_a;&c_j;-</trd>
   </trdgrp>
   <trd lng="bn">&#x9B8;&#x9AC;&#x99A;&#x9C7;&#x9AF;&#x9BC;&#x9C7;</trd>
  <trd lng="bo">&#xF58;&#xF44;&#xF0B;&#xF46;&#xF7A;&#xF0B;&#xF56;&#xF0B;</trd>
  <trd lng="bs">ve&#x107;ina</trd>
  <trd lng="co">pi&#xF9;</trd>
  <trd lng="cs">nejv&iacute;ce</trd>
   <trd lng="da">mest</trd>
  <trdgrp lng="de">
      <trd>meist</trd>,
      <trd>-st</trd>,
      <trd>-t</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>most</trd>,
     <trd>-est</trd>
   </trdgrp>
   <trd lng="et">k&#xF5;ige</trd>
  <trd lng="eu">gehien</trd>
  <trd lng="fi">eniten <klr>(superlatiivin merkki, enint&auml; m&auml;&auml;r&auml;&auml; ilmaiseva partikkeli)</klr></trd>
   <trd lng="fr">le plus</trd>
   <trd lng="fy">measte</trd>
  <trd lng="gl">m&#xE1;is</trd>
  <trd lng="gu">&#xAB8;&#xACC;&#xAA5;&#xAC0;</trd>
  <trd lng="ha">mafi</trd>
  <trd lng="hi">&#x938;&#x92C;&#x938;&#x947;</trd>
  <trd lng="hr">ve&#x107;ina</trd>
  <trd lng="hu">leg-...bb</trd>
   <trd lng="hy">&#x574;&#x565;&#x56E; &#x574;&#x561;&#x57D;&#x568;</trd>
  <trd lng="is">mest</trd>
  <trd lng="ja">&#x6700;&#x3082;</trd>
  <trd lng="jw">paling</trd>
  <trd lng="ka">&#x10E7;&#x10D5;&#x10D4;&#x10DA;&#x10D0;&#x10D6;&#x10D4;</trd>
  <trd lng="km">&#x1785;&#x17D2;&#x179A;&#x17BE;&#x1793;&#x1794;&#x17C6;&#x1795;&#x17BB;&#x178F;</trd>
  <trd lng="ko">&#xAC00;&#xC7A5;</trd>
  <trd lng="lo">&#xEAB;&#xEBC;&#xEB2;&#xE8D;&#xE97;&#xEB5;&#xEC8;&#xEAA;&#xEB8;&#xE94;</trd>
  <trd lng="ml">&#xD0F;&#xD31;&#xD4D;&#xD31;&#xD35;&#xD41;&#xD02;</trd>
  <trd lng="mr">&#x938;&#x930;&#x94D;&#x935;&#x93E;&#x924;</trd>
  <trd lng="my">&#x1021;&#x1019;&#x103B;&#x102C;&#x1038;&#x1006;&#x102F;&#x1036;&#x1038;</trd>
  <trd lng="ne">&#x938;&#x92C;&#x948; &#x92D;&#x928;&#x94D;&#x926;&#x93E; &#x930;&#x93E;&#x92E;&#x94D;&#x930;&#x94B;</trd>
  <trd lng="nl">meest</trd>
   <trd lng="ny">kwambiri</trd>
