<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>prujn</rad>/o
</kap>
<drv mrk="prujn.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="prujn.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Roso a&ubreve; nebulo dum malvarmaj noktoj glacii&gcirc;inta:
      <ekz>
        la <tld/>o briligis la arbaron, kvaza&ubreve; &gcirc;i estus pudrita per
        diamanta polvo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &scirc;i vi&scirc;is per la mano la palpebrojn kovritajn de
        <tld/>o<fnt>B</fnt>;
      </ekz>
      <ekz>
        oni a&ubreve;das la murmureton de l' <tld/>aj steletoj, falantaj
        teren<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_n;&c_je;&c_j;</trd>,
    <trd>&c_n;&c_a;&c_m;&c_a;&c_r;&c_a;&c_z;&c_mol;</trd>,
    <trd>&c_sh;&c_e;&c_r;&c_a;&c_n;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">Reif</trd>
  <trdgrp lng="en">
    <trd>hoar</trd>,
    <trd>rime</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gel&eacute;e blanche</trd>,
    <trd>givre</trd>
  </trdgrp>
  <trd lng="hu">d&eacute;r</trd>
  <trdgrp lng="nl">
    <trd>rijp<klr tip="ind"> (rijm)</klr></trd>,
    <trd>rijm<klr tip="ind"> (rijp)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_je;&c_j;</trd>,
    <trd>&c_i;&c_z;&c_m;&c_o;&c_r;&c_o;&c_z;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
