<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: prujn.xml,v 1.27 2025/10/07 20:30:32 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>prujn</rad>/o
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="prujn.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="prujn.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      <ref cel="ros.0o">Roso</ref> a&ubreve; nebulo dum malvarmaj noktoj glacii&gcirc;inta:
      <ekz>
        &ccirc;iuj arboj staris kovritaj de <tld/>o, kvaza&ubreve; ili estus aliformigitaj en
        blankajn koralojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la supra&jcirc;o de la dezerto ku&scirc;as io delikata, sen&scirc;ela, delikata kiel <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 16:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj vitra&jcirc;oj de l&#x2019; vagonoj estis <klr>[&#x2026;]</klr> kovritaj de densa <tld/>o
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        venis la blanka vintro kun la <klr>[&#x2026;]</klr> krakanta ne&gcirc;o, la pale smeralda &ccirc;ielo,
        la roza prujno sur la arboj
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XXVIII</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="flor.negxo0o.prujno">ne&gcirc;ofloro<sncref/></ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_n;&c_je;&c_j;</trd>,
    <trd>&c_n;&c_a;&c_m;&c_a;&c_r;&c_a;&c_z;&c_mol;</trd>,
    <trd>&c_sh;&c_e;&c_r;&c_a;&c_n;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_l;&c_a;&c_n;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>jinovatka</trd>,
    <trd>j&iacute;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Reif</trd>,
    <trd>Rauhreif</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>frost</trd>,
    <trd>hoar</trd>,
    <trd>rime</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>escarcha</trd>,
    <trd>helada</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_b;&a_sh;&a_m;</trd>,
    <trd>&a_b;&a_r;&a_f;&f_k;</trd>,
    <trd>&f_y;&a_kh;&a_b;&a_s;&a_t;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gel&eacute;e blanche</trd>,
    <trd>givre</trd>
  </trdgrp>
  <trd lng="hu">d&eacute;r</trd>
  <trd lng="ja">&#x971C;  <pr>&#x3057;&#x3082;</pr></trd>
  <trdgrp lng="nl">
    <trd>rijp<klr tip="ind"> (rijm)</klr></trd>,
    <trd>rijm<klr tip="ind"> (rijp)</klr></trd>
  </trdgrp>
  <trd lng="pl">szron</trd>
  <trd lng="pt">geada</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_je;&c_j;</trd>,
    <trd>&c_i;&c_z;&c_m;&c_o;&c_r;&c_o;&c_z;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>inova&tcaron;</trd>,
    <trd>osuhe&lcaron;</trd>,
    <trd>srie&ncaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ib;&c_n;&c_ib;&c_j;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_o;&c_r;&c_o;&c_z;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x971C; <pr>shu&amacron;ng</pr></trd>,
    <trd>&#x96FE;&#x51C7; <pr>w&ugrave;s&omacron;ng</pr></trd>,
    <trd>&#x9727;&#x51C7; <pr>w&ugrave;s&omacron;ng</pr></trd>,
    <trd>&#x96E8;&#x51C7; <pr>y&#x1D4;s&omacron;ng</pr></trd>,
    <trd>&#x767D;&#x971C; <pr>b&aacute;ishu&amacron;ng</pr></trd>
  </trdgrp>
</drv>
</art>
