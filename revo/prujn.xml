<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: prujn.xml,v 1.25 2024/02/17 07:52:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>prujn</rad>/o
</kap>
<drv mrk="prujn.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="prujn.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Roso a&ubreve; nebulo dum malvarmaj noktoj glacii&gcirc;inta:
      <ekz>
        la <tld/>o briligis la arbaron, kvaza&ubreve; &gcirc;i estus
        pudrita per
        diamanta polvo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &scirc;i vi&scirc;is per la mano la palpebrojn kovritajn de
        <tld/>o<fnt>B</fnt>;
      </ekz>
      <ekz>
        oni a&ubreve;das la murmureton de l' <tld/>aj steletoj, falantaj
        teren<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_n;&c_je;&c_j;</trd>,
    <trd>&c_n;&c_a;&c_m;&c_a;&c_r;&c_a;&c_z;&c_mol;</trd>,
    <trd>&c_sh;&c_e;&c_r;&c_a;&c_n;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_l;&c_a;&c_n;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>jinovatka</trd>,
    <trd>j&#xED;n&#xED;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Reif</trd>,
    <trd>Rauhreif</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>frost</trd>,
    <trd>hoar</trd>,
    <trd>rime</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>escarcha</trd>,
    <trd>helada</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_b;&a_sh;&a_m;</trd>,
    <trd>&a_b;&a_r;&a_f;&f_k;</trd>,
    <trd>&f_y;&a_kh;&a_b;&a_s;&a_t;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gel&eacute;e blanche</trd>,
    <trd>givre</trd>
  </trdgrp>
  <trd lng="hu">d&eacute;r</trd>
  <trd lng="ja">&#x971C;  <pr>&#x3057;&#x3082;</pr></trd>
  <trdgrp lng="nl">
    <trd>rijp<klr tip="ind"> (rijm)</klr></trd>,
    <trd>rijm<klr tip="ind"> (rijp)</klr></trd>
  </trdgrp>
  <trd lng="pl">szron</trd>
  <trd lng="pt">geada</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_je;&c_j;</trd>,
    <trd>&c_i;&c_z;&c_m;&c_o;&c_r;&c_o;&c_z;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>inova&tcaron;</trd>,
    <trd>osuhe&#x13E;</trd>,
    <trd>srie&ncaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ib;&#x43D;&c_ib;&c_j;</trd>,
    <trd>&#x43F;&c_a;&#x43C;&#x43E;&c_r;&#x43E;&c_z;&#x44C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x971C; <pr>shu&#x101;ng</pr></trd>,
    <trd>&#x96FE;&#x51C7; <pr>w&#xF9;s&#x14D;ng</pr></trd>,
    <trd>&#x9727;&#x51C7; <pr>w&#xF9;s&#x14D;ng</pr></trd>,
    <trd>&#x96E8;&#x51C7; <pr>y&#x1D4;s&#x14D;ng</pr></trd>,
    <trd>&#x767D;&#x971C; <pr>b&#xE1;ishu&#x101;ng</pr></trd>
  </trdgrp>
</drv>
</art>
