<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>lice</rad>/o
</kap>
<drv mrk="lice.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PED</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="gimnaz.0o">gimnazio</ref>
      <ekz>
        la angla okupas la unuan lokon pri populareco en la urbaj
        elementaj lernejoj, &gcirc;eneralklerigaj <tld/>oj kaj fakaj
        mezlernejoj<fnt><aut>Stanis&lstroke;aw &Sacute;migielski</aut>:
        <vrk>Instruisto
        de la angla ser&ccirc;ata</vrk>, <bib>Monato</bib><lok>jaro 2001a,
        numero 2a, p. 19a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_c;&c_e;&c_j;</trd>  
  <trd lng="br">lise</trd>
  <trd lng="de">Gymnasium</trd>
  <trd lng="el">&gamma;&ypsilon;&my;&ny;&alfa_ton;&sigma;&jota;&omikron;</trd>
  <trdgrp lng="en">
    <trd>grammar school</trd>,
    <trd>high school</trd>,
    <trd>secondary school</trd>
  </trdgrp>
  <trd lng="fr">lyc&eacute;e</trd>
  <trd lng="hu">l&iacute;ceum</trd>  
  <trd lng="nl">lyceum</trd>
  <trd lng="pt">liceu</trd>
  <trd lng="ru">&c_l;&c_i;&c_c;&c_je;&c_j;</trd>
</drv>

<drv mrk="lice.0ano">
  <kap><tld/>ano</kap>
  <uzo tip="fak">PED</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="gimnaz.0ano">gimnaziano</ref>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_c;&c_e;&c_ib;&c_s;&c_t;</trd>      
  <trd lng="br">lisead</trd>
  <trd lng="de">Gymnasiast</trd>
  <trdgrp lng="en">
    <trd>pupil</trd>,
    <trd>student</trd>
  </trdgrp>
  <trd lng="fr">lyc&eacute;en</trd>
  <trd lng="hu">l&iacute;ceumi tanul&oacute;</trd> 
  <trd lng="nl">leerling in een <ind>lyceum</ind></trd> 
  <trd lng="ru">&c_l;&c_i;&c_c;&c_je;&c_i;&c_s;&c_t;</trd>
</drv>
</art>
