<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: zoomorf.xml,v 1.6 2024/07/29 01:46:10 revo Exp $">
<kap>
  <rad>zoomorf</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="zoomorf.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="zoomorf.0a.KOMUNE">
    <dif>
      Bestoforma:
      <ekz>
        en la materia kulturo estis disvastigita planta kaj <tld/>a
        simbolaro
        <fnt>
          <aut>V. I. Ionesov</aut>
          <vrk><url ref="&STEB;arkeologio/baktrio/baktrio2.htm">
          Kulturo kaj socio de Norda Baktrio</url></vrk>
          <lok>Scienca Revuo, 1992:1 (43), p. 3a-8a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_a;&c_m;&c_o;&c_r;&c_f;&c_n;&c_y;</trd>,
    <trd>&c_zh;&c_y;&c_v;&c_jo;&c_l;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;</trd>,
    <trd>&c_zh;&c_y;&c_v;&c_jo;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="en">zoomorphic</trd>
  <trd lng="pl">zoomorficzny</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_v;&c_a;&c_r;&c_i;&c_n;&c_o;&c_p;&c_o;&c_d;&c_ib;&c_b;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_o;&c_o;&c_m;&c_o;&c_r;&c_f;&c_ib;&c_ch;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>

