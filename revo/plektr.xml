<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>plektr</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="plektr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Mane tenata a&ubreve; &ccirc;efingre fiksita ilo,
      per kiu oni pin&ccirc;as la kordojn de
      <ref tip="malprt" cel="kordinstrumento.0o">kordinstrumento</ref>.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>m&eacute;diator</trd>,
    <trd>plectre</trd>
  </trdgrp>
</drv>

<drv mrk="plektr.0i">
  <kap><tld/>i</kap>
  <snc>
    <dif>
      Sonigi muzikilon per plektro.
    </dif>
  </snc>
</drv>
</art>

