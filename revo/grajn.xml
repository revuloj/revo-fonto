<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: grajn.xml,v 1.39 2023/11/11 11:36:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>grajn</rad>/o
</kap>
<drv mrk="grajn.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="grajn.0o.globeto">
    <dif>
      Malgranda malmola globa a&jcirc;o, <ref tip="super" cel="er.0o">ero</ref>:
      <ekz>
        hajlo<tld/>o <klr>(<ref cel="hajl.0ero">hajlero</ref>)</klr>
        <fnt>
          <bib>ViV</bib>
        </fnt>;
      </ekz>
      <ekz>
        sablo<tld/>o <klr>(<ref cel="sabl.0ero">sablero</ref>)</klr>
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        sur la fundo de Ta&hcirc;o efektive trovi&gcirc;as <tld/>oj da oro
        <fnt>
          <bib>RBa</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni &jcirc;etis al &scirc;i sur la tamburon <klr>[&#x2026;]</klr> iafoje orajn <tld/>ojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        penetris <klr>[&#x2026;]</klr> <tld/>eto da vitro en la okulon
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        via idaro estus kiel sablo, kaj viaj devenantoj kiel &gcirc;iaj <tld/>etoj
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 48:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de smeraldo
        <fnt>
          <bib>SatirRak</bib>
        </fnt>.
      </ekz>
    </dif>
  <subsnc mrk="grajn.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      &Ccirc;iu frukto de <ref tip="malprt" cel="gren.0o.planto">greno</ref>
      entenata en la spiko kaj uzata por fari farunon:
      <ekz>
        tritika <tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de hordeo
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili man&gcirc;is <klr>[&#x2026;]</klr> macojn kaj rostitajn <tld/>ojn
        <fnt>
          <bib>MT</bib>
          <lok>Josuo 5:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a&jcirc;o el fre&scirc;aj <tld/>oj
        <fnt><bib>MT</bib> <lok>&Lev; 2:14</lok></fnt>.
      </ekz>
      <ekz>
        e&ccirc; blinda kokino povas trovi <tld/>on
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">grain</trd>
    <trd lng="hu">mag</trd>
    <trd lng="nl">graankorrel</trd>
    <trd lng="pl">ziarno</trd>
    <trdgrp lng="ru">
      <trd>&c_z;&c_je;&c_r;&c_n;&c_o;</trd>,
      <trd>&c_z;&c_jo;&c_r;&c_n;&c_y;&c_sh;&c_k;&c_o;</trd>
    </trdgrp>
  </subsnc>
  <subsnc mrk="grajn.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="dif" cel="grajn.sem0o">Sem<tld/>o</ref>:
      <ekz>
        <tld/>o de sinapo
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sema <tld/>o <klr>[&#x2026;]</klr> disvolvi&gcirc;os
        <fnt>
          <bib>Fab3</bib>
          <lok>Malnova tomba &scirc;tono</lok>
        </fnt>;
      </ekz>
      <ekz>
        sin nutri per la <tld/>oj de lotuso
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        venu kaj helpu min elekti la lentojn <klr>[&#x2026;]</klr> la kolombetoj klinis la
        kapojn kaj komencis pok, pok, pok, <klr>[&#x2026;]</klr> kaj kolektis &ccirc;iujn <tld/>ojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>Cindrulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        se oni ne havis proprajn kafo<tld/>ojn, oni ne povis prepari kafon
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne la florpoto gravas, en kiu, sed nur la <tld/>o, el kiu io elkreskas
        <fnt>
          <aut>J. Dorosmai, trad. J. Dorosmai jun. kaj J. Horvath</aut>
          <vrk>Fabloj kaj Aforismoj</vrk>
          <lok>2002</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        semi en la viraj koroj fajrajn <tld/>ojn de la deziro
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">vet&odblac;mag</trd>
    <trdgrp lng="nl">
      <trd>zaaigoed</trd>,
      <trd>zaaigraan</trd>
    </trdgrp>
    <trd lng="pl">ziarno</trd>
    <trd lng="pt">semente</trd>
    <trdgrp lng="ru">
      <trd>&c_z;&c_je;&c_r;&c_n;&c_o;</trd>,
      <trd>&c_z;&c_jo;&c_r;&c_n;&c_y;&c_sh;&c_k;&c_o;</trd>
    </trdgrp>
  </subsnc>
<!-- ne diferencas de semgrajno, &ccirc;u?
  <subsnc mrk="grajn.0o.kerno">
    <dif>
      Malgranda <ref tip="dif" cel="kern.0o.BOT">kerno</ref> de
      kernofruktoj, ekzemple de pomo, piro.
    </dif>
    <trdgrp lng="en">
      <trd>seed</trd>,
      <trd>pip</trd>
    </trdgrp>
    <trd lng="hu">mag<klr tip="ind"> (csonth&eacute;jas)</klr></trd>
    <trd lng="nl">pit</trd>
    <trdgrp lng="pl">
    <trd>ziarno</trd>,
    <trd>ziarenko</trd>,
    <trd>pestka</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_jo;&c_r;&c_n;&c_y;&c_sh;&c_k;&c_o;</trd>,
      <trd>&c_k;&c_o;&c_s;&c_t;&c_o;&c_ch;&c_k;&c_a;</trd>
    </trdgrp>
  </subsnc>
-->
    <trdgrp lng="en">
      <trd>grain</trd>,
      <trd>granule</trd>,
      <trd>speck</trd>,
      <trd>particle</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>r&ouml;g<klr tip="ind"> (szemcse)</klr></trd>,
      <trd>szemcse</trd>
    </trdgrp>
    <trd lng="nl">korrel</trd>
    <trdgrp lng="pl">
      <trd>ziarno</trd>,
      <trd>drobina</trd>,
      <trd>granulka</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_u;&c_p;&c_i;&c_c;&c_a;</trd>,
      <trd>&c_k;&c_r;&c_u;&c_p;&c_i;&c_n;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="grajn.0o.iometo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Iometo da:
      <ekz>
        <tld/>eto da vero
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o da malbono /
        Por la okul&#x2019; de l&#x2019; mondo ofte kovras /
        La tutan indon de plej bona homo
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        en mia koro ka&scirc;i&gcirc;as <tld/>o de malsa&gcirc;o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gran.0o">grano</ref>,
      <ref cel="gut.0o.FIG_kvanteto">guto<sncref/></ref>,
      <ref cel="ombr.0o.FIG_ero">ombro<sncref/></ref>
    </refgrp>
    <trdgrp lng="en">
      <trd>grain</trd>,
      <trd>granule</trd>,
      <trd>speck</trd>,
      <trd>particle</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>csipetnyi</trd>,
      <trd>cs&ouml;ppnyi</trd>
    </trdgrp>
    <trd lng="nl">greintje</trd>
    <trdgrp lng="pl">
      <trd>ziarno</trd>,
      <trd>odrobina</trd>
    </trdgrp>
    <trd lng="pt">pedacinho<klr tip="amb"> (fig.)</klr></trd>
    <trd lng="ru">&c_k;&c_r;&c_u;&c_p;&c_i;&c_c;&c_a;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_je;&c_r;&c_n;&c_je;</trd>,
    <trd>&c_k;&c_r;&c_u;&c_p;&c_y;</trd>,
    <trd>&c_k;&c_r;&c_u;&c_p;&c_k;&c_a;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_n;&c_u;&c_l;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&t_-a;&t_ba;&t_rata;&t_shabchu;&t_tsheg;&t_ra;&t_gigu;&t_ga;&t_sa;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>granula</trd>,
    <trd>granule</trd>,
    <trd>gran&aacute;lie</trd>,
    <trd>sbalek</trd>,
    <trd>zrno</trd>
  </trdgrp>
  <trd lng="de">Korn</trd>
  <trd lng="es">grano</trd>
  <trdgrp lng="fr">
    <trd>grain</trd>,
    <trd>graine</trd>,
    <trd>p&eacute;pin</trd>,
    <trd>goutte<klr tip="ind"> (petite quantit&eacute;)</klr></trd>,
    <trd>soup&ccedil;on<klr tip="ind"> (petite quantit&eacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7A40;&#x7C92;  <pr>&#x3053;&#x304F;&#x308A;&#x3085;&#x3046;</pr></trd>,
    <trd>&#x5B9F;  <pr>&#x305D;&#x3046;&#x3058;&#x307F;</pr></trd>,
    <trd>&#x7A2E;&#x5B50;  <pr>&#x3057;&#x3085;&#x3046;&#x3058;</pr></trd>,
    <trd>&#x7C92;  <pr>&#x3064;&#x3076;</pr></trd>,
    <trd>&#x5FAE;&#x91CF;  <pr>&#x3073;&#x308A;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>gr&atilde;o</trd>,
    <trd>semente</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>granula</trd>,
    <trd>zrno</trd>
  </trdgrp>
  <trd lng="tr">tane<klr>(bu&gbreve;day ve benzeri)</klr></trd>
  <trd lng="uk">&#x437;&#x435;&#x440;&#x43D;&#x43E;&#x432;&#x456;&#xA0;&#x432;&#x456;&#x434;&#x445;&#x43E;&#x434;&#x438;</trd>
  <trdgrp lng="zh">
    <trd>&#x7A40;&#x7C92; <pr>g&#x1D4;l&#xEC;</pr></trd>,
    <trd>&#x7C92; <pr>l&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="grajn.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="grajn.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="lst" lst="voko:man&gcirc;a&jcirc;oj" cel="mangx.0ajxo">Man&gcirc;a&jcirc;o</ref> 
      el krude pistitaj <tld/>oj:
      <ekz>
        <tld/>a&jcirc;on el fre&scirc;aj <tld/>oj alportu kiel farunoferon
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 2:14</lok>
        </fnt>;
      </ekz>
    </dif>
    <ref tip="sin" cel="gri.0ajxo">gria&jcirc;o</ref>
    <refgrp tip="vid">
      <ref cel="musli.0o">muslio</ref>
    </refgrp>
    <trd lng="de">Gr&uuml;tze</trd>
    <trd lng="en">groat <klr>(grain)</klr></trd>
  </snc>
  <trd lng="uk">&#x437;&#x435;&#x440;&#x43D;&#x43E;&#x432;&#x456;&#xA0;&#x432;&#x456;&#x434;&#x445;&#x43E;&#x434;&#x438;</trd>
</drv>

<drv mrk="grajn.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="grajn.0igi.KOMUNE">
    <dif>
      Fari <tld/>ojn el ia maso:
      <ekz>
        estis instalitaj novaj aparatoj, ekzemple por <klr>[&#x2026;]</klr> <tld/>igo de
        parafina vakso
        <fnt>
          <bib>Monato</bib>
          <aut>Wu Guojiang</aut>
          <vrk><url ref="&Monato;/2008/009548.php">Rafinejo jubileas</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="dis.0igi">disigi</ref>,
       <ref cel="er.0igi">erigi</ref>,
       <ref cel="hak.0i.trancxi">haki</ref>,
       <ref cel="rasp.0i">raspi</ref>,
       <ref cel="split.0i">spliti</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>zerkleinern</trd>,
      <trd>granulieren</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x9846;&#x7C92;&#x72C0; <pr>k&#x113;l&#xEC;zhu&#xE0;ng</pr></trd>,
    <trd>&#x5236;&#x7C92; <pr>zh&#xEC;l&#xEC;</pr></trd>,
    <trd>&#x9020;&#x7C92; <pr>z&#xE0;ol&#xEC;</pr></trd>,
    <trd>&#x7C92;&#x5316; <pr>l&#xEC;hu&#xE0;</pr></trd>,
    <trd>&#x4F7F;&#x6210;&#x9846;&#x7C92; <pr>sh&#x1D0;ch&#xE9;ngk&#x113;l&#xEC;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_r;&c_a;&c_b;&c_n;&c_ib;&c_c;&c_mol; <klr>(&c_u; &c_k;&c_r;&c_u;&c_p;&c_u;)</klr></trd>,
    <trd>&c_g;&c_r;&c_a;&c_n;&c_u;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>drobit</trd>,
    <trd>granulovat</trd>
  </trdgrp>
  <trd lng="en">granulate</trd>
  <trdgrp lng="fr">
    <trd>concasser</trd>,
    <trd>grainer<klr tip="ind"> (r&eacute;duire en grains)</klr></trd>,
    <trd>grener<klr tip="ind"> (r&eacute;duire en grains)</klr></trd>
  </trdgrp>
  <trd lng="hu">granul&aacute;l</trd>
  <trd lng="ja">&#x7C92;&#x72B6;&#x306B;&#x3059;&#x308B;  <pr>&#x3064;&#x3076;&#x3058;&#x3087;&#x3046;&#x306B;&#x3059;&#x308B;</pr></trd>
  <trd lng="nl">verpulveren</trd>
  <trdgrp lng="pl">
    <trd>rozdrabnia&cacute;</trd>,
    <trd>granulowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>granar<klr tip="amb"> (reduzir a gr&atilde;os)</klr></trd>,
    <trd>granular<klr tip="amb"> (reduzir a gr&atilde;os)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_m;&c_je;&c_l;&c_mol;&c_ch;&c_a;&c_t;&c_mol; &c_v; &c_k;&c_r;&c_u;&c_p;&c_u;</trd>
  <trdgrp lng="sk">
    <trd>drobi&tcaron;</trd>,
    <trd>granulova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x440;&#x43E;&#x437;&#x434;&#x440;&#x456;&#x431;&#x43D;&#x44E;&#x432;&#x430;&#x442;&#x438; &#x432; &#x437;&#x435;&#x440;&#x43D;&#x430;</trd>,
    <trd>&#x443;&#x442;&#x432;&#x43E;&#x440;&#x44E;&#x432;&#x430;&#x442;&#x438; &#x437;&#x435;&#x440;&#x43D;&#x430;</trd>
  </trdgrp>
</drv>

