<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>grajn</rad>/o
</kap>
<drv mrk="grajn.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="grajn.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      &Ccirc;iu frukto de la <ref tip="malprt" cel="gren.0o.planto">greno</ref>
      entenata en la spiko kaj uzata por fari farunon:
      <ekz>
        <tld/>o de tritiko, aveno, rizo, pizo, pipro;
      </ekz>
      <ekz>
        <tld/>a&jcirc;o el fre&scirc;aj <tld/>oj
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Levidoj 2, 14</lok>)</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_z;&c_je;&c_r;&c_n;&c_o;</trd>,
      <trd>&c_z;&c_jo;&c_r;&c_n;&c_y;&c_sh;&c_k;&c_o;</trd>
    </trdgrp>
  </snc>
  <snc mrk="grajn.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <ref tip="dif" cel="grajn.sem0o">Sem<tld/>o</ref>
    <trdgrp lng="ru">
      <trd>&c_z;&c_je;&c_r;&c_n;&c_o;</trd>,
      <trd>&c_z;&c_jo;&c_r;&c_n;&c_y;&c_sh;&c_k;&c_o;</trd>
    </trdgrp>
  </snc>
  <snc mrk="grajn.0o.kerno">
    <dif>
      Malgranda <ref tip="dif" cel="kern.0o.BOT">kerno</ref> de kernofruktoj,
      ekz. de pomo, piro.
    </dif>
    <trdgrp lng="ru">
      <trd>&c_z;&c_jo;&c_r;&c_n;&c_y;&c_sh;&c_k;&c_o;</trd>,
      <trd>&c_k;&c_o;&c_s;&c_t;&c_o;&c_ch;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="grajn.0o.globeto">
    <dif>
      Malgranda malmola globa a&jcirc;o, komparebla al <tld/>o:
      <ekz>
        <tld/>o de hajlo, pulvo;
      </ekz>
      <ekz>
        sur la fundo de la rivero trovi&gcirc;as <tld/>oj da oro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>eto da vitro <klr>[penetris]</klr> en lian okulon
        <fnt>(<aut>Andersen, trad. Zamenhof</aut>: <vrk>Fabeloj,
        Ne&gcirc;a re&gcirc;ino</vrk>)</fnt>;
      </ekz>
      <ekz>
        <tld/>etoj de sablo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>oj de kolzono.
      </ekz>
    </dif>
    <refgrp tip="super">
      <ref cel="er1.0o">ero</ref>.
    </refgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_u;&c_p;&c_i;&c_c;&c_a;</trd>,
      <trd>&c_k;&c_r;&c_u;&c_p;&c_i;&c_n;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="grajn.0o.iometo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Iometo da:
      <ekz>
        estas tamen en la afero <tld/>o da vero;
      </ekz>
      <ekz>
        li ne havas en la kapo e&ccirc; unu <tld/>on da sa&gcirc;o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gut.0o">guto</ref>,
      <ref cel="ombr.0o">ombro</ref>.
    </refgrp>
    <trd lng="ru">&c_k;&c_r;&c_u;&c_p;&c_i;&c_c;&c_a;</trd>
  </snc>
  <trd lng="de">Korn</trd>
  <trdgrp lng="fr">
    <trd>grain</trd>,
    <trd>graine</trd>,
    <trd>p&eacute;pin</trd>,
    <trd>goutte<klr tip="ind"> (petite quantit&eacute;)</klr></trd>,
    <trd>soup&ccedil;on<klr tip="ind"> (petite quantit&eacute;)</klr></trd></trdgrp> 
</drv>
<drv mrk="grajn.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari <tld/>ojn el ia maso.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>grainer<klr tip="ind"> (r&eacute;duire en grains)</klr></trd>,
    <trd>grener<klr tip="ind"> (r&eacute;duire en grains)</klr></trd></trdgrp> 
  <trd lng="ru">&c_r;&c_a;&c_z;&c_m;&c_je;&c_l;&c_mol;&c_ch;&c_a;&c_t;&c_mol; &c_v; &c_k;&c_r;&c_u;&c_p;&c_u;</trd>
</drv>
<drv mrk="grajn.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Kreski aperigante sem<tld/>ojn.
    </dif>
  </snc>
  <trd lng="fr">monter en <ind>graine</ind></trd>
  <trd lng="ru">&c_z;&c_r;&c_je;&c_t;&c_mol;</trd>
</drv>
<drv mrk="grajn.dis0igi">
  <kap>dis<tld/>igi</kap>
  <snc>
    <dif>
      De&scirc;iri la <tld/>ojn:
      <ekz>
        dis<tld/>igi spikon, vinberaron.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>&eacute;grainer<klr tip="ind"> (d&eacute;tacher les grains)</klr></trd>,
    <trd>&eacute;grener<klr tip="ind"> (d&eacute;tacher les grains)</klr></trd></trdgrp> 
</drv>
<drv mrk="grajn.0oledo">
  <kap><tld/>oledo</kap>
  <snc>
    <dif>
      Malglata ledo, kvaza&ubreve; kovrita per <tld/>oj.
    </dif>
    <ref tip="sin" cel="sxagri.0o">&scirc;agrino.</ref>
  </snc>
  <trdgrp lng="fr">
    <trd>cuir grenu</trd>,
    <trd>chagrin<klr tip="ind"> (cuir)</klr></trd></trdgrp> 
</drv>
<drv mrk="grajn.sem0o">
  <kap>sem<tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="prt" cel="embri.0o">Embrio</ref> de floroplanto
      kun nutra&jcirc;o kaj gardanta &scirc;elo, entenata
      en la <ref tip="malprt" cel="frukt.0o.BOT">frukto</ref>.
      La sem<tld/>o esti&gcirc;as el fekundi&gcirc;inta
      <ref cel="ovul.0o">ovulo</ref> kaj produktas la
      <ref cel="gxerm.0o">&gcirc;ermon</ref>.
    </dif>
  </snc>
  <trd lng="de">Samenkorn</trd>
  <trd lng="fr">graine<klr tip="ind"> (bot.)</klr></trd>
  <trd lng="ru">&c_s;&c_je;&c_m;&c_ja;</trd>
</drv>
</art>
