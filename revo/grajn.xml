<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>grajn</rad>/o
</kap>
<drv mrk="grajn.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="grajn.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      &Ccirc;iu frukto de la <ref tip="malprt" cel="gren.0o">greno</ref>
      entenata en la spiko kaj uzata por fari farunon:
      <ekz>
        <tld/>o de tritiko, aveno, rizo, pizo, pipro;
      </ekz>
      <ekz>
        <tld/>a&jcirc;o el fre&scirc;aj <tld/>oj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="grajn.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <ref tip="dif" cel="grajn.sem0o">sem<tld/>o</ref>
  </snc>
  <snc mrk="grajn.0o.kerno">
    <dif>
      Malgranda <ref tip="dif" cel="kern.0o.BOT">kerno</ref> de kernofruktoj, ekz.
      de pomo, piro.
    </dif>
  </snc>
  <snc mrk="grajn.0o.globeto">
    <dif>
      Malgranda malmola globa a&jcirc;o, komparebla al <tld/>o:
      <ekz>
        <tld/>o de hajlo, pulvo;
      </ekz>
      <ekz>
        sur la fundo de la rivero trovi&gcirc;as <tld/>oj da oro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>eto da vitro penetris en lian okulon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>etoj de sablo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>oj de kolzono.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="er1.0o">ero</ref>.
    </refgrp>
  </snc>
  <snc mrk="grajn.0o.iometo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Iometo da:
      <ekz>
        estas tamen en la afero <tld/>o da vero;
      </ekz>
      <ekz>
        li ne havas en la kapo e&ccirc; unu <tld/>on da sa&gcirc;o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="gut.0o">guto</ref>,
      <ref cel="ombr.0o">ombro</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Korn</trd>
</drv>
<drv mrk="grajn.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari <tld/>ojn el ia maso.
    </dif>
  </snc>
</drv>
<drv mrk="grajn.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Kreski aperigante sem<tld/>ojn.
    </dif>
  </snc>
</drv>
<drv mrk="grajn.dis0igi">
  <kap>dis<tld/>igi</kap>
  <snc>
    <dif>
      De&scirc;iri la <tld/>ojn:
      <ekz>
        dis<tld/>igi spikon, vinberaron.
      </ekz>
    </dif>
  </snc>
</drv>
<drv mrk="grajn.0oledo">
  <kap><tld/>oledo</kap>
  <snc>
    <dif>
      Malglata ledo, kvaza&ubreve; kovrita per <tld/>oj; &scirc;agrino.
    </dif>
  </snc>
</drv>
<drv mrk="grajn.sem0o">
  <kap>sem<tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="prt" cel="embri.0o">Embrio</ref> de floroplanto
      kun nutra&jcirc;o kaj gardanta &scirc;elo, entenita
      en la <ref tip="malprt" cel="frukt.0o.BOT">frukto</ref>.
      La sem<tld/>o esti&gcirc;as el fekundi&gcirc;inta
      <ref cel="ovul.0o">ovulo</ref> kaj produktas la 
      <ref cel="gxerm.0o">&gcirc;ermon</ref>.
    </dif>
  </snc>
  <trd lng="de">Samenkorn</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
