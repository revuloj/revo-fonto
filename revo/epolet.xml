<art mrk="$Id: epolet.xml,v 1.23 2023/11/07 20:34:11 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>epolet</rad>/o
</kap>
<drv mrk="epolet.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="epolet.0o.TEKS">
    <uzo tip="fak">MIL</uzo>
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Militista sur&scirc;ultra ornama&jcirc;o, ordinare <ref tip="super" cel="insign.0o.signo">insigno</ref> montranta la rangon:
      <ekz>
        balda&ubreve; rekoninte armeajn <tld/>ojn, ili forturnis sin kun
        indigno
        <fnt><bib>Mary</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_g;&c_o;&c_n;</trd>,
    <trd>&c_e;&c_p;&c_a;&c_l;&c_je;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>epoleta</trd>,
    <trd>n&#xE1;ramen&#xED;k</trd>,
    <trd>v&#xFD;lo&#x17E;ka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Achselst&uuml;ck</trd>,
    <trd>Schulterst&uuml;ck</trd>,
    <trd>Schulterklappe</trd>,
    <trd>Epaulette</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>epaulet</trd>,
    <trd>epaulette</trd>
  </trdgrp>
  <trd lng="es">divisa<klr> (insignia militar indicando
  rango)</klr></trd>
  <trd lng="fr">&eacute;paulette</trd>
  <trdgrp lng="hu">
    <trd>v&aacute;llrojt</trd>,
    <trd>v&aacute;llbojt</trd>,
    <trd>epolett</trd>
  </trdgrp>
  <trd lng="it">spallina<klr> (mil.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x80A9;&#x7AE0;  <pr>&#x3051;&#x3093;&#x3057;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x80A9;&#x98FE;&#x308A;  <pr>&#x304B;&#x305F;&#x304B;&#x3056;&#x308A;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>epaulet</trd>,
    <trd>schouderstuk<klr tip="ind"> (mil.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pagon</trd>,
    <trd>epolet</trd>
  </trdgrp>
  <trd lng="pt">dragona</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_g;&c_o;&c_n;</trd>,
    <trd>&c_e;&c_p;&c_o;&c_l;&c_je;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>n&#xE1;plecn&#xED;k</trd>,
    <trd>v&#xFD;lo&#x17E;ka</trd>
  </trdgrp>
  <trd lng="sv">axelklaff</trd>
  <trdgrp lng="uk">
    <trd>&#x435;&#x43F;&#x43E;&#x43B;&#x435;&#x442;</trd>,
    <trd>&#x43F;&#x43E;&#x433;&#x43E;&#x43D;</trd>
  </trdgrp>
  <trd lng="zh">&#x80A9;&#x7AE0; <pr>ji&#x101;nzh&#x101;ng</pr></trd>
</drv>
</art>
