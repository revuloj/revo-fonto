  <trd lng="hu">metrikus <ind>t&eacute;r</ind></trd>
  <trd lng="pl">przestrze&nacute; metryczna</trd>
  <trd
  lng="ru">&c_m;&c_je;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je;
  <ind>&c_p;&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;</ind></trd>
