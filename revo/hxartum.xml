<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hxartum.xml,v 1.5 2022/04/30 08:42:42 revo Exp $">
<kap>
  <rad>&Hcirc;artum</rad>/o,
  <var><kap><rad var="K">Kartum</rad>/o
    <fnt><bib>PIV1</bib></fnt></kap>
  </var>
</kap>
<drv mrk="hxartum.0o">
  <kap><tld/>o, <var><kap><tld var="K"/>o</kap></var></kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="hxartum.0o.GEOG">
    <dif>
      <ref tip="lst" lst="voko:&ccirc;efurboj"
      cel="urb.cxef0o.POL">&Ccirc;efurbo</ref> de <ref tip="malprt"
      cel="sudan.S0o">Sudano</ref> (<frm>32&#730;35'E, 15&#730;36'N</frm>):
      <ekz>
        brita instruistino <klr>[&#8230;]</klr> &ccirc;e elementa lernejo en <tld/>o, Sudano, estis
        arestita, &ccirc;ar &scirc;i permesis, ke plu&scirc;urso uzata dum instruado portu la nomon
        Muhamado
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2008/009392.php"
              >La plu&scirc;urso kaj la profeto</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekde 1982 instruis li en <tld var="K"/>o kaj en diversaj oficiraj lernejoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=49117"
              >John Garang</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ar">&a_A;&a_l;&a_kh;&a_r;&a_T;&a_w;&a_m;</trd>
  <trd lng="ca">Khartum</trd>
  <trd lng="de">Khartum</trd>
  <trdgrp lng="en">
    <trd>Khartoum</trd>
    <trd>Khartum</trd>
  </trdgrp>
  <trd lng="es">Jartum</trd>
  <trd lng="fr">Khartoum</trd>
  <trd lng="nl">Khartoem</trd>
  <trd lng="pl">Chartum</trd>
  <trd lng="pt">Cartum</trd>
  <trd lng="ru">&c_H;&c_a;&c_r;&c_t;&c_u;&c_m;</trd>
  <trd lng="sv">Khartoum</trd>
  <trd lng="uk">&c_H;&c_a;&c_r;&c_t;&c_u;&c_m;</trd>
</drv>
</art>

