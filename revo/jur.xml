<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc> <ofc>3</ofc>
<rad>jur</rad>/o
</kap>
<drv mrk="jur.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <snc mrk="jur.0o.jus">
    <dif>
      Aro da reguloj, kiuj difinas la <ref tip="vid"
      cel="rajt.0o">rajtojn</ref> kaj devojn de la homoj a&ubreve;
      kolektiva&jcirc;oj, kaj kies malrespekto povas sekvigi socian formon de
      <ref tip="vid" cel="jugx.0i">ju&gcirc;o</ref> a&ubreve; puno:
      <ekz>
        kutima <tld/>o <klr>(difinita de la kutimoj)</klr>;
      </ekz>
      <ekz>
        romia <tld/>o;
      </ekz>
      <ekz>
        <ref cel="kanon.0a.JUR">kanona</ref> <tld/>o;
      </ekz>
      <ekz>
        <ind>internacia <tld/>o</ind><trd lng="lat">ius gentium</trd>;
      </ekz>
      <ekz>
        pozitiva <tld/>o <klr>(difinita de la <ref tip="vid"
        cel="legx.0o">le&gcirc;oj</ref> a&ubreve; aliaj normaj tekstoj)</klr>;
      </ekz>
      <ekz>
        natura <tld/>o <klr>(principoj, valoroj, kiuj ekzistas ekster la
        pozitiva <tld/>o, per kiuj oni povas ju&gcirc;i ion <ref tip="vid"
        cel="just.0a">justa</ref> a&ubreve; maljusta, <ref tip="vid"
        cel="prav.0a">prava</ref> a&ubreve; malprava: <ref tip="vid"
        cel="moral.0o">moralo</ref>)</klr>;
      </ekz>
      <ekz>
        <tld/>filozofio;
      </ekz>
      <ekz>
        <tld/>historio;
      </ekz>
      <ekz>
        <tld/>sociologio;
      </ekz>
      <ekz>
        <tld/>oscienco.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Recht</trd>
  <trd lng="en">law</trd>
  <trd lng="fr">droit</trd>
  <trd lng="lat">ius</trd>
  <trd lng="nl">recht</trd>
  <trd lng="pt">lei</trd>
  <trd lng="ru">&c_p;&c_r;&c_a;&c_v;&c_o;</trd>
</drv>

<drv mrk="jur.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">JUR</uzo>
  <snc mrk="jur.0a.ghen">
    <dif>
      Koncernanta la <tld/>on:
      <ekz>
        <tld/>aj konsiloj;
      </ekz>
      <ekz>
        <tld/>aj normoj;
      </ekz>
      <ekz>
        <tld/>aj sciencoj.
      </ekz>
    </dif>
    <trd lng="de">rechtlich</trd>
    <trdgrp lng="pt">
      <trd>jur&iacute;dico</trd>,
      <trd>legal</trd>
    </trdgrp>
  </snc>
  <snc mrk="jur.0a.jus">
    <dif>
      Difinita de la <tld/>o:
      <ekz>
        <ind><tld/>a persono</ind><trd lng="fr">personnalit&eacute;
        juridique</trd>;
      </ekz>
      <ekz>
        <tld/>a protekto.
      </ekz>
    </dif>
    <trd lng="de">juristisch</trd>
  </snc>
  <trd lng="fr">juridique</trd>
  <trd lng="nl">juridisch</trd>
  <trd lng="pt">jur&iacute;dico</trd>
  <trd lng="ru">&c_p;&c_r;&c_a;&c_v;&c_o;&c_v;&c_o;&c_j;</trd>
</drv>

<drv mrk="jur.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Fakulo pri <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Jurist</trd>
  <trd lng="fr">juriste</trd>
  <trd lng="nl">jurist</trd>
  <trdgrp lng="pt">
    <trd>jurista</trd>,
    <trd>jurisconsulto</trd>
  </trdgrp>
  <trd lng="ru">&c_ju;&c_r;&c_i;&c_s;&c_t;</trd>
</drv>
</art>
