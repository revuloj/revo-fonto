<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kelidoni</rad>/o
</kap>
<drv mrk="kelido.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="papava.0oj">papavacoj</ref>
      (<trd lng="la">Chelidonium</trd>),
      kies suko estas oran&gcirc;flava kaj ka&ubreve;stika:
      <ekz>
        la ka&ubreve;stika suko de la <tld/>o estas uzata kontra&ubreve; la
        verukoj.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/kelido.jpg">
    <tld/>o <klr>(Chelidonium majus)</klr>
    <trd lng="hu">v&eacute;rehull&oacute; fecskef&udblac;</trd>
  </bld>
  <trd lng="en">greater celadine</trd>
  <trd lng="de">Sch&ouml;llkraut</trd>
  <trd lng="hu">fecskef&udblac;</trd>
  <trd lng="nl">stinkende <ind>gouwe</ind></trd>
  <trd lng="ru">&c_ch;&c_i;&c_s;&c_t;&c_o;&c_t;&c_je;&c_l;</trd>
</drv>
</art>
