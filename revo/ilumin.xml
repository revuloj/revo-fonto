<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ilumin</rad>/i
</kap>
<drv mrk="ilumin.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ornami per lumiloj domojn, stratojn, parkojn, kc, okaze de festo.  
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>beleuchten</trd>,
    <trd>anstrahlen</trd>
  </trdgrp>
  <trd lng="en">illuminate</trd>
  <trd lng="fr">illuminer<klr tip="ind"> (dans un but d&eacute;coratif)</klr></trd>
  <trd lng="nl">verlichten</trd>
  <trd lng="ru">&c_i;&c_l;&c_l;&c_ju;&c_m;&c_i;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="ilumin.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Ornama festa lumigado.
    </dif>
  </snc>
  <trd lng="fr">illumination<klr tip="ind"> (d&eacute;corative)</klr></trd>
  <trd lng="nl">verlichting</trd>
  <trd lng="ru">&c_i;&c_l;&c_l;&c_ju;&c_m;&c_i;&c_n;&c_a;&c_c;&c_i;&c_ja;</trd>
</drv>
<drv mrk="ilumin.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Ornama lumilo. 
    </dif>
    <refgrp tip="vid">
      <ref cel="fajr">artfajra&jcirc;o</ref>,
      <ref cel="lanter.0o">lanterno</ref>,
      <ref cel="lampio.0o">lampiono</ref>.
    </refgrp>
  </snc>
</drv>
</art>
