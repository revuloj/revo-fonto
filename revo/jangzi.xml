<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: jangzi.xml,v 1.8 2025/12/17 02:21:09 afido Exp $">
    <kap>
      <rad>Jangzi</rad>/o
    </kap>

<drv mrk="jangzi.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
    <dif>
      Granda <ref tip="lst" lst="voko:riveroj" cel="river.0o.GEOG">rivero</ref>
      en la Orienta Azio, en <ref tip="malprt" cel="cxin.CX0ujo">&Ccirc;inujo</ref>,
      la plej longa en E&ubreve;razio (6300&#x202F;km), fluanta
      orienten, de Tibeto en la Orientan &Ccirc;inan Maron:
      <ekz>
        la ruino atestas, ke anka&ubreve; la baseno de <tld/>o estas fonto de la &ccirc;ina
        civilizacio
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="&Monato;/2009/009901.php">Enigmoj de Sanxingdui</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o fontas en la alto de &ccirc;irka&ubreve; 5.500 m sur la Tibeta Altebena&jcirc;o
        <fnt>
          <vrk><url ref="&Viki;/Jangzio">Jangzio - Vikipedio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="river.Blua_0o">Blua Rivero</ref>
    <trdgrp lng="be">
      <trd>&c_Ja;&c_n;&c_d;&c_z;&c_y;</trd>,
      <trd>&c_B;&c_l;&c_a;&c_k;&c_ib;&c_t;&c_n;&c_a;&c_ja;
  &c_r;&c_a;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="ca">Iang-Ts&eacute;</trd>
    <trdgrp lng="de">
      <trd>Jangtsekiang</trd>,
      <trd>Jangtse-Fluss</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>Yangtze Kiang</trd>,
      <trd>Chang Jiang</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>Chang jiang</trd>,
      <trd>Yang-tseu-kiang</trd>,
      <trd>Yangzi jiang</trd>
    </trdgrp>
    <trd lng="id"><baz>sungai</baz>Sungai Yangtze</trd>
    <trd lng="nl">Jangtsekiang</trd>
    <trd lng="pl">Jangcy</trd>
    <trdgrp lng="ru">
      <trd>&c_Ja;&c_n;&c_c;&c_z;&c_y;</trd>,
      <trd>&c_G;&c_o;&c_l;&c_u;&c_b;&c_a;&c_ja; &c_r;&c_je;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <rim>
    Malofte oni renkontas anka&ubreve; la literumon &leftquot;Jangceo&rightquot;.
  </rim>
  <trd lng="ja">&#x63DA;&#x5B50;&#x6C5F;  <pr>&#12424;&#12358;&#12377;&#12371;&#12358;</pr></trd>
  <trd lng="uk">&#x42F;&#x43D;&c_c;&c_z;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#38263;&#x6C5F; <pr>Ch&#xE1;ngJi&amacron;ng</pr></trd>,
    <trd>&#38263;&#x6C5F; <pr>ch&#xE1;ngji&amacron;ng</pr></trd>
  </trdgrp>
</drv>
</art>

