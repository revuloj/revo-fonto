<vortaro>
<art mrk="$Id: klerik1.xml,v 1.19 2023/10/18 16:50:28 revo Exp $">
<kap>
  <rad>klerikal</rad>/a <fnt>B</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="klerik1.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="klerik1.0a.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="klerik.0isma.POL">Klerikisma</ref>:
      <ekz>
        <tld/>a partio;
      </ekz>
      <ekz>
        <ind>kontra&ubreve;<tld/>a</ind> agitado.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_je;&c_r;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trd lng="ca">clerical</trd>
  <trd lng="cs">klerik&aacute;ln&iacute;</trd>
  <trdgrp lng="de">
    <trd>klerikal</trd>,
    <trd>geistlich</trd>,
    <trd>kirchenfreundlich</trd>
  </trdgrp>
  <trd lng="fr">cl&eacute;rical<klr tip="ind"> (favorable au
  cl&eacute;ricalisme)</klr></trd>
  <trd lng="hu">klerik&aacute;lis</trd>
  <trdgrp lng="ja">
    <trd>&#x6559;&#x6A29;&#x64C1;&#x8B77;&#x306E;  <pr>&#x304D;&#x3087;&#x3046;&#x3051;&#x3093;&#x3088;&#x3046;&#x3054;&#x306E;</pr></trd>,
    <trd>&#x8056;&#x8077;&#x8005;&#x4E3B;&#x7FA9;&#x306E;  <pr>&#x305B;&#x3044;&#x3057;&#x3087;&#x304F;&#x3057;&#x3083;&#x3057;&#x3085;&#x304E;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">klerikaal</trd>
  <trd lng="pl">klerykalny</trd>     
  <trd lng="ro">clerical</trd>
  <trd lng="ru">&c_k;&c_l;&c_je;&c_r;&c_i;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">klerik&aacute;lny</trd>
  <trd lng="uk">&#x43A;&#x43B;&#x435;&#x440;&#x438;&#x43A;&#x430;&#x43B;&#x44C;&#x43D;&#x438;&#x439;</trd>
</drv>

<drv mrk="klerik1.0ismo">
  <kap><tld/>ismo</kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="klerik1.0ismo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="klerik.0ismo.POL">Klerikismo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_je;&c_r;&c_y;&c_k;&c_a;&c_l;&c_ib;&c_z;&c_m;</trd>
  <trd lng="ca">clericalisme</trd>
  <trdgrp lng="de">
    <trd>Klerikalismus</trd>,
    <trd>Kirchenfreundlichkeit</trd>
  </trdgrp>
  <trd lng="fr">cl&eacute;ricalisme</trd>
  <trd lng="hu">klerikalizmus</trd>  
  <trdgrp lng="ja">
    <trd>&#x6559;&#x6A29;&#x4E3B;&#x7FA9;  <pr>&#x304D;&#x3087;&#x3046;&#x3051;&#x3093;&#x3057;&#x3085;&#x304E;</pr></trd>,
    <trd>&#x8056;&#x8077;&#x8005;&#x4E2D;&#x5FC3;&#x4E3B;&#x7FA9;  <pr>&#x305B;&#x3044;&#x3057;&#x3087;&#x304F;&#x3057;&#x3083;&#x3061;&#x3085;&#x3046;&#x3057;&#x3093;&#x3057;&#x3085;&#x304E;</pr></trd>
  </trdgrp>
  <trd lng="nl">clericalisme</trd>
  <trd lng="pl">klerykalizm</trd>     
  <trd lng="ro">clericalism</trd>
  <trd lng="ru">&c_k;&c_l;&c_je;&c_r;&c_i;&c_k;&c_a;&c_l;&c_i;&c_z;&c_m;</trd>
  <trd lng="uk">&#x43A;&#x43B;&#x435;&#x440;&#x438;&#x43A;&#x430;&#x43B;&#x456;&#x437;&#x43C;</trd>
</drv>
</art>
