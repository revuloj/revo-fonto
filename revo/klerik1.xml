<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>klerikal</rad>/a
  <fnt>B</fnt>
</kap>
<drv mrk="klerik1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Favora al la klerikaro kaj al &gcirc;ia superregado:
      <ekz>
        <ind><tld/>a</ind> partio;
        <trd lng="ru">&c_k;&c_l;&c_je;&c_r;&c_i;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
      </ekz>
      <ekz>
        <ind>kontra&ubreve;<tld/>a</ind> agitado.
        <trd lng="ru">&c_a;&c_n;&c_t;&c_i;&c_k;&c_l;&c_je;&c_r;&c_i;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="ru">&c_k;&c_l;&c_je;&c_r;&c_i;&c_k;&c_a;&c_l;</trd>
</drv>
<drv mrk="klerik1.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      Politiko favora al la klerikaro.  
    </dif>
  </snc>
  <trd lng="ru">&c_k;&c_l;&c_je;&c_r;&c_i;&c_k;&c_a;&c_l;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
