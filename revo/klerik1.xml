<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>klerikal</rad>/a
  <fnt>B</fnt>
</kap>
<drv mrk="klerik1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Favora al la klerikaro kaj al &gcirc;ia superregado:
      <ekz>
        <tld/>a partio;
      </ekz>
      <ekz>
        <ind>kontra&ubreve;<tld/>a</ind> agitado.
        <trdgrp lng="fr">
           <trd>anticl&eacute;rical</trd>
           <trd>anticl&eacute;ricale</trd>
           <trd>anticl&eacute;ricaux</trd>
        </trdgrp>
        <trd lng="nl">antiklerikaal</trd>
        <trdgrp lng="de">
           <trd>antiklerikal</trd>,
           <trd>kirchenfeindlich</trd>
        </trdgrp>
        <trd
lng="ru">&c_a;&c_n;&c_t;&c_i;&c_k;&c_l;&c_je;&c_r;&c_i;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>klerikal</trd>,
     <trd>geistlich</trd>,
     <trd>kirchenfreundlich</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cl&eacute;rical</trd>
    <trd>cl&eacute;ricale</trd>
    <trd>cl&eacute;ricaux</trd>
  </trdgrp>
  <trd lng="nl">klerikaal</trd>
  <trd
lng="ru">&c_k;&c_l;&c_je;&c_r;&c_i;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>
<drv mrk="klerik1.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      Politiko favora al la klerikaro.  
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Klerikalismus</trd>,
     <trd>Kirchenfreundlichkeit</trd>
  </trdgrp>
  <trd lng="fr">cl&eacute;ricalisme</trd>
  <trd lng="nl">clericalisme</trd>
  <trd lng="ru">&c_k;&c_l;&c_je;&c_r;&c_i;&c_k;&c_a;&c_l;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
