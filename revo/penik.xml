<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: penik.xml,v 1.25 2022/05/01 17:51:19 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>penik</rad>/o <fnt><bib>FK</bib></fnt>
</kap>
<drv mrk="penik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="penik.0o.TEK">
    <dif>
      Ilo el <ref tip="prt" cel="fask.0o.KOMUNE">fasko</ref> da haroj
      kunligitaj &ccirc;e ekstremo de bastoneto, uzata por skribi,
      &scirc;miri per kolorilo, gluo, kuracilo ks:
      <ekz>
        &scirc;mirado per <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la skribisto prenis inkujon kaj <tld/>on kaj en da&ubreve;ro de nelonga tempo skribis
        dumaniere: Amon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> sinjorino febre sin &jcirc;etis al la spegulo kaj, malferminte kelke
        da potetoj, komencis ru&gcirc;e kolorigi sian viza&gcirc;on per <tld/>o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        eblas farbi &gcirc;in per gua&scirc;o kaj tre fajna <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Sylvain Lelarge</aut>
          <vrk><url ref="&Monato;/2004/008496.html">La argilaj najbaretoj de Jesuo provenca tradicio</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_e;&c_n;&c_d;&c_z;&c_a;&c_l;&c_mol;</trd>,
      <trd>&c_k;&c_v;&c_a;&c_ch;</trd>
    </trdgrp>
    <trd lng="bg">&c_ch;&c_je;&c_t;&c_k;&c_a; &c_z;&c_a; &c_r;&c_i;&c_s;&c_u;&c_v;&c_a;&c_n;&c_je;</trd>
    <trdgrp lng="ca">
      <trd>pinzell</trd>,
      <trd>brotxa</trd>
    </trdgrp>
    <trd lng="de">Pinsel</trd>
    <trdgrp lng="en">
      <trd>paintbrush</trd>,
      <trd>brush<klr> (art)</klr></trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>pincel</trd>,
      <trd>brocha</trd>
    </trdgrp>
    <trd lng="fr">pinceau</trd>
    <trd lng="hu">ecset</trd>
    <trd lng="nl">penseel</trd>
    <trd lng="pl">p&eogonek;dzel</trd>
    <trd lng="pt">pincel</trd>
    <trd lng="sv">pensel</trd>
  </snc>
  <snc mrk="penik.0o.kvasto">
    <dif>
      <ref tip="dif" cel="kvast.0o">Kvasto</ref>:
      <ekz>
        li ku&scirc;is en la belega lito kun la longaj veluraj kurtenoj kaj la pezaj oraj
        <tld/>oj
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz>
      <ekz>
        veluraj tapi&scirc;oj kaj grandaj oraj <tld/>oj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fi&scirc;kaptisto kaj lia edzino</lok>
        </fnt>;
      </ekz>
      <ekz>
        puf-se&gcirc;oj <klr>[&#x2026;]</klr> kun bluaj atlasaj kovriloj kaj amaso da nekompreneblaj
        la&ccirc;a&jcirc;oj kaj <tld/>a&jcirc;oj
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_u;&c_t;&c_a;&c_s;</trd>
    <trdgrp lng="ca">
      <trd>borla</trd>,
      <trd>plomall</trd>
    </trdgrp>
    <trd lng="de">Quaste</trd>
    <trd lng="en">tassel</trd>
    <trdgrp lng="es">
      <trd>borla</trd>,
      <trd>penacho</trd>
    </trdgrp>
    <trd lng="hu">bojt</trd>
    <trd lng="nl">kwast</trd>
    <trdgrp lng="pl">
      <trd>fr&eogonek;dzel</trd>,
      <trd>chwost<klr> (przest.)</klr></trd>
    </trdgrp>
    <trd lng="pt">borla</trd>
  </snc>
  <snc mrk="penik.0o.beraro">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <refgrp tip="dif">
        <ref cel="ber.0aro">Beraro</ref>,
        <ref cel="grapol.0o">grapolo</ref>
      </refgrp>:
      <ekz>
        ili detran&ccirc;is tie bran&ccirc;on kun unu <tld/>o da vinberoj
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 13:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        li disdonis <klr>[&#x2026;]</klr> al &ccirc;iu po unu <klr>[&#x2026;]</klr> <tld/>o da sekvinberoj
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 6:19</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_g;&c_r;&c_o;&c_n;&c_k;&c_a;</trd>
    <trd lng="ca">ra&iuml;m</trd>
    <trd lng="de">Traube</trd>
    <trd lng="en">cluster</trd>
    <trd lng="es">racimo</trd>
    <trdgrp lng="pl">
      <trd>grono</trd>,
      <trd>ki&sacute;&cacute;</trd>
    </trdgrp>
    <trd lng="nl">tros<klr tip="ind"> (ontr.)</klr></trd>
  </snc>
  <trd lng="bo">&#xF5E;&#xF0B;&#xF66;&#xF99;&#xF74;&#xF42;&#xF0B;</trd>
  <trd lng="cs">&scaron;t&ecaron;tec</trd>
  <trd lng="ru">&c_k;&c_i;&c_s;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>kystka</trd>,
    <trd>strapec</trd>,
    <trd>&scaron;tetec</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x435;&#x43D;&#x437;&#x435;&#x43B;&#x44C;</trd>,
    <trd>&#x449;&#x456;&#x442;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>

<drv mrk="penik.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="penik.0i.TEK">
    <dif>
      Uzi <tld/>on por pentri, glui ks.:
      <ekz>
        vi ver&scirc;ajne <tld/>is per jodo lian a&ccirc;an viza&gcirc;on
        <fnt>
          <vrk><url ref="http://miresperanto.com/biblioteko/okupacio/02.htm">02 - miresperanto.com</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_z;&c_a;&c_c;&c_mol;
    <klr tip="amb">(&c_p;&c_e;&c_n;&c_d;&c_z;&c_l;&c_je;&c_m;)</klr>
    </trd>
  </trdgrp>
  <trd lng="ca">pinzellar</trd>
  <trd lng="cs">nat&iacute;rat &scaron;t&ecaron;tcem</trd>
  <trd lng="de">pinseln</trd>
  <trdgrp lng="en">
    <trd>paint</trd>,
    <trd>use a paintbrush</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>pintar</trd>,
    <trd>pincelar</trd>
  </trdgrp>
  <trd lng="fr">peindre<klr> (au pinceau)</klr></trd>
  <trdgrp lng="hu">
    <trd>ecsetel</trd>,
    <trd>beken <ind>ecsettel</ind></trd>
  </trdgrp>
  <trd lng="nl">penselen</trd>
  <trdgrp lng="pl">
    <trd>p&eogonek;dzlowa&cacute;</trd>,
    <trd>malowa&cacute; <ind>p&eogonek;dzlem</ind></trd>
  </trdgrp>
  <trd lng="ru">
  &c_m;&c_a;&c_z;&c_a;&c_t;&c_mol;
  <ind>&c_k;&c_i;&c_s;&c_t;&c_mol;&c_ju;</ind></trd>
  <trd lng="sk">natiera&tcaron; &scaron;tetcom</trd>
  <trd lng="sv">pensla</trd>
</drv>
</art>
