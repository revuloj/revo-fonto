<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>penik</rad>/o
</kap>
<drv mrk="penik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Densa tufo da haroj kunligitaj &ccirc;e ekstremo de bastoneto, uzata por
      &scirc;miri per kolorilo, gluo, kuracilo ks.
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_e;&c_n;&c_d;&c_z;&c_a;&c_l;&c_mol;</trd>,
      <trd>&c_k;&c_v;&c_a;&c_ch;</trd>
    </trdgrp>
    <trd lng="hu">ecset</trd>    
    <trd lng="nl">penseel</trd>
  </snc>
  <snc>
    <dif>
      <ref tip="dif" cel="kvast.0o">Kvasto</ref>:
      <ekz>
        li svingis la <tld/>ojn de sia vesto<fnt>B</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_u;&c_t;&c_a;&c_s;</trd>
    <trd lng="hu">bojt</trd>    
    <trd lng="nl">kwast</trd>
  </snc>
  <snc>
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="ber.0aro">Beraro</ref>, <ref tip="dif" cel="grapol.0o">grapolo</ref>:
      <ekz>
        li disdonis al &ccirc;iuj po unu <tld/>o da sekvinberoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_g;&c_r;&c_o;&c_n;&c_k;&c_a;</trd>
  </snc>
  <trd lng="ru">&c_k;&c_i;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="penik.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Uzi <tld/>on por pentri, glui ks.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_z;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_p;&c_e;&c_n;&c_d;&c_z;&c_l;&c_je;&c_m;)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ecsetel</trd>,
    <trd>beken <ind>ecsettel</ind></trd>
  </trdgrp>
  <trd lng="nl">penselen</trd>
  <trd lng="ru">&c_m;&c_a;&c_z;&c_a;&c_t;&c_mol; <ind>&c_k;&c_i;&c_s;&c_t;&c_mol;&c_ju;</ind></trd>
</drv>
</art>
