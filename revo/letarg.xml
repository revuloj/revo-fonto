<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: letarg.xml,v 1.20 2022/03/22 18:26:51 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>letargi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="letarg.0o">
  <kap><tld/>o</kap>
  <snc mrk="letarg.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Stato de &scirc;ajna morto, karakterizata per preska&ubreve; tuta halto de
      spirado kaj de sanga cirkulado.
    </dif>
    <refgrp tip="vid">
      <ref cel="dorm.0o">dormo</ref>,
      <ref cel="komat.0o">komato</ref>
    </refgrp>
    <trdgrp lng="de">
       <trd>Lethargie</trd>,
       <trd>Schlafsucht</trd>
    </trdgrp>
    <trd lng="en">torpidity</trd>
    <trd lng="hu">&aacute;lomk&oacute;r</trd>    
    <trd lng="nl">schijndood</trd>
  </snc>
  <snc mrk="letarg.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plena <ref tip="super" cel="inert.0eco.psika">inerteco</ref>
      kaj <ref tip="super" cel="indife.0eco">indiferenteco</ref>:
      <ekz>
        amba&ubreve; &ccirc;i tiuj fenomenoj, la varmego kaj la <tld/>o de l' insektoj anta&ubreve;diras
        fulmotondron
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Ramzes</nom> sidi&gcirc;is apud li kaj post momento kvaza&ubreve; falis en <tld/>on, kredeble li
        ekdormis
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
       <trd>Lethargie</trd>,
       <trd>Teilnahmslosigkeit</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>lethargy</trd>,
      <trd>drowsiness</trd>,
      <trd>inertness</trd>,
      <trd>apathy</trd>
    </trdgrp>
    <trdgrp lng="hu">
       <trd>leverts&eacute;g</trd>,
       <trd>letargia</trd>
    </trdgrp>    
    <trdgrp lng="nl">
      <trd>lethargie</trd>,
      <trd>onverschilligheid</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_l;&c_je;&c_t;&c_a;&c_r;&c_g;&c_ib;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>letargie</trd>,
    <trd>strnulost</trd>
  </trdgrp>
  <trdgrp lng="el">
    <trd>&lambda;&eta;&theta;&alfa;&rho;&gamma;&jota_ton;&alfa;</trd>,
    <trd>&lambda;&eta_ton;&theta;&alfa;&rho;&gamma;&omikron;&sigma_fina;</trd>
  </trdgrp>
  <trd lng="es">letargo</trd>
  <trd lng="fr">l&eacute;thargie</trd>
  <trd lng="pl">letarg</trd>
  <trd lng="pt">letargia</trd>
  <trd lng="ru">&c_l;&c_je;&c_t;&c_a;&c_r;&c_g;&c_i;&c_ja;</trd>
  <trd lng="sk">letargia</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x435;&#x442;&#x430;&#x440;&#x433;&#x456;&#x44F;</trd>,
    <trd>&#x43B;&#x435;&#x442;&#x430;&#x440;&#x433;&#x456;&#x447;&#x43D;&#x438;&#x439; &#x441;&#x43E;&#x43D;</trd>
  </trdgrp>
</drv>
</art>
