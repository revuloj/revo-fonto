<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>letargi</rad>/o
</kap>
<drv mrk="letarg.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Stato de &scirc;ajna morto, karakterizata per preska&ubreve; tuta halto de
      spirado kaj de sanga cirkulado.
    </dif>
    <refgrp tip="sin">
      <ref cel="dorm.0o">dormo</ref>.
    </refgrp>
    <trdgrp lng="de">
       <trd>Lethargie</trd>,
       <trd>Schlafsucht</trd>
    </trdgrp>
    <trd lng="en">torpidity</trd>
    <trd lng="hu">tetszhal&aacute;l</trd>    
    <trd lng="nl">schijndood</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plena inerteco kaj indiferenteco.
    </dif>
    <trdgrp lng="de">
       <trd>Lethargie</trd>,
       <trd>Teilnahmslosigkeit</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>lethargy</trd>,
      <trd>drowsiness</trd>,
      <trd>inertness</trd>,
      <trd>apathy</trd>
    </trdgrp>
    <trdgrp lng="hu">
       <trd>leverts&eacute;g</trd>,
       <trd>letargia</trd>
    </trdgrp>    
    <trdgrp lng="nl">
      <trd>lethargie</trd>,
      <trd>onverschilligheid</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_l;&c_je;&c_t;&c_a;&c_r;&c_g;&c_ib;&c_ja;</trd>
  <trdgrp lng="el">
    <trd>&lambda;&eta;&theta;&alfa;&rho;&gamma;&jota_ton;&alfa;</trd>,
    <trd>&lambda;&eta_ton;&theta;&alfa;&rho;&gamma;&omikron;&sigma_fina;</trd>
  </trdgrp>
  <trd lng="es">letargo</trd>
  <trd lng="pt">letargia</trd>
  <trd lng="ru">&c_l;&c_je;&c_t;&c_a;&c_r;&c_g;&c_i;&c_ja;</trd>
</drv>
</art>
