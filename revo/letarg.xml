<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>letargi</rad>/o
</kap>
<drv mrk="letarg.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Stato de &scirc;ajna morto, karakterizata per preska&ubreve; tuta halto de
      spirado kaj de sanga cirkulado.
    </dif>
    <refgrp tip="sin">
      <ref cel="dorm.0o">dormo</ref>.
    </refgrp>
    <trd lng="en">torpidity</trd>
    <trd lng="nl">schijndood</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plena inerteco kaj indiferenteco.
    </dif>
    <trdgrp lng="en">
      <trd>lethargy</trd>,
      <trd>drowsiness</trd>,
      <trd>inertness</trd>,
      <trd>apathy</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>lethargie</trd>,
      <trd>onverschilligheid</trd>
    </trdgrp>
  </snc>
  <trd lng="es">letargo</trd>
  <trd lng="ru">&c_l;&c_je;&c_t;&c_a;&c_r;&c_g;&c_i;&c_ja;</trd>
</drv>
</art>
