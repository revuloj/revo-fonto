<vortaro>
<art mrk="$Id: rujx.xml,v 1.8 2014/06/10 14:10:15 revo Exp $">
<kap><rad>ru&jcirc;</rad>/o <fnt><bib>PIV1</bib></fnt></kap>
<drv mrk="rujx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Lipa <ref tip="super" cel="sxmink.0o">&scirc;minko</ref> ordinare ru&gcirc;a
      kaj konservata kiel solida entuba bastoneto:
      <ekz>
        pudro, &scirc;minko, <tld/>o kaj pomado
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 67a, p. 431a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_b;&c_n;&c_a;&c_ja; <ind>&c_p;&c_a;&c_m;&c_a;&c_d;&c_a;</ind></trd>
  <trd lng="de">Lippenrouge</trd>
  <trdgrp lng="fr">
    <trd>rouge &agrave; l&egrave;vres</trd>
  </trdgrp>
  <trd lng="hu">r&uacute;zs</trd>
  <trdgrp lng="id">
    <trd>gincu</trd>,
    <trd>lipstik</trd>
  </trdgrp>
  <trd lng="nl">lippenstift</trd>
  <trd lng="pt">batom</trd>
  <trd lng="ru">&c_g;&c_u;&c_b;&c_n;&c_a;&c_ja; <ind>&c_p;&c_o;&c_m;&c_a;&c_d;&c_a;</ind></trd>
</drv>
</art>
