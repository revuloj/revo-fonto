<art mrk="$Id: rujx.xml,v 1.14 2024/04/14 06:28:51 revo Exp $">
<kap><rad>ru&jcirc;</rad>/o <fnt><bib>PIV1</bib></fnt></kap>
<drv mrk="rujx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Lipa <ref tip="super" cel="sxmink.0o">&scirc;minko</ref> ordinare ru&gcirc;a
      kaj konservata kiel solida entuba bastoneto:
      <ekz>
        pudro, &scirc;minko, <tld/>o kaj pomado
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 67a, p. 431a</lok></fnt>;
      </ekz>
      <ekz>
        strikta jupo helbruna, ne tro mallonga, helflava longmanika bluzo, la novaj
        &scirc;ikaj &scirc;uoj, nur iom da <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2006/008760.php">En fremda lando</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_b;&c_n;&c_a;&c_ja; <ind>&c_p;&c_a;&c_m;&c_a;&c_d;&c_a;</ind></trd>
  <trd lng="de">Lippenrouge</trd>
  <trd lng="en">lipstick</trd>
  <trdgrp lng="fr">
    <trd>rouge &agrave; l&egrave;vres</trd>
  </trdgrp>
  <trd lng="hu">r&uacute;zs</trd>
  <trdgrp lng="id">
    <trd>gincu</trd>,
    <trd>lipstik</trd>
  </trdgrp>
  <trd lng="nl">lippenstift</trd>
  <trd lng="pl">r&oacute;&zdot;</trd>
  <trd lng="pt">batom</trd>
  <trd lng="ru">&c_g;&c_u;&c_b;&c_n;&c_a;&c_ja; <ind>&c_p;&c_o;&c_m;&c_a;&c_d;&c_a;</ind></trd>
  <trd lng="uk">&c_r;&c_u;&c_m;&#x2019;&c_ja;&c_n;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x53E3;&#x7D05; <pr>k&#x1D2;uh&#xF3;ng</pr></trd>,
    <trd>&#x53E3;&#x7EA2; <pr>k&#x1D2;uh&#xF3;ng</pr></trd>
  </trdgrp>
</drv>
