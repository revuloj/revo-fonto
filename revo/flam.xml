  <trdgrp lng="nl">
    <trd>ontvlammen</trd>,
    <trd>ontbranden</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>arder</trd>,
    <trd>quemarse</trd>
  </trdgrp>
  <trd lng="pl">zap&lstroke;on&aogonek;&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_zh;&c_je;&c_ch;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_o;&c_r;&c_je;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_o;&c_s;&c_p;&c_y;&c_l;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="flam.0igxema">
  <kap><tld/>i&gcirc;ema</kap>
  <snc>
    <dif>
      Facile <tld/>anta:
      <ekz>
        <tld/>i&gcirc;ema substanco;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i&gcirc;ema karaktero<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_o;&c_g;&c_n;&c_je;&c_n;&c_je;&c_b;&c_ja;&c_s;&c_p;&c_je;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;&c_mol;&c_ch;&c_y;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">inflamable</trd>
  <trdgrp lng="de">
    <trd>entz&uuml;ndlich</trd>,
    <trd>brennbar</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>flammable</trd>,
    <trd>inflammable</trd>
  </trdgrp>
  <trd lng="fr">inflammable</trd>
  <trd lng="he">&dalet;&lamed;&yod;&qof;</trd>
  <trdgrp lng="hu">
    <trd>gy&uacute;l&eacute;kony</trd>,
    <trd>lobban&eacute;kony</trd>
  </trdgrp>
  <trd lng="nl">brandbaar</trd>
  <trd lng="es">inflamable</trd>
  <trd lng="pl">&lstroke;atwopalny</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_je;&c_g;&c_k;&c_o; &c_v;&c_o;&c_s;&c_p;&c_l;&c_a;&c_m;&c_je;&c_n;&c_ja;&c_ju;&c_shch;&c_i;&c_j;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_o;&c_s;&c_p;&c_l;&c_a;&c_m;&c_je;&c_n;&c_i;&c_m;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_g;&c_n;&c_je;&c_o;&c_p;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_s;&c_p;&c_y;&c_l;&c_mol;&c_ch;&c_i;&c_v;&c_y;&c_j;
    </trd>
  </trdgrp>
</drv>

<drv mrk="flam.0ingo">
  <kap><tld/>ingo</kap>
  <snc>
    <dif>
      Ekstrema&jcirc;o de tubeto, de kiu brulas la gaso.
    </dif>
    <refgrp tip="vid">
      <ref cel="gas.0beko">gasbeko</ref>
    </refgrp>
  </snc>
  <adm>
    Kontrastigi rilate al "brulilo".
    <aut>MB</aut>
  </adm>
  <trd lng="ca">cremador</trd>
  <trdgrp lng="de">
    <trd>Brenner</trd>,
    <trd>Brennd&uuml;se</trd>
  </trdgrp>
  <trd lng="en">burner</trd>
  <trd lng="fr">br&ucirc;leur</trd>
  <trd lng="he">&mem;&bet;&ayin;&resh;</trd>
  <trdgrp lng="hu">
    <trd>&eacute;g&odblac;fej</trd>,
    <trd>g&aacute;zr&oacute;zsa</trd>
  </trdgrp>
