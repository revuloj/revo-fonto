<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: flam.xml,v 1.38 2023/11/09 20:48:33 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>flam</rad>/o
</kap>
<drv mrk="flam.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="flam.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Hela brilo, produktata de gasoj eli&gcirc;antaj el brulanta
      materialo:
      <ekz>
        <tld/>o de ligno, karbo, gaso, papero, lampo, fajro, incendio;
      </ekz>
      <ekz>
        blue brulanta <tld/>o <klr>[&#x2026;]</klr> lumigis la tutan salonon
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;ielo lumis, kvaza&ubreve; &gcirc;i estus en <tld/>oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam la <tld/>o estingi&gcirc;is kaj de la papero restis nur la cindro
        <fnt>
          <bib>Fab3</bib>
          <lok>Lino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj aperis sub la tegmento, la seka pajlo ekbrulis
        <fnt>
          <bib>Fab2</bib>
          <lok>Najbaraj familioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la brullignaro, avide la <tld/>oj el&scirc;ovas siajn langojn supren &ccirc;irka&ubreve; la
        virino
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la alumeto eligis varman helan <tld/>on
        <fnt>
          <bib>Fab2</bib>
          <lok>Knabineto kun alumetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la neregula movi&gcirc;ado de la kandela <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Dano Holger</lok>
        </fnt>;
      </ekz>
      <ekz>
        malgranda estas la <tld/>o, tamen ne mankas la fumo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        el <tld/>o sin eltiris, en fajron eniris
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_o;&c_l;&c_y;&c_m;&c_ja;</trd>
    <trdgrp lng="ca">
      <trd>flama</trd>,
      <trd>branda</trd>
    </trdgrp>
    <trd lng="de">Flamme</trd>
    <trd lng="hu">l&aacute;ng</trd>
    <trd lng="nl">vlam</trd>
  <trdgrp lng="pt">
    <trd>chama</trd>,
    <trd>flama</trd>,
    <trd>labareda</trd>
  </trdgrp>
    <trd lng="ru">&c_p;&c_l;&c_a;&c_m;&c_ja;</trd>
    <trdgrp lng="zh">
    <trd>&#x720C; <pr>ku&#x1CE;ng</pr></trd>,
    <trd>&#x706B;&#x82D7; <pr>hu&#x1D2;mi&#xE1;o</pr></trd>,
    <trd>&#x706B;&#x7130; <pr>hu&#x1D2;y&#xE0;n</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="flam.0o.fervoro">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref cel="ekscit.0o">Eksciteco</ref>, <ref cel="ard.0o.pasio">ardo</ref>, 
      <ref cel="pasi.0o">pasio</ref>:
      <ekz>
        <tld/>o de kolero
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 29:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj okuloj estis kiel fajra <tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 1:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de la koro
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekzalt">ekzalteco</ref>,
      <ref cel="entuzi.0o">entuziasmo</ref>,
      <ref cel="fajr.0o.fervoro">fajro<sncref/></ref>,
      <ref cel="fervor.0o">fervoro</ref>,
      <ref cel="verv.0o">vervo</ref>,
      <ref cel="vigl">vigleco</ref>
    </refgrp>
    <trd lng="be">&c_z;&c_a;&c_p;&c_a;&c_l;</trd>
    <trd lng="de">Feuer</trd>
    <trdgrp lng="hu">
      <trd>izz&aacute;s<klr tip="ind"> (&aacute;tv.)</klr></trd>,
      <trd>hev&uuml;let</trd>,
      <trd>l&aacute;ngol&aacute;s<klr tip="ind"> (&aacute;tv.)</klr>
      </trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>deler</trd>,
      <trd>fervor</trd>
    </trdgrp>
    <trd lng="nl">vuur<klr tip="ind"> (fig.)</klr></trd>
  <trdgrp lng="pt">
    <trd>paix&atilde;o <klr tip="amb">(ardente)</klr></trd>,
    <trd>chama</trd>,
    <trd>flama</trd>
  </trdgrp>
    <trd lng="ru">&c_p;&c_y;&c_l;</trd>
  </snc>
  <trd lng="cs">plamen</trd>
  <trd lng="en">flame</trd>
  <trd lng="es">flama</trd>
  <trd lng="fr">flamme</trd>
  <trdgrp lng="he">
    <trd>&lamed;&he;&bet;&he;</trd>,
    <trd>&lamed;&he;&tet;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x708E;  <pr>&#x3048;&#x3093;</pr></trd>,
    <trd>&#x706B;&#x708E;  <pr>&#x304B;&#x3048;&#x3093;</pr></trd>,
    <trd>&#x60C5;&#x71B1;&#x306E;&#x708E;  <pr>&#x3058;&#x3087;&#x3046;&#x306D;&#x3064;&#x306E;&#x3048;&#x3093;</pr></trd>,
    <trd>&#x6FC0;&#x60C5;  <pr>&#x3052;&#x304D;&#x3058;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="pl">p&lstroke;omie&nacute;</trd>
  <trd lng="sk">plame&ncaron;</trd>
  <trd lng="tr">alev</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_l;&c_u;&c_m;&#x2019;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_s;&c_t;&c_r;&c_a;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="flam.0a">
  <kap><tld/>a</kap>
  <snc mrk="flam.0a.FIZ">
    <dif>
      Brulanta kun <tld/>o, aspektanta kiel <tld/>o:
      <ekz>
        elkuras flam<tld/>aj fajreroj
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 41:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a kurteno <klr>[&#x2026;]</klr> stari&gcirc;as subite, kiel baro netransirebla,
        <klr>[&#x2026;]</klr> Viktoro sin sentis pereonta
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;io kunkunfandi&gcirc;is en <tld/>aj koloroj
        <fnt>
          <bib>Fab2</bib>
          <lok>Sonorilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a brilo de liaj pupiloj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia anta&ubreve;a paleco cedis la lokon al <tld/>a ru&gcirc;o
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_l;&c_y;&c_m;&c_n;&c_je;&c_ju;&c_ch;&c_y;</trd>,
      <trd>&c_p;&c_a;&c_l;&c_y;&c_m;&c_ja;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>flam&iacute;ger</trd>,
      <trd>flamejant</trd>
    </trdgrp>
    <trd lng="de">flammend</trd>
    <trd lng="hu">l&aacute;ngol&oacute;</trd>
    <trd lng="nl">vlammend</trd>
    <trd lng="pl">p&lstroke;on&aogonek;cy</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_a;&c_m;&c_je;&c_n;&c_je;&c_ju;&c_shch;&c_i;&c_j;</trd>,
      <trd>&c_p;&c_y;&c_l;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x715D; <pr>m&#xE8;i</pr></trd>,
    <trd>&#x5192;&#x706B;&#x7130; <pr>m&#xE0;ohu&#x1D2;y&#xE0;n</pr></trd>,
    <trd>&#x53D1;&#x4EAE;&#x5149; <pr>f&#x101;li&#xE0;nggu&#x101;ng</pr></trd>,
    <trd>&#x7143; <pr>ku&#x1D0;</pr></trd>,
    <trd>&#x71E1; <pr>y&#xEC;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="flam.0a.anime">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="sin" cel="ard.0a.fervora">Arda</ref>, <ref tip="sin" cel="pasi.0a">pasia</ref>:
      <ekz>
        <tld/>a amo
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun <tld/>a elokventeco li defendas la planon de sia amiko
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a fanatikeco
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a kolero
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 32:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a furiozo min kaptas
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 119:53</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> <tld/>a junulo, varmege, pasie &scirc;in amas
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi sentas min forta, kiel <tld/>a &ccirc;evalo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        Venero, tiel vivoplena kaj <tld/>a, kiel Ticiano tion vidis
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Mefres</nom> silentis kaj fikse rigardis la <tld/>ajn okulojn de la Greko
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a indigno metis tiun malbelan vorton en la bu&scirc;on de via servisto
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        karbe nigraj <tld/>aj okuloj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        tute egale, &ccirc;u li estas malamiko de Esperanto a&ubreve; &gcirc;ia plej <tld/>a amiko
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista 
          en Washington en la 15a de a&ubreve;gusto 1910</vrk>
        </fnt>;
      </ekz>
      <ekz>
        du koroj trapikitaj de <tld/>aj sagoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        li skribadis al mi <klr>[&#x2026;]</klr> leterojn sopiregajn kaj <tld/>ajn
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_l;&c_y;&c_m;&c_ja;&c_n;&c_y;</trd>,
      <trd>&c_p;&c_a;&c_l;&c_k;&c_ib;</trd>,
      <trd>&c_z;&c_a;&c_w;&c_z;&c_ja;&c_t;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>fog&oacute;s</trd>,
      <trd>fervent</trd>,
      <trd>en&egrave;rgic</trd>
    </trdgrp>
    <trd lng="de">feurig</trd>
    <trd lng="en">fervent</trd>
    <trdgrp lng="hu">
      <trd>l&aacute;ngol&oacute;<klr tip="ind"> (&aacute;tv.)</klr>
      </trd>,
      <trd>izz&oacute;<klr tip="ind"> (&aacute;tv.)</klr></trd>
    </trdgrp>
    <trd lng="nl">vurig</trd>
    <trd lng="pl">p&lstroke;omienny</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_a;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_p;&c_y;&c_l;&c_k;&c_i;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>horliv&yacute;</trd>,
    <trd>plamenn&yacute;</trd>,
    <trd>planouc&iacute;</trd>
  </trdgrp>
  <trd lng="en">flaming</trd>
  <trd lng="es">ardiente</trd>
  <trd lng="fr">enflamm&eacute;</trd>
  <trdgrp lng="he">
    <trd>&bet;&vav;&ayin;&resh;</trd>,
    <trd>&lamed;&vav;&he;&tet;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x708E;&#x306E;  <pr>&#x3048;&#x3093;&#x306E;</pr></trd>,
    <trd>&#x708E;&#x3092;&#x3042;&#x3052;&#x308B;  <pr>&#x3048;&#x3093;&#x3092;&#x3042;&#x3052;&#x308B;</pr></trd>,
    <trd>&#x708E;&#x306E;&#x3088;&#x3046;&#x306A;  <pr>&#x3048;&#x3093;&#x306E;&#x3088;&#x3046;&#x306A;</pr></trd>,
    <trd>&#x6FC0;&#x3057;&#x3044;  <pr>&#x306F;&#x3052;&#x3057;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="sk">plamenn&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_l;&c_u;&c_m;&#x2019;&c_ja;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_u;&c_m;&#x2019;&c_ja;&c_n;&c_ib;&c_ju;&c_ch;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_a;&c_ju;&c_ch;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="flam.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="flam.0i.KOMUNE">
    <dif>
      Esti en <tld/>a stato:
      <ekz>
        <tld/>is fajro tia, ke oni povus rosti sur &gcirc;i tutan bovon
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>is &ccirc;iuj kandeloj de lustro kaj de kandelabroj
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la suno <tld/>is sur &ccirc;iam sennuba &ccirc;ielo
        <fnt>
          <bib>HsT</bib>
          <lok>La nepo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la norda brilo <tld/>is super la glacimontoj de la nordo
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la juvelaj magazenoj kiel bluaj, ru&gcirc;aj, verdaj fajroj, <tld/>as gemoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>is klara fajro el bran&ccirc;etoj
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ard.0i">ardi</ref>,
      <ref cel="bol.0i">boli</ref>,
      <ref cel="brul.0i">bruli</ref>,
      <ref cel="fajr.0i.KOMUNE">fajri<sncref/></ref>,
      <ref cel="flagr.0i">flagri</ref>
    </refgrp>
  </snc>
  <snc mrk="flam.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Elmontri fortan pasion; esti en plena ekscito:
      <ekz>
        lia kolero <tld/>as
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 30:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        tro multe da nigra sango kolekti&gcirc;is en via koro kaj tial via kapo <tld/>as
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun miro ili rigardas unu alian, iliaj viza&gcirc;oj <tld/>as
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 13:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj okuloj <tld/>is
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> kantisto de <nom>Astarte</nom> <klr>[&#x2026;]</klr> pasie <tld/>is al
        <nom>Kama</nom>
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i per honto anta&ubreve; si mem
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj piedoj tiel tremis, &scirc;ia kapo tiel <tld/>is kaj la brusto tiel doloris, ke
        &scirc;i falis sur se&gcirc;on
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fajr.0i.flagri">fajri<sncref/></ref>,
      <ref cel="febr.0i.anime">febri<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_y;&c_m;&c_n;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&t_ba;&t_ra;&t_tsheg;</trd>
  <trdgrp lng="ca">
    <trd>cremar <klr>(intr.)</klr></trd>,
    <trd>flamejar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ho&rcaron;et</trd>,
    <trd>pl&aacute;polat</trd>,
    <trd>pl&aacute;t</trd>
  </trdgrp>
  <trd lng="de">brennen</trd>
  <trdgrp lng="en">
    <trd>burn</trd>,
    <trd>flame</trd>
  </trdgrp>
  <trd lng="es">arder</trd>
  <trdgrp lng="fr">
    <trd>br&ucirc;ler</trd>,
    <trd>flamber</trd>,
    <trd>flamboyer</trd>
  </trdgrp>
  <trd lng="he">&lamed;<ind>&bet;&ayin;&vav;&resh;</ind></trd>
  <trd lng="hu">l&aacute;ngol</trd>
  <trd lng="ja">&#x708E;&#x3092;&#x4E0A;&#x3052;&#x3066;&#x71C3;&#x3048;&#x308B;  <pr>&#x3048;&#x3093;&#x3092;&#x3042;&#x3052;&#x3066;&#x3082;&#x3048;&#x308B;</pr></trd>
  <trdgrp lng="nl">
    <trd>vlammen</trd>,
    <trd>branden</trd>
