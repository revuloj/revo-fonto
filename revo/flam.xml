<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: flam.xml,v 1.29 2021/05/28 12:14:58 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>flam</rad>/o
</kap>
<drv mrk="flam.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="flam.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Hela brilo, produktata de gasoj eli&gcirc;antaj el brulanta
      materialo:
      <ekz>
        <tld/>o de ligno, karbo, gaso, papero, lampo, fajro, incendio;
      </ekz><ekz>
        esti en <tld/>oj <klr>(brulegi)</klr>;
      </ekz><ekz>
        trafi el la fajro sub <tld/>on<fnt>Z</fnt>
        <klr>(fali en ankora&ubreve; pli malbonan situacion)</klr>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_o;&c_l;&c_y;&c_m;&c_ja;</trd>
    <trdgrp lng="ca">
      <trd>flama</trd>,
      <trd>branda</trd>
    </trdgrp>
    <trd lng="de">Flamme</trd>
    <trd lng="hu">l&aacute;ng</trd>
    <trd lng="nl">vlam</trd>
  <trdgrp lng="pt">
    <trd>chama</trd>,
    <trd>flama</trd>,
    <trd>labareda</trd>
  </trdgrp>
    <trd lng="ru">&c_p;&c_l;&c_a;&c_m;&c_ja;</trd>
  </snc>
  <snc mrk="flam.0o.fervoro">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Eksciteco, ardo, pasio:
      <ekz>
        en la <tld/>o de kolero.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ard.0o">ardo</ref>,
      <ref cel="ekzalt">ekzalteco</ref>,
      <ref cel="entuzi.0o">entuziasmo</ref>,
      <ref cel="fajr.0o.fervoro">fajro</ref>,
      <ref cel="fervor.0o">fervoro</ref>,
      <ref cel="pasi.0o">pasio</ref>,
      <ref cel="verv.0o">vervo</ref>,
      <ref cel="vigl">vigleco</ref>
    </refgrp>
    <trd lng="be">&c_z;&c_a;&c_p;&c_a;&c_l;</trd>
    <trd lng="de">Feuer</trd>
    <trdgrp lng="hu">
      <trd>izz&aacute;s<klr tip="ind"> (&aacute;tv.)</klr></trd>,
      <trd>hev&uuml;let</trd>,
      <trd>l&aacute;ngol&aacute;s<klr tip="ind"> (&aacute;tv.)</klr>
      </trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>deler</trd>,
      <trd>fervor</trd>
    </trdgrp>
    <trd lng="nl">vuur<klr tip="ind"> (fig.)</klr></trd>
  <trdgrp lng="pt">
    <trd>paix&atilde;o <klr tip="amb">(ardente)</klr></trd>,
    <trd>chama</trd>,
    <trd>flama</trd>
  </trdgrp>
    <trd lng="ru">&c_p;&c_y;&c_l;</trd>
  </snc>
  <trd lng="cs">plamen</trd>
  <trd lng="en">flame</trd>
  <trd lng="es">flama</trd>
  <trd lng="fr">flamme</trd>
  <trdgrp lng="he">
    <trd>&lamed;&he;&bet;&he;</trd>,
    <trd>&lamed;&he;&tet;</trd>
  </trdgrp>
  <trd lng="pl">p&lstroke;omie&nacute;</trd>
  <trd lng="sk">plame&ncaron;</trd>
  <trd lng="tr">alev</trd>
</drv>

<drv mrk="flam.0a">
  <kap><tld/>a</kap>
  <snc mrk="flam.0a.FIZ">
    <dif>
      Brulanta kun <tld/>o.
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_l;&c_y;&c_m;&c_n;&c_je;&c_ju;&c_ch;&c_y;</trd>,
      <trd>&c_p;&c_a;&c_l;&c_y;&c_m;&c_ja;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>flam&iacute;ger</trd>,
      <trd>flamejant</trd>
    </trdgrp>
    <trd lng="de">flammend</trd>
    <trd lng="hu">l&aacute;ngol&oacute;</trd>
    <trd lng="nl">vlammend</trd>
    <trd lng="pl">p&lstroke;on&aogonek;cy</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_a;&c_m;&c_je;&c_n;&c_je;&c_ju;&c_shch;&c_i;&c_j;</trd>,
      <trd>&c_p;&c_y;&c_l;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="flam.0a.anime">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Arda, pasia:
      <ekz>
        <tld/>a &ccirc;evalo;
      </ekz><ekz>
        <tld/>a deziro, kapo<fnt>Z</fnt>,
      </ekz><ekz>
        amiko de Esperanto<fnt>Z</fnt>;
      </ekz><ekz>
        <tld/>a amikeco, stilo;
      </ekz><ekz>
        <tld/>aj okuloj.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_l;&c_y;&c_m;&c_ja;&c_n;&c_y;</trd>,
      <trd>&c_p;&c_a;&c_l;&c_k;&c_ib;</trd>,
      <trd>&c_z;&c_a;&c_w;&c_z;&c_ja;&c_t;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>fog&oacute;s</trd>,
      <trd>fervent</trd>,
      <trd>en&egrave;rgic</trd>
    </trdgrp>
    <trd lng="de">feurig</trd>
    <trd lng="en">fervent</trd>
    <trdgrp lng="hu">
      <trd>l&aacute;ngol&oacute;<klr tip="ind"> (&aacute;tv.)</klr>
      </trd>,
      <trd>izz&oacute;<klr tip="ind"> (&aacute;tv.)</klr></trd>
    </trdgrp>
    <trd lng="nl">vurig</trd>
    <trd lng="pl">p&lstroke;omienny</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_a;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_p;&c_y;&c_l;&c_k;&c_i;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>horliv&yacute;</trd>,
    <trd>plamenn&yacute;</trd>,
    <trd>planouc&iacute;</trd>
  </trdgrp>
  <trd lng="en">flaming</trd>
  <trd lng="es">ardiente</trd>
  <trd lng="fr">enflamm&eacute;</trd>
  <trdgrp lng="he">
    <trd>&bet;&vav;&ayin;&resh;</trd>,
    <trd>&lamed;&vav;&he;&tet;</trd>
  </trdgrp>
  <trd lng="sk">plamenn&yacute;</trd>
</drv>

<drv mrk="flam.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="flam.0i.KOMUNE">
    <dif>
      Esti en <tld/>a stato:
      <ekz>
        pro nesingardo de iu fumanto la arbaro <tld/>as;
      </ekz><ekz>
        <tld/>as la ru&gcirc;o de l' sunlevi&gcirc;o;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i per kolero<fnt>Z</fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i por virino<fnt>B</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ard.0i">ardi</ref>,
      <ref cel="bol.0i">boli</ref>,
      <ref cel="brul.0i">bruli</ref>,
      <ref cel="febr.0i.anime">febri <sncref/></ref>,
      <ref cel="flagr.0i">flagri</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_y;&c_m;&c_n;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&#xF56;&#xF62;&#xF0B;</trd>
  <trdgrp lng="ca">
    <trd>cremar <klr>(intr.)</klr></trd>,
    <trd>flamejar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ho&rcaron;et</trd>,
    <trd>pl&aacute;polat</trd>,
    <trd>pl&aacute;t</trd>
  </trdgrp>
  <trd lng="de">brennen</trd>
  <trdgrp lng="en">
    <trd>burn</trd>,
    <trd>flame</trd>
  </trdgrp>
  <trd lng="es">arder</trd>
  <trdgrp lng="fr">
    <trd>br&ucirc;ler</trd>,
    <trd>flamber</trd>,
    <trd>flamboyer</trd>
  </trdgrp>
  <trd lng="he">&lamed;<ind>&bet;&ayin;&vav;&resh;</ind></trd>
  <trd lng="hu">l&aacute;ngol</trd>
  <trdgrp lng="nl">
    <trd>vlammen</trd>,
    <trd>branden</trd>
  </trdgrp>
  <trd lng="pl">p&lstroke;on&aogonek;&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_y;&c_l;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_m;&c_je;&c_n;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>horie&tcaron;</trd>,
    <trd>pl&aacute;pola&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="flam.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="flam.0igi.KOMUNE">
    <dif>
      Meti en <tld/>an staton:
      <ekz>
        <ind>ek<tld/>igi</ind> fajron, lampon;
        <trd lng="ca">encendre, botar foc</trd>
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>igi deziron, ribelemon en la koroj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_p;&c_a;&c_l;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>inflamar</trd>,
    <trd>incendiar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>rozn&iacute;tit</trd>,
    <trd>rozp&aacute;lit</trd>,
    <trd>zap&aacute;lit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>anz&uuml;nden</trd>,
    <trd>entfachen</trd>,
    <trd>entflammen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>kindle</trd>,
    <trd>light</trd>,
    <trd>inflame</trd>
  </trdgrp>
  <trd lng="es">quemar</trd>
  <trdgrp lng="fr">
    <trd>allumer</trd>,
    <trd>enflammer</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&lamed;<ind>&he;&dalet;&lamed;&yod;&qof;</ind></trd>,
    <trd>&lamed;<ind>&he;&bet;&ayin;&yod;&resh;</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>l&aacute;ngra lobbant</trd>,
    <trd>meggy&uacute;jt</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>aansteken</trd>,
    <trd>doen <ind>branden</ind></trd>,
    <trd>doen <ind>ontvlammen</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_zh;&c_je;&c_ch;&c_mol;</trd>,
    <trd>&c_v;&c_o;&c_s;&c_p;&c_l;&c_a;&c_m;&c_je;&c_n;&c_i;&c_t;&c_mol;
    </trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rozplameni&tcaron;</trd>,
    <trd>zap&aacute;li&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="flam.ek0i">
  <kap>ek<tld/>i</kap>
  <snc mrk="flam.ek0i.KOMUNE">
    <dif>
      Ekmeti&gcirc;i en <tld/>an staton:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        lia viza&gcirc;o ek<tld/>is pro honto, pro ofendita fiero;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        ribelo subite ek<tld/>is.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_a;&c_r;&c_e;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_u;&c_z;&c_g;&c_a;&c_r;&c_e;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>tornar-se roig</trd>,
    <trd>inflamar-se</trd>
  </trdgrp>
  <trd lng="cs">zapalovat se za&zcaron;ehovat se</trd>
  <trdgrp lng="en">
    <trd>catch <ind>fire</ind></trd>,
    <trd>take <ind>fire</ind></trd>,
    <trd>enkindle</trd>,
    <trd><ind>flare</ind> up</trd>
  </trdgrp>
  <trd lng="es">prender fuego</trd>
  <trd lng="fr">s'<ind>enflammer</ind></trd>
  <trdgrp lng="he">
    <trd>&lamed;<ind>&he;&tav;&lamed;&qof;&het;</ind></trd>,
    <trd>&lamed;&ayin;&lamed;&vav;&tav; &bet;<ind>&alef;&shin;</ind>
    </trd>,
    <trd>&lamed;<ind>&ayin;&lamed;&vav;&tav;</ind>
    &bet;&lamed;&he;&bet;&vav;&tav;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>l&aacute;ngra lobban</trd>,
    <trd>meggyullad</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ontvlammen</trd>,
    <trd>ontbranden</trd>
  </trdgrp>
  <trd lng="pl">zap&lstroke;on&aogonek;&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_zh;&c_je;&c_ch;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_o;&c_r;&c_je;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_o;&c_z;&c_g;&c_o;&c_r;&c_je;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_o;&c_s;&c_p;&c_l;&c_a;&c_m;&c_je;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vzb&lacute;knu&tcaron;</trd>,
    <trd>vzplan&uacute;&tcaron;</trd>
  </trdgrp>
</drv>

