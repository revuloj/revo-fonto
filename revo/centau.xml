<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: centau.xml,v 1.29 2023/10/31 13:37:03 revo Exp $">
<kap>
  <rad>centa&ubreve;r</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="centau.0o">
  <kap><tld/>o</kap>
  <snc mrk="centau.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Mita estulo kun homa kapo kaj torso kaj &ccirc;evala korpo.
    </dif>
    <ekz>&Ccirc;u ekzistas en Ofiro iuj <tld/>oj?
      <fnt>
        <aut>Karel &Ccaron;apek, trad. Josef Vondrou&scaron;ek</aut>:
        <vrk>Libro de apokrifoj</vrk>.
      </fnt>
    </ekz>
    <trd lng="zh">&#x4EBA;&#x9996;&#x99AC;&#x8EAB;&#x7684;&#x602A;&#x7269; <pr>r&#xE9;nsh&#x1D2;um&#x1CE;sh&#x113;ndegu&#xE0;iw&#xF9;</pr></trd>
</snc>
  <trd lng="be">&c_k;&c_je;&c_n;&c_t;&c_a;&c_w;&c_r;</trd>
  <trd lng="ca">centaure</trd>
  <trd lng="el">
  &kappa;&epsilon_ton;&ny;&tau;&alfa;&ypsilon;&rho;&omikron;&sigma_fina;
  </trd>
  <trd lng="de">Zentauer</trd>
  <trd lng="en">centaur</trd>
  <trd lng="es">centauro</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_A;&a_n;&a_t;&a_w;&a_r;</trd>,
    <trd>&a_q;&a_n;&a_T;&a_w;&a_r;&a_s;</trd>
  </trdgrp>
  <trd lng="fr">centaure<klr tip="ind"> (cr&eacute;ature mythique)</klr></trd>
  <trd lng="hu">kentaur</trd>
  <trd lng="it">centauro</trd>
  <trd lng="pl">centaur</trd>
  <trd lng="pt">centauro</trd>
  <trd lng="ro">centaur</trd>
  <trd lng="ru">&c_k;&c_je;&c_n;&c_t;&c_a;&c_v;&c_r;</trd>
  <trd lng="sk">kentaurus</trd>
</drv>

<drv mrk="centau.C0o">
  <kap><tld lit="C"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
  </kap>
  <mlg>Cen</mlg>
  <snc>
    <dif>
      Sud-hemisfera <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref> (<trd lng="la">Centaurus</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_C;&c_e;&c_n;&c_t;&c_a;&c_w;&c_r;</trd>
  <trd lng="ca">Centaure</trd>
  <trd lng="de">Zentaur</trd>
  <trd lng="en">Centaurus</trd>
  <trd lng="es">Centauro</trd>
  <trd lng="fa">&a_q;&a_n;&a_T;&a_w;&a_r;&a_s;</trd>
  <trd lng="fr">Centaure<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="he">&#x5E7;&#x5E0;&#x5D8;&#x5D0;&#x5D5;&#x5E8;</trd>
  <trd lng="pl">Centaur  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Centaurul  <klr>(constela&#x21B;ie)</klr></trd>
  <trd lng="uk">&#x43A;&c_je;&#x43D;&c_t;&c_a;&c_v;&c_r;</trd>
</drv>

</art>
