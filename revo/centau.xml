<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>centa&ubreve;r</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="centau.0o">
  <kap><tld/>o</kap>
  <snc mrk="centau.0o.MIT">
  <uzo tip="fak">MIT</uzo>
    <dif>
      Mita estulo kun homa kapo kaj torso kaj &ccirc;evala korpo.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_je;&c_n;&c_t;&c_a;&c_w;&c_r;</trd>
  <trd lng="ca">centaure</trd>
  <trd lng="el">
  &kappa;&epsilon_ton;&ny;&tau;&alfa;&ypsilon;&rho;&omikron;&sigma_fina;
  </trd>
  <trd lng="de">Zentauer</trd>
  <trd lng="en">centaur</trd>
  <trd lng="es">centauro</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_A;&a_n;&a_t;&a_w;&a_r;</trd>,
    <trd>&a_q;&a_n;&a_T;&a_w;&a_r;&a_s;</trd>
  </trdgrp>
  <trd lng="fr">centaure</trd>
  <trd lng="hu">kentaur</trd>
  <trd lng="it">centauro</trd>
  <trd lng="pl">centaur</trd>
  <trd lng="pt">centauro</trd>
  <trd lng="ru">&c_k;&c_je;&c_n;&c_t;&c_a;&c_v;&c_r;</trd>
  <trd lng="sk">kentaurus</trd>
</drv>

<drv mrk="centau.C0o">
  <kap><tld lit="C"/>o</kap>
  <mlg>Cen</mlg>
  <snc>
    <dif>
      Sud-hemisfera <ref tip="lst" cel="stel.0figuro"
      lst="voko:stelfiguroj">stelfiguro</ref> (<trd
      lng="la">Centaurus</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_C;&c_e;&c_n;&c_t;&c_a;&c_w;&c_r;</trd>
  <trd lng="ca">Centaure</trd>
  <trd lng="de">Zentaur</trd>
  <trd lng="en">Centaurus</trd>
  <trd lng="es">Centauro</trd>
  <trd lng="fa">&a_q;&a_n;&a_T;&a_w;&a_r;&a_s;</trd>
  <trd lng="fr">Centaure<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="pl">Centaur</trd>
</drv>

</art>
