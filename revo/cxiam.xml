    <trdgrp lng="ro">
      <trd>mereu</trd>,
      <trd>totodeauna</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_s;&c_je;&c_g;&c_d;&c_a;</trd>
    <trd lng="sk">v&zcaron;dy</trd>
    <trd lng="sv">alltid</trd>
    <trd lng="vo">egelo</trd>
  </snc>
  <snc mrk="cxiam.0.senhalte">
    <dif>
      <ref tip="super" cel="halt.sen0a">Senhalte</ref> kaj kvankam <ref cel="trans.0">trans</ref>
      kutima limo, ne nepre senlime:
      <ekz>
        la sonado de la korno fari&gcirc;adis <tld/> pli kaj pli forta
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 19:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fremdulo, kiu estos inter vi, levi&gcirc;os super vi <tld/> pli kaj pli alte, kaj
        vi mallevi&gcirc;os <tld/> pli kaj pli malalte
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 28:43</lok>
        </fnt>;
      </ekz>
      <ekz>
        la knabo <nom>Samuel</nom> <tld/> pli kreskis, kaj li pla&ccirc;is <klr>[&#x2026;]</klr> al la homoj
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 2:26</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>David</nom> fari&gcirc;adis <tld/> pli forta, kaj la domo de <nom>Saul</nom> fari&gcirc;adis <tld/> pli
        malforta
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 3:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam la bela muziko ludis <tld/> plue, &scirc;i ne povis plu elteni, &ccirc;ar tio estis tro
        alloga
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        li iris <tld/> pluen kaj pluen, tiel malproksimen, kiel li neniam anta&ubreve;e estis
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
