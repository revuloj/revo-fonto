<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>&ccirc;iam</rad></kap>
<drv mrk="cxiam.0">
  <kap><ofc>*</ofc><tld/></kap>
  <gra><vspec>adv</vspec></gra> 
  <snc num="1">
    <dif>
      En &ccirc;iu tempo:
      <ekz>
        mi trinkas <tld/> mian kafon sen sukero.
      </ekz>
    </dif>
  </snc>
  <snc num="2">
    <dif>
      En la tuta pasinta tempo:
      <ekz>
        li <tld/> ser&ccirc;is la veron.
      </ekz>
    </dif>
  </snc>
  <snc num="3">
    <dif>
      En tuta la estonta tempo:
      <ekz>
        mi restos por <tld/> via amiko.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">immer</trd>
  <trd lng="es">siempre</trd>
</drv>
<drv mrk="cxiam.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Konstanta:
      <ekz>
        mia <tld/>a amo al vi;
      </ekz>
      <ekz>
        por<tld/>a adia&ubreve;
        <fnt>(<aut>Zamenhof</aut>: <vrk>Fabeloj de Andersen,
        Vojkamarado</vrk>)</fnt>.
      </ekz>  
    </dif>
  </snc>
  <trdgrp lng="de"><trd>st&auml;ndig</trd>, <trd>stetig</trd></trdgrp>
</drv>
</art>
