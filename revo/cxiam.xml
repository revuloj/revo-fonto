      <trd>&c_z;&c_a;&c_w;&c_s;&c_jo;&c_d;&c_y;</trd>,
      <trd>&c_z;&c_a;&c_w;&c_zh;&c_d;&c_y;</trd>
    </trdgrp>
    <trd lng="en">always</trd>
    <trd lng="es">siempre</trd>
    <trd lng="fa">&a_h;&a_m;&f_y;&a_sh;&a_h;</trd>
