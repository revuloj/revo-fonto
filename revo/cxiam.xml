<drv mrk="cxiam.por0a">
  <kap>por<tld/>a</kap>
  <snc mrk="cxiam.por0a.KOMUNE">
    <dif>
      <tld lit="&Ccirc;"/> da&ubreve;ronta:
      <ekz>
        por<tld/>a adia&ubreve;
        <fnt><bib>Fab1</bib>, <lok>Vojkamarado</lok></fnt>;
      </ekz>
      <ekz>
        la kampoj &ccirc;irka&ubreve; iliaj urboj ne estu vendataj, &ccirc;ar tio estas ilia por<tld/>a
        poseda&jcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 25:34</lok>
        </fnt>;
      </ekz>
      <ekz>
        tro rapida edzi&gcirc;o &dash; por<tld/>a kateni&gcirc;o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        atakis &scirc;in mallumaj pensoj, <klr>[&#8230;]</klr> timo je la por<tld/>a morto
        <fnt>
          <bib>Fab4</bib>
          <lok>Infano en la tombo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_je;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_a;&c_w;&c_s;&c_jo;&c_d;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>immerw&auml;hrend</trd>,
    <trd>st&auml;ndig</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_h;&a_m;&f_y;&a_sh;&f_g;&f_y;</trd>,
    <trd>&a_j;&a_A;&a_w;&a_d;&a_A;&a_n;&a_h;</trd>,
    <trd>&a_A;&a_b;&a_d;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">perp&eacute;tuel</trd>
  <trdgrp lng="ja">
    <trd>&#x6C38;&#36960;&#x306E;  <pr>&#12360;&#12356;&#12360;&#12435;&#x306E;</pr></trd>,
    <trd>&#x6C38;&#x4E45;&#x306E;  <pr>&#12360;&#12356;&#x304D;&#12421;&#12358;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>voor <ind>altijd</ind></trd>,
    <trd>eeuwig</trd>
  </trdgrp>
  <trdgrp lng="no">
