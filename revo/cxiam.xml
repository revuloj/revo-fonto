<drv mrk="cxiam.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="cxiam.0a.KOMUNE">
    <dif>
      Konstanta, sen&ccirc;esa:
      <ekz>
        ekzemple: tiama, &ccirc;iama, kioma, tiea, &ccirc;i-tiea, tieulo,
        tiamulo <fnt><bib>F</bib>, <lok>&FE; 30</lok></fnt>;
      </ekz>
      <ekz>
        la boneco de Dio estas <tld/>a
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 52:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        en katenoj <tld/>aj
        <fnt>
          <bib>NT</bib>
          <lok>Judas 1:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        regis &ccirc;irka&ubreve; li preska&ubreve; <tld/>a nokto
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la supro de l' Blanka Monto / Estas blanka, &ccirc;ar / &Gcirc;in kovras ne&gcirc;o <tld/>a
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi petegas vin per la rajtoj de <klr>[&#x2026;]</klr> nia <tld/>a reciproka amo
        <fnt>
          <bib>Hamlet</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_w;&c_s;&c_jo;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_l;&c_y;</trd>,
    <trd>&c_z;&c_v;&c_y;&c_k;&c_l;&c_y;</trd>,
    <trd>&c_v;&c_je;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ewig</trd>,
    <trd>immerw&auml;hrend</trd>,
    <trd>st&auml;ndig</trd>,
    <trd>stetig</trd>
  </trdgrp>
  <trd lng="el">
  &sigma;&ypsilon;&ny;&epsilon;&chi;&omega_ton;&sigma_fina;</trd>
  <trd lng="fa">&a_h;&a_m;&f_y;&a_sh;&f_g;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>permanent</trd>,
    <trd>de toujours</trd>
  </trdgrp>
  <trd lng="he">&tav;&mem;&yod;&dalet;&yod;</trd>
