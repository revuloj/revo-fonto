<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>&ccirc;iam</rad></kap>
<drv mrk="cxiam.0">
  <kap><ofc>*</ofc><tld/></kap>
  <gra><vspec>adv</vspec></gra>
  <snc num="1">
    <dif>
      En &ccirc;iu tempo:
      <ekz>
        mi trinkas <tld/> mian kafon sen sukero.
      </ekz>
    </dif>
  </snc>
  <snc num="2">
    <dif>
      En la tuta pasinta tempo:
      <ekz>
        li <tld/> ser&ccirc;is la veron.
      </ekz>
    </dif>
  </snc>
  <snc num="3">
    <dif>
      En tuta la estonta tempo:
      <ekz>
        mi restos por <tld/> via amiko.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">immer</trd>
  <trdgrp lng="el">
    <trd>&pi;&alfa_ton;&ny;&tau;&omikron;&tau;&epsilon;</trd>,
    <trd>&pi;&alfa_ton;&ny;&tau;&alfa;</trd>
  </trdgrp>
  <trd lng="es">siempre</trd>
  <trd lng="fr">toujours</trd>
  <trd lng="he">&tav;&mem;&yod;&dalet;</trd>
  <trd lng="it">sempre</trd>
  <trd lng="nl">altijd</trd>
  <trd lng="pt">sempre</trd>
  <trd lng="ru">&c_v;&c_s;&c_je;&c_g;&c_d;&c_a;</trd>
  <trd lng="sk">v&zcaron;dy</trd>
  <trd lng="sv">alltid</trd>
</drv>
<drv mrk="cxiam.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Konstanta:
      <ekz>
        mia <tld/>a amo al vi;
      </ekz>
      <ekz>
        por<tld/>a adia&ubreve;
        <fnt>(<aut>Zamenhof</aut>: <vrk>Fabeloj de Andersen,
        Vojkamarado</vrk>)</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de"><trd>st&auml;ndig</trd>, <trd>stetig</trd></trdgrp>
  <trd lng="el">&sigma;&ypsilon;&ny;&epsilon;&chi;&omega_ton;&sigma_fina;</trd>
  <trd lng="he">&tav;&mem;&yod;&dalet;&yod;</trd>
  <trdgrp lng="nl">
    <trd>voortdurend</trd>,
    <trd>eeuwig</trd>
  </trdgrp>
  <trd lng="pt">constante</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_s;&c_je;&c_g;&c_d;&c_a;&c_sh;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_t;&c_o;&c_ja;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_b;&c_y;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">ve&ccaron;n&yacute;</trd>
</drv>
</art>
