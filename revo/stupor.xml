<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>stupor</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="stupor.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PSI</uzo>
  <snc>
    <dif>
      Senmoveco, sensenteco pro spirita &scirc;oko, ekzemple pro mirego:
      <ekz>la knabino eksentis <tld/>on
      <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 61a, p. 323a</lok></fnt>.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konste1.0igxo">konsterni&gcirc;o</ref>,
      <ref cel="perple.0o">perplekso</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_u;&c_p;&c_a;&c_r;</trd>,
    <trd>&c_a;&c_s;&c_l;&c_u;&c_p;&c_ja;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ahurissement</trd>
    <trd>h&eacute;b&egrave;tement</trd>
    <trd>stup&eacute;faction</trd>
    <trd>stupeur</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>stupor</trd>
    <trd>eltompul&aacute;s</trd>
    <trd>megdermed&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>stupor</trd>,
    <trd>verminderd <ind>bewustzijn</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_u;&c_p;&c_o;&c_r;</trd>,
    <trd>&c_o;&c_c;&c_je;&c_p;&c_je;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="stupor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Resti senreaga pro spirita &scirc;oko:
      <ekz>
        Rafo levis sin kaj <tld/>is anta&ubreve; &scirc;ia lito
        <fnt><aut>Shi Chengtai</aut>: <vrk>La kompensa kiso</vrk>,
        <bib>Monato</bib><lok>jaro 1995a, numero 6a, p. 27a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gap.0i">gapi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol; &c_u; &c_s;&c_t;&c_u;&c_p;&c_a;&c_r;&c_y;</trd>,
    <trd>&c_a;&c_s;&c_l;&c_u;&c_p;&c_ja;&c_n;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>rester <ind>clou&eacute;</ind> sur place</trd>
    <trd>&ecirc;tre <ind>stup&eacute;fi&eacute;</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megdermed</trd>,
    <trd>eltompul</trd>,
    <trd>stuporba esik</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_y;&c_t;&c_mol; &c_v; <ind>&c_s;&c_t;&c_u;&c_p;&c_o;&c_r;&c_je;</ind></trd>,
    <trd>&c_o;&c_c;&c_je;&c_p;&c_je;&c_n;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>

