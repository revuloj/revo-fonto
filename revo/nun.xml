    <trd>present<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="el">&sigma;&ypsilon_ton;&gamma;&chi;&rho;&omikron;&ny;&omikron;&sigma_fina;</trd>
  <trd lng="es">actual</trd>
  <trd lng="fi">nykyinen</trd>
  <trdgrp lng="fr">
    <trd>actuel</trd>,
    <trd>pr&eacute;sent<klr> (actuel)</klr></trd>
  </trdgrp>
