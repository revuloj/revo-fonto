<drv mrk="nun.0">
  <kap><ofc>*</ofc><tld/></kap>
  <gra><vspec>adverbo</vspec></gra>
  <snc mrk="nun.0.KOMUNE">
    <dif>
      En plej proksima tempo:
    </dif>
    <subsnc mrk="nun.0.cxitempe">
      <dif>
        En la momento mem, dum kiu ni parolas:
        <ekz>
          li <tld/> dormas;
        </ekz>
        <ekz>
          <tld/> mi foriras;
        </ekz>
        <ekz>
          <tld/> ser&ccirc;u vian &ccirc;apelon;
        </ekz>
        <ekz>
          <tld/>, kiam <klr>[&#x2026;]</klr> restas fari nur kelkajn 
          pa&scirc;ojn <klr>[&#x2026;]</klr> li estos devigita ekiri returnen
          <fnt>
            <bib>FK</bib>
            <vrk>Nokto</vrk>
          </fnt>;
        </ekz>
        <ekz>
          vi estas ankora&ubreve; <tld/> &ccirc;iam
          mensoganto
          <fnt>
            <bib>F</bib>
            <lok>&FE; 22</lok>
          </fnt>;
        </ekz>
        <ekz>
          ri&ccirc;igadi la lingvon per novaj vortoj oni povas jam
          <tld/>
          <fnt>
            <bib>F</bib>
            <lok>Anta&ubreve;parolo</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="fi">nyt</trd>
      <trd lng="he">&kaf;&resh;&gimel;&ayin;</trd>
      <trd lng="vo">nu</trd>
    </subsnc>
    <subsnc mrk="nun.0.cxiepoke">
      <dif>
        En la estanta tempo, epoko:
        <ekz>
          uzado de a&ubreve;tomobiloj <tld/>
          pli kaj pli ofti&gcirc;as;
        </ekz>
        <ekz>
          io granda <tld/> naski&gcirc;as
          <fnt>
            <bib>Paroloj</bib>
            <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista en
            Boulogne sur Mer en la 5a de a&ubreve;gusto 1905</vrk>
          </fnt>;
        </ekz>
        <ekz>
          <tld/> la unuan fojon la revo de miljaroj komencas
          reali&gcirc;i
          <fnt>
            <bib>Paroloj</bib>
            <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista en
            Boulogne sur Mer en la 5a de a&ubreve;gusto 1905</vrk>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="fi">
        <trd>nyt</trd>,
        <trd>n&auml;in&auml; aikoina</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="nun.0.ekdenun">
      <dif>
        Sekve post io, da&ubreve;rigante post tio:
        <ekz>
          <tld/> ni transiros al la
          dua demando
          <fnt>
            <bib>EE</bib>
          </fnt>;
        </ekz>
        <ekz>
          kaj <tld/> ni &ccirc;ion forgesu
          <fnt>
            <bib>Paroloj</bib>
            <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en
            Dresden en la 17a de a&ubreve;gusto 1908</vrk>
          </fnt>;
        </ekz>
        <ekz>
          <tld/> ni rigardu, kio sekvas
          <fnt>
            <bib>EE</bib>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="nu.0">nu</ref>
      </refgrp>
      <trd lng="fi">nyt (sitten)</trd>
    </subsnc>
  </snc>
  <refgrp tip="vid">
     <ref cel="jxus.0">&jcirc;us</ref>,
     <ref cel="tuj.0.tempe">tuj<sncref/></ref>
  </refgrp>
