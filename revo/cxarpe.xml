<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>&ccirc;arpent</rad>/i
</kap>
<drv mrk="cxarpe.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1">
    <dif>
      Prilabori lignon a&ubreve; feron por konstrui:
      <ekz>
        <tld/>i kolonojn
        <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Sentencoj 9, 1</lok>)</fnt>.
      </ekz> 
    </dif>
    <trd lng="ru">&c_p;&c_l;&c_o;&c_t;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc num="2">
    <dif>
      Kunmeti trabojn por fari konstruojn:
      <ekz>
        malforte <tld/>ita domo.
      </ekz>
    </dif>
  </snc>
  <snc num="3">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Forte konstrui:
      <ekz>
        homo fortike <tld/>ita;
      </ekz>
      <ekz>
        bone <tld/>ita dramo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">zimmern</trd>
  <trd lng="en">build</trd>
</drv>
<drv mrk="cxarpe.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Kunmeta&jcirc;o el lignaj a&ubreve; feraj pecoj por subteni
      konstrua&jcirc;on
    </dif>
    <refgrp tip="sin">
      <ref cel="trab.0ajxo0aro">trabaro</ref>,
      <ref cel="armatu.0o">armaturo</ref>,
      <ref cel="skelet.0o">skeleto</ref>.
    </refgrp>
  </snc>
</drv>
<drv mrk="cxarpe.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Homo, kies metio estas <tld/>i.  
    </dif>
  </snc>
  <trd lng="de">Zimmermann</trd>
  <trd lng="ru">&c_p;&c_l;&c_o;&c_t;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
