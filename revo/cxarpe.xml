<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cxarpe.xml,v 1.25 2023/08/31 10:10:27 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&ccirc;arpent</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="cxarpe.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="cxarpe.0i.prilabori">
    <dif>
      <ref tip="super" cel="labor.pri0i.KOMUNE">Prilabori</ref>
      lignon a&ubreve; feron por konstrui:
      <ekz>
        <tld/>i kolonojn
        <fnt><bib>MT</bib>, <lok>&Sen; 9:1</lok></fnt>;
      </ekz>
      <ekz>
        dume la <tld/>istoj povos <tld/>i la trunkojn
        <fnt>
          <bib>HsT</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rabot.0i">raboti</ref>,
      <ref cel="seg.0i">segi</ref>,
      <ref cel="tajl.0i">tajli</ref>
    </refgrp>
    <trd lng="be">&c_c;&c_ja;&c_s;&c_mol;&c_l;&c_ja;&c_r;&c_y;&c_c;&c_mol;</trd>
    <trd lng="pl">ciosa&cacute;</trd>
    <trd lng="ru">&c_p;&c_l;&c_o;&c_t;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="cxarpe.0i.kunmeti">
    <dif>
      <ref tip="super" cel="met.kun0i.KOMUNE">Kunmeti</ref> 
      trabojn por fari konstruojn:
      <ekz>
        mi elportis lignotabulojn, la&ubreve; mia kapablo <tld/>is 
        &scirc;ipeton, per takeloj mi malfacile surakvigis &gcirc;in
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi ordonis al skulptisto <tld/>i tombo&scirc;tonon por Gulo
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i prenis sur sin la taskon prepari kafon kaj 
        man&gcirc;a&jcirc;on por la patro, kiu havis
        <tld/>an laboron en la pre&gcirc;eja vila&gcirc;o
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>;
      </ekz>
      <ekz>
        ofte mi laboris sur grandaj &scirc;iptrena&jcirc;oj, flosoj, sur kiuj ni mem <tld/>is
        iaspecan kabaneton
        <fnt>
          <bib>KKr</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="pl">zbija&cacute;</trd>
  </snc>
  <snc mrk="cxarpe.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Konstrui, <ref tip="super" cel="form.0i">formi</ref>
      ion fortikan:
      <ekz>
        homo fortike <tld/>ita;
      </ekz>
      <ekz>
        li estis bele altakreska, jes ja, <ind>fortike <tld/>ita</ind>
        <fnt>
          <bib>CKv</bib>
        </fnt>;
        <trd lng="hu">j&oacute; k&ouml;t&eacute;s&udblac;</trd>
      </ekz>
      <ekz>
        mi devis rideti <klr>[&#x2026;]</klr> kaj el mia tuta 
        koro kompatis tiun bravaspektan maristle&ubreve;tenanton, 
        kiu volus <tld/>i homon el tia esta&jcirc;o
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        multon Li trasuferis dum <tld/>ado de ritmoj
        <fnt>
          <bib>MMa</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>budowa&cacute;</trd>
    </trdgrp>
  </snc>
  <trd lng="de">zimmern</trd>
  <trdgrp lng="en">
    <trd>carpenter</trd>,
    <trd>build</trd>,
    <trd>construct</trd>
  </trdgrp>
  <trd lng="fr">charpenter</trd>
  <trdgrp lng="he">
    <trd>&lamed;&nun;&gimel;&resh;</trd>,
    <trd>&lamed;&ayin;&samekh;&vav;&qof; &bet;&nun;&gimel;&resh;&vav;&tav;</trd>
  </trdgrp>
  <trd lng="hu">&aacute;csol</trd>
  <trdgrp lng="ja">
    <trd>&#x5927;&#x5DE5;&#x4ED5;&#x4E8B;&#x3067;&#x4F5C;&#x308B;  [<ind>&#x3060;&#x3044;&#x304F;&#x3057;&#x3054;&#x3068;&#x3067;&#x3064;&#x304F;&#x308B;</ind>]</trd>,
    <trd>&#x9AA8;&#x7D44;&#x307F;&#x3092;&#x4F5C;&#x308B;  [<ind>&#x307B;&#x306D;&#x3050;&#x307F;&#x3092;&#x3064;&#x304F;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">timmeren</trd>
  <trd lng="pt">carpintejar</trd>
  <trd lng="sk">tesa&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x442;&#x435;&#x441;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x442;&#x435;&#x441;&#x43B;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="cxarpe.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="cxarpe.0ajxo.KON">
    <dif>
      Kunmeta&jcirc;o el lignaj a&ubreve; metalaj pecoj por subteni
      <ref tip="super" cel="konstr.0ajxo">konstrua&jcirc;o</ref>n:
      <ekz>
        <tld/>a&jcirc;o el oro
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 3:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la egalrajteco de tiuj du federacioj konstituas la <tld/>a&jcirc;on de la Islama
        Komisio de Hispanio
        <fnt>
          <bib>MD02</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="armatu.0o">armaturo</ref>,
      <ref cel="skelet.0o.KON">skeleto<sncref/></ref>,
      <ref cel="trab.0aro.KON">trabaro</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Fachwerk</trd>,
    <trd>Ger&uuml;st</trd>
  </trdgrp><trdgrp lng="en">
    <trd>carpentry</trd>,
    <trd>framework</trd>,
    <trd>structure</trd>
  </trdgrp>
  <trd lng="fr">charpente</trd>
  <trd lng="he">&nun;&#x5B7;&gimel;&#x5B8;&resh;&vav;&#x5BC;&tav;</trd>
  <trdgrp lng="hu">
    <trd>&aacute;csolat</trd>,
    <trd>v&aacute;zszerkezet</trd>
  </trdgrp>
  <trd lng="ja">&#x9AA8;&#x7D44;&#x307F;  [<ind>&#x307B;&#x306D;&#x3050;&#x307F;</ind>]</trd>
  <trd lng="nl">timmerwerk</trd>
  <trdgrp lng="pl">
    <trd>ciesielka</trd>,
    <trd>wi&eogonek;&zacute;ba</trd>,
    <trd>szkielet<klr> (np. domu)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x437;&#x440;&#x443;&#x431;</trd>,
    <trd>&#x43A;&#x456;&#x441;&#x442;&#x44F;&#x43A;</trd>,
    <trd>&#x43E;&#x441;&#x442;&#x43E;&#x432;</trd>,
    <trd>&#x442;&#x435;&#x441;&#x43B;&#x44F;&#x440;&#x441;&#x44C;&#x43A;&#x430; &#x440;&#x43E;&#x431;&#x43E;&#x442;&#x430;</trd>
  </trdgrp>
</drv>

<drv mrk="cxarpe.0isto">
  <kap><ofc>*</ofc><tld/>isto</kap>
  <snc mrk="cxarpe.0isto.KON">
    <dif>
      Homo, kies <ref tip="lst" lst="voko:metioj" cel="meti.0o">metio</ref>
      estas <tld/>i:
      <ekz>
        <klr>[li sendis]</klr> <tld/>istojn kaj 
        masonistojn kaj ili konstruis domon al
        <nom>David</nom>
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 5:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili dehakas arbon en la arbaro, kaj <tld/>isto prilaboras &gcirc;in per hakilo
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 10:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        la hakilojn ili portas kvaza&ubreve; <tld/>istoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la lignaj barieroj, kiuj nun estis 
        forrompataj de la <tld/>istoj, staris
        ankora&ubreve; multaj senlaboruloj
        <fnt>
          <bib>RBa</bib>
        </fnt>;
      </ekz>
      <ekz>
        la gejunuloj estas materialo el kiu formi&gcirc;as 
        for&gcirc;istoj, <tld/>istoj, lamenistoj
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi devas komenci kiel <tld/>ista lernanto, 
        mi devos porti &ccirc;apon, kvankam mi
        kutimis porti silkan &ccirc;apelon
        <fnt>
          <bib>Fab3</bib>
          <lok>Io</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia mortinta praavo estis profesie mueleja <tld/>isto
        <fnt><bib>RabistFab</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_ja;&c_s;&c_mol;&c_l;&c_ja;&c_r;</trd>
  <trd lng="de">Zimmermann</trd>
  <trd lng="el">&xi;&ypsilon;&lambda;&omikron;&ypsilon;&rho;&gamma;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">carpenter</trd>
  <trd lng="es">carpintero</trd>
  <trd lng="fr">charpentier</trd>
  <trd lng="he">&nun;&gimel;&resh;</trd>
  <trd lng="hu">&aacute;cs</trd>
  <trd lng="ja">&#x5927;&#x5DE5;  [<ind>&#x3060;&#x3044;&#x304F;</ind>]</trd>
  <trd lng="nl">timmerwerk</trd>
  <trd lng="pl">cie&sacute;la</trd>
  <trd lng="pt">carpinteiro</trd>
  <trd lng="ru">&c_p;&c_l;&c_o;&c_t;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">tes&aacute;r</trd>
  <trd lng="uk">&#x442;&#x435;&#x441;&#x43B;&#x44F;</trd>
</drv>
</art>
