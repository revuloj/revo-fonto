<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kvitan.xml,v 1.28 2023/12/03 09:59:05 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kvitanc</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kvitan.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="kvitan.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Skriba&jcirc;o per kiu oni kvitigas iun pri sumo, kiun li devis pagi;
      kvitatesto.
    </dif>
    <trd lng="be">&c_r;&c_a;&c_s;&c_mol;&c_p;&c_ib;&c_s;&c_k;&c_a;</trd>
    <trd lng="bg">&c_k;&c_v;&c_i;&c_t;&c_a;&c_n;&c_c;&c_i;&c_ja;</trd>
    <trd lng="de">Quittung</trd>
    <trd lng="pl">pokwitowanie</trd>
    <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_i;&c_s;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="kvitan.0o.POSX">
    <uzo tip="fak">POSX</uzo>
    <uzo tip="fak">TEL</uzo>
    <dif>
      Skriba&jcirc;o per kiu oni atestas, ke oni bone ricevis leteron
      a&ubreve; paka&jcirc;on; ricevatesto:
      <ekz>
        &ccirc;e la ricevo de nia letero, ni petas vin resendi <tld/>on.
      </ekz>
    </dif>
    <trd lng="de">Empfangsbest&auml;tigung</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_v;&c_ib;&c_t;&c_a;&c_n;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_c;&c_e;&c_t;&c_l;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="bo">&#xF56;&#xFB1;&#xF74;&#xF44;&#xF0B;&#xF60;&#xF5B;&#xF72;&#xF53;&#xF0B;</trd>
  <trd lng="br">kuita&ntilde;s</trd>
  <trdgrp lng="en">
    <trd>receipt</trd>,
    <trd>written acknowledgment of receipt</trd>
  </trdgrp>
  <trd lng="es">recibo</trd>
  <trd lng="fr">quittance</trd>
  <trd lng="hu">nyugta</trd>
  <trdgrp lng="ja">
    <trd>&#x9818;&#x53CE;&#x66F8;  <pr>&#x308A;&#x3087;&#x3046;&#x3057;&#x3085;&#x3046;&#x3057;&#x3087;</pr></trd>,
    <trd>&#x53D7;&#x9818;&#x66F8;  <pr>&#x3058;&#x3085;&#x308A;&#x3087;&#x3046;&#x3057;&#x3087;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bonnetje<klr tip="ind"> (kwitantie)</klr></trd>,
    <trd>kwitantie</trd>,
    <trd>ontvangstbewijs</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kwit</trd>,
    <trd>pokwitowanie</trd>
  </trdgrp>  
  <trdgrp lng="pt">
    <trd>recibo <klr tip="ind">(documento)</klr></trd>,
    <trd>quita&ccedil;&atilde;o</trd>
  </trdgrp>
  <trd lng="ro">chitan&#x21B;&abreve;</trd>
  <trd lng="ru">&c_k;&c_v;&c_i;&c_t;&c_a;&c_n;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sv">kvitto</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x432;&#x438;&#x442;&#x430;&#x43D;&#x446;&#x456;&#x44F;</trd>,
    <trd>&#x440;&#x43E;&#x437;&#x43F;&#x438;&#x441;&#x43A;&#x430; </trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x56DE;&#x6761; <pr>hu&#xED;ti&#xE1;o</pr></trd>,
    <trd>&#x56DE;&#x689D; <pr>hu&#xED;ti&#xE1;o</pr></trd>,
    <trd>&#x56DE;&#x57F7; <pr>hu&#xED;zh&#xED;</pr></trd>,
    <trd>&#x56DE;&#x6267; <pr>hu&#xED;zh&#xED;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kvitan.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kvitan.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Doni <tld/>on al iu:
      <ekz>
        li <tld/>is min pri mia lupago.
      </ekz>
    </dif>
  </snc>
  <snc mrk="kvitan.0i.POSX">
    <uzo tip="fak">POSX</uzo>
    <uzo tip="fak">TEL</uzo>
    <dif>
      Sendi ricevateston:
      <ekz>
        la centro <tld/>is la raporton kaj
        ekestis silento
        <fnt><aut>I. Nemere</aut>, <vrk>Sur Kampo Granita</vrk>, 
        <lok>1983</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_d;&c_a;&c_c;&c_mol; <ind>&c_k;&c_v;&c_ib;&c_t;&c_a;&c_n;&c_c;&c_y;&c_ju;</ind></trd>,
    <trd>&c_v;&c_y;&c_d;&c_a;&c_c;&c_mol; <ind>&c_r;&c_a;&c_s;&c_mol;&c_p;&c_ib;&c_s;&c_k;&c_u;</ind></trd>
  </trdgrp>
  <trd lng="br">kuiteza&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>potvrdit p&rcaron;&iacute;jem</trd>,
    <trd>potvrdit vyrovn&aacute;n&iacute; z&aacute;vazku</trd>
  </trdgrp>
  <trd lng="de">quittieren</trd>
  <trdgrp lng="en">
    <trd>give a receipt</trd>,
    <trd>acknowledge receipt of something</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>accuser r&eacute;ception</trd>,
    <trd>quittancer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nyugt&aacute;z</trd>,
    <trd>nyugt&aacute;t ad</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x9818;&#x53CE;&#x66F8;&#x3092;&#x51FA;&#x3059;  <pr>&#x308A;&#x3087;&#x3046;&#x3057;&#x3085;&#x3046;&#x3057;&#x3087;&#x3092;&#x3060;&#x3059;</pr></trd>,
    <trd>&#x53D7;&#x9818;&#x66F8;&#x3092;&#x51FA;&#x3059;  <pr>&#x3058;&#x3085;&#x308A;&#x3087;&#x3046;&#x3057;&#x3087;&#x3092;&#x3060;&#x3059;</pr></trd>
  </trdgrp>
  <trd lng="nl">kwijten</trd>
  <trdgrp lng="pl">
    <trd>kwitowa&cacute;</trd>,
    <trd>potwierdza&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>confirma</trd>,
    <trd>corobora</trd>
  </trdgrp>  
  <trd lng="ru">&c_v;&c_y;&c_d;&c_a;&c_t;&c_mol; &c_k;&c_v;&c_i;&c_t;&c_a;&c_n;&c_c;&c_i;&c_ju;</trd>
  <trdgrp lng="sk">
    <trd>kvitova&tcaron;</trd>,
    <trd>potvrdi&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x438;&#x434;&#x430;&#x432;&#x430;&#x442;&#x438; &#x43A;&#x432;&#x438;&#x442;&#x430;&#x43D;&#x446;&#x456;&#x44E; (&#x43D;&#x430; &#x449;&#x43E;-&#x43D;. &#x43A;&#x43E;&#x43C;&#x443;-&#x43D;.)</trd>,
    <trd>&#x434;&#x430;&#x432;&#x430;&#x442;&#x438; &#x440;&#x43E;&#x437;&#x43F;&#x438;&#x441;&#x43A;&#x443; (&#x43F;&#x440;&#x43E; &#x449;&#x43E;-&#x43D;. &#x43A;&#x43E;&#x43C;&#x443;-&#x43D;.)</trd>
  </trdgrp>
</drv>
</art>
