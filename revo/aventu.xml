<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>aventur</rad>/o
</kap>
<drv mrk="aventu.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Neatendita kaj riska neordinara okaza&jcirc;o.
    </dif>
  </snc>
  <trd lng="de">Abenteuer</trd>
  <trd lng="en">adventure</trd>
  <trd lng="fr">aventure</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_k;&c_l;&c_ju;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_h;&c_o;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="aventu.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <dif>
      Deziranta kaj ser&ccirc;anta <tld/>ojn.
    </dif>
  </snc>
  <trd lng="de">abenteuerlustig</trd>
  <trd lng="fr">aventureux</trd>
</drv>

<drv mrk="aventu.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Homo, kiu havas neniun difinitan profesion kaj trovas vivrimedojn
      nur en
      riskoplenaj plej ofte nehonestaj artifikoj.
    </dif>
    <refgrp tip="sin">
      <ref cel="fripon.0o">fripono</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Abenteurer</trd>
  <trd lng="fr">aventurier</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_k;&c_a;&c_t;&c_je;&c_l;&c_mol; &c_p;&c_r;&c_i;&c_k;&c_l;&c_ju;&c_ch;&c_je;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_a;&c_v;&c_a;&c_n;&c_t;&c_ju;&c_r;&c_i;&c_s;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="aventu.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Homo, kiu spertis multajn <tld/>ojn.
    </dif>
  </snc>
  <trd lng="de">Abenteurer</trd>
  <trd lng="fr">aventurier</trd>
</drv>
</art>
