<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: krista1.xml,v 1.17 2023/12/03 09:59:01 revo Exp $">
<kap>
  <rad>kristalografi</rad>/o
</kap>
<drv mrk="krista1.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Scienco, pritraktanta la
      <ref tip="vid" cel="krista.0o">kristalojn</ref>
      kaj la le&gcirc;ojn pri ilia formado.
    </dif>
  </snc>
  <snc mrk="krista1.0o.apliko">
    <uzo tip="fak">TEK</uzo>
    <dif>
       Apliko de tiu scienco:
      <ekz>
        la malkovro, ke la enzimoj estas kristaligeblaj, komencigis konkuradon 
        pri la trovo de la tridimensia strukturo de la enzimoj per teknikoj kia la X-radia <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1148">Enzimo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_sh;&c_t;&c_a;&c_l;&c_ja;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>
  <trd lng="de">Kristallographie</trd>
  <trd lng="en">crystallography</trd>
  <trd lng="fr">cristallographie</trd>
  <trdgrp lng="hu">
    <trd>krist&aacute;lytan</trd>,
    <trd>krisztallogr&aacute;fia</trd>
  </trdgrp>
  <trd lng="ja">&#x7D50;&#x6676;&#x5B66;  <pr>&#x3051;&#x3063;&#x3057;&#x3087;&#x3046;&#x304C;&#x304F;</pr></trd>
  <trd lng="nl">kristallografie</trd>
  <trd lng="pl">krystalografia</trd>
  <trd lng="pt">cristalografia</trd>
  <trd lng="ru">&c_k;&c_r;&c_i;&c_s;&c_t;&c_a;&c_l;&c_l;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
  <trd lng="uk">&#x43A;&#x440;&#x438;&#x441;&#x442;&#x430;&#x43B;&#x43E;&#x433;&#x440;&#x430;&#x444;&#x456;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x7D50;&#x6676;&#x5B78; <pr>ji&#xE9;j&#x12B;ngxu&#xE9;</pr></trd>,
    <trd>&#x7ED3;&#x6676;&#x5B66; <pr>ji&#xE9;j&#x12B;ngxu&#xE9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="krista1.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Rilatanta al <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_sh;&c_t;&c_a;&c_l;&c_ja;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="de">kristallographisch</trd>
  <trd lng="en">crystallographic</trd>
  <trd lng="fr">cristallographique</trd>
  <trdgrp lng="hu">
    <trd>krist&aacute;lytani</trd>,
    <trd>krisztallogr&aacute;fiai</trd>
  </trdgrp>
  <trd lng="ja">&#x7D50;&#x6676;&#x5B66;&#x306E;  <pr>&#x3051;&#x3063;&#x3057;&#x3087;&#x3046;&#x304C;&#x304F;&#x306E;</pr></trd>
  <trd lng="nl">kristallografisch</trd>
  <trd lng="pl">krystalograficzny</trd>
  <trd lng="ru">&c_k;&c_r;&c_i;&c_s;&c_t;&c_a;&c_l;&c_l;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="uk">&#x43A;&#x440;&#x438;&#x441;&#x442;&#x430;&#x43B;&#x43E;&#x433;&#x440;&#x430;&#x444;&#x456;&#x447;&#x43D;&#x438;&#x439;</trd>
</drv>
