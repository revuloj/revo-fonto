<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>driv</rad>/i
</kap>
<drv mrk="driv.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="driv.0i.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      <ref tip="vid" cel="flos.0i">Flosi</ref> a&ubreve; <ref tip="vid"
      cel="glis.0i.AVI">glisi</ref> pu&scirc;ate de la fluo a&ubreve; de la vento,
      deviante el la difinita vojo:
      <ekz>
        baleno <klr>...</klr> <tld/>anta malviva sur la akvo
        <fnt><bib>Munchhausen</bib>, <lok>&ccirc;apitro 7a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>treiben</trd>,
    <trd>abtreiben</trd>
  </trdgrp>
  <trd lng="en">drift</trd>
  <trd lng="es">ir a la <ind>deriva</ind></trd>
  <trd lng="fr">d&eacute;river<klr tip="ind"> (sur les flots)</klr></trd>
  <trd lng="hu">sodr&oacute;dik</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_r;&c_je;&c_j;&c_f;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_l;&c_y;&c_t;&c_mol; &c_p;&c_o; &c_t;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_ju;</trd>,
    <trd>&c_p;&c_l;&c_y;&c_t;&c_mol; &c_p;&c_o; &c_v;&c_je;&c_t;&c_r;&c_u;</trd>
  </trdgrp>
</drv>

<drv mrk="driv.for0i">
  <kap>for<tld/>i</kap>
  <snc mrk="driv.for0i.MAR">
    <dif>
      Longe <tld/>i, malproksimi&gcirc;ante:
      <ekz>
        la nuboj <klr>...</klr> dispeli&gcirc;is kaj for<tld/>is en la spaco
        kiel malpeza, molkontura vapora&jcirc;o
        <fnt><bib>InfanTorent2</bib>, <lok>dua parto, &ccirc;apitro
        2a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">partir &agrave; la <ind>d&eacute;rive</ind></trd>
</drv>
</art>
