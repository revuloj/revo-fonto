<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: driv.xml,v 1.31 2024/02/19 09:23:56 revo Exp $">
<kap>
  <ofc>9</ofc><rad>driv</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="driv.0i">
  <kap><ofc>9</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="driv.0i.MAR">
    <uzo tip="fak">MAR</uzo>
    <uzo tip="fak">AVI</uzo>
    <dif>
      <ref tip="vid" cel="flos.0i">Flosi</ref> la&ubreve; la pu&scirc;o
      de akvofluo a&ubreve; vento (senstire, en alia direkto ol intencite, sen
      klara direkto):
      <ekz>
        la &scirc;ipo <tld/>is pro motordifekto
        <fnt><bib>OA9</bib></fnt>;
      </ekz>
      <ekz>
        baleno <klr>[&#8230;]</klr> <tld/>anta malviva sur la akvo
        <fnt><bib>Munchhausen</bib>, <lok>&ccirc;apitro 7a</lok></fnt>;
      </ekz>
      <ekz>
        kiam ili longe rigardas &gcirc;in, kun <tld/>antaj someraj nuboj
        super &gcirc;i <klr>[...]</klr>, &scirc;ajnas al ili, ke la granda
        konstrua&jcirc;o kvaza&ubreve; levi&gcirc;as kaj &scirc;vebas
        <fnt>
          <aut>Stellan Engholm</aut>
          <vrk>Vivo vokas</vrk>
          <lok>1946</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kur&scirc;a terlango terhistorie formata el mar- kaj vent-<tld/>igata sablo
        <fnt>
          <aut>Laimius Stra&zcaron;nickas</aut>
            <vrk><url ref="&Monato;/2001/007273.php">Terlango -- natura valoro</url></vrk>
            <bib>Monato</bib>
            <lok>2001/02, p. 21</lok>
          </fnt>;
      </ekz>
      <ekz>
        li <tld/>is dek mejlojn norden kun la fluo kaj ku&scirc;is en la akvo tri semajnojn,
        <klr>[&#8230;]</klr> li estis preska&ubreve; nerekonebla
        <fnt>
          <bib>VRA</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a&jcirc;oj da&ubreve;re kaj amase albordi&gcirc;os
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2012/010800.php">Alflosa&jcirc;oj trans la oceano</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu benata stato, kiu ne estas kompleta dormo, sed en kiu oni pla&ccirc;e <tld/>as de
        unu revo al alia
        <fnt>
          <bib>CKv</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>ante inter son&gcirc;oj kaj veki&gcirc;o
        <fnt>
          <aut>Anna L&ouml;wenstein</aut>
          <vrk>La &scirc;tona urbo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i en la dekstrakonservativan partispektron
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2005/008502.php">La Blok blokita ... a&ubreve; &ccirc;u?</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="glis.0i">glisi</ref>,
      <ref cel="glit.0i.antauxenigxi">gliti<sncref/></ref>,
      <ref cel="flu.laux0e">la&ubreve;flue</ref>,
      <ref cel="nagx.0i">na&gcirc;i</ref>,
      <ref cel="vag.0i">vagi</ref>,
      <ref cel="vel.0i">veli</ref>
    </refgrp>
    <trdgrp lng="it">
    <trd>scadere<klr> (mar.)</klr></trd>
    <trd>scarrocciare</trd>
    <trd>andare alla <ind>deriva</ind></trd>
  </trdgrp>
  </snc>
  <snc mrk="driv.0i.devojigxi">
    <dif>
      <ref tip="super" cel="flank.de0igxi">Deflanki&gcirc;i</ref> de la intencita direkto pro pu&scirc;o de
      vento
      a&ubreve; akvofluo:
      <ekz>
        la kuglo <tld/>is pro la forta vento, kaj tial &gcirc;i ne
        trafis la celon
        <fnt><bib>OA9</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="devi.0i.FIG">devii<sncref/></ref>,
       <ref cel="voj.de0igxi">devoji&gcirc;i</ref>,
       <ref cel="ir.flanken0i">flankeniri</ref>,
       <ref cel="jor.0i">jori</ref>
    </refgrp>
    <trd lng="de">abdriften</trd>
    <trd lng="it">deviare<klr> (intr.)</klr></trd>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_e;&c_j;&c_f;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>treiben</trd>,
  </trdgrp>
  <trd lng="en">drift</trd>
  <trd lng="es">ir a la <ind>deriva</ind></trd>
  <trdgrp lng="fr">
    <trd>d&eacute;river<klr tip="ind"> (aller &agrave; la
    d&eacute;rive)</klr></trd>
    <trd>aller &agrave; la <ind>d&eacute;rive</ind></trd>
    <trd>&ecirc;tre &agrave; la <ind>d&eacute;rive</ind></trd>
  </trdgrp>
  <trd lng="hu">sodr&oacute;dik</trd>
  <trdgrp lng="ja">
    <trd>&#x6F02;&#x6D41;&#12377;&#x308B;  <pr>&#12402;&#12423;&#12358;&#x308A;&#12421;&#12358;&#12377;&#x308B;</pr></trd>,
    <trd>&#x6D41;&#12373;&#x308C;&#x308B;  <pr>&#x306A;&#x304C;&#12373;&#x308C;&#x308B;</pr></trd>,
    <trd>&#x504F;&#x6D41;&#12377;&#x308B;  <pr>&#12408;&#12435;&#x306A;&#x304C;&#12377;&#x308B;</pr></trd>,
    <trd>&#x308F;&#x304D;&#x306B;&#x305D;&#x308C;&#x308B;</trd>
  </trdgrp>
  <trd lng="pl">dryfowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>derivar <klr tip="amb">(n&aacute;ut.)</klr></trd>,
    <trd>ficar &agrave; <ind>deriva</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_r;&c_je;&c_j;&c_f;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_l;&c_y;&c_t;&c_mol; &c_p;&c_o;
    &c_t;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_ju;</trd>,
    <trd>&c_p;&c_l;&c_y;&c_t;&c_mol; &c_p;&c_o;
    &c_v;&c_je;&c_t;&c_r;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_r;&c_je;&c_j;&c_f;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&#x43B;&c_i;&c_s;&c_t;&c_i; &c_z;&c_a; &c_t;&c_je;&c_ch;&c_ib;&c_jeu;&#x44E; (&c_a;&c_b;&#x43E; &#x43F;&#x43E; &c_v;&c_ib;&c_t;&c_r;&c_u;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6CB2; <pr>du&#xF2;</pr></trd>,
    <trd>&#x88AB;&#x5F79;&#x4F7F;&#x8FC7;&#x6E21;
    <pr>b&#xE8;iy&#xEC;sh&#x1D0;gu&#xF2;d&#xF9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="driv.0o">
  <kap><ofc>9</ofc><tld/>o <fnt><bib>PV</bib></fnt></kap>
  <snc mrk="driv.0o.ago0i">
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <snc mrk="driv.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malrapida &scirc;an&gcirc;i&gcirc;ado a&ubreve; movi&gcirc;ado en
      ia sistemo (sen klara celo, direkto a&ubreve; intenco):
      <ekz>
        lingva <tld/>o
        <fnt><bib>OA9</bib></fnt>;
      </ekz>
      <ekz>
        la malrapida <tld/>o de la nacia gazetaro, pli kaj pli troiganta, partoprenas en
        la venenigo de la politika klimato
        <fnt>
          <bib>MD11</bib>
          <vrk>&Ccirc;u &jcirc;urnalistoj a&ubreve; militantoj?</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Drift</trd>
    <trdgrp lng="zh">
    <trd>&#x8FF4;&#x65CB; <pr>hu&#xED;xu&#xE1;n</pr></trd>,
    <trd>&#x6F02;&#x6D6E; <pr>pi&amacron;of&#xFA;</pr></trd>,
    <trd>&#x98A8;&#x843D; <pr>f&emacron;nglu&#xF2;</pr></trd>,
    <trd>&#36880;&#x6E10;&#38519;&#20837; <pr>zh&#xFA;ji&#xE0;nxi&#xE0;nr&#xF9;</pr></trd>,
    <trd>&#x6F02;&#x79EF;&#29289; <pr>pi&#xE0;oj&#x12B;w&#xF9;</pr></trd>,
    <trd>&#x4F7F;&#x6F02;&#x6D41; <pr>sh&#x1D0;pi&amacron;oli&#xFA;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_d;&c_r;&c_e;&c_j;&c_f;</trd>
  <trd lng="en">drift</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;rive<klr tip="ind"> (action de
    d&eacute;river)</klr></trd>
    <trd>d&eacute;rapage<klr tip="ind"> (d&eacute;rive)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>deriva</trd>
    <trd>scarroccio</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6F02;&#x6D41;  <pr>&#12402;&#12423;&#12358;&#x308A;&#12421;&#12358;</pr></trd>,
    <trd>&#x504F;&#x6D41;  <pr>&#12408;&#12435;&#x308A;&#12421;&#12358;</pr></trd>,
    <trd>&#x30C9;&#x30EA;&#x30D5;&#x30C8;</trd>
  </trdgrp>
  <trd lng="pl">dryf</trd>
</drv>

<drv mrk="driv.for0i">
  <kap>for<tld/>i</kap>
  <snc mrk="driv.for0i.MAR">
    <dif>
      Longe <tld/>i, malproksimi&gcirc;ante:
      <ekz>
        la nuboj <klr>[&#8230;]</klr> dispeli&gcirc;is kaj for<tld/>is en la
        spaco
        kiel malpeza, molkontura vapora&jcirc;o
        <fnt><bib>InfanTorent2</bib>, <lok>dua parto, &ccirc;apitro
        2a</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la ministro pri financo en Baden-Virtembergo jam plendis pro la for<tld/>antaj
        1,5 miliardoj, sed la procedo &scirc;ajnas esti plene la&ubreve;le&gcirc;a
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2012/010784.php">Profitdona fuzio</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">abtreiben</trd>
    <trd lng="zh">&#x88AB;&#x5F79;&#x4F7F;&#x8FC7;&#x6E21;
    <pr>b&#xE8;iy&#xEC;sh&#x1D0;gu&#xF2;d&#xF9;</pr></trd>
</snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_r;&c_e;&c_j;&c_f;&c_a;&c_v;&c_a;&c_c;&c_mol; &c_u;&c_d;&c_a;&c_l;&c_je;&c_ch;</trd>,
    <trd>&c_a;&c_d;&c_p;&c_l;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">odplavit se</trd>
  <trd lng="en">drift away</trd>
  <trdgrp lng="fr">
    <trd>partir &agrave; la <ind>d&eacute;rive</ind></trd>
    <trd>s'&eacute;loigner en <ind>d&eacute;rivant</ind></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>allontanarsi alla <ind>deriva</ind></trd>
  </trdgrp>
  <trd lng="pl">zdryfowa&cacute;</trd>
  <trd lng="sk">odplavi&tcaron; sa</trd>
</drv>

<drv mrk="driv.kontinento0o">
  <kap>kontinento<tld/>o,
    <var><kap>kontinenta <tld/>o
    <fnt><bib>OA9</bib></fnt></kap></var>
  </kap>
  <snc mrk="driv.kontinento0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      La&ubreve; geologia teorio tre malrapida glito de kontinentoj tra
      jarmilionoj:
      <ekz>
        &gcirc;ia ekzisto en la kanjono elokvente pruvis la doktrinon de
        kontinenta <tld/>o
        <fnt>
        <aut>Pang Naiying</aut>,
        <vrk>Tra la kanjono de Yarlung Zangbo</vrk>,
        <lok><url ref="http://www.china.org.cn/EL/EL/ElPopolaCxinio/">
        El popola &Ccirc;inio, 1999-10</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Kontinentaldrift</trd>
    <trd lng="zh">&#22823;&#38470;&#x6F02;&#x79FB;&#x5B66;&#x8BF4;
    <pr>d&#xE0;l&#xF9;pi&amacron;oy&#xED;xu&#xE9;shu&#x14D;</pr></trd>
</snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_r;&c_e;&c_j;&c_f; &c_m;&c_a;&c_c;&c_je;&c_r;&c_y;&c_k;&c_o;&c_w;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_t;&c_y;&c_n;&c_e;&c_n;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_y; <ind>&c_d;&c_r;&c_e;&c_j;&c_f;</ind></trd>
  </trdgrp>
  <trd lng="en">continental drift</trd>
  <trd lng="fr">d&eacute;rive des continents</trd>
  <trd lng="it"><ind>deriva</ind> dei continenti</trd>
  <trd lng="ja">&#22823;&#38520;&#x79FB;&#x52D5;  <pr>&#x305F;&#12356;&#x308A;&#x304F;&#12356;&#12393;&#12358;</pr></trd>
  <trdgrp lng="pl">
    <trd>dryf kontynentalny</trd>,
    <trd>w&eogonek;dr&oacute;wka kontynent&oacute;w</trd>,
    <trd>epejroforeza</trd>
  </trdgrp>
</drv>
</art>
