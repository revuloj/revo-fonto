<vortaro>
<art mrk="$Id$">
<kap>
  <rad>bide</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="bide.0o">
  <kap><tld/>o</kap>
  <snc mrk="bide.0o.MEB">
    <dif>
      Malgranda pelvo, fojfoje kun kvar piedoj,
      sur kiu oni rajde sidi&gcirc;as por lavi la mezan parton de la korpo.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ib;&c_d;&c_e;</trd>
  <trd lng="de">Bidet</trd>
  <trd lng="en">bidet</trd>
  <trd lng="fr">bidet</trd>
  <trdgrp lng="hu">
    <trd>bid&eacute;</trd>,
    <trd>&uuml;l&odblac;mosd&oacute;</trd>
  </trdgrp>
  <trd lng="nl">bidet</trd>
  <trd lng="ru">&c_b;&c_i;&c_d;&c_je;</trd>
  <trd lng="sk">bidet</trd>
</drv>
</art>
