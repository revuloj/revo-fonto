<art mrk="$Id: bide.xml,v 1.19 2025/03/28 06:05:24 revo Exp $">
<kap>
  <rad>bide</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="bide.0o">
  <kap><tld/>o</kap>
  <snc mrk="bide.0o.MEB">
    <dif>
      Malgranda <ref tip="super" cel="pelv.0o.vazo">pelvo</ref>,
      sur kiu oni rajde sidi&gcirc;as por lavi la mezan parton de la
      korpo:
      <ekz>
        lavnecesejo estas kombino inter necesejo kaj <tld/>o, kiu ebligas anusan
        higienon per akvo anstata&ubreve; per neceseja papero
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=287495">Lavnecesejo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>sidkuvo</ke>, <ke>sidpelvo</ke>.
  </rim>

  <trd lng="be">&c_b;&c_ib;&c_d;&c_e;</trd>
  <trd lng="bg">&c_b;&c_i;&c_d;&c_je;</trd>
  <trd lng="ca">bidet</trd>
  <trd lng="cs">bidet</trd>
  <trd lng="de">Bidet</trd>
  <trd lng="en">bidet</trd>
  <trd lng="fr">bidet</trd>
  <trdgrp lng="he">
    <trd>&#1489;&#1497;&#1491;&#1492;</trd>,
    <trd>&#1488;&#1502;&#1489;&#1496; &#1497;&#1513;&#1497;&#1489;&#1492;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bid&eacute;</trd>,
    <trd>&uuml;l&odblac;mosd&oacute;</trd>
  </trdgrp>
  <trd lng="ja">&#12499;&#12487;</trd>
  <trd lng="nl">bidet</trd>
  <trd lng="pl">bidet</trd>
  <trd lng="ru">&c_b;&c_i;&c_d;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>bidet</trd>,
    <trd>sedacie um&yacute;vadlo</trd>
  </trdgrp>
  <trd lng="uk">&#1073;&#1110;&#1076;&#1077;</trd>
</drv>
</art>
