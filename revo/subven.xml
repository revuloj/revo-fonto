<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: subven.xml,v 1.21 2022/03/29 14:19:46 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>subvenci</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="subven.0o">
  <kap><tld/>o</kap>
  <snc mrk="subven.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Oficiala <ref tip="super" cel="help.mon0o.KOMUNE">monhelpo</ref>:
      <ekz>
        <tld/>oj estis ofte uzataj por kura&gcirc;igi investadon en specifaj
        industrioj
        <fnt>
          <aut>D. Rogers</aut>,
          <vrk>Ekonomia miraklo a&ubreve; malsano?</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 1997a, numero 2a, p. 14a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mecena.0o">mecenato</ref>
    </refgrp>
  </snc>
  <trd lng="bg">&c_s;&c_u;&c_b;&c_s;&c_i;&c_d;&c_i;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>fin. podpora z ve&#x159;ejn&#xFD;ch prost&#x159;edk&#x16F;</trd>,
    <trd>subvence</trd>
  </trdgrp>
  <trd lng="de">Subvention</trd>
  <trdgrp lng="en"><trd>subsidy</trd>, <trd>grant</trd></trdgrp>
  <trd lng="fa">&f_y;&a_A;&a_r;&a_A;&a_n;&a_h;</trd>
  <trd lng="fr">subvention</trd>
  <trdgrp lng="hu">
    <trd>seg&eacute;ly</trd>,
    <trd>t&aacute;mogat&aacute;s <klr tip="ind">(p&eacute;nz&uuml;gyi)</klr></trd>,
    <trd>szubvenci&oacute;</trd>
  </trdgrp>
  <trd lng="it">sovvenzione</trd>
  <trd lng="nl">subsidie</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_b;&c_s;&c_i;&c_d;&c_i;&c_ja;</trd>,
    <trd>&c_d;&c_o;&c_t;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_d;&c_je;&c_n;&c_je;&c_zh;&c_n;&c_o;&c_je; <ind>&c_p;&c_o;&c_s;&c_o;&c_b;&c_i;&c_je;</ind></trd>
  </trdgrp>
  <trd lng="sk">subvencia</trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x443;&#x431;&#x432;&#x435;&#x43D;&#x446;&#x456;&#x44F;</trd>,
    <trd>&#x441;&#x443;&#x431;&#x441;&#x438;&#x434;&#x456;&#x44F;</trd>,
    <trd>&#x434;&#x43E;&#x442;&#x430;&#x446;&#x456;&#x44F;</trd>,
    <trd>&#x433;&#x440;&#x43E;&#x448;&#x43E;&#x432;&#x430; &#x434;&#x43E;&#x43F;&#x43E;&#x43C;&#x43E;&#x433;&#x430;</trd>
  </trdgrp>
</drv>

<drv mrk="subven.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="subven.0i.POL">
    <dif>
      Provizi oficialan monhelpon:
      <ekz>
        Usono plendas &ccirc;ar EU da&ubreve;re <tld/>as sian
        agrikulturon
        <fnt>
          <aut>R. Rotsaert</aut>,
          <vrk>Jarmila konferenco &hcirc;aose malsukcesis</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2000a, numero 2a, p. 10a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>dotovat</trd>,
    <trd>podporovat</trd>,
    <trd>subvencovat</trd>
  </trdgrp>
  <trd lng="de">subventionieren</trd>
  <trd lng="en">subsidize</trd>
  <trd lng="fa">&f_y;&a_A;&a_r;&a_A;&a_n;&a_h; &a_d;&a_A;&a_d;&a_n;<klr> (&a_b;&a_h;)</klr></trd>
  <trdgrp lng="fr">
    <trd>subventionner</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>seg&eacute;lyez</trd>,
    <trd>t&aacute;mogat <klr tip="ind">(p&eacute;nz&uuml;gyileg)</klr></trd>,
    <trd>dot&aacute;l</trd>
  </trdgrp>
  <trd lng="it">sovvenzionare</trd>
  <trd lng="nl">subsidi&euml;ren</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_b;&c_s;&c_i;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_o;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol; <ind>&c_d;&c_o;&c_t;&c_a;&c_c;&c_i;&c_ju;</ind></trd>,
    <trd>&c_o;&c_k;&c_a;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol; <ind>&c_d;&c_je;&c_n;&c_je;&c_zh;&c_n;&c_u;&c_ju;
    &c_p;&c_o;&c_m;&c_o;&c_shch;&c_mol;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dotova&#x165;</trd>,
    <trd>podporova&#x165;</trd>,
    <trd>subvencova&#x165;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x441;&#x443;&#x431;&#x441;&#x438;&#x434;&#x456;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43D;&#x430;&#x434;&#x430;&#x432;&#x430;&#x442;&#x438; &#x433;&#x440;&#x43E;&#x448;&#x43E;&#x432;&#x443; &#x434;&#x43E;&#x43F;&#x43E;&#x43C;&#x43E;&#x433;&#x443;</trd>
  </trdgrp>
</drv>

<drv mrk="subven.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="subven.0ado.POL">
    <dif>
      Da&ubreve;ra helpo per mondonacoj:
      <ekz>
        dum la intertraktado pri la &Gcirc;enerala Konvencio pri Dogano kaj
        Komerco, Usono kritikis la <tld/>adon de film-produktado
        <fnt>
          <aut>R. F&ouml;&szlig;meier</aut>:
          <vrk>La merkato de lingvoj</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 1997a, numero 11a, p. 22a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Subvention</trd>
  <trdgrp lng="fr">
    <trd>subvention<klr> (r&eacute;guli&egrave;re)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>seg&eacute;lyez&eacute;s</trd>,
    <trd>t&aacute;mogat&aacute;s <klr tip="ind">(p&eacute;nz&uuml;gyi)</klr></trd>,
    <trd>dot&aacute;l&aacute;s</trd>
  </trdgrp>
  <trd lng="it">sovvenzione</trd>
  <trd lng="nl">subsidi&euml;ren<klr tip="ind"> (zn.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_b;&c_s;&c_i;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;&c_ja;</trd>,
    <trd>&c_d;&c_o;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
