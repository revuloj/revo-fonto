<drv mrk="kalkul.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kalkul.pri0i.zorge">
    <dif>
      Zorge pripensi estontan agon helpe de <tld/>ado; pesi &ccirc;iujn
      konsekvencojn de sia agado:
      <ekz>
        &scirc;i konsideris la longecon de la vojo kaj pri<tld/>is,
        ke, se &scirc;i ne sekvos la landvojon, <klr>[...]</klr>
        &scirc;i &scirc;paros preska&ubreve; du mejlojn
        <fnt><bib>Fab4</bib><lok>Anneto</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i ne pri<tld/>is sian agon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi &ccirc;iam agas orde, kun pripenso kaj <ind>pri<tld/>o</ind>
        <fnt><bib>Fab4</bib><lok>Kuristoj</lok></fnt>.
        <trdgrp lng="fr">
          <trd>pr&eacute;voyance</trd>,
          <trd>pr&eacute;m&eacute;ditation</trd>,
          <trd>calcul<klr tip="ind"> (pr&eacute;voyance)</klr></trd>,
          <trd>pond&eacute;ration<klr tip="ind">
          (pr&eacute;voyance)</klr></trd>
        </trdgrp>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="plan.0i">plani</ref>
    </refgrp>
    <trd lng="be">&c_p;&c_r;&c_a;&c_l;&c_ib;&c_ch;&c_y;&c_c;&c_mol;</trd>
    <trdgrp lng="ca">
      <trd>preveure</trd>,
      <trd>calcular</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>prever</trd>,
      <trd>calcular</trd>
    </trdgrp>
    <trd lng="de">durchrechnen</trd>
    <trdgrp lng="fr">
      <trd>calculer<klr tip="ind"> (peser les
      cons&eacute;quences)</klr></trd>,
      <trd>pr&eacute;m&eacute;diter</trd>,
      <trd>pr&eacute;voir<klr tip="ind">
      (pr&eacute;m&eacute;diter)</klr></trd>
    </trdgrp>
    <trd lng="hu">m&eacute;rlegel</trd>
    <trd lng="nl">overwegen</trd>
    <trd lng="pl">rozwa&zdot;a&cacute;</trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_ch;&c_i;&c_t;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="kalkul.pri0i.nombri_homojn">
    <uzo tip="fak">BIB</uzo>
    <dif>
      Nombri la homojn:
      <ekz>
        pri<tld/>u la tutan komunumon de la Izraelidoj la&ubreve; iliaj
        familioj
        <fnt><bib>MT</bib><lok>&Nom; 1:2</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_l;&c_ib;&c_ch;&c_y;&c_c;&c_mol; <klr tip="amb">(&c_l;&c_ju;&c_d;&c_z;&c_je;&c_j;,
      &c_n;&c_a;&c_s;&c_je;&c_l;&c_mol;&c_n;&c_ib;&c_c;&c_t;&c_v;&c_a;)</klr></trd>,
      <trd>&c_p;&c_a;&c_d;&c_l;&c_ib;&c_ch;&c_y;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="ca">censar</trd>
    <trd lng="es">censar</trd>
    <trd lng="fr">recenser<klr tip="ind"> (une population)</klr></trd>
    <trd lng="pl">policzy&cacute;<klr> (ludzi, mieszka&nacute;c&oacute;w)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_o;&c_s;&c_ch;&c_i;&c_t;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_o;&c_d;&c_s;&c_ch;&c_i;&c_t;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>propo&ccaron;&iacute;tat</trd>,
    <trd>spo&ccaron;&iacute;tat</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>prepo&ccaron;&iacute;ta&tcaron;</trd>,
    <trd>spo&ccaron;&iacute;ta&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="kalkul.diferenciala0o">
  <kap>diferenciala <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 20</lok></fnt>
