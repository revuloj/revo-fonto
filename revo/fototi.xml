<kap><rad>fototipi</rad>/o<fnt><bib>PVEF</bib></fnt></kap>
<drv mrk="fototi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TIP</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="proced.0o">Procedo</ref>, uzata &ccirc;irka&ubreve; la jaro 1900,
      permesanta ricevi reliefajn <ref cel="klisx.0o.TIP">kli&scirc;o</ref>jn, el kiuj
      oni povas reprodukti bildojn per presarto:
      <ekz>
        en 1889 d-ro <nom>Alfons Heyer</nom> malkovris la planon <klr>[&#8230;]</klr>, <klr>[de kiu la
        urbo farigis]</klr> en 1897 per <tld/>o kolorajn kopiojn en origina grando
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=559202">Corputius-Plano</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_o;&c_t;&c_a;&c_t;&c_y;&c_p;&c_ib;&c_ja;</trd>
  <trd lng="ca">fotot&iacute;pia</trd>
  <trd lng="cs">fototypie</trd>
  <trdgrp lng="de">
    <trd>Lichtdruck</trd>,
    <trd>Phototypie</trd>
  </trdgrp>
  <trd lng="en">phototypy <klr>(typography)</klr></trd>
  <trd lng="es">fototipia</trd>
  <trd lng="fr">phototypie</trd>
  <trd lng="hu">fotot&iacute;pia</trd>
  <trdgrp lng="ja">
    <trd>&#20889;&#30495;&#20984;&#29256;&#34899;  <pr>&#12375;&#12419;&#12375;&#12435;&#12392;&#12388;&#12399;&#12435;&#12376;&#12421;&#12388;</pr></trd>,
    <trd>&#12501;&#12457;&#12488;&#12479;&#12452;&#12503;&#34899;  <pr>&#12501;&#12457;&#12488;&#12479;&#12452;&#12503;&#12376;&#12421;&#12388;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>lichtdruk</trd>,
    <trd>fototypie</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fototipia</trd>,
    <trd>colografia</trd>,
    <trd>colotipia</trd>
