  <trd lng="uk">&c_f;&c_o;&c_t;&c_o;&c_t;&c_i;&c_p;&c_ib;&c_ja;</trd>
  <trd lng="zh">&#29634;&#32599;&#29256;&#21360;&#21047; <pr>k&emacron;lu&oacute;b&#462;ny&igrave;nshu&agrave;</pr></trd>
