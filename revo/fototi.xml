<kap><rad>fototipi</rad>/o</kap>
<drv mrk="fototi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TIP</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="proced.0o">Procedo</ref>, uzata &ccirc;irka&ubreve; la jaro 1900,
      permesanta ricevi reliefajn <ref cel="klisx.0o.TIP">kli&scirc;o</ref>jn, el kiuj 
      oni povas reprodukti bildojn per presarto:
      <ekz>
        en 1889 d-ro <nom>Alfons Heyer</nom> malkovris la planon <klr>[&#x2026;]</klr>, <klr>[de kiu la
        urbo farigis]</klr> en 1897 per <tld/>o kolorajn kopiojn en origina grando
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=559202">Corputius-Plano</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_o;&c_t;&c_a;&c_t;&c_y;&c_p;&c_ib;&c_ja;</trd>
  <trd lng="ca">fotot&iacute;pia</trd>
  <trd lng="cs">fototypie</trd>
  <trdgrp lng="de">
    <trd>Lichtdruck</trd>,
    <trd>Phototypie</trd>
  </trdgrp>
  <trd lng="es">fototipia</trd>
  <trd lng="fr">phototypie</trd>
  <trd lng="hu">fotot&iacute;pia</trd>
  <trdgrp lng="ja">
    <trd>&#x5199;&#x771F;&#x51F8;&#x7248;&#x8853;  <pr>&#x3057;&#x3083;&#x3057;&#x3093;&#x3068;&#x3064;&#x306F;&#x3093;&#x3058;&#x3085;&#x3064;</pr></trd>,
    <trd>&#x30D5;&#x30A9;&#x30C8;&#x30BF;&#x30A4;&#x30D7;&#x8853;  <pr>&#x30D5;&#x30A9;&#x30C8;&#x30BF;&#x30A4;&#x30D7;&#x3058;&#x3085;&#x3064;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>lichtdruk</trd>,
    <trd>fototypie</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fototipia</trd>,
    <trd>colografia</trd>,
    <trd>colotipia</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_o;&c_t;&c_o;&c_t;&c_i;&c_p;&c_i;&c_ja;</trd>
  <trd lng="sk">fototypia</trd>
  <trd lng="uk">&#x444;&#x43E;&#x442;&#x43E;&#x442;&#x438;&#x43F;&#x456;&#x44F;</trd>
  <trd lng="zh">&#x73C2;&#x7F57;&#x7248;&#x5370;&#x5237; <pr>k&#x113;lu&#xF3;b&#x1CE;ny&#xEC;nshu&#xE0;</pr></trd>
