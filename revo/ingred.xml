<art mrk="$Id: ingred.xml,v 1.20 2022/10/18 18:54:36 revo Exp $">
<kap>
  <ofc>6</ofc>
  <rad>ingredienc</rad>/o
  <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ingred.0o">
  <kap><tld/>o</kap>
  <snc mrk="ingred.0o.KOMUNE">
    <uzo tip="fak">KUI</uzo>
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="konsis.0ajxo.KOMUNE">Konsista&jcirc;o</ref>;
      &ccirc;iu aparta substanco en miksa&jcirc;o, trinka&jcirc;o, medikamento...:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la plej maldol&ccirc;a <tld/>o en lia sortopokalo estis
        memripro&ccirc;oj pri nedireble blinda malsa&gcirc;o kaj malforteco 
        de la volo
        <fnt><bib>I&scirc;tar</bib><lok>&ccirc;apitro 17a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="elemen.0o">elemento</ref>,
      <ref cel="er.0o">ero</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_ib;&c_n;&c_g;&c_r;&c_e;&c_d;&c_y;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_o;&c_v;&c_a;&c_ja; &c_ch;&c_a;&c_s;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ingredience</trd>,
    <trd>n&#xE1;poje a j.</trd>,
    <trd>p&rcaron;im&#xED;&scaron;enina</trd>,
    <trd>p&rcaron;&#xED;sada</trd>,
    <trd>sou&#x10D;&#xE1;stka l&#xE9;ku</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Zutat</trd>,
    <trd>Bestandteil</trd>,
    <trd>Ingredienz<klr> [ark]</klr></trd>
  </trdgrp>
  <trd lng="en">ingredient</trd>
  <trd lng="es">ingrediente</trd>
  <trd lng="fr">ingr&eacute;dient</trd>
  <trdgrp lng="hu">
    <trd>&ouml;sszetev&odblac;</trd>,
    <trd>alkot&oacute;elem</trd>,
    <trd>hozz&aacute;val&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bestanddeel</trd>,
    <trd>ingredi&euml;nt</trd>
  </trdgrp>
  <trd lng="pt">ingrediente</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_g;&c_r;&c_je;&c_d;&c_i;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_s;&c_o;&c_s;&c_t;&c_a;&c_v;&c_n;&c_a;&c_ja;
    &c_ch;&c_a;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ingrediencia</trd>,
    <trd>primie&scaron;anina</trd>,
    <trd>pr&#xED;sada</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ib;&#x43D;&c_gu;&c_r;&c_je;&c_d;&c_ib;&c_jeu;&#x43D;&c_t;</trd>,
    <trd>&c_s;&#x43A;&#x43B;&c_a;&c_d;&#x43E;&c_v;&c_a; &c_ch;&c_a;&c_s;&c_t;&c_i;&#x43D;&c_a;</trd>,
    <trd>&c_s;&#x43A;&#x43B;&c_a;&c_d;&#x43D;&c_i;&#x43A;</trd>
  </trdgrp>
</drv>
