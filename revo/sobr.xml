<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sobr</rad>/a
</kap>
<drv mrk="sobr.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="sobr.0a.KUI">
    <dif>
      Modere man&gcirc;anta a&ubreve; trinkanta.
    </dif>
    <refgrp tip="vid">
      <ref cel="abstin.0i">abstini</ref>,
      <ref cel="fast.0i">fasti</ref>
    </refgrp>
  </snc>
  <snc mrk="sobr.0a.nedrinkinta">
    <dif>
      Ne sub influo de alkoholo.
    </dif>
    <refgrp tip="vid">
      <ref cel="ebri">malebria</ref>,
      <ref cel="drink.0i">nedrinkinta</ref>
    </refgrp>
  </snc>
  <snc mrk="sobr.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Modera, singarda, senpasia kaj senpartia:
      <ekz>
        <tld/>aj esprimoj;
      </ekz>
      <ekz>
        &ccirc;iu <tld/>e rigardanta homo povas tuj rimarki, ke<fnt>Z</fnt>.
      </ekz>
    </dif>
    </snc>
  <snc mrk="sobr.0a.kuirata">
    <dif>
      Man&gcirc;a&jcirc;o sen spico kaj salo.
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_m;&c_ja;&c_r;&c_k;&c_o;&c_w;&c_n;&c_y;</trd>, 
      <trd>&c_u;&c_s;&c_t;&c_r;&c_y;&c_m;&c_a;&c_n;&c_y;</trd>, 
      <trd>&c_s;&c_mol;&c_c;&c_ib;&c_p;&c_l;&c_y;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_c;&c_mol;&c_v;&c_ja;&c_r;&c_o;&c_z;&c_y;</trd>
  <trd lng="ca">sobri</trd>
  <trdgrp lng="cs">
    <trd>st&rcaron;&iacute;dm&yacute;</trd>,
    <trd>st&rcaron;&iacute;zliv&yacute;</trd>,
    <trd>zdr&zcaron;enliv&yacute;</trd>
  </trdgrp>
  <trd lng="de">n&uuml;chtern</trd>
  <trd lng="en">sober</trd>
  <trd lng="es">sobrio, -a</trd>
  <trd lng="fr">sobre</trd>
  <trd lng="hu">j&oacute;zan</trd>
  <trdgrp lng="nl">
    <trd>nuchter</trd>,
    <trd>sober</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_z;&c_v;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>striezliv&yacute;</trd>,
    <trd>triezvy</trd>
  </trdgrp>
</drv>

<drv mrk="sobr.mal0a">
  <kap><ofc>*</ofc>mal<tld/>a <fnt><bib>F</bib>, <lok>Universala
  Vortaro, <url
  ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5203">mal'sobr'</url></lok></fnt></kap>
  <snc>
    <dif>
     Nemodere man&gcirc;anta a&ubreve; trinkanta.
    </dif>
  </snc>
  <trd lng="fr">goinfre</trd>
  <snc>
    <uzo tip="stl">ARK</uzo>
    <dif>
    U.V. senco: sub influo de alkoholo.
    </dif>
     <ref tip="sin" cel="ebri.0a">ebria.</ref>
     <ref tip="vid" cel="drink.0i">drinkinta</ref>
  <trd lng="de">betrunken</trd>
  <trd lng="en">drunk</trd>
  <trd lng="fr">ivre</trd>
</snc>
  </drv>

<drv mrk="sobr.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="sobr.0igxi.KOMUNE">
    <dif>
      Moderi&gcirc;i, eviti ebriecon, eksceson, troigon, patoson...:
      <ekz>
        tiu, kiu vivis sendiscipline, sed poste <tld/>i&gcirc;is, briligas la 
        mondon kiel la luno libera de nuboj
        <fnt><aut>trad. T. T. Anuruddha</aut>, <vrk>La Vojo al Nirvano 
        a&ubreve; la Dhammapado, 13:172</vrk>, <lok>B&#7891;-T&aacute;t-Csoma-Instituto
        por  
        Budhologio, 1973</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>moderar-se</trd>,
    <trd>abstenir-se</trd>
  </trdgrp>
</drv>
</art>
