  <trd lng="hu">rom&aacute;nc</trd>
  <trd lng="ja">&#x30ED;&#x30DE;&#x30F3;&#x30B9;</trd>
  <trd lng="nl">romance</trd>
  <trd lng="pl">romanca</trd>
  <trdgrp lng="pt">
    <trd>roman&ccedil;a</trd>,
    <trd>romance</trd>
  </trdgrp>
  <trd lng="ro">roman&#x21B;&abreve;</trd>
  <trd lng="ru">&c_r;&c_o;&c_m;&c_a;&c_n;&c_s;</trd>
  <trd lng="sk">romanca</trd>
  <trd lng="uk">&c_r;&#x43E;&#x43C;&c_a;&#x43D;&c_s;</trd>
