<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: boaed.xml,v 1.3 2023/11/23 12:45:49 revo Exp $">
<kap>
  <rad>boaed</rad>/oj
</kap>
<drv mrk="boaed.0oj">
  <kap>
    <tld/>oj
  </kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref>
      el la ordo <ref tip="malprt" cel="skvam.0uloj">skvamuloj</ref>
      (<trd lng="la">Boidae</trd>), de grandaj
      sufokserpentoj:
      <ekz>
        la <tld/>oj estas familio de nevenenaj serpentoj trovi&gcirc;antaj en Ameriko, Afriko,
        E&ubreve;ropo, Azio, kaj kelkaj el la insuloj de Pacifiko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=506294"
              >Boaedoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
       <ref cel="boa.0o.ZOO">boao</ref>,
       <ref cel="piton.0o">pitono</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_d;&c_a;&c_v;&c_y; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>,
    <trd>&c_n;&c_je;&c_s;&c_a;&c_p;&c_r;&c_a;&c_w;&c_d;&c_n;&c_a;&c_n;&c_o;&c_g;&c_ib;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Boas</trd>,
    <trd>Riesenschlangen</trd>
  </trdgrp>
</drv>
</art>

