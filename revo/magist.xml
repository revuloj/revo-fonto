<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>magistr</rad>/o <fnt>Z</fnt>
</kap>
<drv mrk="magist.0o">
  <kap><tld/>o</kap>
  <snc mrk="magist.0o.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      Universitata <ref tip="super" cel="titol.0o.RANG">titolo</ref>
      ekzistanta en pluraj landoj kaj ranganta inter
      bakala&ubreve;ro a&ubreve; abituriento kaj doktoro:
      <ekz>
        ekde 1390 li studis en la praga universitato, en 1393 li fari&gcirc;is bakala&ubreve;ro, 
        en 1396 <tld/>o de liberaj artoj, ekde 1398 li prelegis en la artisma fakultato 
        de la universitato, en 1400 li estis ordinita pastro
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007031.html"
              >Katolikismo: Johano Hus &dash; &ccirc;u ne plu herezulo?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la novaj universitataj gradoj estu bakala&ubreve;ro (ses-semestra studado), <tld/>o
        (pliaj du a&ubreve; kvar semestroj) kaj doktoro
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="http://www.monato.net/2010/010037.php"
              >Reformo fu&scirc;ita</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">m&agrave;ster</trd>
    <trd lng="de">Magister</trd>
    <trdgrp lng="fr">
      <trd>ma&icirc;tre<klr> (titre univ.)</klr></trd>
    </trdgrp>
    <trd lng="en">master's degree</trd>
    <trdgrp lng="fa">
      <trd>&f_k;&a_A;&a_r;&a_sh;&a_n;&a_A;&a_s;&f_y; &a_A;&a_r;&a_sh;&a_d;</trd>,
      <trd>&a_f;&a_w;&a_q; &a_l;&f_y;&a_s;&a_A;&a_n;&a_s;</trd>
    </trdgrp>
    <trd lng="hu">magiszter<klr tip="ind"> (tudom&aacute;nyos
    fokozat)</klr></trd>
    <trd lng="pl">magister</trd>
  </snc>
  <snc mrk="magist.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <ref tip="dif" cel="pedant.0o">Pedanto</ref>
    <trd lng="ca">setci&egrave;ncies</trd>
    <trdgrp lng="fr">
      <trd>cuistre</trd>,
      <trd>p&eacute;dant<klr> (subst.)</klr></trd>
    </trdgrp>
    <trd lng="pl">pedant</trd>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_g;&c_ib;&c_s;&c_t;&c_a;&c_r;</trd>
  <trd lng="bg">&c_m;&c_a;&c_g;&c_i;&c_s;&c_t;&c_malmol;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>magistr</trd>,
    <trd>mistr</trd>
  </trdgrp>
  <trd lng="de">Oberlehrer</trd>
  <trdgrp lng="es">
    <trd>m&aacute;ster</trd>,
    <trd>maestro</trd>
  </trdgrp>
  <trd lng="nl">licentiaat</trd>
  <trd lng="pt">mestre <klr>(t&iacute;t. univers.)</klr></trd>
  <trd lng="ru">&c_m;&c_a;&c_g;&c_i;&c_s;&c_t;&c_r;</trd>
  <trd lng="sk">magister</trd>
</drv>
</art>
