<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: magist.xml,v 1.30 2023/09/02 06:04:07 revo Exp $">
<kap>
  <rad>magistr</rad>/o <fnt>Z</fnt>
</kap>
<drv mrk="magist.0o">
  <kap><tld/>o</kap>
  <snc mrk="magist.0o.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      Universitata <ref tip="super" cel="titol.0o.RANG">titolo</ref>
      ekzistanta en pluraj landoj kaj ranganta inter
      <ref cel="bakala.0o">bakala&ubreve;ro</ref> a&ubreve; abituriento kaj <ref cel="doktor.0o.SCI">doktoro</ref>:
      <ekz>
        pleje ruinigitaj butikistoj, forpelitaj <tld/>oj kaj skribistoj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekde 1390 li studis en la praga universitato, en 1393 li fari&gcirc;is
        bakala&ubreve;ro, en 1396 <tld/>o de liberaj artoj, ekde 1398 li prelegis en la
        artisma fakultato de la universitato, en 1400 li estis ordinita pastro
        <fnt>
          <aut>Ji&rcaron;&iacute; Patera</aut>
            <vrk><url ref="&Monato;/2000/007031.php">Johano Hus &ndash; &ccirc;u ne plu herezulo?</url></vrk>
            <bib>Monato</bib>
            <lok>2000/08, p. 19</lok>
          </fnt>;
      </ekz>
      <ekz>
        la novaj universitataj gradoj estu bakala&ubreve;ro (ses-semestra
        studado), <tld/>o  (pliaj du a&ubreve; kvar semestroj) kaj doktoro
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2010/010037.php">Reformo fu&scirc;ita</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="diplom.0o">diplomo</ref>
    <trd lng="ca">m&agrave;ster</trd>
    <trdgrp lng="de">
      <trd>Magister</trd>,
      <trd>Master</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>ma&icirc;tre<klr> (titre univ.)</klr></trd>
    </trdgrp>
    <trd lng="en">master's degree</trd>
    <trdgrp lng="fa">
      <trd>&f_k;&a_A;&a_r;&a_sh;&a_n;&a_A;&a_s;&f_y; &a_A;&a_r;&a_sh;&a_d;</trd>,
      <trd>&a_f;&a_w;&a_q; &a_l;&f_y;&a_s;&a_A;&a_n;&a_s;</trd>
    </trdgrp>
    <trd lng="hu">magiszter<klr tip="ind"> (tudom&aacute;nyos
    fokozat)</klr></trd>
    <trd lng="pl">magister</trd>
  </snc>
  <snc mrk="magist.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <ref tip="dif" cel="pedant.0ulo">Pedanto</ref>
    <trd lng="ca">setci&egrave;ncies</trd>
    <trdgrp lng="fr">
      <trd>cuistre</trd>,
      <trd>p&eacute;dant<klr> (subst.)</klr></trd>
    </trdgrp>
    <trd lng="pl">pedant</trd>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_g;&c_ib;&c_s;&c_t;&c_a;&c_r;</trd>
  <trd lng="bg">&c_m;&c_a;&c_g;&c_i;&c_s;&c_t;&c_malmol;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>magistr</trd>,
    <trd>mistr</trd>
  </trdgrp>
  <trd lng="de">Oberlehrer</trd>
  <trdgrp lng="es">
    <trd>m&aacute;ster</trd>,
    <trd>maestro</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4FEE;&#x58EB;  [<ind>&#x3057;&#x3085;&#x3046;&#x3057;</ind>]</trd>,
    <trd>&#x5B66;&#x58EB;  [<ind>&#x304C;&#x304F;&#x3057;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">licentiaat</trd>
  <trd lng="pt">mestre <klr>(t&iacute;t. univers.)</klr></trd>
  <trd lng="ru">&c_m;&c_a;&c_g;&c_i;&c_s;&c_t;&c_r;</trd>
  <trd lng="sk">magister</trd>
  <trd lng="uk">&#x43C;&c_a;&c_gu;&c_ib;&c_s;&c_t;&c_r;</trd>
</drv>
</art>
