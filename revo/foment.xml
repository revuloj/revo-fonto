<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>foment</rad>/i
<fnt>Z</fnt>
</kap>
<drv mrk="foment.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Apliki varman kuracilon sur malsanan korpoparton por mildigi,
      kvietigi &gcirc;ian doloron. 
    </dif>
  </snc>
  <trd lng="ru">&c_k;&c_l;&c_a;&c_s;&c_t;&c_mol; &c_p;&c_r;&c_i;&c_p;&c_a;&c_r;&c_k;&c_i;</trd>
</drv>
<drv mrk="foment.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i. 
    </dif>
    <trd lng="ru">&c_p;&c_r;&c_i;&c_p;&c_a;&c_r;&c_k;&c_a; <klr>(&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;)</klr></trd>
  </snc>
</drv>
<drv mrk="foment.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Tio, kion oni <tld/>as. 
    </dif>
  </snc>
  <trd lng="ru">&c_s;&c_o;&c_g;&c_r;&c_je;&c_v;&c_a;&c_ju;&c_shch;&c_i;&c_j; &c_k;&c_o;&c_m;&c_p;&c_r;&c_je;&c_s;&c_s;</trd>
</drv>
<drv mrk="foment.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Havanta ecojn de <tld/>a&jcirc;o:
      <ekz><tld/>aj kompresoj.</ekz>
    </dif>
  </snc>
</drv>
</art>
<!--
$Log$
-->
</vortaro>
