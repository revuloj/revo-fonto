  <trdgrp lng="zh">
    <trd>&#x5811; <pr>qi&#xE0;n</pr></trd>,
    <trd>&#x6C9F;&#x6E20; <pr>g&#x14D;uq&#xFA;</pr></trd>,
    <trd>&#x51FC; <pr>d&#xE0;ng</pr></trd>,
    <trd>&#x58D5; <pr>h&#xE1;o</pr></trd>,
    <trd>&#x8FD0;&#x6CB3; <pr>y&#xF9;nh&#xE9;</pr></trd>,
    <trd>&#x6C34;&#x6C9F; <pr>shu&#x1D0;g&#x14D;u</pr></trd>,
