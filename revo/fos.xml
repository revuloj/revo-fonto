    <trd>&#x7D30;&#x5FC3;&#x6AA2;&#x67E5; <pr>x&#xEC;x&#x12B;nji&#x1CE;nch&#xE1;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fos.pik0ilo">
  <kap>pik<tld/>ilo</kap>
  <snc mrk="fos.pik0ilo.AGR">
    <dif>
      <ref tip="dif" cel="piocx.0o">Pio&ccirc;o</ref>:
      <ekz>
        per fortika frapo de sia malgranda pik<tld/>ilo, li rompis la glacian elstara&jcirc;on
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        uzante la pinton de sia pik<tld/>ilo kiel levilon, li faligis la brikojn, kiuj
        estis plej malbone kunligitaj
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekoka</lok>
        </fnt>;
      </ekz>
      <ekz>
        la homo de nov&scirc;tonepoko setli&gcirc;is, da&ubreve;rigis terkultivadon per pik<tld/>ilo, bredis bestojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3648">Rado</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_t;&c_y;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">krump&aacute;&ccaron;</trd>
  <trdgrp lng="de">
    <trd>Spitzhacke</trd>,
    <trd>zweizinkige <ind>Hacke</ind></trd>,
    <trd>Kreuzhacke</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>pick</trd>,
    <trd>pickaxe</trd>
  </trdgrp>
  <trd lng="es">pico</trd>
  <trd lng="fr">pioche</trd>
  <trd lng="hu">kapa</trd>
  <trd lng="nl">houweel</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_o;&c_t;&c_y;&c_g;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_u;&c_p;</trd>
  </trdgrp>
