<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fos.xml,v 1.39 2022/04/27 17:19:11 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fos</rad>/i
</kap>
<drv mrk="fos.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fos.0i.tero">
    <dif>
      Fari kava&jcirc;on eltirante teron:
      <ekz>
        ili <tld/>is anta&ubreve; mi <tld/>on
        <fnt><bib>MT</bib>, <lok>&Psa; 57:6</lok></fnt>;
      </ekz>
      <ekz>
        &gcirc;ardeno <klr>[...]</klr>, kie &scirc;i povis la&ubreve; sia pla&ccirc;o kaj bontrovo <tld/>i kaj planti
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i &jcirc;eti&gcirc;is sur la teron kaj <tld/>is per siaj manoj tombon en la malmola tero
        <fnt>
          <bib>Fab4</bib>
          <lok>Anneto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la armeo plen&scirc;utis lian kanalon, kiun li <tld/>is en la dezerto dum dek jaroj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        li komencis <tld/>i per amba&ubreve; manoj la teron
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Urbo de Imperiestroj</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bor.0i">bori</ref>,
      <ref cel="kav.0igi">kavigi</ref>,
      <ref cel="plug.0i">plugi</ref>
    </refgrp>
  <trdgrp lng="es">
    <trd>cavar</trd>,
    <trd>excavar</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>cavar</trd>,
    <trd>escavar</trd>,
    <trd>fossar</trd>,
    <trd>minar</trd>,
    <trd>revolver</trd>
  </trdgrp>
  </snc>
  <snc mrk="fos.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Profunde esplori demandon:
      <ekz>
        filozofie <tld/>i en teorioj
        <fnt>
          <bib>LR</bib>
          <vrk><url ref="&EduInf;lern/librejo/0015/0004.php">Akcento &#x2010; 
          Pri la reguloj de la akcento</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="es">profundizar<klr> (en el conocimiento)</klr></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_p;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hloubit <klr>(j&aacute;mu)</klr></trd>,
    <trd>kopat</trd>,
    <trd>p&rcaron;ehrab&aacute;vat</trd>,
    <trd>&scaron;&tcaron;ourat se</trd>
  </trdgrp>
  <trd lng="de">graben</trd>
  <trd lng="en">dig</trd>
  <trd lng="es">excavar</trd>
  <trdgrp lng="fr">
    <trd>creuser</trd>,
    <trd>approfondir<klr tip="ind"> (examiner &agrave; fond)</klr></trd>,
    <trd>foncer<klr> (creuser)</klr></trd>,
    <trd>fouiller<klr tip="ind"> (creuser pour trouver qc)</klr></trd>,
    <trd>retourner<klr tip="ind"> (la terre)</klr></trd>
  </trdgrp>
  <trd lng="hu">&aacute;s</trd>
  <trd lng="nl">graven</trd>
  <trdgrp lng="pt">
    <trd>esquadrinhar</trd>,
    <trd>escarafunchar</trd>,
    <trd>fu&ccedil;ar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_y;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kopa&tcaron;</trd>,
    <trd>ry&tcaron;</trd>,
    <trd>r&yacute;&lcaron;ova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&#x43F;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x440;&#x438;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="fos.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="fos.0ado.KOMUNE">
    <dif>
      Ago <tld/>i:
      <ekz>
        por la <tld/>ado &scirc;i povis uzi nur fortan bran&ccirc;on de arbo kaj siajn manojn
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ado de kanalo inter Ru&gcirc;a kaj Meza maro
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sen konstanta <tld/>ado <klr>(esplorado)</klr> de &jcirc;urnalistoj la diversaj 
        skandaloj eble ne tiel rapide, detale kaj komplete evidenti&gcirc;us
        <fnt>
          <bib>Monato</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_p;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="cs">kop&aacute;n&iacute;</trd>
  <trdgrp lng="de">
    <trd>Graben</trd>,
    <trd>Grabung</trd>
  </trdgrp>
  <trd lng="en">digging</trd>
  <trd lng="es">excavaci&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>creusage</trd>,
    <trd>creusement</trd>
  </trdgrp>
  <trd lng="hu">&aacute;s&aacute;s</trd>
  <trd lng="nl">graafwerk</trd>
  <trd lng="ru">&c_r;&c_y;&c_t;&c_mol;&c_jo;</trd>
  <trd lng="sk">kopanie</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&#x43F;&#x430;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x440;&#x438;&#x442;&#x442;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="fos.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="fos.0ajxo.KOMUNE">
    <dif>
      <tld lit="F"/>ita, ordinare longa kava&jcirc;o:
      <ekz>
        se blindulo kondukos blindulon, amba&ubreve; falos en <tld/>a&jcirc;on  
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 15:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        profunda akvo<tld/>a&jcirc;o <klr>(<ref tip="sub" cel="kanal.0o.AKV">kanalo<sncref/></ref>)</klr>
        limas kun &gcirc;ardeno kaj najbara tereno nordflanke
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://www.esperanto.be/fel/mon/006878.html">
            DOMMASTRUMO: &Gcirc;ardenumi</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[burgo]</klr> &ccirc;irka&ubreve;ita de tri <tld/>a&jcirc;oj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Drako por Heroo Hinreko</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="galeri.0o">galerio</ref>,
      <ref cel="min.0ejo">minejo</ref>,
      <ref cel="put.0o">puto</ref>,
      <ref cel="sulk.0o">sulko</ref>,
      <ref cel="sxakt.0o">&scirc;akto</ref>,
      <ref cel="tomb.0o">tombo</ref>,
      <ref cel="trancx1.0o">tran&ccirc;eo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_o;&c_w;</trd>,
    <trd>&c_a;&c_k;&c_o;&c_p;</trd>,
    <trd>&c_t;&c_r;&c_a;&c_sh;&c_e;&c_ja;</trd>
  </trdgrp>
  <trd lng="de">Graben</trd>
  <trdgrp lng="en">
    <trd>ditch</trd>,
    <trd>hole</trd>,
    <trd>trench</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>foso</trd>,
    <trd>hoyo</trd>,
    <trd>zanja</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fosse</trd>,
    <trd>tranch&eacute;e</trd>
  </trdgrp>
  <trd lng="hu">&aacute;rok</trd>
  <trdgrp lng="nl">
    <trd>kuil</trd>,
    <trd>greppel</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ja;&c_m;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_a;&c_n;&c_sh;&c_je;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x440;&#x456;&#x432;</trd>,
    <trd>&#x43A;&#x430;&#x43D;&#x430;&#x432;&#x430;</trd>
  </trdgrp>
</drv>

<drv mrk="fos.0ilo">
  <kap><ofc>*</ofc><tld/>ilo</kap>
  <snc mrk="fos.0ilo.KOMUNE">
    <dif>
      &Ccirc;ia ilo por <tld/>i:
      <ekz>
        per <tld/>ilo ni <tld/>as
        <fnt><bib>F</bib>, <lok>&FE; 34</lok></fnt>;
      </ekz>
      <ekz>
        en la unua vico staris la <tld/>istoj kun pintohakiloj, en la dua kun pio&ccirc;oj, en la tria kun
        <tld/>iloj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ileton vi devas havi &ccirc;e vi sur rimeno; kaj 
        kiam vi sidos ekstere, <tld/>u per &gcirc;i kaj reen kovru vian elirinta&jcirc;on 
        <klr>(feka&jcirc;on)</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 23:13</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_l;&c_a;&c_d;&c_a; &c_d;&c_l;&c_ja;
    &c_k;&c_a;&c_p;&c_a;&c_n;&c_mol;&c_n;&c_ja;</trd>,
    <trd>&c_r;&c_y;&c_d;&c_l;&c_jo;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">n&aacute;stroj na obd&ecaron;l&aacute;v&aacute;n&iacute; p&uring;dy</trd>
  <trdgrp lng="de">
    <trd>Grabwerkzeug</trd>,
    <trd>Schaufel</trd>,
    <trd>Spaten</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>digging implement</trd>,
    <trd>shovel</trd>,
    <trd>spade</trd>
  </trdgrp>
  <trd lng="es">pala</trd>
  <trd lng="fr">pelle</trd>
  <trd lng="hu">&aacute;s&oacute;</trd>
  <trdgrp lng="nl">
    <trd>schop</trd>,
    <trd>spade</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_o;&c_p;&c_a;&c_t;&c_a;</trd>
  <trd lng="sk">n&aacute;stroj na obr&aacute;banie p&ocirc;dy</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x43E;&#x43F;&#x430;&#x442;&#x430;</trd>,
    <trd>&#x437;&#x430;&#x441;&#x442;&#x443;&#x43F;</trd>
  </trdgrp>
</drv>

<drv mrk="fos.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fos.cxirkaux0i.KOMUNE">
    <dif>
      <tld lit="F"/>i &ccirc;iuflanke:
      <ekz>
        li &ccirc;irka&ubreve;<tld/>is <klr>[sian &gcirc;ardenon]</klr>,
        sen&scirc;tonigis &gcirc;in, kaj plantis en &gcirc;i plej
        bonspecajn vinberbran&ccirc;ojn
        <fnt><bib>MT</bib>, <lok>&Jes; 5:2</lok></fnt>;
      </ekz>
      <ekz>
        mi hastis anta&ubreve;en por vidi &gcirc;in pla&ubreve;di
        en la <ind>&ccirc;irka&ubreve;<tld/>a&jcirc;o</ind>n tuj
        anta&ubreve; la fermita levponto
        <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;apitro 25</lok></fnt>.
        <trdgrp lng="fr">
          <trd>foss&eacute;<klr tip="ind"> (circulaire)</klr></trd>,
          <trd>douves<klr tip="ind"> (foss&eacute;)</klr></trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>k&ouml;r&aacute;rok</trd>,
          <trd>v&aacute;r&aacute;rok</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_k;&c_a;&c_p;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>umgraben</trd>
    <trd>mit einem Graben <ind>umgeben</ind></trd>
  </trdgrp>
  <trd lng="en">dig around</trd>
  <trdgrp lng="es">
    <trd>excavar<klr tip="ind"> (por todas partes)</klr></trd>,
    <trd>voltear<klr> (la tierra)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>creuser<klr tip="ind"> (de toutes parts)</klr></trd>,
    <trd>d&eacute;foncer<klr> (creuser de toutes parts)</klr></trd>,
    <trd>retourner<klr tip="ind"> (la terre)</klr></trd>
  </trdgrp>
  <trd lng="hu">k&ouml;rbe&aacute;s</trd>
  <adm>
    La ekzemplo kun "cxirkaux~a&jcirc;o" suspektigas, ke "&ccirc;irka&ubreve;~i"
    povus signifi anka&ubreve; "~i &ccirc;e la &ccirc;irka&ubreve;o de". Indus ser&ccirc;i
    ekzemplojn.
    <aut>MB</aut>
  </adm>
  <trd lng="uk">&#x43E;&#x431;&#x43A;&#x43E;&#x43F;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
</drv>

<drv mrk="fos.dis0i">
  <kap>dis<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fos.dis0i.KOMUNE">
    <dif>
      Malferme <tld/>i, difektante la surfacon:
      <ekz>
        li kunkun&scirc;ovelis per la piedo velkintajn foliojn sur la dis<tld/>itan teron
        <fnt>
          <bib>Fab2</bib>
          <lok>Elfo de la rozo</lok>
        </fnt>;
      </ekz>
      <ekz>
        duone dis<tld/>ita pavimo el brikoj
        <fnt><bib>Fab1</bib>, <lok>Galo&scirc;oj de feli&ccirc;o</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_k;&c_o;&c_p;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>rozkopat</trd>,
    <trd>rozr&yacute;t</trd>
  </trdgrp>
  <trd lng="en">dig up</trd>
  <trdgrp lng="es">
    <trd>abrir una excavaci&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;foncer<klr tip="ind"> (creuser profond&eacute;ment)</klr></trd>,
    <trd>&eacute;ventrer<klr> (en creusant)</klr></trd>
  </trdgrp>
  <trd lng="hu">sz&eacute;t&aacute;s</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_k;&c_o;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_r;&c_y;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rozkopa&tcaron;</trd>,
    <trd>rozry&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x440;&#x43E;&#x437;&#x43A;&#x43E;&#x43F;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x440;&#x43E;&#x437;&#x440;&#x438;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="fos.en0i">
  <kap>en<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fos.en0i.KOMUNE">
    <dif>
      <tld lit="F"/>i por enterigi:
      <ekz>
        Jakob enfosis [la ringojn] sub la kverko
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 35:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        la skarabo tre oportune povis englitigi sin kaj sin en<tld/>i en la fre&scirc;an sterkon
        <fnt>
          <bib>Fab4</bib>
          <lok>Sterkskarabo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_k;&c_o;&c_p;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_o;&c_p;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hrab&aacute;n&iacute;m zakr&yacute;t</trd>,
    <trd>zahrabat</trd>,
    <trd>zakopat</trd>
  </trdgrp>
  <trd lng="de">eingraben</trd>
  <trd lng="en">bury</trd>
  <trd lng="es">enterrar</trd>
  <trdgrp lng="fr">
    <trd>enfouir</trd>,
    <trd>ensevelir</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>be&aacute;s</trd>,
    <trd>bele&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">begraven</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_k;&c_o;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_o;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_r;&c_y;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zakopa&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x437;&#x430;&#x43A;&#x43E;&#x43F;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x437;&#x430;&#x440;&#x438;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="fos.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fos.el0i.elterigi">
    <dif>
      Elterigi, eltiri per <tld/>ado:
      <ekz>
        kanalo, el<tld/>ita en la sabloj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili el<tld/>is en sia lando reton de kanaloj, kelkmilojn da mejloj longan
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        putoj, rapide el<tld/>itaj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i el<tld/>is kavon en la tero, tiel profunde, kiel &scirc;i povis, &scirc;i volis fari por ili tombon
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        li el<tld/>is al si longan trairejon tra la tero de sia lo&gcirc;ejo &gcirc;is &scirc;ia
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_k;&c_o;&c_p;&c_v;&c_a;&c_c;&c_mol;</trd>,
