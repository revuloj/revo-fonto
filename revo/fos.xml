<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fos</rad>/i
</kap>
<drv mrk="fos.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari kava&jcirc;on eltirante teron:
      <ekz>
        <tld/>i teron<fnt>B</fnt>, rokon, sablon;
      </ekz>
      <ekz>
        <tld/>i kanalon<fnt>Z</fnt>,
        kav(a&jcirc;)on<fnt>Z</fnt>, puton<fnt>B</fnt>, tombon;
      </ekz>
      <ekz>
        ili <tld/>is anta&ubreve; mi <tld/>on
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Psalmaro 57, 6</lok>)</fnt>;
      </ekz>
      <ekz>
        dis<tld/>i la teron<fnt>Z</fnt>;
      </ekz>
      <ekz>
        duone dis<tld/>ita pavimo el brikoj
        <fnt>(<aut>Andersen, trad. Zamenhof</aut>: <vrk>Fabeloj,
        Galo&scirc;oj de feli&ccirc;o</vrk>)</fnt>.
      </ekz> 
    </dif>
    <refgrp tip="sin">
      <ref cel="kav.0igi">kavigi</ref>,
      <ref cel="bor.0i">bori</ref>,
      <ref cel="plug.0i">plugi</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Ser&ccirc;i ion ka&scirc;itan, farante kava&jcirc;on. 
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Profunde esplori demandon:
      <ekz>
        filozofie <tld/>i en teorioj<fnt>Z</fnt>.
      </ekz> 
    </dif>
  </snc>
  <trd lng="de">graben</trd>
  <trd lng="en">dig</trd>
</drv>
<drv mrk="fos.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Ago <tld/>i. 
    </dif>
  </snc>
</drv>
<drv mrk="fos.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      <tld/>ita, ordinare longa kava&jcirc;o. 
    </dif>
    <refgrp tip="sin">
      <ref cel="put.0o">puto</ref>,
      <ref cel="tomb.0o">tombo</ref>,
      <ref cel="min.0ejo">minejo</ref>,
      <ref cel="sulk.0o">sulko</ref>,
      <ref cel="galeri.0o">galerio</ref>,
      <ref cel="sxakt.0o">&scirc;akto</ref>,
      <ref cel="trancx1.0o">tran&ccirc;eo</ref>
    </refgrp>
  </snc>
  <trd lng="de">Graben</trd>
</drv>
<drv mrk="fos.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      &Ccirc;ia ilo por <tld/>i:
      <ekz>
        pik<tld/>ilo <klr>(pio&ccirc;o)</klr>;
      </ekz>
      <ekz>
        plat<tld/>ilo <klr>(&scirc;pato)</klr>. 
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Grabwerkzeug</trd>,
    <trd>Schaufel</trd>,
    <trd>Spaten</trd>
  </trdgrp>
</drv>
<drv mrk="fos.en0i">
  <kap>en<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld/>i por enterigi. 
    </dif>
  </snc>
  <trd lng="de">eingraben</trd>
</drv>
<drv mrk="fos.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      = <tld/>i. 
    </dif>
  </snc>
  <snc>
    <dif>
      Elterigi, eltiri per <tld/>ado:
      <ekz>
        el<tld/>isto de oro.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">ausgraben</trd>
</drv>
<drv mrk="fos.sub0i">
  <kap>sub<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Subtere <tld/>i.  
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ka&scirc;e prepari la ruinigon, la malsukceson de; perfide malutili al,
      nevideble iom post iom difekti:
      <ekz>
        tia metio sub<tld/>as la sanon;
      </ekz>
      <ekz>
        la trolaboro, la &ccirc;agreno sub<tld/>as lin;
      </ekz>
      <ekz>
        sub<tld/>i per kalumnioj la reputacion de iu;
      </ekz>
      <ekz>
        kelkaj personoj komencis sub<tld/>adi la a&ubreve;toritaton de niaj
        kongresoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiaj komplotoj sub<tld/>is la potencon de la regno.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">untergraben</trd>
</drv>
<drv mrk="fos.sercx0i">
  <kap>ser&ccirc;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      = <tld/>i 2. 
    </dif>
  </snc>
  <snc>
    <dif>
      Ser&ccirc;i ion ka&scirc;itan:
      <ekz>
        ser&ccirc;<tld/>i en sia po&scirc;o<fnt>Z</fnt>.
      </ekz>  
    </dif>
  </snc>
</drv>
</art>
