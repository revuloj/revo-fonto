    <trd>&c_k;&c_a;&c_p;&c_a;&c_c;&c_mol; <klr>(&c_d;&c_l;&c_ja;
    &c_p;&c_o;&c_sh;&c_u;&c_k;&c_u;)</klr></trd>,
