<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>klavikord</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="klavikord.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      E&ubreve;ropdevena klavara
      <ref tip="super" cel="instru2.kord0o">kordinstrumento</ref>
      uzata de la 14a &gcirc;is la 17a jarcento,
      pratipo de piano,
      en kiu kordojn frapas internaj marteletoj movataj
      de premo kaj malpremo de klavoj.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_ja;&c_v;&c_ib;&c_k;&c_o;&c_r;&c_d;&c_y;</trd>
  <trdgrp lng="de">
    <trd>Klavichord</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>clavichord</trd>
  </trdgrp>
  <trd lng="es">clavicordio</trd>
  <trdgrp lng="fr">
    <trd>clavicorde</trd>,
    <trd>manicordion</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_l;&c_a;&c_v;&c_i;&c_k;&c_o;&c_r;&c_d;</trd>,
    <trd>&c_k;&c_l;&c_a;&c_v;&c_i;&c_k;&c_o;&c_r;&c_d;&c_y;</trd>
  </trdgrp>
</drv>
</art>

