<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bronz.xml,v 1.34 2023/10/29 19:11:03 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bronz</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="bronz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bronz.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="super" cel="aloj.0o">Alojo</ref> el
      <ref tip="prt" cel="kupr.0o">kupro</ref> kaj
      <ref tip="prt" cel="stan.0o">stano</ref>,
      al kiu oni ofte aldonas alian metalon (<ref tip="vid" cel="zink.0o">zinkon</ref>, <ref tip="vid" cel="plumb.0o">plumbon</ref>, k.s.):
      <ekz>
        sonoriloj, kiuj havas la kolorojn de <tld/>o kaj kupro
        <fnt>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2001/007297.php">Kolekti
          sonorilojn</url></vrk>
          <bib>Monato</bib>
          <lok>2001/02, p. 24</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiuj ornamoj estas faritaj el <tld/>o kaj kolora emajlo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni fandis <tld/>on por vazoj kaj armiloj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sonoris lia <tld/>ovo&ccirc;o
        <klr>(sonorile sonanta vo&ccirc;o)</klr>
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Vidanta Mano</lok>
        </fnt>
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x9752;&#x9285; <pr>q&#x12B;ngt&#xF3;ng</pr></trd>,
    <trd>&#x9752;&#x9285;&#x85DD;&#x8853;&#x88FD;&#x54C1; <pr>q&#x12B;ngt&#xF3;ngy&#xEC;sh&#xF9;zh&#xEC;p&#x1D0;n</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_b;&c_r;&c_o;&c_n;&c_z;&c_a;</trd>
  <trd lng="bg">&c_b;&c_r;&c_o;&c_n;&c_z;</trd>
  <trd lng="br">arem</trd>
  <trdgrp lng="cs">
    <trd>bronz</trd>,
    <trd>sp&ecaron;&zcaron; <klr>(zast.)</klr></trd>
  </trdgrp>
  <trd lng="de">Bronze</trd>
  <trd lng="en">bronze</trd>
  <trd lng="es">bronce</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&a_f;&a_r;&a_gh;</trd>,
    <trd>&a_b;&a_r;&a_n;&a_z;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>airain</trd>,
    <trd>bronze <klr tip="ind">(m&eacute;tal)</klr></trd>
  </trdgrp>
  <trd lng="he">&#x5D1;&#x5BC;&#x5B0;&#x5E8;&#x5D5;&#x5B9;&#x5E0;&#x5D6;&#x5B8;&#x5D4;</trd>
  <trd lng="hu">bronz</trd>
  <trd lng="ja">&#x9752;&#x9285;  <pr>&#x305B;&#x3044;&#x3069;&#x3046;</pr></trd>
  <trd lng="nl">brons</trd>
  <trd lng="pl">br&aogonek;z</trd>
  <trd lng="pt">bronze</trd>
  <trd lng="ru">&c_b;&c_r;&c_o;&c_n;&c_z;&c_a;</trd>
  <trd lng="sk">bronz</trd>
  <trd lng="sv">brons</trd>
  <trd lng="uk">&c_b;&c_r;&#x43E;&#x43D;&c_z;&c_a;</trd>
</drv>

<drv mrk="bronz.0a">
  <kap><tld/>a</kap>
  <snc mrk="bronz.0a.KEM">
    <dif>
      El <tld/>o:
      <ekz>
        mitologiaj <tld/>aj kaj marmoraj grupaj statuetoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        gajnis <tld/>ajn medalojn
        <klr>(premii&gcirc;is kiel tria en sporta konkurso)</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2000/006824.php">Litovio:
          Monda &ccirc;ampionado de dancoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj postamentoj,
        <tld/>a horlo&gcirc;o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a kulero
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a glavo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        tri sonoj de la <tld/>a lado, kaj la soleno estis finita
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
    <trd>aus <ind>Bronze</ind></trd>,
    <trd>bronzen</trd>
    </trdgrp>
    <trd lng="en">bronze</trd>
    <trd lng="es">de <ind>bronce</ind></trd>
    <trd lng="fa">&a_b;&a_r;&a_n;&a_z;&f_y;</trd>
    <trdgrp lng="hu">
    <trd>bronz-</trd>,
    <trd>bronzb&oacute;l k&eacute;sz&uuml;lt</trd>
    </trdgrp>
    <trd lng="nl">bronzen</trd>
    <trdgrp lng="pl">
      <trd>z <ind>br&aogonek;zu</ind></trd>,
      <trd>br&aogonek;zowy<klr> (z br&aogonek;zu)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="bronz.0a.bronzaspekta">
    <dif>
      Simila al <tld/>o:
      <ekz>
        <tld/>a koro <klr>(malmola kiel <tld/>o)</klr>;
      </ekz>
      <ekz>
        <tld/>a viza&gcirc;o <klr>(<tld/><ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">kolora</ref>)</klr>;
      </ekz>
      <ekz>
        kiam mi etendos mian <tld/>an manon, ili falos sur la viza&gcirc;on
        kaj la tuta malordo fini&gcirc;os per paco kaj &gcirc;enerala prospero
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a vo&ccirc;o
        <klr>(sonorila)</klr>
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Kunvoja&gcirc;antoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fa">
      <trd>&a_b;&a_r;&a_n;&a_z;&a_h;</trd>,
      <trd>&a_A_madda;&a_f;&a_t;&a_A;&a_b;&#x200C;&a_s;&a_w;&a_kh;&a_t;&a_h;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>bronzos</trd>,
      <trd>bronzszer&udblac;</trd>
    </trdgrp>
    <trd lng="nl">gebronsd</trd>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_o;&c_n;&c_z;&c_a;&c_v;&c_y;</trd>
  <trd lng="br">arem</trd>
  <trdgrp lng="cs">
    <trd>bronzov&yacute;</trd>,
    <trd>sp&ecaron;&zcaron;ov&yacute; <klr>(zast.)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
  <trd>en <ind>bronze</ind></trd>,
  <trd>de <ind>bronze</ind></trd>
  </trdgrp>
  <trd lng="ja">&#x9752;&#x9285;&#x306E;  <pr>&#x305B;&#x3044;&#x3069;&#x3046;&#x306E;</pr></trd>
  <trdgrp lng="pl">
    <trd>kamienny<klr> (np. o twarzy)</klr></trd>,
    <trd>z <ind>kamienia</ind><klr> (np. o sercu)</klr></trd>,
    <trd>szary<klr> (np. o kolorze twarzy)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_r;&c_o;&c_n;&c_z;&c_o;&c_v;&c_y;&c_j;</trd>
  <trd lng="sk">bronzov&yacute;</trd>
  <trd lng="uk">&c_b;&c_r;&#x43E;&#x43D;&c_z;&#x43E;&c_v;&c_i;&c_j;</trd>
</drv>

<drv mrk="bronz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="bronz.0i.KEM">
    <dif>
      Surkovri per tavoleto el <tld/>o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li <tld/>is ilian duron
        <fnt>
          <aut>Ken Miner</aut>
          <vrk><url ref="http://www.sunflower.com/~miner/POEZIO_package/poezio.html">Tiel
          malgranda lando</url></vrk>
          <lok>poezio - sunflower.com</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fa">&a_b;&a_r;&a_n;&a_z; &f_g;&a_r;&a_f;&a_t;&a_n;</trd>
    <trd lng="hu">bronzzal bevon</trd>
    <trd lng="nl">verbronzen</trd>
    <trd lng="pl">br&aogonek;zowi&cacute;<klr> (pokrywa&cacute;
    br&aogonek;zem)</klr></trd>
  </snc>
  <snc mrk="bronz.0i.bronzaspektigi">
    <dif>
      Doni al io a&ubreve; havi aspekton de <tld/>o:
      <ekz>
        la suno <tld/>as la ha&ubreve;ton;
      </ekz>
      <ekz>
        kaj edzino kies ha&ubreve;to <tld/>as per la natura brun'
        <fnt>
          <aut>Carl Anton Axelsson, trad. Sten Johansson</aut>
          <vrk><url ref="http://www.angelfire.com/ok/stenjo/mevoj.html">Valso kun
          mevoj</url></vrk>
          <lok>angelfire.com</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="brun.sun0igi">sunbrunigi</ref>.
    </refgrp>
    <trdgrp lng="fa">
      <trd>&a_b;&a_r;&a_n;&a_z;&a_n;&a_m;&a_A; &f_k;&a_r;&a_d;&a_n;</trd>,
      <trd>&a_b;&a_r;&a_n;&a_z;&a_h; &f_k;&a_r;&a_d;&a_n;</trd>
    </trdgrp>
    <trd lng="hu">lebarn&iacute;t<klr tip="ind"> (pl. nap)</klr></trd>
    <trd lng="nl">bronzen</trd>
    <trdgrp lng="pl">
      <trd>br&aogonek;zowi&cacute;</trd>
      <trd>pokrywa&cacute; <ind>br&aogonek;zem</ind><klr> (nadawa&cacute;
      br&aogonek;zowy kolor)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_a;&c_n;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="fr">bronzer <klr tip="ind">(donner l'aspect du
    bronze)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x9752;&#x9285;&#x3092;&#x5F35;&#x308B;  <pr>&#x305B;&#x3044;&#x3069;&#x3046;&#x3092;&#x306F;&#x308B;</pr></trd>,
    <trd>&#x9752;&#x9285;&#x8272;&#x306B;&#x3059;&#x308B;  <pr>&#x305B;&#x3044;&#x3069;&#x3046;&#x3044;&#x308D;&#x306B;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_r;&c_o;&c_n;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="uk">&c_b;&c_r;&#x43E;&#x43D;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="bronz.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="bronz.0ajxo.BELA">
    <dif>
      Objekto el <tld/>o, plej ofte artisma:
      <ekz>
        mi sendos al vi specimenojn de tieaj teksa&jcirc;oj, arboj kaj
        <tld/>a&jcirc;oj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x9752;&#x9285;&#x5668; <pr>q&#x12B;ngt&#xF3;ngq&#xEC;</pr></trd>
</snc>
  <trd lng="be">&c_v;&c_y;&c_r;&c_a;&c_b; &c_z; &c_b;&c_r;&c_o;&c_n;&c_z;&c_y;</trd>
  <trd lng="br">aremenn</trd>
  <trd lng="de">Bronze</trd>
  <trd lng="en">bronze</trd>
  <trd lng="fa">&a_m;&a_j;&a_s;&a_m;&a_h;&a_hamza_sure; <ind>&a_b;&a_r;&a_n;&a_z;</ind></trd>
  <trd lng="fr">bronze <klr tip="ind">(objet)</klr></trd>
  <trd lng="he">&#x5D1;&#x5BC;&#x5B0;&#x5E8;&#x5D5;&#x5B9;&#x5E0;&#x5D6;&#x5B8;&#x5D4;</trd>
  <trd lng="hu">bronzt&aacute;rgy</trd>
  <trdgrp lng="ja">
    <trd>&#x9752;&#x9285;&#x88FD;&#x54C1;  <pr>&#x305B;&#x3044;&#x3069;&#x3046;&#x305B;&#x3044;&#x3072;&#x3093;</pr></trd>,
    <trd>&#x30D6;&#x30ED;&#x30F3;&#x30BA;&#x50CF;  <pr>&#x30D6;&#x30ED;&#x30F3;&#x30BA;&#x305E;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">brons</trd>
  <trd lng="ru">&c_i;&c_z;&c_d;&c_je;&c_l;&c_i;&c_je; &c_i;&c_z;
  &c_b;&c_r;&c_o;&c_n;&c_z;&c_y;</trd>
  <trd lng="uk">&c_v;&c_i;&c_r;&c_ib;&c_b; &c_ib;&c_z; &c_b;&c_r;&#x43E;&#x43D;&c_z;&c_i;</trd>
</drv>
</art>

