<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: grinc.xml,v 1.31 2025/08/19 22:31:00 revo Exp $">
<kap><ofc>*</ofc>
  <rad>grinc</rad>/i
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="grinc.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="bru.0i.soni">Brui</ref> akre, malagrable
      kaj tre akute pro kunfroti&gcirc;o kun alia malmola objekto:
      <ekz>
        <tld/>u per la dentoj, &scirc;prucigu fajron <klr>[&#8230;]</klr> &mdash; min amuzas la kolerego
        de virino
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Likon</nom> komencis <tld/>i
        <klr>(prefere <ref cel="grinc.0igi">grincigi</ref>)</klr> la dentojn
        en senforta kolero
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni a&ubreve;dis &scirc;losilon, kiu <tld/>is en seruro
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la bordoj de la kanalo balanci&gcirc;is la <tld/>antaj gruoj kun siteloj de
        fruktiga akvo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        la granda industriejo <klr>[&#8230;]</klr> mu&gcirc;is kaj siblis, zumis kaj sonoris,
        <tld/>is kaj tintis
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;enoj de vaporkranoj <tld/>as kaj krakas
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
          <lok>El Odeso &gcirc;is Konstantinopolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vagonaro kolere ek<tld/>is per la bufroj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>is la fosiloj, ebenigantaj per tero la kavojn
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LVIII</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="knar.0i.brui">knari</ref>,
      <ref cel="kricx.0i">kri&ccirc;i</ref>,
      <ref cel="tint.0i.SON">tinti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_y;&c_p;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_y;&c_g;&c_a;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_y;&c_p;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>grinyolar</trd>,
    <trd>carrisquejar</trd>,
    <trd>xerricar</trd>
  </trdgrp>
  <trd lng="cs">sk&rcaron;&iacute;pat</trd>
  <trdgrp lng="de">
    <trd>knirschen</trd>,
    <trd>kreischen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>make a <ind>grind</ind>ing sound</trd>,
    <trd>grind <klr>(sound)</klr></trd>,
    <trd>creak</trd>,
    <trd>squeak</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>rechinar</trd>,
    <trd>chirriar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>crisser<klr tip="ind"> (en &eacute;mettant un son
    suraigu)</klr></trd>,
    <trd>couiner<klr tip="ind"> (en &eacute;mettant un son
    suraigu)</klr></trd>,
    <trd>grincer<klr tip="ind"> (en &eacute;mettant un son
    suraigu)</klr></trd>,
    <trd>striduler</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csikorog</trd>,
    <trd>nyikorog</trd>
  </trdgrp>
  <trd lng="ja">&#12365;&#12375;&#12416;</trd>
  <trd lng="nl">knarsen</trd>
  <trd lng="pt">ranger</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_r;&c_i;&c_p;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_je;&c_zh;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">&scaron;kr&iacute;pa&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_k;&c_r;&c_je;&c_g;&c_o;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_je;&c_g;&c_o;&c_t;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_i;&c_p;&c_ib;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#29273;&#40786;&#21676;&#24471;&#21679;&#21679;&#38911;
    <pr>y&aacute;ch&#464;y&#462;od&eacute;g&emacron;g&emacron;xi&#462;ng</pr></trd>,
    <trd>&#21676;&#29273;&#20999;&#40786; <pr>y&#462;oy&aacute;qi&egrave;ch&#464;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="grinc.0o">
  <kap>
    <tld/>o,
    <var><kap><tld/>ado</kap></var>
  </kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        <tld/>ado de segiloj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        eksilentis la <tld/>o de la veturilaj radoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        el la kunpremitaj makzeloj eli&gcirc;adis malbona&ubreve;gura <tld/>o
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;APITRO VII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_r;&c_o;&c_g;&c_a;&c_t;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_y;&c_g;&c_a;&c_t;</trd>,
    <trd>&c_r;&c_y;&c_p;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Knirschen</trd>,
    <trd>Kreischen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>grinding <klr>(sound)</klr></trd>,
    <trd>creaking</trd>,
    <trd>squeaking</trd>
  </trdgrp>
  <trd lng="es">rechinido</trd>
  <trd lng="ja">&#12365;&#12375;&#12416;&#38899;  <pr>&#12365;&#12375;&#12416;&#12362;&#12392;</pr></trd>
</drv>

<drv mrk="grinc.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari, ke io <tld/>u:
      <ekz>
        mia malamanto <tld/>igas kontra&ubreve; mi siajn dentojn
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 16:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        li &scirc;a&ubreve;mas kaj <tld/>igas la dentojn
        <fnt>
          <bib>NT</bib>
          <lok>Marko 9:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        li rapide <klr>[&#8230;]</klr> eliris tre &gcirc;entile:&ndash; humilan saluton! (sed interne li
        <tld/>igis siajn impulsojn)
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_y;&c_p;&c_je;&c_c;&c_mol; <klr>(&c_ch;&c_y;&c_m;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>,
    <trd>&c_s;&c_k;&c_r;&c_y;&c_g;&c_a;&c_t;&c_a;&c_c;&c_mol; <klr>(&c_ch;&c_y;&c_m;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  </trdgrp>
  <trd lng="ca">fer <ind>grinyolar</ind></trd>
  <trd lng="de"><ind>knirschen</ind> lassen</trd>
  <trd lng="en">make <ind>grind</ind> <klr>(sound)</klr></trd>
  <trd lng="es">hacer <ind>rechinar</ind></trd>
  <trd lng="fr">faire <ind>grincer</ind></trd>
  <trdgrp lng="hu">
    <trd>csikorogat</trd>,
    <trd>nyikorogat</trd>
  </trdgrp>
  <trd lng="ja">&#12365;&#12375;&#12414;&#12379;&#12427;</trd>
  <trd lng="nl">doen <ind>knarsen</ind></trd>
  <trd lng="pt">fazer algo <ind>ranger</ind></trd>
</drv>
</art>
