<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>grinc</rad>/i</kap>
<drv mrk="grinc.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="bru.0i.soni">Brui</ref> akre, malagrable
      kaj tre akute pro kunfroti&gcirc;o kun alia malmola objekto:
      <ekz>
        fero <tld/>as sur vitro<fnt>B</fnt>;
      </ekz>
      <ekz>
        &scirc;losilo <tld/>as en seruro;
      </ekz>
      <ekz>
        <tld/>as orgeneto, grilo, ma&scirc;ino<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>as segilo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>as dentoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind><tld/>i per la dentoj</ind><fnt>Z</fnt>.
        <trd lng="fr">grincer des dents</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_y;&c_p;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_y;&c_g;&c_a;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_y;&c_p;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>grinyolar</trd>,
    <trd>carrisquejar</trd>,
    <trd>xerricar</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>knirschen</trd>,
    <trd>kreischen</trd>
  </trdgrp>
  <trd lng="en">grind</trd>
  <trdgrp lng="es">
    <trd>rechinar</trd>,
    <trd>chirriar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>crisser<klr tip="ind"> (en &eacute;mettant un son suraigu)</klr></trd>,
    <trd>couiner<klr tip="ind"> (en &eacute;mettant un son suraigu)</klr></trd>,
    <trd>grincer<klr tip="ind"> (en &eacute;mettant un son suraigu)</klr></trd>,
    <trd>striduler</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csikorog</trd>,
    <trd>nyikorog</trd>
  </trdgrp>
  <trd lng="nl">knarsen</trd>
  <trd lng="pt">ranger</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_r;&c_i;&c_p;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_je;&c_zh;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="grinc.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari, ke io <tld/>u:
      <ekz>
        <tld/>igi la dentojn al iu<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">faire <ind>grincer</ind></trd>
  <trdgrp lng="hu">
    <trd>csikorogat</trd>,
    <trd>nyikorogat</trd>
  </trdgrp>
  <trd lng="nl">doen <ind>knarsen</ind></trd>
  <trd lng="pt">fazer algo <ind>ranger</ind></trd>
</drv>
</art>
