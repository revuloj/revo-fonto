<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>depon</rad>/i
</kap>
<drv mrk="depon.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Konfidi al iu multvalora&jcirc;on gardotan:
      <ekz>
        <tld/>i monon en banko<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i monon en &scirc;parkaso;
      </ekz>
      <ekz>
        <tld/>i paka&jcirc;on en stacia pakgardejo;
      </ekz>
      <ekz>
        <tld/>i provizon da man&gcirc;a&jcirc;oj en
        la kabano;
      </ekz>
      <ekz>
        ili <tld/>u la vestojn en la sanktaj &ccirc;ambroj
        <fnt><bib>MT</bib>, <lok>&Jeh; 44:19</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;iu traktato devas esti
        <tld/>ita en la Sekretariejo de la Ligo;
      </ekz>
      <ekz>
        <tld/>ita&jcirc;on trompe forpreni<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>deponieren</trd>,
    <trd>hinterlegen</trd>,
    <trd>in <ind>Verwahrung</ind> geben</trd>
  </trdgrp>
  <trd lng="en">deposit</trd>
  <trd lng="es">depositar</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;poser<klr tip="ind"> (laisser en d&eacute;p&ocirc;t)</klr></trd>,
    <trd>laisser en <ind>d&eacute;p&ocirc;t</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>let&eacute;tbe helyez</trd>,
    <trd>depon&aacute;l</trd>,
    <trd>meg&odblac;rz&eacute;sre &aacute;tad</trd>
  </trdgrp>
  <trd lng="pt">depositar</trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_r;&c_a;&c_n;&c_i;&c_t;&c_mol;<klr> (&c_v; &c_b;&c_a;&c_n;&c_k;&c_je;, &c_v; &c_k;&c_a;&c_m;&c_je;&c_r;&c_je; &c_h;&c_r;&c_a;&c_n;&c_je;&c_n;&c_i;&c_ja; &c_i; &c_t;.&c_p;.)</klr></trd>,
    <trd>&c_s;&c_d;&c_a;&c_t;&c_mol; &c_n;&c_a; <ind>&c_h;&c_r;&c_a;&c_n;&c_je;&c_n;&c_i;&c_je;</ind></trd>,
    <trd>&c_v;&c_n;&c_je;&c_s;&c_t;&c_i; &c_v; <ind>&c_d;&c_je;&c_p;&c_o;&c_z;&c_i;&c_t;</ind></trd>,
    <trd>&c_d;&c_je;&c_p;&c_o;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_n;&c_je;&c_s;&c_t;&c_i; <ind>&c_v;&c_k;&c_l;&c_a;&c_d;</ind><klr> (&c_v; &c_b;&c_a;&c_n;&c_k;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="depon.0atesto">
  <kap><tld/>atesto</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Atesta slipo pri deponita a&jcirc;o.
    </dif>
    <trdgrp lng="de">
      <trd>Depositenschein</trd>,
      <trd>Verwahrschein</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>let&eacute;ti jegy</trd>,
      <trd>bet&eacute;ti elismerv&eacute;ny</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_je;&c_p;&c_o;&c_z;&c_i;&c_t;&c_n;&c_o;&c_je; &c_s;&c_v;&c_i;&c_d;&c_je;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>,
      <trd>&c_d;&c_je;&c_p;&c_o;&c_z;&c_i;&c_t;&c_n;&c_y;&c_j; &c_s;&c_je;&c_r;&c_t;&c_i;&c_f;&c_i;&c_k;&c_a;&c_t;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="depon.0ejo">
  <kap><tld/>ejo</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Loko, kie oni deponas multvalora&jcirc;on gardotan.
    </dif>
    <trdgrp lng="de">
      <trd>Depot</trd>,
      <trd>Aufbewahrungsstelle</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>let&eacute;ti hely</trd>,
      <trd>&eacute;rt&eacute;kmeg&odblac;rz&odblac;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_h;&c_r;&c_a;&c_n;&c_i;&c_l;&c_i;&c_shch;&c_je;</trd>,
      <trd>&c_k;&c_a;&c_m;&c_je;&c_r;&c_a; &c_h;&c_r;&c_a;&c_n;&c_je;&c_n;&c_i;&c_ja;</trd>,
      <trd>&c_s;&c_k;&c_l;&c_a;&c_d;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Konto, kie oni deponas libromonon, valorpaperojn k.s.
    </dif>
    <trdgrp lng="de">
      <trd>Depot</trd>,
      <trd>Hinterlegungsstelle</trd>
    </trdgrp>
    <trd lng="hu">let&eacute;ti <ind>sz&aacute;mla</ind></trd>
    <trdgrp lng="ru">
      <trd>&c_b;&c_a;&c_n;&c_k;&c_o;&c_v;&c_s;&c_k;&c_i;&c_j; <ind>&c_v;&c_k;&c_l;&c_a;&c_d;</ind></trd>,
      <trd>&c_d;&c_je;&c_p;&c_o;&c_z;&c_i;&c_t;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="depon.0banko">
  <kap><tld/>banko</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Banko, kie oni povas deponi multvalora&jcirc;ojn.
    </dif>
    <trdgrp lng="de">
      <trd>Depositenbank</trd>,
      <trd>Verwahrbank</trd>
    </trdgrp>
    <trd lng="hu">&eacute;rt&eacute;kmeg&odblac;rz&odblac; <ind>bank</ind></trd>
  </snc>
  <snc>
    <dif>
      Banko, kie oni povas deponi valorpaperojn.
    </dif>
    <trdgrp lng="de">
      <trd>Depotbank</trd>,
      <trd>Effektenverwahrbank</trd>
    </trdgrp>
    <trd lng="hu">let&eacute;ti <ind>bank</ind></trd>
  </snc>
  <trd lng="ru">&c_d;&c_je;&c_p;&c_o;&c_z;&c_i;&c_t;&c_n;&c_y;&c_j; <ind>&c_b;&c_a;&c_n;&c_k;</ind></trd>
</drv>
</art>
