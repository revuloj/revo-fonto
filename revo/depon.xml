<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: depon.xml,v 1.23 2021/03/08 14:29:16 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>depon</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="depon.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="depon.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Konfidi al iu multvalora&jcirc;on gardotan:
      <ekz>
        <tld/>i monon en banko<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i monon en &scirc;parkaso;
      </ekz>
      <ekz>
        <tld/>i paka&jcirc;on en stacia pakgardejo;
      </ekz>
      <ekz>
        <tld/>i provizon da man&gcirc;a&jcirc;oj en
        la kabano;
      </ekz>
      <ekz>
        ili <tld/>u la vestojn en la sanktaj &ccirc;ambroj
        <fnt><bib>MT</bib>, <lok>&Jeh; 44:19</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;iu traktato devas esti
        <tld/>ita en la Sekretariejo de la Ligo;
      </ekz>
      <ekz>
        <tld/>ita&jcirc;on trompe forpreni<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>ingressar</trd>,
      <trd>dipositar</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>deponieren</trd>,
      <trd>hinterlegen</trd>,
      <trd>in <ind>Verwahrung</ind> geben</trd>
    </trdgrp>
    <trd lng="en">deposit</trd>
    <trd lng="es">depositar</trd>
    <trdgrp lng="fa">
      <trd>&a_s;&f_p;&a_r;&a_d;&a_n;</trd>,
      <trd>&a_s;&f_p;&a_r;&a_d;&a_h; &f_g;&a_dh;&a_A;&a_sh;&a_t;&a_n;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;poser<klr tip="ind"> (laisser en
      d&eacute;p&ocirc;t)</klr></trd>,
      <trd>laisser en <ind>d&eacute;p&ocirc;t</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>let&eacute;tbe helyez</trd>,
      <trd>depon&aacute;l</trd>,
      <trd>meg&odblac;rz&eacute;sre &aacute;tad</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>deponowa&cacute;</trd>,
      <trd>powierza&cacute;</trd>,
      <trd>oddawa&cacute; w <ind>depozyt</ind></trd>
    </trdgrp>
    <trd lng="pt">depositar</trd>
    <trdgrp lng="ru">
      <trd>&c_h;&c_r;&c_a;&c_n;&c_i;&c_t;&c_mol;<klr> (&c_v;
      &c_b;&c_a;&c_n;&c_k;&c_je;, &c_v; &c_k;&c_a;&c_m;&c_je;&c_r;&c_je;
      &c_h;&c_r;&c_a;&c_n;&c_je;&c_n;&c_i;&c_ja; &c_i;
      &c_t;.&c_p;.)</klr></trd>,
      <trd>&c_s;&c_d;&c_a;&c_t;&c_mol; &c_n;&c_a;
      <ind>&c_h;&c_r;&c_a;&c_n;&c_je;&c_n;&c_i;&c_je;</ind></trd>,
      <trd>&c_v;&c_n;&c_je;&c_s;&c_t;&c_i; &c_v;
      <ind>&c_d;&c_je;&c_p;&c_o;&c_z;&c_i;&c_t;</ind></trd>,
      <trd>&c_d;&c_je;&c_p;&c_o;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_n;&c_je;&c_s;&c_t;&c_i;
      <ind>&c_v;&c_k;&c_l;&c_a;&c_d;</ind><klr> (&c_v;
      &c_b;&c_a;&c_n;&c_k;)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="depon.0i.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Surfundigi, precipitigi, postlasi sur io:
      <ekz>
        la sablo, se &gcirc;i ne estus elprenita, sin <tld/>us
        pli fore kaj &gcirc;enus la funkciadon de la stacio
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Prilaboro_de_la_uzitaj_akvoj">
          Prilaboro de la uzitaj akvoj</url>, 2012-06-16</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="precip1.0i.KEM">precipiti</ref>,
      <ref cel="sedime.0igxi.GEOL">sedimenti&gcirc;i</ref>
    </refgrp>
    <trd lng="fr">d&eacute;poser<klr> (hydrol.)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>prov&#xE1;d&#x11B;t p&#x159;epis <klr>(&#x10D;&#xE1;sti dat z jedn&#xE9; pam&#x11B;ti do jin&#xE9;)</klr></trd>,
    <trd>prov&#xE1;d&#x11B;t v&#xFD;pis</trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5B0;&#x5D4;&#x5B7;&#x5E4;&#x5B0;&#x5E7;&#x5B4;&#x5D9;&#x5D3;</trd>
  <trd lng="sk">ulo&#x17E;i&#x165; do &#xFA;schovy</trd>
</drv>

<drv mrk="depon.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="depon.0ajxo.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Tio, kion iu <tld/>as.
    </dif>
    <trd lng="fr">d&eacute;p&ocirc;t<klr> (de valeur)</klr></trd>
  </snc>
  <snc mrk="depon.0ajxo.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      <ref tip="vid" cel="fund.sur0ajxo.MIN">Surfunda&jcirc;o</ref>
      a&ubreve; materia tavolo, kiun postlasas natura, &hcirc;emia,
      te&hcirc;nika okazo:
      <ekz>
        ostoj de terciaraj mamuloj estis malkovritaj
        <klr>...</klr> en lagaj <tld/>a&jcirc;oj
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 9a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fecx.0o.AGR">fe&ccirc;o<sncref/></ref>,
      <ref cel="sedime.0o">sedimento</ref>
    </refgrp>
    <trd lng="fr">d&eacute;p&ocirc;t<klr> (g&eacute;ol.)</klr></trd>
  </snc>
  <trd lng="he">&#x5E4;&#x5E7;&#x5D3;&#x5D5;&#x5DF;</trd>
</drv>

<drv mrk="depon.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="depon.0ejo.loko">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Loko, kie oni <tld/>as multvalora&jcirc;on gardotan.
    </dif>
    <trdgrp lng="de">
      <trd>Depot</trd>,
      <trd>Aufbewahrungsstelle</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_A;&a_n;&a_b;&a_A;&a_r; &a_s;&f_p;&a_r;&a_d;&a_h;&zwnj;&a_h;&a_A;</trd>,
      <trd>&a_s;&f_p;&a_r;&a_d;&a_n;&f_g;&a_A;&a_h;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>let&eacute;ti hely</trd>,
      <trd>&eacute;rt&eacute;kmeg&odblac;rz&odblac;</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>depozyt</trd>,
      <trd>skrytka bankowa</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_h;&c_r;&c_a;&c_n;&c_i;&c_l;&c_i;&c_shch;&c_je;</trd>,
      <trd>&c_k;&c_a;&c_m;&c_je;&c_r;&c_a;
      &c_h;&c_r;&c_a;&c_n;&c_je;&c_n;&c_i;&c_ja;</trd>,
      <trd>&c_s;&c_k;&c_l;&c_a;&c_d;</trd>
    </trdgrp>
  </snc>
  <snc mrk="depon.0ejo.konto">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Konto, kie oni <tld/>as libromonon, valorpaperojn k.s.
    </dif>
    <trdgrp lng="de">
      <trd>Depot</trd>,
      <trd>Hinterlegungsstelle</trd>
    </trdgrp>
    <trd lng="fa">&a_H;&a_s;&a_A;&a_b; &a_s;&f_p;&a_r;&a_d;&a_h;</trd>
    <trd lng="hu">let&eacute;ti <ind>sz&aacute;mla</ind></trd>
    <trd lng="pl">depozyt</trd>
    <trdgrp lng="ru">
      <trd>&c_b;&c_a;&c_n;&c_k;&c_o;&c_v;&c_s;&c_k;&c_i;&c_j;
      <ind>&c_v;&c_k;&c_l;&c_a;&c_d;</ind></trd>,
      <trd>&c_d;&c_je;&c_p;&c_o;&c_z;&c_i;&c_t;</trd>
    </trdgrp>
  </snc>
  <trd lng="ca">dip&ograve;sit</trd>
  <trdgrp lng="cs">
    <trd>deponie</trd>,
    <trd>skladi&#x161;t&#x11B;</trd>,
    <trd>skl&#xE1;dka</trd>,
    <trd>&#xFA;schovna</trd>
  </trdgrp>
  <trd lng="sk">&#xFA;schov&#x148;a</trd>
</drv>

<drv mrk="depon.mal0i">
  <kap>mal<tld/>i</kap>
  <snc mrk="depon.mal0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Repreni al si <tld/>itan valora&jcirc;on:
      <ekz>
        ili povas mal<tld/>i maksimume 200000 enojn
        <fnt>
          <aut>Hori Y.</aut>,
          <vrk><url ref="http://www.satesperanto.org/La-22an-de-marto.html">
          La 22an de marto</url></vrk>,
          <lok>SAT, 2011=03-22</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>retirar</trd>,
    <trd>treure</trd>
  </trdgrp>
  <trd lng="fr">retirer<klr> (de l'argent)</klr></trd>
</drv>

<drv mrk="depon.0atesto">
  <kap><tld/>atesto</kap>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Atesta slipo pri <tld/>ita a&jcirc;o.
    </dif>
    <trdgrp lng="ca">
      <trd>resguard d'ingr&eacute;s</trd>,
      <trd>certificat de dip&ograve;sit</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Depositenschein</trd>,
      <trd>Verwahrschein</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_r;&a_s;&f_y;&a_d; &a_s;&f_p;&a_r;&a_d;&a_h;</trd>,
      <trd>&a_b;&a_r;&f_g;&a_h;&a_hamza_sure; &a_s;&f_p;&a_r;&a_d;&a_h;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>let&eacute;ti jegy</trd>,
      <trd>bet&eacute;ti elismerv&eacute;ny</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_je;&c_p;&c_o;&c_z;&c_i;&c_t;&c_n;&c_o;&c_je;
      &c_s;&c_v;&c_i;&c_d;&c_je;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>,
      <trd>&c_d;&c_je;&c_p;&c_o;&c_z;&c_i;&c_t;&c_n;&c_y;&c_j;
      &c_s;&c_je;&c_r;&c_t;&c_i;&c_f;&c_i;&c_k;&c_a;&c_t;</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">stvrzenka o &#xFA;schov&#x11B;</trd>
  <trd lng="he">&#x5EA;&#x5E2;&#x5D5;&#x5D3;&#x5EA; &#x5E4;&#x5D9;&#x5E7;&#x5D3;&#x5D5;&#x5DF;</trd>
  <trd lng="sk">potvrdenka o &#xFA;schove</trd>
</drv>
</art>
