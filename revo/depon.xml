<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: depon.xml,v 1.31 2022/10/28 08:46:57 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>depon</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="depon.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="depon.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="konfid.0i.fidante_transdoni">Konfidi</ref> 
      al iu multvalora&jcirc;on gardotan:
      <ekz>
        ili demetu de si siajn vestojn <klr>[&#8230;]</klr> kaj <tld/>u ilin en la sanktaj
        &ccirc;ambroj
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 44:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni ser&ccirc;u en la domo <klr>[&#8230;]</klr> kie la trezoroj estis <tld/>itaj
        <fnt>
          <bib>MT</bib>
          <lok>Ezra 6:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        li trompe forprenis <klr>[&#8230;]</klr> la <tld/>ita&jcirc;on <klr>[&#8230;]</klr> <tld/>ita &ccirc;e
        li
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 6:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia mono <klr>[&#8230;]</klr> estas <tld/>ita en la oficejo
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bieno estis vendita kaj la mono <tld/>ita
        <fnt>
          <bib>HsT</bib>
        </fnt>;
      </ekz>
      <ekz>
        la materialon oni tamen povis savi, &ccirc;ar &gcirc;i estis &gcirc;ustatempe <tld/>ita en la
        kastela kelo
        <fnt>
          <bib>EeP</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konser.0i.protekti">konservi<sncref/></ref>,
      <ref cel="stok.0i">stoki</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_d;&c_e;&c_p;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_d;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; &c_n;&c_a; <ind>&c_d;&c_e;&c_p;&c_a;&c_z;&c_y;&c_t;</ind></trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>ingressar</trd>,
      <trd>dipositar</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>deponieren</trd>,
      <trd>hinterlegen</trd>,
      <trd>in <ind>Verwahrung</ind> geben</trd>
    </trdgrp>
    <trd lng="en">deposit</trd>
    <trd lng="es">depositar</trd>
    <trdgrp lng="fa">
      <trd>&a_s;&f_p;&a_r;&a_d;&a_n;</trd>,
      <trd>&a_s;&f_p;&a_r;&a_d;&a_h; &f_g;&a_dh;&a_A;&a_sh;&a_t;&a_n;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;poser<klr tip="ind"> (laisser en
      d&eacute;p&ocirc;t)</klr></trd>,
      <trd>laisser en <ind>d&eacute;p&ocirc;t</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>let&eacute;tbe helyez</trd>,
      <trd>depon&aacute;l</trd>,
      <trd>meg&odblac;rz&eacute;sre &aacute;tad</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>deponowa&cacute;</trd>,
      <trd>powierza&cacute;</trd>,
      <trd>oddawa&cacute; w <ind>depozyt</ind></trd>
    </trdgrp>
    <trd lng="pt">depositar</trd>
    <trdgrp lng="ru">
      <trd>&c_h;&c_r;&c_a;&c_n;&c_i;&c_t;&c_mol;<klr> (&c_v;
      &c_b;&c_a;&c_n;&c_k;&c_je;, &c_v; &c_k;&c_a;&c_m;&c_je;&c_r;&c_je;
      &c_h;&c_r;&c_a;&c_n;&c_je;&c_n;&c_i;&c_ja; &c_i;
      &c_t;.&c_p;.)</klr></trd>,
      <trd>&c_s;&c_d;&c_a;&c_t;&c_mol; &c_n;&c_a;
      <ind>&c_h;&c_r;&c_a;&c_n;&c_je;&c_n;&c_i;&c_je;</ind></trd>,
      <trd>&c_v;&c_n;&c_je;&c_s;&c_t;&c_i; &c_v;
      <ind>&c_d;&c_je;&c_p;&c_o;&c_z;&c_i;&c_t;</ind></trd>,
      <trd>&c_d;&c_je;&c_p;&c_o;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_n;&c_je;&c_s;&c_t;&c_i;
      <ind>&c_v;&c_k;&c_l;&c_a;&c_d;</ind><klr> (&c_v;
      &c_b;&c_a;&c_n;&c_k;)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="depon.0i.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Surfundigi, <ref cel="precip1.0i.MET">precipiti</ref>, postlasi sur io:
      <ekz>
        la sablo, se &gcirc;i ne estus elprenita, sin <tld/>us
        pli fore kaj &gcirc;enus la funkciadon de la stacio
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Prilaboro_de_la_uzitaj_akvoj">
          Prilaboro de la uzitaj akvoj</url>, 2012-06-16</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="precip1.0i.KEM">precipiti</ref>,
      <ref cel="sedime.0igxi.GEOL">sedimenti&gcirc;i</ref>
    </refgrp>
    <trd lng="be">&c_a;&c_d;&c_k;&c_l;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>
    <trd lng="de">ablagern</trd>
    <trd lng="fr">d&eacute;poser<klr> (hydrol.)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>prov&aacute;d&ecaron;t p&rcaron;epis <klr>(&ccaron;&aacute;sti dat z jedn&eacute; pam&ecaron;ti do jin&eacute;)</klr></trd>,
    <trd>prov&aacute;d&ecaron;t v&yacute;pis</trd>
  </trdgrp>
  <trd lng="he">&lamed;&#x5B0;&he;&#x5B7;&pe;&#x5B0;&qof;&#x5B4;&yod;&dalet;</trd>
  <trd lng="sk">ulo&zcaron;i&tcaron; do &uacute;schovy</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_b;&c_je;&c_r;&c_ib;&c_g;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &#x43D;&c_a; &c_z;&c_b;&c_je;&c_r;&c_ib;&c_g;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_v;&#x43D;&#x43E;&c_s;&c_i;&c_t;&c_i; &c_v;&#x43A;&#x43B;&c_a;&c_d; (&c_u; &c_b;&c_a;&#x43D;&#x43A;)</trd>,
    <trd>&c_d;&c_je;&#x43F;&#x43E;&#x43D;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="depon.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="depon.0ajxo.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Tio, kion iu <tld/>as:
      <ekz>
        anstata&ubreve;igi la privatan proprieton per la komuna <klr>[&#8230;]</klr>, tiel ke &gcirc;iaj
        membroj ne havos e&ccirc; unu cendon da <tld/>a&jcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="&Monato;/2011/010245.php">Floras enklavo komunista</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_e;&c_p;&c_a;&c_z;&c_y;&c_t;</trd>,
      <trd>&c_u;&c_k;&c_l;&c_a;&c_d; <klr>(&c_d;&c_e;&c_p;&c_a;&c_z;&c_y;&c_t;&c_n;&c_y;)</klr></trd>,
      <trd>&c_d;&c_e;&c_p;&c_a;&c_n;&c_a;&c_v;&c_a;&c_n;&c_a;&c_ja; &c_r;&c_e;&c_ch;</trd>
    </trdgrp>
    <trd lng="de">Verwahrgut</trd>
    <trd lng="fr">d&eacute;p&ocirc;t<klr> (de valeur)</klr></trd>
  </snc>
  <snc mrk="depon.0ajxo.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      <ref tip="vid" cel="fund.sur0ajxo.MIN">Surfunda&jcirc;o</ref>
      a&ubreve; materia tavolo, kiun postlasas natura, &hcirc;emia,
      te&hcirc;nika okazo:
      <ekz>
        ostoj de terciaraj mamuloj estis malkovritaj
        <klr>[&#8230;]</klr> en lagaj <tld/>a&jcirc;oj
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 9a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fecx.0o.AGR">fe&ccirc;o<sncref/></ref>,
      <ref cel="sedime.0o">sedimento</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_a;&c_d;&c_k;&c_l;&c_a;&c_d;</trd>,
      <trd>&c_a;&c_d;&c_k;&c_l;&c_a;&c_d;&c_a;&c_n;&c_mol;&c_n;&c_je; <klr>(&c_g;&c_je;&c_a;&c_l;.)</klr></trd>
    </trdgrp>
    <trd lng="de">Ablagerung</trd>
    <trd lng="fr">d&eacute;p&ocirc;t<klr> (g&eacute;ol.)</klr></trd>
  </snc>
  <trd lng="he">&pe;&qof;&dalet;&vav;&fnun;</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_ib;&c_ch;</trd>,
    <trd>&c_z;&c_d;&c_a;&#x43D;&c_a; &#x43D;&c_a; &c_z;&c_b;&c_je;&c_r;&c_je;&c_zh;&c_je;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_v;&#x43D;&c_je;&c_s;&#x43E;&#x43A;</trd>,
    <trd>&c_d;&c_je;&#x43F;&#x43E;&c_z;&c_i;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="depon.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="depon.0ejo.loko">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Loko, kie oni <tld/>as multvalora&jcirc;on gardotan:
      <ekz>
        du longajn murojn okupis <tld/>ejoj por dokumentoj kaj ricevitaj leteroj,
        argilaj kaj papirusaj
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili estas anka&ubreve; dokumento-<tld/>ejoj kaj konsultejoj por historiistoj
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        la apuda pordo ekzemple indikas: kiras<tld/>ejo
        <fnt>
          <bib>KKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        enuigaj havenaj instala&jcirc;oj, <tld/>ejoj, staploj kaj al la tristaj kontor&ccirc;eloj
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        en Serbio estis konstruitaj novaj municio-<tld/>ejoj
        <fnt>
          <bib>Monato</bib>
          <aut>Dimitrije Jani&ccaron;i&cacute;</aut>
          <vrk><url ref="&Monato;/2013/010867.php">Detruo-dilemo</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni planas anka&ubreve; la konstruon de grandaj gas<tld/>ejoj en Albanio
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2013/010879.php">TAP: projekto por sud-e&ubreve;ropa
          gaskoridoro</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="arhxiv.0ejo">ar&hcirc;ivejo</ref>,
      <ref cel="arsena.0o.armilejo">arsenalo<sncref/></ref>,
      <ref cel="met.for0ejo">formetejo</ref>,
      <ref cel="konser.0ejo">konservejo</ref>,
      <ref cel="proviz.0ejo">provizejo</ref>,
      <ref cel="stok.0ejo">stokejo</ref>,
      <ref cel="ten.0ejo">tenejo</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_d;&c_e;&c_p;&c_o;</trd>,
      <trd>&c_s;&c_h;&c_o;&c_v;&c_ib;&c_sh;&c_ch;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Depot</trd>,
      <trd>Aufbewahrungsstelle</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_A;&a_n;&a_b;&a_A;&a_r; &a_s;&f_p;&a_r;&a_d;&a_h;&#8204;&a_h;&a_A;</trd>,
      <trd>&a_s;&f_p;&a_r;&a_d;&a_n;&f_g;&a_A;&a_h;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>let&eacute;ti hely</trd>,
      <trd>&eacute;rt&eacute;kmeg&odblac;rz&odblac;</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>depozyt</trd>,
      <trd>skrytka bankowa</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_h;&c_r;&c_a;&c_n;&c_i;&c_l;&c_i;&c_shch;&c_je;</trd>,
      <trd>&c_k;&c_a;&c_m;&c_je;&c_r;&c_a;
      &c_h;&c_r;&c_a;&c_n;&c_je;&c_n;&c_i;&c_ja;</trd>,
      <trd>&c_s;&c_k;&c_l;&c_a;&c_d;</trd>
    </trdgrp>
  </snc>
  <snc mrk="depon.0ejo.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      <ref tip="dif" cel="kusx.0ejo.GEOL">Ku&scirc;ejo<sncref/></ref> de 
      mineralo <ref cel="depon.0i.GEOL">deponita<sncref/></ref>:
      <ekz>
        Usono havas vastajn <tld/>ejojn de skisto kaj natura gaso
        <fnt>
          <bib>Monato</bib>
          <aut>Lenio Marobin</aut>
          <vrk><url ref="&Monato;/2013/011023.php">Frakasado igos Usonon
          energio-sendependa</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_d;&c_o;&c_v;&c_ib;&c_sh;&c_ch;&c_a;</trd>
    <trd lng="de">Lagerst&auml;tte</trd>
  </snc>
  <snc mrk="depon.0ejo.KOMUNE">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Loko, kien oni formetas grandajn kvantojn da &scirc;uta&jcirc;o, rubo ks:
      <ekz>
        <tld/>ejo de skorio, cindro, sablo;
      </ekz>
      <ekz>
        arkeologoj malkovris rubo<tld/>ejon el la tria jarcento
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2011/010488.php">Rubo de la romianoj</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ejoj de poluitaj resta&jcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2010/009967.php">Adia&ubreve; al la nuklea &scirc;tato</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        aran&gcirc;o de <tld/>ejo de uzita nuklea brula&jcirc;o, la plej dan&gcirc;era radioaktiva
        resta&jcirc;o, kostus &ccirc;irka&ubreve; 2,6 miliardojn da e&ubreve;roj
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2006/008809.php">Litovio volas resti nukle-energia
          &scirc;tato</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[multaj]</klr> produktoj <klr>[&#8230;]</klr> fini&gcirc;os en maro a&ubreve; rubo<tld/>ejo
        <fnt>
          <bib>Kon11</bib>
        </fnt>;
      </ekz>
      <ekz>
        faki&gcirc;i pri tersurfaca protekto (grundoprotekto, konstruado de rub<tld/>ejoj,
        rekultivado de rub<tld/>ejoj <klr>[&#8230;]</klr>)
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#8470;3</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="rub.0ejo">rubejo</ref>
    </refgrp>
    <trd lng="be">&c_s;&c_mol;&c_m;&c_je;&c_t;&c_n;&c_ib;&c_k;</trd>
    <trdgrp lng="de">
      <trd>Halde</trd>,
      <trd>Deponie</trd>,
      <trd>Kippe</trd>
    </trdgrp>
  </snc>
  <snc mrk="depon.0ejo.konto">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="kont.0o.KOME">Konto</ref>, kie oni <tld/>as libromonon, valorpaperojn
      k.s.
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_e;&c_p;&c_a;&c_z;&c_y;&c_t;</trd>,
      <trd>&c_u;&c_k;&c_l;&c_a;&c_d; <klr>(&c_b;&c_a;&c_n;&c_k;&c_a;&c_w;&c_s;&c_k;&c_ib;)</klr></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Depot</trd>,
      <trd>Hinterlegungsstelle</trd>
    </trdgrp>
    <trd lng="fa">&a_H;&a_s;&a_A;&a_b; &a_s;&f_p;&a_r;&a_d;&a_h;</trd>
    <trd lng="hu">let&eacute;ti <ind>sz&aacute;mla</ind></trd>
    <trd lng="pl">depozyt</trd>
    <trdgrp lng="ru">
      <trd>&c_b;&c_a;&c_n;&c_k;&c_o;&c_v;&c_s;&c_k;&c_i;&c_j;
      <ind>&c_v;&c_k;&c_l;&c_a;&c_d;</ind></trd>,
      <trd>&c_d;&c_je;&c_p;&c_o;&c_z;&c_i;&c_t;</trd>
    </trdgrp>
  </snc>
  <trd lng="ca">dip&ograve;sit</trd>
  <trdgrp lng="cs">
    <trd>deponie</trd>,
    <trd>skladi&scaron;t&ecaron;</trd>,
    <trd>skl&aacute;dka</trd>,
    <trd>&uacute;schovna</trd>
  </trdgrp>
  <trd lng="sk">&uacute;schov&ncaron;a</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43A;&#x43B;&c_a;&c_d;</trd>,
    <trd>&c_s;&c_h;&#x43E;&c_v;&c_i;&c_shch;&c_je;</trd>,
    <trd>&#x43A;&c_a;&#x43C;&c_je;&c_r;&c_a; &c_s;&c_h;&#x43E;&c_v;&c_u; (&c_b;&c_a;&c_gu;&c_a;&c_zh;&c_u;)</trd>,
    <trd>&#x43A;&c_a;&c_s;&c_a; (&c_d;&#x43B;&#x44F; &c_z;&c_b;&c_je;&c_r;&c_je;&c_zh;&c_je;&#x43D;&#x43D;&#x44F; &c_g;&c_r;&#x43E;&c_sh;&c_je;&c_j;)</trd>
  </trdgrp>
</drv>

<drv mrk="depon.0ujo">
  <kap>
    <tld/>ujo
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="uj.0o">Ujo</ref> en kiu oni <tld/>as ion:
      <ekz>
        la akvobata pumpilo povas peli parton de la alvenanta akvo supren, &gcirc;is la
        <tld/>ujo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="https://tekstaro.com/t?nomo=artikoloj-el-monato">Hidra&ubreve;liko: Akvobata
          pumpilo &ndash; forgesita sed utiligebla</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
