<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>urin</rad>/i
</kap>
<drv mrk="urin.0i">
<kap><ofc>*</ofc><tld/>i</kap>
<gra><vspec>ntr</vspec></gra>
<snc>
<dif>
Eligi <tld/>on.
</dif>
</snc>
<trdgrp lng="de">
   <trd>urinieren</trd>,
   <trd>Wasser lassen</trd>,
   <trd>pinkeln</trd>
</trdgrp>
<trd lng="el">&omikron;&ypsilon;&rho;&omega_ton;</trd>
<trd lng="en">urine</trd>
<trd lng="fr">uriner</trd>
<trd lng="nl">urineren</trd>
<trd lng="pt">urinar</trd>
<trd lng="ru">&c_m;&c_o;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
<trdgrp lng="sv">
  <trd>urinera</trd>,
  <trd>kasta <ind>vatten</ind></trd>,
  <trd>kissa</trd>
</trdgrp>
</drv>
<drv mrk="urin.0o">
<kap><tld/>o</kap>
<uzo tip="fak">ANA</uzo>
<uzo tip="fak">MED</uzo>
<snc>
<dif>
Flava <ref tip="super" cel="ekskre1.0o">ekskrementa likva&jcirc;o</ref>,
tirita el la sango per la renoj k eligata de la veziko.
</dif>
</snc>
<trd lng="de">Urin</trd>
<trdgrp lng="de">
   <trd>Urin</trd>,
   <trd>Harn</trd>
</trdgrp>
<trd lng="el">&omikron;&ypsilon_ton;&rho;&omikron;</trd>
<trd lng="en">urine</trd>
<trd lng="fr">urine</trd>
<trd lng="nl">urine</trd>
<trd lng="pt">urina</trd>
<trd lng="ru">&c_m;&c_o;&c_ch;&c_a;</trd>
<trd lng="sv">urin</trd>
</drv>
</art>
