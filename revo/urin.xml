<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: urin.xml,v 1.44 2023/10/21 16:50:28 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>urin</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="urin.0o">
   <kap><tld/>o</kap>
   <snc mrk="urin.0o.FIZL">
     <dif>
       Flava likva <ref tip="super" cel="ekskre1.0o">ekskremento</ref>,
       tirita el la sango per la renoj kaj eligata de la veziko tra la
       uretro:
       <ekz>
         la <tld/>o kaj feka&jcirc;oj de intense breditaj bestoj poluas
         riverojn
         <fnt>
         <aut>Krys Ungar</aut>,
         <vrk><url ref="&Monato;/1995/004583.php">Vivu vigle kaj vegetare!</url></vrk>,
         <bib>Monato</bib>,
         <lok>1995/07, p. 18</lok>
         </fnt>.
       </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_m;&c_a;&c_ch;&c_a;</trd>
  <trd lng="bg">&c_u;&c_r;&c_i;&c_n;&c_a;</trd>
  <trd lng="bo">&#xF42;&#xF45;&#xF72;&#xF53;&#xF0B;&#xF54;&#xF0B;</trd>
  <trdgrp lng="br">
     <trd>troazh<klr> [homoj]</klr></trd>,
     <trd>staot<klr> [bestoj]</klr></trd>
   </trdgrp>
  <trd lng="ca">orina</trd>
  <trd lng="cs">mo&ccaron;</trd>
  <trdgrp lng="de">
     <trd>Urin</trd>,
     <trd>Harn</trd>
   </trdgrp>
  <trd lng="el">&omikron;&ypsilon_ton;&rho;&omikron;</trd>
  <trd lng="en">urine</trd>
  <trdgrp lng="es">
     <trd>or&iacute;n</trd>,
     <trd>orina</trd>
   </trdgrp>
  <trd lng="fi">virtsa</trd>
  <trd lng="fr">urine</trd>
  <trd lng="hu">vizelet</trd>
  <trdgrp lng="id">
     <trd><ind>air</ind> kencing</trd>,
     <trd>pipis</trd>
     <trd>urine</trd>
   </trdgrp>
  <trdgrp lng="it">
     <trd>pip&igrave;</trd>,
     <trd>urina</trd>,
     <trd>orina</trd>,
   </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5C3F;  <pr>&#x306B;&#12423;&#12358;</pr></trd>,
    <trd>&#x5C0F;&#x4FBF;  <pr>&#12375;&#12423;&#12358;&#12409;&#12435;</pr></trd>,
    <trd>&#x5C0F;&#x6C34;  <pr>&#12375;&#12423;&#12358;&#12377;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">urine</trd>
  <trdgrp lng="pl">
     <trd>uryna</trd>,
     <trd>mocz</trd>,
     <trd>siki <klr>(pot.)</klr></trd>
   </trdgrp>
  <trd lng="pt">urina</trd>
  <trd lng="ru">&c_m;&c_o;&c_ch;&c_a;</trd>
  <trd lng="sk">mo&ccaron;</trd>
  <trdgrp lng="sv">
     <trd>urin</trd>,
     <trd>kiss</trd>
   </trdgrp>
  <trd lng="uk">&c_s;&c_je;&c_ch;&c_a;</trd>
</drv>

<drv mrk="urin.0i">
   <kap><ofc>*</ofc><tld/>i</kap>
   <gra><vspec>ntr</vspec></gra>
   <snc mrk="urin.0i.FIZL">
     <uzo tip="fak">FIZL</uzo>
     <dif>
       Eligi <tld/>on.
     </dif>
   </snc>
  <trd lng="be">&c_m;&c_a;&c_ch;&c_y;&c_c;&c_c;&c_a;</trd>
  <trd lng="bg">&c_u;&c_r;&c_i;&c_n;&c_i;&c_r;&c_a;&c_m;</trd>
  <trdgrp lng="br">
     <trd>troazha&ntilde;<klr> [homoj]</klr></trd>,
     <trd>staota&ntilde;<klr> [bestoj]</klr></trd>
   </trdgrp>
  <trd lng="ca">orinar</trd>
  <trdgrp lng="cs">
    <trd>mo&ccaron;it</trd>,
    <trd>vymo&ccaron;it se</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>urinieren</trd>,
     <trd>Wasser lassen</trd>,
     <trd>pinkeln</trd>
   </trdgrp>
  <trd lng="el">&omikron;&ypsilon;&rho;&omega_ton;</trd>
  <trd lng="en">urinate</trd>
  <trd lng="es">orinar</trd>
  <trd lng="fi">virtsata</trd>
  <trdgrp lng="fr">
     <trd>faire <ind>pipi</ind></trd>,
     <trd>uriner</trd>
   </trdgrp>
  <trd lng="hu">vizel</trd>
  <trdgrp lng="id">
     <trd><ind>buang air</ind> kecil</trd>,
     <trd>ber<ind>kemih</ind></trd>,
     <trd>kencing</trd>,
     <trd>pipis</trd>
   </trdgrp>
  <trdgrp lng="it">
     <trd>fare <ind>pip&igrave;</ind></trd>,
     <trd>urinare</trd>,
     <trd>orinare</trd>,
   </trdgrp>
  <trdgrp lng="ja">
    <trd>&#25490;&#x5C3F;&#12377;&#x308B;  <pr>&#x306F;&#12356;&#x306B;&#12423;&#12358;&#12377;&#x308B;</pr></trd>,
    <trd>&#x653E;&#x5C3F;&#12377;&#x308B; <pr>&#12411;&#12358;&#12395;&#12423;&#12358;&#12377;&#12427;</pr></trd>,
    <trd>&#x5C0F;&#x4FBF;&#12377;&#x308B;  <pr>&#12375;&#12423;&#12358;&#12409;&#12435;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
     <trd>urineren</trd>,
     <trd>plassen</trd>
   </trdgrp>
  <trdgrp lng="pl">
     <trd>oddawa&cacute; <ind>mocz</ind></trd>,
     <trd>sika&cacute; <klr>(pot.)</klr></trd>
   </trdgrp>
  <trdgrp lng="pt">
    <trd>urinar</trd>,
    <trd>mijar</trd>
   </trdgrp>
  <trd lng="ru">&c_m;&c_o;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="sk">mo&ccaron;i&tcaron;</trd>
  <trdgrp lng="sv">
     <trd>urinera</trd>,
     <trd>kasta <ind>vatten</ind></trd>,
     <trd>kissa</trd>
   </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&#x43F;&c_u;&c_s;&#x43A;&c_a;&c_t;&c_i; &c_s;&c_je;&c_ch;&c_u;</trd>,
    <trd>&c_s;&c_c;&#x44F;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="urin.0ado">
   <kap><tld/>ado</kap>
   <snc mrk="urin.0ado.FIZL">
     <dif>
       Ellasado de urino:
       <ekz>
         li ne maltrankvili&gcirc;is pri <tld/>ado, &ccirc;ar per tiu
         tubo lia veziko dreni&gcirc;is al sublita botelo
         <fnt>
          <aut>David Curtis</aut>
          <vrk><url ref="&Monato;/2002/pdf/monato200201.pdf#page=28">Koratako</url></vrk>
          <bib>Monato</bib>
          <lok>2002/01, p. 28</lok>
        </fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_m;&c_o;&c_ch;&c_a;&c_s;&c_p;&c_u;&c_s;&c_k;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="br">
     <trd>troazhadenn<klr> [homoj]</klr></trd>,
     <trd>staotadenn<klr> [bestoj]</klr></trd>
   </trdgrp>
  <trdgrp lng="ca">
     <trd>orinada</trd>,
     <trd>micci&oacute;</trd>
   </trdgrp>
  <trd lng="cs">mo&ccaron;en&iacute;</trd>
  <trd lng="de">Urinieren</trd>
  <trd lng="en">urination</trd>
  <trd lng="fi">virtsaaminen</trd>
  <trdgrp lng="fr">
     <trd>diur&egrave;se</trd>,
     <trd>miction</trd>
   </trdgrp>
  <trd lng="hu">vizel&eacute;s</trd>
  <trdgrp lng="id">
    <trd><baz>kencing</baz> pengencingan</trd>,
    <trd>urinasi</trd>
  </trdgrp>
  <trdgrp lng="it">
   <trd>minzione</trd>, <trd>diuresi</trd>,
   <trd>pisciata<klr> (volg.)</klr></trd></trdgrp>
  <trdgrp lng="ja">
    <trd>&#25490;&#x5C3F;  <pr>&#x306F;&#12356;&#x306B;&#12423;&#12358;</pr></trd>,
    <trd>&#x5C0F;&#x4FBF;  <pr>&#12375;&#12423;&#12358;&#12409;&#12435;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
     <trd>urineren<klr tip="ind"> (subst.)</klr></trd>,
     <trd>plassen<klr tip="ind"> (subst.)</klr></trd>
   </trdgrp>
  <trdgrp lng="pl">
     <trd>oddawanie <ind>moczu</ind></trd>,
     <trd>sikanie <klr>(pot.)</klr></trd>
   </trdgrp>
  <trd lng="ru">&c_m;&c_o;&c_ch;&c_je;&c_i;&c_s;&c_p;&c_u;&c_s;&c_k;&c_a;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">mo&ccaron;enie</trd>
  <trdgrp lng="sv">
     <trd>urinerande<klr> (handling)</klr></trd>,
     <trd>kissande<klr> (handling)</klr></trd>
   </trdgrp>
  <trd lng="uk">&c_s;&c_je;&c_ch;&#x43E;&c_v;&c_i;&#x43F;&c_u;&c_s;&#x43A;&c_a;&#x43D;&#x43D;&#x44F;</trd>
</drv>
</art>
