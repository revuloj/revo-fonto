<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: slavistik.xml,v 1.4 2025/04/02 02:02:18 afido Exp $">
<kap>
    <rad>slavistik</rad>/o <fnt><bib>Viki</bib></fnt>
</kap>
<drv mrk="slavistik.0o">
  <kap><tld/>o</kap>
  <snc mrk="slavistik.0o.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      Parto de <ref tip="malprt" cel="lingv.0oscienco.LIN">lingvoscienco</ref>
      kiu studas <ref tip="vid" cel="slav.0a_lingvo.LIN">slavajn lingvojn</ref>:
      <ekz>
        <nom>Baudouin de Courtenay</nom> studis en Germanio kaj poste laboris kiel
        profesoro pri slavistiko en universitatoj de Ruslando
        <fnt>
          <aut>A. K&uuml;nzli</aut>
          <vrk><url ref="https://sezonoj.ru/2013/03/221trezoro/">
          Nia trezoro: Baudouin de Courtenay</url></vrk>
          <lok>La Ondo de Esperanto, 2013, &#8470;3</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_l;&c_a;&c_v;&c_ib;&c_s;&c_t;&c_y;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_v;&c_ja;&c_n;&c_a;&c_z;&c_n;&c_a;&c_w;&c_s;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Slawistik</trd>
  <trdgrp lng="en">
    <trd>Slavistics</trd>,
    <trd><ind>Slavic</ind> studies <klr>(US)</klr></trd>,
    <trd><ind>Slavonic</ind> studies <klr>(UK)</klr></trd>
  </trdgrp>
  <trd lng="fr">slavistique</trd>
</drv>
</art>

