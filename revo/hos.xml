<kap>
  <rad>hos</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="hos.0o">
  <kap><tld/>o</kap>
  <snc mrk="hos.0o.TEK">
    <dif>
      <ref cel="tub.fleks0o.TEK">Flekstubo</ref>:
      <ekz>
        kelkaj flegistoj kun <tld/>oj seninfektigas min
        de la kapo &gcirc;is la piedfingroj
        <fnt>
          <aut>E. Mendoza, trad. O. Kowalski</aut>
          <vrk>Sen nova&jcirc;oj pri Gurb</vrk>
          <lok>2019</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: (en &gcirc;enerala nefaka lingvo ... prefere diru <ke>mola tubo</ke>.
  </rim>

  <trd lng="ca">m&agrave;nega <klr tip="amb">(tub flexible)</klr></trd>
  <trd lng="es">manguera</trd>
  <trdgrp lng="fr">
    <trd>flexible<klr> (tuyau)</klr></trd>,
    <trd>tuyau souple</trd>
  </trdgrp>
  <trd lng="be">&c_sh;&c_l;&c_ja;&c_n;&c_g;</trd>
  <trd lng="uk">&c_sh;&c_l;&c_a;&c_n;&c_gu;</trd>
  <trd lng="zh">&#36575;&#31649; <pr>ru&#462;ngu&#462;n</pr></trd>
</drv>
