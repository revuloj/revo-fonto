<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <rad>urtikac</rad>/oj
</kap>
<drv mrk="urtika.0oj">
   <kap>
     <tld/>oj
   </kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
      <ref tip="super" cel="famili.0o.BIO">Familio</ref> de herboj kaj sta&ubreve;doj
      el la ordo de rozoparencaj
      <ref tip="lst" lst="voko:rozoparencoj" cel="flor.0oplant">floroplantoj</ref>
      (<trd lng="la">Urticaceae</trd>) ofte kun brulharoj;
      folioj kontra&ubreve;aj a&ubreve; kruce alternaj, segildentaj;
      umbeloj kunmetitaj, cumoj sidantaj a&ubreve; tirsoj;
      ofte 4 &gcirc;is 5, malofte 2 &gcirc;is 6 tepaloj;
      ovolujo supra, unufaka:
      <ekz>
        la manko de vestoj indikas, ke li estis vestita de &scirc;tofoj el plantaj 
        fibroj, kiel lino, kanabo a&ubreve; <tld/>oj, kiujn detruis la mar&ccirc;a acido
        <fnt>
          <bib>Viki</bib>
          <lok><url ref='https://eo.wikipedia.org/?curid=249787'>Viro de Tollund</url></lok>
        </fnt>.
      </ekz>
     </dif>
     <ref tip="prt" cel="urtik.0o">urtiko</ref>
   </snc>
   <trd lng="ca">urtic&agrave;cies</trd>
   <trd lng="de">Brennnesselgew&auml;chse</trd>
   <trd lng="es">urtic&aacute;ceas</trd>
   <trd lng="fi">nokkoskasvit</trd>
   <trd lng="fr">urticac&eacute;es</trd>
   <trd lng="hu">csal&aacute;nf&eacute;l&eacute;k</trd>
   <trd lng="it">urticacee</trd>
   <trd lng="nl">brandnetelfamilie</trd>
   <trd lng="pt">urtic&aacute;ceas</trd>
   <trd lng="ru">&c_k;&c_r;&c_a;&c_p;&c_i;&c_v;&c_n;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
