<vortaro>
<art mrk="$Id: urtika.xml,v 1.18 2024/05/10 13:18:59 revo Exp $">
<kap>
   <rad>urtikac</rad>/oj
</kap>
<drv mrk="urtika.0oj">
   <kap>
     <tld/>oj
   </kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
      <ref tip="lst" lst="voko:botanikaj_familioj" cel="famili.0o.BIO">Familio</ref> de herboj kaj
      sta&ubreve;doj
      el la ordo de rozoparencaj
      <ref tip="malprt" cel="flor.0oplant">floroplantoj</ref>
      (<trd lng="la">Urticaceae</trd>) ofte kun brulharoj;
      folioj kontra&ubreve;aj a&ubreve; kruce alternaj, segildentaj;
      umbeloj kunmetitaj, cumoj sidantaj a&ubreve; tirsoj;
      ofte 4 &gcirc;is 5, malofte 2 &gcirc;is 6 tepaloj;
      ovolujo supra, unufaka:
      <ekz>
        la manko de vestoj indikas, ke li estis vestita de &scirc;tofoj el
        plantaj
        fibroj, kiel lino, kanabo a&ubreve; <tld/>oj, kiujn detruis la mar&ccirc;a
        acido
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=249787">Viro de
          Tollund</url></lok>
        </fnt>.
      </ekz>
     </dif>
     <ref tip="prt" cel="urtik.0o">urtiko</ref>
   </snc>
   <trd lng="be">&c_k;&c_r;&c_a;&c_p;&c_ib;&c_w;&c_n;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#x2019;&c_ja;)</klr></trd>
   <trd lng="ca">urtic&agrave;cies</trd>
   <trd lng="de">Brennnesselgew&auml;chse</trd>
   <trd lng="es">urtic&aacute;ceas</trd>
   <trd lng="fi">nokkoskasvit</trd>
   <trd lng="fr">urticac&eacute;es</trd>
   <trd lng="hu">csal&aacute;nf&eacute;l&eacute;k</trd>
   <trd lng="it">urticacee</trd>
   <trd lng="nl">brandnetelfamilie</trd>
   <trd lng="pl">pokrzywowate</trd>
   <trd lng="pt">urtic&aacute;ceas</trd>
   <trd lng="ru">&c_k;&c_r;&c_a;&c_p;&c_i;&c_v;&c_n;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&#x43A;&c_r;&#x43E;&#x43F;&c_i;&c_v;&#x43E;&c_v;&c_ib;</trd>
  <trdgrp lng="zh">
    <trd>&#x8368;&#x9EBB;&#x76EE; <pr>x&#xFA;nm&#xE1;m&#xF9;</pr></trd>,
    <trd>&#x8541;&#x9EBB;&#x76EE; <pr>x&#xFA;nm&#xE1;m&#xF9;</pr></trd>
  </trdgrp>
</drv>
</art>
