<kap>
  <ofc>9</ofc><rad>polp</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="polp.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="polp.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Marbesto havanta ok brakojn kun su&ccirc;iloj,
      genro el familio
      <ref tip="super" cel="cefalo.0oj">cefalopodoj</ref>
      (<trd lng="la">Octopoda</trd>):
      <ekz>
        a&ubreve;straliaj sciencistoj malkovris <tld/>ojn en Indonezio,
        kiuj 
        kolektas kokos&scirc;elojn kiel &scirc;irmilojn
        <fnt>
          <aut>Arono kaj Karlina</aut>,
          <vrk><url ref="http://radioverda.com/">Radio
          Verda</url></vrk>,
          <lok>2009-12-23</lok>
        </fnt>.        
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_s;&c_mol;&c_m;&c_ib;&c_n;&c_o;&c_g;</trd>,
    <trd>&c_s;&c_p;&c_r;&c_u;&c_t;</trd>
  </trdgrp>
  <trd lng="bg">&c_o;&c_k;&c_t;&c_o;&c_p;&c_o;&c_d;</trd>
  <trd lng="en">octopus</trd>
  <trd lng="es">pulpo</trd>
  <trdgrp lng="fr">
    <trd>pieuvre</trd>,
    <trd>poulpe</trd>
  </trdgrp>
  <trd lng="gl">polbo</trd>
  <trd lng="hu">nyolckar&uacute; <ind>polip</ind></trd>
  <trd lng="id">gurita</trd>
  <trdgrp lng="it">
    <trd>polpo</trd>,
    <trd>piovra</trd>
  </trdgrp>
  <trd lng="ja">&#x30BF;&#x30B3;</trd>
  <trd lng="nl">octopus</trd>
  <trd lng="pl">o&sacute;miornica</trd>
  <trd lng="pt">polvo</trd>
  <trd lng="ro">caracati&#x21B;&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_s;&c_mol;&c_m;&c_i;&c_n;&c_o;&c_g;</trd>,
    <trd>&c_s;&c_p;&c_r;&c_u;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x441;&#x43F;&#x440;&#x443;&#x442;</trd>,
    <trd>&#x432;&#x43E;&#x441;&#x44C;&#x43C;&#x438;&#x43D;&#x456;&#x433;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7AE0;&#x9B5A; <pr>zh&#x101;ngy&#xFA;</pr></trd>,
    <trd>&#x7AE0;&#x9C7C; <pr>zh&#x101;ngy&#xFA;</pr></trd>,
    <trd>&#x9C46; <pr>zh&#x101;ng</pr></trd>
  </trdgrp>
</drv>
