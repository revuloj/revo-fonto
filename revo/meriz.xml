<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: meriz.xml,v 1.23 2023/10/19 15:13:16 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>meriz</rad>/o
</kap>
<drv mrk="meriz.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="frukt.0o.BOT">Frukto</ref> de <tld/>arbo:
      <ekz>
        abundas <ind>&scirc;napso</ind>j <klr>(<ref cel="brand.0o">brando</ref>j)</klr> 
        anka&ubreve; el frambo, rubuso, prunuso, mirabelo, <tld/>o, persiko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=304334">&Scirc;napso</url></lok>
        </fnt>;
        <trd lng="de">Schnaps</trd>
        <trd lng="en">schnapps</trd>
        <trd lng="fr">schnaps</trd>
        <trd lng="nl">schnaps</trd>
      </ekz>
      <ekz>
        batu lin! &ndash; li kriis furioze, <klr>[&#x2026;]</klr> kaj mem kaptis per la manoj <tld/>an
        &ccirc;ibukon
        <fnt>
          <bib>LOdE</bib>
          <aut>Valentin Melnikov</aut>
          <vrk><url ref="&Tekstaro;?nomo=ondo-de-esperanto">Malvivaj animoj</url></vrk>
        </fnt>.
      </ekz>      
      <ekz>
        Li staris sub mia pistolo, elektante el sia &ccirc;apo maturajn <tld/>ojn kaj elbu&scirc;igante kernojn
        <fnt>
          <aut>A. S. Pu&scirc;kin (tradikis Andreo Fi&scirc;er)</aut>
          <vrk><url ref="&Gutenberg;files/63105/63105-h/63105-h.htm">Tri
          Noveloj</url></vrk>
        </fnt>.
      </ekz>      
      <ekz>
        Kaj anta&ubreve; miaj okuloj aperas pendantaj pezaj grapoloj de maturaj <tld/>oj kaj tre dol&ccirc;aj
        vinberoj
        <fnt>
          <aut>Vladimir Opletajev </aut>
          <vrk><url ref="https://ural-sib.org/eo/comment/reply/218">Tra jaroj fu&gcirc;antaj (Intervjuo
          kun Raja Kudrjavceva)</url></vrk>
        </fnt>.
      </ekz>      
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_r;&c_e;&c_sh;&c_n;&c_ja; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="es">cereza</trd>
  <trd lng="fr">merise</trd>
  <trdgrp lng="hu">
    <trd>erdei <ind>cseresznye</ind></trd>,
    <trd>mad&aacute;rcseresznye</trd>
  </trdgrp>
  <trd lng="ja">&#x30B5;&#x30AF;&#x30E9;&#x30F3;&#x30DC;</trd>
  <trd lng="nl">kriek</trd>
  <trd lng="pl">czere&sacute;nia</trd>
  <trdgrp lng="pt">
    <trd>ginja</trd>,
    <trd>ginja-galega</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_je;&c_sh;&c_n;&c_ja;
<klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="uk">&c_ch;&c_je;&c_r;&c_je;&c_sh;&#x43D;&#x44F; (&#x43F;&#x43B;&c_ib;&c_d;)</trd>
</drv>

<drv mrk="meriz.0arbo">
  <kap>
    <tld/>arbo
  </kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      Sova&gcirc;a formo de la dol&ccirc;a
      <ref tip="super" cel="cxeriz.0ujo">&ccirc;erizujo</ref>;
      <ref tip="lst" lst="voko:botanikaj_specioj" cel="speci.0o.BIO">specio</ref>
      de <ref tip="super" cel="prunus.0o">prunuso</ref> (<trd lng="la">Prunus avium</trd>)
    </dif>
    <bld lok="&WCU;/thumb/c/c9/Prunus_avium_RF.jpg/640px-Prunus_avium_RF.jpg" prm="CC BY     4.0">floranta bran&ccirc;o de <tld/>ujo (Prunus avium), &ccirc;e Dreieich, Germanujo
        <fnt>
          <aut>Robert Flogaus-Faust, propra verko</aut>
          <lok><url ref="&WCW;/File:Prunus_avium_RF.jpg">&WCW;/File:Prunus_avium_RF.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_r;&c_e;&c_sh;&c_n;&c_ja; <klr>(&c_d;&c_r;&c_e;&c_v;&c_a;)</klr></trd>
  <trd lng="de">Vogelkirsche</trd>
  <trd lng="en">bird-cherry</trd>
  <trd lng="es">cerezo</trd>
  <trd lng="fr">merisier</trd>
  <trd lng="hu">mad&aacute;rcseresznyefa</trd>
  <trdgrp lng="ja">
    <trd>&#x30B5;&#x30AF;&#x30E9;&#x30F3;&#x30DC;</trd>,
    <trd>&#x7518;&#x679C;&#x30AA;&#x30A6;&#x30C8;&#x30A6;  <pr>&#x3042;&#x307E;&#x304B;&#x30AA;&#x30A6;&#x30C8;&#x30A6;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kriekelaar</trd>,
    <trd>kriekenboom</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>czere&sacute;nia</trd>,
    <trd>trze&sacute;nia</trd>,
    <trd>czere&sacute;nia ptasia</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>ginjeira</trd>,
    <trd>cerejeira</trd>,
    <trd>cerejeira-da-europa</trd>,
    <trd>ginjeira-das-ginjas-galegas</trd>,
    <trd>ginjeiro</trd>    
  </trdgrp>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_je;&c_sh;&c_n;&c_ja;
<klr>(&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&c_ch;&c_je;&c_r;&c_je;&c_sh;&#x43D;&#x44F; (&c_d;&c_je;&c_r;&c_je;&c_v;&#x43E;)</trd>
</drv>
</art>
