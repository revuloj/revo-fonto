<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>meriz</rad>/o
</kap>
<uzo tip="fak">BOT</uzo>
<drv mrk="meriz.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Frukto de <tld/>arbo.
    </dif>
  </snc>
<trd lng="nl">kriek</trd>
</drv>
<drv mrk="meriz.0arbo">
  <kap><tld/>arbo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Sova&gcirc;a formo de la
      <ref tip="dif" cel="cxeriz.0ujo">dol&ccirc;a &ccirc;erizujo</ref>.
    </dif>
  </snc>
  <trd lng="de">Vogelkirsche</trd>
  <trd lng="en">bird-cherry</trd>
  <trd lng="fr">merisier</trd>
  <trdgrp lng="nl">
    <trd>kriekelaar</trd>,
    <trd>kriekenboom</trd>
  </trdgrp>
</drv>
</art>
