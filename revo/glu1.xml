<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: glu1.xml,v 1.7 2020/09/30 12:37:05 revo Exp $">
<kap>
  <rad>glu</rad> <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="glu1.0">
  <kap>glu <tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="glu1.0.SON">
    <dif>
      <ref tip="lst" cel="blek.0o" lst="voko:blekoj">Bleko</ref>
      de <ref tip="vid" cel="meleag.0o.ZOO">meleagro</ref>:
      <ekz>
        la meleagro <klr>[&#8230;]</klr> kriis
        <ctl><tld/>-<tld/>-<tld/>!</ctl>
        <fnt>
          <bib>Fab2</bib>,
          <vrk>Dio de dormo</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">putt-putt!</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>citosl.- hlas krocana</trd>,
    <trd>glo, glo</trd>,
    <trd>zvuk bublaj&iacute;c&iacute; tekutiny</trd>
  </trdgrp>
  <trd lng="en">gobble</trd>
  <trd lng="fr">glou-glou<klr> (dindon)</klr></trd>
  <trd lng="pl">gul gul <klr>(g&lstroke;os indyka)</klr></trd>
  <trdgrp lng="sk">
    <trd><klr>(zvuk bublaj&uacute;cej tekutiny)</klr></trd>,
    <trd>glo</trd>,
    <trd>glo</trd>
  </trdgrp>
</drv>
</art>

