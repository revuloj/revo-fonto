<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mangostan</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="mangostan.0o">
  <kap><tld/>o</kap>
  <snc mrk="mangostan.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;ebla <ref tip="super" cel="frukt.0o.KUI">frukto</ref>
      de <tld/>arbo, kies dika kaj malhela &scirc;elo &scirc;irmas molan,
      diafanan, bongustan karnon.
    </dif>
  </snc>
  <snc mrk="mangostan.0o.BOT">
    <ref tip="dif" cel="mangostan.0arbo.BOT"><tld/>arbo</ref>
      <ekz>
        Lam Son Komunumo <klr>...</klr> estis konata pro siaj
        &gcirc;ardenoj apud la Ngoan Muc montpasejo, kie estas 
        kultivataj diversaj specoj de fruktarboj kiel <tld/>o, 
        rambutano <klr>...</klr>
        <fnt>
          <aut>Alta</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/76813/321545">
          &Gcirc;ardenoj sur la monto-flanko</url></vrk>,
          <lok>2011-04-17</lok>
        </fnt>.
      </ekz>
  </snc>
  <trd lng="en">mangosteen</trd>
  <trd lng="fr">mangoustan</trd>
</drv>

<drv mrk="mangostan.0arbo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="mangostan.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Alta arbo el Sudorientazio, provizanta
      laktosukon kaj fruktojn
      (<trd lng="la">Garcinia mangostana</trd>).
    </dif>
  </snc>
  <trd lng="fr">mangoustanier</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>