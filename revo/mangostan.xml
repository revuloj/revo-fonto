<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mangostan.xml,v 1.9 2024/01/25 20:10:34 revo Exp $">
<kap>
  <rad>mangostan</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="mangostan.0o">
  <kap><tld/>o</kap>
  <snc mrk="mangostan.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;ebla <ref tip="super" cel="frukt.0o.KUI">frukto</ref>
      de <tld/>arbo, kies dika kaj malhela &scirc;elo &scirc;irmas molan,
      diafanan, bongustan karnon:
      <ekz>
        plantoj kun multe da polifenoloj ekzemple estas aroniaj beroj,
        la folioj kaj beroj de ru&gcirc;aj vinberoj, <klr>[&#x2026;]</klr> la &scirc;elo kaj
        fruktokarno de la frukto <tld/>o, la suko de granato
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=212232">Polifenolo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Mangostanfrucht</trd>,
      <trd>Mangostan</trd>
    </trdgrp>
    <trd lng="zh">&#x5C71;&#x7AF9;&#x679C; <pr>sh&#x101;nzh&#xFA;gu&#x1D2;</pr></trd>
</snc>
  <snc mrk="mangostan.0o.BOT">
    <dif>
      <ref tip="dif" cel="mangostan.0arbo.BOT"><tld lit="M"/>arbo</ref>:
      <ekz>
        <nom>Lam Son</nom> Komunumo <klr>[&#x2026;]</klr> estis konata pro siaj
        &gcirc;ardenoj apud la <nom>Ngoan Muc</nom> montpasejo, kie estas
        kultivataj diversaj specoj de fruktarboj kiel <tld/>o,
        rambutano
        <fnt>
          <aut>Alta</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/76813/321545">
          &Gcirc;ardenoj sur la monto-flanko</url></vrk>,
          <lok>2011-04-17</lok>
        </fnt>;
      </ekz>
      <ekz>
        afrotropisa arbo el la kluziacoj estas <tld lit="M"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=21778">Kluziacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Mangostane</trd>
  </snc>
  <trd lng="cs">mangostan</trd>
  <trd lng="en">mangosteen</trd>
  <trd lng="fr">mangoustan</trd>
  <trd lng="id">manggis</trd>
  <trd lng="pl">mangostan</trd>
  <trd lng="sk">garc&iacute;nia mangost&aacute;nov&aacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_a;&c_n;&c_gu;&c_o;&c_s;&c_t;&c_a;&c_n;</trd>,
    <trd>&c_m;&c_a;&c_n;&c_gu;&c_u;&c_s;&c_t;&c_a;&c_n;</trd>
  </trdgrp>
</drv>

<drv mrk="mangostan.0arbo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="mangostan.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Alta <ref tip="super" cel="arb.0o.BOT">arbo</ref> el Sudorientazio, provizanta
      laktosukon kaj fruktojn (<trd lng="la">Garcinia mangostana</trd>).
    </dif>
    <bld lok="&WCU;/thumb/1/1a/Berthe_Hoola_van_Nooten48.jpg/360px-Berthe_Hoola_van_Nooten48.jpg" prm="PD">botanika tabulo de <tld/>o (Garcinia mangostana)
        <fnt>
          <aut>Berthe Hoola van Nooten (1817-1892), Pieter De Pannemaeker (litografio)</aut>
          <lok><url ref="&WCW;/File:Berthe_Hoola_van_Nooten48.jpg">https://commons.wikimedia.org/wiki/File:Berthe_Hoola_van_Nooten48.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="de">
      <trd>Mangostane</trd>,
      <trd>Mangostanbaum</trd>
    </trdgrp>
    <trd lng="zh">&#x5C71;&#x7AF9; <pr>sh&#x101;nzh&#xFA;</pr></trd>
</snc>
  <trd lng="fr">mangoustanier</trd>
  <trdgrp lng="pl">
    <trd>mangostan w&lstroke;a&sacute;ciwy</trd>,
    <trd>garcynia</trd>,
    <trd>smaczelina</trd>,
    <trd>&zdot;&oacute;&lstroke;topla</trd>,
    <trd>&zdot;&oacute;&lstroke;ciecz</trd>
  </trdgrp>
</drv>
</art>

