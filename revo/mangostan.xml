<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mangostan.xml,v 1.7 2022/03/23 14:01:05 revo Exp $">
<kap>
  <rad>mangostan</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="mangostan.0o">
  <kap><tld/>o</kap>
  <snc mrk="mangostan.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;ebla <ref tip="super" cel="frukt.0o.KUI">frukto</ref>
      de <tld/>arbo, kies dika kaj malhela &scirc;elo &scirc;irmas molan,
      diafanan, bongustan karnon:
      <ekz>
        plantoj kun multe da polifenoloj ekzemple estas aroniaj beroj,
        la 
        folioj kaj beroj de ru&gcirc;aj vinberoj, <klr>[&#x2026;]</klr> la &scirc;elo kaj
        fruktokarno de la frukto <tld/>o, la suko de granato,
        <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=212232">Polifenolo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Mangostanfrucht</trd>,
      <trd>Mangostan</trd>
    </trdgrp>
  </snc>
  <snc mrk="mangostan.0o.BOT">
    <dif>
      <ref tip="dif" cel="mangostan.0arbo.BOT"><tld/>arbo</ref>
      <ekz>
        Lam Son Komunumo <klr>...</klr> estis konata pro siaj
        &gcirc;ardenoj apud la Ngoan Muc montpasejo, kie estas 
        kultivataj diversaj specoj de fruktarboj kiel <tld/>o, 
        rambutano <klr>...</klr>
        <fnt>
          <aut>Alta</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/76813/321545">
          &Gcirc;ardenoj sur la monto-flanko</url></vrk>,
          <lok>2011-04-17</lok>
        </fnt>;
      </ekz>
      <ekz>
        afrotropisa arbo el la kluziacoj estas <tld lit="M"/>o (Garcinia
        mangostana)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=21778">Kluziacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Mangostane</trd>
  </snc>
  <trd lng="cs">mangostan</trd>
  <trd lng="en">mangosteen</trd>
  <trd lng="fr">mangoustan</trd>
  <trd lng="id">manggis</trd>
  <trd lng="pl">mangostan</trd>
  <trd lng="sk">garc&iacute;nia mangost&aacute;nov&aacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x430;&#x43D;&#x491;&#x43E;&#x441;&#x442;&#x430;&#x43D;</trd>,
    <trd>&#x43C;&#x430;&#x43D;&#x491;&#x443;&#x441;&#x442;&#x430;&#x43D;</trd>
  </trdgrp>
</drv>

<drv mrk="mangostan.0arbo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="mangostan.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Alta arbo el Sudorientazio, provizanta
      laktosukon kaj fruktojn
      (<trd lng="la">Garcinia mangostana</trd>).
    </dif>
    <trdgrp lng="de">
      <trd>Mangostane</trd>,
      <trd>Mangostanbaum</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">mangoustanier</trd>
  <trdgrp lng="pl">
    <trd>mangostan w&lstroke;a&sacute;ciwy</trd>,
    <trd>garcynia</trd>,
    <trd>smaczelina</trd>,
    <trd>&zdot;&oacute;&lstroke;topla</trd>,
    <trd>&zdot;&oacute;&lstroke;ciecz</trd>
  </trdgrp>
</drv>
</art>

