<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: koagul.xml,v 1.23 2024/08/08 09:28:41 revo Exp $">
<kap>
  <rad>koagul</rad>/i <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="koagul.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="koagul.0i.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Transigi al solida a&ubreve; duonsolida stato;
      malfluidigi, kazeigi.
    </dif>
    <rim>
      En <ctl>Quo vadis</ctl> de Lidja Zamenhof aperas kelkaj ekzemploj
      de netransitiva uzo.
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_t;&c_o;&c_p;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_v;&c_a;&c_r;&c_o;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol; <ind>&c_k;&c_a;&c_a;&c_g;&c_u;&c_l;&c_ja;&c_c;&c_y;&c_ju;</ind></trd>
  </trdgrp>
  <trd lng="ca">coagular</trd>
  <trdgrp lng="cs">
    <trd>koagulovat</trd>,
    <trd>sr&aacute;&zcaron;et</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gerinnen lassen</trd>,
    <trd>verklumpen</trd>
  </trdgrp>
  <trd lng="en"><klr>(cause to) </klr>coagulate</trd>
  <trd lng="es">coagular</trd>
  <trd lng="fr">coaguler<klr tip="ind"> (tr.)</klr></trd>
  <trdgrp lng="hu">
    <trd>megalvaszt</trd>,
    <trd>megdermeszt</trd>,
    <trd>koagul&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x51DD;&#x7D50;&#12373;&#x305B;&#x308B;  <pr>&#x304E;&#12423;&#12358;&#12369;&#12388;&#12373;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x51DD;&#x56FA;&#12373;&#x305B;&#x308B;  <pr>&#x304E;&#12423;&#12358;&#12371;&#12373;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>doen <ind>coaguleren</ind></trd>,
    <trd>doen <ind>stollen</ind></trd>
  </trdgrp>
  <trd lng="pl">koagulowa&cacute;</trd>
  <trdgrp lng="ro">
    <trd>coagula</trd>,
    <trd>&icirc;nchega</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_v;&c_jo;&c_r;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_v;&c_o;&c_r;&c_a;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol; <ind>&c_k;&c_o;&c_a;&c_g;&c_u;&c_l;&c_ja;&c_c;&c_i;&c_ju;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>koagulova&tcaron;</trd>,
    <trd>zr&aacute;&zcaron;a&tcaron; sa</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>p&inodot;ht&inodot;la&scedil;t&inodot;rmak</trd>,
    <trd>dondurmak</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&c_a;&c_g;&c_u;&#x43B;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&#x43F;&c_r;&c_i;&c_ch;&c_i;&#x43D;&#x44F;&c_t;&c_i; &#x43A;&#x43E;&c_a;&c_g;&c_u;&#x43B;&#x44F;&c_c;&c_ib;&#x44E;</trd>,
    <trd>&c_z;&c_v;&c_u;&c_r;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x51DD;&#x7D50; <pr>n&#xED;ngji&#xE9;</pr></trd>,
    <trd>&#x6DF7;&#x51DD; <pr>h&#xF9;nn&#xED;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="koagul.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="koagul.0ajxo.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Loka solidi&gcirc;o de korpa flua&jcirc;o;
      <ref tip="sin" cel="grumel.0o.FIZL">grumelo<sncref/></ref>:
      <ekz>
        kiam mia amiko post amorado kontenti&gcirc;as,
        en la el&jcirc;eta&jcirc;o estas koagula&jcirc;oj
        <fnt>
          <aut>trad. Pa&ubreve;lin'</aut>,
          <vrk><url ref="http://cindymckee.com/librejo/Adoleskantinaj_Demandoj.pdf">
          Adoleskantinaj demandoj</url></vrk>,
          <lok>1999</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_g;&c_o;&c_r;&c_t;&c_a;&c_k;</trd>,
    <trd>&c_z;&c_g;&c_u;&c_s;&c_t;&c_a;&c_k;</trd>,
    <trd>&c_k;&c_a;&c_a;&c_g;&c_u;&c_l;&c_ja;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>caillot</trd>,
    <trd>thrombus</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x51DD;&#x7D50;&#29289;  <pr>&#x304E;&#12423;&#12358;&#12369;&#12388;&#12406;&#12388;</pr></trd>,
    <trd>&#x51DD;&#x56FA;&#29289;  <pr>&#x304E;&#12423;&#12358;&#12371;&#12406;&#12388;</pr></trd>
  </trdgrp>
  <trd lng="pl">skrzep</trd>
  <trdgrp lng="ro">
    <trd>cheag</trd>,
    <trd>craniu</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&c_a;&c_g;&c_u;&#x43B;&#x44F;&c_t;</trd>,
    <trd>&c_z;&c_g;&c_u;&c_s;&c_t;&#x43E;&#x43A;</trd>,
    <trd>&#x43E;&c_s;&c_a;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x51DD;&#x7D50; <pr>n&#xED;ngji&#xE9;</pr></trd>,
    <trd>&#34880;&#26643; <pr>xu&#xE8;shu&amacron;n</pr></trd>,
    <trd>&#34880;&#39173; <pr>xu&#xE8;b&#x1D0;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="koagul.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="koagul.0igxi.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Transi&gcirc;i al solida a&ubreve; duonsolida stato:
      <ekz>
        <ind><tld/>i&gcirc;inta sango</ind>
        <trdgrp lng="fr">
          <trd>sang caill&eacute;</trd>,
          <trd>sang coagul&eacute;</trd>
        </trdgrp>
        <trd lng="nl">gestold <ind>bloed</ind></trd>
        <trd lng="ru">&c_s;&c_v;&c_je;&c_r;&c_n;&c_u;&c_v;&c_sh;&c_a;&c_ja;&c_s;&c_ja; &c_k;&c_r;&c_o;&c_v;&c_mol;</trd>
      </ekz>
    </dif>
    <trd lng="en">coagulate</trd>
    <trdgrp lng="es">
      <trd>coagular<klr tip="ind"> (intr.)</klr></trd>,
      <trd>coagularse</trd>,
    </trdgrp>
    <trdgrp lng="fr">
      <trd>coaguler<klr tip="ind"> (intr.)</klr></trd>,
      <trd>se <ind>coaguler</ind></trd>,
      <trd>figer<klr tip="ind"> (intr.)</klr></trd>,
      <trd>cailler<klr tip="ind"> (former des caillots)</klr></trd>,
      <trd>se <ind>cailler</ind><klr tip="ind"> (former des
      caillots)</klr></trd>,
      <trd>&eacute;paissir<klr tip="ind"> (intr., en p. d'un
      liquide)</klr></trd>,
      <trd>prendre<klr tip="ind"> (devenir solide)</klr></trd>,
    </trdgrp>
    <trdgrp lng="hu">
      <trd>megdermed</trd>,
      <trd>megalvad</trd>,
      <trd>koagul&aacute;l&oacute;dik</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>coaguleren</trd>,
      <trd>stollen</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>g&eogonek;stnie&cacute;</trd>,
      <trd>zastygn&aogonek;&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>se &icirc;ngro&#537;a</trd>,
      <trd>&icirc;nghe&#x21B;a</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_v;&c_jo;&c_r;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;<klr tip="amb"> (&c_o;
      &c_k;&c_r;&c_o;&c_v;&c_i;)</klr></trd>,
      <trd>&c_k;&c_o;&c_a;&c_g;&c_u;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="tr">p&inodot;ht&inodot;la&scedil;mak</trd>
    <trdgrp lng="zh">
    <trd>&#x51DD;&#x7D50; <pr>n&#xED;ngji&#xE9;</pr></trd>,
    <trd>&#34880;&#x51DD; <pr>xu&#xE8;n&#xED;ng</pr></trd>,
    <trd>&#x51DD;&#x805A; <pr>n&#xED;ngj&#xF9;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_g;&c_o;&c_r;&c_t;&c_v;&c_a;&c_c;&c_c;&c_a; <klr tip="amb">(&c_n;&c_a;&c_p;&c_r;. &c_a;&c_b; &c_k;&c_r;&c_y;&c_v;&c_ib;)</klr></trd>,
    <trd>&c_z;&c_g;&c_u;&c_s;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_a;&c_a;&c_g;&c_u;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x51DD;&#x7D50;&#12377;&#x308B;  <pr>&#x304E;&#12423;&#12358;&#12369;&#12388;&#12377;&#x308B;</pr></trd>,
    <trd>&#x51DD;&#x56FA;&#12377;&#x308B;  <pr>&#x304E;&#12423;&#12358;&#12371;&#12377;&#x308B;</pr></trd>,
    <trd>&#x56FA;&#x307E;&#x308B;  <pr>&#x304B;&#x305F;&#x307E;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&c_a;&c_g;&c_u;&#x43B;&#x44E;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&c_z;&c_s;&c_ib;&c_d;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&c_z;&c_v;&c_u;&c_r;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
