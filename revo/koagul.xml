<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>koagul</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="koagul.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">MED</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malfluidigi, kazeigi:
      <ekz>
        <ind><tld/>i&gcirc;inta sango</ind>
        <trd lng="ru">&c_s;&c_v;&c_je;&c_r;&c_n;&c_u;&c_v;&c_sh;&c_a;&c_ja;&c_s;&c_ja; &c_k;&c_r;&c_o;&c_v;&c_mol;</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>gerinnen lassen</trd>,
     <trd>verklumpen</trd>
  </trdgrp>
  <trd lng="en"><klr>(cause to) </klr>coagulate</trd>
  <trdgrp lng="nl">
    <trd>(doen) <ind>coaguleren</ind></trd>,
    <trd>klonters vormen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_v;&c_jo;&c_r;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_v;&c_o;&c_r;&c_a;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol; &c_k;&c_o;&c_a;&c_g;&c_u;&c_l;&c_ja;&c_c;&c_i;&c_ju;</trd>
  </trdgrp>
</drv>
</art>
