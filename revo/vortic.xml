<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>vortic</rad>/o
</kap>
<drv mrk="vortic.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Akvoturni&gcirc;o a&ubreve; ventoturni&gcirc;o.
    </dif>
    <refgrp tip="sin">
      <ref cel="kirl">kirla&jcirc;o</ref>,
      <ref cel="ciklon.0o">ciklono</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ib;&c_r;</trd>,
    <trd>&c_v;&c_ib;&c_h;&c_o;&c_r;</trd>,
    <trd>&c_v;&c_ib;&c_h;&c_u;&c_r;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Wirbel</trd>,
    <trd>Strudel</trd>
  </trdgrp>
  <trd lng="en">vortex</trd>
  <trdgrp lng="es">
    <trd>v&oacute;rtice</trd>,
    <trd>remolino</trd>,
    <trd>torbellino</trd>
  </trdgrp>
  <trd lng="he">&mem;&ayin;&resh;&bet;&vav;&lamed;&tav;</trd>
  <trd lng="hu">&ouml;rv&eacute;ny</trd>
  <trdgrp lng="nl">
    <trd>draaikolk</trd>,
    <trd>wervelwind</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>v&oacute;rtice</trd>,
    <trd>redemoinho</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_d;&c_o;&c_v;&c_o;&c_r;&c_o;&c_t;</trd>,
    <trd>&c_v;&c_i;&c_h;&c_r;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
