<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>vortic</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vortic.0o">
  <kap><tld/>o</kap>
  <snc mrk="vortic.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Akvoturni&gcirc;o a&ubreve; ventoturni&gcirc;o.
    </dif>
    <refgrp tip="vid">
      <ref cel="ciklon.0o.MET">ciklono</ref>,
      <ref cel="kirl">kirla&jcirc;o</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ib;&c_r;</trd>,
    <trd>&c_v;&c_ib;&c_h;&c_o;&c_r;</trd>,
    <trd>&c_v;&c_ib;&c_h;&c_u;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">v&ograve;rtex</trd>
  <trdgrp lng="de">
    <trd>Wirbel</trd>,
    <trd>Strudel</trd>
  </trdgrp>
  <trd lng="en">vortex</trd>
  <trdgrp lng="es">
    <trd>v&oacute;rtice</trd>,
    <trd>remolino</trd>,
    <trd>torbellino</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&f_g;&a_r;&a_d;&a_A;&a_b;</trd>,
    <trd>&f_g;&a_r;&a_d;&a_b;&a_A;&a_d;</trd>
  </trdgrp>
  <trd lng="fr">vortex</trd>
  <trd lng="he">&mem;&ayin;&resh;&bet;&vav;&lamed;&tav;</trd>
  <trd lng="hu">&ouml;rv&eacute;ny</trd>
  <trdgrp lng="nl">
    <trd>draaikolk</trd>,
    <trd>wervelwind</trd>
  </trdgrp>
  <trd lng="pl">wir</trd>
  <trdgrp lng="pt">
    <trd>v&oacute;rtice</trd>,
    <trd>redemoinho</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_d;&c_o;&c_v;&c_o;&c_r;&c_o;&c_t;</trd>,
    <trd>&c_v;&c_i;&c_h;&c_r;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
