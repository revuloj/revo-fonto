<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>vortic</rad>/o
</kap>
<drv mrk="vortic.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Akvoturni&gcirc;o a&ubreve; ventoturni&gcirc;o.
    </dif>
    <refgrp tip="sin">
      <ref cel="kirl">kirla&jcirc;o</ref>,
      <ref cel="ciklon.0o">ciklono</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Wirbel</trd>,
    <trd>Strudel</trd>
  </trdgrp>
  <trd lng="en">vortex</trd>
  <trdgrp lng="es">
    <trd>v&oacute;rtice</trd>,
    <trd>remolino</trd>,
    <trd>torbellino</trd>
  </trdgrp>
  <trd lng="he">&mem;&ayin;&resh;&bet;&vav;&lamed;&tav;</trd>
  <trdgrp lng="nl">
    <trd>draaikolk</trd>,
    <trd>wervelwind</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_d;&c_o;&c_v;&c_o;&c_r;&c_o;&c_t;</trd>,
    <trd>&c_v;&c_i;&c_h;&c_r;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
