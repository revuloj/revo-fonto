<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vortic.xml,v 1.23 2023/10/21 16:50:36 revo Exp $">
<kap>
  <rad>vortic</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vortic.0o">
  <kap><tld/>o</kap>
  <snc mrk="vortic.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Rotacianta regiono de <ref tip="malprt" cel="flu.0ajxo">flua&jcirc;o</ref>;
      <ref tip="malprt" cel="kirl.0o.KOMUNE">kirlo<sncref/></ref>:
      <ekz>
        la nord-pacifika rubo<tld/>o estas la plej granda el la kvin rubocirkuloj
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van de Velde kaj Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2014/011347.php">Konsumantoj agiti&gcirc;as kaj konscii&gcirc;as</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Koreio estis en la <tld/>o de milito
        <fnt>
          <bib>Monato</bib>
          <aut>Ma Young-tae</aut>
          <vrk><url ref="&Monato;/2005/008651.php">Tokto mankas takto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li mem sur sia blanka drako same estis turnita la&ubreve; cirklo, kiel alumeto en grandioza <tld/>o
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>En la Lando de la Noktuloj</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="ciklon.0o.MET">ciklono</ref>
    <refgrp tip="vid">
      <ref cel="turn.0igxi.ruligxi">turni&gcirc;i<sncref/></ref>,
      <ref cel="funel.0o">funelo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ib;&c_r;</trd>,
    <trd>&c_v;&c_ib;&c_h;&c_o;&c_r;</trd>,
    <trd>&c_v;&c_ib;&c_h;&c_u;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">v&ograve;rtex</trd>
  <trd lng="cs">vortex</trd>
  <trdgrp lng="de">
    <trd>Wirbel</trd>,
    <trd>Strudel</trd>
  </trdgrp>
  <trd lng="en">vortex</trd>
  <trdgrp lng="es">
    <trd>v&oacute;rtice</trd>,
    <trd>remolino</trd>,
    <trd>torbellino</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&f_g;&a_r;&a_d;&a_A;&a_b;</trd>,
    <trd>&f_g;&a_r;&a_d;&a_b;&a_A;&a_d;</trd>
  </trdgrp>
  <trd lng="fr">vortex</trd>
  <trd lng="he">&mem;&ayin;&resh;&bet;&vav;&lamed;&tav;</trd>
  <trd lng="hu">&ouml;rv&eacute;ny</trd>
  <trd lng="ja">&#x6E26;  <pr>&#x3046;&#x305A;</pr></trd>
  <trdgrp lng="nl">
    <trd>draaikolk</trd>,
    <trd>wervelwind</trd>
  </trdgrp>
  <trd lng="pl">wir</trd>
  <trdgrp lng="pt">
    <trd>v&oacute;rtice</trd>,
    <trd>redemoinho</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_d;&c_o;&c_v;&c_o;&c_r;&c_o;&c_t;</trd>,
    <trd>&c_v;&c_i;&c_h;&c_r;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kr&uacute;t&ncaron;ava</trd>,
    <trd>v&iacute;r</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x438;&#x440;</trd>,
    <trd>&#x432;&#x438;&#x445;&#x43E;&#x440;</trd>
  </trdgrp>
</drv>
</art>
