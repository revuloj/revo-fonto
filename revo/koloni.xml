<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>koloni</rad>/o
</kap>

<drv mrk="koloni.0o">
  <kap><tld/>o</kap>
  <snc mrk="koloni.0o.setlejo">
    <dif>
      Teritorio, lo&gcirc;ata kaj administrata de elmigrintoj
      en fremda lando a&ubreve; neokupita regiono; pli vaste:
      tereno, sur kiun oni instalis specifan lo&gcirc;antaron:
      <ekz>
        Marsejlo estis greka <tld/>o;
      </ekz>
      <ekz>
        <tld/>o komunista de A&ubreve;straliaj elmigrantoj estas nun
        fondata en Paragvajo
        <fnt><bib>FK</bib><lok>Bagateloj</lok></fnt>;
      </ekz>
      <ekz>
        venis ordono forkonduki &scirc;in en la orientan dezerton,
        kie apartigita de homoj, ekzistis <tld/>o de lepruloj
        <fnt><bib>Far2</bib><lok>&ccirc;ap. 16</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj <klr>[dometoj]</klr> identas, dronante en
        verda&jcirc;o
        de legom&gcirc;ardenoj, &gcirc;ardenetoj kaj florejoj: tio estas
        la laboristaj <tld/>oj
        <fnt><bib>Metrop</bib><lok>&ccirc;ap. 1</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Siedlung</trd>
    <trd lng="fr">colonie<klr tip="ind"> (de peuplement)</klr></trd>
    <trd lng="pl">kolonia</trd>
    <trd lng="pt">col&ocirc;nia</trd>
  </snc>
  <snc mrk="koloni.0o.lando">
    <dif>
      Lando, administrata kaj ekspluatata de &scirc;tato
      ekster siaj landlimoj:
      <ekz>
        la transmaraj <tld/>oj de Germanujo akiris
        sendependecon;
      </ekz>
      <ekz>
        la &ccirc;efa vidinda&jcirc;o de la <tld lit="K"/>a Ekspozicio
        en
        Parizo estis la fama Templo de Angkor
        <fnt><aut>Raymond Schwartz</aut><vrk>La Stranga Butiko</vrk>
        <lok>Angkor' a&ubreve;!</lok></fnt>;
      </ekz>
      <ekz>
        &gcirc;enerale la <ind><tld/>a</ind>j laboristoj estas
        ja pli ekspluatataj ol la laboristoj el la metropolo
        <fnt><bib>Lanti</bib><lok>Denove pri la morto de araba
        SAT-ano</lok></fnt>.
        <trdgrp lng="fr">
          <trd>de <ind>colonie</ind></trd>,
          <trd>colonial</trd>
        </trdgrp>
      </ekz>
    </dif>
    <ref tip="vid" cel="domini.0o.POL">dominio</ref>
    <trd lng="fr">colonie<klr tip="ind"> (territoire
    occup&eacute;)</klr></trd>
    <trd lng="pl">kolonia</trd>
    <trdgrp lng="pt">
      <trd>col&ocirc;nia</trd>,
      <trd>dom&iacute;nio</trd>,    
      <trd>possess&atilde;o</trd>
    </trdgrp>
  </snc>
  <snc mrk="koloni.0o.alilandanaro">
    <dif>
      Aro da samnacianoj lo&gcirc;antaj eksterlande en sama urbo
      a&ubreve; regiono:
      <ekz>
        la germana <tld/>o en Parizo;
      </ekz>
      <ekz>
        la <ind>pola <tld/>o</ind> en Usono.
        <trd lng="pl">polonia</trd>
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>communaut&eacute;<klr tip="ind"> (d'une nation dans une
      autre)</klr></trd>,
      <trd>colonie<klr tip="ind"> (groupe d'immigr&eacute;s de
      m&ecirc;me
      nationalit&eacute;)</klr></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>col&ocirc;nia</trd>,
      <trd>comunidade</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_jo;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_k;&c_o;&c_l;&c_o;&c_n;&c_i;&c_ja;</trd>
  <trd lng="de">Kolonie</trd>
  <trd lng="en">colony</trd>
  <trd lng="nl">kolonie</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_o;&c_n;&c_i;&c_ja;</trd>
</drv>

<drv mrk="koloni.0i">
  <kap><tld/>i<fnt>Z</fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Igi <tld/>o <sncref ref="koloni.0o.setlejo"/>,<sncref
      ref="koloni.0o.lando"/>:
      <ekz>
        Rusoj <tld/>is Siberion;
      </ekz>
      <ekz>
        <ind><tld/>ado</ind>n de la Norda Ameriko
        komencis britoj
        <fnt>
        <bib>Voja&gcirc;o</bib>
        <aut>B. Kolker</aut>
        <vrk>Galope tra la historio de Usono</vrk>
        <lok>leciono 2a</lok>
        </fnt>.
        <trd lng="fr">colonisation</trd>
        <trd lng="pl">kolonizacja</trd>
      </ekz>
    </dif>
  </snc>
  <trd
  lng="be">&c_k;&c_a;&c_l;&c_ja;&c_n;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd
  lng="bg">&c_k;&c_o;&c_l;&c_o;&c_n;&c_i;&c_z;&c_i;&c_r;&c_a;&c_m;</trd>
  <trdgrp lng="de">
    <trd>kolonisieren</trd>,
    <trd>besiedeln</trd>
  </trdgrp>
  <trd lng="en">colonize</trd>
  <trd lng="fr">coloniser</trd>
  <trdgrp lng="nl">
    <trd>koloniseren</trd>,
    <trd>kolonialiseren</trd>
  </trdgrp>
  <trd lng="pl">kolonizowa&cacute;</trd>
  <trd
  lng="ru">&c_k;&c_o;&c_l;&c_o;&c_n;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="koloni.0ano">
  <kap><tld/>ano</kap>
  <snc>
    <dif>
      Ano de <tld/>o:
      <ekz>
        ankora&ubreve; Las Casas predikis kontra&ubreve; la moro
        de fumado, kiun oni heredis de la <tld/>anoj
        <fnt><bib>FK</bib><lok>Bagateloj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">colonist</trd>
  <trdgrp lng="fr">
    <trd>colon</trd>,
    <trd>membre de la communaut&eacute;<klr tip="ind">
    (&eacute;trang&egrave;re)</klr></trd>
  </trdgrp>
  <trd lng="nl">kolonist</trd>
  <trd lng="pl">kolonista</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_l;&c_o;&c_n;&c_i;&c_s;&c_t;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_je;&c_l;&c_je;&c_n;&c_je;&c_c;</trd>
  </trdgrp>
</drv>

<drv mrk="koloni.0igi">
  <kap><tld/>igi</kap>
  <fnt><bib>PV</bib></fnt>
  <snc>
    <ref tip="dif" cel="koloni.0i"><tld/>i</ref>
    <ekz>
      por <tld/>igi la Lunon per poloj oni bezonas multegajn
      kosmoraketojn
      <fnt><bib>SatirRak</bib><lok>Enciclopeditis</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="en">colonize</trd>
  <trdgrp lng="nl">
    <trd>koloniseren</trd>,
    <trd>kolonialiseren</trd>
  </trdgrp>
  <trd lng="pl">kolonizowa&cacute;</trd>
</drv>

<drv mrk="koloni.0ismo">
  <kap><tld/>ismo</kap>
  <uzo tip="fak">EKON</uzo>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Ekonomia sistemo, en kiu la regna prospero
      bazi&gcirc;as sur ekspluatado de &gcirc;iaj
      <tld/>oj <sncref ref="koloni.0o.lando"/>.
    </dif>
  </snc>
  <trd lng="en">colonialism</trd>
  <trd lng="fr">colonialisme</trd>
  <trd lng="nl">kolonialisme</trd>
  <trd lng="pl">kolonializm</trd>
  <trd
  lng="ru">&c_k;&c_o;&c_l;&c_o;&c_n;&c_i;&c_a;&c_l;&c_i;&c_z;&c_m;</trd>
</drv>

<drv mrk="koloni.kontraux0ismo">
  <kap>kontra&ubreve;<tld/>ismo</kap>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Politika movado, kontra&ubreve;a al
      <tld/>ado kaj ekspluatado de
      <tld/>oj <sncref ref="koloni.0o.lando"/>.
    </dif>
  </snc>
  <trd lng="en">anticolonialism</trd>
  <trd lng="fr">anticolonialisme</trd>
  <trd lng="nl">antikolonialisme</trd>
  <trd lng="pl">antykolonializm</trd>
  <trd
  lng="ru">&c_a;&c_n;&c_t;&c_i;&c_k;&c_o;&c_l;&c_o;&c_n;&c_i;&c_a;&c_l;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
