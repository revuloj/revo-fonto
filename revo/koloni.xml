<drv mrk="koloni.0i">
  <kap><tld/>i<fnt>Z</fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="koloni.0i.POL">
    <dif>
      Igi <tld/>on<sncref ref="koloni.0o.setlejo"/>,<sncref ref="koloni.0o.lando"/>:
      <ekz>
        Rusoj <tld/>is Siberion;
      </ekz>
      <ekz>
        <ind><tld/>ado</ind>n de la Norda Ameriko
        komencis britoj
        <fnt>
          <bib>Voja&gcirc;o</bib>
          <aut>B. Kolker</aut>
          <vrk>Galope tra la historio de Usono</vrk>
          <lok>leciono 2a</lok>
        </fnt>;
        <trd lng="fr">colonisation</trd>
        <trd lng="pl">kolonizacja</trd>
      </ekz>
      <ekz>
        Angilon malkovris Kristoforo Kolumbo en 1493, britoj <tld/>is
        &gcirc;in en 1650
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="&Monato;/2014/011219.php">Observanto
          observata</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_ja;&c_n;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_k;&c_o;&c_l;&c_o;&c_n;&c_i;&c_z;&c_i;&c_r;&c_a;&c_m;</trd>
  <trd lng="cs">kolonizovat</trd>
  <trdgrp lng="de">
    <trd>kolonisieren</trd>,
    <trd>besiedeln</trd>
  </trdgrp>
  <trd lng="en">colonize</trd>
  <trd lng="es">colonizar</trd>
  <trd lng="fr">coloniser</trd>
  <trdgrp lng="nl">
    <trd>koloniseren</trd>,
    <trd>kolonialiseren</trd>
  </trdgrp>
  <trd lng="pl">kolonizowa&cacute;</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_o;&c_n;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>kolonizova&tcaron;</trd>,
    <trd>os&iacute;dli&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="koloni.0ano">
  <kap><tld/>ano</kap>
  <snc mrk="koloni.0ano.POL">
    <dif>
      Ano de <tld/>o:
      <ekz>
        ankora&ubreve; Las Casas predikis kontra&ubreve; la moro
        de fumado, kiun oni heredis de la <tld/>anoj
        <fnt><bib>FK</bib>, <lok>Bagateloj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">koloniz&aacute;tor</trd>
  <trd lng="en">colonist</trd>
  <trd lng="es">colono</trd>
  <trdgrp lng="fr">
    <trd>colon</trd>,
    <trd>membre de la communaut&eacute;<klr tip="ind">
    (&eacute;trang&egrave;re)</klr></trd>
  </trdgrp>
  <trd lng="nl">kolonist</trd>
  <trd lng="pl">kolonista</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_l;&c_o;&c_n;&c_i;&c_s;&c_t;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_je;&c_l;&c_je;&c_n;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="sk">koloniz&aacute;tor</trd>
</drv>

<drv mrk="koloni.0igi">
  <kap><tld/>igi</kap>
  <fnt><bib>PV</bib></fnt>
  <snc mrk="koloni.0igi.POL">
    <dif>
      <ref tip="dif" cel="koloni.0i"><tld lit="K"/>i</ref>:
      <ekz>
        por <tld/>igi la Lunon per poloj oni bezonas multegajn
        kosmoraketojn
        <fnt><bib>SatirRak</bib>, <lok>Enciclopeditis</lok></fnt>;
      </ekz>
      <ekz>
        Gerando estis <tld/>igita de bretonoj en la dua duono de la 6a
        jarcento
        <fnt>
          <bib>Monato</bib>
          <aut>Jean-Yves Santerre</aut>
          <vrk><url ref="&Monato;/2009/009817.php">Fortika&jcirc;urbo
          Gerando</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">kolonizovat</trd>
  <trd lng="en">colonize</trd>
  <trdgrp lng="nl">
    <trd>koloniseren</trd>,
    <trd>kolonialiseren</trd>
  </trdgrp>
  <trd lng="pl">kolonizowa&cacute;</trd>
  <trdgrp lng="sk">
    <trd>kolonizova&tcaron;</trd>,
    <trd>os&iacute;dli&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="koloni.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="koloni.0ismo.POL">
    <uzo tip="fak">EKON</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Ekonomia sistemo, en kiu la regna prospero
      bazi&gcirc;as sur ekspluatado de &gcirc;iaj
      <tld/>oj <sncref ref="koloni.0o.lando"/>:
      <ekz>
        la konferenco celis liberigi aziajn kaj afrikajn naciojn de
        imperialismo
        kaj <tld/>ismo, kaj krei ekonomian kaj kulturan kunlaboron inter
        la koncernaj landoj
        <fnt>
          <bib>Monato</bib>
          <aut>Syauqi Ahmad Zulfauzi Stya Lacksana</aut>
          <vrk><url ref="&Monato;/2015/011595.php">Aktivas
          la bonaj amikoj</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        por tiuj, kiuj preferas fermi la okulojn anta&ubreve; la krimoj
        de <tld/>ismo, &ccirc;i tiu verko ne estas rekomendinda
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2014/011108.php">Perfidita
          popolo</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">kolonialismus</trd>
  <trd lng="en">colonialism</trd>
  <trd lng="fr">colonialisme</trd>
  <trd lng="nl">kolonialisme</trd>
  <trd lng="pl">kolonializm</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_o;&c_n;&c_i;&c_a;&c_l;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">kolonializmus</trd>
</drv>

<drv mrk="koloni.eks0o">
  <kap>eks<tld/>o</kap>
  <snc mrk="koloni.eks0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Loko, kiu iam estis <tld/>o de
      fremda regno:
      <ekz>
         plej multaj &scirc;tatoj, aparte en eks<tld/>oj
         kaj la orienta E&ubreve;ropo, estas plurlingvaj
         <fnt>
           <aut>UNDP, trad. R. McCoy</aut>,
           <vrk><url ref="http://www.uea.org/pdf/kultura_libereco.pdf">
           Raporto pri Homa Evoluo 2004</url></vrk>,
           <lok>2005</lok>
         </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">former colony</trd>
  <trd lng="fr">ancienne colonie</trd>
  <trd lng="pl">ekskolonia</trd>
</drv>

<drv mrk="koloni.kontraux0ismo">
  <kap>kontra&ubreve;<tld/>ismo</kap>
  <snc mrk="koloni.kontraux0ismo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Politika movado, kontra&ubreve;a al
      <tld/>ado kaj ekspluatado de
      <tld/>oj <sncref ref="koloni.0o.lando"/>.
    </dif>
  </snc>
  <trd lng="en">anticolonialism</trd>
  <trd lng="fr">anticolonialisme</trd>
  <trd lng="nl">antikolonialisme</trd>
  <trd lng="pl">antykolonializm</trd>
  <trd lng="ru">&c_a;&c_n;&c_t;&c_i;&c_k;&c_o;&c_l;&c_o;&c_n;&c_i;&c_a;&c_l;&c_i;&c_z;&c_m;</trd>
</drv>

<drv mrk="koloni.nov0ismo">
  <kap>nov<tld/>ismo</kap>
  <snc mrk="koloni.nov0ismo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Politika, ekonomia a&ubreve; militforta influado flanke de pli
      ri&ccirc;a 
      regno, kiu ofte estis kolonianto:
      <ekz>
        ili nun disvolvas nov<tld/>ismon en Afriko dividante 
        esperantistojn afrikajn por regi
        <fnt>
          <aut>G. Koffi</aut>,
          <vrk>en: <url ref="http://www.liberafolio.org/2016/12/26/tejo-ekamikas-kun-la-civito/#comment-129">
          TEJO ekamikas kun la Civito</url></vrk>,
          <lok>libera folio, 2016-12-26</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">n&eacute;ocolonialisme</trd>
</drv>
