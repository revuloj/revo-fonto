<drv mrk="koloni.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="koloni.0i.POL">
    <dif>
      Igi <tld/>on<sncref ref="koloni.0o.setlejo"/>,<sncref ref="koloni.0o.lando"/>:
      <ekz>
        Rusoj <tld/>is Siberion;
      </ekz>
      <ekz>
        <ind><tld/>ado</ind>n de la Norda Ameriko
        komencis britoj
        <fnt>
          <bib>Voja&gcirc;o</bib>
          <aut>B. Kolker</aut>
          <vrk>Galope tra la historio de Usono</vrk>
          <lok>leciono 2a</lok>
        </fnt>;
        <trd lng="fr">colonisation</trd>
        <trd lng="pl">kolonizacja</trd>
      </ekz>
      <ekz>
        Angilon malkovris Kristoforo Kolumbo en 1493, britoj <tld/>is
        &gcirc;in en 1650
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="&Monato;/2014/011219.php">Observanto
          observata</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_ja;&c_n;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_k;&c_o;&c_l;&c_o;&c_n;&c_i;&c_z;&c_i;&c_r;&c_a;&c_m;</trd>
  <trd lng="cs">kolonizovat</trd>
  <trdgrp lng="de">
    <trd>kolonisieren</trd>,
    <trd>besiedeln</trd>
  </trdgrp>
  <trd lng="en">colonize</trd>
  <trd lng="es">colonizar</trd>
  <trd lng="fr">coloniser</trd>
  <trdgrp lng="ja">
    <trd>&#x690D;&#x6C11;&#x3059;&#x308B;  <pr>&#x3057;&#x3087;&#x304F;&#x307F;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x690D;&#x6C11;&#x5730;&#x306B;&#x3059;&#x308B;  <pr>&#x3057;&#x3087;&#x304F;&#x307F;&#x3093;&#x3061;&#x306B;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>koloniseren</trd>,
    <trd>kolonialiseren</trd>
  </trdgrp>
  <trd lng="pl">kolonizowa&cacute;</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_o;&c_n;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>kolonizova&tcaron;</trd>,
    <trd>os&iacute;dli&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_s;&c_n;&c_o;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;
    &c_k;&c_o;&c_l;&c_o;&c_n;&c_ib;&c_ju;/&c_k;&c_o;&c_l;&c_o;&c_n;&c_ib;&c_ji;</trd>,
    <trd>&c_s;&c_je;&c_l;&c_i;&c_t;&c_i;&c_s;&c_ja; &c_k;&c_o;&c_l;&c_o;&c_n;&c_ib;&c_ja;&c_m;&c_i;</trd>
  </trdgrp>
