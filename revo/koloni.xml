<drv mrk="koloni.0igi">
  <kap><tld/>igi</kap>
  <fnt><bib>PV</bib></fnt>
  <snc mrk="koloni.0igi.POL">
    <dif>
      <ref tip="dif" cel="koloni.0i"><tld lit="K"/>i</ref>:
      <ekz>
        por <tld/>igi la Lunon per poloj oni bezonas multegajn
        kosmoraketojn
        <fnt><bib>SatirRak</bib>, <lok>Enciclopeditis</lok></fnt>;
      </ekz>
      <ekz>
        Gerando estis <tld/>igita de bretonoj en la dua duono de la 6a
        jarcento
        <fnt>
          <bib>Monato</bib>
          <aut>Jean-Yves Santerre</aut>
          <vrk><url ref="&Monato;/2009/009817.php">Fortika&jcirc;urbo
          Gerando</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_ja;&c_n;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">kolonizovat</trd>
  <trd lng="de">kolonisieren</trd>
  <trd lng="en">colonize</trd>
  <trd lng="ja">&#x690D;&#x6C11;&#x5730;&#x5316;&#x3059;&#x308B;  <pr>&#x3057;&#x3087;&#x304F;&#x307F;&#x3093;&#x3061;&#x304B;&#x3059;&#x308B;</pr></trd>
  <trdgrp lng="nl">
    <trd>koloniseren</trd>,
    <trd>kolonialiseren</trd>
  </trdgrp>
  <trd lng="pl">kolonizowa&cacute;</trd>
