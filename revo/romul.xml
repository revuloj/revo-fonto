<vortaro>
  <art mrk="$Id: romul.xml,v 1.1 2026/01/25 10:36:18 afido Exp $">
    <kap>
      <rad>Romul</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="romul.0o">
  <kap><tld/>o</kap>
  <snc mrk="romul.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Latindevena vira <ref tip="lst" lst="voko:personaj_nomoj" cel="nom.persona0o.GEN">nomo</ref>;
      kaj, la&ubreve; la legendo, unu el la fondintoj de <ref cel="rom.0o">Romo</ref>:
      <ekz>
        la nova guberniestro deklamas kun grandiozaj gestoj pri la gloro de Romo,
        komencante de <tld/>o kaj Remo
        <fnt>
          <bib>MJA</bib>
          <lok>Parto Kvina</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Romulus</trd>
    <trd lng="en">Romulus</trd>
    <trd lng="fr">Romulus</trd>
    <trd lng="ru">&c_R;&c_o;&c_m;&c_u;&c_l;</trd>
  </snc>
</drv>

  </art>

