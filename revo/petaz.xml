<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: petaz.xml,v 1.8 2022/08/15 21:30:14 revo Exp $">
<kap>
  <rad>petaz</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="petaz.0o">
  <kap><tld/>o</kap>
  <snc mrk="petaz.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Duongloba
      <ref tip="super" cel="cxapel.0o.kapvesto">&ccirc;apelo</ref>
      kun granda plata rando &scirc;irma kontra&ubreve; suno a&ubreve; pluvo:
       <ekz>
        la romianoj uzis la <tld/>on por protekti sin
        kontra&ubreve; la suno en la teatro
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Petazo">
          petazo</url>, 2011-11-15</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_p;&c_je;&c_t;&c_a;&c_s;&c_o;&c_s;</trd>
  <trd lng="cs">petasos</trd>
  <trd lng="de">Petasos</trd>
  <trd lng="en">petasos</trd>
  <trd lng="fr">p&eacute;tase</trd>
  <trd lng="nl">petasos</trd>
  <trd lng="pl">petasos</trd>
  <trd lng="ro">petasus</trd>
  <trd lng="sk">petasos</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_je;&c_t;&c_a;&c_s;</trd>,
    <trd>&#x43A;&c_a;&#x43F;&c_je;&#x43B;&#x44E;&c_h; &c_ib;&c_z; &c_sh;&c_i;&c_r;&#x43E;&#x43A;&c_i;&#x43C;&c_i; &#x43A;&c_r;&c_ib;&c_s;&c_a;&#x43C;&c_i;</trd>
  </trdgrp>
</drv>
</art>

