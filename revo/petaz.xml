<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: petaz.xml,v 1.9 2024/02/29 09:22:03 revo Exp $">
<kap>
  <rad>petaz</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="petaz.0o">
  <kap><tld/>o</kap>
  <snc mrk="petaz.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Duongloba
      <ref tip="super" cel="cxapel.0o.kapvesto">&ccirc;apelo</ref>
      kun granda plata rando &scirc;irma kontra&ubreve; suno a&ubreve; pluvo:
       <ekz>
        la romianoj uzis la <tld/>on por protekti sin
        kontra&ubreve; la suno en la teatro
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Petazo">
          petazo</url>, 2011-11-15</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/7/7c/Petasos.jpg/250px-Petasos.jpg"
      prm="PD"
      >antikva greka &ccirc;apelo <tld/>o
        <fnt>
          <aut>Meyers</aut>
          <lok><url ref="&WCW;/File:Petasos.jpg"
              >https://commons.wikimedia.org/wiki/File:Petasos.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="bg">&c_p;&c_je;&c_t;&c_a;&c_s;&c_o;&c_s;</trd>
  <trd lng="cs">petasos</trd>
  <trd lng="de">Petasos</trd>
  <trd lng="en">petasos</trd>
  <trd lng="fr">p&eacute;tase</trd>
  <trd lng="grc">&pi;&epsilon_ton;&tau;&alfa;&sigma;&omikron;&sigma_fina;</trd>
  <trd lng="nl">petasos</trd>
  <trd lng="pl">petasos</trd>
  <trd lng="ro">petasus</trd>
  <trd lng="sk">petasos</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_je;&c_t;&c_a;&c_s;</trd>,
    <trd>&c_k;&c_a;&c_p;&c_je;&c_l;&c_ju;&c_h; &c_ib;&c_z; &c_sh;&c_i;&c_r;&c_o;&c_k;&c_i;&c_m;&c_i; &c_k;&c_r;&c_ib;&c_s;&c_a;&c_m;&c_i;</trd>
  </trdgrp>
</drv>
</art>

