<kap>
  <rad>timid</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="timid.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="timid.0a.PSI">
    <dif>
      <ref tip="dif" cel="auxdac.mal0a">Mala&ubreve;daca</ref>:
      <ekz>
        mi estas <tld/>a kun virinoj
        <fnt>
          <aut>F. Dostojevskij, trad. G. Arosev</aut>,
          <vrk>Blankaj noktoj</vrk>,
          <lok>2002</lok>
        </fnt>;
      </ekz><ekz>
        leporo <klr>[&#x2026;]</klr> kutime estas <tld/>a kaj negregema animalo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=22474">Leporo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="gxen.sin0a.hontema">sin&gcirc;ena<sncref/></ref>
  </snc>
  <trdgrp lng="cs">
    <trd>boj&aacute;cn&yacute;</trd>,
    <trd>trp&iacute;c&iacute; tr&eacute;mou</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sch&uuml;chtern</trd>,
    <trd>versch&auml;mt</trd>,
    <trd>schamhaft</trd>,
    <trd>scheu</trd>
  </trdgrp>
  <trd lng="en">shy</trd>
  <trdgrp lng="fr">
    <trd>timide</trd>,
    <trd>pusillanime</trd>,
    <trd>timor&eacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_o;&c_b;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_je;&c_n;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bojazliv&yacute;</trd>,
    <trd>plach&yacute;</trd>,
    <trd>&lcaron;akav&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&#x435;&#x441;&#x43C;&#x456;&#x43B;&#x438;&#x432;&#x438;&#x439;</trd>,
    <trd>&#x441;&#x43E;&#x440;&#x43E;&#x43C;&#x43B;&#x438;&#x432;&#x438;&#x439;</trd>,
    <trd>&#x441;&#x43E;&#x440;&#x43E;&#x43C;&#x2019;&#x44F;&#x437;&#x43B;&#x438;&#x432;&#x438;&#x439;</trd>,
    <trd>&#x43D;&#x435;&#x440;&#x456;&#x448;&#x443;&#x447;&#x438;&#x439;</trd>
  </trdgrp>
</drv>
</art>

