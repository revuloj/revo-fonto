<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: timid.xml,v 1.10 2024/01/18 21:22:52 revo Exp $">
<kap>
  <rad>timid</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="timid.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="timid.0a.PSI">
    <dif>
      <ref tip="dif" cel="auxdac.mal0a">Mala&ubreve;daca</ref>:
      <ekz>
        mi estas <tld/>a kun virinoj
        <fnt>
          <aut>F. Dostojevskij, trad. G. Arosev</aut>,
          <vrk>Blankaj noktoj</vrk>,
          <lok>2002</lok>
        </fnt>;
      </ekz><ekz>
        leporo <klr>[&#8230;]</klr> kutime estas <tld/>a kaj negregema animalo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=22474">Leporo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="gxen.sin0a.hontema">sin&gcirc;ena<sncref/></ref>
    <refgrp tip="vid">
      <ref cel="ten.sinde0ema">deteni&gcirc;ema</ref>,
      <ref cel="hont.0ema">hontema</ref>,
      <ref cel="tim.0ema">timema</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_s;&c_mol;&c_m;&c_je;&c_l;&c_y;</trd>,
    <trd>&c_b;&c_a;&c_ja;&c_z;&c_mol;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>boj&aacute;cn&yacute;</trd>,
    <trd>trp&iacute;c&iacute; tr&eacute;mou</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sch&uuml;chtern</trd>,
    <trd>versch&auml;mt</trd>,
    <trd>schamhaft</trd>,
    <trd>scheu</trd>
  </trdgrp>
  <trd lng="en">shy</trd>
  <trdgrp lng="fr">
    <trd>timide</trd>,
    <trd>pusillanime</trd>,
    <trd>timor&eacute;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>l&eogonek;kliwy</trd>,
    <trd>nie&sacute;mia&lstroke;y</trd>,
    <trd>trwo&zdot;liwy</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_o;&c_b;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_je;&c_n;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bojazliv&yacute;</trd>,
    <trd>plach&yacute;</trd>,
    <trd>&lcaron;akav&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_je;&c_s;&#x43C;&c_ib;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_s;&#x43E;&c_r;&#x43E;&#x43C;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_s;&#x43E;&c_r;&#x43E;&#x43C;&#8217;&#x44F;&c_z;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&#x43D;&c_je;&c_r;&c_ib;&c_sh;&c_u;&c_ch;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>

