<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lekant.xml,v 1.28 2026/02/22 18:00:21 revo Exp $">
  <kap>
     <ofc>*</ofc>
     <rad>lekant</rad>/o
  <fnt><bib>UV</bib></fnt></kap>
<drv mrk="lekant.0o">
   <kap>
     <ofc>*</ofc><tld/>o
     <fnt><bib>Fab1</bib></fnt>
   </kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
       el la familio <ref tip="malprt" cel="astera.0oj">asteracoj</ref>
       (<trd lng="la">Leucanthemum</trd>),
       kampa vegeta&jcirc;o, kies floraro havas rande blankajn kaj
       meze flavajn korolojn:
      <ekz>
        la bluaj violoj kaj la ru&gcirc;aj amarantoj, la <tld/>oj kaj la
        konvaloj
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        meze de plej densa verda herbo, kreskis <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Lekanto</lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
   <bld
   lok="&WCU;/thumb/3/3b/Leucanthemum_atratum_Wurzeralm.jpg/640px-Leucanthemum_atratum_Wurzeralm.jpg"
   prm="CC-BY-SA-3.0"><tld/>ofloroj (Leucanthemum atratum), Wurzeralm, A&ubreve;strujo
       <fnt>
         <aut>User:Tigerente, propra verko</aut>
         <lok><url
         ref="&WCW;/File:Leucanthemum_atratum_Wurzeralm.jpg">&WCW;/File:Leucanthemum_atratum_Wurzeralm.jpg</url></lok>
       </fnt>
   </bld>
   <trdgrp lng="be">
     <trd>&c_m;&c_a;&c_r;&c_g;&c_a;&c_r;&c_y;&c_t;&c_k;&c_a;</trd>,
     <trd>&c_s;&c_t;&c_a;&c_k;&c_r;&c_o;&c_t;&c_k;&c_a;</trd>
   </trdgrp>
   <trd lng="ca">margarida</trd>
   <trd lng="de">Margerite</trd>
   <trd lng="el">&my;&alfa;&rho;&gamma;&alfa;&rho;&jota_ton;&tau;&alfa;</trd>
   <trd lng="en">daisy</trd>
   <trd lng="es">margarita</trd>
   <trd lng="fa">&a_m;&f_y;&a_n;&a_A;</trd>
   <trd lng="fr">marguerite</trd>
   <trd lng="hu">mezei <ind>margitvir&aacute;g</ind></trd>
   <trdgrp lng="ja">
    <trd>&#12510;&#12540;&#12460;&#12524;&#12483;&#12488;</trd>,
    <trd>&#12514;&#12463;&#12471;&#12517;&#12531;&#12462;&#12463;</trd>
  </trdgrp>
  <trd lng="nl">margriet</trd>
   <trd lng="pl">jastrun</trd>
   <trd lng="pt">margarida</trd>
   <trd lng="ro">margaret&abreve;</trd>
   <trdgrp lng="ru">
     <trd>&c_m;&c_a;&c_r;&c_g;&c_a;&c_r;&c_i;&c_t;&c_k;&c_a;</trd>,
     <trd>&c_n;&c_i;&c_v;&c_ja;&c_n;&c_i;&c_k;</trd>
   </trdgrp>
  <trd lng="uk">&c_k;&c_o;&c_r;&c_o;&c_l;&c_i;&c_c;&c_ja;</trd>
</drv>

<drv mrk="lekant.0eto">
   <kap>
     <tld/>eto
     <fnt><bib>Fab1</bib></fnt>
   </kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
       el la familio <ref tip="malprt" cel="astera.0oj">asteracoj</ref>
       (<trd lng="la">Bellis perennis</trd>), malalta vegeta&jcirc;o kies
       floraro tre similas
       al tiu de <tld/>o, sed estas pli malgranda:
      <ekz>
        la blua kampanulo kaj la malgranda <tld/>eto
        <fnt>
          <bib>Fab4</bib>
          <lok>Lasta son&gcirc;o de maljuna kverko</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;iaj okuloj trafis la lekanteton: vi anka&ubreve; devas &ccirc;i tie
        forvelki, vi kompatinda floreto
        <fnt>
          <bib>Fab1</bib>
          <lok>Lekanto</lok>
        </fnt>!
      </ekz>
      <ekz>
        pro sia malalteco kaj plurjara subtera tigo (rizomo), <tld/>eto
        bone toleras tondadon kaj estas tial iam konsiderata trudherbo
        en
        gazonoj, kvankam &gcirc;i estas anka&ubreve; kulturata pro la ornamaj floroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=211051">Lekanteto</url></lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
   <bld lok="&WCU;/thumb/5/54/Bellis_perennis_01.jpg/640px-Bellis_perennis_01.jpg" prm="CC BY-SA   
   3.0"><tld/>eto (Bellis perennis)
       <fnt>
         <aut>JP Hamon, propra verko</aut>
         <lok><url
         ref="&WCW;/File:Bellis_perennis_01.jpg">&WCW;/File:Bellis_perennis_01.jpg</url></lok>
       </fnt>
   </bld>
   <bld lok="&WCU;/thumb/f/fd/Bellis_perennis5.jpg/640px-Bellis_perennis5.jpg"
   prm="CC-BY-SA-3.0">kulturata varia&jcirc;o de <tld/>eto (Bellis perennis)
       <fnt>
         <aut>Kenpei, propra foto</aut>
         <lok><url ref="&WCW;/File:Bellis_perennis5.jpg">&WCW;/File:Bellis_perennis5.jpg</url></lok>
       </fnt>
   </bld>
   <trd lng="be">&c_s;&c_t;&c_a;&c_k;&c_r;&c_o;&c_t;&c_k;&c_a;
   &c_m;&c_n;&c_o;&c_g;&c_a;&c_l;&c_je;&c_t;&c_n;&c_ja;&c_ja;</trd>
   <trd lng="ca">margaridoia perenne</trd>
   <trdgrp lng="de">
     <trd>G&auml;nsebl&uuml;mchen <klr>(sova&gcirc;a)</klr></trd>,
     <trd>Tausendsch&ouml;n <klr>(kulturita, rozkolora)</klr></trd>
   </trdgrp>
   <trd lng="en">English daisy</trd>
   <trd lng="fa">&a_m;&f_y;&a_n;&a_A;&f_y; &f_ch;&a_m;&a_n;&f_y;</trd>
   <trd lng="fr">p&acirc;querette</trd>
   <trd lng="hu">sz&aacute;zszorsz&eacute;p</trd>
   <trdgrp lng="ja">
    <trd>&#12487;&#12540;&#12472;&#12540;</trd>,
    <trd>&#12498;&#12490;&#12462;&#12463;</trd>
  </trdgrp>
  <trd lng="nl">madeliefje</trd>
   <trd lng="pl">stokrotka</trd>
   <trd lng="ro">b&abreve;nu&#539;</trd>
   <trd lng="ru">&c_m;&c_a;&c_r;&c_g;&c_a;&c_r;&c_i;&c_t;&c_k;&c_a;</trd>
  <trd lng="uk">&c_s;&c_t;&c_o;&c_k;&c_r;&c_o;&c_t;&c_k;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#38607;&#33738; <pr>ch&uacute;j&uacute;</pr></trd>,
    <trd>&#38619;&#33738; <pr>ch&uacute;j&uacute;</pr></trd>,
    <trd>&#27861;&#22269;&#33738; <pr>f&#462;gu&oacute;j&uacute;</pr></trd>,
    <trd>&#27861;&#22283;&#33738; <pr>f&#462;gu&oacute;j&uacute;</pr></trd>,
    <trd>&#31532;&#19968;&#27969; <pr>d&igrave;y&#299;li&uacute;</pr></trd>,
    <trd>&#34224;&#21046;&#30340;&#28779;&#33151;&#32905; <pr>x&#363;nzh&igrave;dehu&#466;tu&#464;r&ograve;u</pr></trd>
  </trdgrp>
</drv>
</art>
