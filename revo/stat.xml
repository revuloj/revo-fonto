<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>stat</rad>/o
    </kap>
    <drv mrk="stat.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="stat.0o.atributa">
        <dif>
          Ies (parolante pri io a&ubreve; iu) maniero esti rilate al ties
          &ccirc;efaj atributoj:
          <ekz>
            la solida, likva, gasa <tld/>o de akvo;
          </ekz><ekz>
            la <tld/>o de la aferoj, de la animo, de la trafiko;
          </ekz><ekz>
            ne forgesu la nunan <tld/>on de Esperanto<fnt>Z</fnt>;
          </ekz><ekz>
            estas dan&gcirc;era da&ubreve;rigi &ccirc;i tiun <tld/>on de l' necerteco<fnt>B</fnt>.
          </ekz> 
        </dif>
        <refgrp tip="vid">
          <ref cel="kondic1.0o">kondi&ccirc;o</ref>,
          <ref cel="fart">farto</ref>
        </refgrp>
        <trd lng="ru">&c_s;&c_o;&c_s;&c_t;&c_o;&c_ja;&c_n;&c_i;&c_je;</trd>
      </snc>
      <snc mrk="stat.0o.rango">
        <uzo tip="fak">HIS</uzo>
        <dif>
          Socipolitika klaso en hierarkia socio:
          <ekz>
            ili reprezentas per si &ccirc;iujn landojn, &ccirc;iujn rangojn,
            <tld/>ojn kaj klasojn de la homoj<fnt>Z</fnt>;
          </ekz><ekz>
            la <ind>Tria <tld/>o</ind>
            <klr>(&ccirc;io krom la nobelaro kaj pastraro en la fe&ubreve;da Francujo)</klr>;
            <trd lng="fr">le <ind>Tiers &Eacute;tat</ind></trd>
            <trd lng="ru">&c_t;&c_r;&c_je;&c_t;&c_mol;&c_je; &c_s;&c_o;&c_s;&c_l;&c_o;&c_v;&c_i;&c_je;</trd>
          </ekz><ekz>
            <ind>&Gcirc;enerala <tld lit="S"/>asembleo</ind>
            <klr>(asembleo de <ctl>&ccirc;iuj tri <tld/>oj</ctl> de la regno)</klr>.
            <trd lng="de">Generalst&auml;nde</trd>
            <trd lng="fr">&Eacute;tats G&eacute;n&eacute;raux</trd>
            <trd lng="ru">&c_g;&c_je;&c_n;&c_je;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_je; &c_sh;&c_t;&c_a;&c_t;&c_y;</trd>
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="kondic1.0o">kondi&ccirc;o</ref>,
          <ref cel="rang.0o">rango</ref>,
          <ref cel="situac.0o.rango">situacio</ref>,
          <ref cel="pozici.0o">pozicio</ref>
        </refgrp>
        <trd lng="ru">&c_s;&c_o;&c_s;&c_l;&c_o;&c_v;&c_i;&c_je;</trd>
      </snc>
      <trd lng="en">condition</trd>
      <trdgrp lng="nl">
        <trd>toestand</trd>,
        <trd>gesteldheid</trd>,
        <trd>staat</trd>,
        <trd>situatie</trd>
      </trdgrp>
      <trdgrp lng="sv">
        <trd>tillst&aring;nd</trd>,
        <trd>skick</trd>
      </trdgrp>
    </drv>
    <drv mrk="stat.0i">
      <kap><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
        <dif>
          Esti en iu <tld/>&c_o;:
          <ekz>
            kiel <tld/>as la aferoj?
          </ekz><ekz>
            la pordo <tld/>is malfermita.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="ru">
        <trd>&c_o;&c_b;&c_s;&c_t;&c_o;&c_ja;&c_t;&c_mol;</trd>,
        <trd>&c_n;&c_a;&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja; <klr>(&c_v; &c_k;&c_a;&c_k;&c_o;&c_m;-&c_l;. &c_s;&c_o;&c_s;&c_t;&c_o;&c_ja;&c_n;&c_i;&c_i;)</klr></trd>
      </trdgrp>
    </drv>
    <drv mrk="stat.bon0o">
      <kap>bon<tld/>o</kap>
      <snc>
        <dif>
          Prospera situacio:
          <ekz>
            kiu fidas Dion, tiu estos bon<tld/>a<fnt>Z</fnt>.
          </ekz> 
        </dif>
      </snc>
      <trd lng="nl">welvaart</trd>
      <trd lng="ru">&c_b;&c_l;&c_a;&c_g;&c_o;&c_s;&c_o;&c_s;&c_t;&c_o;&c_ja;&c_n;&c_i;&c_je;</trd>
    </drv>
    <drv mrk="stat.anim0o">
      <kap>anim<tld/>o</kap>
      <snc>
        <dif>
          <tld/>o de spirito a&ubreve; de koro 
        </dif>
        <refgrp tip="sin">
          <ref cel="humor.0o">humoro</ref>,
          <ref cel="agord.0o">agordo 2</ref>,
          <ref cel="inklin.0o">inklino</ref>.
        </refgrp>
      </snc>
      <trdgrp lng="nl">
        <trd>gemoedsgesteldheid</trd>,
        <trd>stemming</trd>
      </trdgrp>
      <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je; &c_d;&c_u;&c_h;&c_a;</trd>
    </drv>
  </art>
