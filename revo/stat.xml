<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>stat</rad>/o
    </kap>
    <drv mrk="stat.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="stat.0o.atributa">
        <dif>
          Ies (parolante pri io a&ubreve; iu) maniero esti rilate al ties
          &ccirc;efaj atributoj:
          <ekz>
            la <tld/>o de la aferoj<fnt><bib>Far2</bib>,
            <lok>&ccirc;ap. 17</lok></fnt>;
          </ekz>
          <ekz>
             en <tld/>o de frenezo<fnt><bib>Far3</bib>, 
             <lok>&ccirc;ap. 12</lok></fnt>;
          </ekz>
          <ekz>
            forgesante la nunan <tld/>on de 
            Esperanto<fnt><bib>Paroloj</bib>: <vrk>Parolado anta&ubreve;
            la Kvara Kongreso Esperantista en Dresden en la 17a de 
            a&ubreve;gusto 1908</vrk></fnt>;
          </ekz><ekz>
            estas dan&gcirc;era afero da&ubreve;rigi &ccirc;i tiun <tld/>on
            de l' necerteco<fnt><bib>Far3</bib>, <lok>&ccirc;ap.
            16</lok></fnt>;
          </ekz>
          <ekz>
           vivu stomako la&ubreve; <tld/>o de l' sako<fnt><bib>PrV</bib></fnt>;
          </ekz>
          <ekz>
            <uzo tip="fak">FIZ</uzo>
            la <ref cel="solid.0a.FIZ">solida</ref>, 
            <ref cel="likv.0a.FIZ">likva</ref>, 
            <ref cel="gas.0a.FIZ">gasa</ref> <tld/>o de akvo;
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="kondic1.0o">kondi&ccirc;o</ref>,
          <ref cel="kvalit.0o">kvalito</ref>,
          <ref cel="fart">farto</ref>,
          <ref cel="a">-a</ref>
        </refgrp>
        <trd lng="be">&c_s;&c_t;&c_a;&c_n;</trd>
        <trdgrp lng="de">
          <trd>Zustand</trd>,
          <trd>Stand <klr tip="amb">(einer Angelegenheit)</klr></trd>,
          <trd>Sachstand</trd>,
          <trd>Verfassung</trd>
        </trdgrp>
        <trd lng="hu">&aacute;llapot</trd>
        <trd lng="ru">&c_s;&c_o;&c_s;&c_t;&c_o;&c_ja;&c_n;&c_i;&c_je;</trd>
      </snc>
      <snc mrk="stat.0o.rango">
        <uzo tip="fak">HIS</uzo>
        <dif>
          Socipolitika klaso en hierarkia socio:
          <ekz>
            <klr>[ili]</klr> reprezentas per si preska&ubreve; &ccirc;iujn landojn
            <klr>[...]</klr>, &ccirc;iujn naciojn <klr>[...],</klr> rangojn, 
            <tld/>ojn kaj klasojn de la 
            homoj<fnt><bib>Paroloj</bib>: <vrk>Parolado anta&ubreve; 
            la Unua Kongreso Esperantista en Boulogne sur Mer en la 5a
            de 
            a&ubreve;gusto 1905</vrk></fnt>;
          </ekz><ekz>
            la <ind>Tria <tld/>o</ind>
            <klr>(&ccirc;io krom la nobelaro kaj pastraro en la
fe&ubreve;da Francujo)</klr>;
            <trd lng="be">&c_t;&c_r;&c_e;&c_c;&c_ja;&c_je;
&c_s;&c_a;&c_s;&c_l;&c_o;&c_w;&c_je;</trd>
            <trd lng="fr">le <ind>Tiers &Eacute;tat</ind></trd>
            <trd lng="hu">a <ind>harmadik rend</ind></trd>
            <trd lng="ru">&c_t;&c_r;&c_je;&c_t;&c_mol;&c_je;
&c_s;&c_o;&c_s;&c_l;&c_o;&c_v;&c_i;&c_je;</trd>
          </ekz><ekz>
            <ind>&Gcirc;enerala <tld lit="S"/>asembleo</ind>
            <klr>(asembleo de <ctl>&ccirc;iuj tri <tld/>oj</ctl> de la
regno)</klr>.
            <trd lng="de">Generalst&auml;nde</trd>
            <trd lng="fr">&Eacute;tats G&eacute;n&eacute;raux</trd>
            <trd lng="hu">rendi orsz&aacute;ggy&udblac;l&eacute;s</trd>
            <trd
lng="ru">&c_g;&c_je;&c_n;&c_je;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_je;
&c_sh;&c_t;&c_a;&c_t;&c_y;</trd>
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="kondic1.0o">kondi&ccirc;o</ref>,
          <ref cel="rang.0o">rango</ref>,
          <ref cel="situac.0o.rango">situacio</ref>,
          <ref cel="pozici.0o">pozicio</ref>
        </refgrp>
        <trd lng="be">&c_s;&c_a;&c_s;&c_l;&c_o;&c_w;&c_je;</trd>
        <trdgrp lng="de">
          <trd>Stand</trd>,
          <trd><klr>(Gesellschafts-)</klr>Schicht</trd>,
          <trd>Status</trd>
        </trdgrp>
        <trd lng="hu">rend</trd>
        <trd lng="ru">&c_s;&c_o;&c_s;&c_l;&c_o;&c_v;&c_i;&c_je;</trd>
      </snc>
      <trdgrp lng="en">
        <trd>condition</trd>,
        <trd>state</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>toestand</trd>,
        <trd>gesteldheid</trd>,
        <trd>staat</trd>,
        <trd>situatie</trd>
      </trdgrp>
      <trdgrp lng="sv">
        <trd>tillst&aring;nd</trd>,
        <trd>skick</trd>
      </trdgrp>
    </drv>
    <drv mrk="stat.0i">
      <kap><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
        <dif>
          Esti en iu <tld/>&c_o;:
          <ekz>
            scii&gcirc;u pri miaj aferoj, 
            kiel mi <tld/>as<fnt><bib>NT</bib>,
            <lok>Efesanoj 6:20</lok></fnt>;
          </ekz><ekz>
            la pordo <tld/>is malfermita.
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="star.0i.kusxi">stari<sncref/></ref>,
          <ref cel="est.0i.kunParticipo">esti<sncref/></ref>
        </refgrp>
      </snc>
      <trdgrp lng="be">
        <trd>&c_b;&c_y;&c_c;&c_mol; <klr tip="amb">(&c_u;
&c_n;&c_je;&c_j;&c_k;&c_ib;&c_m;
&c_s;&c_t;&c_a;&c_n;&c_je;)</klr></trd>,
        <trd>&c_z;&c_n;&c_a;&c_h;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_c;&c_a;
<klr tip="amb">(&c_u; &c_n;&c_je;&c_j;&c_k;&c_ib;&c_m;
&c_s;&c_t;&c_a;&c_n;&c_je;)</klr></trd>,
        <trd>&c_ib;&c_s;&c_mol;&c_c;&c_ib; <klr
tip="amb">(&c_p;&c_r;&c_a; &c_s;&c_p;&c_r;&c_a;&c_v;&c_y;)</klr></trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>&aacute;ll <klr tip="ind">(dolog)</klr></trd>,
        <trd>van</trd>,
        <trd>tal&aacute;lhat&oacute;</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_o;&c_b;&c_s;&c_t;&c_o;&c_ja;&c_t;&c_mol;</trd>,
        <trd>&c_n;&c_a;&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja;
<klr>(&c_v; &c_k;&c_a;&c_k;&c_o;&c_m;-&c_l;.
&c_s;&c_o;&c_s;&c_t;&c_o;&c_ja;&c_n;&c_i;&c_i;)</klr></trd>
      </trdgrp>
    </drv>
    <drv mrk="stat.bon0o">
      <kap>bon<tld/>o</kap>
      <snc>
        <dif>
          Prospera situacio:
          <ekz>
            laboro donas bonstaton, mallaboro 
            malsaton<fnt><bib>PrV</bib></fnt>;
          </ekz>
          <ekz>
            kiu fidas la Eternulon, tiu estos 
            bonstata<fnt><bib>MT</bib>, <lok>Sentencoj
            28:25</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="hu">j&oacute;l&eacute;t</trd>
      <trd lng="nl">welvaart</trd>
      <trd lng="ru">&c_b;&c_l;&c_a;&c_g;&c_o;&c_s;&c_o;&c_s;&c_t;&c_o;&c_ja;&c_n;&c_i;&c_je;</trd>
    </drv>
    <drv mrk="stat.anim0o">
      <kap>anim<tld/>o</kap>
      <snc>
        <dif>
          <tld lit="S"/>o de spirito a&ubreve; de koro:
          <ekz>
            oni havis la impreson kvaza&ubreve; la tono sonas el ia pre&gcirc;ejo 
            profunde interne en la silenta bonodora arbaro kaj la homoj 
            rigardis en tiu direkto kaj ilia anim<tld/>o fari&gcirc;is 
            solena<fnt><bib>Fab2</bib>, <lok>Sonorilo</lok></fnt>.
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="humor.0o">humoro</ref>,
          <ref cel="agord.0o.humoro">agordo</ref>,
          <ref cel="inklin.0o">inklino</ref>
        </refgrp>
      </snc>
      <trd lng="be">&c_s;&c_t;&c_a;&c_n; &c_d;&c_u;&c_sh;&c_y;</trd>
      <trd lng="en">state of mind</trd>
      <trd lng="hu">lelki&aacute;llapot</trd>
      <trdgrp lng="de">
        <trd>Gem&uuml;tsverfassung</trd>
        <trd>Stimmung</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>gemoedsgesteldheid</trd>,
        <trd>stemming</trd>
      </trdgrp>
      <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;
&c_d;&c_u;&c_h;&c_a;</trd>
    </drv>
  </art>
