<drv mrk="stat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="stat.0o.atributa">
    <dif>
      Momentaj ecoj, maniero de iu afero:
      <ekz>
         en <tld/>o de frenezo
         <fnt><bib>Far3</bib>, <lok>&ccirc;ap. 12a</lok></fnt>;
      </ekz>
      <ekz>
        la infekta&jcirc;o restis en la anta&ubreve;a <tld/>o kaj <klr>[&#x2026;]</klr> ne disvasti&gcirc;is sur la ha&ubreve;to
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 13:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;io estas en bona <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 6:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        mizera <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>&Hcirc;abakuk 3:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        ridado estas la signo de plej alta spirita <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        mizera vila&gcirc;a dometo, kiu estis en tia kaduka <tld/>o,
        ke &gcirc;i mem ne sciis, sur kiun flankon &gcirc;i falu
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        forgesante la nunan <tld/>on de Esperanto
        <fnt><bib>Paroloj</bib>, <vrk>anta&ubreve;
        la Kvara Kongreso Esperantista en Dresden, 1908</vrk></fnt>;
      </ekz><ekz>
        estas dan&gcirc;era afero da&ubreve;rigi &ccirc;i tiun <tld/>on
        de l' necerteco
        <fnt><bib>Far3</bib>, <lok>&ccirc;ap. 16a</lok></fnt>;
      </ekz>
      <ekz>
        vivu stomako la&ubreve; <tld/>o de l'
        sako<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kondic1.0o">kondi&ccirc;o</ref>,
      <ref cel="kvalit.0o">kvalito</ref>,
      <ref cel="fart">farto</ref>,
      <ref cel="a">-a</ref>
    </refgrp>
    <trd lng="be">&c_s;&c_t;&c_a;&c_n;</trd>
    <trdgrp lng="de">
      <trd>Zustand</trd>,
      <trd>Stand <klr tip="amb">(einer Angelegenheit)</klr></trd>,
      <trd>Sachstand</trd>,
      <trd>Verfassung</trd>
    </trdgrp>
    <trd lng="hu">&aacute;llapot</trd>
    <trd lng="ru">&c_s;&c_o;&c_s;&c_t;&c_o;&c_ja;&c_n;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="stat.0o.rango">
    <dif>
      Maniero de ies esto rilate la cirkonstancoj;
      ekz. rilate la socion, t.e. <ref tip="sub" cel="status.0o">statuso</ref>:
      <ekz>
        a&ubreve; plej ri&ccirc;a <tld/>o, a&ubreve; plena malsato
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        bona <tld/>o sa&gcirc;igas, malbona malsa&gcirc;igas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        nova <tld/>o &dash; nova vivo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi estas en tre premita <tld/>o, la Fili&scirc;toj militas kontra&ubreve; mi
        <fnt>
          <bib>MT</bib>
          <lok>I. samuel 28:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fina <tld/>o de tiu homo
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 12:45</lok>
        </fnt>;
      </ekz>
      <ekz>
        li trovi&gcirc;is en <tld/>o duondorma
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum la unua jaro nia <tld/>o estis bona, ni havis servistojn kaj servistinojn
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        bon<tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Ib kaj malgranda kristino</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <klr>[&#x2026;]</klr> &scirc;miris &scirc;ian belan viza&gcirc;on per malbonodora
        &scirc;mira&jcirc;o kaj malordigis &scirc;iajn belajn harojn <klr>[&#x2026;]</klr>
        kiam &scirc;ia patro ekvidis &scirc;in en tia <tld/>o, li eksentis teruron
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> reprezentas per si preska&ubreve; &ccirc;iujn
        landojn <klr>[...]</klr>, &ccirc;iujn naciojn <klr>[...],</klr> rangojn,
        <tld/>ojn kaj klasojn de la homoj
        <fnt>
          <bib>Paroloj</bib>, <vrk>anta&ubreve;
          la Unua Kongreso Esperantista en Boulogne sur Mer, 1905</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kondic1.0o">kondi&ccirc;o</ref>,
      <ref cel="pozici.0o">pozicio</ref>,
      <ref cel="rang.0o">rango</ref>,
      <ref cel="situac.0o.rango">situacio<sncref/></ref>
    </refgrp>
    <trd lng="be">&c_s;&c_a;&c_s;&c_l;&c_o;&c_w;&c_je;</trd>
    <trdgrp lng="de">
      <trd>Stand</trd>,
      <trd><klr>(Gesellschafts-)</klr>Schicht</trd>,
      <trd>Status</trd>
    </trdgrp>
    <trd lng="hu">rend</trd>
    <trd lng="ru">&c_s;&c_o;&c_s;&c_l;&c_o;&c_v;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="stat.0o.FIZ">
    <uzo tip="fak">FIZ</uzo><uzo tip="fak">KEM</uzo>
    <dif>
      <klr>(de <ref cel="materi.0o">materio</ref>)</klr> Fundamenta ekzistoformo
      karakterizita de movebleco de la atomoj kaj molekuloj de iu substanco
      kaj la forto de iliaj interagoj:
      <ekz>
        la <ref cel="solid.0a.FIZ">solida</ref>,
        <ref cel="likv.0a.FIZ">likva</ref>,
        <ref cel="gas.0a.FIZ">gasa</ref> <tld/>o de akvo;
      </ekz>
      <ekz>
        transformi <nom>LNG</nom> de likva (malvarmigita je -161&deg; &gcirc;is -164&deg;C) al gasforma <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2014/011400.php">Nova vojo por natura gaso</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_t;&c_a;&c_n; &c_r;&c_e;&c_ch;&c_y;&c_v;&c_a;</trd>,
      <trd>&c_a;&c_g;&c_r;&c_e;&c_g;&c_a;&c_t;&c_n;&c_y; <ind>&c_s;&c_t;&c_a;&c_n;</ind></trd>
    </trdgrp>
    <trd lng="de">Aggregatzustand</trd>
    <trd lng="en">state of matter</trd>
    <trd lng="fr">&eacute;tat de la mati&egrave;re</trd>
    <trd lng="ru">&c_a;&c_g;&c_r;&c_je;&c_g;&c_a;&c_t;&c_n;&c_o;&c_je; &c_s;&c_o;&c_s;&c_t;&c_o;&c_ja;&c_n;&c_i;&c_je;</trd>
  </snc>

