<drv mrk="vauxd.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Svisa <ref tip="super" cel="kanton.0o">kantono</ref>
