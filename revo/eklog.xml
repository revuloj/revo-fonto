<art mrk="$Id: eklog.xml,v 1.20 2022/03/16 15:54:27 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>eklog</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="eklog.0o">
  <kap><tld/>o</kap>
  <snc mrk="eklog.0o.POE">
    <uzo tip="fak">BELE</uzo>
    <uzo tip="fak">POE</uzo>
    <dif>
      <ref tip="dif" cel="idili.0o.POE">Idilio<sncref/></ref>:
      <ekz>
        orientdevenajn mitojn <klr>...</klr> li poeziigis
        en la kvara <tld/>o 
        <fnt>
          <aut>T. Haecker, trad. V. O&scaron;lak</aut>,
          <vrk>Vergilio, la patro de Okcidento</vrk>,
          <lok>1998</lok>
        </fnt>;
      </ekz>
      <ekz>
        William Auld, Spiro de l ' Pasio: Eklogo pri la sankta afero <klr>[...]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=102935">Kvaropo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_l;&c_o;&c_g;&c_a;</trd>
  <trd lng="ca">&egrave;gloga</trd>
  <trdgrp lng="cs">
    <trd>ekloga</trd>,
    <trd>past&yacute;&rcaron;sk&aacute; p&iacute;se&ncaron;</trd>,
    <trd>selanka</trd>
  </trdgrp>
  <trd lng="de">Ekloge</trd>
  <trd lng="en">eclogue</trd>
  <trd lng="es">&eacute;gloga</trd>
  <trd lng="fr">&eacute;glogue</trd>
  <trdgrp lng="hu">
    <trd>ekloga</trd>,
    <trd>p&aacute;sztork&ouml;ltem&eacute;ny</trd>
  </trdgrp>
  <trd lng="pl">ekloga</trd>
  <trdgrp lng="pt">
    <trd>&eacute;cloga</trd>,
    <trd>pastorela</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_k;&c_l;&c_o;&c_g;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_t;&c_o;&c_r;&c_a;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ekloga</trd>,
    <trd>pastierska piese&ncaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x435;&#x43A;&#x43B;&#x43E;&#x433;&#x430;</trd>,
    <trd>&#x43F;&#x430;&#x441;&#x442;&#x43E;&#x440;&#x430;&#x43B;&#x44C;</trd>
  </trdgrp>
</drv>
</art>
