<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>brav</rad>/a
</kap>
<drv mrk="brav.0a">
   <kap><ofc>*</ofc><tld/>a</kap>
   <snc mrk="brav.0a.kuragxa">
     <dif>
       Lerta kaj preta por kontraŭstari danĝeron;
       <refgrp tip="sin">
         <ref cel="kuragx.0a">kuraĝa</ref>,
         <ref cel="tim.mal0a">maltima</ref>
       </refgrp>, sentima:
       <ekz>
         nia lando venkos, ĉar nia militistaro estas granda kaj <tld/>a
         <fnt><bib>F</bib>, <lok>&FE;34.</lok></fnt>.
       </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>храбры</trd>,
       <trd>бадзёры</trd>,
       <trd>бравы</trd>,
       <trd>адважны</trd>
     </trdgrp>
     <trdgrp lng="de">
       <trd>tapfer</trd>,
       <trd>edel</trd>,
       <trd>wacker</trd>
     </trdgrp>
     <trdgrp lng="hu">
       <trd>vitéz</trd>,
       <trd>bátor</trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>храбрый</trd>,
       <trd>отважный</trd>,
       <trd>бравый</trd>
     </trdgrp>
   </snc>
   <snc mrk="brav.0a.honesta">
     <dif>
       Preta por plenumi sian devon,
       <refgrp tip="sin">
         <ref cel="honest.0a.honorinda">honesta</ref>,
         <ref cel="lojal.0a">lojala</ref>
       </refgrp>:
       <ekz>
         <tld/>a virino estas krono por sia edzo,
         sed senhonora estas kiel puso en liaj ostoj
         <fnt><bib>MT</bib>, <lok>&Sen; 12:4.</lok></fnt>.
       </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>добры</trd>,
       <trd>варты</trd>
     </trdgrp>
     <trdgrp lng="de">
       <trd>brav</trd>,
       <trd>treu</trd>,
       <trd>fidel</trd>,
       <trd>tüchtig</trd>
     </trdgrp>
     <trdgrp lng="hu">
       <trd>derék</trd>,
       <trd>jóravaló</trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>достойный</trd>,
       <trd>добродетельный<klr tip="amb"> (библ.)</klr></trd>,
       <trd>добрый</trd>
     </trdgrp>
   </snc>
   <snc mrk="brav.0a.bonkora">
     <dif>
       Preta por fari komplezon al aliaj; bonkora.
     </dif>
     <rim>
       Mi tre dubas ke <tld/>a posedas tiun sencon en Esperanto.
       La PIV-oj ĝin ne transprenis.
       <aut>Sergio Pokrovskij</aut>
     </rim>
     <trd lng="de">gütig</trd>
     <trd lng="hu">rendes</trd>
   </snc>
   <trdgrp lng="en">
     <trd>worthy</trd>,
     <trd>valiant</trd>,
     <trd>gallant</trd>,
     <trd>brave</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>brave</trd>,
     <trd>courageux</trd>
   </trdgrp>
   <trd lng="sk">odvážny</trd>
</drv>

<drv mrk="brav.0e">
   <kap><tld/>e</kap>
   <snc>
     <dif>
       <tld lit="B"/>amaniere.
     </dif>
     <trdgrp lng="be">
       <trd>брава</trd>,
       <trd>сьмела</trd>,
       <trd>адважна</trd>
     </trdgrp>
     <trdgrp lng="de">
       <trd>tapfer</trd>,
       <trd>edel</trd>,
       <trd>wacker</trd>,
       <trd>brav</trd>,
       <trd>treu</trd>,
       <trd>fidel</trd>,
       <trd>tüchtig</trd>,
       <trd>gütig</trd>
     </trdgrp>
     <trd lng="fr">bravement</trd>
     <trdgrp lng="hu">
       <trd>vitézül</trd>,
       <trd>derekasan</trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>смело</trd>,
       <trd>отважно</trd>
     </trdgrp>
     <trd lng="sk">odvážne</trd>
   </snc>
   <snc>
     <dif>
       Bone! <klr>(por aprobi kaj aplaŭdi)</klr>.
     </dif>
     <trd lng="be">брава!</trd>
     <trd lng="de">bravo</trd>
     <trd lng="en">bravo!</trd>
     <trd lng="fr">bravo!</trd>
     <trd lng="hu">bravó!</trd>
     <trd lng="ru">браво!</trd>
   </snc>
</drv>

<drv mrk="brav.0acxa">
   <kap><tld/>aĉa</kap>
   <snc>
     <dif>
       Elmontranta troan, simulatan <tld/>on:
       <ekz>
         ni ne posedas ilian <tld/>aĉan memfidon
         <fnt><bib>PatrojFiloj</bib> <lok>ĉapitro 10a, p. 57a</lok></fnt>.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="fanfar1.0i">fanfaroni</ref>,
       <ref cel="spit.0ema">spitema</ref>,
       <ref cel="sxajn.0igi">ŝajnigi</ref>
     </refgrp>
   </snc>
   <trdgrp lng="be">
     <trd>зухаваты</trd>,
     <trd>рызыкоўны</trd>,
     <trd>зухвальскі</trd>
   </trdgrp>
   <trd lng="en">impudent</trd>
   <trdgrp lng="fr">
     <trd>bravache</trd>,
     <trd>fanfaron</trd>,
     <trd>présomptueux</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>хвастливый</trd>,
     <trd>бахвальский</trd>,
     <trd>самонадеянный</trd>
   </trdgrp>
</drv>

<drv mrk="brav.0acxo">
   <kap><tld/>aĉo</kap>
   <snc>
     <dif>
       Flegata aspekto de <tld/>o, de spita memfido.
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>рызыканцтва</trd>,
     <trd>зухаватасьць</trd>
   </trdgrp>
   <trd lng="en">impudence</trd>
   <trdgrp lng="fr">
     <trd>crânerie</trd>,
     <trd>présomption</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>фанфаронство</trd>,
     <trd>бахвальство</trd>,
     <trd>хвастливость</trd>,
     <trd>самонадеянность</trd>
   </trdgrp>
</drv>

<drv mrk="brav.0ulo">
   <kap><tld/>ulo</kap>
   <snc>
     <dif>
       Viro <tld/>a:
       <ekz>
         <uzo tip="stl">FRAZ</uzo>
         <ind>kontraŭ muŝoj <tld/>ulo, kontraŭ
          homoj timulo</ind>
         <trd lng="ru">молодец среди овец</trd>
         <fnt><bib>PrV</bib></fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>храбрэц</trd>,
     <trd>маладзец</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>Held</trd>,
     <trd>tapferer <ind>Kerl</ind></trd>,
     <trd>wackerer <ind>Mann</ind></trd>,
     <trd>braver <ind>Mann</ind></trd>,
     <trd>treue <ind>Seele</ind></trd>,
     <trd>fideler <ind>Kamerad</ind></trd>,
     <trd>tüchtiger <ind>Bursche</ind></trd>,
     <trd>gütiger <ind>Mensch</ind></trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>молодец</trd>,
     <trd>храбрец</trd>
   </trdgrp>
</drv>

<drv mrk="brav.0ulino">
   <kap><tld/>ulino</kap>
   <snc>
     <dif>
       Ino <tld/>a:
       <ekz>
         estas sciate en la tuta urbo, ke vi estas <tld/>ulino
         <fnt><bib>MT</bib>, <lok>&Rut; 3:11</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="de">
     <trd>Heldin</trd>,
     <trd>tapferes <ind>Kind</ind></trd>,
     <trd>tapfere <ind>Streiterin</ind></trd>,
     <trd>wackeres <ind>Weib</ind></trd>,
     <trd>braves <ind>Weib</ind></trd>,
     <trd>treue <ind>Seele</ind></trd>,
     <trd>tüchtiges <ind>Mädchen</ind></trd>,
     <trd>gütige <ind>Frau</ind></trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>славная женщина</trd>,
     <trd>молодец<klr tip="amb"> (о женщине)</klr></trd>
   </trdgrp>
</drv>
</art>
