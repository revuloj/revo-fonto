   <rad>brav</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="brav.0a">
   <kap><ofc>*</ofc><tld/>a</kap>
   <snc mrk="brav.0a.kuragxa">
     <dif>
       Lerta kaj preta por kontra&ubreve;stari <ref cel="dangxe.0o">dan&gcirc;ero</ref>n;
       <refgrp tip="vid">
         <ref cel="kuragx.0a">kura&gcirc;a</ref>,
         <ref cel="tim.mal0a">maltima</ref>
       </refgrp>, sentima:
      <ekz>
        <tld/>a heroo
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 6:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj urboj falis, kaj liaj plej <tld/>aj junuloj iris al bu&ccirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 48:15</lok>
        </fnt>;
      </ekz>
       <ekz>
         nia lando venkos, &ccirc;ar nia militistaro estas granda kaj
         <tld/>a
         <fnt><bib>F</bib>, <lok>&FE;34.</lok></fnt>;
       </ekz>
      <ekz>
        <tld/>a batalanto kontra&ubreve; plado bolanta
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a homo en sia domo
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
     </dif>
    <refgrp tip="vid">
      <ref cel="sxance.ne0ebla">ne&scirc;ancelebla</ref>,
      <ref cel="persis.0ema">persistema</ref>
    </refgrp>
     <trdgrp lng="be">
       <trd>&c_h;&c_r;&c_a;&c_b;&c_r;&c_y;</trd>,
       <trd>&c_b;&c_a;&c_d;&c_z;&c_jo;&c_r;&c_y;</trd>,
       <trd>&c_b;&c_r;&c_a;&c_v;&c_y;</trd>,
       <trd>&c_a;&c_d;&c_v;&c_a;&c_zh;&c_n;&c_y;</trd>
     </trdgrp>
     <trdgrp lng="de">
       <trd>tapfer</trd>,
       <trd>edel</trd>,
       <trd>wacker</trd>
     </trdgrp>
     <trdgrp lng="es">
       <trd>bravo</trd>,
       <trd>valeroso</trd>,
       <trd>aguerrido</trd>
     </trdgrp>
     <trdgrp lng="hu">
       <trd>vit&eacute;z</trd>,
       <trd>b&aacute;tor</trd>
     </trdgrp>
     <trdgrp lng="nl">
       <trd>bekwaam</trd>,
       <trd>dapper</trd>,
       <trd>koen</trd>,
       <trd>kranig</trd>,
       <trd>flink</trd>
     </trdgrp>
     <trdgrp lng="pl">
       <trd>dzielny</trd>,
       <trd>m&eogonek;&zdot;ny</trd>,
       <trd>&sacute;mia&lstroke;y</trd>,
       <trd>chwacki</trd>,
       <trd>dziarski</trd>,
       <trd>bitny</trd>,
       <trd>waleczny</trd>
     </trdgrp>
     <trdgrp lng="pt">
       <trd>bravo</trd>,
       <trd>valente</trd>
     </trdgrp>
     <trdgrp lng="ro">
       <trd>viteaz</trd>,
       <trd>&icirc;ndr&abreve;zne&#x21B;</trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>&c_h;&c_r;&c_a;&c_b;&c_r;&c_y;&c_j;</trd>,
       <trd>&c_o;&c_t;&c_v;&c_a;&c_zh;&c_n;&c_y;&c_j;</trd>,
       <trd>&c_b;&c_r;&c_a;&c_v;&c_y;&c_j;</trd>
     </trdgrp>
   </snc>
   <snc mrk="brav.0a.honesta">
     <dif>
       Preta por plenumi sian devon,
       <refgrp tip="vid">
         <ref cel="honest.0a.honorinda">honesta</ref>,
         <ref cel="lojal.0a">lojala</ref>
       </refgrp>:
       <ekz>
         <tld/>a virino estas krono por sia edzo, sed senhonora estas
         kiel puso en liaj ostoj
         <fnt><bib>MT</bib>, <lok>&Sen; 12:4.</lok></fnt>.
       </ekz>
      <ekz>
        vi elser&ccirc;u inter la tuta popolo homojn <tld/>ajn, timantajn Dion, homojn
        veramajn, neprofitamajn
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 18:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a homo, metiisto, svati&gcirc;as al vi
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>.
      </ekz>
     </dif>
      <refgrp tip="vid">
         <ref cel="asidu.0a">asidua</ref>,
         <ref cel="fidel.0a">fidela</ref>,
         <ref cel="fid.0inda">fidinda</ref>,
         <ref cel="solid.0a.fidinda">solida<sncref/></ref>
      </refgrp>
     <trdgrp lng="be">
       <trd>&c_d;&c_o;&c_b;&c_r;&c_y;</trd>,
       <trd>&c_v;&c_a;&c_r;&c_t;&c_y;</trd>
     </trdgrp>
     <trdgrp lng="de">
       <trd>brav</trd>,
       <trd>treu</trd>,
       <trd>fidel</trd>,
       <trd>t&uuml;chtig</trd>
     </trdgrp>
     <trd lng="es">devoto</trd>
     <trdgrp lng="hu">
       <trd>der&eacute;k</trd>,
       <trd>j&oacute;raval&oacute;</trd>
     </trdgrp>
     <trd lng="nl">braaf</trd>
     <trdgrp lng="pl">
       <trd>dzielny</trd>,
       <trd>m&eogonek;&zdot;ny</trd>,
       <trd>dobry</trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>&c_d;&c_o;&c_s;&c_t;&c_o;&c_j;&c_n;&c_y;&c_j;</trd>,
       <trd>&c_d;&c_o;&c_b;&c_r;&c_o;&c_d;&c_je;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;<klr tip="amb"> (&c_b;&c_i;&c_b;&c_l;.)</klr></trd>,
       <trd>&c_d;&c_o;&c_b;&c_r;&c_y;&c_j;</trd>
     </trdgrp>
   </snc>
<!--
   <snc mrk="brav.0a.bonkora">
     <dif>
       Preta por fari komplezon al aliaj; bonkora.
     </dif>
     <rim>
       Mi tre dubas ke <tld/>a posedas tiun sencon en Esperanto.
       La PIV-oj &gcirc;in ne transprenis.
       <aut>Sergio Pokrovskij</aut>
     </rim>
     <trd lng="de">g&uuml;tig</trd>
     <trd lng="hu">rendes</trd>
   </snc>
-->
  <trdgrp lng="cs">
    <trd>chrabr&yacute;</trd>,
    <trd>state&ccaron;n&yacute;</trd>,
    <trd>udatn&yacute;</trd>,
    <trd>&rcaron;&aacute;dn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>worthy</trd>,
     <trd>valiant</trd>,
     <trd>gallant</trd>,
     <trd>brave</trd>
  </trdgrp>
  <trdgrp lng="fr">
     <trd>brave</trd>,
     <trd>courageux</trd>
  </trdgrp>
  <trd lng="he">&nun;&he;&dalet;&resh;</trd>
  <trd lng="io">brava</trd>
  <trdgrp lng="ja">
    <trd>&#x52C7;&#x6562;&#x306A;  [<ind>&#x3086;&#x3046;&#x304B;&#x3093;&#x306A;</ind>]</trd>,
    <trd>&#x52C7;&#x307E;&#x3057;&#x3044;  [<ind>&#x3044;&#x3055;&#x307E;&#x3057;&#x3044;</ind>]</trd>,
    <trd>&#x308A;&#x308A;&#x3057;&#x3044;</trd>,
    <trd>&#x5B9F;&#x76F4;&#x306A;  [<ind>&#x3058;&#x3063;&#x3061;&#x3087;&#x304F;&#x306A;</ind>]</trd>,
    <trd>&#x5F8B;&#x7FA9;&#x306A;  [<ind>&#x308A;&#x3061;&#x304E;&#x306A;</ind>]</trd>,
    <trd>&#x308A;&#x3063;&#x3071;&#x306A;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chrabr&yacute;</trd>,
    <trd>ochotn&yacute;</trd>,
    <trd>odv&aacute;&zcaron;ny</trd>,
    <trd>stato&ccaron;n&yacute;</trd>,
    <trd>udatn&yacute;</trd>,
    <trd>&ccaron;estn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x441;&#x43C;&#x456;&#x43B;&#x438;&#x432;&#x438;&#x439;</trd>,
    <trd>&#x43C;&#x443;&#x436;&#x43D;&#x456;&#x439;</trd>,
    <trd>&#x432;&#x456;&#x434;&#x432;&#x430;&#x436;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="brav.0e">
  <kap><tld/>e</kap>
  <snc mrk="brav.0e.maniere">
    <dif>
      <tld lit="B"/>amaniere:
      <ekz>
        ili <klr>[&#x2026;]</klr> eltiris la tran&ccirc;ilojn kaj <tld/>e komencis la atakon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tablo, la orazeno kaj bastono el sako</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis soldato, servis <tld/>e kaj kura&gcirc;e, sed post la milito li ricevis
        eksi&gcirc;on
        <fnt>
          <bib>ElektFab</bib>
          <lok>Sesope oni trairas la tutan mondon</lok>
        </fnt>;
      </ekz>
      <ekz>
        se nur vi iros <tld/>e anta&ubreve;en, la timigiloj malaperos
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        forte ni staru, <tld/>e laboru
        <fnt>
          <aut>L. L. Zamenhof</aut>
          <vrk>Al la Fratoj</vrk>
        </fnt>;
      </ekz>
      <ekz>
        li tiel <tld/>e ellernis parkere sian predikon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>.
      </ekz>
    </dif>
<!--
     <subsnc mrk="brav.0e.maniere.kuragxa">
       <dif>
         La&ubreve; <ref cel="brav.0a.kuragxa"><tld/>a<sncref/></ref> (kura&gcirc;a).
       </dif>
       <trdgrp lng="nl">
         <trd>bekwaam</trd>,
         <trd>dapper</trd>
       </trdgrp>
     </subsnc>
     <subsnc mrk="brav.0e.maniere.honesta">
       <dif>
         La&ubreve; <ref cel="brav.0a.honesta"><tld/>a<sncref/></ref> (lojala).
       </dif>
       <trdgrp lng="nl">
         <trd>braaf</trd>
       </trdgrp>
     </subsnc>
-->
     <trdgrp lng="be">
       <trd>&c_b;&c_r;&c_a;&c_v;&c_a;</trd>,
       <trd>&c_s;&c_mol;&c_m;&c_je;&c_l;&c_a;</trd>,
       <trd>&c_a;&c_d;&c_v;&c_a;&c_zh;&c_n;&c_a;</trd>
     </trdgrp>
     <trdgrp lng="de">
       <trd>tapfer</trd>,
       <trd>edel</trd>,
       <trd>wacker</trd>,
       <trd>brav</trd>,
       <trd>treu</trd>,
       <trd>fidel</trd>,
       <trd>t&uuml;chtig</trd>,
       <trd>g&uuml;tig</trd>
     </trdgrp>
     <trdgrp lng="es">
       <trd>bravamente</trd>,
       <trd>valerosamente</trd>,
       <trd>aguerridamente</trd>
     </trdgrp>
     <trd lng="fr">bravement</trd>
     <trdgrp lng="hu">
       <trd>vit&eacute;z&uuml;l</trd>,
       <trd>derekasan</trd>
     </trdgrp>
     <trdgrp lng="nl">
       <trd>bekwaam</trd>,
       <trd>dapper</trd>,
       <trd>braaf</trd>
     </trdgrp>
     <trdgrp lng="pl">
       <trd>dzielnie</trd>,
       <trd>m&eogonek;&zdot;nie</trd>,
       <trd>odwa&zdot;nie</trd>,
       <trd>&sacute;mia&lstroke;o</trd>,
       <trd>chwacko</trd>,
       <trd>po <ind>chwacku</ind></trd>,
       <trd>dziarsko</trd>,
       <trd>bitnie</trd>,
       <trd>walecznie</trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>&c_s;&c_m;&c_je;&c_l;&c_o;</trd>,
       <trd>&c_o;&c_t;&c_v;&c_a;&c_zh;&c_n;&c_o;</trd>
     </trdgrp>
     <trd lng="sk">odv&aacute;&zcaron;ne</trd>
   </snc>
   <snc mrk="brav.0e.bone">
     <dif>
      <ref cel="bon.0e">Bone</ref>! <klr>(por aprobi kaj apla&ubreve;di)</klr>:
      <ekz>
        <tld/>e! <tld/>e! &gcirc;i estis bone dirita
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        ha, <tld/>e, mia talp&#x2019;! vi bone fosas
        <fnt>
          <bib>Hamlet</bib>
        </fnt>!
      </ekz>
    </dif>
    <rim>
      Foje oni renkontas anka&ubreve; la formon &leftquot;<tld/>o&rightquot;:
      <ekz>
        <tld/>o! ekkriis <nom>Przyjemski</nom>
        <fnt>
          <bib>IK</bib>
        </fnt>.
      </ekz>
    </rim>
    <trd lng="be">&c_b;&c_r;&c_a;&c_v;&c_a;!</trd>
    <trd lng="de">bravo</trd>
    <trd lng="en">bravo!</trd>
    <trd lng="es">bravo <klr>(expresi&oacute;n de aplauso)</klr></trd>
    <trd lng="fr">bravo!</trd>
    <trd lng="hu">brav&oacute;!</trd>
    <trdgrp lng="nl">
      <trd>bravo</trd>,
      <trd>goed zo</trd>
    </trdgrp>
    <trd lng="pl">brawo!</trd>
    <trd lng="ru">&c_b;&c_r;&c_a;&c_v;&c_o;!</trd>
  </snc>
  <trd lng="cs">state&ccaron;n&ecaron;</trd>
  <trd lng="ja">&#x52C7;&#x6562;&#x306B;  [<ind>&#x3086;&#x3046;&#x304B;&#x3093;&#x306B;</ind>]</trd>
  <trd lng="sk">stato&ccaron;ne</trd>
</drv>

<drv mrk="brav.0acxa">
   <kap><tld/>a&ccirc;a</kap>
   <snc mrk="brav.0acxa.KOMUNE">
     <dif>
       Elmontranta troan, simulatan <tld/>on:
       <ekz>
         ni ne posedas ilian <tld/>a&ccirc;an memfidon
         <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 10a, p.
         57a</lok></fnt>;
       </ekz>
      <ekz>
        la <tld/>a&ccirc;a Faetono fanfaronis pri sia dia deveno
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Faetono">Faetono</url></vrk>
        </fnt>.
      </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="aplomb.0a">aplomba</ref>,
       <ref cel="fanfar1.0i">fanfaroni</ref>,
       <ref cel="spit.0ema">spitema</ref>,
       <ref cel="sxajn.0igi">&scirc;ajnigi</ref>
     </refgrp>
   </snc>
   <trdgrp lng="be">
     <trd>&c_z;&c_u;&c_h;&c_a;&c_v;&c_a;&c_t;&c_y;</trd>,
     <trd>&c_r;&c_y;&c_z;&c_y;&c_k;&c_o;&c_w;&c_n;&c_y;</trd>,
     <trd>&c_z;&c_u;&c_h;&c_v;&c_a;&c_l;&c_mol;&c_s;&c_k;&c_ib;</trd>
   </trdgrp>
   <trd lng="en">impudent</trd>
   <trdgrp lng="fr">
     <trd>bravache</trd>,
     <trd>fanfaron</trd>,
     <trd>pr&eacute;somptueux</trd>
   </trdgrp>
   <trd lng="nl">aanmatigend</trd>
   <trd lng="pl">brawurowy</trd>
   <trdgrp lng="ru">
     <trd>&c_h;&c_v;&c_a;&c_s;&c_t;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>,
     <trd>&c_b;&c_a;&c_h;&c_v;&c_a;&c_l;&c_mol;&c_s;&c_k;&c_i;&c_j;</trd>,
     <trd>&c_s;&c_a;&c_m;&c_o;&c_n;&c_a;&c_d;&c_je;&c_ja;&c_n;&c_n;&c_y;&c_j;</trd>
   </trdgrp>
</drv>

