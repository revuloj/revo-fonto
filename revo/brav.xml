</drv>

<drv mrk="brav.0e">
  <kap><tld/>e</kap>
  <snc mrk="brav.0e.maniere">
    <dif>
      <tld lit="B"/>amaniere:
      <ekz>
        ili <klr>[&#8230;]</klr> eltiris la tran&ccirc;ilojn kaj <tld/>e komencis la atakon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tablo, la orazeno kaj bastono el sako</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis soldato, servis <tld/>e kaj kura&gcirc;e, sed post la milito li ricevis
        eksi&gcirc;on
        <fnt>
          <bib>ElektFab</bib>
          <lok>Sesope oni trairas la tutan mondon</lok>
        </fnt>;
      </ekz>
      <ekz>
        se nur vi iros <tld/>e anta&ubreve;en, la timigiloj malaperos
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        forte ni staru, <tld/>e laboru
        <fnt>
          <aut>L. L. Zamenhof</aut>
          <vrk>Al la Fratoj</vrk>
        </fnt>;
      </ekz>
      <ekz>
        li tiel <tld/>e ellernis parkere sian predikon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>.
      </ekz>
    </dif>
<!--
     <subsnc mrk="brav.0e.maniere.kuragxa">
       <dif>
         La&ubreve; <ref cel="brav.0a.kuragxa"><tld/>a<sncref/></ref> (kura&gcirc;a).
       </dif>
       <trdgrp lng="nl">
         <trd>bekwaam</trd>,
         <trd>dapper</trd>
       </trdgrp>
     </subsnc>
     <subsnc mrk="brav.0e.maniere.honesta">
       <dif>
         La&ubreve; <ref cel="brav.0a.honesta"><tld/>a<sncref/></ref> (lojala).
       </dif>
       <trdgrp lng="nl">
         <trd>braaf</trd>
       </trdgrp>
     </subsnc>
-->
     <trdgrp lng="be">
       <trd>&c_b;&c_r;&c_a;&c_v;&c_a;</trd>,
       <trd>&c_s;&c_mol;&c_m;&c_je;&c_l;&c_a;</trd>,
       <trd>&c_a;&c_d;&c_v;&c_a;&c_zh;&c_n;&c_a;</trd>
     </trdgrp>
     <trdgrp lng="de">
       <trd>tapfer</trd>,
       <trd>edel</trd>,
       <trd>wacker</trd>,
       <trd>brav</trd>,
       <trd>treu</trd>,
       <trd>fidel</trd>,
       <trd>t&uuml;chtig</trd>,
       <trd>g&uuml;tig</trd>
     </trdgrp>
     <trdgrp lng="es">
       <trd>bravamente</trd>,
       <trd>valerosamente</trd>,
       <trd>aguerridamente</trd>
     </trdgrp>
     <trd lng="fr">bravement</trd>
     <trdgrp lng="hu">
       <trd>vit&eacute;z&uuml;l</trd>,
       <trd>derekasan</trd>
     </trdgrp>
     <trdgrp lng="nl">
       <trd>bekwaam</trd>,
       <trd>dapper</trd>,
       <trd>braaf</trd>
     </trdgrp>
     <trdgrp lng="pl">
       <trd>dzielnie</trd>,
       <trd>m&eogonek;&zdot;nie</trd>,
       <trd>odwa&zdot;nie</trd>,
       <trd>&sacute;mia&lstroke;o</trd>,
       <trd>chwacko</trd>,
       <trd>po <ind>chwacku</ind></trd>,
       <trd>dziarsko</trd>,
       <trd>bitnie</trd>,
       <trd>walecznie</trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>&c_s;&c_m;&c_je;&c_l;&c_o;</trd>,
       <trd>&c_o;&c_t;&c_v;&c_a;&c_zh;&c_n;&c_o;</trd>
     </trdgrp>
     <trd lng="sk">odv&aacute;&zcaron;ne</trd>
   </snc>
   <snc mrk="brav.0e.bone">
     <dif>
      <ref cel="bon.0e">Bone</ref>! <klr>(por aprobi kaj apla&ubreve;di)</klr>:
      <ekz>
        <tld/>e! <tld/>e! &gcirc;i estis bone dirita
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        ha, <tld/>e, mia talp&#8217;! vi bone fosas
        <fnt>
          <bib>Hamlet</bib>
        </fnt>!
      </ekz>
    </dif>
    <rim>
      Foje oni renkontas anka&ubreve; la formon &leftquot;<tld/>o&rightquot;:
      <ekz>
        <tld/>o! ekkriis <nom>Przyjemski</nom>
        <fnt>
          <bib>IK</bib>
        </fnt>.
      </ekz>
    </rim>
    <trd lng="be">&c_b;&c_r;&c_a;&c_v;&c_a;!</trd>
    <trd lng="de">bravo</trd>
    <trd lng="en">bravo!</trd>
    <trd lng="es">bravo <klr>(expresi&oacute;n de aplauso)</klr></trd>
    <trd lng="fr">bravo!</trd>
    <trd lng="hu">brav&oacute;!</trd>
    <trdgrp lng="nl">
      <trd>bravo</trd>,
      <trd>goed zo</trd>
    </trdgrp>
    <trd lng="pl">brawo!</trd>
    <trd lng="ru">&c_b;&c_r;&c_a;&c_v;&c_o;!</trd>
  </snc>
  <trd lng="cs">state&ccaron;n&ecaron;</trd>
  <trd lng="ja">&#x52C7;&#25954;&#x306B;  <pr>&#12422;&#12358;&#x304B;&#12435;&#x306B;</pr></trd>
  <trd lng="sk">stato&ccaron;ne</trd>
</drv>

<drv mrk="brav.0acxa">
   <kap><tld/>a&ccirc;a</kap>
   <snc mrk="brav.0acxa.KOMUNE">
     <dif>
       Elmontranta troan, simulatan <tld/>on:
       <ekz>
         ni ne posedas ilian <tld/>a&ccirc;an memfidon
         <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 10a, p.
         57a</lok></fnt>;
       </ekz>
      <ekz>
        la <tld/>a&ccirc;a Faetono fanfaronis pri sia dia deveno
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Faetono">Faetono</url></vrk>
        </fnt>.
      </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="aplomb.0a">aplomba</ref>,
       <ref cel="fanfar1.0i">fanfaroni</ref>,
       <ref cel="spit.0ema">spitema</ref>,
       <ref cel="sxajn.0igi">&scirc;ajnigi</ref>
     </refgrp>
   </snc>
   <trdgrp lng="be">
     <trd>&c_z;&c_u;&c_h;&c_a;&c_v;&c_a;&c_t;&c_y;</trd>,
     <trd>&c_r;&c_y;&c_z;&c_y;&c_k;&c_o;&c_w;&c_n;&c_y;</trd>,
     <trd>&c_z;&c_u;&c_h;&c_v;&c_a;&c_l;&c_mol;&c_s;&c_k;&c_ib;</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>angeberisch</trd>,
     <trd>anma&szlig;end</trd>
   </trdgrp>
   <trd lng="en">impudent</trd>
   <trdgrp lng="fr">
     <trd>bravache</trd>,
     <trd>fanfaron</trd>,
     <trd>pr&eacute;somptueux</trd>
   </trdgrp>
   <trd lng="nl">aanmatigend</trd>
   <trd lng="pl">brawurowy</trd>
   <trdgrp lng="ru">
     <trd>&c_h;&c_v;&c_a;&c_s;&c_t;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>,
     <trd>&c_b;&c_a;&c_h;&c_v;&c_a;&c_l;&c_mol;&c_s;&c_k;&c_i;&c_j;</trd>,
     <trd>&c_s;&c_a;&c_m;&c_o;&c_n;&c_a;&c_d;&c_je;&c_ja;&c_n;&c_n;&c_y;&c_j;</trd>
   </trdgrp>
</drv>

