<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>brav</rad>/a
</kap>
<drv mrk="brav.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc num="1">
    <dif>
      Preta por kontra&ubreve;stari dan&gcirc;eron; sentima, kura&gcirc;a:
      <ekz>kontra&ubreve; mu&scirc;oj <tld/>ulo, kontra&ubreve; homoj
        timulo<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        estas sciate en la tuta urbo, ke vi estas <tld/>ulino
        <fnt><bib>MT</bib>, <lok>&Rut; 3:11</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>vit&eacute;z</trd>,
      <trd>b&aacute;tor</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_m;&c_je;&c_l;&c_y;&c_j;</trd>,
      <trd>&c_h;&c_r;&c_a;&c_b;&c_r;&c_y;&c_j;</trd>,
      <trd>&c_o;&c_t;&c_v;&c_a;&c_zh;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <dif>
      Preta por plenumi sian devon; honesta, senartifika, lojala. 
    </dif>
    <trdgrp lng="hu">
      <trd>der&eacute;k</trd>,
      <trd>j&oacute;raval&oacute;</trd>
    </trdgrp>
  </snc>
  <snc num="3">
    <dif>
      Preta por fari komplezon al aliaj; bonkora. 
    </dif>
    <trd lng="hu">rendes</trd>
  </snc>
  <trdgrp lng="de">
    <trd>tapfer</trd>,
    <trd>edel</trd>,
    <trd>wacker</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>brave</trd>,
    <trd>courageux</trd></trdgrp> 
  <trd lng="sk">odv&aacute;&zcaron;ny</trd>
</drv>
<drv mrk="brav.0e">
  <kap><tld/>e</kap>
  <snc num="1">
    <dif>
      <tld lit="B"/>amaniere. 
    </dif>
    <trd lng="fr">bravement</trd>
    <trdgrp lng="hu">
      <trd>vit&eacute;z&uuml;l</trd>,
      <trd>derekasan</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_m;&c_je;&c_l;&c_o;</trd>,
      <trd>&c_o;&c_t;&c_v;&c_a;&c_zh;&c_n;&c_o;</trd>
    </trdgrp>
    <trd lng="sk">odv&aacute;&zcaron;ne</trd>
  </snc>
  <snc num="2">
    <dif>
      Bone! <klr>(por aprobi k apla&ubreve;di)</klr>.  
    </dif>
    <trd lng="fr">bravo!</trd>
    <trd lng="hu">brav&oacute;!</trd>
    <trd lng="ru">&c_b;&c_r;&c_a;&c_v;&c_o;!</trd>
  </snc>
</drv>
</art>
