<drv mrk="brav.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="brav.0eco.KOMUNE">
    <dif>
      Senheziteco anta&ubreve; dan&gcirc;eroj a&ubreve; malfacilaj
      taskoj:
      <ekz>
        per via <tld/>eco vi gajnis altan rangon inter ni
        <fnt>
          <aut>E. R. Burroughs, trad. K. R. C. Sturmer</aut>,
          <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0029/index.php">
          Princino de Marso</url></vrk>,
          <lok>1938</lok>
        </fnt>;
      </ekz>
      <ekz>
        knaboj same kiel knabinoj, estis edukataj al <tld/>eco, al nobleco kaj al fiero
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Destino de Atrejo</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia trobraveco, kun kiu li superforte venis &ccirc;i tien, pruvas, ke &ccirc;i tiu krimulo
        ne estas Egiptano
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="nl">
      <trd>dapperheid</trd>,
      <trd>koenheid</trd>,
      <trd>moed</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">state&ccaron;nost</trd>
  <trd lng="fr">bravoure</trd>
  <trd lng="he">&alef;&vav;&mem;&ftsadi;</trd>
  <trd lng="sk">stato&ccaron;nos&tcaron;</trd>
</drv>

<drv mrk="brav.0ulo">
   <kap><tld/>ulo</kap>
   <snc mrk="brav.0ulo.KOMUNE">
     <dif>
       <tld lit="B"/>a homo:
      <ekz>
        li intencos veni kun <klr>[&#x2026;]</klr> siaj <tld/>uloj
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 11:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        li faris elekton el &ccirc;iuj plej<tld/>uloj en Izrael
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 10:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen kia <tld/>ulo vi estas! li diris al si mem admirante sian kura&gcirc;on
        <fnt>
          <bib>ElektFab</bib>
          <lok>La kura&gcirc;a tajloreto</lok>
        </fnt>;
      </ekz>
      <ekz>
