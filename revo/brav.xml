<drv mrk="brav.0a">
   <kap><ofc>*</ofc><tld/>a</kap>
   <snc mrk="brav.0a.kuragxa">
     <dif>
       Lerta kaj preta por kontra&ubreve;stari <ref cel="dangxe.0o">dan&gcirc;ero</ref>n;
       <refgrp tip="vid">
         <ref cel="kuragx.0a">kura&gcirc;a</ref>,
         <ref cel="tim.mal0a">maltima</ref>
       </refgrp>, sentima:
      <ekz>
        <tld/>a heroo
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 6:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj urboj falis, kaj liaj plej <tld/>aj junuloj iris al bu&ccirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 48:15</lok>
        </fnt>;
      </ekz>
       <ekz>
         nia lando venkos, &ccirc;ar nia militistaro estas granda kaj
         <tld/>a
         <fnt><bib>F</bib>, <lok>&FE;34.</lok></fnt>;
       </ekz>
      <ekz>
        <tld/>a batalanto kontra&ubreve; plado bolanta
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a homo en sia domo
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
     </dif>
    <refgrp tip="vid">
      <ref cel="sxance.ne0ebla">ne&scirc;ancelebla</ref>,
      <ref cel="persis.0ema">persistema</ref>
    </refgrp>
     <trdgrp lng="be">
       <trd>&c_h;&c_r;&c_a;&c_b;&c_r;&c_y;</trd>,
       <trd>&c_b;&c_a;&c_d;&c_z;&c_jo;&c_r;&c_y;</trd>,
       <trd>&c_b;&c_r;&c_a;&c_v;&c_y;</trd>,
       <trd>&c_a;&c_d;&c_v;&c_a;&c_zh;&c_n;&c_y;</trd>
     </trdgrp>
     <trdgrp lng="de">
       <trd>tapfer</trd>,
       <trd>edel</trd>,
       <trd>wacker</trd>
     </trdgrp>
     <trdgrp lng="es">
       <trd>bravo</trd>,
       <trd>valeroso</trd>,
       <trd>aguerrido</trd>
     </trdgrp>
     <trdgrp lng="hu">
       <trd>vit&eacute;z</trd>,
       <trd>b&aacute;tor</trd>
     </trdgrp>
     <trdgrp lng="nl">
       <trd>bekwaam</trd>,
       <trd>dapper</trd>,
       <trd>koen</trd>,
       <trd>kranig</trd>,
       <trd>flink</trd>
     </trdgrp>
     <trdgrp lng="pl">
       <trd>dzielny</trd>,
       <trd>m&eogonek;&zdot;ny</trd>,
       <trd>&sacute;mia&lstroke;y</trd>,
       <trd>chwacki</trd>,
       <trd>dziarski</trd>,
       <trd>bitny</trd>,
       <trd>waleczny</trd>
     </trdgrp>
     <trdgrp lng="pt">
       <trd>bravo</trd>,
       <trd>valente</trd>
     </trdgrp>
     <trdgrp lng="ro">
       <trd>viteaz</trd>,
       <trd>&icirc;ndr&abreve;zne&#x21B;</trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>&c_h;&c_r;&c_a;&c_b;&c_r;&c_y;&c_j;</trd>,
       <trd>&c_o;&c_t;&c_v;&c_a;&c_zh;&c_n;&c_y;&c_j;</trd>,
       <trd>&c_b;&c_r;&c_a;&c_v;&c_y;&c_j;</trd>
     </trdgrp>
     <trdgrp lng="zh">
    <trd>&#x5F37;&#x608D; <pr>qi&#xE1;ngh&#xE0;n</pr></trd>,
    <trd>&#x98AF;&#x723D; <pr>s&#xE0;shu&#x1CE;ng</pr></trd>,
    <trd>&#x795E;&#x52C7; <pr>sh&#xE9;ny&#x1D2;ng</pr></trd>,
    <trd>&#x648A; <pr>xi&#xE0;n</pr></trd>,
    <trd>&#x96C4;&#x8D73;&#x8D73; <pr>xi&#xF3;ngji&#x16B;ji&#x16B;</pr></trd>,
    <trd>&#x52C7;&#25954;&#22320; <pr>y&#x1D2;ngg&#x1CE;nde</pr></trd>,
    <trd>&#x52C7;&#x6B66; <pr>y&#x1D2;ngw&#x1D4;</pr></trd>
  </trdgrp>
</snc>
   <snc mrk="brav.0a.honesta">
     <dif>
       Preta por plenumi sian devon,
       <refgrp tip="vid">
         <ref cel="honest.0a.honorinda">honesta</ref>,
         <ref cel="lojal.0a">lojala</ref>
       </refgrp>:
       <ekz>
         <tld/>a virino estas krono por sia edzo, sed senhonora estas
         kiel puso en liaj ostoj
         <fnt><bib>MT</bib>, <lok>&Sen; 12:4.</lok></fnt>.
       </ekz>
      <ekz>
        vi elser&ccirc;u inter la tuta popolo homojn <tld/>ajn, timantajn Dion, homojn
        veramajn, neprofitamajn
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 18:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a homo, metiisto, svati&gcirc;as al vi
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>.
      </ekz>
     </dif>
      <refgrp tip="vid">
         <ref cel="asidu.0a">asidua</ref>,
         <ref cel="fidel.0a">fidela</ref>,
         <ref cel="fid.0inda">fidinda</ref>,
         <ref cel="solid.0a.fidinda">solida<sncref/></ref>
      </refgrp>
     <trdgrp lng="be">
       <trd>&c_d;&c_o;&c_b;&c_r;&c_y;</trd>,
       <trd>&c_v;&c_a;&c_r;&c_t;&c_y;</trd>
     </trdgrp>
     <trdgrp lng="de">
       <trd>brav</trd>,
       <trd>treu</trd>,
       <trd>fidel</trd>,
       <trd>t&uuml;chtig</trd>
     </trdgrp>
     <trd lng="es">devoto</trd>
     <trdgrp lng="hu">
       <trd>der&eacute;k</trd>,
       <trd>j&oacute;raval&oacute;</trd>
     </trdgrp>
     <trd lng="nl">braaf</trd>
     <trdgrp lng="pl">
       <trd>dzielny</trd>,
       <trd>m&eogonek;&zdot;ny</trd>,
       <trd>dobry</trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>&c_d;&c_o;&c_s;&c_t;&c_o;&c_j;&c_n;&c_y;&c_j;</trd>,
       <trd>&c_d;&c_o;&c_b;&c_r;&c_o;&c_d;&c_je;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;<klr tip="amb"> (&c_b;&c_i;&c_b;&c_l;.)</klr></trd>,
       <trd>&c_d;&c_o;&c_b;&c_r;&c_y;&c_j;</trd>
     </trdgrp>
     <trd lng="zh">&#x826F;&#21892; <pr>li&#xE1;ngsh&#xE0;n</pr></trd>
</snc>
