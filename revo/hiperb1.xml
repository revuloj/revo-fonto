<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hiperboloid</rad>/o
</kap>
<drv mrk="hiperb1.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 36</lok></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="kvadri1.0o">Kvadriko</ref>, kiu havas
      centran <ref cel="simetr.0eco">simetriecon</ref> kaj sekcas iujn
      ebenojn la&ubreve; <ref cel="hiperb.0o.linio">hiperboloj</ref>:
      <ekz>
        rotacia a&ubreve; rivolua <tld/>o;
      </ekz>
      <ekz>
        unupeca, dupeca <tld/>o;
      </ekz>
      <ekz>
        unupeca <tld/>o estas <ref cel="rekt.0ara.surfaco">rektara</ref>
        surfaco.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Hyperboloid</trd>
  <trd lng="es">hiperboloide</trd>
  <trd lng="en">hyperboloid</trd>
  <trd lng="fr">hyperbolo&iuml;de</trd>
  <trd lng="nl">hyperbolo&iuml;de</trd>
  <trd lng="pl">hiperboloida</trd>
  <trd lng="pt">hiperb&oacute;loide

</trd>
  <trd lng="ru">&c_g;&c_i;&c_p;&c_je;&c_r;&c_b;&c_o;&c_l;&c_o;&c_i;&c_d;</trd>
</drv>
</art>
