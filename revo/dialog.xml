<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
<ofc>1</ofc>
      <rad>dialog</rad>/o
    </kap>
    <drv mrk="dialog.0o">
      <kap><tld/>o</kap>
      <snc mrk="dialog.0o.interparolo">
        <dif>
          Interparolado.
        </dif>
      </snc>
      <snc mrk="dialog.0o.verko">
        <dif>
          Literatura verko en formo de interparolado.
        </dif>
      </snc>
      <snc mrk="dialog.0o.KOMP">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Re&gcirc;imo de laboro kun komputa sistemo kiam la uzanto regas la
          programplenumon per siaj komandoj dum la rultempo.
        </dif>
        <rim>
          <ctl><tld lit="D"/>a re&gcirc;imo</ctl> kutime implicas
          inter&scirc;an&gcirc;on de tekstaj komandoj a&ubreve; demandoj kaj
          respondoj a&ubreve; <ref tip="prt" cel="invit.0o.KOMP">invitoj</ref>;
          en <ctl>interaga re&gcirc;imo</ctl>
          eblas uzi pli evoluintajn programajn a&ubreve; aparatajn
          rimedojn.
        </rim>
        <ref tip="prt" cel="seanc.0o">seanco</ref>
        <ref tip="ant" cel="fon.0a.regximo">fona re&gcirc;imo</ref>
      </snc>
      <trd lng="en">dialog</trd>
      <trd lng="ru">&c_d;&c_i;&c_a;&c_l;&c_o;&c_g;</trd>
    </drv>
    <drv mrk="dialog.0i">
      <kap><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
        <dif>
          Interparoli.
        </dif>
      </snc>
      <trd lng="ru">&c_v;&c_je;&c_s;&c_t;&c_i; <ind>&c_d;&c_i;&c_a;&c_l;&c_o;&c_g;</ind></trd>
    </drv>
  </art>

