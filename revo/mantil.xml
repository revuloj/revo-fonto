<art mrk="$Id: mantil.xml,v 1.20 2023/09/02 06:04:09 revo Exp $">
<kap>
  <rad>mantil</rad>/o <fnt><bib>PV</bib></fnt>
  <fnt>Z</fnt>
</kap>
<drv mrk="mantil.0o">
  <kap><tld/>o</kap>
  <snc mrk="mantil.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="vest.super0o">Surpervesto</ref>
      el lar&gcirc;a punta tuko kovranta kapon kaj &scirc;ultrojn,
      populara &ccirc;e hispaninoj &gcirc;is en la 19a jarcento:
      <ekz>
        Mario envenas <klr>...</klr> Ludovikino demetas
        al &scirc;i la <tld/>on
        <fnt>
          <aut>J. Benavente, trad. V. Inglada Ors</aut>,
          <vrk>La Nesia Hejmo</vrk>,
          <lok><url ref="http://www.gazetejo.org/eo/lasunohispana">
          La Suno Hispana</url>, 1909-01 (61)</lok>
        </fnt>.        
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_t;&c_y;&c_l;&c_mol;&c_ja;</trd>
  <trd lng="ca">mantellina</trd>
  <trd lng="cs">pl&aacute;&scaron;t&ecaron;nka</trd>
  <trdgrp lng="de">
    <trd>Mantilla</trd>,
    <trd>Mantille</trd>
  </trdgrp>
  <trd lng="en">mantilla</trd>
  <trd lng="es">mantilla</trd>
  <trdgrp lng="fr">
    <trd>mantille</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>mantilla</trd>,
    <trd>csipkekend&odblac;</trd>
  </trdgrp>
  <trd lng="ja">&#x30DE;&#x30F3;&#x30C6;&#x30A3;&#x30FC;&#x30E9;</trd>
  <trd lng="nl">mantilla</trd>
  <trd lng="pl">mantyla</trd>
  <trd lng="pt">mantilha</trd>
  <trd lng="ro">&#x219;al</trd>
  <trd lng="ru">&c_m;&c_a;&c_n;&c_t;&c_i;&c_l;&c_mol;&c_ja;</trd>
  <trd lng="sk">z&aacute;vojov&yacute; &scaron;&aacute;l</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x430;&#x43D;&#x442;&#x438;&#x43B;&#x44C;&#x44F;</trd>,
    <trd>&#x433;&#x43E;&#x43B;&#x43E;&#x432;&#x43D;&#x438;&#x439; &#x43C;&#x435;&#x440;&#x435;&#x436;&#x438;&#x432;&#x43D;&#x438;&#x439; &#x448;&#x430;&#x440;&#x444;</trd>
  </trdgrp>
</drv>
</art>
