<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mantil</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="mantil.0o">
  <kap><tld/>o</kap>
  <snc mrk="mantil.0o.TEKS">
  <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="vest.surper0o">Surpervesto</ref>
      el lar&gcirc;a punta tuko kovranta kapon kaj &scirc;ultrojn,
      komuna interalie &ccirc;e hispaninoj &gcirc;is en la 19a jarcento.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_t;&c_y;&c_l;&c_mol;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Mantilla</trd>,
    <trd>Mantille</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>mantille</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>mantilla</trd>,
    <trd>csipkekend&odblac;</trd>
  </trdgrp>
  <trd lng="nl">mantilla</trd>
  <trd lng="ru">&c_m;&c_a;&c_n;&c_t;&c_i;&c_l;&c_mol;&c_ja;</trd>
</drv>
</art>
