<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>orangutang</rad>/o <fnt>Z</fnt></kap>
<drv mrk="orangu.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">ZOO</uzo>
   <fnt><bib>ZR</bib></fnt>
   <snc>
     <dif>
       Genro el familio <ref tip="super" cel="orangu1.0oj"><tld/>edoj</ref>
       (<trd lng="la">Pongo</trd>), de grandaj simioj kun ru&gcirc;eta felo,
       vivantaj sur la insuloj Sumatro kaj Borneo.
     </dif>
   </snc>
   <trd lng="be">&c_a;&c_r;&c_a;&c_n;&c_g;&c_u;&c_t;&c_a;&c_n;&c_g;</trd>
   <trd lng="de">Orang-Utan</trd>
   <trd 
lng="el">&omikron;&ypsilon;&rho;&alfa;&gamma;&kappa;&omikron;&ypsilon;&tau;&alfa_ton;&gamma;&kappa;&omikron;&sigma_fina;</trd>
   <trd lng="hu">orangut&aacute;n</trd>
   <trd lng="nl">orang-utang</trd>
   <trd lng="pt">orangotango</trd>
   <trd lng="fr">orang-outang</trd>
   <trd lng="ru">&c_o;&c_r;&c_a;&c_n;&c_g;&c_u;&c_t;&c_a;&c_n;</trd>
</drv>
</art>
