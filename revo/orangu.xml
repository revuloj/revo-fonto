<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>orangutang</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="orangu.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="orangu.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Pongo</trd>), de grandaj simioj kun ru&gcirc;eta felo,
      vivantaj sur la insuloj Sumatro kaj Borneo.
    </dif>
  </snc>
  <trd lng="de">Orang-Utan</trd>
  <trd lng="nl">orang-utang</trd>
</drv>
<drv mrk="orangu.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="simi.0oj">simioj</ref>
      (<trd lng="la">Pongidae</trd>), plej similaj al la homo.
    </dif>
  </snc>
  <trd lng="de">Menschenaffen</trd>
  <trd lng="nl">mensapen</trd>
</drv>
</art>

