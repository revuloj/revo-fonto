<drv mrk="nederl.N0o">
  <kap><ofc>8</ofc><tld/>o
    <fnt><aut>Akademio de Esperanto</aut>,
<vrk><url ref="&AdE;/aktoj/aktoj2/8oa.html"> 8a
Oficiala Aldono</url></vrk>, <lok>II.C.1</lok></fnt>
    <fnt><aut>Akademio de Esperanto</aut>,
<vrk><url ref="&AdE;/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>
    <fnt><bib>Viki</bib>, <lok><url ref="&Viki;/Nederlando">Nederlando</url></lok></fnt>
  </kap>
  <mlg kod="ISO-3166">NL</mlg>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o">&Scirc;tato</ref> situanta en okcidenta
      <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref> kaj limanta
      okcidente kaj norde al la Norda Maro, oriente al <ref tip="vid" cel="german1.G0ujo">Germanujo</ref> kaj sude al <ref tip="vid" cel="belg.B0ujo">Belgujo</ref> kun &ccirc;efurbo <ref tip="prt" cel="amsterdam.A0o">Amsterdamo</ref>:
      <ekz>
        ekde 1830 Belgio kaj <tld/>o estas apartaj najbaraj &scirc;tatoj
        <fnt>
          <aut>Johan Van Eenoo</aut>:
          <vrk>La bru&gcirc;a Centuriae plantarum rariorum retrovita en
          Krakovo</vrk>,
          <bib>Monato</bib>
          <lok>jaro 1998a, numero 1a, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_N;&c_ib;&c_d;&c_e;&c_r;&c_l;&c_ja;&c_n;&c_d;&c_y;</trd>,
    <trd>&c_G;&c_a;&c_l;&c_ja;&c_n;&c_d;&c_y;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_N;&c_i;&c_d;&c_je;&c_r;&c_l;&c_a;&c_n;&c_d;&c_i;&c_ja;</trd>,
    <trd>&c_H;&c_o;&c_l;&c_a;&c_n;&c_d;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>Izelvroio&ugrave;</trd>,
    <trd>Nederland</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>Nizozemsko</trd>,
    <trd>Nizozem&iacute;</trd>
  </trdgrp>
  <trd lng="de">Niederlande</trd>
  <trd lng="el">&Omikron;&lambda;&lambda;&alfa;&ny;&delta;&jota_ton;&alfa;</trd>
  <trdgrp lng="en">
      <trd>The <ind>Netherlands</ind></trd>
      <trd>Holland</trd>
  </trdgrp>
  <trd lng="es">Pa&iacute;ses Bajos</trd>
  <trdgrp lng="fr">
    <trd>Pays-Bas</trd>,
    <trd>Royaume des Pays-Bas</trd>
  </trdgrp>
  <trd lng="fy">Nederl&acirc;n</trd>
  <trd lng="hu">Hollandia</trd>
  <trd lng="id">Belanda</trd>
  <trd lng="io">Nederlando</trd>
  <trd lng="ja">&#x30AA;&#x30E9;&#x30F3;&#x30C0;  <pr>&#x304A;&#x3089;&#x3093;&#x3060;</pr></trd>
  <trd lng="ko">&#xB124;&#xB35C;&#xB780;&#xB4DC;</trd>
  <trd lng="nl">Nederland</trd>
  <trd lng="no">Nederland</trd>
  <trd lng="pl">Holandia</trd>
  <trd lng="tok">ma Netelan</trd>
  <trdgrp lng="pt">
    <trd>Pa&iacute;ses Baixos</trd>,
    <trd>Holanda</trd>
  </trdgrp>
  <trd lng="ro">Olanda</trd>
  <trdgrp lng="ru">
    <trd>&c_N;&c_i;&c_d;&c_je;&c_r;&c_l;&c_a;&c_n;&c_d;&c_y;</trd>,
    <trd>&c_G;&c_o;&c_l;&c_l;&c_a;&c_n;&c_d;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">Holandsko</trd>
  <trd lng="sw">Uholanzi</trd>
  <trdgrp lng="uk">
    <trd>&#x41D;&#x456;&#x434;&#x435;&#x440;&#x43B;&#x430;&#x43D;&#x434;&#x438;</trd>,
    <trd>&#x413;&#x43E;&#x43B;&#x43B;&#x430;&#x43D;&#x434;&#x456;&#x44F;</trd>
  </trdgrp>
  <trd lng="vo">Ned&auml;n</trd>
  <trdgrp lng="zh">
    <trd>&#x8377;&#x5170; <pr>H&#xE9;l&#xE1;n</pr></trd>,
    <trd>&#x8377;&#x862D; <pr>H&#xE9;l&#xE1;n</pr></trd>,
    <trd>&#x83CF;&#x5170; <pr>H&#xE9;l&#xE1;n</pr></trd>,
    <trd>&#x83CF;&#x862D; <pr>H&#xE9;l&#xE1;n</pr></trd>,
    <trd>&#x5C3C;&#x5FB7;&#x5170; <pr>N&#xED;d&#xE9;l&#xE1;n</pr></trd>,
    <trd>&#x5C3C;&#x5FB7;&#x862D; <pr>N&#xED;d&#xE9;l&#xE1;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="nederl.0ano">
  <kap><tld lit="n"/>ano</kap>
  <snc mrk="nederl.0ano.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="nederl.N0o"><tld/>o</ref>:
      <ekz>
        <tld/>anoj de surinama deveno
        <fnt>
          <aut>Roland Rotsaert</aut>:
          <vrk>Piramidaj investoj: ne nur en Albanio!</vrk>,
          <bib>Monato</bib>
          <lok>jaro 1997a, numero 7a, p. 24a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">hollandiai</trd>
  </snc>
  <snc mrk="nederl.0ano.nacieco">
    <dif>
      Homo kun <tld lit="n"/>a nacieco.
    </dif>
    <trd lng="hu">holland</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ib;&c_d;&c_e;&c_r;&c_l;&c_ja;&c_n;&c_d;&c_z;&c_je;&c_c;</trd>,
    <trd>&c_g;&c_a;&c_l;&c_ja;&c_n;&c_d;&c_z;&c_je;&c_c;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_n;&c_i;&c_d;&c_je;&c_r;&c_l;&c_a;&c_n;&c_d;&c_je;&c_c;</trd>,
    <trd>&c_h;&c_o;&c_l;&c_a;&c_n;&c_d;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="br">Izelvroad</trd>
  <trdgrp lng="de">
    <trd>Niederl&auml;nder</trd>,
    <trd>Niederl&auml;nderin</trd>
  </trdgrp>
  <trd lng="el">&omikron;&lambda;&lambda;&alfa;&ny;&delta;&omikron_ton;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>Dutch person</trd>,
    <trd>Dutchman</trd>,
    <trd>Dutchwoman</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>N&eacute;erlandais</trd>
    <trd>N&eacute;erlandaise<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="fy">Nederlanner</trd>
  <trd lng="io">Nederlandano</trd>
  <trd lng="nl">Nederlander</trd>
  <trd lng="no">nederlender</trd>
  <trd lng="pl">Holender</trd>
  <trd lng="tok">jan pi ma Netelan</trd>
  <trd lng="pt">holand&ecirc;s</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_i;&c_d;&c_je;&c_r;&c_l;&c_a;&c_n;&c_d;&c_je;&c_c;</trd>,
    <trd>&c_g;&c_o;&c_l;&c_l;&c_a;&c_n;&c_d;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="vo">Ned&auml;nan</trd>
  <trdgrp lng="zh">
    <trd>&#x8377;&#x5170;&#x4EBA; <pr>h&#xE9;l&#xE1;nr&#xE9;n</pr></trd>,
    <trd>&#x8377;&#x862D;&#x4EBA; <pr>h&#xE9;l&#xE1;nr&#xE9;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="nederl.0a">
  <kap><tld lit="n"/>a</kap>
  <snc mrk="nederl.0a.gxen">
    <dif>
      Rilatanta al <tld lit="n"/>anoj, ilia nacieco a&ubreve; &scirc;tato:
      <ekz>
        jen rakonto de <tld/>a adoleskulo, kreskanta tra la periodo de
        la dua mondmilito
        <fnt>
          <aut>Stefan MacGill</aut>:
          <vrk>Rutine tra tumulto</vrk>,
          <bib>Monato</bib>
          <lok>jaro 2002a, numero 4a, p. 20a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="br">izelvroat</trd>
    <trd lng="el">&omikron;&lambda;&lambda;&alfa;&ny;&delta;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
    <trd lng="tok">pi ma Netelan</trd>
    <trd lng="vo">Ned&auml;nik</trd>
  </snc>
  <snc mrk="nederl.0a.lingv">
    <dif>
      <ref tip="super" cel="lingv.0o.homa">Lingvo</ref> parolata de
      la <ref tip="vid" cel="nederl.0ano"><tld lit="n"/>anoj</ref> kaj
      <ref tip="vid" cel="flandr.0o">flandroj</ref>:
      <ekz>
        estas &gcirc;ojige, ke &ndash; e&ccirc; preska&ubreve; samtempe kun interalie la <tld/>a
        versio &ndash; anka&ubreve; en Esperanto aperis lia &jcirc;usa studa&jcirc;o
        <fnt>
          <aut>Gerrit Berveling</aut>:
          <vrk>Pri kelkaj radikoj de la kristana kontra&ubreve;judismo (Peter
          J. Tomson, La afero Jesuo kaj la Judoj)</vrk>,
          <bib>Monato</bib>
          <lok>jaro 2001a, numero 10a, p. 25a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="br">izelvroeg</trd>
    <trd lng="el">&omikron;&lambda;&lambda;&alfa;&ny;&delta;&jota;&kappa;&alfa_ton;</trd>
    <trd lng="tok">toki Netelan</trd>
    <trd lng="vo">Ned&auml;nap&uuml;k</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ib;&c_d;&c_e;&c_r;&c_l;&c_ja;&c_n;&c_d;&c_z;&c_k;&c_ib;</trd>,
    <trd>&c_g;&c_a;&c_l;&c_ja;&c_n;&c_d;&c_z;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_n;&c_i;&c_d;&c_je;&c_r;&c_l;&c_a;&c_n;&c_d;&c_s;&c_k;&c_i;</trd>,
    <trd>&c_h;&c_o;&c_l;&c_a;&c_n;&c_d;&c_s;&c_k;&c_i;</trd>
  </trdgrp>
  <trd lng="de">niederl&auml;ndisch</trd>
  <trd lng="en">Dutch</trd>
  <trd lng="fr">n&eacute;erlandais<klr>(e) (adj.)</klr></trd>
  <trd lng="fy">Nederl&acirc;nsk</trd>
  <trd lng="hu">holland</trd>
  <trd lng="io">Nederlandana</trd>
  <trd lng="nl">Nederlands</trd>
  <trd lng="no">nederlandsk</trd>
  <trdgrp lng="pl">
    <trd>niderlandzki</trd>,
    <trd>holenderski</trd>
  </trdgrp>
  <trd lng="pt">holand&ecirc;s</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_i;&c_d;&c_je;&c_r;&c_l;&c_a;&c_n;&c_d;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_g;&c_o;&c_l;&c_l;&c_a;&c_n;&c_d;&c_s;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8377;&#x5170;&#x8BED; <pr>H&#xE9;l&#xE1;ny&#x1D4;</pr></trd>,
    <trd>&#x8377;&#x862D;&#x8A9E; <pr>H&#xE9;l&#xE1;ny&#x1D4;</pr></trd>
  </trdgrp>
</drv>
</art>
