<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Nederland</rad>/o
</kap>
<drv mrk="nederl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="sxtat.0o">&Scirc;tato</ref> situanta en
      okcidenta <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref>
      kaj limanta okcidente kaj norde al la Norda Maro, oriente
      al <ref tip="vid" cel="german1.G0ujo">Germanujo</ref> kaj sude al
      <ref tip="vid" cel="belg.B0ujo">Belgujo</ref>.
    </dif>
  </snc>
  <trd lng="de">Niederlande</trd>
  <trd lng="el">&Omikron;&lambda;&lambda;&alfa;&ny;&delta;&jota_ton;&alfa;</trd>
  <trd lng="en">The <ind>Netherlands</ind></trd>
  <trd lng="fr">Pays Bas</trd>
  <trd lng="nl">Nederland</trd>
  <trd lng="pl">Holandia</trd>
  <trd lng="pt">Holanda</trd>
  <trdgrp lng="ru">
    <trd>&c_N;&c_i;&c_d;&c_je;&c_r;&c_l;&c_a;&c_n;&c_d;&c_y;</trd>,
    <trd>&c_G;&c_o;&c_l;&c_l;&c_a;&c_n;&c_d;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="nederl.0ano">
  <kap><tld lit="n"/>ano</kap>
  <snc mrk="nederl.0ano.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="nederl.0o"><tld/>o</ref>.
    </dif>
  </snc>
  <snc mrk="nederl.0ano.nacieco">
    <dif>
      Homo kun <tld lit="n"/>a nacieco.
    </dif>
  </snc>
  <trd lng="de">Niederl&auml;nder</trd>
  <trd lng="el">&omikron;&lambda;&lambda;&alfa;&ny;&delta;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">Dutch</trd>
  <trd lng="fr">N&eacute;erlandais</trd>
  <trd lng="nl">Nederlander</trd>
  <trd lng="pl">Holender</trd>
  <trd lng="pt">holand&ecirc;s</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_i;&c_d;&c_je;&c_r;&c_l;&c_a;&c_n;&c_d;&c_je;&c_c;</trd>,
    <trd>&c_g;&c_o;&c_l;&c_l;&c_a;&c_n;&c_d;&c_je;&c_c;</trd>
  </trdgrp>
</drv>

<drv mrk="nederl.0a">
  <kap><tld lit="n"/>a</kap>
  <snc mrk="nederl.0a.gxen">
    <dif>
      Rilatanta al <tld lit="n"/>anoj, ilia nacieco a&ubreve; &scirc;tato.
    </dif>
    <trd lng="el">&omikron;&lambda;&lambda;&alfa;&ny;&delta;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  </snc>
  <snc mrk="nederl.0a.lingv">
    <dif>
      <ref tip="super" cel="lingv.0o.homa">Lingvo</ref> parolata de
      la <ref tip="vid" cel="nederl.0ano"><tld lit="n"/>anoj</ref> kaj
      <ref tip="vid" cel="flandr.0o">flandroj</ref>.
    </dif>
    <trd lng="el">&omikron;&lambda;&lambda;&alfa;&ny;&delta;&jota;&kappa;&alfa_ton;</trd>
  </snc>
  <trd lng="de">niederl&auml;ndisch</trd>
  <trd lng="en">dutch</trd>
  <trd lng="fr">n&eacute;erlandais</trd>
  <trd lng="nl">Nederlands</trd>
  <trdgrp lng="pl">
    <trd>niderlandzki</trd>,
    <trd>holenderski</trd>
  </trdgrp>
  <trd lng="pt">holand&ecirc;s</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_i;&c_d;&c_je;&c_r;&c_l;&c_a;&c_n;&c_d;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_g;&c_o;&c_l;&c_l;&c_a;&c_n;&c_d;&c_s;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
