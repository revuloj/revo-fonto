<art mrk="$Id: svis.xml,v 1.41 2024/10/31 10:34:37 revo Exp $">
<kap><ofc>10</ofc>
  <rad>svis</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="svis.0o">
  <kap><ofc>10</ofc> <tld/>o</kap>
  <snc mrk="svis.0o.sxtatano">
    <dif>
      &Scirc;tatano de
      <ref tip="malprt" cel="svis.S0ujo"><tld lit="S"/>ujo</ref>.
    </dif>
    <trd lng="de">schweizer Staatsb&uuml;rger</trd>
    <trd lng="nl">Zwitser</trd>
    <trd lng="pl">Szwajcar</trd>
  </snc>
  <snc mrk="svis.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco:
      <ekz>
        ni pasigis tie tri interesajn tagojn kun <nom>D. Watts</nom>, <nom>W.R. McGuire</nom>
        kaj la <tld/>o <nom>J. Hess</nom>
        <fnt>
        <aut>Wim de Smet</aut>,
        <vrk><url ref="&Monato;/1995/004815.php">Kiu murdis Dian
        Fossey?</url></vrk>,
        <bib>Monato</bib>,
        <lok>1995/12, p. 23-26</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">su&iacute;s</trd>
    <trd lng="de">Schweizer</trd>
    <trd lng="en">Swiss</trd>
    <trd lng="es">suizo</trd>
    <trd lng="nl">Zwitser</trd>
    <trd lng="no">sveitser</trd>
    <trd lng="pl">Szwajcar</trd>
    <trd lng="zh">&#29790;&#22763;&#20154; <pr>Ru&igrave;sh&igrave;r&eacute;n</pr></trd>
  </snc>
  <trd lng="be">&c_sh;&c_v;&c_a;&c_j;&c_c;&c_a;&c_r;&c_a;&c_c;</trd>
  <trd
  lng="bo">&t_sa;&t_shabchu;&t_da;&t_tsheg;&t_sa;&t_gigu;&t_-a;&t_gigu;&t_tsheg;&t_ma;&t_gigu;&t_tsheg;</trd>
  <trd lng="cs">&Scaron;v&yacute;car</trd>
  <trd lng="el">&epsilon;&lambda;&beta;&epsilon;&tau;&omikron_ton;&sigma_fina;</trd>
  <trd lng="fr">Suisse<klr> (subst.)</klr></trd>
  <trd lng="hu">sv&aacute;jci</trd>
  <trdgrp lng="io">
    <trd>Suisiano</trd>,
    <trd>Suiso</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>svizzero<klr> (sost.)</klr></trd>,
    <trd>elvetico<klr> (sost.)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#12473;&#12452;&#12473;&#20154;  <pr>&#12473;&#12452;&#12473;&#12376;&#12435;</pr></trd>
  <trd lng="pt">su&iacute;&ccedil;o</trd>
  <trd lng="ru">&c_sh;&c_v;&c_je;&c_j;&c_c;&c_a;&c_r;&c_je;&c_c;</trd>
  <trd lng="sk">&Scaron;vaj&ccaron;iar</trd>
  <trd lng="uk">&c_sh;&c_v;&c_je;&c_j;&c_c;&c_a;&c_r;&c_je;&c_c;&c_mol;</trd>
  <trd lng="vo">Jveiz&auml;nan</trd>
</drv>

<drv mrk="svis.0a">
  <kap><ofc>10</ofc><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato:
      <ekz>
        li igis &scirc;erpon <nom>Tenzing Norgay</nom>, kiu jaron
        anta&ubreve;e enkadre
        de <tld/>a alpista
        ekspedicio atingis 300 m anta&ubreve; la supro mem, plena membro
        de la
        alpista teamo.
        <fnt><aut>E. Georgiev</aut>,
        <vrk><url ref="&Monato;/2000/007116.html">Sir Edmund Hillary: Vivo
        dedi&ccirc;e al
        la &scirc;erpoj</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/10, p. 18-19</lok>
        </fnt>;
      </ekz>
      <ekz>
        unuamondaj entreprenistoj
        kiuj volis realigi iun grandan projekton en triamonda lando,
        pagis, tra <tld/>aj a&ubreve;
        alilandaj bankkontoj, 10, 20, e&ccirc; 40 % de la
        la&ubreve;kontrakta prezo al decidpovaj
        personoj el tiu lando
        <fnt><aut>Roland Rotsaert</aut>,
        <vrk><url ref="&Monato;/2000/006766.html">&Scirc;mirmono ne plu
        akcepteblas en
        internacia komerco</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/02, p. 8</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_v;&c_a;&c_j;&c_c;&c_a;&c_r;&c_s;&c_k;&c_ib;</trd>
  <trd lng="ca">su&iacute;s</trd>
  <trd lng="cs">&scaron;v&yacute;carsk&yacute;</trd>
  <trd lng="de">Schweizer</trd>
  <trd lng="el">&epsilon;&lambda;&beta;&epsilon;&tau;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">Swiss</trd>
  <trd lng="es">suizo</trd>
  <trd lng="fr">suisse<klr> (adj.)</klr></trd>
  <trd lng="fy">Switsersk</trd>
  <trd lng="hu">sv&aacute;jci</trd>
  <trdgrp lng="io">
    <trd>Suisiana</trd>,
    <trd>Suisa</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>svizzero<klr> (agg.)</klr></trd>,
    <trd>elvetico<klr> (agg.)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#12473;&#12452;&#12473;&#12398;  <pr>&#12377;&#12356;&#12377;&#12398;</pr></trd>
  <trd lng="nl">Zwitsers</trd>
  <trd lng="no">sveitsisk</trd>
  <trd lng="pl">szwajcarski</trd>
  <trd lng="pt">su&iacute;&ccedil;o</trd>
  <trd lng="ru">&c_sh;&c_v;&c_je;&c_j;&c_c;&c_a;&c_r;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">&scaron;vaj&ccaron;iarsky</trd>
  <trd lng="uk">&c_sh;&c_v;&c_je;&c_j;&c_c;&c_a;&c_r;&c_s;&c_mol;&c_k;&c_i;&c_j;</trd>
  <trd lng="vo">Jveiz&auml;nik</trd>
</drv>

<drv mrk="svis.S0ujo">
  <kap><ofc>10</ofc> <tld lit="S"/>ujo <fnt><bib>ZR</bib></fnt></kap>
  <snc mrk="svis.S0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o">&Scirc;tato</ref> situanta
      en meza <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref>
      kaj limanta oriente al A&ubreve;strujo, okcidente al
      <ref tip="vid" cel="franc.F0ujo">Francujo</ref>, norde
      al <ref tip="vid" cel="german1.G0ujo">Germanujo</ref> kaj sude al
      Italujo; alinome
      <ref tip="sin" cel="svisi.0o">Svisio</ref>,
      <ref tip="sin" cel="svis.S0lando">Svislando</ref>:
      <ekz>
        mi volus vidi la belegan <tld lit="S"/>ujon
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili alvenas en <tld lit="S"/>ujon, faras ekskurson sur la Rhona glaciejo
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_Sh;&c_v;&c_a;&c_j;&c_c;&c_a;&c_r;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_Sh;&c_v;&c_je;&c_j;&c_c;&c_a;&c_r;&c_i;&c_ja;</trd>
  <trd lng="ca">Su&iuml;ssa</trd>
  <trd lng="cs">&Scaron;v&yacute;carsko</trd>
  <trd lng="da">Svejts</trd>
  <trd lng="de">Schweiz</trd>
  <trd lng="el">&Epsilon;&lambda;&beta;&epsilon;&tau;&jota_ton;&alfa;</trd>
  <trd lng="en">Switzerland</trd>
  <trd lng="es">Suiza<klr tip="amb"> (naci&oacute;n)</klr></trd>
  <trdgrp lng="fr">
    <trd>Suisse</trd>,
    <trd>Conf&eacute;d&eacute;ration suisse</trd>
  </trdgrp>
  <trd lng="fy">Switserl&acirc;n</trd>
  <trd lng="gd">an <ind>Eilv&eacute;is</ind></trd>
  <trd lng="hu">Sv&aacute;jc</trd>
  <trd lng="id">Swiss</trd>
  <trd lng="io">Suisia</trd>
  <trd lng="is">Sviss</trd>
  <trd lng="it">Svizzera</trd>
  <trd lng="lat">Helvetia</trd>
  <trd lng="lt">&Scaron;veicarija</trd>
  <trd lng="lv">&Scaron;veice</trd>
  <trd lng="nl">Zwitserland</trd>
  <trd lng="no">Sveits</trd>
  <trd lng="pl">Szwajcaria</trd>
  <trd lng="pt">Su&iacute;&ccedil;a</trd>
  <trd lng="ru">&c_Sh;&c_v;&c_je;&c_j;&c_c;&c_a;&c_r;&c_i;&c_ja;</trd>
  <trd lng="sk">&Scaron;vaj&ccaron;iarsko</trd>
  <trd lng="sv">Svejts</trd>
  <trd lng="tok">ma Suwasi</trd>
  <trd lng="tr">&Idot;svi&ccedil;re</trd>
  <trd lng="vo">Jveiz&auml;n</trd>
  <trd lng="zh">&#29790;&#22763; <pr>Ru&igrave;sh&igrave;</pr></trd>
</drv>

<drv mrk="svis.S0lando">
  <kap>
    <ofc>10</ofc> <tld lit="S"/>lando
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="svis.S0lando.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="svis.S0ujo">Svisujo</ref>:
      <ekz>
        jam en a&ubreve;tuno 1914, li skribis al D-ro <nom>Uhlman</nom> en <tld lit="S"/>lando
        <fnt>
          <bib>VivZam</bib>
          <lok>Vivo de Zamenhof</lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      La nomo estas gentobaza (vd <fnt><bib>RLN</bib></fnt>), sed uzas la finvorton -land
      anstata&ubreve; -uj/-i pro internacieco a&ubreve; Esperanta tradicio.
    </rim>
  </snc>
  <trd lng="be">&c_Sh;&c_v;&c_a;&c_j;&c_c;&c_a;&c_r;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_Sh;&c_v;&c_je;&c_j;&c_c;&c_a;&c_r;&c_i;&c_ja;</trd>
  <trd lng="ca">Su&iuml;ssa</trd>
