<art mrk="$Id$">
<kap>
   <ofc>9</ofc><rad>iks</rad>/o
</kap>
<drv mrk="iks.0o">
   <kap><tld/>o</kap>
   <snc>
   <uzo tip="fak">TIP</uzo>
     <dif>
       Neesperanta <ref tip="super" cel="liter.0o.propra">litero</ref>
       <em>X, x</em>:
       <ekz>
         Cxu vi havas problemojn tajpi esperantajn tekstojn? Uzu
         <tld/>ojn!
         <fnt><lok>
         <url ref="http://www.purl.org/NET/mihxil/iksoj.html">
         Iksoj</url></lok></fnt>
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_ib;&c_k;&c_s;</trd>
   <trd lng="de">X</trd>
   <trdgrp lng="en">
     <trd>x</trd>,
     <trd>ex</trd>
   </trdgrp>
   <trd lng="es">equis</trd>
   <trdgrp lng="fi">
      <trd>x</trd>,
      <trd>&auml;ks&auml;</trd>
   </trdgrp>
   <trdgrp lng="fr">
  <trd>x</trd>,
  <trd>X <klr> (iks)</klr></trd>
   </trdgrp>   
   <trd lng="hu">iksz</trd>
   <trdgrp lng="it">
   <trd>ics</trd>, 
   <trd>x</trd>
   </trdgrp>
   <trd lng="nl">x</trd>
   <trd lng="pl">iks</trd>
   <trd lng="pt">xis</trd>
   <trd lng="ru">&c_i;&c_k;&c_s;</trd>
   <trd lng="sv">x</trd>
</drv>
</art>
