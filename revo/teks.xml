<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>teks</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="teks.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="teks.0i.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Dense interkrucigi fadenojn por fari &scirc;tofon:
      <ekz>
        <tld/>i kanabon, linon, lanon, silkon;
      </ekz>
      <ekz>
        <tld/>i drapon, katunon, tolon.
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_k;&c_a;&c_c;&c_mol;</trd>
    <trd lng="hu">sz&odblac;</trd>
    <trd lng="it">tessere</trd>
    <trd lng="pl">tka&cacute;</trd>
  <trdgrp lng="pt">
    <trd>tecer</trd>
  </trdgrp>
    <trd lng="ru">&c_t;&c_k;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="teks.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Zorge kombini:
      <ekz>
        <tld/>i mensogojn kaj ruzojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la poeto lerte en<tld/>is tiun legendon en sia rakonto;
      </ekz>
      <ekz>
        la historio de la hungara nacio estas tra<tld/>ita
        <klr>(plenigita, kunmetita, miksita)</klr> de multaj
        katastrofoj.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_l;&c_je;&c_s;&c_mol;&c_c;&c_ib;</trd>,
      <trd>&c_s;&c_p;&c_l;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_a;&c_p;&c_l;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>sz&odblac; <klr tip="ind">(&aacute;tv.)</klr></trd>,
      <trd>kieszel</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>tessere<klr> (fig.)</klr></trd>,
      <trd>ordire<klr> (fig.)</klr></trd>,
      <trd>tramare<klr> (fig.)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_je;&c_s;&c_t;&c_i;<klr tip="ind"> (&c_p;&c_je;&c_r;&c_je;&c_n;.)</klr></trd>,
      <trd>&c_s;&c_p;&c_l;&c_je;&c_t;&c_a;&c_t;&c_mol; <klr tip="amb">(&c_p;&c_je;&c_r;&c_je;&c_n;.)</klr></trd>
    </trdgrp>
  </snc>
  <refgrp tip="vid">
    <ref cel="sxpin.0i">&scirc;pini</ref>,
    <ref cel="plekt.0i">plekti</ref>,
    <ref cel="trik.0i">triki</ref>.
  </refgrp>
  <trd lng="de">weben</trd>
  <trd lng="en">weave</trd>
  <trd lng="es">tejer</trd>
  <trd lng="fr">tisser</trd>
  <trd lng="nl">weven</trd>
  <trd lng="no">veve</trd>
  <trd lng="pl">tka&cacute;</trd>
  <trdgrp lng="sk">
    <trd>snova&tcaron;</trd>,
    <trd>utka&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>snovat</trd>,
    <trd>tk&aacute;t</trd>,
    <trd>utkat</trd>
  </trdgrp>
</drv>

<drv mrk="teks.0a">
  <kap><tld/>a</kap>
  <snc mrk="teks.0a.TEKS">
  <uzo tip="fak">TEKS</uzo>
    <dif>
      <tld lit="T"/>ebla, uzata por <tld/>ado.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_k;&c_a;&c_c;&c_k;&c_ib;</trd>,
    <trd>&c_t;&c_e;&c_k;&c_s;&c_t;&c_y;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="fr">textile<klr tip="amb"> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>sz&ouml;v&odblac;-</trd>,
    <trd>textil-</trd>
  </trdgrp>
  <trd lng="it">tessile</trd>
  <trd lng="pl">tkacki</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_k;&c_a;&c_c;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_t;&c_je;&c_k;&c_s;&c_t;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="teks.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="teks.0ajxo.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Io <tld/>ita<sncref ref="teks.0i.TEKS"/>;
      <ref tip="sin" cel="sxtof.0o">&scirc;tofo</ref>:
      <ekz>
        silka <tld/>a&jcirc;o.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_t;&c_k;&c_a;&c_n;&c_ib;&c_n;&c_a;</trd>,
      <trd>&c_t;&c_e;&c_k;&c_s;&c_t;&c_y;&c_l;&c_mol;</trd>
    </trdgrp>
    <trd lng="es">tela</trd>
    <trd lng="hu">sz&ouml;vet</trd>
    <trd lng="pl">tkanina</trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_k;&c_a;&c_n;&c_mol;</trd>,
      <trd>&c_t;&c_je;&c_k;&c_s;&c_t;&c_i;&c_l;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="teks.0ajxo.ANA">
    <uzo tip="fak">ANA</uzo>
    <ref tip="dif" cel="hist.0o">histo</ref>
    <trd lng="be">&c_t;&c_k;&c_a;&c_n;&c_k;&c_a;</trd>
    <trd lng="es">tejido</trd>
    <trd lng="hu">sz&ouml;vet <klr tip="amb">(biol.)</klr></trd>
    <trd lng="pl">tkanka</trd>
    <trd lng="ru">&c_t;&c_k;&c_a;&c_n;&c_mol; <klr tip="amb">(&c_b;&c_i;&c_o;&c_l;.)</klr></trd>
  </snc>
  <snc mrk="teks.0ajxo.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Aro da interligitaj aferoj:
      <ekz>
        la historio de tiu lando estas longa <tld/>a&jcirc;o da heroaj luktoj
        por la libereco;
      </ekz>
      <ekz>
        tiu artikolo estas nur ia <tld/>a&jcirc;o el malveroj;
      </ekz>
      <ekz>
        <uzo tip="fak">KOMP</uzo>
        la <ind>Tut-Tera <tld lit="T"/>a&jcirc;o</ind>
        <klr>(= <ref cel="ttt.0">TTT</ref>)</klr>.
        <trd lng="en">World-Wide Web</trd>
        <trd lng="hu">vil&aacute;gh&aacute;l&oacute;</trd>
        <trd lng="it">World-Wide Web</trd>
        <trd lng="pl">sie&cacute;</trd>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_p;&c_l;&c_ja;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_a;&c_p;&c_l;&c_ja;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>h&aacute;l&oacute;</trd>,
      <trd>sz&ouml;vev&eacute;ny</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_p;&c_l;&c_je;&c_t;&c_je;&c_n;&c_i;&c_je; <klr tip="amb">(&c_p;&c_je;&c_r;&c_je;&c_n;.)</klr></trd>
  </snc>
  <refgrp tip="vid">
    <ref cel="ret.0o">reto</ref>.
  </refgrp>
  <trd lng="fr">tissu<klr tip="amb"> (subst.)</klr></trd>
  <trd lng="it">tessuto</trd>
  <trd lng="nl">weefsel</trd>
</drv>

<drv mrk="teks.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="teks.0ilo.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Ilo a&ubreve; ma&scirc;ino, per kiu oni <tld/>as:
      <ekz>
        &scirc;i pendigis lampon super la <tld/>ilo kaj <tld/>adis
        <fnt>
          <aut>Mahbube Mirkadiri</aut>,
          <vrk>Donaco el voja&gcirc;o</vrk>,
          <bib>LOdE</bib>,
          <lok>2003:3 (101)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">tkalcovsk&yacute; stav</trd>
  <trd lng="fr">m&eacute;tier &agrave; tisser</trd>
  <trd lng="it">telaio<klr> (tes.)</klr></trd>
  <trdgrp lng="sk">
    <trd>krosn&aacute;</trd>,
    <trd>tk&aacute;&ccaron;sky stav</trd>
  </trdgrp>
</drv>

<drv mrk="teks.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="teks.0isto.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Laboristo, kiu <tld/>as hejme a&ubreve; fabrike:
      <ekz>
        tie estis kvartalo de taglaboruloj, kudristinoj kaj <tld/>istoj
        <fnt>
          <aut>Alain Fournier, trad. Roger Bernard</aut>,
          <vrk>La Granda Meaulnes</vrk>,
          <lok>p. 95</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">tisserand</trd>
</drv>

<drv mrk="teks.0bazo">
  <kap><tld/>bazo<fnt>Z</fnt></kap>
  <snc mrk="teks.0bazo.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Aro da paralelaj fadenoj konsistiganta la fundamenta&jcirc;on de
      <tld/>a&jcirc;o; <ref tip="sin" cel="varp.0o">varpo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_n;&c_o;&c_v;&c_a; <klr tip="amb">(&c_t;&c_k;&c_a;&c_n;&c_ib;&c_n;&c_y;)</klr></trd>
  <trd lng="cs">osnova</trd>
  <trd lng="hu">l&aacute;nc <klr tip="amb">(sz&ouml;vetben)</klr></trd>
  <trd lng="it">ordito</trd>
  <trd lng="nl">schering</trd>
  <trd lng="pl">osnowa</trd>
  <trd lng="ru">&c_o;&c_s;&c_n;&c_o;&c_v;&c_a;<klr tip="ind"> (&c_t;&c_je;&c_k;&c_s;.)</klr></trd>
  <trdgrp lng="sk">
    <trd>prie&ccaron;ne nite <klr>(text.)</klr></trd>,
    <trd>&uacute;tok</trd>
  </trdgrp>
</drv>

<drv mrk="teks.0arangxo">
  <kap><tld/>aran&gcirc;o <fnt><bib>PIV1</bib></fnt></kap>
  <snc mrk="teks.0arangxo.TEKS">
  <uzo tip="fak">TEKS</uzo>
    <dif>
      Aran&gcirc;o de la fadenoj en <tld/>a&jcirc;o:
      <ekz>
        la <ind>tola <tld/>aran&gcirc;o</ind>
        <trdgrp lng="en">
          <trd>plain weave</trd>,
          <trd>tabby weave</trd>
        </trdgrp>
        <trd lng="hu">v&aacute;szonk&ouml;t&eacute;s</trd>
        <trd lng="it">tela</trd>
        <trd lng="pl">splot</trd>
        <trd lng="ru">&c_p;&c_o;&c_l;&c_o;&c_t;&c_n;&c_ja;&c_n;&c_o;&c_je; &c_p;&c_je;&c_r;&c_je;&c_p;&c_l;&c_je;&c_t;&c_je;&c_n;&c_i;&c_je;</trd>
      </ekz>
      <ekz>
        la <ind>satena <tld/>aran&gcirc;o</ind>
        <trd lng="de">Atlasbindung</trd>
        <trd lng="en">satin weave</trd>
        <trd lng="hu">atlaszk&ouml;t&eacute;s</trd>
        <trd lng="it">raso</trd>
        <trd lng="ru">&c_a;&c_t;&c_l;&c_a;&c_s;&c_n;&c_o;&c_je; &c_p;&c_je;&c_r;&c_je;&c_p;&c_l;&c_je;&c_t;&c_je;&c_n;&c_i;&c_je;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_p;&c_l;&c_ja;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_je; <klr>(&c_n;&c_ib;&c_t;&c_a;&c_k; &c_u; &c_t;&c_k;&c_a;&c_n;&c_ib;&c_n;&c_je;)</klr></trd>
  <trd lng="en">weave</trd>
  <trd lng="hu">alapk&ouml;t&eacute;s</trd>
  <trdgrp lng="it">
    <trd>intreccio<klr> (tes.)</klr></trd>,
    <trd>armatura<klr> (tes.)</klr></trd>
    <trd>disegno<klr> (tes.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_p;&c_l;&c_je;&c_t;&c_je;&c_n;&c_i;&c_je; &c_n;&c_i;&c_t;&c_je;&c_j;</trd>
</drv>
</art>
