<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>inhal</rad>/i
</kap>
<drv mrk="inhal.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Enspiri vaporon a&ubreve; gason porkurace:
      <ekz><tld/>i eteron.</ekz>
    </dif>
    <trd lng="nl">inhaleren</trd>
    <trd lng="ru">&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; &c_i;&c_n;&c_g;&c_a;&c_l;&c_ja;&c_c;&c_i;&c_ju; <klr>(&c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>
    <trd lng="sv">inhalera</trd>
  </snc>
  <trdgrp lng="de">
      <trd>inhalieren</trd>,
      <trd>einatmen</trd>
  </trdgrp>
  <trd lng="es">inhalar</trd>
</drv>
<drv mrk="inhal.0ado">
  <kap><tld/>(ad)o</kap>
  <snc>
    <dif>
      Ago <tld/>i a&ubreve; la rezulto de tiu ago. 
    </dif>
  </snc>
  <trdgrp lng="de">
      <trd>Inhalieren</trd>,
      <trd>Inhalation</trd>,
      <trd>Einatmen</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_n;&c_g;&c_a;&c_l;&c_ja;&c_c;&c_i;&c_ja;</trd>
</drv>
<drv mrk="inhal.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      <tld lit="I"/>ata gaso, vaporo. 
    </dif>
  </snc>
  <trdgrp lng="de">
      <trd>Inhalat</trd>,
      <trd>einzuatmende D&auml;mpfe</trd>
  </trdgrp>
</drv>
<drv mrk="inhal.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Aparato por <tld/>i.  
    </dif>
  </snc>
  <trd lng="de">Inhalator</trd>
  <trd lng="es">inhalador</trd>
  <trd lng="nl">inhalatietoestel</trd>
  <trd lng="ru">&c_i;&c_n;&c_g;&c_a;&c_l;&c_ja;&c_t;&c_o;&c_r;</trd>
</drv>
</art>
