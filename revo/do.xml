  <trd lng="ka">&#x10D0;&#x10DB;&#x10D2;&#x10D5;&#x10D0;&#x10E0;&#x10D0;&#x10D3;,</trd>
  <trd lng="km">&#x178A;&#x17BC;&#x1785;&#x17D2;&#x1793;&#x17C1;&#x17C7;</trd>
