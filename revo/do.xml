<art mrk="$Id: do.xml,v 1.67 2021/06/18 20:41:31 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>do</rad> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="do.0">
   <kap><ofc>*</ofc><tld/></kap>
   <snc mrk="do.0.konsekvence">
     <gra><vspec>konjunkcio</vspec></gra>
     <dif>
       <ref tip="vid" cel="sekv.0e.ARG">Sekve</ref> de tio,
       <ref tip="vid" cel="konsek1.0e">konsekvence</ref>,
       konklude:
       <ekz>
         &ccirc;ion vi vidis, mi <tld/> ne ripetas
         <fnt>
           <bib>Paroloj</bib>
           <vrk>Ferma Parolado en Washington</vrk>,
           <lok>1910</lok>
         </fnt>.
       </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>&c_t;&c_a;&c_k;</trd>,
       <trd>&c_t;&c_a;&c_k;&c_ib;&c_m; &c_ch;&c_y;&c_n;&c_a;&c_m;</trd>
     </trdgrp>
     <trd lng="nl">dus</trd>
     <trdgrp lng="ru">
       <trd>&c_i;&c_t;&c_a;&c_k;</trd>,
       <trd>&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;</trd>,
       <trd>&c_t;&c_o;</trd>
     </trdgrp>
     <trdgrp lng="de">
       <trd>also</trd>,
       <trd>dann</trd>,
       <trd>folglich</trd>
     </trdgrp>
     <trd lng="fi">siis</trd>
     <trdgrp lng="pl">
       <trd>wi&eogonek;c</trd>,
       <trd>tak wi&eogonek;c</trd>,
       <trd>st&aogonek;d</trd>
     </trdgrp>
     <trd lng="vo">also</trd>
   </snc>
   <snc mrk="do.0.emfaze">
     <gra><vspec>adverbo</vspec></gra>
     <dif>
       Vere, efektive.
     </dif>
     <subsnc mrk="do.0.surprize">
       <uzo tip="klr">(en demando)</uzo>
       <dif>
         Surprize, &ccirc;u vere?:
         <ekz>
           kion <tld/> vi volus ricevi
           <fnt><bib>MT</bib>, <lok>&Eli; 2:20</lok></fnt>?
         </ekz>
         <ekz>
           kie <tld/> li estas?
         </ekz>
       </dif>
       <trdgrp lng="be">
         <trd>&c_zh;</trd>,
         <trd>&c_zh;&c_a;</trd>
       </trdgrp>
       <trdgrp lng="de">
         <trd>also</trd>,
         <trd>denn</trd>
       </trdgrp>
       <trdgrp lng="fi">
         <trd>siis</trd>,
         <trd>sitten</trd>
       </trdgrp>
       <trd lng="nl">dan</trd>
       <trdgrp lng="pl">
         <trd>wi&eogonek;c</trd>,
         <trd>-&zdot;e<klr> (jako sufiks)</klr></trd>
       </trdgrp>
       <trd lng="ru">&c_zh;&c_je;</trd>
     </subsnc>
     <subsnc mrk="do.0.insiste">
       <uzo tip="klr">(en ordono)</uzo>
       <dif>
         Nepre, vere:
         <ekz>
           venu <tld/>;
         </ekz>
         <ekz>
           a&ubreve;skultu <tld/>
           <fnt><bib>MT</bib>, <lok>&Sa1; 15:1</lok></fnt>;
         </ekz>
         <ekz>
           demandu <tld/> tiun homon.
         </ekz>
       </dif>
       <trdgrp lng="be">
         <trd>&c_zh;</trd>,
         <trd>&c_zh;&c_a;</trd>
       </trdgrp>
       <trdgrp lng="de">
         <trd>doch</trd>
       </trdgrp>
       <trdgrp lng="fi">
         <trd>(no...) sitten</trd>,
         <trd>siis</trd>,
       </trdgrp>
       <trd lng="nl">dan</trd>
       <trdgrp lng="pl">
         <trd>wi&eogonek;c</trd>,
         <trd>-&zdot;e<klr> (jako sufiks)</klr></trd>
       </trdgrp>
       <trd lng="ru">&c_zh;&c_je;</trd>
     </subsnc>
   </snc>
