<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>do</rad></kap>
<drv mrk="do.0">
   <kap><ofc>*</ofc><tld/></kap>
   <snc mrk="do.0.konsekvence">
     <dif>
       Konj. montranta la logikan sekvon, la konkludon.
     </dif>
     <refgrp tip="sin">
       <ref cel="konsek1.0e">konsekvence</ref>,
       <ref cel="sekv">sekve</ref>.
     </refgrp>
     <trdgrp lng="be">
       <trd>&c_t;&c_a;&c_k;</trd>,
       <trd>&c_t;&c_a;&c_k;&c_ib;&c_m; &c_ch;&c_y;&c_n;&c_a;&c_m;</trd>
     </trdgrp>
     <trd lng="nl">dus</trd>
     <trdgrp lng="ru">
       <trd>&c_i;&c_t;&c_a;&c_k;</trd>,
       <trd>&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;</trd>,
       <trd>&c_t;&c_o;</trd>
     </trdgrp>
     <trdgrp lng="de">
       <trd>also</trd>,
       <trd>dann</trd>,
       <trd>folglich</trd>
     </trdgrp>
     <trd lng="fi">siis</trd>
     <trdgrp lng="pl">
       <trd>wi&eogonek;c</trd>,
       <trd>tak wi&eogonek;c</trd>,
       <trd>st&aogonek;d</trd>
     </trdgrp>
     <trd lng="vo">also</trd>
   </snc>
   <snc mrk="do.0.surprize">
     <dif>
       Konj. montranta la surprizon:
       <ekz>
         kion <tld/> vi volus ricevi
         <fnt><bib>MT</bib>, <lok>&Eli; 2:20</lok></fnt>?
       </ekz>
       <ekz>
         kie <tld/> li estas?
       </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>&c_zh;</trd>,
       <trd>&c_zh;&c_a;</trd>
     </trdgrp>
     <trdgrp lng="de">
       <trd>also</trd>,
       <trd>denn</trd>
     </trdgrp>
     <trdgrp lng="fi">
       <trd>siis</trd>,
       <trd>sitten</trd>
     </trdgrp>
     <trd lng="nl">dan</trd>
     <trdgrp lng="pl">
       <trd>wi&eogonek;c</trd>,
       <trd>-&zdot;e<klr> (jako sufiks)</klr></trd>
     </trdgrp>
     <trd lng="ru">&c_zh;&c_je;</trd>
   </snc>
   <snc mrk="do.0.insiste">
     <dif>
       Konj. uzata por insisti:
       <ekz>
         venu <tld/>;
       </ekz>
       <ekz>
         a&ubreve;skultu <tld/>
         <fnt><bib>MT</bib>, <lok>&Sa1; 15:1</lok></fnt>;
       </ekz>
       <ekz>
         demandu <tld/> tiun homon.
       </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>&c_zh;</trd>,
       <trd>&c_zh;&c_a;</trd>
     </trdgrp>
     <trdgrp lng="de">
       <trd>doch</trd>
     </trdgrp>
     <trdgrp lng="fi">
       <trd>(no...) sitten</trd>,
       <trd>siis</trd>,
     </trdgrp>
     <trd lng="nl">dan</trd>
     <trdgrp lng="pl">
       <trd>wi&eogonek;c</trd>,
       <trd>-&zdot;e<klr> (jako sufiks)</klr></trd>
     </trdgrp>
     <trd lng="ru">&c_zh;&c_je;</trd>
   </snc>
   <trd lng="cs">tedy</trd>
   <trd lng="fr">donc</trd>
    <trdgrp lng="pt">
      <trd>logo</trd>,
      <trd>ent&atilde;o</trd>,
      <trd>pois</trd>,
      <trd>portanto</trd>,
      <trd>por <ind>conseguinte</ind></trd>
    </trdgrp>
   <trdgrp lng="sk">
     <trd>tak</trd>,
     <trd>nu&zcaron;</trd>
   </trdgrp>
</drv>
</art>
