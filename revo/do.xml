  <trd lng="my">&#x1011;&#x102D;&#x102F;&#x101E;&#x102D;&#x102F;&#x1037;</trd>
  <trd lng="ne">&#x92F;&#x938;&#x930;&#x940;</trd>
  <trd lng="ny">motero</trd>
  <trd lng="pa">&#xA07;&#xA38; &#xA2A;&#xA4D;&#xA30;&#xA15;&#xA3E;&#xA30;</trd>
  <trd lng="ps">&#x67E;&#x647; &#x62F;&#x6D0; &#x62A;&#x648;&#x6AB;&#x647;</trd>
