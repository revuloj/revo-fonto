<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: unucikl.xml,v 1.9 2023/09/05 09:43:53 revo Exp $">
<kap>
  <rad>unucikl</rad>/o
</kap>
<drv mrk="unucikl.0o">
  <kap><tld/>o</kap>
  <snc mrk="unucikl.0o.CIK">
    <dif>
      <ref tip="super" cel="veloci.0o.CIK">Velocipedo</ref>
      kun unu rado:
      <ekz>
        lerta&jcirc;oj: <klr>...</klr> manprodukti simplajn
ma&scirc;inojn, reciti 
        Pi (3.14159...), rajdi <tld/>on
        <fnt>
          <aut>Kozima H.</aut>,
          <vrk><url ref="http://www.k3.dion.ne.jp/~xkozima/index-esp.html">
          hejmpa&gcirc;o</url></vrk>,
          <lok>2006-06</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">velociped s jedn&#xED;m kolem</trd>
  <trd lng="en">unicycle</trd>
  <trd lng="fr">monocycle</trd>
  <trdgrp lng="hu">
    <trd>egykerek&udblac; <ind>bicikli</ind></trd>,
    <trd>monocikli</trd>
  </trdgrp>
  <trd lng="it">monociclo</trd>
  <trd lng="ja">&#x4E00;&#x8F2A;&#x8ECA;  [<ind>&#x3044;&#x3061;&#x308A;&#x3093;&#x3057;&#x3083;</ind>]</trd>
  <trd lng="pl">monocykl</trd>
  <trd lng="pt">monociclo</trd>
  <trd lng="sk">velocip&#xE9;d s jedn&#xFD;m kolesom</trd>
  <trd lng="sv">enhjuling</trd>
</drv>

<drv mrk="unucikl.0i">
  <kap><tld/>i</kap>
  <snc mrk="unucikl.0i.CIK">
    <dif>
      Veturi per <tld/>o:
      <ekz>
        mi <tld/>as al la universitato
        <fnt>
          <aut>C. Smith</aut>,
          <vrk><url ref="http://www.argilo.net/">
          Tereno de Argilo</url></vrk>,
          <lok>2006-01-16</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="hu">egykerek&udblac;n biciklizik</trd>
  <trd lng="it">andare in <ind>monociclo</ind></trd>
  <trd lng="pl">je&zacute;dzi&cacute; <ind>monocyklem</ind></trd>
  <trd lng="pt">andar de <ind>monociclo</ind></trd>
  <trd lng="sv">cykla p&aring; <ind>enhjuling</ind></trd>
</drv>

<drv mrk="unucikl.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="unucikl.0isto.CIK">
    <dif>
      Kutima, sperta uzanto de <tld/>o:
      <ekz>
        iutage mi rigardis <klr>...</klr> <tld/>istinon sur la piedvojo 
        <fnt>
          <aut>Frank</aut>,
          <vrk><url ref="http://www.vohla.de/einrad-eo.html">
          Unuciklo</url></vrk>,
          <lok>2007-08-07</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="it">monociclista</trd>
  <trd lng="pl">monocyklista</trd>
  <trd lng="pt">monociclista</trd>
  <trd lng="sv">enhjulingcyklist</trd>
</drv>
</art>

