<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kuskus.xml,v 1.12 2023/10/18 17:23:25 revo Exp $">
<kap>
  <rad>kuskus</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kuskus.0o">
  <kap><tld/>o</kap>
  <snc mrk="kuskus.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Populara <ref tip="lst" lst="voko:man&gcirc;a&jcirc;oj" cel="mangx.0ajxo.homa">man&gcirc;a&jcirc;o</ref>
      el Nordafriko el vaporumita a&ubreve; boligita <ref tip="super" cel="gri.0o.semolo">grio</ref>;
      &gcirc;in ofte akompanas fabaco, boligitaj legomoj, eventuale viando:
      <ekz>
        tunizia <tld/>o
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="&Sezonoj;2014/11/svisujo/">
          Vivanta lingvo de vivanta komunumo</url></vrk>,
          <lok>2014-11-23</lok>
        </fnt>;
      </ekz>
      <ekz>
        bazaj tritikaj mediteraneaj man&gcirc;a&jcirc;oj inkluzivas produktojn kiel pasta&jcirc;oj kaj
        <ref cel="semol.0o">semol</ref>a&jcirc;oj kiel <tld/>o kaj bulguro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=312621">Mediteranea kuirarto</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/e/ee/Couscous_au_poulet%2C_Tunisie%2C_12_janvier_2019.jpg/637px-Couscous_au_poulet%2C_Tunisie%2C_12_janvier_2019.jpg" prm="CC BY-SA 3.0"><tld/>a&jcirc;o de Tunizio
        <fnt>
          <aut>Habib M&#x2019;henni, propra verko</aut>
          <lok><url ref="&WCW;/File:Couscous_au_poulet,_Tunisie,_12_janvier_2019.jpg">&WCW;/File:Couscous_au_poulet,_Tunisie,_12_janvier_2019.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="ar">&a_k;&a_s;&a_k;&a_s;&a_y;</trd>
    <trdgrp lng="de">
      <trd>Couscous</trd>,
      <trd>Kuskus</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_s;&c_k;&c_u;&c_s;</trd>
  <trdgrp lng="cs">
    <trd>arabsk&eacute; knedl&iacute;&ccaron;ky</trd>,
    <trd>karban&aacute;tky</trd>,
    <trd>kus-kus</trd>,
    <trd>kuskus</trd>
  </trdgrp>
  <trd lng="fr">couscous</trd>
  <trdgrp lng="ja">
    <trd>&#x30AF;&#x30B9;&#x30AF;&#x30B9;  <pr>&#x304F;&#x3059;&#x304F;&#x3059;</pr></trd>,
    <trd>&#x30AF;&#x30B9;&#x30AF;&#x30B9;&#x6599;&#x7406;  <pr>&#x304F;&#x3059;&#x304F;&#x3059;&#x308A;&#x3087;&#x3046;&#x308A;</pr></trd>
  </trdgrp>
  <trd lng="nl">couscous</trd>
  <trd lng="no">couscous</trd>
  <trd lng="pl">kuskus</trd>
  <trd lng="ro">cu&#x219; cu&#x219;</trd>
  <trdgrp lng="sk">
    <trd>kus-kus</trd>,
    <trd>kuskus</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&c_u;&c_s;&#x43A;&c_u;&c_s;</trd>
</drv>
</art>

