<art mrk="$Id: domici.xml,v 1.20 2025/03/29 10:30:52 revo Exp $">
<kap>
  <rad>domicil</rad>/o
</kap>
<drv mrk="domici.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      La&ubreve;le&gcirc;a <ref tip="super" cel="rest.0adejo">restadejo</ref>
      de persono, oficiala <ref tip="super" cel="logx.0ejo">lo&gcirc;ejo</ref>:
      <ekz>
        <nom>Duden</nom> fari&gcirc;is direktoro de la gimnazio en <nom>Hersfeld</nom>, en 1905 li elektis
        kiel
        dumpensian <tld/>on <nom>Wiesbaden-Sonnenberg</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2378">Konrad Duden</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        inter tiuj fundamentaj rajtoj oni kalkulas &ccirc;efe: privatajn liberecojn: rajto je
        privata vivo, je <tld/>o, libereco je korespondado, je religio, je esprimado
        ...
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=108002">&Gcirc;eneralaj jurprincipoj (E&ubreve;ropa
          Unio)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="adres.0o.POSX">adreso<sncref/></ref>
    <trdgrp lng="be">
      <trd>&c_m;&c_je;&c_s;&c_c;&c_a;
      &c_zh;&c_y;&c_h;&c_a;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>,
      <trd>&c_d;&c_a;&c_m;&c_ib;&c_c;&c_y;&c_l;&c_ib;&c_j;</trd>
    </trdgrp>
    <trdgrp lng="de">
       <trd>Wohnsitz</trd>,
       <trd>Domizil</trd>
    </trdgrp>
    <trd lng="es">domicilio</trd>
    <trd lng="fr">domicile</trd>
    <trd lng="hu">&aacute;lland&oacute; <ind>lakhely</ind></trd>
    <trdgrp lng="pl">
      <trd>domicyl</trd>,
      <trd>zameldowanie</trd>
    </trdgrp>
    <trd lng="pt">domic&iacute;lio</trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_p;&c_i;&c_s;&c_k;&c_a;</trd>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>lo&#285;loko</ke>.
  </rim>

  <trdgrp lng="cs">
    <trd>bydli&scaron;t&ecaron;</trd>,
    <trd>domicil <klr>(mezin&aacute;rodn&iacute; nebo sm&ecaron;ne&ccaron;n&eacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#20303;&#25152;  <pr>&#12376;&#12421;&#12358;&#12375;&#12423;</pr></trd>,
    <trd>&#26412;&#31821;  <pr>&#12411;&#12435;&#12379;&#12365;</pr></trd>
  </trdgrp>
  <trd lng="nl">domicilie</trd>
  <trd lng="sk">bydlisko</trd>
  <trdgrp lng="uk">
    <trd>&#1084;&c_ib;&c_s;&c_c;&c_je; &#1087;&c_r;&#1086;&#1087;&c_i;&c_s;&#1082;&c_i;</trd>,
    <trd>&#1084;&c_ib;&c_s;&c_c;&c_je; &c_r;&c_je;&c_jeu;&c_s;&c_t;&c_r;&c_a;&c_c;&c_ib;&c_ji;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#25448; <pr>sh&#283;</pr></trd>,
    <trd>&#23507; <pr>y&#249;</pr></trd>
  </trdgrp>
</drv>
</art>
