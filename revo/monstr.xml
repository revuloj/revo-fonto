<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: monstr.xml,v 1.25 2024/01/26 19:05:53 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>monstr</rad>/o
</kap>
<drv mrk="monstr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="monstr.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">MIT</uzo>
    <dif>
      <ref tip="super" cel="viv.0ajxo">Viva&jcirc;o</ref> besta a&ubreve; vegeta kun nenormala formo kaj
      strukturo:
      <ekz>
        &ccirc;u mi estas <klr>[&#x2026;]</klr> mara <tld/>o, ke Vi starigis gardon por mi?
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 7:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Jona</nom> estis tri tagojn kaj tri noktojn en la ventro de la mar<tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 12:40</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la maro kaptas la homon teruraj <tld/>oj kun fi&scirc;aj vostoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        la re&gcirc;o montradis al &scirc;i la <klr>[&#x2026;]</klr> birdojn, sova&gcirc;ajn bestojn kaj <tld/>ojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fidela Johano</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hximer.0o.BELA">&hcirc;imero<sncref/></ref>
    </refgrp>
    <trd lng="hu">sz&ouml;rnysz&uuml;l&ouml;tt</trd>
    <trd lng="pl">monstrum</trd>
    <trd lng="ru">&c_u;&c_r;&c_o;&c_d;</trd>
  </snc>
  <snc mrk="monstr.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="est.0ajxo.ulo">Esta&jcirc;o</ref>, kiu mirigas a&ubreve; timigas
      pro siaj ekscesaj, nenormalaj ecoj:
      <ekz>
        la lar&gcirc;a bu&scirc;o de la <tld/>o kuntiri&gcirc;is kaj fari&gcirc;is malgranda kaj ru&gcirc;a
        <klr>[&#x2026;]</klr> kaj jen tie ku&scirc;is denove &scirc;ia propra malgranda bela infano
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la diabloj venis pli kaj pli proksime <klr>[&#x2026;]</klr>, kvieti&gcirc;u, diablaj
        <tld/>oj, li kriis
        <fnt>
          <bib>ElektFab</bib>
          <lok>Kamarado Gaja</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi beda&ubreve;ras vian fraton? ne vi, <tld/>o, vi lin malamas
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>!
      </ekz>
      <ekz>
        tia estas la uzado, kiun tiu malgranda <tld/>o sciis fari je la instruado, kiun
        mi donis al li
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        nokte e&ccirc; <tld/>o estas belulo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; <tld/>on admiras, kiu ame deliras
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="brut.0o.FIG">bruto<sncref/></ref>
    </refgrp>
    <trdgrp lng="hu">
      <trd>sz&ouml;rny</trd>,
      <trd>torzalak</trd>,
      <trd>monstrum</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>monstrum</trd>,
      <trd>potw&oacute;r</trd>,
      <trd>poczwara</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_u;&c_d;&c_o;&c_v;&c_i;&c_shch;&c_je;</trd>,
      <trd>&c_m;&c_o;&c_n;&c_s;&c_t;&c_r;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_o;&c_n;&c_s;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_p;&c_a;&c_ch;&c_v;&c_a;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_ch;&c_u;&c_d;&c_o;&c_v;&c_i;&c_shch;&c_je;</trd>
  <trd lng="ca">monstre</trd>
  <trdgrp lng="cs">
    <trd>netvor</trd>,
    <trd>obluda</trd>,
    <trd>p&rcaron;&iacute;&scaron;era</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Monster</trd>,
    <trd>Ungeheuer</trd>
  </trdgrp>
  <trd lng="en">monster</trd>
  <trd lng="es">monstruo</trd>
  <trd lng="fr">monstre</trd>
  <trdgrp lng="ja">
    <trd>&#x602A;&#x7269;  <pr>&#x304B;&#x3044;&#x3076;&#x3064;</pr></trd>,
    <trd>&#x5316;&#x3051;&#x7269;  <pr>&#x3070;&#x3051;&#x3082;&#x306E;</pr></trd>,
    <trd>&#x602A;&#x7363;  <pr>&#x304B;&#x3044;&#x3058;&#x3085;&#x3046;</pr></trd>,
    <trd>&#x6B8B;&#x5FCD;&#x306A;&#x4EBA;  <pr>&#x3056;&#x3093;&#x306B;&#x3093;&#x306A;&#x3058;&#x3093;</pr></trd>,
    <trd>&#x6975;&#x60AA;&#x4EBA;  <pr>&#x3054;&#x304F;&#x3042;&#x304F;&#x306B;&#x3093;</pr></trd>,
    <trd>&#x4EBA;&#x975E;&#x4EBA;  <pr>&#x306B;&#x3093;&#x3074;&#x306B;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">monster</trd>
  <trd lng="pt">monstro</trd>
  <trdgrp lng="sk">
    <trd>netvor</trd>,
    <trd>obluda</trd>,
    <trd>ohava</trd>,
    <trd>ozruta</trd>,
    <trd>pr&iacute;&scaron;era</trd>
  </trdgrp>
  <trd lng="sv">monster</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&c_t;&c_v;&#x43E;&c_r;&c_a;</trd>,
    <trd>&c_ch;&c_u;&c_d;&#x43E;&c_v;&c_i;&c_s;&#x44C;&#x43A;&#x43E;</trd>,
    <trd>&#x43C;&#x43E;&#x43D;&c_s;&c_t;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x602A;&#x517D; <pr>gu&#xE0;ish&#xF2;u</pr></trd>,
    <trd>&#x602A;&#x7378; <pr>gu&#xE0;ish&#xF2;u</pr></trd>,
    <trd>&#x5996;&#x602A; <pr>y&#x101;ogu&#xE0;i</pr></trd>,
    <trd>&#x602A; <pr>gu&#xE0;i</pr></trd>,
    <trd>&#x7075;&#x5F02; <pr>l&#xED;ngy&#xEC;</pr></trd>,
    <trd>&#x9748;&#x7570; <pr>l&#xED;ngy&#xEC;</pr></trd>,
    <trd>&#x9B54;&#x5934; <pr>m&#xF3;t&#xF3;u</pr></trd>,
    <trd>&#x9B54;&#x982D; <pr>m&#xF3;t&#xF3;u</pr></trd>,
    <trd>&#x602A;&#x7269;&#x4F3C; <pr>gu&#xE0;iw&#xF9;s&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="monstr.0a">
  <kap><tld/>a</kap>
  <snc mrk="monstr.0a.BIO">
    <dif>
      Prezentanta nenormalan formon a&ubreve; strukturon:
      <ekz>
        <tld/>a ido de buldozo kaj buldogino
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a polpo
        <fnt>
          <bib>SkandalJozef</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="anomal.0a">anomalia</ref>
    </refgrp>
    <trdgrp lng="hu">
      <trd>torz</trd>,
      <trd>elfajzott</trd>,
      <trd>torzsz&uuml;l&ouml;tt</trd>
    </trdgrp>
    <trd lng="pl">monstrualny</trd>
  </snc>
  <snc mrk="monstr.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Miriga, timiga a&ubreve; malpla&ccirc;ega pro siaj nenormalaj
      ecoj:
      <ekz>
        &scirc;i ku&scirc;is tie, lace etendita sur la kanapo, ludante kun <tld/>a ventumilo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        tia maldankemo &scirc;ajnas al mi tute <tld/>a
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        a&ubreve;dante la teruran bruon, <klr>[ili]</klr> pensis, ke <tld/>a fantomo venis
        <klr>[kaj]</klr> forkuris timigitaj
        <fnt>
          <bib>ElektFab</bib>
          <lok>La bremenaj muzikistoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        supre, sur la mal&gcirc;oja &ccirc;ielo, rapidis grizaj, <tld/>aj korpoj de la nuboj
        <fnt>
          <bib>Lasta</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi tie murmuras inter la dentoj, kvaza&ubreve; ia <tld/>a sekreto sidus sur via lango
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="terur.0a.FIG">terura<sncref/></ref>
    </refgrp>
    <trdgrp lng="hu">
      <trd>sz&ouml;rny&udblac;</trd>,
      <trd>torz</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>monstrualny</trd>,
      <trd>potworny</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_ch;&c_v;&c_a;&c_r;&c_n;&c_y;</trd>
  <trd lng="ca">monstru&oacute;s</trd>
  <trdgrp lng="cs">
    <trd>nehor&aacute;zn&yacute;</trd>,
    <trd>nestv&uring;rn&yacute;</trd>,
    <trd>obludn&yacute;</trd>,
    <trd>p&rcaron;&iacute;&scaron;ern&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>monstr&ouml;s</trd>,
    <trd>ungeheuer</trd>,
    <trd>abscheulich</trd>
  </trdgrp>
  <trd lng="en">monstrous</trd>
  <trd lng="es">monstruoso</trd>
  <trd lng="fr">monstrueux</trd>
  <trdgrp lng="ja">
    <trd>&#x602A;&#x7269;&#x306E;  <pr>&#x304B;&#x3044;&#x3076;&#x3064;&#x306E;</pr></trd>,
    <trd>&#x5947;&#x602A;&#x306A;  <pr>&#x304D;&#x304B;&#x3044;&#x306A;</pr></trd>,
    <trd>&#x919C;&#x60AA;&#x306A;  <pr>&#x3057;&#x3085;&#x3046;&#x3042;&#x304F;&#x306A;</pr></trd>,
    <trd>&#x6050;&#x308B;&#x3079;&#x304D;  <pr>&#x304A;&#x305D;&#x308B;&#x3079;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="nl">monsterachtig</trd>
  <trd lng="ru">&c_ch;&c_u;&c_d;&c_o;&c_v;&c_i;&c_shch;&c_n;&c_y;&c_j;
  </trd>
  <trdgrp lng="sk">
    <trd>ohavn&yacute;</trd>,
    <trd>ozrutn&yacute;</trd>,
    <trd>potvorsk&yacute;</trd>,
    <trd>pr&iacute;&scaron;ern&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_i;&c_v;&#x43E;&c_v;&c_i;&c_zh;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43F;&#x43E;&c_t;&c_v;&#x43E;&c_r;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_h;&c_ib;&c_t;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x602A;&#x5F02; <pr>gu&#xE0;iy&#xEC;</pr></trd>,
    <trd>&#x602A;&#x7570; <pr>gu&#xE0;iy&#xEC;</pr></trd>,
    <trd>&#x53EF;&#x6015; <pr>k&#x11B;p&#xE0;</pr></trd>
  </trdgrp>
</drv>
</art>
