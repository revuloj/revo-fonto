<vortaro>
  <art mrk="$Id: lim1.xml,v 1.1 2020/07/06 18:33:01 afido Exp $">
    <kap>
      <rad>Lim</rad>/o
    </kap>

<drv mrk="lim1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      La <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&ccirc;efurbo</ref>
      de <ref tip="malprt" cel="peru.P0o">Peruo</ref>:
      <ekz>
        en Limo, &ccirc;efurbo de Peruo, ekzistas bonega privata muzeo pri la anta&ubreve;kolumbaj
        kulturoj en tiu lando
        <fnt>
          <vrk><url ref="https://www.lingq.com/ru/learn-esperanto-online/courses/243802/antaukolumbaj-kulturoj-en-peruo-903093/"
              >Anta&ubreve;kolumbaj Kulturoj en Peruo</url></vrk>
          <lok>Esperanta Retradio 2016</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o havas pli ol ok milionojn da lo&gcirc;antoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=34864"
              >Limo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Lima</trd>
    <trd lng="en">Lima</trd>
    <trd lng="fr">Lima</trd>
  </snc>
  <ref tip="hom" cel="lim.0o">limo</ref>
</drv>

  </art>

