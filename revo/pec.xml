<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pec</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pec.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pec.0o.rompajxo">
    <dif>
      Derompita a&ubreve; detran&ccirc;ita parto de solida korpo:
      <ekz>
        <tld/>o da pano;
      </ekz>
      <ekz>
        <tld/>o de rompita briko;
      </ekz>
      <ekz>
        kuiru tiujn bonajn <tld/>ojn, la lumbojn kaj la
        &scirc;ultrojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        bela <tld/>o da &scirc;tofo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>o de botelo;
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_a;&c_v;&c_a;&c_l;&c_a;&c_k;</trd>,
      <trd>&c_ch;&c_a;&c_s;&c_t;&c_k;&c_a;</trd>,
      <trd>&c_a;&c_b;&c_r;&c_y;&c_v;&c_a;&c_k;</trd>,
      <trd>&c_a;&c_b;&c_l;&c_o;&c_m;&c_a;&c_k;</trd>,
      <trd>&c_f;&c_r;&c_a;&c_g;&c_m;&c_e;&c_n;&c_t;</trd>
    </trdgrp>
    <trd lng="de">St&uuml;ck</trd>
    <trd lng="es">pedazo</trd>
    <trd lng="hu">darab</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_u;&c_s;&c_o;&c_k;</trd>,
      <trd>&c_ch;&c_a;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_l;&c_o;&c_m;&c_o;&c_k;</trd>,
      <trd>&c_o;&c_b;&c_r;&c_y;&c_v;&c_o;&c_k;</trd>,
      <trd>&c_f;&c_r;&c_a;&c_g;&c_m;&c_je;&c_n;&c_t;</trd>
    </trdgrp>
  </snc>
  <snc mrk="pec.0o.ero">
    <dif>
      &Ccirc;iu el la apartigeblaj partoj, kies kunmetado kaj kuni&gcirc;o
      konsistigas kompletan tuton:
      <ekz>
        <tld/>oj de ma&scirc;ino, horlo&gcirc;o, &ccirc;arpenta&jcirc;o,
        jungilo, mozaiko, kolekto;
      </ekz>
      <ekz>
        ili vestas al li &ccirc;iun <tld/>on de la novaj vestoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ni <tld/>o post <tld/>o prezentos al niaj legantoj la tutan
        lingvon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        grandan gravecon havas tiu <tld/>o da tempo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ricevi salajron po<tld/>an, po <tld/>o;
      </ekz>
      <ekz>
        divers<tld/>a vesto.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="elemen.0o">elemento</ref>,
      <ref cel="er.0o">ero</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_ch;&c_a;&c_s;&c_t;&c_k;&c_a;</trd>,
      <trd>&c_d;&c_e;&c_t;&c_a;&c_l;&c_mol;</trd>,
      <trd>&c_f;&c_r;&c_a;&c_g;&c_m;&c_e;&c_n;&c_t;</trd>
    </trdgrp>
    <trd lng="de">Teil</trd>
    <trd lng="es">pieza</trd>
    <trd lng="hu">alkatr&eacute;sz</trd>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_a;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_d;&c_je;&c_t;&c_a;&c_l;&c_mol;</trd>,
      <trd>&c_f;&c_r;&c_a;&c_g;&c_m;&c_je;&c_n;&c_t;</trd>
    </trdgrp>
  </snc>
  <snc mrk="pec.0o.verka">
    <dif>
      Apartigita parto de spirita verko:
      <ekz>
        en la libro estas anka&ubreve; <tld/>oj sistemaj por lerni kaj
        ripeti<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi volas legi al vi tre belan <tld/>on;
      </ekz>
      <ekz>
        <tld/>o de muziko<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_ch;&c_a;&c_s;&c_t;&c_k;&c_a;</trd>,
      <trd>&c_u;&c_r;&c_y;&c_v;&c_a;&c_k;</trd>,
      <trd>&c_f;&c_r;&c_a;&c_g;&c_m;&c_e;&c_n;&c_t;</trd>
    </trdgrp>
    <trd lng="de">St&uuml;ck</trd>
    <trd lng="es">fragmento</trd>
    <trdgrp lng="hu">
      <trd>darab</trd>,
      <trd>r&eacute;szlet</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_a;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_r;&c_y;&c_v;&c_o;&c_k;</trd>,
      <trd>&c_f;&c_r;&c_a;&c_g;&c_m;&c_je;&c_n;&c_t;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>kus</trd>,
    <trd>kousek</trd>
  </trdgrp>
  <trd lng="en">piece</trd>
  <trdgrp lng="fr">
    <trd>&eacute;l&eacute;ment <klr> (d'un tout)</klr></trd>,
    <trd>morceau</trd>,
    <trd>partie</trd>,
    <trd>passage<klr> (d'une oeuvre)</klr></trd>,
    <trd>pi&egrave;ce<klr tip="amb"> (morceau)</klr></trd>,
  </trdgrp>
  <trd lng="nl">stuk</trd>
  <trdgrp lng="pt">
    <trd>peda&ccedil;o</trd>,
    <trd>bocado</trd>,
    <trd>naco</trd>,
    <trd>pe&ccedil;a</trd>,
    <trd>lan&ccedil;o</trd>,                
    <trd>trecho</trd>,
    <trd>caco</trd>    
  </trdgrp>
</drv>

<drv mrk="pec.0eto">
  <kap><tld/>eto</kap>
  <snc>
    <dif>
      Malgranda, sensignifa derompa&jcirc;o a&ubreve; ero:
      <ekz>
        dis&scirc;iri al <tld/>etoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>eton de sia koro &scirc;i povas perdi en la vojo
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>fragment</trd>,
    <trd>parcelle</trd>
  </trdgrp>
  <trd lng="nl">stukje</trd>
</drv>

<drv mrk="pec.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malkunigi la partojn de a&jcirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_a;&c_b;&c_r;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_n;&c_je;&c_sh;&c_t;&c_a; &c_n;&c_a;
  &c_ch;&c_a;&c_s;&c_t;&c_k;&c_ib;)</klr></trd>
  <trd lng="es">fragmentar</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;sosser<klr> (fig.)</klr></trd>,
    <trd>mettre en pi&egrave;ces</trd>
  </trdgrp>
  <trd lng="hu">darabol</trd>
  <trd lng="nl">in <ind>stuk</ind>ken doen</trd>
</drv>

<drv mrk="pec.busx0o">
  <kap>bu&scirc;<tld/>o</kap> <fnt><bib>MuzTerm</bib></fnt>
  <snc mrk="pec.busx0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Parto de
      <ref tip="malprt" cel="instru2.blov0o">blovinstrumento</ref>
      tenata en a&ubreve; &ccirc;e la bu&scirc;o.
    </dif>
    <refgrp tip="vid">
      <ref cel="ancx.0o">an&ccirc;o</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_a;&c_m;&c_b;&c_u;&c_sh;&c_u;&c_r;</trd>,
      <trd>&c_m;&c_u;&c_sh;&c_t;&c_u;&c_k; <klr tip="amb">(&c_m;&c_u;&c_z;&c_y;&c_ch;&c_n;&c_a;&c_g;&c_a;
      &c_ib;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_e;&c_n;&c_t;&c_a;)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>bec<klr> (d'un instrument de musique)</klr></trd>,
      <trd>embouchure <klr> (d'un instrument de musique)</klr></trd>
    </trdgrp>
    <trd lng="hu">f&uacute;v&oacute;ka</trd>
    <trd lng="nl">mondstuk</trd>
  </snc>
</drv>

<drv mrk="pec.dis0igi">
  <kap>dis<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Disigi en <tld/>ojn:
      <ekz>
        dis<tld/>igi &scirc;tonon, vazon, paperon;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        estas doma&gcirc;e, ke per tiuj &ccirc;i bagateloj li dis<tld/>igas
        sian grandan talenton.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_b;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_l;&c_a;&c_m;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_a;&c_r;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_ch;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">zerst&uuml;ckeln</trd>
  <trd lng="es">despedazar</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;chiqueter</trd>,
    <trd>d&eacute;chirer en mille morceaux</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&eacute;tdarabol</trd>,
    <trd>sz&eacute;tszed</trd>
  </trdgrp>
  <trd lng="nl">verbrijzelen</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_d;&c_je;&c_l;&c_i;&c_t;&c_mol; &c_n;&c_a;
    &c_ch;&c_a;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_b;&c_i;&c_t;&c_mol; &c_n;&c_a;
    &c_k;&c_u;&c_s;&c_k;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="pec.sxir0o">
  <kap>&scirc;ir<tld/>o</kap>
  <snc mrk="pec.sxir0o.KOMUNE">
    <dif>
      <tld lit="P"/>o de&scirc;irita de tuto,
      <ref tip="vid" cel="fragme.0o">fragmento</ref>:
      <ekz>
        de tiuj domoj
        restis nenio
        krom iu
        &scirc;ir<tld/>o de muro        
        <fnt>
          <aut>G. Ungaretti, trad. H. Tylipska</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/35492/150033">
          Sankta Martino en Karso</url></vrk>,
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="stump.0o">stumpo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>d&eacute;bris</trd>,
    <trd>fragment</trd>
  </trdgrp>
</drv>
</art>
