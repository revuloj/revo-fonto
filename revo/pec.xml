<drv mrk="pec.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pec.0o.rompajxo">
    <dif>
      Derompita a&ubreve; detran&ccirc;ita <ref tip="super" cel="part.0o.KOMUNE">parto</ref>
      de solida korpo:
      <ekz>
        <tld/>o da pano
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 18:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        kolekti <klr>[pecojn]</klr> da pajlo
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 5:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bruloferon <klr>[&#8230;]</klr> dishaku &gcirc;in en <tld/>ojn
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 1:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iujn bonajn pecojn, la lumbojn kaj &scirc;ultrojn <klr>[&#8230;]</klr> boligu bone
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 24:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        kavigita <tld/>o da froma&gcirc;o
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la kelon estis enportita tuta fasko da vakskandelaj stumpetoj, la grandajn
        <tld/>ojn oni bruligis
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova strata lanterno</lok>
        </fnt>;
      </ekz>
      <ekz>
        krevinta en mil <tld/>ojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        briletantaj <tld/>oj da glacio
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        alkro&ccirc;ita &ccirc;e rompita <tld/>o el masto
        <fnt>
          <bib>Kandid</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_a;&c_v;&c_a;&c_l;&c_a;&c_k;</trd>,
      <trd>&c_ch;&c_a;&c_s;&c_t;&c_k;&c_a;</trd>,
      <trd>&c_a;&c_b;&c_r;&c_y;&c_v;&c_a;&c_k;</trd>,
      <trd>&c_a;&c_b;&c_l;&c_o;&c_m;&c_a;&c_k;</trd>,
      <trd>&c_f;&c_r;&c_a;&c_g;&c_m;&c_e;&c_n;&c_t;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Bruchst&uuml;ck</trd>,
      <trd>Brocken</trd>
    </trdgrp>
    <trd lng="es">pedazo</trd>
    <trd lng="hu">darab</trd>
    <trdgrp lng="id">
      <trd><baz>bagi</baz>bagian</trd>,
      <trd><baz>potong</baz>potongan</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_u;&c_s;&c_o;&c_k;</trd>,
      <trd>&c_ch;&c_a;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_l;&c_o;&c_m;&c_o;&c_k;</trd>,
      <trd>&c_o;&c_b;&c_r;&c_y;&c_v;&c_o;&c_k;</trd>,
      <trd>&c_f;&c_r;&c_a;&c_g;&c_m;&c_je;&c_n;&c_t;</trd>
    </trdgrp>
  </snc>
  <snc mrk="pec.0o.ero">
    <dif>
      &Ccirc;iu el la apartigeblaj partoj, kies kunmetado kaj kuni&gcirc;o
      konsistigas kompletan <ref tip="malprt" cel="tut.0o">tuto</ref>n:
      <ekz>
        <tld/>oj de ma&scirc;ino, horlo&gcirc;o, &ccirc;arpenta&jcirc;o,
        jungilo, mozaiko, kolekto;
      </ekz>
      <ekz>
        ili tramar&scirc;is <tld/>on de la vojo
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi donas al vi unu <tld/>on da lando
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 48:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili vestas al li &ccirc;iun <tld/>on de la novaj vestoj
        <fnt>
          <bib>Fab1</bib>
          <lok>La novaj vestoj de la re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        li priskribis <tld/>on post <tld/>o <klr>[&scirc;ian]</klr> nunan aspekton
        <fnt>
          <bib>RBa</bib>
        </fnt>;
      </ekz>
      <ekz>
        la lasta <tld/>o, la zigzaga monta supreniro <klr>[&#8230;]</klr> estas malrapide
        farebla
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ok jaroj estas granda <tld/>o da tempo
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista en Boulogne sur Mer en la 5a de a&ubreve;gusto
          1905</vrk>
        </fnt>;
      </ekz>
      <ekz>
        al <tld/>' <tld/>on algluas, kiu neston konstruas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="elemen.0o">elemento</ref>,
      <ref cel="er.0o">ero</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_ch;&c_a;&c_s;&c_t;&c_k;&c_a;</trd>,
      <trd>&c_d;&c_e;&c_t;&c_a;&c_l;&c_mol;</trd>,
      <trd>&c_f;&c_r;&c_a;&c_g;&c_m;&c_e;&c_n;&c_t;</trd>
    </trdgrp>
    <trd lng="de">Teil</trd>
    <trd lng="es">pieza</trd>
    <trd lng="hu">alkatr&eacute;sz</trd>
    <trd lng="id"><baz>bagi</baz>bagian</trd>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_a;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_d;&c_je;&c_t;&c_a;&c_l;&c_mol;</trd>,
      <trd>&c_f;&c_r;&c_a;&c_g;&c_m;&c_je;&c_n;&c_t;</trd>
    </trdgrp>
  </snc>
  <snc mrk="pec.0o.verka">
    <dif>
      Apartigita parto de spirita <ref tip="malprt" cel="verk.0o.propra">verko</ref>:
      <ekz>
        tridek-tri fojojn &gcirc;i kantis tiun saman <tld/>on
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la libro estos anka&ubreve; <tld/>oj sistemaj, por lerni kaj ripeti
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        diligente Japanoj kutimas traduki mallongajn literaturajn <tld/>ojn, &ccirc;efe poemojn
        <fnt>
          <bib>EeP</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fragme.0o">fragmento</ref>,
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_ch;&c_a;&c_s;&c_t;&c_k;&c_a;</trd>,
      <trd>&c_u;&c_r;&c_y;&c_v;&c_a;&c_k;</trd>,
