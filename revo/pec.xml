<drv mrk="pec.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pec.0o.rompajxo">
    <dif>
      Derompita a&ubreve; detran&ccirc;ita parto de solida korpo:
      <ekz>
        <tld/>o da pano;
      </ekz>
      <ekz>
        <tld/>o de rompita briko;
      </ekz>
      <ekz>
        kuiru tiujn bonajn <tld/>ojn, la lumbojn kaj la
        &scirc;ultrojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        bela <tld/>o da &scirc;tofo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>o de botelo;
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_a;&c_v;&c_a;&c_l;&c_a;&c_k;</trd>,
      <trd>&c_ch;&c_a;&c_s;&c_t;&c_k;&c_a;</trd>,
      <trd>&c_a;&c_b;&c_r;&c_y;&c_v;&c_a;&c_k;</trd>,
      <trd>&c_a;&c_b;&c_l;&c_o;&c_m;&c_a;&c_k;</trd>,
      <trd>&c_f;&c_r;&c_a;&c_g;&c_m;&c_e;&c_n;&c_t;</trd>
    </trdgrp>
    <trd lng="de">St&uuml;ck</trd>
    <trd lng="es">pedazo</trd>
    <trd lng="hu">darab</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_u;&c_s;&c_o;&c_k;</trd>,
      <trd>&c_ch;&c_a;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_l;&c_o;&c_m;&c_o;&c_k;</trd>,
      <trd>&c_o;&c_b;&c_r;&c_y;&c_v;&c_o;&c_k;</trd>,
      <trd>&c_f;&c_r;&c_a;&c_g;&c_m;&c_je;&c_n;&c_t;</trd>
    </trdgrp>
  </snc>
  <snc mrk="pec.0o.ero">
    <dif>
      &Ccirc;iu el la apartigeblaj partoj, kies kunmetado kaj kuni&gcirc;o
      konsistigas kompletan tuton:
      <ekz>
        <tld/>oj de ma&scirc;ino, horlo&gcirc;o, &ccirc;arpenta&jcirc;o,
        jungilo, mozaiko, kolekto;
      </ekz>
      <ekz>
        ili vestas al li &ccirc;iun <tld/>on de la novaj vestoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ni <tld/>o post <tld/>o prezentos al niaj legantoj la tutan
        lingvon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        grandan gravecon havas tiu <tld/>o da tempo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ricevi salajron po<tld/>an, po <tld/>o;
      </ekz>
      <ekz>
        divers<tld/>a vesto.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="elemen.0o">elemento</ref>,
      <ref cel="er.0o">ero</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_ch;&c_a;&c_s;&c_t;&c_k;&c_a;</trd>,
      <trd>&c_d;&c_e;&c_t;&c_a;&c_l;&c_mol;</trd>,
      <trd>&c_f;&c_r;&c_a;&c_g;&c_m;&c_e;&c_n;&c_t;</trd>
    </trdgrp>
    <trd lng="de">Teil</trd>
    <trd lng="es">pieza</trd>
    <trd lng="hu">alkatr&eacute;sz</trd>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_a;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_d;&c_je;&c_t;&c_a;&c_l;&c_mol;</trd>,
      <trd>&c_f;&c_r;&c_a;&c_g;&c_m;&c_je;&c_n;&c_t;</trd>
    </trdgrp>
  </snc>
  <snc mrk="pec.0o.verka">
    <dif>
      Apartigita parto de spirita verko:
      <ekz>
        en la libro estas anka&ubreve; <tld/>oj sistemaj por lerni kaj
        ripeti<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi volas legi al vi tre belan <tld/>on;
      </ekz>
      <ekz>
        <tld/>o de muziko<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_ch;&c_a;&c_s;&c_t;&c_k;&c_a;</trd>,
      <trd>&c_u;&c_r;&c_y;&c_v;&c_a;&c_k;</trd>,
      <trd>&c_f;&c_r;&c_a;&c_g;&c_m;&c_e;&c_n;&c_t;</trd>
    </trdgrp>
    <trd lng="de">St&uuml;ck</trd>
    <trd lng="es">fragmento</trd>
    <trdgrp lng="hu">
      <trd>darab</trd>,
      <trd>r&eacute;szlet</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_a;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_r;&c_y;&c_v;&c_o;&c_k;</trd>,
      <trd>&c_f;&c_r;&c_a;&c_g;&c_m;&c_je;&c_n;&c_t;</trd>
    </trdgrp>
  </snc>
  <trd lng="af">stukkie</trd>
  <trd lng="am">&#x1241;&#x122B;&#x132D;</trd>
  <trd lng="ar">&#x642;&#x637;&#x639;&#x629;</trd>
  <trd lng="az">par&#xE7;a</trd>
  <trd lng="bn">&#x99F;&#x9C1;&#x995;&#x9B0;&#x9BE;</trd>
  <trd lng="bo">&#xF62;&#xFA1;&#xF7C;&#xF42;&#xF0B;&#xF62;&#xFA1;&#xF7C;&#xF42;&#xF0B;</trd>
  <trd lng="bs">komad</trd>
  <trd lng="co">pezzu</trd>
  <trdgrp lng="cs">
    <trd>kousek</trd>,
    <trd>kus</trd>,
    <trd>v&ecaron;c</trd>,
    <trd>&ccaron;&aacute;st celku</trd>
  </trdgrp>
  <trd lng="da">stykke</trd>
  <trd lng="en">piece</trd>
  <trd lng="et">t&#xF6;&#xF6;</trd>
  <trd lng="eu">pieza</trd>
  <trdgrp lng="fr">
    <trd>&eacute;l&eacute;ment <klr> (d'un tout)</klr></trd>,
    <trd>morceau</trd>,
    <trd>partie</trd>,
    <trd>passage<klr> (d'une oeuvre)</klr></trd>,
    <trd>pi&egrave;ce<klr tip="amb"> (morceau)</klr></trd>,
  </trdgrp>
  <trd lng="fy">stik</trd>
  <trd lng="gl">peza</trd>
  <trd lng="gu">&#xAAD;&#xABE;&#xA97;</trd>
  <trd lng="ha">yanki</trd>
  <trd lng="hi">&#x91F;&#x941;&#x915;&#x921;&#x93C;&#x93E;</trd>
  <trd lng="hr">komad</trd>
  <trd lng="hy">&#x56F;&#x57F;&#x578;&#x580;</trd>
  <trd lng="is">stykki</trd>
  <trd lng="ja">&#x4F5C;&#x54C1;</trd>
  <trd lng="ka">&#x10EA;&#x10D0;&#x10DA;&#x10D8;</trd>
  <trd lng="km">&#x178A;&#x17BB;&#x17C6;</trd>
  <trd lng="ko">&#xC870;&#xAC01;</trd>
  <trd lng="lo">&#xEAA;&#xEB4;&#xEC9;&#xE99;</trd>
  <trd lng="ml">&#xD15;&#xD37;&#xD23;&#xD02;</trd>
  <trd lng="mr">&#x924;&#x941;&#x915;&#x921;&#x93E;</trd>
  <trd lng="my">&#x1021;&#x1015;&#x102D;&#x102F;&#x1004;&#x103A;&#x1038;&#x1021;&#x1005;</trd>
  <trd lng="ne">&#x91F;&#x941;&#x915;&#x94D;&#x930;&#x93E;</trd>
  <trd lng="nl">stuk</trd>
  <trd lng="ny">chidutswa</trd>
  <trd lng="pa">&#xA1F;&#xA41;&#xA15;&#xA5C;&#xA47;</trd>
  <trd lng="ps">&#x67C;&#x648;&#x67C;&#x647;</trd>
  <trdgrp lng="pt">
    <trd>peda&ccedil;o</trd>,
    <trd>bocado</trd>,
    <trd>naco</trd>,
    <trd>pe&ccedil;a</trd>,
    <trd>lan&ccedil;o</trd>,                
    <trd>trecho</trd>,
    <trd>caco</trd>    
  </trdgrp>
  <trd lng="sd">&#x67D;&#x6AA;&#x631;&#x648;</trd>
