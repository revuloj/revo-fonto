  <trd lng="yi">&#x5E9;&#x5D8;&#x5D9;&#x5E7;</trd>
  <trd lng="zu">ucezu</trd>
</drv>

<drv mrk="pec.0eto">
  <kap><ofc>*</ofc><tld/>eto</kap>
  <snc>
    <dif>
      Malgranda, sensignifa derompa&jcirc;o a&ubreve; ero:
      <ekz>
        dis&scirc;iri al <tld/>etoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
      Mi dis&scirc;iris la leteron kaj dis&jcirc;etis &gcirc;iajn <tld/>etojn en &ccirc;iujn
      angulojn de la &ccirc;ambro.
      <fnt><bib>F</bib>, <lok>&FE; 42</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>eton de sia koro &scirc;i povas perdi en la vojo
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>kousek</trd>,
    <trd>kous&iacute;&ccaron;ek</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fragment</trd>,
    <trd>parcelle</trd>
  </trdgrp>
  <trd lng="nl">stukje</trd>
  <trd lng="sk">k&uacute;sok</trd>
</drv>

<drv mrk="pec.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malkunigi la partojn de a&jcirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_a;&c_b;&c_r;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_n;&c_je;&c_sh;&c_t;&c_a; &c_n;&c_a;
  &c_ch;&c_a;&c_s;&c_t;&c_k;&c_ib;)</klr></trd>
  <trdgrp lng="cs">
    <trd>drtit</trd>,
    <trd>ml&iacute;t</trd>,
    <trd>rozb&iacute;t na kusy</trd>,
    <trd>rozkouskovat</trd>,
    <trd>rozm&ecaron;l&ncaron;ovat</trd>,
    <trd>&scaron;rotovat</trd>
  </trdgrp>
  <trd lng="es">fragmentar</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;sosser<klr> (fig.)</klr></trd>,
    <trd>mettre en pi&egrave;ces</trd>
  </trdgrp>
  <trd lng="hu">darabol</trd>
  <trd lng="nl">in <ind>stuk</ind>ken doen</trd>
  <trdgrp lng="sk">
    <trd>drvi&tcaron;</trd>,
    <trd>rozbi&tcaron; na kusy</trd>
  </trdgrp>
</drv>

<drv mrk="pec.busx0o">
  <kap>bu&scirc;<tld/>o</kap> <fnt><bib>MuzTerm</bib></fnt>
  <snc mrk="pec.busx0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Parto de
      <ref tip="malprt" cel="instru2.blov0o">blovinstrumento</ref>
      tenata en a&ubreve; &ccirc;e la bu&scirc;o.
    </dif>
    <refgrp tip="vid">
      <ref cel="ancx.0o">an&ccirc;o</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_a;&c_m;&c_b;&c_u;&c_sh;&c_u;&c_r;</trd>,
      <trd>&c_m;&c_u;&c_sh;&c_t;&c_u;&c_k; <klr tip="amb">(&c_m;&c_u;&c_z;&c_y;&c_ch;&c_n;&c_a;&c_g;&c_a;
      &c_ib;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_e;&c_n;&c_t;&c_a;)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>bec<klr> (d'un instrument de musique)</klr></trd>,
      <trd>embouchure <klr> (d'un instrument de musique)</klr></trd>
    </trdgrp>
    <trd lng="hu">f&uacute;v&oacute;ka</trd>
    <trd lng="nl">mondstuk</trd>
  </snc>
</drv>

<drv mrk="pec.dis0igi">
  <kap>dis<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Disigi en <tld/>ojn:
      <ekz>
        dis<tld/>igi &scirc;tonon, vazon, paperon;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        estas doma&gcirc;e, ke per tiuj &ccirc;i bagateloj li dis<tld/>igas
        sian grandan talenton.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_b;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_l;&c_a;&c_m;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_a;&c_r;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_sh;&c_ch;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>drtit</trd>,
    <trd>ml&iacute;t</trd>,
    <trd>rozb&iacute;t na kusy</trd>,
    <trd>rozkouskovat</trd>,
    <trd>rozm&ecaron;l&ncaron;ovat</trd>,
    <trd>&scaron;rotovat</trd>
  </trdgrp>
  <trd lng="de">zerst&uuml;ckeln</trd>
  <trd lng="es">despedazar</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;chiqueter</trd>,
    <trd>d&eacute;chirer en mille morceaux</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&eacute;tdarabol</trd>,
    <trd>sz&eacute;tszed</trd>
  </trdgrp>
  <trd lng="nl">verbrijzelen</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_d;&c_je;&c_l;&c_i;&c_t;&c_mol; &c_n;&c_a;
    &c_ch;&c_a;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_b;&c_i;&c_t;&c_mol; &c_n;&c_a;
    &c_k;&c_u;&c_s;&c_k;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>drobi&tcaron;</trd>,
    <trd>rozdrvi&tcaron;</trd>,
    <trd>rozk&uacute;skova&tcaron;</trd>,
    <trd>roztrieska&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="pec.sxir0o">
  <kap>&scirc;ir<tld/>o</kap>
  <snc mrk="pec.sxir0o.KOMUNE">
    <dif>
      <tld lit="P"/>o de&scirc;irita de tuto,
      <ref tip="vid" cel="fragme.0o">fragmento</ref>,
      <ref tip="vid" cel="stump.0o">stumpo</ref>:
      <ekz>
        de tiuj domoj
        restis nenio
        krom iu
        &scirc;ir<tld/>o de muro        
        <fnt>
          <aut>G. Ungaretti, trad. H. Tylipska</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/35492/150033">
          Sankta Martino en Karso</url></vrk>,
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>d&eacute;bris</trd>,
    <trd>fragment</trd>,
    <trd>parcelle</trd>
  </trdgrp>
</drv>

<drv mrk="pec.unu0a">
  <kap>unu<tld/>a</kap>
  <snc mrk="pec.unu0a.KOMUNE">
    <dif>
      Konsistanta el nur unu <tld/>o.
    </dif>
    <refgrp tip="vid">
      <ref cel="kompak.0a.sentrua">kompakta<sncref/></ref>,
      <ref cel="kontin3.0a.KOMUNE">kontinua</ref>
    </refgrp>
  </snc>
  <trd lng="br">en ur <ind>pezh</ind></trd>
  <trdgrp lng="ca">
    <trd>compacte</trd>,
    <trd>consistent</trd>
  </trdgrp>
  <trd lng="de">einteilig</trd>
  <trdgrp lng="fi">
    <trd>yht&auml; kappaletta (oleva)</trd>,
    <trd>saumaton</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d'une <ind>pi&egrave;ce</ind></trd>,
    <trd>monolithique<klr> (fig.)</klr></trd>
  </trdgrp>
  <trd lng="hu">egyr&eacute;szes</trd>
  <trd lng="it">monopezzo</trd>
  <trd lng="nl">eendelig</trd>
  <trdgrp lng="ru">
    <trd>&c_c;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_m;&c_o;&c_n;&c_o;&c_l;&c_i;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
