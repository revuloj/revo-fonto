<drv mrk="pec.0eto">
  <kap><ofc>*</ofc><tld/>eto</kap>
  <snc mrk="pec.0eto.KOMUNE">
    <dif>
      Malgranda, sensignifa derompa&jcirc;o a&ubreve; <ref tip="sin" cel="er.0o">ero</ref>:
      <ekz>
        <tld/>eto da vitro
        <fnt>
          <bib>Fab2</bib>
          <lok>Flikilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ver&scirc;as akvon <klr>[&#8230;]</klr> sur <tld/>eton da &scirc;tofo
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        dis&scirc;iri al <tld/>etoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
      Mi dis&scirc;iris la leteron kaj dis&jcirc;etis &gcirc;iajn <tld/>etojn en &ccirc;iujn
      angulojn de la &ccirc;ambro.
      <fnt><bib>F</bib> <lok>&FE; 42</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>eton de sia koro &scirc;i povas perdi en la vojo
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_v;&c_a;&c_l;&c_a;&c_ch;&c_a;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>kousek</trd>,
    <trd>kous&iacute;&ccaron;ek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>St&uuml;ckchen</trd>,
    <trd>Splitter</trd>,
    <trd>Br&ouml;sel</trd>
  </trdgrp><trdgrp lng="fr">
    <trd>fragment</trd>,
    <trd>parcelle</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>serpih</baz>serpihan</trd>,
    <trd><ind>remah</ind>-remah</trd>
  </trdgrp>
  <trdgrp lng="ja">
