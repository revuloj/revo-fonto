<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pec</rad>/o
</kap>
<drv mrk="pec.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pec.0o.rompajxo">
    <dif>
      Derompita a&ubreve; detran&ccirc;ita parto de solida korpo:
      <ekz>
        <tld/>o da pano;
      </ekz>
      <ekz>
        <tld/>o de rompita briko;
      </ekz>
      <ekz>
        kuiru tiujn bonajn <tld/>ojn, la lumbojn kaj la
        &scirc;ultrojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        bela <tld/>o da &scirc;tofo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>o de botelo;
      </ekz>
      <ekz>
        dis&scirc;iri al <tld/>etoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fragme.0o">fragmento</ref>.
    </refgrp>
    <trd lng="de">St&uuml;ck</trd>
    <trdgrp lng="fr">
      <trd>morceau</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_u;&c_s;&c_o;&c_k;</trd>,
      <trd>&c_ch;&c_a;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_l;&c_o;&c_m;&c_o;&c_k;</trd>,
      <trd>&c_o;&c_b;&c_r;&c_y;&c_v;&c_o;&c_k;</trd>,
      <trd>&c_f;&c_r;&c_a;&c_g;&c_m;&c_je;&c_n;&c_t;</trd>
    </trdgrp>
  </snc>
  <snc mrk="pec.0o.ero">
    <dif>
      &Ccirc;iu el la apartigeblaj partoj, kies kunmetado kaj kuni&gcirc;o
      konsistigas kompletan tuton:
      <ekz>
        <tld/>oj de ma&scirc;ino, horlo&gcirc;o, &ccirc;arpenta&jcirc;o,
        jungilo, mozaiko, kolekto;
      </ekz>
      <ekz>
        ili vestas al li &ccirc;iun <tld/>on de la novaj vestoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ni <tld/>o post <tld/>o prezentos al niaj legantoj la tutan
        lingvon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        grandan gravecon havas tiu <tld/>o da tempo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ricevi salajron po<tld/>an, po <tld/>o;
      </ekz>
      <ekz>
        divers<tld/>a vesto.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="er.0o">ero</ref>,
      <ref cel="elemen.0o">elemento</ref>.
    </refgrp>
    <trd lng="de">Teil</trd>
    <trdgrp lng="fr">
      <trd>&eacute;l&eacute;ment <klr> (d'un tout)</klr></trd>,
      <trd>partie</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_a;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_d;&c_je;&c_t;&c_a;&c_l;&c_mol;</trd>,
      <trd>&c_f;&c_r;&c_a;&c_g;&c_m;&c_je;&c_n;&c_t;</trd>
    </trdgrp>
  </snc>
  <snc mrk="pec.0o.verka">
    <dif>
      Apartigita parto de spirita verko:
      <ekz>
        en la libro estas anka&ubreve; <tld/>oj sistemaj por lerni kaj
        ripeti<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi volas legi al vi tre belan <tld/>on;
      </ekz>
      <ekz>
        <tld/>o de muziko<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">St&uuml;ck</trd>
    <trdgrp lng="fr">
      <trd>passage<klr> (d'une oeuvre)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_a;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_r;&c_y;&c_v;&c_o;&c_k;</trd>,
      <trd>&c_f;&c_r;&c_a;&c_g;&c_m;&c_je;&c_n;&c_t;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>kus</trd>,
    <trd>kousek</trd>
  </trdgrp>
  <trd lng="en">piece</trd>
  <trd lng="nl">stuk</trd>
</drv>

<drv mrk="pec.busx0o">
  <kap>bu&scirc;<tld/>o</kap> <fnt><bib>MuzTerm</bib></fnt>
  <snc>
    <dif>
      Parto de
      <ref tip="malprt" cel="instru2.blov0o">blovinstrumento</ref>
      tenata en a&ubreve; &ccirc;e la bu&scirc;o.
    </dif>
    <refgrp tip="vid">
      <ref cel="ancx.0o">an&ccirc;o</ref>
    </refgrp>
    <trdgrp lng="fr">
      <trd>bec<klr> (d'un instrument de musique)</klr></trd>,
      <trd>embouchure <klr> (d'un instrument de musique)</klr></trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="pec.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malkunigi la partojn de a&jcirc;o.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>d&eacute;sosser<klr> (fig.)</klr></trd>,
    <trd>mettre en pi&egrave;ces</trd>
  </trdgrp>
</drv>

<drv mrk="pec.dis0igi">
  <kap>dis<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Disigi en <tld/>ojn:
      <ekz>
        dis<tld/>igi &scirc;tonon, vazon, paperon;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        estas doma&gcirc;e, ke per tiuj &ccirc;i bagateloj li dis<tld/>igas
        sian grandan talenton.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">zerst&uuml;ckeln</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;chiqueter</trd>,
    <trd>d&eacute;chirer en mille morcea&ubreve;</trd>
  </trdgrp>
  <trd lng="nl">verbrijzelen</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_d;&c_je;&c_l;&c_i;&c_t;&c_mol; &c_n;&c_a;
&c_ch;&c_a;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_b;&c_i;&c_t;&c_mol; &c_n;&c_a;
&c_k;&c_u;&c_s;&c_k;&c_i;</trd>
  </trdgrp>
</drv>
</art>
