<drv mrk="pec.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pec.0igi.KOMUNE">
    <dif>
      Malkunigi la partojn de a&jcirc;o:
      <ekz>
        <tld/>igita karno
        <fnt>
          <bib>SkandalJozef</bib>
        </fnt>;
      </ekz>
      <ekz>
        pasto el <klr>[&#8230;]</klr> <tld/>igita pano, lakto, ovoj
        <fnt>
          <bib>Monato</bib>
          <aut>Petro De Smet'</aut>
          <vrk><url ref="&Monato;/2008/009292.php">Sidante en pudingo, mi alvokas miajn
          savantojn!</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i perdis preska&ubreve; &ccirc;iujn dentojn, do oni donas al &scirc;i bulojn, kiuj estas faritaj
        el <tld/>igitaj junaj herboj
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Yasuo</aut>
          <vrk><url ref="&Monato;/2003/008142.html">Elefantoj ne forgesas ... kaj ne
          forgesitas</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
