  <trd lng="ny">mwana wankhuku</trd>
  <trd lng="pa">&#2606;&#2591;&#2608;&#2624;</trd>
  <trd lng="ps">&a_l;&f_p;&a_h;</trd>
  <trd lng="sd">&a_n;&a_w;&a_t;</trd>
  <trd lng="si">&#3482;&#3537;&#3517;&#3530;&#3517;</trd>
  <trdgrp lng="sk">
    <trd>chlap</trd>,
    <trd>indiv&iacute;duum</trd>,
    <trd>osoba</trd>
  </trdgrp>
  <trd lng="sq">zogth</trd>
  <trd lng="ta">&#2965;&#3009;&#2974;&#3021;&#2970;&#3009;</trd>
  <trd lng="te">&#3098;&#3135;&#3093;&#3149;</trd>
  <trd lng="th">&#3648;&#3592;&#3637;&#3658;&#3618;&#3610;</trd>
  <trd lng="tp">jan</trd>
  <trd lng="ur">&a_l;&#1681;&f_k;&f_y;</trd>
