<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ul.xml,v 1.52 2021/06/16 17:00:29 revo Exp $">
<kap>
     <ofc>*</ofc>
     -<rad>ul</rad>/
</kap>
<subart>
<drv mrk="ul.0">
 <kap>-<tld/></kap>
 <snc>
   <dif>
     <ref tip="super" cel="sufiks.0o">Sufikso</ref> signifanta vivantan <ref tip="super" cel="est.0ajxo">esta&jcirc;o</ref>n 
     karakterizatan per tio, kion esprimas la radiko:
     <ekz>
       <ref cel="gxib.0ulo">&gcirc;ib<tld/>o</ref>, <ref cel="virg.0ulino">virgulino</ref>,
       <ref cel="tim.0ulo">tim<tld/>o</ref>, <ref cel="ricx.0ulo">ri&ccirc;ulo</ref>,
       <ref cel="proksi.0ulo">proksimulo</ref>, <ref cel="kompet.0ulo">kompetentulo</ref>,
       <ref cel="tiam.0ulo">tiam<tld/>o</ref>;
     </ekz>
      <ekz>
        prav<tld/>o <klr>[kaj]</klr> malprav<tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 9:27</lok>
        </fnt>;
      </ekz>
      <ekz>
         <ind>unuokul<tld/>o</ind>,
         <trd lng="fi">silm&auml;puoli</trd>
         <trd lng="fy">ieneach</trd>
         <trd lng="hu">f&eacute;lszem&udblac; <klr tip="amb">(ember)</klr></trd>
         <trd lng="nl">eenoog</trd>
         <trd lng="ru">&c_o;&c_d;&c_n;&c_o;&c_g;&c_l;&c_a;&c_z;&c_y;&c_j; <klr tip="amb">(&c_s;&c_u;&c_shch;.)</klr></trd>
      </ekz>
      <ekz>
       multinfan<tld/>ino
        <fnt>
          <bib>MT</bib>
          <lok>I. samuel 2:5</lok>
        </fnt>;
      </ekz>
      <ekz>
       <ref cel="mam.0uloj">mam<tld/>oj</ref>,
       <ref cel="verteb.0ulo">vertebr<tld/>o</ref>
      </ekz>
      <ekz>
        monkolekto pro la sankt<tld/>oj
        <fnt>
          <bib>NT</bib>
          <lok>I. Korintanoj 16:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        sekreton de ali<tld/>o ne malka&scirc;u
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 25:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi ne estas &ccirc;itie<tld/>o
        <fnt>
          <bib>Gmn</bib>
        </fnt>;
      </ekz>
      <ekz>
       mi estas sciencisto, sed mi ne pretendas esti
       <ref cel="scienc.0ulo">scienc<tld/>o</ref>;
      </ekz>
      <ekz>
       la vivo kaj kompetent<tld/>a uzado fiksis
       <klr>[&#8230;]</klr> alian formon
        <fnt>
          <bib>LR</bib>
          <lok>4. Vortaroj</lok>
        </fnt>;
     </ekz>
      <ekz>
        li devas fari la kutimajn jun<tld/>ajn malsa&gcirc;a&jcirc;ojn
        <fnt>
          <bib>Fab4</bib>
          <lok>Sterkskarabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li imagis lin &ccirc;iurilate mez<tld/>o
        <klr>(homo de meza talento ktp)</klr>;
        <fnt>
          <bib>ChC</bib>
        </fnt>;
      </ekz>
    <ekz>
      grandaj mirakloj aperis per la trovita malviva infano, kiun la eklezio fine e&ccirc;
      sankt<tld/>igis
      <fnt>
        <bib>RBa</bib>
      </fnt>.
    </ekz>
   </dif>
  </snc>
  <trd lng="de"><klr>-</klr>er</trd>
  <trdgrp lng="fi">
    <trd>olio<klr tip="amb"> (sananjohdin)</klr><klr>, 
      yleens&auml; ihminen tai el&auml;in
      jolla on kantasanan ilmoittama ominaisuus</klr></trd>
  </trdgrp>
  <trd lng="hu">ember</trd>
  <trd lng="vo">-an</trd>
  <trd lng="tp">jan</trd>
</drv>
</subart>

<subart>
<dif>
Samsignifa memstara vortero.
</dif>
<drv mrk="ul.0o">
 <kap><tld/>o</kap>
   <snc>
     <dif>
       Esta&jcirc;o:
       <ekz>
         dikmalalta <ind><tld/>o</ind><fnt>Z</fnt>;
         <trd lng="de">Untersetzter</trd>
         <trdgrp lng="fi">
           <trd>olento</trd>,
           <trd>tyyppi<klr tip="ind">
(ihminen)</klr></trd>
         </trdgrp>
         <trd lng="hu">k&ouml;pc&ouml;s alak</trd>
         <trdgrp lng="ru">
           <trd>&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>,
           <trd>&c_t;&c_i;&c_p;<klr tip="ind">
(&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;)</klr></trd>,
           <trd>&c_l;&c_i;&c_c;&c_o;<klr tip="ind">
(&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;)</klr></trd>,
           <trd>&c_i;&c_n;&c_d;&c_i;&c_v;&c_i;&c_d;</trd>
         </trdgrp>
       </ekz>
       <ekz>
         <ind><tld/>a&ccirc;o</ind>;
         <trd lng="hu">szakadt alak</trd>
       </ekz>
       <ekz>
         <ind>fi<tld/>o</ind>.
         <trd lng="de">schmieriger Typ</trd>
         <trdgrp lng="fi">
           <trd>lurjus</trd>,
           <trd>heitti&ouml;</trd>,
         </trdgrp>
         <trd lng="hu">gazember</trd>
         <trdgrp lng="ru">
           <trd>&c_m;&c_je;&c_r;&c_z;&c_a;&c_v;&c_je;&c_c;</trd>,
           <trd>&c_p;&c_o;&c_d;&c_l;&c_je;&c_c;</trd>,
           <trd>&c_n;&c_je;&c_g;&c_o;&c_d;&c_ja;&c_j;</trd>
         </trdgrp>
       </ekz>
     </dif>
   </snc>
  <trd lng="af">chick</trd>
  <trd lng="am">&#4907;&#4905;&#4725;</trd>
  <trd lng="ar">&a_k;&a_t;&a_k;&a_w;&a_t;</trd>
  <trd lng="az">c&uuml;c&#601;</trd>
  <trd lng="bn">&#2453;&#2497;&#2453;&#2509;&#2453;&#2497;&#2463;</trd>
  <trd lng="co">pulcino</trd>
  <trdgrp lng="cs">
    <trd>chlap</trd>,
    <trd>individuum</trd>,
    <trd>osoba</trd>
  </trdgrp>
  <trd lng="da">chick</trd>
  <trd lng="de">Typ</trd>
  <trdgrp lng="en">
    <trd>individual</trd>,
    <trd>person</trd>,
    <trd>bloke</trd>,
    <trd>chap</trd>,
    <trd>chick</trd>,
    <trd>dude</trd>,
    <trd>fellow</trd>,
    <trd>gal</trd>,
    <trd>guy</trd>
  </trdgrp>
  <trd lng="et">tibu</trd>
  <trd lng="fr">individu</trd>
  <trd lng="gu">&#2730;&#2709;&#2765;&#2743;&#2752;&#2728;&#2753;&#2690;
  &#2732;&#2714;&#2765;&#2714;&#2753;&#2690;</trd>
  <trd lng="hi">&#2354;&#2337;&#2364;&#2325;&#2368;</trd>
  <trd lng="hr">pili&cacute;</trd>
  <trd lng="hu">alak</trd>
  <trd lng="hy">&#1353;&#1387;&#1391;</trd>
  <trd lng="ja">&#12402;&#12424;&#12371;</trd>
  <trd lng="jw">cah ayu</trd>
  <trd lng="ka">&#4325;&#4304;&#4311;&#4315;&#4312;&#4321;</trd>
  <trd lng="ko">&#48337;&#50500;&#47532;</trd>
  <trd lng="lo">&#3735;&#3761;&#3719;&#3755;&#3749;&#3762;&#3725;</trd>
  <trd lng="ml">&#3349;&#3402;&#3380;&#3391;</trd>
  <trd lng="mr">&#2330;&#2367;&#2325;</trd>
  <trd lng="my">&#4122;&#4100;&#4154;&#4152;</trd>
  <trd lng="ne">&#2325;&#2375;&#2335;&#2368;</trd>
  <trdgrp lng="nl">
