  <trd lng="gl">pinto</trd>
  <trd lng="gu">&#xAAA;&#xA95;&#xACD;&#xAB7;&#xAC0;&#xAA8;&#xAC1;&#xA82;
  &#xAAC;&#xA9A;&#xACD;&#xA9A;&#xAC1;&#xA82;</trd>
