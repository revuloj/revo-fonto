<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ul.xml,v 1.20 2021/05/30 14:12:55 revo Exp $">
<kap>
     <ofc>*</ofc>
     -<rad>ul</rad>/
</kap>
<subart>
<drv mrk="ul.0">
     <kap>-<tld/></kap>
       <snc>
         <dif>
           Sufikso signifanta vivantan esta&jcirc;on karakterizatan per tio,
           kion esprimas la radiko:
           <ekz>
             &gcirc;ib<tld/>o,
           </ekz>
           <ekz>
             <ind>unuokul<tld/>o</ind>,
             <trd lng="fi">silm&auml;puoli</trd>
             <trd lng="fy">ieneach</trd>
             <trd lng="hu">f&eacute;lszem&udblac; <klr tip="amb">(ember)</klr></trd>
             <trd lng="nl">eenoog</trd>
             <trd lng="ru">&c_o;&c_d;&c_n;&c_o;&c_g;&c_l;&c_a;&c_z;&c_y;&c_j; <klr tip="amb">(&c_s;&c_u;&c_shch;.)</klr></trd>
           </ekz>
           <ekz>
             <ind>virg<tld/>ino</ind>;
             <trd lng="ca">poncella</trd>
             <trd lng="cs">panna</trd>
             <trd lng="de">Jungfrau</trd>
             <trd lng="en">virgin</trd>
             <trd lng="fi">neitsyt</trd>
             <trdgrp lng="fr">
               <trd>pucelle</trd>,
               <trd>vierge<klr tip="amb"> (subst.)</klr></trd>
             </trdgrp>
             <trd lng="hu">sz&udblac;z</trd>
             <trdgrp lng="id">
               <trd>dara</trd>,
               <trd>perawan</trd>
             </trdgrp>
             <trd lng="io">virgino</trd>
             <trd lng="nl">maagd</trd>
             <trd lng="ru">&c_d;&c_je;&c_v;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_i;&c_c;&c_a;</trd>
             <trd lng="sk">panna</trd>
             <trd lng="vo">jivirgan</trd>
           </ekz>
           <ekz>
             <ind>mam<tld/>o</ind>,
             <trd lng="bg">&c_b;&c_o;&c_z;&c_a;&c_j;&c_n;&c_i;&c_k;</trd>
             <trd lng="cs">savec</trd>
             <trd lng="en">mammal</trd>
             <trd lng="fi">nis&auml;k&auml;s</trd>
             <trd lng="fy">s&ucirc;chdier</trd>
             <trd lng="io">mamifero</trd>
             <trd lng="nl">zoogdier</trd>
             <trd lng="sk">cicavec</trd>
           </ekz>
           <ekz>vertebr<tld/>o</ekz>
           <ekz>
             tim<tld/>o,
           </ekz>
           <ekz>
             <ind>ri&ccirc;<tld/>o</ind>,
             <trd lng="hu">gazdag ember</trd>
             <trd lng="io">richo</trd>
             <trd lng="ru">&c_b;&c_o;&c_g;&c_a;&c_ch;</trd>
           </ekz>
           <ekz>
             proksim<tld/>o, prav<tld/>o<fnt>Z</fnt>;
           </ekz>
           <ekz>
             mez<tld/>o<fnt>B</fnt>
             <klr>(homo de meza klaso kaj inteligento)</klr>;
           </ekz>
           <ekz>
             tiam<tld/>o;
           </ekz>
           <ekz>
             &ccirc;itie<tld/>o<fnt>Z</fnt>;
           </ekz>
           <ekz>
             mi estas sciencisto, sed mi ne pretendas esti
             scienc<tld/>o;
           </ekz>
           <ekz>
             multinfan<tld/>ino<fnt>Z</fnt>;
           </ekz>
           <ekz>
             la vivo kaj kompetent<tld/>a uzado fiksis
             formon<fnt>Z</fnt>;
           </ekz>
           <ekz>
             la mult<tld/>a pacdeziro;
           </ekz>
           <ekz>
             pardoninda jun<tld/>a petola&jcirc;o;
           </ekz>
           <ekz>
             kompari siajn &scirc;ar&gcirc;ojn kun la ali<tld/>aj;
           </ekz>
           <ekz>
             la eklezio sankt<tld/>igis lin<fnt>Z</fnt>.
           </ekz>
         </dif>
       </snc>
       <trd lng="de"><klr>-</klr>er</trd>
       <trdgrp lng="fi">
         <trd>olio<klr tip="amb"> (sananjohdin)</klr><klr>, 
yleens&auml; ihminen tai el&auml;in
jolla on kantasanan ilmoittama ominaisuus</klr></trd>
       </trdgrp>
       <trd lng="hu">ember</trd>
       <trd lng="vo">-an</trd>
       <trd lng="tp">jan</trd>
</drv>
</subart>

<subart>
<dif>
Samsignifa memstara vortero.
</dif>
<drv mrk="ul.0o">
     <kap><tld/>o</kap>
       <snc>
         <dif>
           Esta&jcirc;o:
           <ekz>
             dikmalalta <ind><tld/>o</ind><fnt>Z</fnt>;
             <trd lng="de">Untersetzter</trd>
             <trdgrp lng="fi">
               <trd>olento</trd>,
               <trd>tyyppi<klr tip="ind">
(ihminen)</klr></trd>
             </trdgrp>
             <trd lng="hu">k&ouml;pc&ouml;s alak</trd>
             <trdgrp lng="ru">
               <trd>&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>,
               <trd>&c_t;&c_i;&c_p;<klr tip="ind">
(&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;)</klr></trd>,
               <trd>&c_l;&c_i;&c_c;&c_o;<klr tip="ind">
(&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;)</klr></trd>,
               <trd>&c_i;&c_n;&c_d;&c_i;&c_v;&c_i;&c_d;</trd>
             </trdgrp>
           </ekz>
           <ekz>
             <ind><tld/>a&ccirc;o</ind>;
             <trd lng="hu">szakadt alak</trd>
           </ekz>
           <ekz>
             <ind>fi<tld/>o</ind>.
             <trd lng="de">schmieriger Typ</trd>
             <trdgrp lng="fi">
               <trd>lurjus</trd>,
               <trd>heitti&ouml;</trd>,
             </trdgrp>
             <trd lng="hu">gazember</trd>
             <trdgrp lng="ru">
               <trd>&c_m;&c_je;&c_r;&c_z;&c_a;&c_v;&c_je;&c_c;</trd>,
               <trd>&c_p;&c_o;&c_d;&c_l;&c_je;&c_c;</trd>,
               <trd>&c_n;&c_je;&c_g;&c_o;&c_d;&c_ja;&c_j;</trd>
             </trdgrp>
           </ekz>
         </dif>
       </snc>
  <trd lng="af">chick</trd>
  <trd lng="am">&#x132B;&#x1329;&#x1275;</trd>
  <trdgrp lng="cs">
    <trd>chlap</trd>,
    <trd>individuum</trd>,
    <trd>osoba</trd>
  </trdgrp>
  <trd lng="de">Typ</trd>
  <trdgrp lng="en">
    <trd>individual</trd>,
    <trd>person</trd>,
    <trd>bloke</trd>,
    <trd>chap</trd>,
    <trd>chick</trd>,
    <trd>dude</trd>,
    <trd>fellow</trd>,
    <trd>gal</trd>,
    <trd>guy</trd>
  </trdgrp>
  <trd lng="fr">individu</trd>
  <trd lng="hu">alak</trd>
  <trd lng="ne">&#x915;&#x947;&#x91F;&#x940;</trd>
  <trdgrp lng="nl">
    <trd>persoon</trd>,
    <trd>gast</trd>,
    <trd>gozer</trd>,
    <trd>kerel</trd>,
    <trd>meid</trd>,
    <trd>snuiter</trd>,
    <trd>vent</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chlap</trd>,
    <trd>indiv&iacute;duum</trd>,
    <trd>osoba</trd>
  </trdgrp>
  <trd lng="sq">zogth</trd>
  <trd lng="tp">jan</trd>
</drv>
</subart>
</art>

