  <trd lng="jv">cah ayu</trd>
  <trd lng="ka">&#x10E5;&#x10D0;&#x10D7;&#x10DB;&#x10D8;&#x10E1;</trd>
  <trd lng="kn">&#xCAE;&#xCB0;&#xCBF;&#xCAF;&#xCA8;&#xCCD;&#xCA8;&#xCC1;</trd>
  <trd lng="ko">&#xBCD1;&#xC544;&#xB9AC;</trd>
