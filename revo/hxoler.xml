<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>&hcirc;oler</rad>/o
</kap>
<drv mrk="hxoler.0o">
   <kap>
     <ofc>*</ofc><tld/>o
   </kap>
   <uzo tip="fak">MED</uzo>
   <snc>
     <dif>
       Epidemia <ref tip="super" cel="san.mal0o">malsano</ref>,
       preska&ubreve; &ccirc;iam mortiga, karakterizata per
       gravaj intestaj malordoj, intensa malvarmi&gcirc;o kaj spasmaj
       kuntiri&gcirc;oj de membroj.
     </dif>
   </snc>
   <trd lng="be">&c_h;&c_a;&c_l;&c_je;&c_r;&c_a;</trd>
   <trd lng="ca">c&ograve;lera</trd>
   <trd lng="de">Cholera</trd>
   <trd lng="en">cholera</trd>
   <trd lng="es">c&oacute;lera</trd>
   <trd lng="fi">kolera</trd>
   <trd lng="fr">chol&eacute;ra</trd>
   <trdgrp lng="he">
     <trd>&het;&vav;&lamed;&yod;&resh;&ayin;</trd>,
     <trd>&kaf;&vav;&lamed;&yod;&resh;&he;</trd>
   </trdgrp>
   <trd lng="hu">kolera</trd>
   <trd lng="nl">cholera</trd>
   <trd lng="pl">cholera</trd>
   <trd lng="pt">c&oacute;lera</trd>
   <trd lng="ru">&c_h;&c_o;&c_l;&c_je;&c_r;&c_a;</trd>
   <trd lng="sk">cholera</trd>
   <trd lng="sv">kolera</trd>
   <trd lng="it">colera</trd>
   <trd lng="la">cholera</trd>
</drv>
</art>
