<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>student</rad>/o
</kap>
<drv mrk="studen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">PED</uzo>
  <snc>
    <dif>
      Lernanto en supera lernejo:
      <ekz>
        li i&gcirc;is <tld/>o de Akademio de Belartoj kaj samtempe &ndash; de jura
        fakultato de la universitato
        <fnt><aut>Natalia Arolovi&ccirc;</aut>: <vrk>La homo de la kosma
        konscio</vrk>, <bib>Monato</bib><lok>jaro 2002a, numero 7a, p.
        20a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_u;&c_d;&c_e;&c_n;&c_t;</trd>
  <trd lng="de">Student</trd>
  <trdgrp lng="en">
    <trd>student<klr> (at a university)</klr></trd>
    <trd>undergrad</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;tudiant<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="nl">student</trd>
  <trd lng="pl">student</trd>
  <trd lng="ru">&c_s;&c_t;&c_u;&c_d;&c_je;&c_n;&c_t;</trd>
  <trd lng="sv">student</trd>
</drv>
</art>
