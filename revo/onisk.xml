<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: onisk.xml,v 1.11 2023/12/28 13:31:36 revo Exp $">
<kap>
  <rad>onisk</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="onisk.0o">
  <kap><tld/>o</kap>
  <snc mrk="onisk.0o.ZOO">
    <dif>
      Iu el <ref tip="malprt" cel="onisk.0oj"><tld/>oj</ref>.
    </dif>
    <ref tip="lst" cel="krust.0ulo" lst="voko:krustuloj">krustulo</ref>
  </snc>
    <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_n;&c_o;&c_zh;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_k;&c_r;&c_y;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="fr">cloporte</trd>
  <trd lng="ja">&#x30EF;&#x30E9;&#x30B8;&#x30E0;&#x30B7;</trd>
  <trd lng="pl">stonoga</trd>
  <trd lng="ro">molia zidurilor</trd>
  <trd lng="uk">&c_s;&c_t;&#x43E;&#x43D;&#x43E;&c_g;&c_a;</trd>
</drv>

<drv mrk="onisk.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="onisk.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Malgrandaj <ref tip="malprt" cel="krust.0uloj">krustuloj</ref>
      kun plurparta &scirc;elo kaj sep kruroparoj,
      da&ubreve;re vivantaj ekster akvo kaj kapablaj fekundi&gcirc;i tie;
      subordo de izopodoj
      (<trd lng="la">Oniscidea</trd>):
      <ekz>
        la el&jcirc;etado de <klr>...</klr> centoj da bruligitaj skaraboj kaj
        grandaj <tld/>oj, kaj dekkelkaj rostitaj vespertoj da&ubreve;ris pli
        ol du horojn
        <fnt>
          <aut>H. Sienkiewicz, trad. M. Sygnarski</aut>,
          <vrk><url ref="http://esperanto.pl/page.php?tid=200000">
          Tra dezerto kaj praarbaro</url></vrk>,
          <lok>Varsovio, 1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_n;&c_o;&c_zh;&c_k;&c_ib; <klr>(&c_p;&c_a;&c_d;&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>,
    <trd>&c_m;&c_a;&c_k;&c_r;&c_y;&c_c;&c_y; <klr>(&c_p;&c_a;&c_d;&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  </trdgrp>
  <trd lng="ca">panerola</trd>
  <trd lng="de">Landasseln</trd>
   <trdgrp lng="en">
     <trd>woodlouse</trd>
     <trd>pill bug</trd>
   </trdgrp>
  <trd lng="fr">cloportes</trd>
  <trd lng="it">onisco</trd>
  <trd lng="pl">stonogi</trd>
  <trd lng="ro">oniscidee</trd>
</drv>
</art>

