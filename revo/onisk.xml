<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: onisk.xml,v 1.12 2024/11/03 23:29:16 revo Exp $">
<kap>
  <rad>onisk</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="onisk.0o">
  <kap><tld/>o</kap>
  <snc mrk="onisk.0o.ZOO">
    <dif>
      Iu el <ref tip="malprt" cel="onisk.0oj"><tld/>oj</ref>.
    </dif>
    <ref tip="super" cel="izopod.0o">izopodo</ref>
    <ref tip="lst" cel="krust.0ulo" lst="voko:krustuloj">krustulo</ref>
  </snc>
    <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_n;&c_o;&c_zh;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_k;&c_r;&c_y;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Landassel</trd>,
    <trd><klr>(komune) </klr>Kellerassel</trd>
  </trdgrp>
  <trd lng="el">&omikron;&ny;&jota_ton;&sigma;&kappa;&omikron;&sigma_fina;</trd>
   <trdgrp lng="en">
     <trd>woodlouse</trd>,
     <trd>slater<klr tip="ind"> (woodlouse)</klr></trd>,
     <trd>pill bug<klr> (se povas volvi&gcirc;i al pilko)</klr></trd>,
     <trd>roly poly<klr tip="ind"> (woodlouse)</klr><klr> (se povas volvi&gcirc;i al pilko)</klr></trd>
   </trdgrp>
  <trdgrp lng="fr">
    <trd>cloporte</trd>,
    <trd>cochon de saint Antoine</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>onisco</trd>
    <trd>porcellino di terra<klr> (specio Armadillidium vulgare)</klr></trd>,
    <trd>porcellino di sant'Antonio<klr> (specio Armadillidium vulgare)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12527;&#12521;&#12472;&#12512;&#12471;<klr> (se ne povas volvi&gcirc;i al pilko)</klr></trd>,
    <trd>&#12480;&#12531;&#12468;&#12512;&#12471;<klr> (se ja povas volvi&gcirc;i al pilko)</klr></trd>,
    <trd>&#33609;&#38795;&#34411; <pr>&#12431;&#12425;&#12375;&#12416;&#12375;</pr><klr> (se ne povas volvi&gcirc;i al pilko)</klr></trd>,
    <trd>&#22243;&#23376;&#34411; <pr>&#12384;&#12435;&#12372;&#12416;&#12375;</pr><klr> (se ja povas volvi&gcirc;i al pilko)</klr></trd>
  </trdgrp>
  <trd lng="lat">oniscus</trd>
  <trdgrp lng="nl">
    <trd>landpissebed</trd>,
    <trd><klr>(komune) </klr>pissebed</trd>
  </trdgrp>
  <trd lng="pl">stonoga</trd>
  <trd lng="ro">molia zidurilor</trd>
  <trd lng="uk">&c_s;&c_t;&#x43E;&#x43D;&#x43E;&c_g;&c_a;</trd>
</drv>

<drv mrk="onisk.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="onisk.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Malgrandaj <ref tip="malprt" cel="krust.0uloj">krustuloj</ref>
      kun plurparta &scirc;elo kaj sep kruroparoj,
      da&ubreve;re vivantaj ekster akvo kaj kapablaj fekundi&gcirc;i tie;
      subordo de <ref tip="malprt" cel="izopod.0oj">izopodoj</ref>
      (<trd lng="la">Oniscidea</trd>):
      <ekz>
        la el&jcirc;etado de <klr>...</klr> centoj da bruligitaj skaraboj kaj
        grandaj <tld/>oj, kaj dekkelkaj rostitaj vespertoj da&ubreve;ris pli
        ol du horojn
        <fnt>
          <aut>H. Sienkiewicz, trad. M. Sygnarski</aut>,
          <vrk><url ref="http://esperanto.pl/page.php?tid=200000">
          Tra dezerto kaj praarbaro</url></vrk>,
          <lok>Varsovio, 1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_n;&c_o;&c_zh;&c_k;&c_ib; <klr>(&c_p;&c_a;&c_d;&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>,
    <trd>&c_m;&c_a;&c_k;&c_r;&c_y;&c_c;&c_y; <klr>(&c_p;&c_a;&c_d;&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  </trdgrp>
  <trd lng="ca">panerola</trd>
  <trd lng="de">Landasseln</trd>
  <trdgrp lng="en">
    <trd>woodlice</trd>,
    <trd>oniscideans</trd>,
    <trd>Oniscidea</trd>
  </trdgrp>
  <trd lng="fr">cloportes</trd>
  <trd lng="ja">&#12527;&#12521;&#12472;&#12512;&#12471;&#20124;&#30446; <pr>&#12431;&#12425;&#12376;&#12416;&#12375;&#12354;&#12418;&#12367;</pr></trd>
  <trd lng="nl">landpissebedden</trd>
  <trd lng="pl">stonogi</trd>
  <trd lng="ro">oniscidee</trd>
</drv>
</art>

