<art mrk="$Id: ideogram.xml,v 1.8 2023/11/19 10:22:02 revo Exp $">
<kap>
  <rad>ideogram</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ideogram.0o">
  <kap><tld/>o</kap>
  <snc mrk="ideogram.0o.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="dif" cel="ideogr.0ajxo.LIN">Ideografia&jcirc;o</ref>:
      <ekz>
        li faris la <tld/>on &#x4EAC; similan al sportanta homo,
        kiu kun etenditaj brakoj kuras anta&ubreve;en
        <fnt>
          <aut>Wa Na</aut>,
          <vrk><url ref="http://sars.china.com.cn/world/shi-living/junularo/31.htm">
          Guo Chunning projektis la emblemon de la Pekinaj
          Olimpikoj</url></vrk>,
          <lok>[vidita en 2013]</lok>
        </fnt>.
      </ekz>
      <ekz>
        studi volapukon a&ubreve; lerni la &ccirc;inajn <tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2005/008422.php">Disleksio</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la pordklapo legeblas papero kun <tld/>o signifanta &leftquot;feli&ccirc;o&#x201D;
        <fnt>
          <bib>Monato</bib>
          <aut>Chen Lina/pg</aut>
          <vrk><url ref="&Monato;/2007/009185.php">&Scirc;an&gcirc;i&gcirc;anta feli&ccirc;o-festo</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a skribsistemo
        <fnt>
          <bib>Monato</bib>
          <aut>Sen Rodin</aut>
          <vrk><url ref="&Monato;/2008/009067.php">&Ccirc;ISM</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_d;&c_e;&c_a;&c_g;&c_r;&c_a;&c_m;&c_a;</trd>
  <trd lng="cs">ideogram</trd>
  <trd lng="de">Ideogramm</trd>
  <trd lng="es">ideograma</trd>
  <trd lng="fa">&a_A;&a_n;&a_d;&f_y;&a_sh;&a_h;&#x200C;&a_n;&f_g;&a_A;&a_sh;&a_t;</trd>
  <trd lng="fr">id&eacute;ogramme</trd>
  <trd lng="hu">ideogramma</trd>
  <trd lng="nl">ideogram</trd>
  <trd lng="pl">ideogram</trd>
  <trd lng="pt">ideograma</trd>
  <trd lng="ru">&c_i;&c_d;&c_je;&c_o;&c_g;&c_r;&c_a;&c_m;&c_m;&c_a;</trd>
  <trd lng="sk">ideogram</trd>
  <trd lng="uk">&c_ib;&c_d;&c_je;&#x43E;&c_g;&c_r;&c_a;&#x43C;&c_a;</trd>
  <trd lng="zh">&#x5F62;&#x610F;&#x6587;&#x5B57; <pr>x&#xED;ngy&#xEC;w&#xE9;nz&#xEC;</pr></trd>
