      <ref tip="sin" cel="korv.0o.specio">korvo</ref>.
    </dif>
    <bld lok="../bld/korv.jpg">
      <tld/>o <klr>(Corvus corax)</klr>
    </bld>
  </snc>
  <trd lng="bg">&c_g;&c_a;&c_r;&c_v;&c_a;&c_n; &c_g;&c_r;&c_o;&c_b;&c_a;&c_r;</trd>
  <trd lng="ca">corb</trd>
  <trd lng="cs">krkavec velk&yacute;</trd>
  <trd lng="de">Kolkrabe</trd>
  <trd lng="en">raven</trd>
  <trd lng="fa">&a_z;&a_A;&a_gh;</trd>
  <trd lng="fr">grand corbeau</trd>
  <trd lng="hu">holl&oacute;</trd>
  <trd lng="id">gagak</trd>
  <trd lng="nl">raaf</trd>
  <trd lng="pl">kruk</trd>
  <trd lng="pt">corvo</trd>
  <trd lng="ru">&c_v;&c_o;&c_r;&c_o;&c_n;</trd>
</drv>
</art>

