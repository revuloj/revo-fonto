<vortaro>
<art mrk="$Id: korak.xml,v 1.7 2023/09/01 15:46:43 revo Exp $">
<kap>
  <rad>korak</rad>/o <fnt><bib>SPIV</bib></fnt>
</kap>
<drv mrk="korak.0o">
  <kap><tld/>o</kap>
  <snc mrk="korak.0o.korvo">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      La specio <ref tip="dif" cel="korv.0o.korako">korvo<sncref/></ref>, t.e.
      Nigra Korvo:
      <ekz>
        la pigoj estas grandaj birdoj <klr>[&#x2026;]</klr> sed <klr>[malpli]</klr> grandaj kiom la Komuna
        <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=26308">Pigo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        alvenas batalantaj gnomoj, alvokitaj de mesa&gcirc;istaj <tld/>oj, por helpo de la sie&gcirc;ataj gnomoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2569">La Hobito</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/20/Corvus_corax_sinuatus%2C_Point_Reyes_National_Seashore.jpg/640px-Corvus_corax_sinuatus%2C_Point_Reyes_National_Seashore.jpg" prm="CC BY-SA 4.0">amerika <tld/>o (Corvus corax sinuatus), Marin County, Kalifornio
         <fnt>
           <aut>&#xA9; Frank Schulenburg, propra verko</aut>
           <lok><url ref="&WCW;/File:Corvus_corax_sinuatus,_Point_Reyes_National_Seashore.jpg">&WCW;/File:Corvus_corax_sinuatus,_Point_Reyes_National_Seashore.jpg</url></lok>
         </fnt>
    </bld>
    <trd lng="la">Corvus corax</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_u;&c_m;&c_k;&c_a;&c_ch;</trd>,
    <trd>&c_k;&c_r;&c_u;&c_k; <klr tip="amb">(&c_p;&c_t;&c_u;&c_sh;&c_k;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="bg">&c_g;&c_a;&c_r;&c_v;&c_a;&c_n; &c_g;&c_r;&c_o;&c_b;&c_a;&c_r;</trd>
  <trd lng="ca">corb</trd>
  <trd lng="cs">krkavec velk&yacute;</trd>
  <trd lng="de">Kolkrabe</trd>
  <trd lng="en">raven</trd>
  <trd lng="fa">&a_z;&a_A;&a_gh;</trd>
  <trd lng="fr">grand corbeau</trd>
  <trd lng="hu">holl&oacute;</trd>
  <trd lng="id">gagak</trd>
  <trd lng="ja">&#x30EF;&#x30BF;&#x30EA;&#x30AC;&#x30E9;&#x30B9;</trd>
  <trd lng="nl">raaf</trd>
  <trd lng="pl">kruk</trd>
  <trd lng="pt">corvo</trd>
  <trd lng="ru">&c_v;&c_o;&c_r;&c_o;&c_n;</trd>
</drv>
</art>

