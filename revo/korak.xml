<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>korak</rad>/o <fnt><bib>SPIV</bib></fnt>
</kap>
<drv mrk="korak.0o">
  <kap><tld/>o</kap>
  <snc mrk="korak.0o.korvo">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      La plej granda <ref cel="speci.0o.BIO" tip="lst"
      lst="voko:zoologiaj_specioj">specio</ref> el
      la genro <ref tip="super" cel="korv.0o.genro">korvo</ref>
      (<trd lng="la">Corvus corax</trd>), anka&ubreve; nomata nigra korvo kaj
      <ref tip="sin" cel="korv.0o.korako">korvo</ref>.
    </dif>
    <bld lok="../bld/korv.jpg">
      <tld/>o <klr>(Corvus corax)</klr>
    </bld>
  </snc>
   <rim>
     individuoj maskla : <em>virkorako</em> ; ina : <em>korakino</em> ;
      juna : <em>korakido</em> ; juna maskla :
      <em>virkorakido</em> ; juna ina : <em>korakidino</em>
   </rim>
  <trd lng="bg">&c_g;&c_a;&c_r;&c_v;&c_a;&c_n; &c_g;&c_r;&c_o;&c_b;&c_a;&c_r;</trd>
  <trd lng="ca">corb</trd>
  <trd lng="cs">krkavec velk&yacute;</trd>
  <trd lng="de">Kolkrabe</trd>
  <trd lng="en">raven</trd>
  <trd lng="fa">&a_z;&a_A;&a_gh;</trd>
  <trd lng="fr">grand corbeau</trd>
  <trd lng="hu">holl&oacute;</trd>
  <trd lng="id">gagak</trd>
  <trd lng="nl">raaf</trd>
  <trd lng="pl">kruk</trd>
  <trd lng="pt">corvo</trd>
  <trd lng="ru">&c_v;&c_o;&c_r;&c_o;&c_n;</trd>
</drv>
</art>

