  </trdgrp>
  <trd lng="et">siis</trd>
  <trd lng="eu">orduz</trd>
  <trdgrp lng="fr">
    <trd>alors</trd>,
    <trd>en ce <ind>temps</ind>-l&agrave;</trd>
    <trd>en ce <ind>cas</ind></trd>
  </trdgrp>
  <trd lng="fy">dan</trd>
