<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: viver.xml,v 1.19 2022/05/06 12:17:22 revo Exp $">
<kap>
  <rad>viver</rad>/o
</kap>
<drv mrk="viver.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="best.rabo0o"
      lst="voko:rabobestoj">Rabobesto</ref> 
      iom katosimila, kun pinta kapo kaj longaj kruroj, produktanta la
      <ref tip="prt" cel="cibet.0o">cibeton</ref>;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="vivere.0oj"><tld/>edoj</ref>
      (<trd lng="la">Viverra</trd>):
      <ekz>
        en &Ccirc;inio homoj ofte uzis la viandon kaj sangon de <tld/>oj kiel nutra&jcirc;on
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2005/008721.php"
              >De kie venis kaj kial &ccirc;esis netipa pne&ubreve;monio?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="cibet.0kato">cibetkato</ref>
    <bld
      lok="&WCU;/thumb/f/f8/Large_Indian_Civet%2C_Viverra_zibetha_in_Kaeng_Krachan_national_park.jpg/640px-Large_Indian_Civet%2C_Viverra_zibetha_in_Kaeng_Krachan_national_park.jpg"
      prm="CC BY-SA 2.0"
      ><tld/>o (Viverra zibetha) en Nacia Parko Kaeng Krachan
        <fnt>
          <aut>tontantravel</aut>
          <lok><url ref="&WCW;/File:Large_Indian_Civet,_Viverra_zibetha_in_Kaeng_Krachan_national_park.jpg"
              >&WCW;/File:Large_Indian_Civet,_Viverra_zibetha_in_Kaeng_Krachan_national_park.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_v;&c_e;&c_r;&c_a;</trd>
  <trd lng="ca">civeta</trd>
  <trd lng="cs">cibetka</trd>
  <trd lng="de">Zibetkatze</trd>
  <trd lng="en">civet</trd>
  <trd lng="fa">&f_g;&a_r;&a_b;&a_h;&zwnj;&f_y;
  &a_z;&a_b;&a_A;&a_d;</trd>
  <trd lng="fr">civette</trd>
  <trd lng="hu">cibetmacska</trd>
  <trdgrp lng="id">
    <trd>luak</trd>,
    <trd>musang</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wiwera</trd>,
    <trd>cyweta</trd>,
    <trd>cybeta</trd>
  </trdgrp>
  <trd lng="pt">almiscareiro</trd>
  <trd lng="ru">&c_v;&c_i;&c_v;&c_je;&c_r;&c_r;&c_a;</trd>
  <trd lng="uk">&c_v;&c_ib;&c_v;&c_je;&c_r;&c_a;</trd>
</drv>
</art>
