<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>viver</rad>/o
</kap>
<drv mrk="viver.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="viver.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Viverra</trd>), produktanta la
      <ref tip="prt" cel="cibet.0o">cibeton</ref>.
    </dif>
    <ref tip="sin" cel="cibet.0kato">cibetkato</ref>
  </snc>
  <trd lng="de">Zibetkatze</trd>
  <trd lng="en">civet</trd>
</drv>
<drv mrk="viver.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="best.rabo0oj">rabobestoj</ref>
      (<trd lng="la">Viverridae</trd>).
    </dif>
  </snc>
  <trd lng="de">Schleichkatzen</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>