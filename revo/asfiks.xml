<vortaro>
<art mrk="$Id$">
<kap>
  <rad>asfiksi</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="asfiks.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Interrompo de la vivaj funkcioj per &ccirc;esigo de spirado, sufoko.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_s;&c_mol;&c_f;&c_ib;&c_k;&c_s;&c_ib;&c_ja;</trd>,
    <trd>&c_u;&c_d;&c_u;&c_sh;&c_sh;&c_a;</trd>
  </trdgrp>
  <trd lng="en">asphyxia</trd>
  <trd lng="es">asfixia</trd>
  <trd lng="fr">asphyxie</trd>
  <trdgrp lng="hu">
    <trd>fullad&aacute;s</trd>,
    <trd>asphyxia</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_d;&c_u;&c_sh;&c_mol;&c_je;</trd>,
    <trd>&c_a;&c_s;&c_f;&c_i;&c_k;&c_s;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">asfyxia</trd>
</drv>

<drv mrk="asfiks.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ka&ubreve;zi la sufokon de iu:
      <ekz>
        <tld/>ita per la gasoj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_d;&c_u;&c_sh;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_u;&c_sh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_u;&c_sh;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="en">asphyxiate</trd>
  <trd lng="es">asfixiar</trd>
  <trd lng="fr">asphyxier</trd>
  <trdgrp lng="hu">
    <trd>megfojt</trd>,
    <trd>fullad&aacute;st id&eacute;z el&odblac;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_d;&c_u;&c_sh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_d;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
