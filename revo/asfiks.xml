<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: asfiks.xml,v 1.24 2023/10/28 08:33:09 revo Exp $">
<kap>
  <rad>asfiksi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="asfiks.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="asfiks.0i.FIZL">
    <dif>
      <ref tip="dif" cel="sufok.0i.FIZL">Sufoki</ref>:
      <ekz>
        <tld/>ita per la gasoj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la konformista, hipokrita kaj ekonomie oportunista dekstro <tld/>as la
        individuon sub la premo de la Internacia Kapitalismo
        <fnt>
          <vrk><url ref="https://sofosagora.net/epistemologia/levitacio-estas-fenomeno-aosoligologio-t8816.html">La levitacio estas fenomeno de la &hcirc;aosoligologio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_d;&c_u;&c_sh;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_u;&c_sh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_u;&c_sh;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">zadusit</trd>
  <trd lng="en">asphyxiate</trd>
  <trd lng="es">asfixiar</trd>
  <trd lng="fr">asphyxier</trd>
  <trd lng="he">&#x5DC;&#x5D4;&#x5E9;&#x5EA;&#x5E0;&#x5E7;</trd>
  <trdgrp lng="hu">
    <trd>megfojt</trd>,
    <trd>fullad&aacute;st id&eacute;z el&odblac;</trd>
  </trdgrp>
  <trd lng="it">asfissiare</trd>
  <trd lng="ja">&#x7A92;&#x606F;&#x3055;&#x305B;&#x308B;  <pr>&#x3061;&#x3063;&#x305D;&#x304F;&#x3055;&#x305B;&#x308B;</pr></trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_d;&c_u;&c_sh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_d;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>zapr&iacute;&ccaron;ini&tcaron; asfyksiu</trd>,
    <trd>zastavenie dychu</trd>
  </trdgrp>
</drv>

<drv mrk="asfiks.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="asfiks.0o.FIZL">
    <dif>
      <ref tip="dif" cel="sufok.0o.FIZL">Sufoko</ref>:
      <ekz>
        la&ubreve;grada manko de oksigeno en la sango, kiu portas al &ccirc;ela <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2013/010896.php">Amanda saporo, semo ne amenda</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x547C;&#x5438;&#x505C;&#x6B62; <pr>h&#x16B;x&#x12B;t&#xED;ngzh&#x1D0;</pr></trd>,
    <trd>&#x7A92;&#x606F; <pr>zh&#xEC;x&#x12B;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_s;&c_mol;&c_f;&c_ib;&c_k;&c_s;&c_ib;&c_ja;</trd>,
    <trd>&c_u;&c_d;&c_u;&c_sh;&c_sh;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">asfyxie</trd>
  <trdgrp lng="de">
    <trd>Asphyxie</trd>,
    <trd>Atemstillstand</trd>
  </trdgrp>
  <trd lng="en">asphyxia</trd>
  <trd lng="es">asfixia</trd>
  <trd lng="fr">asphyxie</trd>
  <trd lng="he">&#x5D7;&#x5B6;&#x5E0;&#x5B6;&#x5E7;</trd>
  <trdgrp lng="hu">
    <trd>fullad&aacute;s</trd>,
    <trd>asphyxia</trd>
  </trdgrp>
  <trd lng="it">asfissia</trd>
  <trdgrp lng="ja">
    <trd>&#x7A92;&#x606F;  <pr>&#x3061;&#x3063;&#x305D;&#x304F;</pr></trd>,
    <trd>&#x4EEE;&#x6B7B;  <pr>&#x304B;&#x3057;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>afiksja</trd>,
    <trd>zamartwica</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_d;&c_u;&c_sh;&c_mol;&c_je;</trd>,
    <trd>&c_a;&c_s;&c_f;&c_i;&c_k;&c_s;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>asfyxia</trd>,
    <trd>zastavenie dychu</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x44F;&#x434;&#x443;&#x445;&#x430;</trd>,
    <trd>&#x430;&#x441;&#x444;&#x456;&#x43A;&#x441;&#x456;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
