<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: asfiks.xml,v 1.27 2025/05/03 06:38:01 revo Exp $">
<kap>
  <rad>asfiksi</rad>/o <fnt><bib>JED</bib></fnt>
</kap>

<drv mrk="asfiks.0o">
  <kap><tld/>o</kap>
  <snc mrk="asfiks.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Spirmanko, spirhalto (ekz-e pro halto de korbato, paralizo de spirmuskoloj, obstrukco de
      spirkanalo):
      <ekz>
        la&ubreve;grada manko de oksigeno en la sango, kiu portas al &ccirc;ela <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2013/010896.php">Amanda saporo, semo ne amenda</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[jen]</klr> la plej konvinka pruvo de la teorio pri la morto de la
        pompejanoj unumomente pro forta ondo de ardaj vulkangasoj &ndash; kiu en stato de
        asfiksio iros en necesejon
        <fnt>
          <bib>Kon11</bib>
          <lok>Historio</lok>
        </fnt>?
      </ekz>
    </dif>
    <ref tip="vid" cel="sufok.0o.FIZL">sufoko</ref>
    <trdgrp lng="zh">
    <trd>&#21628;&#21560;&#20572;&#27490; <pr>h&umacron;x&imacron;t&iacute;ngzh&#464;</pr></trd>,
     <trd>&#31378;&#24687; <pr>zh&igrave;x&imacron;</pr></trd>
   </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_s;&c_mol;&c_f;&c_ib;&c_k;&c_s;&c_ib;&c_ja;</trd>,
    <trd>&c_u;&c_d;&c_u;&c_sh;&c_sh;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">asfyxie</trd>
  <trdgrp lng="de">
    <trd>Asphyxie</trd>,
    <trd>Atemstillstand</trd>
  </trdgrp>
  <trd lng="en">asphyxia</trd>
  <trd lng="es">asfixia</trd>
  <trd lng="fr">asphyxie</trd>
  <trd lng="he">&het;&#1462;&nun;&#1462;&qof;</trd>
  <trdgrp lng="hu">
    <trd>fullad&aacute;s</trd>,
    <trd>asphyxia</trd>
  </trdgrp>
  <trd lng="it">asfissia</trd>
  <trdgrp lng="ja">
    <trd>&#31378;&#24687;  <pr>&#12385;&#12387;&#12381;&#12367;</pr></trd>,
    <trd>&#20206;&#27515;  <pr>&#12363;&#12375;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>afiksja</trd>,
    <trd>zamartwica</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_d;&c_u;&c_sh;&c_mol;&c_je;</trd>,
    <trd>&c_a;&c_s;&c_f;&c_i;&c_k;&c_s;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>asfyxia</trd>,
    <trd>zastavenie dychu</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ja;&c_d;&c_u;&c_h;&c_a;</trd>,
    <trd>&c_a;&c_s;&c_f;&c_ib;&c_k;&c_s;&c_ib;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="asfiks.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="asfiks.0i.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Suferi je <tld/>o.
    </dif>
    <ref tip="vid" cel="sven.0i">sveni</ref>
    <trdgrp lng="de">
      <trd>eine <ind>Asphyxie</ind> erleiden</trd>,
      <trd>einen <ind>Atemstillstand</ind> erleiden</trd>
    </trdgrp>
  </snc>
  <snc>
    <gra><vspec>tr</vspec></gra>
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="sufok.0i.FIZL">Sufoki</ref>:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la konformista, hipokrita kaj ekonomie oportunista dekstro <tld/>as la
        individuon sub la premo de la Internacia Kapitalismo
        <fnt>
          <vrk><url
          ref="https://sofosagora.net/epistemologia/levitacio-estas-fenomeno-aosoligologio-t8816.html">La
          levitacio estas fenomeno de la &hcirc;aosoligologio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_d;&c_u;&c_sh;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_u;&c_sh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_u;&c_sh;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">zadusit</trd>
  <trd lng="en">asphyxiate</trd>
  <trd lng="es">asfixiar</trd>
  <trd lng="fr">asphyxier</trd>
  <trd lng="he">&lamed;&he;&shin;&tav;&nun;&qof;</trd>
  <trdgrp lng="hu">
    <trd>megfojt</trd>,
    <trd>fullad&aacute;st id&eacute;z el&odblac;</trd>
  </trdgrp>
  <trd lng="it">asfissiare</trd>
  <trd lng="ja">&#31378;&#24687;&#12373;&#12379;&#12427;  <pr>&#12385;&#12387;&#12381;&#12367;&#12373;&#12379;&#12427;</pr></trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_d;&c_u;&c_sh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_d;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>zapr&iacute;&ccaron;ini&tcaron; asfyksiu</trd>,
    <trd>zastavenie dychu</trd>
  </trdgrp>
</drv>

</art>
