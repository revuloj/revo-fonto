<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kresce.xml,v 1.18 2023/12/03 09:59:00 revo Exp $">
<kap>
  <rad>krescent</rad>/o
</kap>
<drv mrk="kresce.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">SCI</uzo>
  <snc>
    <dif>
      Duoncirkla <ref tip="lst" lst="voko:formoj" cel="form.0o.aspekto">formo</ref>
      de &ccirc;iaspeca objekto:
      <ekz>
        Arkimedo jam sukcesis pere de tiel nomataj <tld lit="K"/>oj, kiujn oni atribuas
        al la greka matematikisto Hippokrato el &Hcirc;ios (&ccirc; 450 a.K.), montri, ke la
        areoj de tiuj cirklo-partoj povas esti prezentitaj kiel racionaloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=6957">Pi (nombro)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estas la enenturni&gcirc;o, la malo al la elturni&gcirc;o, &gcirc;i okazas kiam la
        maleoloj kaj la piedfingroj estas turnitaj enen, kreantaj D-similan formon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=289069">Terminaro de klasika dancado</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la Ru&gcirc;a Kruco kaj Ru&gcirc;a <tld lit="K"/>o, kiu helpis al viktimoj de la Unua
        Itala-Etiopia milito
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1003">Etiopio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="lun">lunarko</ref>,
      <ref cel="kurb.0a">kurba</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_w;&c_m;&c_je;&c_s;&c_ja;&c_c;</trd>,
    <trd>&c_s;&c_je;&c_r;&c_p; 
<klr>(&c_m;&c_je;&c_s;&c_ja;&c_c;&c_a;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Mondsichel</trd>,
     <trd>Halbmond</trd>
  </trdgrp>
  <trd lng="en">crescent</trd>
  <trd lng="fr">croissant<klr tip="ind"> (forme)</klr></trd>
  <trd lng="ja">&#x4E09;&#x65E5;&#x6708;&#x5F62;  <pr>&#x307F;&#x304B;&#x3065;&#x304D;&#x304C;&#x305F;</pr></trd>
  <trd lng="nl">sikkel<klr tip="ind"> (halve maan)</klr></trd>
  <trdgrp lng="hu">
    <trd>f&eacute;lhold<klr tip="ind"> (alak&uacute; 
t&aacute;rgy)</klr></trd>,
    <trd>sarl&oacute;<klr tip="ind"> (alak&uacute; 
t&aacute;rgy)</klr></trd>,
    <trd>f&eacute;lk&ouml;r</trd>
  </trdgrp>
  <trd lng="pl">p&oacute;&lstroke;ksi&eogonek;&zdot;yc</trd>
  <trd lng="pt">crescente <klr tip="amb">(subst.)</klr></trd>
  <trd lng="ro">semilun&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_l;&c_u;&c_m;&c_je;&c_s;&c_ja;&c_c;</trd>,
    <trd>&c_s;&c_je;&c_r;&c_p; <klr>(&c_l;&c_u;&c_n;&c_y;)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x456;&#x432;&#x43C;&#x456;&#x441;&#x44F;&#x446;&#x44C;</trd>,
    <trd>&#x441;&#x435;&#x440;&#x43F; &#x43C;&#x456;&#x441;&#x44F;&#x446;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x65B0;&#x6708; <pr>x&#x12B;nyu&#xE8;</pr></trd>,
    <trd>&#x670F; <pr>f&#x11B;i</pr></trd>,
    <trd>&#x5F2F;&#x6708; <pr>w&#x101;nyu&#xE8;</pr></trd>,
    <trd>&#x5F4E;&#x6708; <pr>w&#x101;nyu&#xE8;</pr></trd>,
    <trd>&#x5F26; <pr>xu&#xE1;n</pr></trd>,
    <trd>&#x7D43; <pr>xu&#xE1;n</pr></trd>
  </trdgrp>
</drv>
</art>
