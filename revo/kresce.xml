<vortaro>
<art mrk="$Id$">
<kap>
  <rad>krescent</rad>/o
</kap>
<drv mrk="kresce.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">SCI</uzo>
  <snc>
    <dif>
      Duoncirkla formo de &ccirc;iaspeca objekto.
    </dif>
    <refgrp tip="sin">
      <ref cel="lun">lunarko</ref>,
      <ref cel="kurb.0a">kurba</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_w;&c_m;&c_je;&c_s;&c_ja;&c_c;</trd>,
    <trd>&c_s;&c_je;&c_r;&c_p; <klr>(&c_m;&c_je;&c_s;&c_ja;&c_c;&c_a;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Mondsichel</trd>,
     <trd>Halbmond</trd>
  </trdgrp>
  <trd lng="en">crescent</trd>
  <trd lng="nl">sikkel<klr tip="ind"> (halve maan)</klr></trd>
  <trdgrp lng="hu">
    <trd>f&eacute;lhold<klr tip="ind"> (alak&uacute; t&aacute;rgy)</klr></trd>,
    <trd>sarl&oacute;<klr tip="ind"> (alak&uacute; t&aacute;rgy)</klr></trd>,
    <trd>f&eacute;lk&ouml;r</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_l;&c_u;&c_m;&c_je;&c_s;&c_ja;&c_c;</trd>,
    <trd>&c_s;&c_je;&c_r;&c_p; <klr>(&c_l;&c_u;&c_n;&c_y;)</klr></trd>
  </trdgrp>
</drv>
</art>
