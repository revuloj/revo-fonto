<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>krescent</rad>/o
</kap>
<drv mrk="kresce.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">SCI</uzo>
  <snc>
    <dif>
      Duoncirkla formo de &ccirc;iaspeca objekto.
    </dif>
    <refgrp tip="sin">
      <ref cel="lun">lunarko</ref>,
      <ref cel="kurb.0a">kurba</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Mondsichel</trd>,
     <trd>Halbmond</trd>
  </trdgrp>
  <trd lng="en">crescent</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_l;&c_u;&c_m;&c_je;&c_s;&c_ja;&c_c;</trd>,
    <trd>&c_s;&c_je;&c_r;&c_p; <klr>(&c_l;&c_u;&c_n;&c_y;)</klr></trd>
  </trdgrp>
</drv>
</art>
