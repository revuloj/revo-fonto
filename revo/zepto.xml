<art mrk="$Id: zepto.xml,v 1.6 2022/03/24 15:59:19 revo Exp $">
<kap>
    <rad>zepto</rad>/- <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="zepto.0">
  <kap><tld/>-</kap>
  <mlg>z</mlg>
  <gra><vspec>prefikso</vspec></gra>
  <snc mrk="zepto.0.MEZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      &leftquot;Triiliardono da&rightquot;, 10<sup>-21</sup>
      en la Internacia Sistemo de unuoj (SI):
      <ekz>
        zC &ndash; <tld/>kulombo (mil-triilion-ona kulombo)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=52797">Vortoj por grandegaj nombroj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="zeta.0">zeta-</ref>
  </snc>
  <trd lng="be">&c_z;&c_je;&c_p;&c_t;&c_a;-</trd>
  <trd lng="bg">&c_s;&c_je;&c_p;&c_t;&c_o;-</trd>
  <trd lng="ca">zepto-</trd>
  <trd lng="de">Zepto-</trd>
  <trd lng="en">zepto-</trd>
  <trd lng="es">zepto-</trd>
  <trd lng="fa">&a_z;&f_p;&a_t;&a_w;</trd>
  <trd lng="fr">zepto-</trd>
  <trd lng="hu">zepto-</trd>
  <trd lng="it">zepto-</trd>
  <trd lng="nl">zepto-</trd>
  <trd lng="no">zepto-</trd>
  <trd lng="pl">zepto-</trd>
  <trd lng="pt">zepto-</trd>
  <trd lng="ru">&c_z;&c_je;&c_p;&c_t;&c_o;-</trd>
  <trd lng="sv">zepto-</trd>
  <trd lng="uk">&#x434;&#x437;&#x435;&#x43F;&#x442;&#x43E;-</trd>
</drv>
</art>

