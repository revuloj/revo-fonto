<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>dupleks</rad>/a
</kap>
<drv mrk="duplek.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">TEL</uzo>
  <snc>
    <dif>
      Rilatanta al sistemo de senfadena a&ubreve; fadena telegrafio
      a&ubreve; telefonio
      kaj kiu ebligas al &ccirc;iu stacio efektivigi samtempe la sendon
      kaj la
      ricevon de komunikoj.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_u;&c_p;&c_l;&c_je;&c_k;&c_s;&c_n;&c_y;</trd>
  <trd lng="ca">d&uacute;plex</trd>
  <trdgrp lng="cs">
    <trd>duplexní</trd>,
    <trd>obousměrný</trd>
  </trdgrp>
  <trd lng="de">Duplex-</trd>
  <trd lng="en">full <ind>duplex</ind></trd>
  <trd lng="es">d&uacute;plex</trd>
  <trdgrp lng="fr">
    <trd>bidirectionnel</trd>,
    <trd>duplex</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>duplex</trd>,
    <trd>k&eacute;tir&aacute;ny&uacute;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dupleksowy</trd>,
    <trd>dwukierunkowy</trd>
  </trdgrp>
  <trd lng="pt">d&uacute;plex</trd>
  <trd
  lng="ru">&c_d;&c_u;&c_p;&c_l;&c_je;&c_k;&c_s;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">obojsmerný</trd>
</drv>
</art>
