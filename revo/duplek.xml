<vortaro>
<art mrk="$Id$">
<kap>
  <rad>dupleks</rad>/a
</kap>
<drv mrk="duplek.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">TEL</uzo>
  <snc>
    <dif>
      Rilatanta al sistemo de senfadena a&ubreve; fadena telegrafio a&ubreve; telefonio
      kaj kiu ebligas al &ccirc;iu stacio efektivigi samtempe la sendon kaj la
      ricevon de komunikoj.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_u;&c_p;&c_l;&c_je;&c_k;&c_s;&c_n;&c_y;</trd>
  <trd lng="ca">d&uacute;plex</trd>
  <trd lng="de">Duplex-</trd>
  <trd lng="en">full <ind>duplex</ind></trd>
  <trdgrp lng="fr">
    <trd>bidirectionnel</trd>,
    <trd>duplex</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>duplex</trd>,
    <trd>k&eacute;tir&aacute;ny&uacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_u;&c_p;&c_l;&c_je;&c_k;&c_s;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
