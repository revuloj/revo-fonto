<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tomb.xml,v 1.27 2022/05/13 13:08:00 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tomb</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tomb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tomb.0o.FUN">
    <dif>
      Fosa&jcirc;o en tero a&ubreve; &scirc;tona monumento, kien oni metas 
      <ref tip="prt" cel="mort.0into">mortinto</ref>n:
      <ekz>
        en la plej elektitaj niaj <tld/>oj enterigu vian mortintinon
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 23:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        la knabino &ccirc;iutage iris al la <tld/>o de la patrino kaj ploris
        <fnt>
          <bib>ElektFab</bib>
          <lok>Cindrulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iujn tri, &ccirc;ar ili <klr>[&#8230;]</klr> tiel amis unu la alian, oni metis en unu
        <tld/>on
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tri fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni iros en la <tld/>on kun la konscio, ke nia afero ne mortos
        <fnt>
          <bib>EE</bib>
          <lok>IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        tie &ccirc;i sin trovas la <tld/>oj de miaj avoj kaj praavoj
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Pri la homaranismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[iam]</klr> la frunto kovri&gcirc;os per tia <tld/>a paleco
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ploranta super la <tld/>oj de siaj esperoj kaj amo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[&ccirc;io &ccirc;i]</klr> alproksimigas min per unu pa&scirc;o al la <tld/>o
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        stari per unu piedo en la <tld/>o
        <klr>(<ref cel="risk.0i.suferon">riski</ref> tro)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        atendis, meditis, &gcirc;is en <tld/>on englitis
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="kript.0o">kripto</ref>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_g;&c_ib;&c_l;&c_a;</trd>
  <trd lng="bg">&c_g;&c_r;&c_o;&c_b;</trd>
  <trdgrp lng="ca">
    <trd>tomba</trd>,
    <trd>sepulcre</trd>
  </trdgrp>
  <trd lng="cs">hrobov&eacute; m&iacute;sto</trd>
  <trd lng="de">Grab</trd>
  <trdgrp lng="en">
    <trd>grave</trd>,
    <trd>tomb</trd>,
    <trd>vault</trd>
  </trdgrp>
  <trd lng="es">tumba</trd>
  <trdgrp lng="fr">
    <trd>tombe</trd>,
    <trd>tombeau</trd>
  </trdgrp>
  <trd lng="hu">s&iacute;r</trd>
  <trd lng="nl">graf</trd>
  <trdgrp lng="pl">
    <trd>gr&oacute;b</trd>,
    <trd>mogi&lstroke;a</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>t&uacute;mulo</trd>,
    <trd>tumba</trd>,
    <trd>jazigo</trd>,
    <trd>sepultura</trd>
  </trdgrp>
  <trd lng="ro">morm&acirc;nt</trd>
  <trd lng="ru">&c_m;&c_o;&c_g;&c_i;&c_l;&c_a;</trd>
  <trd lng="sk">hrobov&eacute; miesto</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_o;&c_g;&c_i;&c_l;&c_a;</trd>,
    <trd>&c_g;&c_r;&c_ib;&c_b;</trd>
  </trdgrp>
</drv>

<drv mrk="tomb.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="tomb.0ejo.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Loko destinita por enhavi plurajn a&ubreve; 
      multajn <ref tip="prt" cel="tomb.0o"><tld/>ojn</ref>:
      <ekz>
        oni &jcirc;etis la cindron sur la <tld/>ejon de la simpla popolo
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 23:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        li iris ankora&ubreve; sur la <tld/>ejon, al la <tld/>o de sia patro, pre&gcirc;is sian Patro
        nia'n
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiuj geedzoj estis el la lastaj, kiujn oni enterigis sur la malnova mona&hcirc;eja
        <tld/>ejo
        <fnt>
          <bib>Fab3</bib>
          <lok>Malnova tomba &scirc;tono</lok>
        </fnt>;
      </ekz>
      <ekz>
        la piramidoj formas nordan finon de la grandega <tld/>ejo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ctl>La Litomi&scirc;la <tld/>ejo</ctl>
        <klr>(romano de Karolo <nom>Pi&ccirc;</nom>)</klr>
        <fnt>
          <aut>K. Pi&ccirc;</aut>,
          <vrk>La Litomi&scirc;la Tombejo</vrk>,
          <lok>1981</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_o;&c_g;&c_ib;&c_l;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_g;&c_r;&c_o;&c_b;&c_i;&c_shch;&c_je;</trd>
  <trd lng="bo">&t_da;&t_shabchu;&t_ra;&t_tsheg;&t_kha;&t_rata;&t_naro;&t_da;&t_tsheg;</trd>
  <trdgrp lng="ca">
    <trd>fossar</trd>,
    <trd>cementiri</trd>
  </trdgrp>
  <trd lng="cs">h&rcaron;bitov</trd>
  <trd lng="de">Friedhof</trd>
  <trdgrp lng="en">
    <trd>graveyard</trd>,
    <trd>cemetery</trd>,
    <trd>necropolis</trd>
  </trdgrp>
  <trd lng="es">cementerio</trd>
  <trd lng="fr">cimeti&egrave;re</trd>
  <trd lng="hu">temet&odblac;</trd>
  <trd lng="nl">begraafplaats</trd>
  <trd lng="pt">cemit&eacute;rio</trd>
  <trd lng="pl">cmentarz</trd>
  <trd lng="ro">cimitir</trd>
  <trd lng="ru">&c_k;&c_l;&c_a;&c_d;&c_b;&c_i;&c_shch;&c_je;</trd>
  <trd lng="sk">cintor&iacute;n</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_l;&c_a;&c_d;&c_o;&c_v;&c_i;&c_shch;&c_je;</trd>,
    <trd>&c_c;&c_v;&c_i;&c_n;&c_t;&c_a;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="tomb.en0igi">
  <kap>en<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="tomb.en0igi.FUN">
    <dif>
      Meti mortinton en <tld/>on; <ref tip="sin" cel="sepult.0i">sepulti</ref>:
      <ekz>
        en<tld/>igita en bona maljuneco
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 15:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam Jesuo alvenis, li trovis lin jam kvar tagojn en<tld/>igita
        <fnt><bib>NT</bib>, <lok>&Joh; 11:17</lok></fnt>;
      </ekz>
      <ekz>
        pre&gcirc;ejo, en kiu estas en<tld/>igita nia patrino
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;i mallevis la kapon sub la premo de amaso da rememoroj kaj en<tld/>igitaj
        esperoj
        <fnt>
          <bib>IKr</bib>
          <lok>Neniam Plu!</lok>
        </fnt>.
      </ekz>
    </dif> 
  </snc>
  <trd lng="be">&c_h;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_u;
  &c_m;&c_a;&c_g;&c_ib;&c_l;&c_u;)</klr></trd>
  <trd lng="bg">&c_p;&c_o;&c_g;&c_r;&c_je;&c_b;&c_v;&c_a;&c_m;</trd>
  <trdgrp lng="ca">
    <trd>sebollir</trd>,
    <trd>sepultar</trd>,
    <trd>enterrar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>poh&rcaron;b&iacute;t</trd>,
    <trd>ulo&zcaron;it do hrobu</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>beerdigen</trd>,
    <trd>begraben</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>entomb</trd>,
    <trd>bury <klr tip="amb">(in grave)</klr></trd>,
    <trd>inter</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>enterrar<klr> (meter en una tumba)</klr></trd>,
    <trd>sepultar<klr> (meter en una tumba)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ensevelir<klr> (mettre en tombe)</klr></trd>,
    <trd>enterrer<klr> (mettre en tombe)</klr></trd>
  </trdgrp>
  <trd lng="hu">eltemet</trd>
  <trd lng="nl">begraven</trd>
  <trd lng="pl">pochowa&cacute; <klr>(z&lstroke;o&zdot;y&cacute; do grobu)</klr></trd>
  <trdgrp lng="pt">
    <trd>enterrar</trd>,
    <trd>sepultar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_h;&c_o;&c_r;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_h;&c_o;&c_r;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">pochova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_h;&c_o;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_h;&c_o;&c_r;&c_o;&c_n;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_p;&c_u;&c_s;&c_k;&c_a;&c_t;&c_i; &c_v; &c_m;&c_o;&c_g;&c_i;&c_l;&c_u;</trd>
  </trdgrp>
</drv>

<drv mrk="tomb.en0igo">
  <kap>en<tld/>igo</kap>
  <snc mrk="tomb.en0igo.FUN">
    <dif>
      Meto de mortinto en <tld/>on:
      <ekz>
        &scirc;i anta&ubreve;faris la &scirc;miron de mia korpo por la en<tld/>igo
        <fnt>
          <bib>NT</bib>
          <lok>Marko 14:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        li decidis veni al &scirc;ia en<tld/>igo
        <fnt>
          <aut>A. Pu&scirc;kin, trad. A. Kor&jcirc;enkov</aut>,
          <vrk><url ref="http://www.esperanto-ondo.ru/Libroj/V_bibl01.pdf">
          La pika damo</url></vrk>,
          <lok>2003-2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_h;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="cs">ulo&zcaron;en&iacute; do hrobu</trd>
  <trdgrp lng="de">
    <trd>Beerdigung</trd>,
    <trd>Begr&auml;bnis</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>burial</trd>,
    <trd>interment</trd>,
    <trd>funeral</trd>,
    <trd>entombment</trd>
  </trdgrp>
  <trd lng="es">entierro</trd>
  <trd lng="fr">mise au <ind>tombeau</ind></trd>
  <trdgrp lng="pl">
    <trd>pogrzeb</trd>,
    <trd>poch&oacute;wek</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pochovanie</trd>,
    <trd>pohreb</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_h;&c_o;&c_r;&c_o;&c_n;</trd>,
    <trd>&c_p;&c_o;&c_h;&c_o;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
