<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tomb</rad>/o
</kap>
<drv mrk="tomb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Fosa&jcirc;o en tero a&ubreve; &scirc;tona monumento, kien oni metas mortinton:
      <ekz>
        kiel mortigitoj, ku&scirc;antaj en la <tld/>o
        <fnt><bib>MT</bib>, <lok>&Psa; 88:5</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la frunto kovri&gcirc;is per <tld/>a paleco<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_g;&c_ib;&c_l;&c_a;</trd>
  <trd lng="de">Grab</trd>
  <trd lng="fr">tombe</trd>
  <trd lng="hu">s&iacute;r</trd>  
  <trd lng="nl">graf</trd>
  <trdgrp lng="pt">
    <trd>t&uacute;mulo</trd>,
    <trd>tumba</trd>,
    <trd>jazigo</trd>,
    <trd>sepultura</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_o;&c_g;&c_i;&c_l;&c_a;</trd>
</drv>

<drv mrk="tomb.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Loko destinita por enhavi plurajn a&ubreve; multajn <ref tip="prt" cel="tomb.0o"><tld/>ojn</ref>:
      <ekz>
        <ctl>La Litomi&scirc;la <tld/>ejo</ctl>
        <klr>(romano de Karolo Pi&ccirc;)</klr>
        <fnt><aut>Pi&ccirc; Karolo</aut>, <vrk>La Litomi&scirc;la Tombejo. -
        Saarbr&uuml;cken: Artur E. Iltis, 1981</vrk></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_o;&c_g;&c_ib;&c_l;&c_k;&c_ib;</trd>
  <trd lng="de">Friedhof</trd>
  <trd lng="fr">cimeti&egrave;re</trd>
  <trd lng="hu">temet&odblac;</trd>  
  <trd lng="nl">begraafplaats</trd>
  <trd lng="pt">cemit&eacute;rio</trd>
  <trd lng="ru">&c_k;&c_l;&c_a;&c_d;&c_b;&c_i;&c_shch;&c_je;</trd>
</drv>

<drv mrk="tomb.en0igi">
  <kap>en<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="sepult.0i">Sepulti</ref>, metante en <tld/>on:
      <ekz>
        kiam Jesuo alvenis, li trovis lin jam kvar tagojn en<tld/>igita
        <fnt><bib>NT</bib>, <lok>&Joh; 11:17</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_u; &c_m;&c_a;&c_g;&c_ib;&c_l;&c_u;)</klr></trd>
  <trd lng="de">beerdigen</trd>
  <trdgrp lng="fr">
    <trd>ensevelir<klr> (mettre en tombe)</klr></trd>,
    <trd>enterrer<klr> (mettre en tombe)</klr></trd>
  </trdgrp>
  <trd lng="hu">eltemet</trd>  
  <trd lng="nl">begraven</trd>
  <trdgrp lng="pt">
    <trd>enterrar</trd>,
    <trd>sepultar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_h;&c_o;&c_r;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_h;&c_o;&c_r;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
