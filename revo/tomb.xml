<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tomb</rad>/o
</kap>
<drv mrk="tomb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Fosa&jcirc;o en tero a&ubreve; &scirc;tona monumento, kien oni metas mortinton:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la frunto kovri&gcirc;is per <tld/>a paleco<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Grab</trd>
  <trd lng="nl">graf</trd>
  <trd lng="ru">&c_m;&c_o;&c_g;&c_i;&c_l;&c_a;</trd>
</drv>

<drv mrk="tomb.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Loko destinita por enhavi plurajn a&ubreve; multajn <tld/>ojn.
    </dif>
  </snc>
  <trd lng="de">Friedhof</trd>
  <trd lng="nl">begraafplaats</trd>
  <trd lng="ru">&c_k;&c_l;&c_a;&c_d;&c_b;&c_i;&c_shch;&c_je;</trd>
</drv>

<drv mrk="tomb.en0igi">
  <kap>en<tld/>igi</kap>
  <snc>
    <dif>
      Meti en <tld/>on.
    </dif>
  </snc>
  <trd lng="de">beerdigen</trd>
  <trd lng="nl">begraven</trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_o;&c_r;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_h;&c_o;&c_r;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
