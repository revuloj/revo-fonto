<drv mrk="sabl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Sur&scirc;uti per <tld/>o:
      <ekz>
        <tld/>ita vojeto
        <fnt>
          <aut>August Strindberg</aut>
          <vrk><url ref="http://i-espero.info/files/elibroj/eo%20-%20strindberg,%20%20august%20-%20la%20konscienco%20riprocxas.pdf">La konscienco ripro&ccirc;as</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_y;&c_p;&c_a;&c_c;&c_mol;
    <ind>&c_p;&c_ja;&c_s;&c_k;&c_o;&c_m;</ind></trd>
  <trdgrp lng="cs">
    <trd>pokr&yacute;t p&iacute;skem</trd>,
    <trd>posypat p&iacute;skem</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>besanden</trd>,
    <trd>mit Sand <ind>bestreuen</ind></trd>,
  </trdgrp>
  <trd lng="es">enterrar</trd>
  <trd lng="fr">sabler</trd>
  <trd lng="hu">homokkal sz&oacute;r</trd>
  <trd lng="it">sabbiare</trd>
  <trd lng="nl">met <ind>zand</ind> bestrooien</trd>
  <trdgrp lng="pl">
    <trd>sypa&cacute; <ind>piasek</ind></trd>,
    <trd>zapiaszcza&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_y;&c_p;&c_a;&c_t;&c_mol; &c_p;&c_je;&c_s;&c_k;&c_o;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>pieskova&tcaron;</trd>,
    <trd>posypa&tcaron;</trd>,
    <trd>vysypa&tcaron; pieskom</trd>
  </trdgrp>
  <trd lng="sv">sanda</trd>
  <trd lng="tr">kumlamak</trd>
</drv>

<drv mrk="sabl.0ajxo">
  <kap><ofc>*</ofc><tld/>a&jcirc;o <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5188">sabl'a&jcirc;'</url></lok></fnt></kap>
  <snc>
    <dif>
      <tld lit="S"/>a malprofunda&jcirc;o en rivero a&ubreve; maro, malfaciliganta a&ubreve;
      endan&gcirc;eriganta la navigadon: 
      <ekz>
        &scirc;iprompi&gcirc;oj sekvis unu la alian sur la rifoj kaj <tld/>a&jcirc;oj
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili ekvidis unu golfeton kun <tld/>a&jcirc;o, kaj ili konsili&gcirc;is inter si, &ccirc;u ili
        povos surpeli sur &gcirc;in la &scirc;ipon
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 27:39</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Sandbank</trd>
  <trd lng="en">sandbank</trd>
  <trd lng="fr">banc de sable</trd>
</drv>

<drv mrk="sabl.0ero">
  <kap><ofc>*</ofc><tld/>ero
  <fnt><bib>F</bib>, <lok>Universala Vortaro, <url ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-1071">-er'</url></lok></fnt></kap>
   <snc>
    <dif>
      Grajno de <tld/>o:
      <ekz>
        <tld/>ero enfalis en mian okulon
        <fnt><bib>F</bib>, <lok>&FE; 41</lok></fnt>;
      </ekz>
      <ekz>
        pecetoj <klr>[&#x2026;]</klr> ne pli grandaj ol <tld/>ero
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malgrandaj <tld/>eroj brulvundis kvaza&ubreve; fajreroj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        la centrifuga forto koncentras <tld/>erojn pli pezajn ol kvarco en unu lokon
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="https://www.monato.be/2010/009963.php">Ora febro en Litovio</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Sandkorn</trd>
  <trd lng="en">grain of sand</trd>
  <trd lng="fr">grain de sable</trd>
</drv>


<drv mrk="sabl.0osxtono">
  <kap><tld/>o&scirc;tono</kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Speco de roka &scirc;tono, konsistanta el silikaj grajnetoj
      aglomeritaj en kalkecan a&ubreve; silikecan mason:
      <ekz>
        kvarca <tld/>o&scirc;tono je 90&#x202F;% konsistas el kvarco;
      </ekz><ekz>
        la eksteraj muroj de &ccirc;i tiuj konstrua&jcirc;oj
        el griza <tld/>o&scirc;tono de la malsupro &gcirc;is la tegmento estis
        kovritaj per skulpta&jcirc;oj
	<fnt><bib>Far1</bib>, <lok>&Ccirc;ap. 6a.</lok></fnt>;
      </ekz><ekz>
        la altaj rokoj &ccirc;irka&ubreve; la rivero konsistas el ru&gcirc;aj
        <tld/>o&scirc;tonoj, verdecaj argilaj grundoj kaj helaj
        kalk&scirc;tonoj
        <fnt>
          <aut>Georgi Mihalkov</aut>:
          <vrk>Pitoreska loko</vrk>. <bib>Monato</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="grejs.0o">grejso</ref>
  </snc>
  <trd lng="be">&c_p;&c_ja;&c_sh;&c_ch;&c_a;&c_n;&c_ib;&c_k;</trd>
  <trd lng="de">Sandstein</trd>
  <trd lng="en">sandstone</trd>
  <trd lng="fr">gr&egrave;s<klr tip="ind"> (roche)</klr></trd>
  <trd lng="he">&alef;&bet;&fnun; &het;&vav;&lamed;</trd>
  <trd lng="nl">zandsteen</trd>
  <trd lng="hu">homokk&odblac;</trd>
  <trd lng="it">arenaria</trd>
  <trd lng="nl">zandsteen</trd>
  <trd lng="pl">piaskowiec</trd>
  <trdgrp lng="pt">
    <trd>gr&eacute;s<klr tip="amb"> (rocha)</klr></trd>,
    <trd>arenito</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_s;&c_ch;&c_a;&c_n;&c_i;&c_k;</trd>
  <trd lng="tr">kumta&scedil;&inodot;</trd>
</drv>


<drv mrk="sabl.0ujo">
  <kap><ofc>*</ofc><tld/>ujo</kap>
  <snc mrk="sabl.0ujo.cxia">
    <dif>
      &Ccirc;ia ujo por enteni <tld/>on, ekz-e por malglitigi vojojn en vintro, a&ubreve;:
    </dif>  
    <subsnc mrk="sabl.0ujo.skribilara">
      <uzo tip="stl">ARK</uzo>
      <dif>
        uzatan por sekigi inkan skriba&jcirc;on:
        <ekz>
          mia <tld/>ilaro konsistas el inkujo, sablujo, kelke da
          plumoj, krajono kaj inksorbilo
          <fnt><bib>F</bib>, <lok>&FE; 34</lok></fnt>.
        </ekz>
      </dif>
        <trd lng="de">Sandf&auml;sschen</trd>
    </subsnc>
    <subsnc mrk="sabl.0ujo.ludejo">
      <dif>
        <ref tip="dif" cel="sabl.0ludejo"><tld lit="S"/>oludejo</ref>:
        <ekz>
          la <tld/>ujo trovi&gcirc;as tuj ekster la kuireja fenestro
          <fnt>
            <vrk><url ref="https://lavilleheleuc.com/eo/ludejo/">Ludejoj - Domaine de la Ville Heleuc</url></vrk>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <trd lng="de">Sandkiste</trd>
  </snc>
</drv>


<drv mrk="sabl.0ludejo">
  <kap><tld/>oludejo, <var><kap><tld/>okesto <fnt><bib>HejmVort</bib></fnt></kap></var></kap>
  <snc>
    <uzo tip="fak">SPO</uzo>
    <dif>
      <tld lit="S"/>ujo, en kiu infanoj povas ludi fosante 
      truojn, kreante sablokastelojn, ktp:
      <ekz>
        tiu <tld/>o, kiu jam estis en la sablokestoj, ne havis la grandecon, kiun
        EU-direktivo preskribis
        <fnt>
          <bib>Monato</bib>
          <aut>Bo Sandelin</aut>
          <vrk><url ref="https://www.monato.be/2004/008235.html">Svedoj balotis kontra&ubreve; e&ubreve;ro</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel infanoj en <tld/>okesto, kiuj nepre volas havi la saman muldilon, per kiu
        ludas najbara infano
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2007/009248.php">EU-malsano</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        ludantaj infanoj en
        <tld/>oludejo, kiuj ploras, kiam iu de tempo al tempo detruas 
        ilian malbone konstruitan dometon el <tld/>o
        <fnt>
          <aut>Petro</aut>,
          <vrk>
            <url ref="http://eo.wikipedia.org/wiki/Vikipedio:Babilejo/Arkivo_2005_7">
              Ar&hcirc;ivo de Vikipedia babilejo</url>
          </vrk>,
          <bib>Viki</bib>,
          <lok>&ccirc;. Propono</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="cs">p&iacute;skovi&scaron;t&ecaron;</trd>
    <trd lng="pl">piaskownica</trd>
    <trd lng="it">sabbionaia</trd>
    <trd lng="sk">pieskovisko</trd>
    <trd lng="ru">&c_p;&c_je;&c_s;&c_o;&c_ch;&c_n;&c_i;&c_c;&c_a;</trd>
  </snc>
<trd lng="de">Sandkasten</trd>
</drv>


