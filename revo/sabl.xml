<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: sabl.xml,v 1.30 2021/03/20 16:03:44 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>sabl</rad>/o
    </kap>

    <drv mrk="sabl.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc>
        <dif>
          Minerala, diversspeca, pli malpli delikata polvo, formita de
          diseri&gcirc;o de &scirc;tonoj, precipe de kvarco:
          <ekz>
            <uzo tip="stl">FIG</uzo>
            &jcirc;eti <tld/>on en la okulojn <klr>(blindigi)</klr>
            de la iluziita homaro;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            konstrui sur la <tld/>o<fnt>B</fnt>
            <klr>(f. malfortike.
              <ref tip="vid" cel="rok.0o">roko</ref>)</klr>.
          </ekz>
        </dif>
        <ref tip="sin" cel="polv.0o">polvo</ref>
      </snc>
  <trd lng="be">&c_p;&c_ja;&c_s;&c_o;&c_k;</trd>
  <trd lng="bg">&c_p;&c_ja;&c_s;&c_malmol;&c_k;</trd>
  <trd lng="cs">p&iacute;sek</trd>
  <trd lng="de">Sand</trd>
  <trd lng="en">sand</trd>
  <trd lng="es">arena</trd>
  <trd lng="fr">sable</trd>
  <trd lng="he">&het;&vav;&lamed;</trd>
  <trd lng="hu">homok</trd>
  <trd lng="id">pasir</trd>
  <trd lng="it">sabbia</trd>
  <trd lng="nl">zand</trd>
  <trdgrp lng="pl">
        <trd>piasek</trd>,
        <trd>piach</trd>
      </trdgrp>
  <trd lng="pt">areia</trd>
  <trd lng="ru">&c_p;&c_je;&c_s;&c_o;&c_k;</trd>
  <trd lng="sk">piesok</trd>
  <trd lng="sv">sand</trd>
  <trd lng="tr">kum</trd>
</drv>


<drv mrk="sabl.0ajxo">
  <kap><ofc>*</ofc><tld/>a&jcirc;o <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url
  ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5188">sabl'a&jcirc;'</url></lok></fnt></kap>
  <snc>
    <dif>
      sabla malprofunda&jcirc;o en rivero a&ubreve; maro, malfaciliganta a&ubreve;
      endan&gcirc;eriganta la navigadon: 
      <ekz>&scirc;iprompi&gcirc;oj sekvis unu la alian sur la rifoj k sabla&jcirc;oj
      <fnt><bib>Z</bib></fnt>.</ekz>
    </dif>
  </snc>
  <trd lng="de">Sandbank</trd>
  <trd lng="en">sandbank</trd>
  <trd lng="fr">banc de sable</trd>
</drv>

<drv mrk="sabl.0ero">
 <kap><ofc>*</ofc><tld/>ero
 <fnt><bib>F</bib>, <lok>Universala Vortaro, <url
 ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-1071">-er'</url></lok></fnt></kap>
    <snc>
      <dif>
      Grajno de <tld/>o:
      <ekz>
        <tld/>ero enfalis en mian okulon
        <fnt><bib>F</bib>, <lok>&FE; 41</lok></fnt>.
      </ekz>
      </dif>
   </snc>
  <trd lng="en">grain of sand</trd>
  <trd lng="fr">grain de sable</trd>
</drv>

    <drv mrk="sabl.0i">
      <kap><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Sur&scirc;uti per <tld/>o:
          <ekz>
            <tld/>ita vojo<fnt>B</fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_y;&c_p;&c_a;&c_c;&c_mol;
        <ind>&c_p;&c_ja;&c_s;&c_k;&c_o;&c_m;</ind></trd>
  <trdgrp lng="cs">
    <trd>pokr&yacute;t p&iacute;skem</trd>,
    <trd>posypat p&iacute;skem</trd>
  </trdgrp>
  <trdgrp lng="de">
        <trd>sanden</trd>,
        <trd>einsanden</trd>,
        <trd>mit Sand <ind>bestreuen</ind></trd>
      </trdgrp>
  <trd lng="es">enterrar</trd>
  <trd lng="fr">sabler</trd>
  <trd lng="hu">homokkal sz&oacute;r</trd>
  <trd lng="it">sabbiare</trd>
  <trd lng="nl">met <ind>zand</ind> bestrooien</trd>
  <trdgrp lng="pl">
        <trd>sypa&cacute; <ind>piasek</ind></trd>,
        <trd>zapiaszcza&cacute;</trd>
      </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_y;&c_p;&c_a;&c_t;&c_mol; &c_p;&c_je;&c_s;&c_k;&c_o;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>pieskova&tcaron;</trd>,
    <trd>posypa&tcaron;</trd>,
    <trd>vysypa&tcaron; pieskom</trd>
  </trdgrp>
  <trd lng="sv">sanda</trd>
  <trd lng="tr">kumlamak</trd>
</drv>

    <drv mrk="sabl.0osxtono">
      <kap><tld/>o&scirc;tono</kap>
      <uzo tip="fak">MIN</uzo>
      <snc>
        <dif>
          Speco de roka &scirc;tono, konsistanta el silikaj grajnetoj
          aglomeritaj en kalkecan a&ubreve; silikecan mason:
          <ekz>
            kvarca <tld/>o&scirc;tono je 90% konsistas el kvarco;
          </ekz><ekz>
            la eksteraj muroj de &ccirc;i tiuj konstrua&jcirc;oj
            el griza <tld/>o&scirc;tono de la malsupro &gcirc;is la tegmento estis
            kovritaj per skulpta&jcirc;oj
	    <fnt><bib>Far1</bib>, <lok>&Ccirc;ap. 6a.</lok></fnt>;
          </ekz><ekz>
            la altaj rokoj &ccirc;irka&ubreve; la rivero konsistas el ru&gcirc;aj
            <tld/>o&scirc;tonoj, verdecaj argilaj grundoj kaj helaj
            kalk&scirc;tonoj
            <fnt>
              <aut>Georgi MIHALKOV</aut>:
              <vrk>Pitoreska loko</vrk>. <bib>Monato</bib>
            </fnt>.
          </ekz>
        </dif>
        <ref tip="vid" cel="grejs.0o">grejso</ref>
      </snc>
      <trd lng="be">&c_p;&c_ja;&c_sh;&c_ch;&c_a;&c_n;&c_ib;&c_k;</trd>
      <trd lng="de">Sandstein</trd>
      <trd lng="en">sandstone</trd>
      <trd lng="fr">gr&egrave;s<klr tip="ind"> (roche)</klr></trd>
      <trd lng="he">&alef;&bet;&fnun; &het;&vav;&lamed;</trd>
      <trd lng="nl">zandsteen</trd>
      <trd lng="hu">homokk&odblac;</trd>
      <trd lng="it">arenaria</trd>
      <trd lng="nl">zandsteen</trd>
      <trd lng="pl">piaskowiec</trd>
      <trdgrp lng="pt">
        <trd>gr&eacute;s<klr tip="amb"> (rocha)</klr></trd>,
        <trd>arenito</trd>
      </trdgrp>
      <trd lng="ru">&c_p;&c_je;&c_s;&c_ch;&c_a;&c_n;&c_i;&c_k;</trd>
      <trd lng="tr">kumta&scedil;&inodot;</trd>
    </drv>

    <drv mrk="sabl.0ludejo">
      <kap><tld/>oludejo</kap>
      <snc>
        <uzo tip="fak">SPO</uzo>
        <dif>
          Ujo plenigita per sablo, en kiu infanoj povas ludi fosante 
          truojn, kreante sablokastelojn, ktp.
          <ekz>
            ni estas plenkreskuloj kaj ne ludantaj infanoj en
            sabloludejo,
            kiuj ploras, kiam iu de tempo al tempo detruas ilian malbone
            konstruitan dometon el sablo
            <fnt>
              <aut>Petro</aut>,
              <vrk>
                <url
                ref="http://eo.wikipedia.org/wiki/Vikipedio:Babilejo/Arkivo_2005_7">
                  Ar&hcirc;ivo de Vikipedia babilejo</url>
              </vrk>,
              <bib>Viki</bib>,
              <lok>&ccirc;. Propono</lok>
            </fnt>
          </ekz>
        </dif>
        <trd lng="pl">piaskownica</trd>
        <trd lng="it">sabbionaia</trd>
        <trd lng="ru">&c_p;&c_je;&c_s;&c_o;&c_ch;&c_n;&c_i;&c_c;&c_a;</trd>
      </snc>
  <trd lng="cs">p&iacute;skovi&scaron;t&ecaron;</trd>
  <trd lng="sk">pieskovisko</trd>
</drv>

    <drv mrk="sabl.0ujo">
      <kap><ofc>*</ofc><tld/>ujo</kap>
      <snc mrk="sabl.0ujo.ludejo">
        <uzo tip="fak">SPO</uzo>
        <dif>
          <ref tip="dif" cel="sabl.0ludejo">sabloludejo</ref>,
        </dif>
      </snc>
      <snc mrk="sabl.0ujo.ujo">
        <uzo tip="stl">ARK</uzo>
        <dif>
          Ujo enhavanta <tld/>on uzatan por sekigi inkan skriba&jcirc;on
          <ekz>
            mia <tld/>ilaro konsistas el inkujo, sablujo, kelke da
            plumoj,
            krajono kaj inksorbilo
            <fnt><bib>F</bib>, <lok>&FE; 34</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="it">sabbionaia</trd>
      <trd lng="ru">&c_p;&c_je;&c_s;&c_o;&c_ch;&c_n;&c_i;&c_c;&c_a;</trd>
    </drv>

<drv mrk="sabl.0okesto">
   <kap><tld/>okesto <fnt><bib>HejmVort</bib></fnt></kap>
    <snc mrk="sabl.0okesto.ludejo">
      <uzo tip="fak">SPO</uzo>
        <dif>
        <ref tip="dif" cel="sabl.0ludejo">sabloludejo</ref>,
        </dif>
    </snc>
    <trd lng="it">sabbionaia</trd>
    <trd lng="ru">&c_p;&c_je;&c_s;&c_o;&c_ch;&c_n;&c_i;&c_c;&c_a;</trd>
</drv>

  </art>
