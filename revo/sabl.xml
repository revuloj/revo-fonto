<drv mrk="sabl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Minerala, diversspeca, pli malpli delikata polvo, formita de
      diseri&gcirc;o de &scirc;tonoj, precipe de <ref cel="kvarc.0o">kvarco</ref>:
      <ekz>
        la <tld/>o sur la bordo de la maro
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 22:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        delikata blanka <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        se mi ilin kalkulus, ili estus pli multaj ol la <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 139:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        viro malsa&gcirc;a, kiu konstruis sian domon sur la <tld/>o
        <klr>(kp statuego kun piedoj el <ref cel="argil.0o">argilo</ref>)</klr>
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 7:26</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la entrepreno ree perdi&gcirc;is sen rezultato <klr>[&#x2026;]</klr> &gcirc;i nome estis
        <ind>konstruita sur <tld/>o</ind>
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
        <trd lng="de">auf <ind>Sand</ind> gebaut</trd>
      </ekz>
      <ekz>
        en la dezerto <klr>[&#x2026;]</klr> vi trafos en <tld/>an <ref cel="tromb.0o.sablo">trombo</ref>n
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <ref cel="dun.0o">duno</ref>j <klr>[&#x2026;]</klr> staris kvaza&ubreve; <klr>[&#x2026;]</klr> rigidi&gcirc;intaj
        ondoj el
        <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la <tld/>orifo staris &scirc;iprompita&jcirc;o, la maro &gcirc;in kovris
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        forrasti la <tld/>on anta&ubreve; la pordo
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &jcirc;eti <tld/>on en la okulojn <klr>(blindigi)</klr>
        de la iluziita homaro;
      </ekz>
      <ekz>
        li e&ccirc; el <tld/>o vipojn tordas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="gruz.0o">gruzo</ref>,
       <ref cel="polv.0o">polvo</ref>,
       <ref cel="rok.0o">roko</ref>
    </refgrp>
  </snc>
