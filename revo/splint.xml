<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: splint.xml,v 1.8 2023/12/10 20:23:54 revo Exp $">
<kap>
  <rad>splint</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="splint.0o">
  <kap><tld/>o</kap>
  <snc mrk="splint.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Tabuleto a&ubreve; aparato, per kiu oni senmovigas
      parton de membro dum kuraci&gcirc;o de rompita osto:
      <ekz>
        unu estis apogi&gcirc;anta sur bastono, alia estis portanta la brakon en <tld/>o
        <fnt>
          <aut>Jurij German, trad. Jurij Finkel</aut>
          <vrk><url ref="https://libro.ee/file.php?id=1057"
              >La afero, al kiu vi servas</url></vrk>
          <lok>Granda kuracisto, p. 254</lok>
        </fnt>;
      </ekz>
      <ekz>
        doktoro <nom>Sui</nom> foj-refoje al&gcirc;ustigis la dikecon de la
        <tld/>oj kaj stre&ccirc;adon de la banda&gcirc;o
        <fnt>
          <aut>Fang Xiaotong</aut>,
          <vrk><url ref="http://esperanto.china.org.cn/medicino/2007-06/14/content_8387461.htm">
          Sui Shuyi &mdash; eminenta &ccirc;inmedicina ostologo</url></vrk>,
          <lok>&Ccirc;ina Interreta Informa Centro, 2007-06-14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_n;&c_a; <klr tip="amb">(&c_m;&c_e;&c_d;.)</klr></trd>
  <trd lng="ca">f&egrave;rula</trd>
  <trd lng="cs">dlaha</trd>
  <trd lng="de">Schiene <klr>(Medizin)</klr></trd>
  <trdgrp lng="fr">
    <trd>attelle</trd>,
    <trd>&eacute;clisse</trd>,
    <trd>orth&egrave;se</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&lstroke;ubki  {pl} <klr>(med.)</klr></trd>,
    <trd>szyna  <klr>(med.)</klr></trd>
  </trdgrp>
  <trd lng="sk">dlaha</trd>
  <trdgrp lng="uk">
    <trd>&c_l;&c_u;&c_b;&c_o;&c_k;</trd>,
    <trd>&c_sh;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="splint.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="splint.0i.MED">
    <dif>
      Provizi per <tld/>o:
      <ekz>
        oni povus plastri kaj <tld/>i la ripojn de la kavaliro
        <fnt>
        <bib>DonKihxoto</bib>,
        <lok>dua parto, &ccirc;apitro 14a, p. 471a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_k;&c_l;&c_a;&c_d;&c_a;&c_c;&c_mol; <ind>&c_sh;&c_y;&c_n;&c_u;</ind> <klr tip="amb">(&c_m;&c_e;&c_d;)</klr></trd>
  <trdgrp lng="ca">
    <trd>immobilitzar</trd>,
    <trd>encanyar</trd>
  </trdgrp>
  <trd lng="de">schienen</trd>
  <trd lng="fr">&eacute;clisser</trd>
  <trd lng="pl">zak&lstroke;ada&cacute;  <ind>szyn&eogonek;</ind></trd>
  <trd lng="uk">&c_n;&c_a;&c_k;&c_l;&c_a;&c_d;&c_a;&c_t;&c_i; &c_sh;&c_i;&c_n;&c_u;</trd>
</drv>
</art>

