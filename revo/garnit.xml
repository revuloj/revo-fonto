<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>garnitur</rad>/o <fnt><bib>PV</bib></fnt> 
</kap>
<drv mrk="garnit.0o">
  <kap><tld/>o</kap>
  <snc mrk="garnit.0o.KOMUNE">
    <dif>
      Aro da samspecaj a&ubreve; samcelaj objektoj, kiuj prezentas unu
      tuta&jcirc;on, komplekson, sortimenton:
      <ekz>
        <tld/>o de kameno <klr>(horlo&gcirc;o kun vazoj ktp)</klr>;
      </ekz>
      <ekz>
        <tld/>o da butonoj por robo;
      </ekz>
      <ekz>
        <tld/>o da &scirc;ra&ubreve;b&scirc;losiloj;
      </ekz>
      <ekz>
        <tld/>o de skribotablo
        <klr>(skriboplato, inkujo, sorbilo, tondilo ktp)</klr>;
      </ekz>
      <ekz>
        <tld/>o por la ungoj <klr>(fajliloj ktp)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      Ne konfuzu kun:
      <ref cel="garn.0ajxo0ilo">garna&jcirc;o</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="ensemb.0o.KOMUNE">ensemblo</ref>,
      <ref cel="komple2.0o.KOMUNE">kompleto</ref>,
      <ref cel="servic.0o.KUI">servico</ref>,
      <ref cel="sortim.0o.KOMUNE">sortimento</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_b;&c_o;&c_r;</trd>,
    <trd>&c_k;&c_a;&c_m;&c_p;&c_l;&c_je;&c_k;&c_t;</trd>,
    <trd>&c_g;&c_a;&c_r;&c_n;&c_ib;&c_t;&c_u;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>assortiment</trd>,
    <trd>joc</trd>
    <trd>guarnici&oacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Satz</trd>,
    <trd>Garnitur</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>garniture</trd>,
    <trd>accessories</trd>,
    <trd>fittings</trd>,
    <trd>trimmings</trd>,
    <trd>kit</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>guarnici&oacute;n</trd>,
    <trd>accesorios</trd>,
  </trdgrp>
  <trdgrp lng="fr">
    <trd>assortiment</trd>,
    <trd>garniture<klr tip="ind"> (assortiment)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&eacute;szlet</trd>,
    <trd>garnit&uacute;ra</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>stel</trd>,
    <trd>assortiment</trd>,
    <trd>garnituur</trd>
  </trdgrp>
  <trdgrp lng="oc">
    <trd>assortiment</trd>,
    <trd>ensortiment</trd>
    <trd>garnidura</trd>
    <trd>garniment</trd>
  </trdgrp>
  <trdgrp lng="pl">
  <trd>garnitur</trd>,
  <trd>komplet</trd>,
  <trd>zestaw</trd>
</trdgrp>
  <trd lng="pt">guarni&ccedil;&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_b;&c_o;&c_r;</trd>,
    <trd>&c_k;&c_o;&c_m;&c_p;&c_l;&c_je;&c_k;&c_t;</trd>,
    <trd>&c_g;&c_a;&c_r;&c_n;&c_i;&c_t;&c_u;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>stavebnica</trd>,
    <trd>súbor</trd>,
    <trd>súprava</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>s&uuml;s</trd>,
    <trd>s&uuml;s gere&ccedil;leri</trd>, 
    <trd>donat&inodot;m</trd>,
    <trd>garnit&uuml;r</trd>
  </trdgrp>
</drv>
</art>
