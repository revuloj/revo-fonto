<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ulem.xml,v 1.14 2024/12/06 08:36:16 revo Exp $">
<kap>
   <rad>ulem</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="ulem.0o">
  <kap><tld/>o</kap>
  <snc mrk="ulem.0o.ISL">
    <uzo tip="fak">ISL</uzo>
    <dif>
      <ref tip="super" cel="kler.0ulo">Klerulo</ref>, konanto de la islamaj sciencoj precipe
      religiaj kaj
      juraj:
      <ekz>
        de tiu malproksima tempo la tabako estas la plezuro
        <klr>[&#8230;]</klr> de la <tld/>oj, kiuj instruas la sa&gcirc;econ
        <fnt><bib>FK</bib>,
          <lok>2, la naski&gcirc;o de la tabako</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="mula.0o">mulao</ref>
  </snc>
  <trd lng="ar">&a_ayn;&a_fatha;&a_A;&a_l;&a_kasra;&a_m;</trd>
  <trdgrp lng="be">
    <trd>&c_u;&c_l;&c_je;&c_m;</trd>,
    <trd>&c_a;&c_l;&c_ib;&c_m;</trd>
  </trdgrp>
  <trd lng="ca">ulema</trd>
  <trd lng="cs">ulema</trd>
  <trdgrp lng="de">
    <trd>Ulema</trd>,
    <trd>Ulama</trd>
  </trdgrp>
  <trd lng="fi">ulema<klr> (islamin uskonoppinut)</klr></trd>
  <trdgrp lng="fr">
     <trd>oul&eacute;ma</trd>,
     <trd>ul&eacute;ma</trd>
   </trdgrp>
  <trd lng="id">ulama</trd>
  <trd lng="it">ulema</trd>
  <trdgrp lng="ja">
    <trd>&#x30A6;&#x30E9;&#x30DE;&#x30FC;</trd>,
    <trd>&#x30A4;&#x30B9;&#x30E9;&#x30E0;&#x6CD5;&#x5B66;&#32773;
    <pr>&#12356;&#12377;&#12425;&#12416;&#x307B;&#12358;&#x304C;&#x304F;&#12375;&#12419;</pr></trd>
  </trdgrp>
  <trd lng="pl">ulema  <klr>(mahometa&nacute;ski teolog)</klr></trd>
  <trd lng="ru">&c_u;&c_l;&c_je;&c_m;</trd>
  <trd lng="sk">ulema</trd>
  <trd lng="uk">&c_u;&#x43B;&c_je;&#x43C; (&c_b;&#x43E;&c_g;&#x43E;&c_s;&#x43B;&#x43E;&c_v;)</trd>
</drv>
</art>

