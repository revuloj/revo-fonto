<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tremp</rad>/i
</kap>
<drv mrk="tremp.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Meti ion en akvon a&ubreve; alian flua&jcirc;on:
      <ekz>
        <tld/>i plumon en inkon;
      </ekz>
      <ekz>
        <tld/>i tukon en tinkturon;
      </ekz>
      <ekz>
        <tld/>i panon en vinon;
      </ekz>
      <ekz>
        atentu, via maniko <tld/>i&gcirc;as en la sa&ubreve;co.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="ban.0i">bani</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>eintauchen</trd>,
    <trd>eintunken</trd>
  </trdgrp>
  <trd lng="en">dip</trd>
  <trd lng="nl">indopen</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_k;&c_u;&c_n;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_k;&c_u;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_m;&c_a;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
