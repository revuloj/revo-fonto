<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tremp.xml,v 1.21 2024/02/04 01:29:02 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tremp</rad>/i
</kap>
<drv mrk="tremp.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Meti ion en akvon a&ubreve; alian flua&jcirc;on:
      <ekz>
        <tld/>i plumon en inkon;
      </ekz>
      <ekz>
        <tld/>i tukon en tinkturon;
      </ekz>
      <ekz>
        <tld/>i panon en vinon;
      </ekz>
      <ekz>
        ili <klr>[&#8230;]</klr> bu&ccirc;is kapron kaj <tld/>is la veston en la sango
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 37:31</lok>
        </fnt>;
      </ekz>
      <ekz>
        prenu hisopon kaj <tld/>u &gcirc;in en akvo, kaj aspergu la tendon
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 19:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pastro <tld/>os sian dekstran fingron en la oleo
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 14:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi eltordis grandan vi&scirc;ilon, <tld/>itan en malvarman akvon
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        lin tre amas la popolamaso: la mankojn liajn la popolo <tld/>as en sian amon
        <fnt>
          <bib>Hamlet</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="super">
      <ref cel="ban.0i">bani</ref>,
      <ref cel="sek.mal0igi">malsekigi</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="merg.0i">mergi</ref>,
      <ref cel="vring.0i">vringi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_k;&c_u;&c_n;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_g;&c_r;&c_u;&c_zh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mo&ccaron;it</trd>,
    <trd>m&aacute;&ccaron;et</trd>,
    <trd>namo&ccaron;it</trd>,
    <trd>nam&aacute;&ccaron;et</trd>,
    <trd>omo&ccaron;it</trd>,
    <trd>smo&ccaron;it</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>eintauchen</trd>,
    <trd>eintunken</trd>,
    <trd>dippen</trd>
  </trdgrp>
  <trd lng="en">dip</trd>
  <trd lng="es">mojar<klr tip="amb"> (algo en un l&iacute;quido)</klr></trd>
  <trd lng="fr">tremper</trd>
  <trdgrp lng="hu">
    <trd>m&aacute;rt</trd>,
    <trd>belem&aacute;rt</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12402;&#x305F;&#12377;</trd>,
    <trd>&#12388;&#12369;&#x308B;</trd>,
    <trd>&#x306C;&#12425;&#12377;</trd>
  </trdgrp>
  <trd lng="nl">indopen</trd>
  <trdgrp lng="pl">
    <trd>moczy&cacute;</trd>,
    <trd>namoczy&cacute;</trd>
  </trdgrp>
  <trd lng="pt">imergir</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_k;&c_u;&c_n;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_k;&c_u;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_m;&c_a;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>m&aacute;&ccaron;a&tcaron;</trd>,
    <trd>namo&ccaron;i&tcaron;</trd>,
    <trd>omo&ccaron;i&tcaron;</trd>,
    <trd>zmo&ccaron;i&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_u;&#x43C;&#x43E;&c_ch;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43C;&#x43E;&c_ch;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&#x43D;&c_u;&c_r;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="tremp.0igxi">
  <kap>
    <tld/>i&gcirc;i
  </kap>
  <snc>
    <dif>
      Atingi en flua&jcirc;on:
      <ekz>
        la larma punkto jam ne <tld/>i&gcirc;as en la larman lagon, per kio la forkondukado de
        la larmoj en la larman sakon estas malhelpata
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        valoras tiu &ccirc;i tuttaga frankfurta <tld/>i&gcirc;o en la mondon de muziko
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2010/010062.php">Merkato dampita</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_k;&c_u;&c_n;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_g;&c_r;&c_u;&c_zh;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sich <ind>befeuchten</ind></trd>,
    <trd>eintauchen <klr>(intr.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12402;&#x305F;&#x308B;</trd>,
    <trd>&#12388;&#x304B;&#x308B;</trd>,
    <trd>&#x306C;&#x308C;&#x308B;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>moczy&cacute; si&eogonek;</trd>,
    <trd>zamoczy&cacute; si&eogonek;</trd>
  </trdgrp>
</drv>
</art>
