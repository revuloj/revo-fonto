<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>hospital</rad>/o
</kap>
<drv mrk="hospit.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="san.mal0ulejo">Malsanulejo</ref>,
      kie oni kuracas, ordinare senpage, malri&ccirc;ajn malsanulojn.
    </dif>    
  </snc>
  <ref tip="prt" cel="klinik.0o">kliniko</ref>
  <trdgrp lng="de">
     <trd>Armenkrankenhaus</trd>,
     <trd>Armenspital</trd>
  </trdgrp>
  <trd lng="nl">ziekenhuis<klr tip="ind"> (voor armenzorg)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_s;&c_p;&c_i;&c_t;&c_a;&c_l;&c_mol;</trd>,
    <trd>&c_b;&c_o;&c_l;&c_mol;&c_n;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
</drv>
</art>
