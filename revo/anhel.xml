<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>anhel</rad>/i
</kap>
<drv mrk="anhel.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="anhel.0i.KOMUNE">
    <dif>
      Malfacile kaj forte spiri:
      <ekz>
        <tld/>i pro kurado.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_ja;&c_zh;&c_k;&c_a; <ind>&c_d;&c_y;&c_h;&c_a;&c_c;&c_mol;</ind></trd>,
    <trd>&c_z;&c_a;&c_d;&c_y;&c_h;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">esbufegar</trd>
  <trdgrp lng="de">
      <trd>au&szlig;er <ind>Atem</ind> sein</trd>,
      <trd>hyperventilieren</trd>
  </trdgrp>
  <trd lng="en">hyperventilate</trd>
  <trdgrp lng="es">
    <trd>jadear<klr> (por un ejercicio intenso)</klr></trd>,
    <trd>hiperventilar<klr> (respirar anormalmente r&aacute;pido)</klr></trd>,
    <trd>anhelar<klr> (en desuso)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>haleter</trd>,
    <trd>&ecirc;tre essouffl&eacute;</trd>
  </trdgrp>
  <trd lng="hu">liheg</trd>
  <trd lng="nl">hijgen</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_ja;&c_zh;&c_je;&c_l;&c_o; &c_d;&c_y;&c_sh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;&c_t;&c_mol; &c_o;&c_d;&c_y;&c_sh;&c_k;&c_o;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">dych&ccaron;a&tcaron;</trd>
</drv>

<drv mrk="anhel.0o">
  <kap><tld/>o</kap>
  <snc mrk="anhel.0o.KOMUNE">
    <dif>
      Forta, a&ubreve;debla spirado:
      <ekz>
        la knabo ekhaltis kun <tld/>o de admiro
        <fnt><bib>DOz</bib>, <lok>&leftquot;malnovaj amikoj
        rekunestas&rightquot;</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">esbufec</trd>
  <trdgrp lng="fr">
    <trd>hal&egrave;tement</trd>,
    <trd>souffle<klr tip="amb"> (audible)</klr></trd>,
    <trd>soupir<klr tip="amb"> (souffle)</klr></trd>
  </trdgrp>
</drv>

</art>
