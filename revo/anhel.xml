<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>anhel</rad>/i
</kap>
<drv mrk="anhel.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Malfacile kaj forte spiri:
      <ekz>
        <tld/>i pro kurado.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
      <trd>au&szlig;er <ind>Atem</ind> sein</trd>,
      <trd>hyperventilieren</trd>
  </trdgrp>
  <trd lng="en">hyperventilate</trd>
  <trdgrp lng="fr">
    <trd>haleter</trd>,
    <trd>&ecirc;tre essouffl&eacute;</trd>
  </trdgrp>
  <trd lng="hu">liheg</trd>
  <trd lng="nl">hijgen</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_ja;&c_zh;&c_je;&c_l;&c_o; &c_d;&c_y;&c_sh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;&c_t;&c_mol; &c_o;&c_d;&c_y;&c_sh;&c_k;&c_o;&c_j;</trd>
  </trdgrp>
</drv>
</art>
