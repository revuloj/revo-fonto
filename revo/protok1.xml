<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: protok1.xml,v 1.6 2019/09/28 22:10:24 revo Exp $">
<kap>
  <rad>protokanajl</rad>/o
</kap>
<drv mrk="protok1.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <uzo tip="klr">(&scirc;erce)</uzo>
  <snc mrk="protok1.0o.kanajlego">
    <dif>
      <ref tip="dif" cel="kanajl.0ego">Kanajlego</ref>:
      <ekz>
        ar&hcirc;ifriponoj, <tld/>oj, maraj trompistoj!
        <fnt><bib>Revizoro</bib><lok>5:2</lok></fnt>;
      </ekz>
      <ekz>
        for, ci kanibalo <klr>[&#8230;]</klr> alie mi diros al ci, ke ci estas kriminalulo,
        <klr>[&#8230;]</klr> <tld/>o, rabisto <klr>[&#8230;]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=45154"
              >Insulto</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">arcilotr</trd>
  <trd lng="de">Oberschurke</trd>
  <trdgrp lng="hu">
    <trd>akaszt&oacute;favir&aacute;g</trd>,
    <trd>akszt&oacute;f&aacute;raval&oacute;</trd>,
    <trd>h&eacute;tpr&oacute;b&aacute;s gazember</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>aartsschurk</trd>,
    <trd>aartsploert</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>grande <ind>canalha</ind></trd>,
    <trd>velhaco</trd>,
    <trd>patife refinado</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_o;&c_b;&c_je;&c_s;&c_t;&c_i;&c_ja;<klr> (&c_s;&c_m;. "&c_R;&c_je;&c_v;&c_i;&c_z;&c_o;&c_r;" &c_G;&c_o;&c_g;&c_o;&c_l;&c_ja;)</klr></trd>
  <trd lng="sk">arcilotor</trd>
</drv>
</art>

