<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: skink.xml,v 1.17 2023/09/09 23:22:36 revo Exp $">
<kap>
  <rad>skink</rad>/o
  <fnt><bib>MT</bib></fnt>
</kap>
<drv mrk="skink.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el la ordo <ref tip="malprt" cel="skvam.0uloj">skvamuloj</ref>
      (<trd lng="la">Scincus</trd>) el la varmaj landoj:
      <ekz>
        la rampa&jcirc;oj, kiuj rampas sur la tero: la talpo kaj la muso kaj <klr>[&#8230;]</klr> la <tld/>o kaj
        la &hcirc;ameleono
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 11:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        Dornakola ibiso <klr>[&#8230;]</klr> man&gcirc;as anka&ubreve; malgrandajn lacertojn, <tld/>ojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=409391">Dornakola ibiso</url></lok>
        </fnt>
      </ekz>.
    </dif>
    <bld lok="&WCU;/thumb/2/2d/Apothekerskink01.jpg/640px-Apothekerskink01.jpg" prm="CC BY-SA 2.0   
     de"><tld/>o (Scincus scincus)
        <fnt>
          <aut>Wilfried Berns / tiermotive.de</aut>
          <lok><url
          ref="&WCW;/File:Apothekerskink01.jpg">&WCW;/File:Apothekerskink01.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_s;&c_c;&c_y;&c_n;&c_k;</trd>
  <trd lng="cs">scink</trd>
  <trd lng="de">Skink</trd>
  <trd lng="en">skink</trd>
  <trd lng="fr">scinque</trd>
  <trd lng="hu">szkink</trd>
  <trd lng="ja">&#x30B9;&#x30CA;&#x30C8;&#x30AB;&#x30B2;</trd>
  <trd lng="nl">skink</trd>
  <trdgrp lng="pl">
    <trd>scynk</trd>,
    <trd>&sacute;liga</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_c;&c_i;&c_n;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>ja&scaron;terica <klr>(tropick&aacute;)</klr></trd>,
    <trd>scink</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_c;&c_i;&#x43D;&#x43A;</trd>
</drv>
</art>
