  <trdgrp lng="cs">
    <trd>bixin</trd>,
    <trd>roucou</trd>
  </trdgrp>
  <trd lng="en">bixin</trd>
  <trd lng="fr">rocou</trd>
  <trd lng="it">oriana</trd>
  <trd lng="sk">orle&#xE1;n</trd>
  <trd lng="uk">&#x431;&#x430;&#x440;&#x432;&#x43D;&#x438;&#x43A; &#x456;&#x437; &#x43F;&#x43B;&#x43E;&#x434;&#x456;&#x432; &#x431;&#x456;&#x43A;&#x441;&#x438;</trd>
</drv>
</art>

