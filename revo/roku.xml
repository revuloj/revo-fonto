<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: roku.xml,v 1.6 2024/04/18 17:21:59 revo Exp $">
<kap>
  <rad>roku</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="roku.0o">
  <kap><tld/>o</kap>
  <snc mrk="roku.0o.KOS">
    <dif>
      Flavru&gcirc;a <ref tip="super" cel="kolor.0ilo">kolorilo</ref>
      el la semoj de <ref tip="vid" cel="biks.0o.BOT">bikso</ref>, per kiu
      indianoj &scirc;miras la ha&ubreve;ton,
      kaj per kiu nutra&jcirc;industrio kolorigas bombonojn kaj aliajn:
      <ekz>
        ili <klr>[&#8230;]</klr> ali-vestis &scirc;in per koloraj &ccirc;ifonoj, &scirc;miris
        &scirc;ian viza&gcirc;on per fulgo kaj <tld/>o
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_l;&c_je;&c_a;&c_n; <klr tip="amb">(&c_f;&c_a;&c_r;&c_b;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;)</klr></trd>
  <trdgrp lng="cs">
    <trd>bixin</trd>,
    <trd>roucou</trd>
  </trdgrp>
  <trd lng="de">Norbixin</trd>
  <trd lng="en">bixin</trd>
  <trd lng="fr">rocou</trd>
  <trd lng="it">oriana</trd>
  <trd lng="sk">orle&aacute;n</trd>
  <trd lng="uk">&c_b;&c_a;&c_r;&c_v;&c_n;&c_i;&c_k; &c_ib;&c_z; &c_p;&c_l;&c_o;&c_d;&c_ib;&c_v; &c_b;&c_ib;&c_k;&c_s;&c_i;</trd>
</drv>
</art>

