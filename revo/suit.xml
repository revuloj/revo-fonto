<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>suit</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="suit.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Muzika &gcirc;enro populara en e&ubreve;ropa klasiko, en kiu
      unu muzikilo ludas serion de danc-muzikoj en sama ton-skalo.
    </dif>
  </snc>
  <trd lng="fr">suite<klr tip="ind"> (musique)</klr></trd>
  <trdgrp lng="nl">
   <trd>suite<klr tip="ind"> (muziek)</klr></trd>,
   <trd>muzieksuite</trd>
</trdgrp>
<trd lng="ru">&c_s;&c_ju;&c_i;&c_t;&c_a;</trd>
</drv>
</art>
