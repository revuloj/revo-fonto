<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: suit.xml,v 1.16 2024/03/25 07:28:37 revo Exp $">
<kap>
  <rad>suit</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="suit.0o">
  <kap><tld/>o</kap>
  <snc mrk="suit.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="kompon.0ajxo">Kompona&jcirc;o</ref>
      konsistanta el serio de danc-muzika&jcirc;oj:
      <ekz>
        en 1888 kaj 1891, <nom>Grieg</nom> elelektis ok kompona&jcirc;ojn de la verko por fari el ili du
        kvarpartajn <tld/>ojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=29545">Peer Gynt</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_ju;&c_ib;&c_t;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>suita</trd>,
    <trd>svita</trd>
  </trdgrp>
  <trd lng="de">Suite</trd>
  <trd lng="en">suite (music)</trd>
  <trd lng="fr">suite<klr tip="ind"> (musicale)</klr></trd>
  <trd lng="hu">szvit<klr tip="ind"> (zenei)</klr></trd>
  <trd lng="it">suite<klr> (mus.)</klr></trd>
  <trdgrp lng="nl">
    <trd>suite<klr tip="ind"> (muziek)</klr></trd>,
    <trd>muzieksuite</trd>
  </trdgrp>
  <trd lng="pl">suita</trd>
  <trd lng="ru">&c_s;&c_ju;&c_i;&c_t;&c_a;</trd>
  <trd lng="sk">suita</trd>
  <trd lng="uk">&c_s;&c_ju;&c_ji;&c_t;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x7D44;&#x66F2; <pr>z&#x1D4;q&#x1D4;</pr></trd>,
    <trd>&#x7EC4;&#x66F2; <pr>z&#x1D4;q&#x1D4;</pr></trd>
  </trdgrp>
</drv>
</art>
