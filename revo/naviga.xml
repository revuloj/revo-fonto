<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: naviga.xml,v 1.18 2015/07/04 08:37:40 revo Exp $">
    <kap><ofc>3</ofc> <rad>navigaci</rad>/o</kap>

    <drv mrk="naviga.0o">
      <kap><tld/>o</kap>
      <snc mrk="naviga.0o.biro">
        <dif>
          Tuto da metodoj por difini la pozicion kaj kurson de &scirc;ipo,
          avio, a&ubreve;to, militista ta&ccirc;mento ktp.
        </dif>
        <trd lng="en">navigation</trd>
        <trd lng="ru">&c_n;&c_a;&c_v;&c_i;&c_g;&c_a;&c_c;&c_i;&c_ja;</trd>
      </snc>

      <snc mrk="naviga.0o.veturado">
        <uzo tip="fak">MAR</uzo>
        <ref tip="dif" cel="navig.0ado.veturado">navigado<sncref/></ref>

        <trdgrp lng="be">
          <trd>&c_n;&c_a;&c_v;&c_ib;&c_g;&c_a;&c_c;&c_y;&c_ja;</trd>,
          <trd>&c_p;&c_l;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je; <klr>(&c_n;&c_a; &c_k;&c_a;&c_r;&c_a;&c_b;&c_l;&c_ib;)</klr></trd>
        </trdgrp>                      
        <trd lng="br">merdeadenn</trd>
        <trd lng="de">Navigation</trd>
        <trd lng="en">navigation</trd>
        <trdgrp lng="fr">
          <trd>croisi&egrave;re</trd>,
          <trd>navigation</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>haj&oacute;z&aacute;s</trd>,
          <trd>tengerhaj&oacute;z&aacute;s</trd>,
          <trd>navig&aacute;ci&oacute;</trd>,
        </trdgrp>
        <trdgrp lng="nl">
          <trd>scheepvaart</trd>,
          <trd>navigatie</trd>
        </trdgrp>
        <trd lng="pt">navega&ccedil;&atilde;o</trd>
        <trdgrp lng="ru">
          <trd>&c_n;&c_a;&c_v;&c_i;&c_g;&c_a;&c_c;&c_i;&c_ja;</trd>,
          <trd>&c_p;&c_l;&c_a;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
        </trdgrp>
      </snc>
  <trdgrp lng="cs">
    <trd>lodní doprava</trd>,
    <trd>navigace</trd>,
    <trd>plavba</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>lodná doprava</trd>,
    <trd>navigácia</trd>
  </trdgrp>
</drv>

    <drv mrk="naviga.0a">
      <kap><tld/>a</kap>
      <snc mrk="naviga.0a.bira">
        <dif>
          Rilata al <tld/>o:
          <ekz>
            <tld/>aj instrumentoj;
          </ekz><ekz>
            <ind><tld/>a sputniko</ind>
            <klr>(uzata de <tld/>istoj)</klr>;
            <trd lng="en">navigation(al) satellite</trd>
            <trd lng="ru">&c_n;&c_a;&c_v;&c_i;&c_g;&c_a;&c_c;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j; &c_s;&c_p;&c_u;&c_t;&c_n;&c_i;&c_k;</trd>
          </ekz>
        </dif>
      </snc>
      <trd lng="en">navigation(al)</trd>
      <trd lng="ru">&c_n;&c_a;&c_v;&c_i;&c_g;&c_a;&c_c;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
    </drv>

    <drv mrk="naviga.0i">
      <kap><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc mrk="naviga.0i.biri">
        <dif>
          Difini sian pozicion kaj kurson de &scirc;ipo, avio, a&ubreve;to,
          militista ta&ccirc;mento ktp.
        </dif>
        <trd lng="en">lay a course</trd>
        <trdgrp lng="ru">
          <trd>&c_o;&c_p;&c_r;&c_je;&c_d;&c_je;&c_l;&c_i;&c_t;&c_mol; &c_k;&c_u;&c_r;&c_s;</trd>,
          <trd>&c_p;&c_r;&c_o;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_mol; &c_k;&c_u;&c_r;&c_s;</trd>,
          <trd>&c_p;&c_o;&c_s;&c_t;&c_r;&c_o;&c_i;&c_t;&c_mol; &c_k;&c_u;&c_r;&c_s;</trd>
        </trdgrp>
      </snc>

      <snc mrk="naviga.0i.veturi">
        <uzo tip="fak">MAR</uzo>
        <ref tip="dif" cel="navig.0i.veturi">navigi<sncref/></ref>
        
        <trd lng="be">&c_p;&c_l;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_n;&c_a; &c_k;&c_a;&c_r;&c_a;&c_b;&c_l;&c_ib;)</klr></trd>   
        <trd lng="br">merdei&ntilde;</trd>
        <trd lng="de">navigieren</trd>
        <trdgrp lng="en">
          <trd>navigate</trd>,
          <trd>cruise</trd>,
          <trd>sail</trd>,
          <trd>voyage</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>naviguer</trd>,
          <trd>voguer</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>haj&oacute;zik</trd>,
          <trd>navig&aacute;l</trd>,
        </trdgrp>
        <trdgrp lng="nl">
          <trd>navigeren</trd>,
          <trd>varen</trd>
        </trdgrp>
        <trd lng="pt">navegar</trd>
      </snc>
  <trdgrp lng="cs">
    <trd>plavit se</trd>,
    <trd>plout</trd>
  </trdgrp>
  <trd lng="sk">plávať <klr>(loďou)</klr></trd>
</drv>

    <drv mrk="naviga.0ilo">
      <kap><tld/>ilo</kap>
      <snc mrk="naviga.0ilo.birilo">
        <dif>
          Aran&gcirc;a&jcirc;o por difini la pozicion, ellabori kaj montri kurson
          de &scirc;ipo, avio, a&ubreve;to ktp
          <ekz>
            al la tagman&gcirc;a tablo en Kirchohsen Alke kaj Andreas, pera&ubreve;te
            survoje, estis gviditaj de la <tld/>ilo ne al ponto
            ordinara por kruci Vezeron, sed <klr>al la</klr> pramo
            movata nur de la fluo
            <fnt>
              <vrk><url ref="http://www.esperanto.de/unterweser"
                  >La 5-a Rata Rendevuo</url></vrk>.
              <lok>Esperanto-Gazeto, N-ro 24 Feb.&dash;Mai 2009.
                ISSN 1861-3330</lok>
            </fnt>.
          </ekz>
        </dif>
        <trd lng="de">Navigationsger&auml;t</trd>
        <trd lng="ru">&c_n;&c_a;&c_v;&c_i;&c_g;&c_a;&c_t;&c_o;&c_r;</trd>
      </snc>
      <trd lng="sk">plavidlo</trd>
</drv>

    <drv mrk="naviga.0isto">
      <kap><tld/>isto</kap>
      <snc mrk="naviga.0isto.oficiro">
        <dif>
          Tiu el la anaro kondukanta &scirc;ipon, avion ktp, kiu responsas pri
          <tld/>o<sncref ref="naviga.0o.biro"/>:
          <ekz>
            partoprenantoj de konkurso: dana <tld/>isto el teamo de DAF
            Trucks, sendependa franca &scirc;oforo kaj franca motorciklisto
            mortis dum tri aliaj akcidentoj
            <fnt>
              <vrk>Ralio Parizo-Dakaro</vrk>. <bib>Viki</bib>.
            </fnt>
          </ekz>
        </dif>
        <trd lng="de">Navigationsoffizier</trd>
        <trd lng="ru">&c_sh;&c_t;&c_u;&c_r;&c_m;&c_a;&c_n;</trd>
      </snc>
    </drv>

    <drv mrk="naviga.0ejo">
      <kap><tld/>ejo</kap>
      <snc>
        <dif>
          Ejo sur &scirc;ipo kie estas instalita <tld/>a aparataro, la
          laborloko de <tld/>isto:
          <ekz>
            komandejoj estis du&ccirc;ambraj: stirejo anta&ubreve;e, kaj <tld/>ejo
            malanta&ubreve;e
            <fnt>
              <aut>Guido Ricci</aut>:
              <lok><url
              ref="http://mx.groups.yahoo.com/group/esperanto-en-kolombio/message/7982"
                  >&Ccirc;u estas &gcirc;uste diri &leftquot;ponto&rightquot; anstata&ubreve; &leftquot;komandejo&rightquot;</url></lok>
            </fnt>.
          </ekz>
        </dif>
        <trd lng="en">chart house</trd>
        <trd lng="fr">abri de navigation</trd>
        <trd lng="ru">&c_sh;&c_t;&c_u;&c_r;&c_m;&c_a;&c_n;&c_s;&c_k;&c_a;&c_ja; &c_r;&c_u;&c_b;&c_k;&c_a;</trd>
      </snc>
    </drv>


  </art>
