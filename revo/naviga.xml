<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>3</ofc> <rad>navigaci</rad>/o</kap>
<drv mrk="naviga.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      &Scirc;ipveturado kaj precipe marveturado.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_v;&c_ib;&c_g;&c_a;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je; <klr>(&c_n;&c_a; &c_k;&c_a;&c_r;&c_a;&c_b;&c_l;&c_ib;)</klr></trd>
  </trdgrp>
  <trd lng="de">Navigation</trd>
  <trdgrp lng="fr">
    <trd>croisi&egrave;re</trd>,
    <trd>navigation</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>scheepvaart</trd>,
    <trd>navigatie</trd>
  </trdgrp>
  <trd lng="pt">navega&ccedil;&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_v;&c_i;&c_g;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="naviga.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      &Scirc;ipveturi, marveturi.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="navig.0i">navigi</ref>
  </refgrp>
  <trd lng="be">&c_p;&c_l;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_n;&c_a; &c_k;&c_a;&c_r;&c_a;&c_b;&c_l;&c_ib;)</klr></trd>
  <trd lng="de">navigieren</trd>
  <trdgrp lng="fr">
    <trd>naviguer</trd>,
    <trd>voguer</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>navigeren</trd>,
    <trd>varen</trd>
  </trdgrp>
  <trd lng="pt">navegar</trd>
  <trd lng="ru">&c_p;&c_l;&c_y;&c_t;&c_mol; <klr tip="amb">(&c_n;&c_a; &c_s;&c_u;&c_d;&c_n;&c_je;)</klr></trd>
</drv>
</art>
