<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>proviant</rad>/o <fnt>Z</fnt>
</kap>
<drv mrk="provia.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Manĝaj provizoj, nutraĵkonservoj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>правіянт</trd>,
    <trd>харч</trd>
  </trdgrp>
  <trd lng="de">Proviant</trd>
  <trdgrp lng="en">
    <trd>provisions</trd>,
    <trd>rations</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>élelem <klr tip="ind">(katonai)</klr></trd>,
    <trd>élelmiszer <klr tip="ind">(katonai)</klr></trd>
  </trdgrp>
  <trd lng="nl">proviand</trd>
  <trdgrp lng="ru">
    <trd>провиант</trd>,
    <trd>продовольствие</trd>
  </trdgrp>
</drv>

<drv mrk="provia.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Havigi <tld/>on (pp armeo); provizumi.
    </dif>
  </snc>
  <trd lng="be">забясьпечваць <ind>харчам</ind></trd>
  <trdgrp lng="en">
    <trd>provision</trd>,
    <trd>supply</trd>
  </trdgrp>
  <trd lng="hu">élelmez <klr tip="ind">(katonai)</klr></trd>
  <trd lng="nl">provianderen</trd>
  <trd lng="ru">снабжать провиантом</trd>
</drv>

<drv mrk="provia.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Oficisto, kiu prizorgas la <tld/>adon.
    </dif>
  </snc>
  <trd lng="be">інтэндант</trd>
  <trd lng="en">supply officer</trd>
  <trdgrp lng="hu">
    <trd>élelmező tiszt</trd>,
    <trd>GH-s tiszt</trd>
  </trdgrp>
  <trd lng="ru">интендант</trd>
</drv>
</art>
