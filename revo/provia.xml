<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: provia.xml,v 1.23 2025/04/27 16:06:14 revo Exp $">
<kap>
  <rad>proviant</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="provia.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Man&gcirc;aj <ref tip="super" cel="proviz.0o">provizoj</ref>, nutra&jcirc;konservoj:
      <ekz>
        li mem rajdas kun sia gvidanto anta&ubreve;e, &scirc;ar&gcirc;o&ccirc;evalo portas kofrojn, tendon kaj
        <tld/>on, kelke da soldatoj por defendo lin sekvas
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kiel <tld/>on por la voja&gcirc;o, <klr>[la Morto]</klr> elprenas tiun a&ubreve; tiun bonan
        faron nian, kaj permesas, ke &gcirc;i iru kun ni, kaj tio povos esti tre pla&ccirc;a
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
      <ekz>
        paniki&gcirc;is multaj homoj, a&ccirc;etante anta&ubreve; la eklipso-tago man&gcirc;a&jcirc;ojn kaj alian
        <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Gbeglo Koffi/pg</aut>
          <vrk><url ref="&Monato;/2006/008948.php">Konfuzo kaj kontra&ubreve;diroj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ne, diris li la&ubreve;te al si mem, mi devas porciumi mian <tld/>on tre zorge
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Destino de Atrejo</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="viktuali.0o">viktualio</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_v;&c_ib;&c_ja;&c_n;&c_t;</trd>,
    <trd>&c_h;&c_a;&c_r;&c_ch;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>proviant</trd>,
    <trd>z&aacute;soba potravin</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Proviant</trd>,
    <trd>Wegzehrung</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>provisions</trd>,
    <trd>rations</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>provision</trd>,
    <trd>ravitaillement</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;lelem <klr tip="ind">(katonai)</klr></trd>,
    <trd>&eacute;lelmiszer <klr tip="ind">(katonai)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#39135;&#31975;  <pr>&#12375;&#12423;&#12367;&#12426;&#12423;&#12358;</pr></trd>,
    <trd>&#31975;&#39135;  <pr>&#12426;&#12423;&#12358;&#12375;&#12423;&#12367;</pr></trd>,
    <trd>&#35036;&#32102;&#29289;&#36039;  <pr>&#12411;&#12365;&#12421;&#12358;&#12406;&#12387;&#12375;</pr></trd>
  </trdgrp>
  <trd lng="nl">proviand</trd>
  <trdgrp lng="pl">
    <trd>prowiant</trd>,
    <trd>wiktua&lstroke;y {pl}</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_v;&c_i;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_d;&c_o;&c_v;&c_o;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>proviant</trd>,
    <trd>z&aacute;soba potrav&iacute;n</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_o;&c_v;&c_ib;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_d;&c_o;&c_v;&c_o;&c_l;&c_mol;&c_ch;&c_i;&c_j; &c_z;&c_a;&c_p;&c_a;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#31918; <pr>li&aacute;ng</pr></trd>,
    <trd>&#31975; <pr>li&aacute;ng</pr></trd>,
    <trd>&#32102;&#39178; <pr>j&#464;y&#462;ng</pr></trd>,
    <trd>&#32473;&#20859; <pr>j&#464;y&#462;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="provia.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      (pri armeo) Havigi <tld/>on; <ref tip="dif" cel="proviz.0umi">provizumi</ref>:
      <ekz>
        necesis atendi la komenci&gcirc;on de la 19-a jc kaj la blokado<klr>[n]</klr> de <nom>Brest</nom>
        fare de la angloj, por konvinki Napoleonon la 1-an pri la strategia intereso
        malbloki <nom>Brest</nom>-on tra la tero, <tld/>i per nutra&jcirc;oj kaj municioj la
        arsenalojn de <nom>Brest</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=389462">Kanalo inter Nantes kaj
          Brest</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_ch;&c_v;&c_a;&c_c;&c_mol; <ind>&c_h;&c_a;&c_r;&c_ch;&c_a;&c_m;</ind></trd>
  <trd lng="cs">z&aacute;sobovat</trd>
  <trd lng="de">mit <ind>Proviant</ind> ausstatten</trd>
  <trdgrp lng="en">
    <trd>provision</trd>,
    <trd>supply</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ravitailler</trd>,
    <trd>provisionner</trd>
  </trdgrp>
  <trd lng="hu">&eacute;lelmez <klr tip="ind">(katonai)</klr></trd>
  <trd lng="ja">&#39135;&#31975;&#12434;&#35036;&#32102;&#12377;&#12427;  <pr>&#12375;&#12423;&#12367;&#12426;&#12423;&#12358;&#12434;&#12411;&#12365;&#12421;&#12358;&#12377;&#12427;</pr></trd>
  <trd lng="nl">provianderen</trd>
  <trdgrp lng="pl">
    <trd>prowiantowa&cacute;</trd>,
    <trd>aprowizowa&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_n;&c_a;&c_b;&c_zh;&c_a;&c_t;&c_mol; &c_p;&c_r;&c_o;&c_v;&c_i;&c_a;&c_n;&c_t;&c_o;&c_m;</trd>
  <trd lng="sk">z&aacute;sobi&tcaron; potravinami</trd>
  <trd lng="uk">&c_z;&c_a;&c_b;&c_je;&c_z;&c_p;&c_je;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;
  &c_p;&c_r;&c_o;&c_d;&c_o;&c_v;&c_o;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;&c_m;</trd>
</drv>

<drv mrk="provia.0ejo">
  <kap>
    <tld/>ejo
  </kap>
  <snc>
    <dif>
      Loko en kiu oni konservas ian kvanton da <tld/>o, da rezervaj man&gcirc;a&jcirc;oj:
      <ekz>
        li sufi&ccirc;e provizis sin &scirc;telante el la palacaj <tld/>ejoj
        <fnt>
          <aut>J. R. R. Tolkien, trad. C. Gledhill</aut>
          <vrk>La Hobito</vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_r;&c_ch;&c_o;&c_v;&c_y; <ind>&c_s;&c_k;&c_l;&c_a;&c_d;</ind></trd>
<trdgrp lng="fr">
    <trd>d&eacute;pense<klr> (garde-manger)</klr></trd>,
    <trd>garde-manger</trd>,
    <trd>resserre</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>magazyn</trd>,
    <trd>sk&lstroke;ad</trd>
  </trdgrp>
</drv>

<drv mrk="provia.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      <ref tip="super" cel="ofic.0isto">Oficisto</ref> kiu prizorgas la <tld/>adon:
      <ekz>
        la insulo nomi&gcirc;is tiel pro la <tld/>isto <nom>Mart&iacute;n Garc&iacute;a</nom>, kiu forpasis sur&scirc;ipe kaj
        estis tie surterigita
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=472463">Insulo Mart&iacute;n Garc&iacute;a</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_t;&c_e;&c_n;&c_d;&c_a;&c_n;&c_t;</trd>
  <trd lng="cs">z&aacute;sobovatel</trd>
  <trd lng="de">Proviantmeister</trd>
  <trd lng="en">supply officer</trd>
  <trdgrp lng="fr">
    <trd>ravitailleur</trd>,
    <trd>provisionneur</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;lelmez&odblac; tiszt</trd>,
    <trd>GH-s tiszt</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#35036;&#32102;&#20418;  <pr>&#12411;&#12365;&#12421;&#12358;&#12363;&#12363;&#12426;</pr></trd>,
    <trd>&#35036;&#32102;&#23558;&#26657;  <pr>&#12411;&#12365;&#12421;&#12358;&#12375;&#12423;&#12358;&#12371;&#12358;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>intendent</trd>,
    <trd>aprowizator</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_n;&c_t;&c_je;&c_n;&c_d;&c_a;&c_n;&c_t;</trd>
  <trd lng="sk">z&aacute;sobovate&lcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_ib;&c_n;&c_t;&c_je;&c_n;&c_d;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_shch;&c_o; &c_z;&c_a;&c_j;&c_m;&c_a;&c_jeu;&c_t;&c_mol;&c_s;&c_ja;
    &c_p;&c_r;&c_o;&c_d;&c_o;&c_v;&c_o;&c_l;&c_mol;&c_ch;&c_i;&c_m;
    &c_z;&c_a;&c_b;&c_je;&c_z;&c_p;&c_je;&c_ch;&c_je;&c_n;&c_n;&c_ja;&c_m;</trd>
  </trdgrp>
</drv>
