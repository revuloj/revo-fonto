<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>proviant</rad>/o <fnt>Z</fnt>
</kap>
<drv mrk="provia.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Man&gcirc;aj provizoj, nutra&jcirc;konservoj.
    </dif>
  </snc>
  <trd lng="de">Proviant</trd>
  <trd lng="nl">proviand</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_v;&c_i;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_d;&c_o;&c_v;&c_o;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="provia.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Havigi <tld/>on (pp armeo); provizumi.
    </dif>
  </snc>
  <trd lng="nl">provianderen</trd>
  <trd lng="ru">&c_s;&c_n;&c_a;&c_b;&c_zh;&c_a;&c_t;&c_mol; &c_p;&c_r;&c_o;&c_v;&c_i;&c_a;&c_n;&c_t;&c_o;&c_m;</trd>
</drv>

<drv mrk="provia.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Oficisto, kiu prizorgas la <tld/>adon.
    </dif>
  </snc>
  <trd lng="ru">&c_i;&c_n;&c_t;&c_je;&c_n;&c_d;&c_a;&c_n;&c_t;</trd>
</drv>
</art>
