    <trd>roem</trd>
  </trdgrp>
  <trd lng="pt">gl&oacute;ria</trd>
  <trd lng="ru">&c_s;&c_l;&c_a;&c_v;&c_a;</trd>
  <trd lng="sk">sl&aacute;va</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_l;&c_a;&c_v;&c_a;</trd>,
    <trd>&c_ch;&c_je;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_sh;&c_a;&c_n;&c_a;</trd>,
    <trd>&c_h;&c_v;&c_a;&c_l;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
