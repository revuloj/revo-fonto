<drv mrk="glor.sin0i">
  <kap>sin <tld/>i</kap>
  <snc>
    <dif>
      Malmodeste la&ubreve;di sin mem:
      <ekz>
        kiu nenion valoras, plej multe sin <tld/>as
        <fnt><bib>PrV</bib></fnt>
      </ekz>
      <ekz>
        <ind>kiu mem sin <tld/>as, malbone odoras</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>.
        <trd lng="de">Eigenlob stinkt</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fanfar1.0i">fanfaroni</ref>,
      <ref cel="fier.0i">fieri</ref>,
      <ref cel="vant.0a">vanta</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_h;&c_v;&c_a;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
  <trd lng="de">sich <ind>r&uuml;hmen</ind></trd>
  <trdgrp lng="fr">
    <trd>se <ind>glorifier</ind></trd>,
    <trd>s'<ind>enorgueillir</ind></trd>,
    <trd>se <ind>targuer</ind></trd>
  </trdgrp>
  <trd lng="hu">dicsekszik</trd>
  <trd lng="nl">pochen</trd>
  <trd lng="pt">ufanar-se</trd>
  <trdgrp lng="ru">
