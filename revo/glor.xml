<drv mrk="glor.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="glor.0i.honori">
    <dif>
      <ref tip="super" cel="honor.0i">Honori</ref> iun donante al li grandan famecon, konigante &ccirc;ien
      lian indecon, publikigante sian admiron; <ref tip="super" cel="lauxd.0i">la&ubreve;degi</ref>:
      <ekz>
        mi <tld/>is la Eternulon, la Dion de mia sinjoro <nom>Abraham</nom>
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 24:48</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u la forton de Dio
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 68:34</lok>
        </fnt>!
      </ekz>
      <ekz>
        atletoj fakte estas <klr>[&#8230;]</klr> <tld/>itaj ikonoj de individuismo
        <fnt>
          <bib>Monato</bib>
          <aut>Husejn Al-Amily</aut>
          <vrk><url ref="&Monato;/2004/008404.html">Elmontri belecon de
          sporto</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi &gcirc;ojus, se min <tld/>us la popolo
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u la noblan hegemonon
        <fnt>
          <bib>MkM</bib>
        </fnt>!
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="honor.0i">honori</ref>,
      <ref cel="lauxd.0i">la&ubreve;di</ref>,
      <ref cel="ovaci.0i">ovacii</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
