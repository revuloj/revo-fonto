    <trd>&#26628;&#20809;  <pr>&#12360;&#12356;&#12371;&#12358;</pr></trd>,
    <trd>&#26628;&#35465;&#12434;&#12418;&#12383;&#12425;&#12377;&#12418;&#12398;&#12539;&#20154;  <pr>&#12360;&#12356;&#12424;&#12434;&#12418;&#12383;&#12425;&#12377;&#12418;&#12398;&#12539;&#12376;&#12435;</pr></trd>,
    <trd>&#35463;&#12426;  <pr>&#12411;&#12371;&#12426;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
