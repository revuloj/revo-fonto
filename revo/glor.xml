    <trd>&c_p;&c_r;&c_o;&c_s;&c_l;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_p;&c_o;&c_v;&c_n;&c_je;&c_n;&c_i;&c_j; &c_s;&c_l;&c_a;&c_v;&c_i;</trd>,
    <trd>&c_z;&c_n;&c_a;&c_m;&c_je;&c_n;&c_i;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21690; <pr>xu&#462;n</pr></trd>,
    <trd>&#26332; <pr>y&agrave;o</pr></trd>,
    <trd>&#32768; <pr>y&agrave;o</pr></trd>
  </trdgrp>
</drv>

