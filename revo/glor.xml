<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>glor</rad>/i
</kap>
<drv mrk="glor.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Honori iun donante al li grandan famecon, konigante &ccirc;ien lian
      indecon, publikigante sian admiron; la&ubreve;degi:
      <ekz>
        <tld/>i la heroojn;
      </ekz>
      <ekz>
        <tld/>i scienculon per statuo;
      </ekz>
      <ekz>
        <tld/>i la belajn agojn, genian poeton, Dion;
      </ekz>
      <ekz>
        estu <tld/>ata la nomo de Dio!
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="honor.0i">honori</ref>,
      <ref cel="lauxd.0i">la&ubreve;di</ref>.
    </refgrp>
  </snc>
  <adm>
    Mi ne komprenas la intencon de la elemento uzo klr "iu".
    <aut>MB</aut>
  </adm>
  <trdgrp lng="de">
    <trd>r&uuml;hmen</trd>,
    <trd>ehren</trd>
  </trdgrp>
  <trd lng="en">glorify</trd>
  <trdgrp lng="fr">
    <trd>glorifier</trd>,
    <trd>rendre <ind>gloire</ind> (&agrave;)</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>dics&odblac;&iacute;t</trd>,
    <trd>magasztal</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>eren</trd>,
    <trd>roemen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_l;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="glor.sin0i">
  <kap>sin <tld/>i</kap>
  <snc>
    <dif>
      Malmodeste la&ubreve;di sin mem:
      <ekz>kiu nenion valoras, plej multe sin <tld/>as
        <fnt><bib>PrV</bib></fnt></ekz>
      <ekz>
        sin <tld/>i pro sia ri&ccirc;eco.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fanfar1.0i">fanfaroni</ref>,
      <ref cel="fier.0i">fieri</ref>,
      <ref cel="vant.0a">vanta</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>se <ind>glorifier</ind></trd>,
    <trd>s'<ind>enorgueillir</ind></trd>,
    <trd>se <ind>targuer</ind></trd>
  </trdgrp>
  <trd lng="hu">dicsekszik</trd>
  <trd lng="nl">pochen</trd>
</drv>

<drv mrk="glor.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Famobrilo; admiro de la plejmulto por ies merito, talento, genio:
      <ekz>
        sen laboro nenia <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la <tld/>o estas la suno de la mortintoj;
      </ekz>
      <ekz>oni ne pagas per <tld/>o al sia tajloro
        <fnt><bib>PrV</bib></fnt></ekz>
      <ekz>
        konfesi, la&ubreve;di, prikanti ies <tld/>on;
      </ekz>
      <ekz>bona <tld/>o pli valoras ol oro
        <fnt><bib>PrV</bib></fnt></ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      <tld lit="G"/>iga persono a&ubreve; afero; fierinda&jcirc;o:
      <ekz>
        li estas <tld/>o de la nacio<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="esper.0o">espero 3</ref>,
      <ref cel="gxoj.0o">&gcirc;ojo 2</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Ruhm</trd>,
    <trd>Ehre</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gloire</trd>,
    <trd>c&eacute;l&eacute;brit&eacute;</trd>,
    <trd>fleuron</trd>
  </trdgrp>
  <trd lng="hu">dics&odblac;s&eacute;g</trd>
  <trdgrp lng="nl">
    <trd>eer</trd>,
    <trd>roem</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_l;&c_a;&c_v;&c_a;</trd>
</drv>

<drv mrk="glor.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Posedanta <tld/>on:
      <ekz>
        <tld/>a poeto, scienculo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fam.0a">fama</ref>,
      <ref cel="presti1">presti&gcirc;a</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Donanta <tld/>on, <tld/>iga:
      <ekz>
        <tld/>a venko, okazo, laboro, kanto.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>glorieux</trd>,
    <trd>illustre</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>dics&odblac;</trd>,
    <trd>dics&odblac;s&eacute;ges</trd>
  </trdgrp>
<trdgrp lng="nl">
  <trd>eervol</trd>,
  <trd>roemrijk</trd>
  </trdgrp><trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_a;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_l;&c_a;&c_v;&c_l;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="glor.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      La&ubreve; <tld/>a maniero:
      <ekz>
        <tld/>e morti;
      </ekz>
      <ekz>
        ne <tld/>e estas ser&ccirc;i sian <tld/>on
        <fnt><bib>MT</bib>, <lok>&Sen; 25:27</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>r&uuml;hmlich</trd>,
    <trd>glorreich</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>dics&odblac;s&eacute;gesen</trd>,
    <trd>dics&odblac; m&oacute;don</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>eervol</trd>,
    <trd>roemrijk</trd>
  </trdgrp>
</drv>

<drv mrk="glor.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Esti la ka&ubreve;zo de la granda fameco, &ccirc;iekonateco de iu a&ubreve; io:
      <ekz>
        la grandaj scienculoj <tld/>igas la naski&gcirc;landon;
      </ekz>
      <ekz>
        lia verko <tld/>igas lin pli ol la la&ubreve;doj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>concourir &agrave; la gloire (de)</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>dics&odblac;s&eacute;get szerez</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>roemen</trd>,
    <trd>eren</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_s;&c_l;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; &c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_n;&c_y;&c_m;</trd>
  </trdgrp>
</drv>

<drv mrk="glor.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref cel="fam.mis0igi">Misfamigi</ref>; diskonigi la malbonan famon
      de iu a&ubreve; de io:
      <ekz>
        mal<tld/>i friponon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>d&eacute;crier</trd>,
    <trd>d&eacute;noncer<klr tip="ind"> (vilipender)</klr></trd>,
    <trd>vilipender</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&oacute;cs&aacute;rol</trd>,
    <trd>szid</trd>
  </trdgrp>
  <trd lng="nl">onteren</trd>  
  <trdgrp lng="ru">
    <trd>&c_o;&c_p;&c_o;&c_z;&c_o;&c_r;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_je;&c_s;&c_ch;&c_je;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="glor.mal0o">
  <kap>mal<tld/>o</kap>
  <snc>
    <dif>
      Malbonfamo, mal&scirc;ato de la plejmulto por ies morala a&ubreve;
      intelekta valoro:
      <ekz>
        akiri, meriti mal<tld/>on;
      </ekz>
      <ekz>
        disanonci ies mal<tld/>on.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>honte</trd>,
    <trd>infamie</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gyal&aacute;zat</trd>,
    <trd>cs&uacute;fs&aacute;g</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>oneer</trd>,
    <trd>infamie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_z;&c_o;&c_r;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_ch;&c_je;&c_s;&c_t;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="glor.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Havanta mal<tld/>on.
    </dif>
  </snc>
  <trd lng="de">unr&uuml;hmlich</trd>
  <trd lng="nl">eerloos</trd>
  <trd lng="hu">dicstelen</trd>
  <trd lng="ru">&c_p;&c_o;&c_z;&c_o;&c_r;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="glor.mal0igisen0igi">
  <kap>mal<tld/>igi, sen<tld/>igi</kap>
  <snc>
    <dif>
      Esti ka&ubreve;zo de malbonfamo por iu:
      <ekz>
        lia konduto en tiu okazo tute mal<tld/>igis lin.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">d&eacute;shonorer</trd>
  <trd lng="hu">gyal&aacute;zatot hoz r&aacute;</trd>
  <trd lng="nl">onteren</trd>
  <trd lng="ru">&c_o;&c_p;&c_o;&c_z;&c_o;&c_r;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="glor.mal0iga">
  <kap>mal<tld/>iga</kap>
  <snc>
    <dif>
      Donanta mal<tld/>on.
    </dif>
  </snc>
  <trd lng="fr">d&eacute;shonorant</trd>
  <trd lng="hu">gyal&aacute;zatot hoz&oacute;</trd>
  <trd lng="nl">onterend</trd>
  <trd lng="ru">&c_p;&c_o;&c_z;&c_o;&c_r;&c_ja;&c_shch;&c_i;&c_j;</trd>
</drv>

<drv mrk="glor.0krono">
  <kap><tld/>krono</kap>
  <uzo tip="fak">REL</uzo>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <ref tip="dif" cel="auxreo.0o.REL">a&ubreve;reolo</ref>
  </snc>
  <trdgrp lng="fr">
    <trd>aur&eacute;ole<klr tip="ind"> (rel.)</klr></trd>,
    <trd>nimbe<klr tip="ind"> (rel.)</klr></trd>
  </trdgrp>
  <trd lng="de">Ruhmeskrone</trd>
  <trd lng="hu">dicsf&eacute;ny</trd>
  <trd lng="nl">aureool</trd>
</drv>
</art>
