<drv mrk="glor.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Honori iun donante al li grandan famecon, konigante &ccirc;ien
      lian indecon, publikigante sian admiron; la&ubreve;degi:
      <ekz>
        mi <tld/>is la Eternulon, la Dion de mia sinjoro <nom>Abraham</nom>
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 24:48</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u la forton de Dio
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 68:34</lok>
        </fnt>!
      </ekz>
      <ekz>
        atletoj fakte estas <klr>[&#x2026;]</klr> <tld/>itaj ikonoj de individuismo
        <fnt>
          <bib>Monato</bib>
          <aut>Husejn Al-Amily</aut>
          <vrk><url ref="&Monato;/2004/008404.html">Elmontri belecon de
          sporto</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi &gcirc;ojus, se min <tld/>us la popolo
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u la noblan hegemonon
        <fnt>
          <bib>MkM</bib>
        </fnt>!
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="honor.0i">honori</ref>,
      <ref cel="lauxd.0i">la&ubreve;di</ref>,
      <ref cel="ovaci.0i">ovacii</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_l;&c_a;&c_v;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_s;&c_l;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_s;&c_l;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>r&uuml;hmen</trd>,
    <trd>ehren</trd>,
    <trd>preisen</trd>
  </trdgrp>
  <trd lng="en">glorify</trd>
  <trd lng="es">glorificar</trd>
  <trdgrp lng="fr">
    <trd>glorifier</trd>,
    <trd>rendre <ind>gloire</ind> (&agrave;)</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>dics&odblac;&iacute;t</trd>,
    <trd>magasztal</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x307B;&#x3081;&#x305F;&#x305F;&#x3048;&#x308B;</trd>,
    <trd>&#x79F0;&#x8CDB;&#x3059;&#x308B;  [<ind>&#x3057;&#x3087;&#x3046;&#x3055;&#x3093;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x9855;&#x5F70;&#x3059;&#x308B;  [<ind>&#x3051;&#x3093;&#x3057;&#x3087;&#x3046;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x6804;&#x5149;&#x3092;&#x305F;&#x305F;&#x3048;&#x308B;  [<ind>&#x3048;&#x3044;&#x3053;&#x3046;&#x3092;&#x305F;&#x305F;&#x3048;&#x308B;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
