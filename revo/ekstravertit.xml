<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ekstravertit.xml,v 1.8 2022/04/21 09:41:06 revo Exp $">
<kap>
  <rad>ekstravertit</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ekstravertit.0o">
  <kap><tld/>o</kap>
  <snc mrk="ekstravertit.0o.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      <ref tip="dif" cel="ekstravert.0ulo.PSI">Ekstravertulo</ref>:
      <ekz>
        &scirc;ia teorio bazi&gcirc;as sur la persona tipologio de la svisa psikiatro 
        <nom>Carl Gustav Jung</nom>, kiu asocii&gcirc;as i.a. kun la du terminoj <tld/>o
        kaj introvertito
        <fnt>
          <aut>D. Lytov</aut>,
          <vrk>Novaj ideoj jungitaj al malnovaj teorioj</vrk>,
          <bib>Monato</bib>, 
          <lok>2003:12, p. 14a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_s;&c_t;&c_r;&c_a;&c_v;&c_e;&c_r;&c_t;</trd>
  <trd lng="ca">extravertit<klr> (subst.)</klr></trd>
  <trdgrp lng="cs">
    <trd>extrovert</trd>,
    <trd>extrovertovan&yacute; jedinec</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>extrovert</trd>,
    <trd>extravert</trd>
  </trdgrp>
  <trd lng="sk">extrovert</trd>
  <trd lng="uk">&c_je;&#x43A;&c_s;&c_t;&c_r;&c_a;&c_v;&c_je;&c_r;&c_t;</trd>
</drv>

<drv mrk="ekstravertit.0a">
  <kap><tld/>a</kap>
  <snc mrk="ekstravertit.0a.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      <ref tip="dif" cel="ekstravert.0a.PSI">Ekstraverta</ref>:
      <ekz>
        mi ne havas la profilon de estro, certa pri si mem,
        <tld/>a, komunikema
        <fnt>
          <aut>Mark</aut>,
          <vrk><url ref="http://www.ivi-esperanto.org/testimonials/enterpeno.html">
          Anka&ubreve; estroj havas animon</url></vrk>,
          <lok>IVI, 2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i estas <tld/>a, forta, ambicia kaj malka&scirc;a
        <fnt>
          <vrk><url
          ref="https://childhoodbiography.com/eo/maria-zorgema-infaneca-rakonto-plus-neskribitaj-biografiaj-faktoj/">Mariah
          Carey Childhood Story Plus Untold Biography Facts</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la mistera kaj silenta Naoko kaj la amuzo-amanta kaj <tld/>a <nom>Midori</nom>
        <fnt>
          <vrk><url
          ref="https://steemit.com/esperanto/@johano/norwegian-wood-amrakonto-kaj-mojosa-kanto">Norwegian
          Wood /Amrakonto kaj mojosa kanto &mdash; Steemit</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_s;&c_t;&c_r;&c_a;&c_v;&c_e;&c_r;&c_t;&c_n;&c_y;</trd>
  <trd lng="ca">extravertit<klr> (adj.)</klr></trd>
  <trdgrp lng="cs">
    <trd>extravertn&iacute;</trd>,
    <trd>extrovertn&iacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>extroverted</trd>,
    <trd>extraverted</trd>
  </trdgrp>
  <trd lng="sk">extrovertn&yacute;</trd>
  <trd lng="uk">&c_je;&#x43A;&c_s;&c_t;&c_r;&c_a;&c_v;&c_je;&c_r;&c_t;&c_i;&c_v;&#x43D;&c_i;&c_j;</trd>
</drv>
</art>

