<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ekskre.xml,v 1.22 2022/03/16 15:54:27 revo Exp $">
<kap>
  <rad>ekskreci</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ekskre.0o">
  <kap><tld/>o</kap>
  <snc mrk="ekskre.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Procezaro de elimino de la for&jcirc;eta&jcirc;oj de la metabolo ekster la
      organismon, sub formo gasa (elspirita aero), likva (urino, &scirc;vito,
      galo) a&ubreve; e&ccirc; solida (ekz. &ccirc;e iuj dezertaj animaloj):
      <ekz>
        fekado ne estas <tld/>o, sed for&jcirc;eto de la nedigestitaj
        nutra&jcirc;restoj
        <fnt>
          <aut>Plena Ilustrita Vortaro</aut>,
          <vrk><url ref="http://vortaro.net/#ekskrecio">ekskreci/o</url></vrk>
        </fnt>
      </ekz>.
    </dif>
    <refgrp tip="vid">
      <ref cel="sekrec.0o">sekrecio</ref>,
      <ref cel="sekrec.el0o">elsekrecio</ref>
    </refgrp>
  </snc>
  <trd lng="ca">excreci&oacute;</trd>
  <trdgrp lng="cs">
    <trd>exkrece</trd>,
    <trd>vym&#x11B;&#x161;ov&#xE1;n&#xED;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Ausscheidung</trd>,
     <trd>Absonderung</trd>
  </trdgrp>
  <trd lng="en">excretion</trd>
  <trd lng="es">excreci&oacute;n</trd>
  <trd lng="fr">excr&eacute;tion</trd>
  <trdgrp lng="hu">
     <trd>kiv&aacute;laszt&aacute;s<klr tip="ind">
     (exkr&eacute;ci&oacute;)</klr></trd>,
     <trd>exkr&eacute;ci&oacute;</trd>
  </trdgrp>
  <trd lng="it">escrezione</trd>
  <trdgrp lng="pl">
    <trd>wydzielanie</trd>,
    <trd>wydalanie</trd>
  </trdgrp>
  <trd lng="pt">excre&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_v;&c_y;&c_d;&c_je;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">exkr&#xE9;cia</trd>
  <trdgrp lng="uk">
    <trd>&#x435;&#x43A;&#x441;&#x43A;&#x440;&#x435;&#x43A;&#x446;&#x456;&#x44F;</trd>,
    <trd>&#x432;&#x438;&#x434;&#x456;&#x43B;&#x435;&#x43D;&#x43D;&#x44F; (&#x434;&#x456;&#x44F;)</trd>
  </trdgrp>
</drv>

<drv mrk="ekskre.0a">
  <kap><tld/>a</kap>
  <snc mrk="ekskre.0a.FIZL">
    <dif>
      Rilatanta a&ubreve; servanta al <tld/>o.  
    </dif>
  </snc>
  <trd lng="ca">excretori</trd>
  <trdgrp lng="cs">
    <trd>exkre&#x10D;n&#xED;</trd>,
    <trd>t&#xFD;kaj&#xED;c&#xED; se exkrece</trd>,
    <trd>vym&#x11B;&#x161;ovac&#xED;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>ausscheidend</trd>,
     <trd>absondernd</trd>,
     <trd>exkretorisch</trd>
  </trdgrp>
  <trd lng="en">excretal</trd>
  <trdgrp lng="fr">
    <trd>excr&eacute;teur</trd>,
    <trd>excr&eacute;toire</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>exkr&eacute;ci&oacute;s</trd>,
     <trd>v&aacute;lad&eacute;k-</trd>,
     <trd>kiv&aacute;laszt&aacute;si</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>escretivo</trd>, <trd>escretore</trd>, <trd>escretorio</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wydalniczy</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_d;&c_je;&c_l;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_y;&c_v;&c_o;&c_d;&c_n;&c_o;&c_j;</trd>,
    <trd>&c_e;&c_k;&c_s;&c_k;&c_r;&c_je;&c_t;&c_o;&c_r;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">exkre&#x10D;n&#xFD;</trd>
  <trdgrp lng="uk">
    <trd>&#x432;&#x438;&#x434;&#x456;&#x43B;&#x44C;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x432;&#x438;&#x432;&#x456;&#x434;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x435;&#x43A;&#x441;&#x43A;&#x440;&#x435;&#x442;&#x43E;&#x440;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="ekskre.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="ekskre.0ajxo.FIZL">
    <dif>
      Substanco <tld/>ita:
      <ekz>
        la <tld/>a&jcirc;o estas tre viskoza
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 7a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">excrement</trd>
  <trd lng="en">excretion</trd>
  <trd lng="fr">excr&eacute;tion</trd>
  <trd lng="uk">&#x432;&#x438;&#x434;&#x456;&#x43B;&#x435;&#x43D;&#x43D;&#x44F; (&#x440;&#x435;&#x447;&#x43E;&#x432;&#x438;&#x43D;&#x430;)</trd>
</drv>

<drv mrk="ekskre.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ekskre.0i.FIZL">
    <dif>
      Fari ekskrecion.
    </dif>
  </snc>
  <snc mrk="ekskre.0i.elsekrec">
  <uzo tip="stl">EVI</uzo>
  <ref tip="dif" cel="sekrec.el0i">elsekrecii</ref>
  </snc>
  <trd lng="ca">excretar</trd>
  <trd lng="cs">vylou&#x10D;it</trd>
  <trdgrp lng="de">
     <trd>ausscheiden</trd>,
     <trd>absondern</trd>
  </trdgrp>
  <trd lng="en">excrete</trd>
  <trd lng="es">excretar</trd>
  <trd lng="fr">excr&eacute;ter</trd>
  <trd lng="he">&#x5DC;&#x5B0;&#x5D4;&#x5B7;&#x5E4;&#x5B0;&#x5E8;&#x5B4;&#x5D9;&#x5E9;&#x5C1; &#x5E6;&#x5D5;&#x5B9;&#x5D0;&#x5B8;&#x5D4;</trd>
  <trd lng="hu">kiv&aacute;laszt<klr tip="ind">
  (v&aacute;lad&eacute;kot)</klr></trd>
  <trdgrp lng="it">
     <trd>evacuare<klr> (mediante escrezione)</klr></trd>,
     <trd>espellere<klr> (mediante escrezione)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wydziela&cacute;</trd>,
    <trd>wydala&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_d;&c_je;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_z;&c_v;&c_je;&c_r;&c_g;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">vyl&#xFA;&#x10D;i&#x165;</trd>
  <trdgrp lng="uk">
    <trd>&#x432;&#x438;&#x434;&#x456;&#x43B;&#x44F;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x438;&#x43A;&#x438;&#x434;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>
</art>
