<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>harp</rad>/o
</kap>
<drv mrk="harp.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo> 
  <snc>
    <dif>
      Granda triangula muzikinstrumento, staranta sur sia bazo kaj
      provizita per vertikalaj diverslongaj kordoj, kiujn oni pin&ccirc;as
      kaj vibrigas per amba&ubreve; manoj: <ekz>pin&ccirc;i <tld/>on.</ekz>
    </dif>
  </snc>
  <trd lng="de">Harfe</trd>
  <trd lng="sv">harpa</trd>
  <trd lng="ru">&c_a;&c_r;&c_f;&c_a;</trd>
</drv>
<drv mrk="harp.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">MUZ</uzo> 
  <snc>
    <dif>
      Ludi <tld/>on.  
    </dif>
  </snc>
  <trd lng="ru">&c_i;&c_g;&c_r;&c_a;&c_t;&c_mol; &c_n;&c_a; &c_a;&c_r;&c_f;&c_je;</trd>
</drv>
</art>
