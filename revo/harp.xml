<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: harp.xml,v 1.34 2023/11/15 16:45:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>harp</rad>/o
</kap>
<drv mrk="harp.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="harp.0o.MUZ">
    <dif>
      Iu el diversaj
      <ref tip="lst" cel="instru2.kord0o.MUZ" lst="voko:kordinstrumentoj">kordinstrumentoj</ref>
      kun amba&ubreve;mane plukeblaj kordoj starantaj la&ubreve;
      vico orta al la bazo, oblikve stre&ccirc;itaj al agorda stango,
      kontraste al
      <ref tip="vid" cel="citr.0o.MUZ">citroj</ref>,
      <ref tip="vid" cel="lir.0o.MUZ">liroj</ref>,
      <ref tip="vid" cel="liut.0o">liutoj</ref>:
      <ekz>
        vi renkontos grupon da profetoj, irantaj malsupren de la alta&jcirc;o, kaj anta&ubreve; ili
        <ref cel="psalte.0o">psaltero</ref>n, tamburinon, fluton, kaj <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 10:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u li ne kantis jam al la &ccirc;ielaj a&ubreve;skultantoj la nomon Amalio sur la serafa
        <tld/>o
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de Eolo, korda instrumento muzika kiun tonigas ekblovoj de vento
        <fnt>
          <bib>SrT</bib>
          <lok>Libro VIII.</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/6/6f/Eliza_Ridgely_with_a_Harp_NGA.jpg/319px-Eliza_Ridgely_with_a_Harp_NGA.jpg" prm="PD">Eliza Ridgely kun <tld/>o
        <fnt>
          <aut>Thomas Sully, National Gallery of Art, Washington, D. C., online collection</aut>
          <lok><url ref="&WCW;/File:Eliza_Ridgely_with_a_Harp_NGA.jpg">https://commons.wikimedia.org/wiki/File:Eliza_Ridgely_with_a_Harp_NGA.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="ja">&#x30CF;&#x30FC;&#x30D7;</trd>
  <trd lng="kek">aarp</trd>
  <trd lng="be">&c_a;&c_r;&c_f;&c_a;</trd>
  <trd lng="bg">&c_a;&c_r;&c_f;&c_a;</trd>
  <trd lng="br">telenn</trd>
  <trd lng="cs">harfa</trd>
  <trd lng="de">Harfe</trd>
  <trd lng="el">&alfa_ton;&rho;&pi;&alfa;</trd>
  <trd lng="en">harp</trd>
  <trd lng="es">arpa</trd>
  <trd lng="fr">harpe<klr> (musique)</klr></trd>
  <trd lng="hu">h&aacute;rfa</trd>
  <trd lng="id">harpa</trd>
  <trd lng="it">arpa</trd>
  <trd lng="nl">harp</trd>
  <trd lng="pl">harfa</trd>
  <trd lng="pt">harpa</trd>
  <trd lng="ru">&c_a;&c_r;&c_f;&c_a;</trd>
  <trd lng="sk">harfa</trd>
  <trd lng="sv">harpa</trd>
  <trd lng="uk">&c_a;&c_r;&c_f;&c_a;</trd>
  <trd lng="zh">&#x7AD6;&#x7434; <pr>sh&#xF9;q&#xED;n</pr></trd>
</drv>

<drv mrk="harp.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ludi <tld/>on.
    </dif>
  </snc>
<!-- tro &gcirc;enerala rimarko pri gramatika demando, &ccirc;u?
  cetere oni povas uzi la verbon anka&ubreve; netransitive...
  <rim>
  &Ccirc;iuj verboj, derivitaj de nomoj de muzikiloj,
  konsekvence devas esti transitivaj, &ccirc;ar kiel rekta objekto
  servas ludata muzika&jcirc;o, ekz.: <tld/>i suiton. (Jurij Finkel)
  </rim>
-->
  <trd lng="be">&c_g;&c_r;&c_a;&c_c;&c_mol; &c_n;&c_a; <ind>&c_a;&c_r;&c_f;&c_je;</ind></trd>
  <trd lng="bg">&c_s;&c_v;&c_i;&c_r;&c_ja; &c_n;&c_a; &c_a;&c_r;&c_f;&c_a;</trd>
  <trd lng="br">telenna&ntilde;</trd>
  <trd lng="de">Harfe <ind>spielen</ind></trd>
  <trd lng="en">play the harp</trd>
  <trd lng="es">tocar el <ind>arpa</ind></trd>
  <trd lng="fr">jouer de la <ind>harpe</ind></trd>
  <trd lng="hu">h&aacute;rf&aacute;zik</trd>
  <trd lng="it">suonare con l'<ind>arpa</ind></trd>
  <trd lng="ja">&#x30CF;&#x30FC;&#x30D7;&#x3092;&#x6F14;&#x594F;&#x3059;&#x308B;  <pr>&#x306F;&#x30FC;&#x3077;&#x3092;&#x3048;&#x3093;&#x305D;&#x3046;&#x3059;&#x308B;</pr></trd>
  <trd lng="nl">harp spelen</trd>
  <trd lng="pl">gra&cacute; na <ind>harfie</ind></trd>
  <trd lng="pt">tocar <ind>harpa</ind></trd>
  <trd lng="ru">&c_i;&c_g;&c_r;&c_a;&c_t;&c_mol; &c_n;&c_a; 
&c_a;&c_r;&c_f;&c_je;</trd>
  <trdgrp lng="kek"><trd>aarpik</trd><trd>aarpib&#x315;k</trd></trdgrp>
  <trd lng="sk">hra&tcaron; na harfe</trd>
  <trd lng="cs">hr&aacute;t na harfu</trd>
  <trd lng="uk">&c_g;&c_r;&c_a;&c_t;&c_i; &c_n;&c_a; &c_a;&c_r;&c_f;&c_ib;</trd>
</drv>
</art>
