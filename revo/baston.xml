<drv mrk="baston.palp0o">
  <kap>
    palp<tld/>o, <var><kap>blindula <tld/>o</kap></var>
  </kap>
  <snc>
    <dif>
      <tld lit="B"/>o uzata de blindulo por prepalpi la vojon:
      <ekz>
        <klr>[&scirc;i]</klr> trovas bran&ccirc;on, kiun &scirc;i povas uzi kiel palp<tld/>on
        <klr>[&#8230;]</klr>, la tereno estas trompa
        <fnt>
          <bib>VRA</bib>
          <lok>Bj&ouml;rn Erik H&ouml;ijer</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_y;&c_s;&c_mol;&c_c;&c_ib;&c_n;&c_a; <klr>(&c_s;&c_mol;&c_l;&c_ja;&c_p;&c_o;&c_g;&c_a;)</klr></trd>
  <trd lng="ca">bast&oacute; de cec</trd>
