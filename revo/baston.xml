<drv mrk="baston.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="baston.0o.KOMUNE">
     <dif>
       Stangeto, &gcirc;enerale ligna, kiu utilas por sin apogi, por
       bati a&ubreve; kiel insigno:
      <ekz>
        <nom>Jakob</nom> prenis al si <tld/>ojn el verdaj poploj, migdalarboj, kaj platanarboj, kaj
        sen&scirc;eligis sur ili blankajn striojn
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 30:37</lok>
        </fnt>;
      </ekz>
      <ekz>
        li etendis la pinton de la <tld/>o <klr>[&#x2026;]</klr> kaj trempis &gcirc;in en la
        miel&ccirc;elaro
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 14:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi elvenis kun glavoj kaj <tld/>oj, kvaza&ubreve; kontra&ubreve; rabiston
        <fnt>
          <bib>NT</bib>
          <lok>Luko 22:52</lok>
        </fnt>;
      </ekz>
      <ekz>
        vojkamarado, kiu havis sian <tld/>on en la mano kaj la tornistron sur la dorso
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>apoga <tld/>o</ind>
        <fnt>
          <bib>SkandalJozef</bib>
          <lok>Plendoj de Jozefo arestita</lok>
        </fnt>;
        <trdgrp lng="de">
          <trd>Gehstock</trd>,
          <trd>St&uuml;tze</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind>abata <tld/>o</ind>
        <fnt>
          <aut>Evgeni Georgiev</aut>
            <vrk><url ref="&Monato;/2000/007113.php">Ser&ccirc;i la perditan paradizon</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 20</lok>
          </fnt>;
        <trdgrp lng="de">
          <trd>Krummstab</trd>,
          <trd>Abtsstab</trd>
        </trdgrp>
        <trd lng="lat">baculum pastorale</trd>
      </ekz>
      <ekz>
        <ind>balaila <tld/>o</ind>
        <fnt>
          <bib>Fab4</bib>
          <lok>Ne&gcirc;ulo</lok>
        </fnt>;
        <trd lng="de">Besenstiel</trd>
        <trd lng="en">broom stick</trd>
      </ekz>
      <ekz>
        <ind>blindula <tld/>o</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Istv&aacute;n Ertl</aut>
          <vrk><url ref="&Monato;/2013/010910.php">Dobos Delight</url></vrk>
          <lok>2013</lok>
        </fnt>;
        <trdgrp lng="de">
          <trd>Blindenstock</trd>,
          <trd>Langstock</trd>
        </trdgrp>
      </ekz>
      <ekz>
        magia <tld/>o <klr>(sor&ccirc;bastono)</klr>
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 4 (102)</lok>
        </fnt>;
      </ekz>
      <ekz>
        polica <tld/>o
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 17</lok>
        </fnt>;
        <trdgrp lng="de">
          <trd>Schlagstock</trd>,
          <trd>Gummikn&uuml;ppel</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind>teleskopa <tld/>o</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Jozefo Horv&aacute;th</aut>
          <vrk><url ref="&Monato;/2006/009060.php">Tra infanaj okuloj: memoroj pri la eventoj de '56</url></vrk>
          <lok>2006</lok>
        </fnt>;
        <trdgrp lng="de">
          <trd>Teleskopstock</trd>,
          <trd>Ausziehstock</trd>
        </trdgrp>
      </ekz>
      <ekz>
        por praktiki <ind><tld/>mar&scirc;ado</ind>n oni bezonas paron da ta&ubreve;gaj <tld/>oj, tiel altaj
        kiel 7/10 de la korpo
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2005/008790.php">Praktiki bastonmar&scirc;adon</url></vrk>
          <lok>2005</lok>
        </fnt>;
        <trd lng="de">Nordic Walking</trd>
      </ekz>
       <ekz>
         kiu bati deziras trovas <tld/>on
         <fnt><bib>PrV</bib></fnt>
         <klr>(vd <refgrp tip="vid">
           <ref tip="vid" cel="rabi.0a.furioza">rabia<sncref/></ref>
         </refgrp>)</klr>;
       </ekz>
       <ekz>
         <ref cel="baston.lam0o">lam<tld/>o</ref>,
         <tld/>o de policano, de mar&scirc;alo;
       </ekz>
       <ekz>
         <ind>&jcirc;eti <tld/>on en la radon</ind>
         <klr>(estigi malhelpa&jcirc;ojn)</klr>
         <trd lng="fr">mettre un b&acirc;ton dans les roues</trd>
         <fnt><bib>PrV</bib></fnt>.
       </ekz>    
     </dif>
      <refgrp tip="vid">
       <ref cel="kan.0o">kano</ref>,
       <ref cel="lat.0o">lato</ref>,
       <ref cel="palis.0o">paliso</ref>,
       <ref cel="trab.0o">trabo</ref>,
       <ref cel="verg.0o">vergo</ref>
     </refgrp>
     <trdgrp lng="zh">
    <trd>&#x6756; <pr>zh&#xE0;ng</pr></trd>,
    <trd>&#x8B66;&#x68CD; <pr>j&#x1D0;ngg&#xF9;n</pr></trd>
  </trdgrp>
</snc>
