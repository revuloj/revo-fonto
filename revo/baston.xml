<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>baston</rad>/o
</kap>
<drv mrk="baston.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc>
     <dif>
       Stangeto, &gcirc;enerale ligna, kiu utilas por sin apogi, por bati
       a&ubreve; kiel insigno:
       <ekz>
         kiu bati deziras trovas <tld/>on
         <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="rabi.0a">rabia</ref>;
     </refgrp>
     <ekz>
       (promen)<tld/>o, <ref cel="baston.lam0o">lam<tld/>o</ref>,
       <tld/>o de policano, de mar&scirc;alo;
     </ekz>
     <ekz>
       &jcirc;eti <tld/>on en la radon
       <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>
       <klr>(estigi malhelpa&jcirc;ojn)</klr>.
     </ekz>
   </snc>
   <refgrp tip="vid">
     <ref cel="brancx.0o">bran&ccirc;o</ref>,
     <ref cel="kan.0o">kano</ref>,
     <ref cel="lat.0o">lato</ref>,
     <ref cel="palis.0o">paliso</ref>,
     <ref cel="tabul.0o">tabulo</ref>,
     <ref cel="trab.0o">trabo</ref>,
     <ref cel="verg.0o">vergo</ref>.
   </refgrp>
   <trd lng="de">Stock</trd>
   <trd lng="fr">b&acirc;ton</trd>
   <trdgrp lng="ru">
     <trd>&c_p;&c_a;&c_l;&c_k;&c_a;</trd>,
     <trd>&c_p;&c_o;&c_s;&c_o;&c_h;</trd>,
     <trd>&c_t;&c_r;&c_o;&c_s;&c_t;&c_mol;</trd>,
     <trd>&c_zh;&c_je;&c_z;&c_l;</trd>
   </trdgrp>
</drv>
<drv mrk="baston.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc>
     <dif>
       Bati per <tld/>o.
     </dif>
   </snc>
   <trd lng="de">pr&uuml;geln</trd>
   <trd lng="fr">bastonner (tr.)</trd>
   <trd lng="ru">&c_b;&c_i;&c_t;&c_mol; &c_p;&c_a;&c_l;&c_k;&c_o;&c_j;</trd>
</drv>


     <drv mrk="baston.lam0o">
       <kap><ofc>*</ofc>lam<tld/>o</kap>
       <snc>
         <dif>
           <tld lit="B"/>o kun transversa trabeto, sur kiu kripluloj
           sin apogas por pa&scirc;i.
         </dif>
       </snc>
       <trd lng="fr">b&eacute;quille</trd>
     </drv>

</art>
