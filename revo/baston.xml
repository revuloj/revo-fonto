<drv mrk="baston.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="baston.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="bat.0i">Bati</ref> per <tld/>o:
      <ekz>
        ilia re&gcirc;o <klr>[&#x2026;]</klr> mem <tld/>is siajn regnanojn
        <klr>(suferigis sian popolon)</klr>
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ordonis <tld/>i la amasojn
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        fremdan dorson <tld/>i &dash; anka&ubreve; sian doni
        <klr>(toleru mem, per kio vi aliajn traktas)</klr>
        <fnt>
          <bib>Prv</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ib;&c_c;&c_mol;
  <ind>&c_p;&c_a;&c_l;&c_k;&c_a;&c_j;</ind></trd>
  <trd lng="bg">&c_u;&c_d;&c_r;&c_ja;&c_m; &c_s; &c_p;&c_r;&c_malmol;&c_ch;&c_k;&c_a;</trd>
   <trd lng="cs">b&iacute;t hol&iacute;</trd>
  <trdgrp lng="de">
     <trd><ind>Stockschl&auml;ge</ind> verabreichen</trd>,
     <trd>verpr&uuml;geln</trd>
   </trdgrp>
  <trd lng="en">club<klr> (someone or something)</klr></trd>
  <trd lng="fr">bastonner</trd>
  <trd lng="he">&lamed;&he;&kaf;&vav;&tav; &bet;&mem;&qof;&lamed;</trd>
  <trd lng="hu">megbotoz</trd>
  <trdgrp lng="io">
    <trd>bastonagar</trd>,
    <trd>bastonbatar</trd>
  </trdgrp>
  <trd lng="ja">&#x68D2;&#x3067;&#x53E9;&#x304F; <pr>&#x307C;&#x3046;&#x3067; &#x305F;&#x305F;&#x304F;</pr></trd>
  <trd lng="nl">met eens stok slaan</trd>
  <trdgrp lng="pl">
     <trd>bi&cacute; <ind>kijem</ind></trd>,
     <trd>wymierza&cacute; <ind>kije</ind></trd>,
     <trd>dawa&cacute; <ind>kije</ind></trd>
   </trdgrp>
  <trd lng="ru">&c_b;&c_i;&c_t;&c_mol;
  &c_p;&c_a;&c_l;&c_k;&c_o;&c_j;</trd>
  <trd lng="sk">by&tcaron; palicou</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_i;&c_t;&c_i; &c_p;&c_a;&c_l;&c_i;&c_c;&c_je;&c_ju;</trd>,
