     <trdgrp lng="zh">
    <trd>&#x6756; <pr>zh&agrave;ng</pr></trd>,
      <trd>&#x8B66;&#x68CD; <pr>j&#x1D0;ngg&ugrave;n</pr></trd>
    </trdgrp>
  </snc>
