<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: baston.xml,v 1.38 2021/05/30 12:16:28 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>baston</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="baston.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc mrk="baston.0o.KOMUNE">
     <dif>
       Stangeto, &gcirc;enerale ligna, kiu utilas por sin apogi, por
       bati a&ubreve; kiel insigno:
       <ekz>
         kiu bati deziras trovas <tld/>on
         <fnt><bib>PrV</bib></fnt>
         <klr>(vd <refgrp tip="vid">
           <ref tip="vid" cel="rabi.0a.furioza">rabia<sncref/></ref>
         </refgrp>)</klr>;
       </ekz>
       <ekz>
         <ref cel="baston.lam0o">lam<tld/>o</ref>,
         <tld/>o de policano, de mar&scirc;alo;
       </ekz>
       <ekz>
         <ind>&jcirc;eti <tld/>on en la radon</ind>
         <klr>(estigi malhelpa&jcirc;ojn)</klr>
         <trd lng="fr">mettre un b&acirc;ton dans les roues</trd>
         <fnt><bib>PrV</bib></fnt>.
       </ekz>    
     </dif>
      <refgrp tip="vid">
       <ref cel="kan.0o">kano</ref>,
       <ref cel="lat.0o">lato</ref>,
       <ref cel="palis.0o">paliso</ref>,
       <ref cel="trab.0o">trabo</ref>,
       <ref cel="verg.0o">vergo</ref>.
     </refgrp>
   </snc>
  <trd lng="af">stok</trd>
  <trd lng="am">&#x12F1;&#x120B;</trd>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_ib;&c_j;</trd>,
    <trd>&c_t;&c_r;&c_y;&c_s;&c_mol;&c_c;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_zh;&c_a;&c_z;&c_l;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_p;&c_r;&c_malmol;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_b;&c_a;&c_s;&c_t;&c_u;&c_n;</trd>
  </trdgrp>
  <trd lng="cs">h&uring;l</trd>
  <trdgrp lng="de">
     <trd>Stock</trd>,
     <trd>Stab</trd>,
     <trd>Stecken</trd>
   </trdgrp>
  <trdgrp lng="en">
     <trd>stick</trd>,
     <trd>cane</trd>,
     <trd>rod</trd>,
     <trd>baton</trd>,
     <trd>walking <ind>stick</ind></trd>
   </trdgrp>
  <trd lng="es">bast&oacute;n</trd>
  <trd lng="fr">b&acirc;ton</trd>
  <trd lng="he">&#x5DE;&#x5E7;&#x5DC;</trd>
  <trdgrp lng="hu">
    <trd>bot</trd>,
    <trd>p&aacute;lca</trd>
  </trdgrp>
  <trd lng="io">bastono</trd>
  <trdgrp lng="ja">
    <trd>&#x68D2; [<ind>&#x307C;&#x3046;</ind>]</trd>,
    <trd>&#x6756; [<ind>&#x3064;&#x3048;</ind>]</trd>
  </trdgrp>
  <trd lng="ne">&#x91B;&#x921;&#x940;</trd>
  <trdgrp lng="nl">
    <trd>stok</trd>,
    <trd>staf</trd>
  </trdgrp>
  <trdgrp lng="pl">
     <trd>laska</trd>,
     <trd>pa&lstroke;ka</trd>,
     <trd>laga</trd>,
     <trd>kij</trd>
   </trdgrp>
   <trd lng="pt">bast&atilde;o</trd>
   <trd lng="ro">baston</trd>
  <trdgrp lng="ru">
     <trd>&c_p;&c_a;&c_l;&c_k;&c_a;</trd>,
     <trd>&c_p;&c_o;&c_s;&c_o;&c_h;</trd>,
     <trd>&c_t;&c_r;&c_o;&c_s;&c_t;&c_mol;</trd>,
     <trd>&c_zh;&c_je;&c_z;&c_l;</trd>
   </trdgrp>
  <trd lng="sk">palica</trd>
  <trd lng="sq">shkop</trd>
  <trd lng="tr">baston</trd>
</drv>

<drv mrk="baston.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc mrk="baston.0i.KOMUNE">
     <dif>
       Bati per <tld/>o:
        <ekz>
          <uzo tip="stl">FIG</uzo>
          fremdan dorson <tld/>i &#x2010; anka&ubreve; sian doni
          <klr>(toleru mem, per kio vi aliajn traktas)</klr>
          <fnt>
            <bib>Prv</bib>
          </fnt>;
        </ekz>
        <ekz>
          ilia re&gcirc;o [...] mem <tld/>is siajn regnanojn
          <klr>(suferigis sian popolon)</klr>
          <fnt>
            <bib>Far1</bib>
            <lok>&Ccirc;apitro XVIII</lok>
          </fnt>.
        </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_b;&c_ib;&c_c;&c_mol;
  <ind>&c_p;&c_a;&c_l;&c_k;&c_a;&c_j;</ind></trd>
  <trd lng="bg">&c_u;&c_d;&c_r;&c_ja;&c_m; &c_s; &c_p;&c_r;&c_malmol;&c_ch;&c_k;&c_a;</trd>
   <trd lng="cs">b&iacute;t hol&iacute;</trd>
  <trdgrp lng="de">
     <trd>Stockschl&auml;ge verabreichen</trd>,
     <trd>pr&uuml;geln</trd>
   </trdgrp>
  <trd lng="en">club<klr> (someone or something)</klr></trd>
  <trd lng="fr">bastonner</trd>
  <trd lng="he">&#x5DC;&#x5D4;&#x5DB;&#x5D5;&#x5EA; &#x5D1;&#x5DE;&#x5E7;&#x5DC;</trd>
  <trd lng="hu">megbotoz</trd>
  <trdgrp lng="io">
    <trd>bastonagar</trd>,
    <trd>bastonbatar</trd>
  </trdgrp>
  <trd lng="ja">&#x68D2;&#x3067;&#x53E9;&#x304F; [<ind>&#x307C;&#x3046;&#x3067; &#x305F;&#x305F;&#x304F;</ind>]</trd>
  <trd lng="nl">met eens stok slaan</trd>
  <trdgrp lng="pl">
     <trd>bi&cacute; <ind>kijem</ind></trd>,
     <trd>wymierza&cacute; <ind>kije</ind></trd>,
     <trd>dawa&cacute; <ind>kije</ind></trd>
   </trdgrp>
  <trd lng="ru">&c_b;&c_i;&c_t;&c_mol;
  &c_p;&c_a;&c_l;&c_k;&c_o;&c_j;</trd>
  <trd lng="sk">by&tcaron; palicou</trd>
</drv>

<drv mrk="baston.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="baston.0eto.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      <ref tip="vid" cel="cxel.0o.BIO">&Ccirc;elo<sncref/></ref>
      de la retino sensema al intenso de lumo,
      tiel nomata pro sia formo:
      <ekz>
        la malhel-adaptita vido estas bazita sur la
        lumabsorbo far la retina purpura&jcirc;o entenata en la
        eksteraj segmentoj de la <tld/>etoj
        <fnt>
          <aut>J. S&eacute;gal, trad. A. Albault</aut>
          <vrk><url ref="https://scienca-revuo.info/article/viewFile/1721/1700">
          Elementoj de Teorio pri la Vido de la Koloroj</url></vrk>
          <lok>Scienca Revuo, 1953, 5:17-18, p. 8a-20a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konus.0eto.FIZL">konuseto</ref>
    </refgrp>
    <trd lng="de">St&auml;bchen<klr tip="amb"> (Zelle)</klr></trd>
    <trd lng="en">rod<klr tip="amb"> (photoreceptor cell)</klr></trd>
    <trd lng="fr">b&acirc;tonnet<klr tip="amb"> (cellule visuelle)</klr></trd>
    <trd lng="ja">&#x687F;&#x4F53;&#x7D30;&#x80DE; [<ind>&#x304B;&#x3093;&#x305F;&#x3044;&#x3055;&#x3044;&#x307C;&#x3046;</ind>]</trd>
    <trd lng="nl">staafje<klr tip="amb"> (lichtgevoelige
    cel)</klr></trd>
  </snc>
  <trd lng="bo">&#xF66;&#xFA6;&#xFB1;&#xF74;&#xF42;&#xF0B;&#xF54;</trd>
</drv>

<drv mrk="baston.lam0o">
  <kap><ofc>*</ofc>lam<tld/>o</kap>
  <snc mrk="baston.lam0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <tld lit="B"/>o kun transversa trabeto,
      sur kiu maljunulo a&ubreve; irhandikapito sin apogas por
      pa&scirc;i:
      <ekz>
        &ccirc;e la pordo de la pre&gcirc;ejo staris maljuna soldato kun
        lam<tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Ru&gcirc;aj &scirc;uoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la maljunulino ridis kaj kontenta saltis malgra&ubreve; la
        lam<tld/>o
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>;
      </ekz>
      <ekz>
        maljuna virino kun lam<tld/>oj enlamis sur la korton
        <fnt>
          <bib>Fab3</bib>
          <lok>&Ccirc;io sur sian &gcirc;ustan lokon!</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_j;</trd>,
    <trd>&c_t;&c_r;&c_y;&c_s;&c_mol;&c_c;&c_ib;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_a;&c_t;&c_je;&c_r;&c_i;&c_c;&c_a;</trd>
  <trd lng="cs">berla</trd>
  <trd lng="de">Kr&uuml;cke</trd>
  <trdgrp lng="en">
    <trd>crutch</trd>
  </trdgrp>
  <trd lng="es">muleta</trd>
  <trdgrp lng="fr">
    <trd>b&eacute;quille</trd>,
    <trd>canne anglaise</trd>
  </trdgrp>
  <trd lng="hu">mank&oacute;</trd>
  <trd lng="ja">&#x677E;&#x8449;&#x6756; [<ind>&#x307E;&#x3064;&#x3070;&#x3065;&#x3048;</ind>]</trd>
  <trd lng="nl">kruk<klr tip="amb"> 
    (hulpmiddel bij het lopen)</klr></trd>
  <trdgrp lng="pl">
    <trd>laska</trd>,
    <trd>kula<klr> (inwalidzka)</klr></trd>
  </trdgrp>
  <trd lng="sk">barla</trd>
</drv>

<drv mrk="baston.mangx0eto">
  <kap>man&gcirc;<tld/>eto,
  <var><kap>man&gcirc;<tld/>o</kap></var></kap>
  <snc mrk="baston.mangx0eto.KUI">
    <dif>
      Man&gcirc;ilo pare uzata por
      pin&ccirc;e preni pecojn de man&gcirc;a&jcirc;oj;
      <ref tip="sin" cel="hasxi.0o.KUI">ha&scirc;io</ref>:
      <ekz>
        la antikvaj libroj notas, ke man&gcirc;<tld/>etoj
        aperis anta&ubreve; pli ol 1 000 jaroj
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url ref="http://esperanto.cri.cn/chinaabc/chapter18/chapter180302.htm">
          Man&gcirc;bastonetoj&mdash;Mirinda Man&gcirc;ilo</url></vrk>,
          <lok>[vidita en 2011]</lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;ni man&gcirc;u!&rightquot; <nom>Kwang</nom> diris, indikante per
        man&gcirc;<tld/>oj la bovlojn kaj pladojn ar&gcirc;entajn sur la tablo
        <fnt>
          <aut>R. Van Gulik, trad. B. Harris</aut>
          <vrk><url ref="http://esperanto-me.org/Literaturo/cxerkoj/cxerkoj.htm">
          La &Ccirc;erkoj de la Imperiestro</url></vrk>
          <lok>[2004]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_p;&c_r;&c_malmol;&c_ch;&c_i;&c_c;&c_i; &c_z;&c_a; &c_h;&c_r;&c_a;&c_n;&c_je;&c_n;&c_je;</trd>
  <trd lng="da">spisepinde</trd>
  <trd lng="de">Essst&auml;bchen</trd>
  <trd lng="en">chopstick</trd>
  <trd lng="fa">&f_ch;&a_A;&f_p;&a_s;&a_t;&f_y;&f_k;</trd>
  <trd lng="fr">baguette<klr> (couvert de table)</klr></trd>
  <trd lng="ja">&#x7BB8; [<ind>&#x306F;&#x3057;</ind>]</trd>
  <trd lng="ko">&#xC813;&#xAC00;&#xB77D;</trd>
  <trd lng="mk">&c_s;&c_t;&c_a;&c_p;&c_ch;&c_i;&#x45A;&c_a; &c_z;&c_a; &#x458;&c_a;&c_d;&c_je;&#x45A;&c_je;</trd>
  <trd lng="nl">eetstokje</trd>
  <trd lng="sr">&c_sh;&c_t;&c_a;&c_p;&c_i;&#x45B;&c_i; &c_z;&c_a; &#x458;&c_je;&c_l;&c_o;</trd>
  <trd lng="th">&#xE15;&#xE30;&#xE40;&#xE01;&#xE35;&#xE22;&#xE1A;</trd>
  <trd lng="vi">&#x111;&#x169;a</trd>
  <trd lng="zh">&#x7B77;&#x5B50; [<ind>ku&agrave;izi</ind>]</trd>
</drv>

<drv mrk="baston.promen0o">
  <kap>
    promen<tld/>o
  </kap>
  <snc mrk="baston.promen0o.KOMUNE">
    <dif>
      <tld lit="B"/>o per kiu oni apogas sin dum promeno,
      dum piedirado:
      <ekz>
        li volis ekvidi la montegojn ... kaj porti glavon anstata&ubreve; promen<tld/>on
        <fnt>
          <aut>J. R. R. Tolkien, trad. C. Gledhill</aut>
          <vrk>La Hobito</vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">canne</trd>
</drv>
</art>
