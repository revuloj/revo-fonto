<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>atenc</rad>/i
</kap>
<drv mrk="atenc.0i">
   <kap><ofc>*</ofc><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc num="1">
     <dif>
       Malrespekte krimi  kontra&ubreve; iu a&ubreve; io:
       <ekz>
         <tld/>i iun, kontra&ubreve; iu<fnt>Z</fnt>
       </ekz>
       <ekz>
         mortis tiuj, kiuj <tld/>is la vivon de la knabeto
         <fnt>(<vrk>Nova testamento</vrk>,
         <lok>Mateo 2, 20</lok>)</fnt>
         <klr>(provis murdi lin)</klr>.
       </ekz>
     </dif>
   </snc>
   <snc num="2">
     <dif>
       Malrespekte trakti respektinda&jcirc;on:
       <ekz>
         <tld/>i ies rajtojn;
       </ekz>
       <ekz>
         <tld/>i la le&gcirc;ojn, la liberecon;
       </ekz>
       <ekz>
         <tld/>i la lingvon
         <klr>(grave malobei la regulojn  de la lingvo)</klr>.
       </ekz>
     </dif>
   </snc>
   <trd lng="en">assault</trd>
   <trdgrp lng="fr">
     <trd>porter <ind>atteinte</ind> (&agrave;)</trd>,
     <trd>outrager</trd>,
     <trd>attenter (&agrave;)</trd></trdgrp>
   <trdgrp lng="ru">
     <trd>&#1087;&#1086;&#1082;&#1091;&#1096;&#1072;&#1090;&#1100;&#1089;&#1103;</trd>,
     <trd>&#1087;&#1086;&#1089;&#1103;&#1075;&#1072;&#1090;&#1100;</trd>
   </trdgrp>
</drv>
</art>
