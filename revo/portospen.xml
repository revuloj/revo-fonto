<drv mrk="portospen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POL</uzo><uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      La <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&ccirc;efurbo</ref>
      de <ref tip="malprt" cel="trinidad.T0otobago">Trinidado kaj Tobago</ref>:
