    <trd>&#x4E0B;&#x52A3;&#x306A;  <pr>&#x3057;&#x305F;&#x304A;&#x3068;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ellendig</trd>,
    <trd>armzalig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>mizerny</trd>,
    <trd>n&eogonek;dzny</trd>,
    <trd>biedny</trd>,
    <trd>ubogi</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_i;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_i;&c_shch;&c_je;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>biedny</trd>,
    <trd>&uacute;boh&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_je;&c_shch;&c_a;&c_s;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_ib;&c_d;&c_o;&c_l;&c_a;&c_sh;&c_n;&c_i;&c_j; &c_z;&c_l;&c_i;&c_d;&c_a;&c_r;&c_s;&c_mol;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_l;&c_i;&c_d;&c_je;&c_n;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_u;&c_zh;&c_d;&c_je;&c_n;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_u;&c_b;&c_o;&c_g;&c_i;&c_j;</trd>,
    <trd>&c_zh;&c_a;&c_l;&c_ju;&c_g;&c_ib;&c_d;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
