    <trd>&#x591A;&#x707E;&#x591A;&#x96BE; <pr>du&#x14D;z&#x101;idu&#x14D;n&#xE0;n</pr></trd>,
    <trd>&#x60B2;&#x82E6; <pr>b&#x113;ik&#x1D4;</pr></trd>
