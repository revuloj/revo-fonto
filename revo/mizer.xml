<drv mrk="mizer.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Trafi en malri&ccirc;on, en <tld/>on:
      <ekz>
        mi nutros vin tie, <klr>[&#x2026;]</klr> por ke ne <tld/>i&gcirc;u vi kaj via domo
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 45:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[en]</klr> niaj mondaj urbegoj <klr>[&#x2026;]</klr> la homo <klr>[&#x2026;]</klr>
        bagateli&gcirc;as kaj <tld/>i&gcirc;as, &gcirc;is fine li estas nura mikrobo
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
          <lok>1. Kakume.</lok>
        </fnt>;
      </ekz>
      <ekz>
        por kiu devis &ccirc;iu suferi, <tld/>i&gcirc;i kaj vivi en malkomforto
        <fnt>
          <bib>VaK</bib>
          <lok>Deka &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="de">in <ind>Not</ind> geraten</trd>
  </snc>          
  <trdgrp lng="be">
    <trd>&c_z;&c_mol;&c_b;&c_ja;&c_d;&c_n;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_g;&c_a;&c_l;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_g;&c_a;&c_l;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>kouezha&ntilde; an <ind>dienez</ind> war ub.</trd>,
    <trd>mont da <ind>reuzeudig</ind></trd>
  </trdgrp>
  <trd lng="de">in <ind>Armut</ind> geraten</trd>
  <trdgrp lng="en">
    <trd>become <ind>miserable</ind></trd>,
    <trd>become <ind>wretched</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&ecirc;tre dans le <ind>besoin</ind></trd>,
    <trd>tomber dans la <ind>mis&egrave;re</ind></trd>
  </trdgrp>
  <trd lng="he">&lamed;<ind>&he;&tav;&alef;&mem;&lamed;&lamed;</ind>
  </trd>
  <trd lng="nl">in <ind>ellende</ind> geraken</trd>
  <trdgrp lng="pl">
    <trd>zmizernie&cacute;</trd>,
    <trd>zbiednie&cacute;</trd>,
    <trd>wpa&sacute;&cacute; w <ind>bied&eogonek;</ind></trd>,
    <trd>popa&sacute;&cacute; w <ind>bied&eogonek;</ind></trd>,
    <trd>zn&eogonek;dznie&cacute;</trd>,
    <trd>wpa&sacute;&cacute; w <ind>n&eogonek;dz&eogonek;</ind></trd>,
    <trd>popa&sacute;&cacute; w <ind>n&eogonek;dz&eogonek;</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_b;&c_n;&c_i;&c_shch;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="mizer.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="mizer.0ulo.POL">
    <dif>
      Homo, kiu spertas <tld/>on:
      <ekz>
        &scirc;i renkontis <tld/>ulon, kiu diris: &leftquot;donu al mi
        ion por man&gcirc;i&rightquot;
        <fnt><bib>ElektFab</bib>, <lok>steloj talaroj</lok></fnt>;
      </ekz>
      <ekz>
        la princo sentis impreson, ke la sennoma amaso de <tld/>uloj kaj ribeluloj
        superforte pu&scirc;as lin
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> malavare donacis al <tld/>uloj, por ke ili pre&gcirc;u por li
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ursa Felo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i min amas! <tld/>ulo, &ccirc;u vi tion &ccirc;i meritis
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>?
      </ekz>
      <ekz>
        konsoli&gcirc;as <tld/>ulo, se li estas ne sola
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="ricx.mal0ulo">malri&ccirc;ulo</ref>,
      <ref cel="hav.sen0ulo">senhavulo</ref>
    </refgrp>
    <refgrp tip="vid">
