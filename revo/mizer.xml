<drv mrk="mizer.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="mizer.0i.POL">
    <dif>
      Trovi&gcirc;i en <tld/>o:
      <ekz>
        <tld/>is lo&gcirc;antaro de vila&gcirc;oj detruitaj
        <fnt>
          <bib>VivZam</bib>
          <lok>Vivo de Zamenhof</lok>
        </fnt>;
      </ekz>
      <ekz>
        kaj kial ni &ccirc;iuj tiel <tld/>as? nur &ccirc;ar nia patrino nin abrupte forlasis
        <fnt>
          <bib>ChB</bib>
          <lok>30</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u kaj mal&gcirc;oju kaj ploru, via ridado turni&gcirc;u en
        ploron
        <fnt><bib>NT</bib>, <lok>&Jak; 4:9</lok></fnt>;
      </ekz>
      <ekz>
        partoprenado en britaj elektoj &gcirc;enerale <tld/>as: homoj apena&ubreve; komprenas kiel,
        kaj por kiu, ili vo&ccirc;donu
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2011/010372.php">La venkanto heredos &ccirc;ion ...</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>kiam sako <tld/>as amo malaperas</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="de"><ind>Not</ind> ist der Liebe Tod</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_l;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_a;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>beza&ntilde; en <ind>dienez</ind></trd>,
    <trd>mizeri&ntilde;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>b&yacute;t uboh&yacute;</trd>,
    <trd>trp&ecaron;t b&iacute;du</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><ind>elendig</ind> sein</trd>,
    <trd><klr>in </klr><ind>Not</ind> sein</trd>,
    <trd><ind>j&auml;mmerlich</ind> gehen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>be <ind>miserable</ind></trd>,
    <trd>be <ind>wretched</ind></trd>
  </trdgrp>
  <trd lng="fr">&ecirc;tre dans la <ind>mis&egrave;re</ind></trd>
  <trdgrp lng="he">
    <trd>&lamed;&he;&yod;&vav;&tav; <ind>&alef;&vav;&mem;&lamed;&lamed;
    </ind></trd>,
    <trd>&lamed;&he;&yod;&vav;&tav; <ind>&mem;&samekh;&kaf;&fnun;</ind>
    </trd>,
    <trd>&lamed;&he;&yod;&vav;&tav; <ind>&ayin;&lamed;&vav;&bet;</ind>
    </trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nyomorog</trd>,
    <trd>sz&uuml;ks&eacute;get szenved</trd>
  </trdgrp>
  <trd lng="ja">&#x56F0;&#x7AAE;&#x3057;&#x3066;&#x3044;&#x308B;  <pr>&#x3053;&#x3093;&#x304D;&#x3085;&#x3046;&#x3057;&#x3066;&#x3044;&#x308B;</pr></trd>
  <trd lng="nl">in <ind>ellende</ind> verkeren</trd>
  <trdgrp lng="pl">
    <trd>by&cacute; w n&eogonek;dzy</trd>,
    <trd>znajdowa&cacute; si&eogonek; w n&eogonek;dzy</trd>,
    <trd>cierpie&cacute; n&eogonek;dz&eogonek;</trd>,
    <trd>znajdowa&cacute; si&eogonek; w biedzie</trd>,
    <trd>biedowa&cacute;</trd>,
    <trd>klepa&cacute; bied&eogonek;</trd>,
    <trd>trwa&cacute; w ub&oacute;stwie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;
    </trd>,
    <trd>&c_n;&c_u;&c_zh;&c_d;&c_a;&c_t;&c_mol;&c_s;&c_ja;
    <klr tip="ind">
    (&c_b;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;)</klr>
    </trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>biedi&tcaron;</trd>,
    <trd>trie&tcaron; biedu</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_t;&c_i; &c_v; &c_z;&c_l;&c_i;&c_d;&c_n;&c_ja;&c_h;</trd>,
