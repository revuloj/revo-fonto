<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: trihxi.xml,v 1.15 2024/01/25 18:30:28 revo Exp $">
<kap>
  <rad>tri&hcirc;in</rad>/o
</kap>
<drv mrk="trihxi.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">Genro</ref>
      de <ref tip="malprt" cel="verm.faden0oj">fadenvermoj</ref> (<trd lng="la">Trichinella</trd>),
      kiuj parazitas en mamuloj kaj birdoj:
      <ekz>
        homoj infekti&gcirc;as per <tld/>oj precipe tra porkoj, la homo estas erara gastiganto, &ccirc;ar
        neman&gcirc;ate
        de aliaj bestoj ili ne povas plu reprodukti&gcirc;i tiel;
      </ekz>
      <ekz>
        <tld/>oj en la intesto ka&ubreve;zas kapturnon, ventrodoloron, vomadon kaj lakson,
        kiam ili atingas aliajn korporegionojn aldoni&gcirc;as malforti&gcirc;o, febro, edemoj &ccirc;e la viza&gcirc;o
        <klr>&#8230;</klr>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_y;&c_h;&c_ib;&c_n;&c_a;</trd>
  <trd lng="bg">&c_t;&c_r;&c_i;&c_h;&c_i;&c_n;&c_a;</trd>
  <trd lng="ca">triquina</trd>
  <trd lng="de">Trichine</trd>
  <trd lng="en">trichina</trd>
  <trdgrp lng="fr">
    <trd>trichinose</trd>,
    <trd>trichinellose</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>trichina</trd>
    <trd>bors&oacute;f&eacute;reg</trd>,
    <trd>bors&oacute;ka</trd></trdgrp>
  <trd lng="nl">trichine</trd>
  <trdgrp lng="pl">
    <trd>w&lstroke;osie&nacute;</trd>,
    <trd>trychina</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>triquina</trd>,
    <trd>triquinela</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_i;&c_h;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_r;&c_i;&c_h;&c_ib;&#x43D;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_i;&c_h;&c_ib;&#x43D;&c_je;&#x43B;&c_a;</trd>
  </trdgrp>
</drv>
</art>
