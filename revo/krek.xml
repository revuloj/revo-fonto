<vortaro>
  <art mrk="$Id: krek.xml,v 1.9 2026/02/01 03:29:38 afido Exp $">
    <kap>
      <rad>krek</rad>/o <fnt><bib>PVEF</bib></fnt>
    </kap>

<drv mrk="krek.0o">
  <kap><tld/>o</kap>
  <snc mrk="krek.0o.anaso">
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_specioj" cel="speci.0o.BIO">Specio</ref>
      de <ref tip="super" cel="anas.0o.ZOO">anaso</ref>
      (<trd lng="la">Anas crecca</trd>):
      <ekz>
        <tld/>o vivas vintre en mar&ccirc;oj, marbordo, riverfa&ubreve;koj
        <fnt>
          <vrk><url ref="&Viki;/Kreko">Kreko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      <fnt><bib>PIV1</bib></fnt> referencante al <nom>Grabowski</nom> ankora&ubreve; difinas
      &leftquot;<tld/>o&rightquot; kiel
      sinonimo al &leftquot;krekso&rightquot; (<nom>Crex crex</nom>), pri &gcirc;i ver&scirc;ajne temas anka&ubreve;
      tiu cita&jcirc;o de &ccirc;.
      1932:
      <ekz>
        la <tld/>o kantis sian knaran krepuskmelodion
        <fnt>
          <aut>Stellan Engholm</aut>
          <vrk><url ref="&Tekstaro;?nomo=homoj-sur-la-tero">Homoj sur la
          tero</url></vrk>
        </fnt>.
      </ekz>
    </rim>
    <bld lok="&WCU;/thumb/3/30/Eurasian_teal.jpg/640px-Eurasian_teal.jpg" prm="CC BY-SA 2.5"><tld/>o
    maskla
        <fnt>
          <aut>Andreas Trepte, propra verko</aut>
          <lok><url ref="&WCW;/File:Eurasian_teal.jpg">&WCW;/File:Eurasian_teal.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="be">&c_k;&c_a;&c_ch;&c_k;&c_a; &c_c;&c_y;&c_r;&c_a;&c_n;&c_k;&c_a;</trd>
    <trd lng="de">Krickente</trd>
    <trd lng="en">Eurasian teal</trd>
    <trd lng="fr">sarcelle d'hiver</trd>
    <trd lng="pl">cyraneczka zwyczajna</trd>
    <trd lng="zh">&#40427; <pr>s&ugrave;</pr></trd>
</snc>
  <trdgrp lng="ca">
    <trd>xarxet</trd>,
    <trd>sarcet</trd>
  </trdgrp>
  <trd lng="es">cerceta</trd>
  <trd lng="uk">&c_ch;&c_i;&c_r;&c_o;&c_k; &c_s;&c_v;&c_i;&c_s;&c_t;&c_u;&c_n;&c_o;&c_k;</trd>
</drv>

  </art>

