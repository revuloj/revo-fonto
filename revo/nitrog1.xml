<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>nitroglicerin</rad>/o
</kap>
<drv mrk="nitrog1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Likva siropeca kombino, rezultanta el la ago de la nitrata acido sur
      la glicerinon:
      <ekz>
        <tld/>o estas tre dan&gcirc;era eksploda&jcirc;o;
      </ekz>
      <ekz>
        miksita kun porecaj substancoj &gcirc;i liveras dinamiton.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_ib;&c_t;&c_r;&c_a;&c_g;&c_l;&c_ib;&c_c;&c_e;&c_r;&c_y;&c_n;</trd>
  <trd lng="ca">nitroglicerina</trd>
  <trd lng="de">Nitroglyzerin</trd>
  <trd lng="el">&ny;&jota;&tau;&rho;&omikron;&gamma;&lambda;&ypsilon;&kappa;&epsilon;&rho;&jota_ton;&ny;&eta;</trd>
  <trd lng="en">nitroglycerin</trd>
  <trd lng="es">nitroglicerina</trd>
  <trd lng="hu">nitroglicerin</trd>
  <trd lng="nl">nitroglycerine</trd>
  <trdgrp lng="pt">
    <trd>nitroglicerina</trd>,
    <trd>glono&iacute;na</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_i;&c_t;&c_r;&c_o;&c_g;&c_l;&c_i;&c_c;&c_je;&c_r;&c_i;&c_n;</trd>
</drv>
</art>
