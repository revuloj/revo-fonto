<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nitrog1.xml,v 1.25 2024/01/29 15:59:10 revo Exp $">
<kap>
  <rad>nitroglicerin</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="nitrog1.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">KEM</uzo>
    <dif>
      <frm>C<sub>3</sub>H<sub>5</sub>N<sub>3</sub>O<sub>9</sub></frm>, glicerintrinitrato; 
      <ref cel="ester.0o">estero</ref> rezultanta el la ago de la nitrata 
      acido sur la <ref cel="glicer3.0o">glicerino</ref>n:
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> brulas kviete, sed je bato eksplodas
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=22228">Nitroglicerino</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        li precipe ser&ccirc;is metodojn por pli sekura fabrikado kaj pristudis la uzon de
        <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Edward Kusters</aut>
          <vrk><url ref="&Monato;/2006/009100.php">Alfred Nobel: nobelo inter la sciencistoj kaj lingvistoj</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dinami2.0o">dinamito</ref>      
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_ib;&c_t;&c_r;&c_a;&c_g;&c_l;&c_ib;&c_c;&c_e;&c_r;&c_y;&c_n;</trd>
  <trd lng="ca">nitroglicerina</trd>
  <trd lng="cs">nitroglycerin</trd>
  <trd lng="de">Nitroglyzerin</trd>
  <trd lng="el">&ny;&jota;&tau;&rho;&omikron;&gamma;&lambda;&ypsilon;&kappa;&epsilon;&rho;&jota_ton;&ny;&eta;</trd>
  <trd lng="en">nitroglycerin</trd>
  <trd lng="es">nitroglicerina</trd>
  <trd lng="fa">&a_n;&f_y;&a_t;&a_r;&a_w;&f_g;&a_l;&f_y;&a_s;&f_y;&a_r;&f_y;&a_n;</trd>
  <trd lng="fr">nitroglyc&eacute;rine</trd>
  <trd lng="hu">nitroglicerin</trd>
  <trd lng="it">nitroglicerina</trd>
  <trd lng="ja">&#x30CB;&#x30C8;&#x30ED;&#x30B0;&#x30EA;&#x30BB;&#x30EA;&#x30F3;</trd>
  <trd lng="nl">nitroglycerine</trd>
  <trdgrp lng="no">
    <trd>nitroglyserol</trd>,
    <trd>glyseryltrinitrat</trd>,
    <trd>nitroglyserin<klr tip="amb"> (foreldret)</klr></trd>
  </trdgrp>
  <trd lng="pl">nitrogliceryna</trd>
  <trdgrp lng="pt">
    <trd>nitroglicerina</trd>,
    <trd>glono&iacute;na</trd>
  </trdgrp>
  <trd lng="ro">nitroglicerin&abreve;</trd>
  <trd lng="ru">&c_n;&c_i;&c_t;&c_r;&c_o;&c_g;&c_l;&c_i;&c_c;&c_je;&c_r;&c_i;&c_n;</trd>
  <trd lng="sk">nitroglicer&iacute;n</trd>
  <trd lng="uk">&c_n;&c_ib;&c_t;&c_r;&c_o;&c_gu;&c_l;&c_ib;&c_c;&c_je;&c_r;&c_i;&c_n;)</trd>
  <trd lng="zh">&#x785D;&#x5316;&#x7518;&#x6CB9; <pr>xi&#x101;ohu&#xE0;g&#x101;ny&#xF3;u</pr></trd>
</drv>
</art>
