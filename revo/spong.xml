<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>spong</rad>/o
</kap>

<drv mrk="spong.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Malpeza, porori&ccirc;a, tre sorbema materio, produktata de mara polipo
      (nuntempe ofte la artefarita imita&jcirc;o).
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_b;&c_k;&c_a;</trd>
  <trd lng="bg">&c_g;&c_malmol;&c_b;&c_a;</trd>
  <trd lng="de">Schwamm</trd>
  <trd lng="en">sponge</trd>
  <trd lng="fr">&eacute;ponge</trd>
  <trdgrp lng="hu">
    <trd>szivacs</trd>,
    <trd>spongya</trd>
  </trdgrp>
  <trd lng="id">spons</trd>
  <trd lng="it">spugna</trd>
  <trd lng="nl">spons</trd>
  <trd lng="ru">&c_g;&c_u;&c_b;&c_k;&c_a;</trd>
</drv>

<drv mrk="spong.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Vi&scirc;i a&ubreve; sorbigi per <tld/>o.
    </dif>
  </snc>
  <trd lng="en">sponge</trd>
  <trd lng="fr">&eacute;ponger</trd>
  <trdgrp lng="hu">
    <trd>szivaccsal felt&ouml;rl</trd>,
    <trd>szivaccsal felitat</trd>
  </trdgrp>
  <trd lng="it">spugnare</trd>
  <trd lng="nl">afsponsen</trd>
  <trd lng="ru">&c_v;&c_y;&c_t;&c_i;&c_r;&c_a;&c_t;&c_mol; <ind>&c_g;&c_u;&c_b;&c_k;&c_o;&c_j;</ind></trd>
</drv>

<drv mrk="spong.0eca">
  <kap><tld/>eca</kap>
  <snc>
    <dif>
      Havanta la ecojn de <tld/>o:
      <ekz>
        <ind>bova <tld/>eca encefalopatio</ind> (BSE).
        <trdgrp lng="hu">
          <trd>szarvasmarh&aacute;k szivacsos agyvel&odblac;gyullad&aacute;sa</trd>,
          <trd>kergemarhak&oacute;r</trd>
        </trdgrp>
        <trd lng="ru">&c_g;&c_u;&c_b;&c_ch;&c_a;&c_t;&c_a;&c_ja; &c_e;&c_n;&c_c;&c_je;&c_f;&c_a;&c_l;&c_o;&c_p;&c_a;&c_t;&c_i;&c_ja;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_b;&c_ch;&c_a;&c_t;&c_a;&c_ja;</trd>
  <trd lng="en">spongy</trd>
  <trd lng="fr">spongieux</trd>
  <trd lng="hu">szivacsos</trd>
  <trdgrp lng="it">
    <trd>spongiforme</trd>, 
    <trd>spugnoso</trd>
  </trdgrp>
  <trd lng="nl">sponsachtig</trd>
  <trd lng="ru">&c_g;&c_u;&c_b;&c_ch;&c_a;&c_t;&c_y;&c_j;</trd>
</drv>
</art>
