    <trd lng="de">Plasmodium</trd>
    <trd lng="en">plasmodium</trd>
    <trd lng="fr">plasmodium</trd>
    <trd lng="ru">&c_p;&c_l;&c_a;&c_z;&c_m;&c_o;&c_d;&c_i;&c_i;</trd>
  </snc>
<trdgrp lng="be">
  <trd>&c_p;&c_l;&c_ja;&c_z;&c_m;&c_o;&c_d;</trd>,
  <trd>&c_p;&c_l;&c_ja;&c_z;&c_m;&c_o;&c_d;&c_y;&c_j;</trd>
</trdgrp>
