<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ripet.xml,v 1.29 2026/01/06 18:48:06 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ripet</rad>/i
<fnt><bib>UV</bib></fnt></kap>

<drv mrk="ripet.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Denove diri a&ubreve; fari tion, kio anta&ubreve;e estis dirita a&ubreve; farita:
      <ekz>
        a&ubreve;skultu kaj <tld/>u
        <fnt>
          <bib>EeP</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>as, ke tio &ccirc;i estas mia privata opinio
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Tria Kongreso Esperantista en
          Cambridge en la 12a de a&ubreve;gusto 1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        kontra&ubreve;uloj de nia ideo konstante <tld/>adis al ni, ke <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Tria Kongreso Esperantista en
          Cambridge en la 12a de a&ubreve;gusto 1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la jura konsilisto supozis, ke &scirc;i ne komprenas dane, kaj tial li <tld/>is sian
        deziron germane
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> rimarkis ke ili sin <tld/>as, ke ili rakontas &ccirc;iam tion saman
        <fnt>
          <bib>Fab4</bib>
          <lok>Korta koko kaj ventkoko</lok>
        </fnt>;
      </ekz>
      <ekz>
        kaj vi, bruto, jam &gcirc;ojas, ke vi povas &ccirc;ion tion &ccirc;i <tld/>i al mi
        <fnt><bib>Revizoro</bib>, <lok>akto 2a, sceno 2a</lok></fnt>;
      </ekz>
      <ekz>
        kiel hundo revenas al sia vomita&jcirc;o, tiel malsa&gcirc;ulo <tld/>as sian
        malsa&gcirc;a&jcirc;on
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 26:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ado estas plej bona lernado
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gurd.0i">gurdi</ref>,
      <ref cel="imit.0i.agi">imiti<sncref/></ref>,
      <ref cel="raport.0i">raporti</ref>,
      <ref cel="dir.re0i.raporti">rediri<sncref/></ref>,
      <ref cel="recipr.0i">reciproki</ref>,
      <ref cel="don.re0i.rekompenci">redoni<sncref/></ref>,
      <ref cel="macx.re0i">rema&ccirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_w;&c_t;&c_a;&c_r;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="bg">
    <trd>&c_p;&c_o;&c_v;&c_t;&c_a;&c_r;&c_ja;&c_m;</trd>,
    <trd>&c_p;&c_o;&c_v;&c_t;&c_v;&c_o;&c_r;&c_ja;</trd>
  </trdgrp>
  <trd lng="cs">opakovat</trd>
  <trdgrp lng="de">
    <trd>wiederholen</trd>,
    <trd>wiedergeben</trd>
  </trdgrp>
  <trd lng="en">repeat</trd>
  <trd lng="fr">r&eacute;p&eacute;ter</trd>
  <trdgrp lng="hu">
    <trd>ism&eacute;tel</trd>,
    <trd>megism&eacute;tel</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>ulang</baz>mengulang</trd>,
    <trd><baz>ulang</baz>mengulangi</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7E70;&#x308A;&#x8FD4;&#x3059;  <pr>&#x304F;&#x308A;&#x304B;&#x3048;&#x3059;</pr></trd>,
    <trd>&#x53CD;&#x5FA9;&#x3059;&#x308B;  <pr>&#x306F;&#x3093;&#x3077;&#x304F;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x7E70;&#x308A;&#x8FD4;&#x3057;&#x3066;&#x8A00;&#x3046;  <pr>&#x304F;&#x308A;&#x304B;&#x3048;&#x3057;&#x3066;&#x3044;&#x3046;</pr></trd>,
    <trd>&#x5FA9;&#x5531;&#x3059;&#x308B;  <pr>&#x3075;&#x304F;&#x3057;&#x3087;&#x3046;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="ne">&#x92B;&#x947;&#x930;&#x93F;</trd>
  <trd lng="nl">herhalen</trd>
  <trdgrp lng="pl">
    <trd>powt&oacute;rzy&cacute;</trd>,
    <trd>bisowa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">repetir</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_v;&c_t;&c_o;&c_r;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_v;&c_t;&c_o;&c_r;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>op&auml;tova&tcaron;</trd>,
    <trd>zopakova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>upprepa</trd>,
    <trd>repetera</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_v;&c_t;&c_o;&c_r;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_o;&c_v;&c_t;&c_o;&c_r;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_je;&c_p;&c_je;&c_t;&c_i;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x91CD;&#x8FF0; <pr>ch&oacute;ngsh&ugrave;</pr></trd>,
    <trd>&#x91CD;&#x590D; <pr>ch&oacute;ngf&ugrave;</pr></trd>,
    <trd>&#x91CD;&#x8907; <pr>ch&oacute;ngf&ugrave;</pr></trd>,
    <trd>&#x91CD; <pr>ch&oacute;ng</pr></trd>,
    <trd>&#x4E2D;&#x7E7C; <pr>zh&omacron;ngj&igrave;</pr></trd>,
    <trd>&#x4E2D;&#x7EE7; <pr>zh&omacron;ngj&igrave;</pr></trd>,
    <trd>&#x53CD;&#x590D;&#x5730; <pr>f&#x1CE;nf&ugrave;de</pr></trd>,
    <trd>&#x53CD;&#x5FA9;&#x5730; <pr>f&#x1CE;nf&ugrave;de</pr></trd>,
    <trd>&#x91CD;&#x6F14; <pr>ch&oacute;ngy&#x1CE;n</pr></trd>,
    <trd>&#x91CD;&#x505A; <pr>ch&oacute;ngzu&ograve;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ripet.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Reokazi, denove aperi:
      <ekz>
        &scirc;ian kapon plenigis unu sola penso,
        kiu sen&ccirc;ese <tld/>i&gcirc;adis kiel rekanta&jcirc;o
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_w;&c_t;&c_a;&c_r;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_a;&c_d;&c_b;&c_y;&c_v;&c_a;&c_c;&c_c;&c_a; &c_z;&c_n;&c_o;&c_w;</trd>
  </trdgrp>
  <trd lng="de">sich <ind>wiederholen</ind></trd>
  <trdgrp lng="fr">
    <trd>se r&eacute;p&eacute;ter</trd>,
    <trd>se reproduire<klr tip="amb"> (arriver de nouveau)</klr></trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>ulang</baz>berulang</trd>,
    <trd><baz>ulang</baz>terulang</trd>
  </trdgrp>
  <trd lng="ja">&#x7E70;&#x8FD4;&#x3055;&#x308C;&#x308B;
  <pr>&#x304F;&#x308A;&#x304B;&#x3048;&#x3055;&#x308C;&#x308B;</pr></trd>
  <trdgrp lng="pl">
    <trd>powtarza&cacute; si&eogonek;</trd>,
    <trd>nawraca&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_v;&c_t;&c_o;&c_r;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_v;&c_t;&c_o;&c_r;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_v;&c_t;&c_o;&c_r;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_v;&c_t;&c_o;&c_r;&c_ja;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="zh">&#x91CD;&#x6F14; <pr>ch&oacute;ngy&#x1CE;n</pr></trd>
</drv>

<drv mrk="ripet.antaux0i">
  <kap>anta&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Anticipe <tld/>i, ordinare por parkeri a&ubreve; bone lerni:
      <ekz>
        li volis diri ion plu: frazon, kiun li jam formulis
        kaj anta&ubreve;<tld/>adis al si multfoje
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 10a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="parker.0igi">parkerigi</ref>,
      <ref cel="trejn.0i">trejni</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_p;&c_e;&c_t;&c_y;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>wiederholen <klr>(Aufgabe)</klr></trd>,
    <trd>repetieren</trd>,
    <trd>auswendig <ind>lernen</ind> <klr>(wiederholen)</klr></trd>,
    <trd>pauken <klr>(durch Widerholung)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>r&eacute;p&eacute;ter<klr tip="ind"> (&agrave; l'avance)</klr></trd>
  </trdgrp>
  <trd lng="id"><baz>hafal</baz>menghafal</trd>
  <trdgrp lng="pl">
    <trd>powtarza&cacute;</trd>,
    <trd>powt&oacute;rzy&cacute; naprz&oacute;d</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_p;&c_je;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="ripet.mok0i">
  <kap>mok<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Refari a&ubreve; rediri mise, moke imiti:
      <ekz>
        &leftquot;&ccirc;io sur sian &gcirc;ustan lokon!&rightquot; li mok<tld/>is
        la vortojn de la bienmastro
        <fnt><bib>Fab3</bib>, <lok>&leftquot;&Ccirc;io sur sian &gcirc;ustan lokon!&rightquot;</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="parodi.0i">parodii</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_d;&c_r;&c_a;&c_zh;&c_n;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">nach&auml;ffen</trd>
  <trd lng="fr">contrefaire<klr tip="amb"> (par moquerie)</klr></trd>
  <trdgrp lng="pl">
    <trd>przedrze&zacute;nia&cacute;</trd>,
    <trd>wy&sacute;miewa&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_d;&c_r;&c_a;&c_z;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_d;&c_r;&c_a;&c_z;&c_n;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
