<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>geodezi</rad>/o
</kap>
<drv mrk="geodez.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SCI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="scienc.0o.fako">Scienco</ref>, per kiu oni difinas
      la grandon kaj formon de la tero a&ubreve; de parto de la tero.  
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Geod&auml;sie</trd>,
     <trd>Vermessungskunde</trd></trdgrp>
  <trd lng="fr">g&eacute;od&eacute;sie</trd>
  <trdgrp lng="nl">
    <trd>geodesie</trd>,
    <trd>landmeetkunde</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_je;&c_o;&c_d;&c_je;&c_z;&c_i;&c_ja;</trd>
</drv>
</art>
