<art mrk="$Id: geodez.xml,v 1.26 2023/11/11 10:00:39 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>geodezi</rad>/o
</kap>
<drv mrk="geodez.0o">
  <kap><tld/>o</kap>
  <snc mrk="geodez.0o.SCI">
    <uzo tip="fak">GEOD</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="lst" cel="scienc.0o.fako" lst="voko:sciencoj">Scienco</ref>, per kiu oni
      difinas la grandon kaj formon de la tero a&ubreve; de parto de la tero:
      <ekz>
        baza <tld/>a tasko estas difini reciprokan pozicion de unuopaj punktoj de tera
        surfaco
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=28987">Geodezio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_a;&c_d;&c_e;&c_z;&c_ib;&c_ja;</trd>
  <trd lng="ca">geod&egrave;sia</trd>
  <trdgrp lng="cs">
    <trd>geodezie</trd>,
    <trd>zem&ecaron;m&ecaron;&rcaron;i&ccaron;stv&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Geod&auml;sie</trd>,
    <trd>Vermessungskunde</trd>
  </trdgrp>
  <trd lng="en">geodesy</trd>
  <trd lng="es">geodesia</trd>
  <trd lng="fa">&f_zh;&a_y_hamza;&a_w;&a_d;&a_z;&f_y;</trd>
  <trd lng="fr">g&eacute;od&eacute;sie</trd>
  <trdgrp lng="hu">
    <trd>f&ouml;ldm&eacute;r&eacute;stan</trd>,
    <trd>geod&eacute;zia</trd>
  </trdgrp>
  <trd lng="id">geodesi</trd>
  <trd lng="ja">&#x6E2C;&#x5730;&#x5B66;  <pr>&#x305D;&#x304F;&#x3061;&#x304C;&#x304F;</pr></trd>
  <trdgrp lng="nl">
    <trd>geodesie</trd>,
    <trd>landmeetkunde</trd>
  </trdgrp>
  <trd lng="pl">geodezja</trd>
  <trdgrp lng="pt">
    <trd>geod&eacute;sia</trd>,
    <trd>geodesia</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_je;&c_o;&c_d;&c_je;&c_z;&c_i;&c_ja;</trd>
  <trd lng="sk">geod&eacute;zia</trd>
  <trd lng="uk">&#x433;&#x435;&#x43E;&#x434;&#x435;&#x437;&#x456;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x5927;&#x5730;&#x6D4B;&#x91CF;&#x5B66; <pr>d&#xE0;d&#xEC;c&#xE8;li&#xE1;ngxu&#xE9;</pr></trd>,
    <trd>&#x6D4B;&#x5730;&#x7EBF; <pr>c&#xE8;dexi&#xE0;n</pr></trd>,
    <trd>&#x6D4B;&#x5730;&#x5B66; <pr>c&#xE8;dexu&#xE9;</pr></trd>
  </trdgrp>
</drv>
</art>
