<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: taksonom.xml,v 1.5 2024/01/11 01:37:44 revo Exp $">
<kap>
  <rad>taksonom</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="taksonom.0o">
  <kap><tld/>o</kap>
  <snc mrk="taksonom.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="dif" cel="sistematik.0isto.SCI">Sistematikisto</ref>:
      <ekz>
        la&ubreve; kelkaj <tld/>oj <klr>en</klr> marubio estas
        &ccirc;irka&ubreve; 40 specoj
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Marubio">
          Marubio</url>, 2013-03-30</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Ernst Mayr</nom> <klr>[&#8230;]</klr> estis renoma <tld/>o, tropika esploristo, ornitologo
        <klr>[&#8230;]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=229145">Ernst Mayr</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    Konfuzebla kun
    <ref tip="vid" cel="takson.0o.BIO">takson</ref>-nomo.
  </rim>
  <trd lng="de">Taxonom</trd>
  <trd lng="fr">taxonomiste</trd>
  <trd lng="pl">taksonom</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_a;&#x43A;&c_s;&#x43E;&#x43D;&#x43E;&#x43C;</trd>,
    <trd>&c_s;&#x43F;&c_je;&c_c;&c_ib;&c_a;&#x43B;&c_ib;&c_s;&c_t; &c_z; &c_t;&c_a;&#x43A;&c_s;&#x43E;&#x43D;&#x43E;&#x43C;&c_ib;&c_ji;</trd>
  </trdgrp>
</drv>
</art>

