<drv mrk="paf.for0i">
  <kap>
    for<tld/>i
  </kap>
  <snc>
    <dif>
      <tld lit="P"/>e forigi (<tld/>a&jcirc;on a&ubreve; cela&jcirc;on):
      <ekz>
        li metis unu pizon en la <tld/>ilon kaj for<tld/>is &gcirc;in
        <fnt>
          <bib>Fab3</bib>
          <lok>Kvin en unu silikvo</lok>
        </fnt>;
      </ekz>
      <ekz>
        Vilhelmo for<tld/>is la pomon de la kapo de sia filo
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi iam povis for<tld/>i &ccirc;erizon de la bu&scirc;o
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kutimaj sakrokanonadoj for<tld/>is &ccirc;ian municion
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>wegschie&szlig;en</trd>
    <trd>abschie&szlig;en</trd>,
    <trd>umschie&szlig;en</trd>,
    <trd>verscheuchen</trd>
  </trdgrp>
  <trd lng="en">shoot away</trd>
  <trdgrp lng="es">
    <trd>ahuyentar</trd>,
    <trd>espantar</trd>
  </trdgrp>
  <trd lng="fr">effaroucher</trd>
  <trd lng="nl">wegschieten</trd>
  <trd lng="ru">&c_o;&c_t;&c_s;&c_t;&c_r;&c_je;&c_l;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_s;&c_t;&c_r;&c_ib;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_s;&c_t;&c_r;&c_ib;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>


<drv mrk="paf.for0igxi">
  <kap>for<tld/>i&gcirc;i, 
  <var><kap>sin for<tld/>i</kap></var></kap>
  <snc>
    <dif>
      Subite <ref cel="aper.mal0i.nevidebligxi">malaperi</ref>; forkuri:
      <ekz>
        malaperante kiel vaporo la du vizitintoj <tld/>is sin for el la lo&gcirc;ejo tiel
        mistere kaj senspure, kiel ili envenis
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 9</lok>
        </fnt>;
      </ekz>
      <ekz>
        li pafis sin for
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">pry&ccaron;</trd>
  <trdgrp lng="de">
    <trd>davonschie&szlig;en</trd>,
    <trd>davonsausen</trd>,
    <trd>davoneilen</trd>
    <trd>sich <ind>verpissen</ind></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>salir disparado</trd>,
    <trd>despegar<klr> (un cohete)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se tirer<klr tip="ind"> (s'en aller)</klr></trd>,
    <trd>filer<klr tip="ind"> (s'en aller)</klr></trd>
  </trdgrp>
  <trd lng="nl">wegschieten<klr tip="ind"> (snel weglopen)</klr></trd>
  <trd lng="sk">pre&ccaron;</trd>
</drv>


<drv mrk="paf.fingro0o">
  <kap>fingro<tld/>o</kap>
  <snc mrk="paf.fingro0o.MOV">
    <dif>
      Frapo a&ubreve; forbato farita per fingro unue pinte retenita
      &ccirc;e la dikfingro, kaj poste ellasita:
      <ekz>
        per du fingroj la junulino pin&ccirc;is la haron
        kaj malpeze fingro<tld/>is &gcirc;in
        <fnt>
          <aut>Mo Yan, trad. Vejdo</aut>
          <vrk>Travidebla Ru&gcirc;a Rafano</vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_s;&c_t;&c_r;&c_y;&c_ch;&c_k;&c_a; &c_p;&c_a;&c_l;&c_mol;&c_c;&c_a;&c_m;&c_ib;</trd>
  <trd lng="de">Fingerschnips</trd>
  <trdgrp lng="fr">
    <trd>chiquenaude</trd>,
    <trd>pichenette</trd>
  </trdgrp>
  <trd lng="pl">pstrykni&eogonek;cie palcami</trd>
  <trd lng="ro">pocnesc din degete</trd>
</drv>

<drv mrk="paf.masxin0ilo">
  <kap>ma&scirc;in<tld/>ilo</kap>
  <snc mrk="paf.masxin0ilo.ARM">
    <dif>
      <ref tip="super" cel="paf.0ilo.ARM"><tld lit="P"/>ilo</ref>
      &jcirc;etanta rapide kaj a&ubreve;tomate multajn kuglojn:
      <ekz>
        la kirasa&ubreve;tomobiloj minace montras
        ma&scirc;in<tld/>ilojn kaj
        plumba pluvo superver&scirc;as la ribelantajn kvartalojn
        <fnt>
          <aut>V. Varankin</aut>
          <vrk>Metropoliteno</vrk>
          <lok>1933</lok>
        </fnt>;
      </ekz>
      <ekz>
        amba&ubreve;flanke postenis du batalpretaj soldatoj en
        kamuflaj kostumoj kaj kun oblikve tenataj ma&scirc;in<tld/>iloj
        <fnt>
          <aut>Pejno S.</aut>
          <vrk>Tagmeze en Tunizo</vrk>
          <bib>Monato</bib>
          <lok>2001:11, p. 24a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="mitral.0o">mitralo</ref>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_l;&c_ja;&c_m;&c_jo;&c_t;</trd>
  <trd lng="bg">&c_k;&c_a;&c_r;&c_t;&c_je;&c_ch;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="ca">metralladora</trd>
  <trd lng="de">Maschinengewehr</trd>
  <trdgrp lng="en">
    <trd>machine gun</trd>,
    <trd>automatic rifle</trd>
  </trdgrp>
  <trd lng="es">ametralladora</trd>
  <trdgrp lng="fr">
    <trd>arme automatique &agrave; rafale</trd>,
    <trd>pistolet mitrailleur</trd>
  </trdgrp>
  <trd lng="he">&mem;&kaf;&vav;&nun;&tav;
  &yod;&resh;&yod;&yod;&he;</trd>
  <trd lng="hu">g&eacute;puska</trd>
  <trdgrp lng="id">
    <trd><ind>senapan</ind> mesin</trd>,
    <trd>mitraliur</trd>
  </trdgrp>
  <trd lng="nl">machinegeweer</trd>
  <trd lng="no">maskingev&aelig;r</trd>
  <trdgrp lng="pl">
    <trd>karabin</trd>,
    <trd>pistolet maszynowy</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>pu&#537;c&abreve;</trd>,
    <trd>mitralier&abreve;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_u;&c_l;&c_je;&c_m;&c_jo;&c_t;</trd>
  <trd lng="sv">maskingev&auml;r</trd>
  <trd lng="uk">&c_a;&c_v;&c_t;&c_o;&c_m;&c_a;&c_t;</trd>
</drv>

<drv mrk="paf.mort0i">
  <kap>mort<tld/>i</kap>
  <snc mrk="paf.mort0i.KOMUNE">
    <ref tip="dif" cel="mort.paf0igi"><tld/>mortigi</ref>
    <rim>
      Kvankam &leftquot;mort<tld/>i&rightquot; estas pli mallonga, tamen
      &leftquot;<tld/>mortigi&rightquot; estas pli kutima kaj
      preferinda:
      unue, &ccirc;ar temas pri mortigo per pafo (sed se temas pri
      neintenca <tld/>o morta, &leftquot;mort<tld/>i&rightquot; povas
      pli
      konveni);
      kaj due, pro la similsencaj vortoj kiel
      &leftquot;&scirc;tonmortigi&rightquot;.
      <aut>S. Pokrovskij</aut>
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_s;&c_t;&c_r;&c_e;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_s;&c_t;&c_r;&c_e;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_s;&c_t;&c_r;&c_e;&c_l;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_z;&c_a;&c_s;&c_t;&c_r;&c_je;&c_l;&c_v;&c_a;&c_m;</trd>
  <trd lng="ca">matar <klr>(amb una arma)</klr></trd>
  <trdgrp lng="cs">
    <trd>odst&rcaron;elit</trd>,
    <trd>zast&rcaron;elit</trd>
  </trdgrp>
  <trd lng="de">totschie&szlig;en</trd>
  <trd lng="en">shoot to death</trd>
  <trdgrp lng="fr">
    <trd>abattre<klr tip="ind"> (tuer qn)</klr></trd>,
    <trd>fusiller</trd>,
    <trd>flinguer</trd>
  </trdgrp>
  <trd lng="hu">agyonl&odblac;</trd>
  <trd lng="id"><baz>tembak</baz>menembak mati</trd>
  <trd lng="nl">neerschieten</trd>
  <trd lng="no">skyte ned</trd>
  <trd lng="pl">zastrzeli&cacute;</trd>
  <trdgrp lng="ro">
    <trd>omor&icirc;</trd>,
    <trd>trage</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_s;&c_t;&c_r;&c_je;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_r;&c_je;&c_l;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odstreli&tcaron;</trd>,
    <trd>zastreli&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_s;&c_t;&c_r;&c_je;&c_l;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_s;&c_t;&c_r;&c_ib;&c_l;&c_ja;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="paf.pulvo0ilo">
  <kap>pulvo<tld/>ilo</kap>
  <snc mrk="paf.pulvo0ilo.ARM">
    <dif>
      <ref tip="super" cel="paf.0ilo.ARM"><tld lit="P"/>ilo</ref>
      &jcirc;etanta pafa&jcirc;on per eksplodo de <ref cel="pulv.0o">pulvo</ref>:
      <ekz>mi ne rajtis uzi pulvo<tld/>ilon a&ubreve; lancon, dum li
      tenas nur
      glavon
       <fnt>
         <aut>E. Rice Borroughs, trad. K. R. C. Sturmer</aut>
         <vrk>Princino de Marso</vrk>
         <lok>1938</lok>
       </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_g;&c_n;&c_ja;&c_s;&c_t;&c_r;&c_e;&c_l;&c_mol;&c_n;&c_a;&c_ja;
    <ind>&c_z;&c_b;&c_r;&c_o;&c_ja;</ind></trd>
    <trd lng="ca">arma de foc</trd>
    <trd lng="en">firearm</trd>
    <trd lng="fr">arme &agrave; feu</trd>
    <trd lng="hu">t&udblac;zfegyver</trd>
    <trdgrp lng="id">
      <trd><ind>senjata</ind> api</trd>,
      <trd>senpi</trd>
    </trdgrp>
    <trd lng="nl">vuurwapen</trd>
    <trd lng="pl">bro&nacute; palna</trd>
    <trd lng="ro">arm&abreve; de foc</trd>
    <trd lng="ru">&c_o;&c_g;&c_n;&c_je;&c_s;&c_t;&c_r;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je;
      <ind>&c_o;&c_r;&c_u;&c_zh;&c_i;&c_je;</ind></trd>
  </snc>
  <adm>
    Indus enkonduki norman manieron nomi amba&ubreve; tipojn de
    ~ilo en preciza lingva&jcirc;o. PV &scirc;ajne uzas jen
    "eksplod&jcirc;etilo", jen "pulvo~ilo" por la dua senco, sed mi ne
    scias,
    &ccirc;u tio havas bazon en la uzado. "Pulvo~ilo" aspektas pli
    kohera
    kaj estas uzata en la difino de aliaj tiaj armiloj, anka&ubreve; en
    PIV1.
    Tial mi &gcirc;in elektis. Tamen sub "armi" PIV1 enkondukis
    "pafarmilo"
    kun &gcirc;uste tiu senco kaj tio aspektas la&ubreve;  mi nek logika (ja
    "pafi"
    ne neprigas uzon de pulvo), nek kohera kun la ceteraj artikoloj.
    <aut>MB</aut>
  </adm>
  <trd lng="bg">&c_o;&c_g;&c_n;&c_je;&c_s;&c_t;&c_r;&c_je;&c_l;&c_n;&c_o;
  &c_o;&c_r;&c_malmol;&c_zh;&c_i;&c_je;</trd>
  <trd lng="de">Feuerwaffe</trd>
  <trd lng="sk">streln&aacute; zbra&ncaron;</trd>
  <trd lng="cs">paln&aacute; zbra&ncaron;</trd>
</drv>

<drv mrk="paf.sag0o">
  <kap>sag<tld/>o</kap>
  <snc mrk="paf.sag0o.MEZ">
    <dif>
      Malformala
      <ref tip="lst" cel="unu.mezur0o" lst="voko:longoj">mezurunuo</ref>
      de longo, proksimume egala al 70 metroj:
      <ekz>
        dekon da sag<tld/>oj okcidenten
        <fnt><aut>E. de Zilah</aut>, <vrk>La Princo &ccirc;e la
        Hunoj</vrk>, 
        <lok>2011</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Pfeilwurf <klr tip="ind">(Entfernung)</klr></trd>,
      <trd>Steinwurf <klr tip="ind">(Entfernung)</klr></trd>,
      <trd>Wurfreichweite</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_jo;&c_t; &c_s;&c_t;&c_r;&c_a;&c_l;&c_y; <klr>(&c_m;&c_je;&c_r;&c_a; &c_a;&c_d;&c_l;&c_je;&c_g;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  <trd lng="fr">port&eacute;e de fl&egrave;che</trd>
  <trd lng="pl">odleg&lstroke;o&sacute;&cacute; na strza&lstroke; z &lstroke;uku</trd>
  <trd lng="ro">distan&#539;a pe lovitur&abreve; de la arc</trd>
</drv>

<drv mrk="paf.sxrot0ilo">
  <kap>&scirc;rot<tld/>ilo</kap>
  <snc mrk="paf.sxrot0ilo.ARM">
    <dif>
      <ref tip="super" cel="paf.0ilo.ARM"><tld lit="P"/>ilo</ref>
      <tld/>anta 
      <ref tip="vid" cel="sxrot.0o.MIL">&scirc;roton</ref>,
      tio estas metalpecetojn:
      <ekz>
        mi ekkaptis mian &scirc;rot<tld/>ilon
        <fnt>
          <aut>M. Twain, trad. R. Williams</aut>,
          <vrk><url ref="&Sezonoj;2014/05/twain/">
          La familio McWilliams kaj la kontra&ubreve;&scirc;telista
          alarmilo</url></vrk>,
          <lok>La Ondo de Esperanto, 2014:3 (234)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_a;&c_b;&c_a;&c_v;&c_ib;&c_k;</trd>
  <trd lng="de">Schrotflinte</trd>
  <trd lng="fr">fusil de chasse</trd>
  <trd lng="pl">strzelba na &sacute;rut</trd>
  <trd lng="ro">pu&#537;c&abreve; pentru &#537;urub</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_r;&c_o;&c_b;&c_o;&c_v;&c_a; &c_r;&c_u;&c_sh;&c_n;&c_i;&c_c;&c_ja;</trd>,
    <trd>&c_d;&c_r;&c_o;&c_b;&c_o;&c_v;&c_i;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="paf.0ilkapo">
  <kap><tld/>ilkapo</kap>
  <snc mrk="paf.0ilkapo.kolbo">
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="dif" cel="kolb.0o">Kolbo</ref>:
      <ekz>
        li alpremas al li la <tld/>ilkapon al la viza&gcirc;o
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili batis lin teren per la <tld/>ilkapoj
        <fnt>
          <aut>A. Strindberg, trad. G. H. Backman</aut>
          <vrk>La konscienco ripro&ccirc;as</vrk>
          <lok>1928</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni senprokraste lin atakos per bajoneto kaj <tld/>ilkapo
        <fnt>
          <aut>B. von Suttner, trad. A. Caumont</aut>
          <vrk>For la batalilojn!</vrk>
          <lok>1914</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_k;&c_l;&c_a;&c_d;
    <klr>(&c_s;&c_t;&c_r;&c_e;&c_l;&c_mol;&c_b;&c_y;)</klr></trd>,
    <trd>&c_r;&c_u;&c_k;&c_a;&c_ja;&c_c;&c_mol;
    <klr>(&c_p;&c_ib;&c_s;&c_t;&c_a;&c_l;&c_je;&c_t;&c_a;)</klr></trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_p;&c_r;&c_i;&c_k;&c_l;&c_a;&c_d;</trd>,
    <trd>&c_d;&c_r;&c_malmol;&c_zh;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">culata</trd>
  <trd lng="de">Kolben</trd>
  <trdgrp lng="hu">
    <trd>puskatus</trd>,
    <trd>agy<klr tip="amb"> (pusk&aacute;&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="pl">kolba</trd>
  <trd lng="ro">pat de pu&#537;c&abreve;</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_k;&c_l;&c_a;&c_d;</trd>
  <trd lng="sk">hlave&ncaron; <klr>(strelnej zbrane)</klr></trd>
  <trdgrp lng="cs">
    <trd>hlave&ncaron;</trd>,
    <trd>lauf <klr>(hovor.)</klr></trd>
  </trdgrp>
  <trd lng="uk">&c_p;&c_r;&c_i;&c_k;&c_l;&c_a;&c_d; (&c_r;&c_u;&c_sh;&c_n;&c_i;&c_c;&c_ib; &c_ib; &c_t;.&c_p;.)</trd>
</drv>
