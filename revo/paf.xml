      kies fako estas cel<tld/>i.
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_t;&c_r;&c_a;&c_l;&c_o;&c_k;</trd>,
      <trd>&c_s;&c_t;&c_r;&c_a;&c_l;&c_je;&c_c;</trd>
    </trdgrp>
    <trd lng="ca">tirador</trd>
    <trdgrp lng="en">
      <trd>marksman</trd>
    </trdgrp>
    <trdgrp lng="he">
      <trd>&qof;&lamed;&ayin;</trd>,
      <trd>&tsadi;&lamed;&fpe;</trd>
    </trdgrp>
    <trd lng="hu">l&ouml;v&eacute;sz</trd>
    <trd lng="pl">strzelec</trd>
    <trd lng="ru">&c_s;&c_t;&c_r;&c_je;&c_l;&c_o;&c_k;</trd>
  </snc>
</drv>

