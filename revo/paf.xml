<drv mrk="paf.0ilego">
  <kap><ofc>*</ofc><tld/>ilego <fnt><bib>F</bib>, <lok>Universala
  Vortaro, <url ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5137">paf'il'eg'</url></lok></fnt><fnt><bib>F</bib>,
  <lok>Ekzercaro, &para;
  38</lok></fnt></kap>
  <snc mrk="paf.0ilego.ARM">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Granda <ref tip="super" cel="paf.pulvo0ilo.ARM">pulvo<tld/>ilo</ref>
      priservata de pluraj soldatoj-artileriistoj:
      <ekz>
        por mu&scirc;on mortigi oni <tld/>ilegon ne bezonas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ne kunvenis regnestroj, nek ministroj, por &scirc;an&gcirc;i la
        politikan karton de la mondo, ne brilas luksaj vestoj kaj
        multego da imponantaj ordenoj en nia salono, ne bruas
        <tld/>ilegoj &ccirc;irka&ubreve; la modesta domo, en kiu ni
        trovi&gcirc;as
        <fnt>
          <aut>&Z;</aut>,
          <vrk>La parolado anta&ubreve;
            la Unua Kongreso Esperantista (Boulogne sur Mer,
            1905-08-05).</vrk></fnt>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="artile.0o.MIL_armiloj">artilerio</ref>
    <refgrp tip="sub">
      <ref cel="bomb.0opafilo">bombopafilo</ref>,
      <ref cel="hauxbi.0o">ha&ubreve;bizo</ref>,
      <ref cel="kanon1.0o.pafilo">kanono</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_m;&c_a;&c_t;&c_a;</trd>
  <trd lng="bg">&c_o;&c_r;&c_malmol;&c_d;&c_i;&c_je;</trd>
  <trdgrp lng="ca">
    <trd>can&oacute;</trd>,
    <trd>llan&ccedil;acoets</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gesch&uuml;tz</trd>,
    <trd>Kanone</trd>
  </trdgrp>
  <trd lng="en">cannon</trd>
  <trd lng="es">ca&ntilde;&oacute;n</trd>
  <trd lng="fr">canon</trd>
  <trd lng="hu">l&ouml;veg</trd>
  <trd lng="id">meriam</trd>
  <trd lng="nl">kanon</trd>
  <trd lng="no">kanon</trd>
  <trdgrp lng="pl">
    <trd>armata</trd>,
    <trd>dzia&lstroke;o</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_r;&c_u;&c_d;&c_i;&c_je; <klr tip="amb">(&c_a;&c_r;&c_t;.)</klr></trd>
</drv>

<drv mrk="paf.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="paf.0isto.KOMUNE">
    <dif>
      Sportisto a&ubreve; milisto (precipe infanteriano)
      kies fako estas cel<tld/>i:
      <ekz>
        <ind>ark<tld/>isto</ind>
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 21:20</lok>
        </fnt>;
        <trd lng="de">Bogensch&uuml;tze</trd>
      </ekz>
      <ekz>
        <ind>ka&scirc;<tld/>isto</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2003/008191.html">Kompatindaj
          irakanoj</url></vrk>
        </fnt>;
        <trd lng="de">Heckensch&uuml;tze</trd>
        <trd lng="en">sniper</trd>
      </ekz>
      <ekz>
        <ind>sport<tld/>isto</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2009/009838.php">Kia
          konsekvenco!</url></vrk>
        </fnt>;
        <trd lng="de">Sportsch&uuml;tze</trd>
      </ekz>
      <ekz>
        la <tld/>istoj <tld/>is sur viajn servantojn de sur la muro
        <fnt><bib>MT</bib><lok>&Sa2; 11:24</lok></fnt>;
      </ekz>
      <ekz>
        la plej bonaj <tld/>istoj &ccirc;iam celas al la radiaj
        celiloj kaj distanciloj de la malamikaj <tld/>ilegoj
        <fnt>
          <aut>E. Rice Borroughs, trad. K. R. C. Sturmer</aut>
          <vrk>Princino de Marso</vrk>
          <lok>1938</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_t;&c_r;&c_a;&c_l;&c_o;&c_k;</trd>,
      <trd>&c_s;&c_t;&c_r;&c_a;&c_l;&c_je;&c_c;</trd>
    </trdgrp>
    <trd lng="ca">tirador</trd>
    <trdgrp lng="en">
      <trd>marksman</trd>
    </trdgrp>
    <trd lng="fr">tireur</trd>
    <trdgrp lng="he">
      <trd>&qof;&lamed;&ayin;</trd>,
      <trd>&tsadi;&lamed;&fpe;</trd>
    </trdgrp>
    <trd lng="hu">l&ouml;v&eacute;sz</trd>
    <trd lng="pl">strzelec</trd>
    <trd lng="ru">&c_s;&c_t;&c_r;&c_je;&c_l;&c_o;&c_k;</trd>
  </snc>
  <trd lng="bg">&c_s;&c_t;&c_r;&c_je;&c_l;&c_je;&c_c;</trd>
  <trd lng="cs">st&rcaron;elec</trd>
  <trd lng="de">Sch&uuml;tze</trd>
  <trd lng="id"><baz>tembak</baz>penembak jitu</trd>
  <trd lng="sk">strelec</trd>
</drv>

