<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: paf.xml,v 1.153 2022/11/21 14:29:48 revo Exp $">
<kap>
  <ofc>*</ofc><rad>paf</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="paf.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="paf.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="jxet.0i.tra_aero">&Jcirc;eti</ref>
      a&jcirc;on tre
      rapide al iu celo a&ubreve; direkto:
      <ekz>
        <tld/>i &scirc;tonon per &scirc;ton&jcirc;etilo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        francino, <tld/>ante per la ridantaj okuloj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; kiu plej bone <tld/>as, tamen iam maltrafas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <subsnc mrk="paf.0i.ARM">
      <uzo tip="fak">MIL</uzo>
      <dif>
        &Jcirc;eti objekton per rapidiga ilo, celante detrui, vundi
        a&ubreve; mortigi:
        <ekz>
          <tld/>i kuglon per karabeno, obuson per kanono;
        </ekz>
        <ekz>
          li <tld/>&jcirc;etis la
          sagon<fnt><bib>MT</bib><lok>&Sa1;
          20:36</lok></fnt>;
        </ekz>
        <ekz>
          hodia&ubreve; vi <tld/>os vian prov<tld/>on, por ke mi povu proklami
          vin &ccirc;asistoj
          <fnt>
            <bib>ElektFab</bib>
            <lok>Du fratoj</lok>
          </fnt>;
        </ekz>
        <ekz>
          mi blinde <tld/>os, eble trafos
          <fnt>
            <bib>PrV</bib>
          </fnt>;
        </ekz>
        <ekz>
          mi <tld/>os preter &gcirc;in tri sagojn, kvaza&ubreve; <tld/>ante al celo
          <fnt>
            <bib>MT</bib>
            <lok>I. Samuel 20:20</lok>
          </fnt>;
        </ekz>
        <ekz>
          <tld/>i birdon, al birdo;
        </ekz>
        <ekz>
          <tld/>i kontra&ubreve; iu;
        </ekz>
        <ekz>
          <uzo tip="stl">FIG</uzo>
          <ind>tra<tld/>adi la aeron </ind><klr>(vane labori)</klr>
          <trd lng="ca">esfor&ccedil;ar-se en va</trd>
          <fnt><bib>PrV</bib></fnt>.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>abfeuern</trd>,
        <trd>feuern</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="paf.0i.SPO">
      <uzo tip="fak">SPO</uzo>
      <dif>
        &Jcirc;etegi pilkon a&ubreve; alian ludobjekton al cela&jcirc;o:
        <ekz>
          la kroata atakisto &ccirc;iam <tld/>as la pilkon al la
          golgardisto
          <fnt>
            <aut>-</aut>,
            <vrk><url ref="http://www.esperanto.ch/komentoj/sxercoj.html">komentoj</url></vrk>,
            <lok>2006</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="cel.0i">celi</ref>,
      <ref cel="paf.for0i">forpafi</ref>,
      <ref cel="sxot.0i">&scirc;oti</ref>,
      <ref cel="traf.0i">trafi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_r;&c_a;&c_l;&c_ja;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_s;&c_t;&c_r;&c_je;&c_l;&c_ja;&c_m;</trd>
  <trd lng="bo">&t_ma;&t_da;&t_-a;&t_tsheg;&t_ra;&t_gata;&t_yata;&t_ga;&t_tsheg;</trd>
  <trdgrp lng="ca">
    <trd>llan&ccedil;ar</trd>,
    <trd>disparar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>st&rcaron;elit</trd>,
    <trd>st&rcaron;&iacute;let</trd>,
    <trd>vyst&rcaron;elit</trd>,
    <trd>zast&rcaron;elit</trd>
  </trdgrp>
  <trd lng="de">schie&szlig;en</trd>
  <trd lng="en">shoot</trd>
  <trd lng="es">disparar</trd>
  <trd lng="fr">tirer<klr tip="amb"> (un projectile)</klr></trd>
  <trd lng="he">&lamed;&yod;&resh;&vav;&tav;
  \<ind>&yod;&resh;&he;</ind>\</trd>
  <trd lng="hu">l&odblac;</trd>
  <trd lng="id"><baz>tembak</baz>menembak</trd>
  <trd lng="nl">schieten</trd>
  <trd lng="no">skyte</trd>
  <trd lng="pl">strzela&cacute;</trd>
  <trdgrp lng="pt">
    <trd>atirar</trd>
  </trdgrp>
  <trd lng="ro">trage</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_je;&c_l;&c_ja;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>streli&tcaron;</trd>,
    <trd>vystreli&tcaron;</trd>,
    <trd>zastreli&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">skjuta</trd>
  <trd lng="uk">&c_s;&c_t;&c_r;&c_ib;&c_l;&c_ja;&c_t;&c_i;</trd>
</drv>

<drv mrk="paf.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="paf.0.SON">
    <dif>
      <ref tip="lst" cel="bru.0o" lst="voko:sonoj">Bruo</ref>
      de <tld/>o, de eksplodo, de frakaso:
      <ekz>
        la oficiro ekcelas la trian fojon &mdash; <tld/>! li trafis
        <fnt><bib>FK</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="krak.0">krak</ref>
    </refgrp>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_b;&c_u;&c_m;</trd>,
    <trd>&c_b;&c_a;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>pam!</trd>,
    <trd>pum!</trd>
  </trdgrp>
  <trd lng="de">Peng!</trd>
  <trd lng="en">bang<klr> (shooting sound)</klr></trd>
  <trdgrp lng="fr">
    <trd>boum !</trd>,
    <trd>paf !</trd>
  </trdgrp>
  <trd lng="id"><ind>dor</ind>!</trd>
  <trdgrp lng="pl">
    <trd>pif! paf!</trd>,
    <trd>paf!</trd>
  </trdgrp>
  <trd lng="ro">PIF! Paf! paf! paf!</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_i;&c_f;!</trd>,
    <trd>&c_p;&c_a;&c_f;!</trd>,
    <trd>&c_p;&c_i;&c_f; &c_p;&c_a;&c_f;!</trd>
  </trdgrp>
</drv>

<drv mrk="paf.0o">
  <kap><tld/>o</kap>
  <snc mrk="paf.0o.ARM">
    <dif>
      Ago <tld/>i a&ubreve; rezulto de tiu ago:
      <ekz>
        mi a&ubreve;dis du <tld/>ojn<fnt><bib>F</bib>, <lok>Ekzercaro,
        &para; 40</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>o el <tld/>arko
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 21:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de la <tld/>ilego
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        ni eka&ubreve;dis <tld/>ojn kaj tuj poste aliajn krakojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        zumis la kugleta&jcirc;o kaj krakis <tld/>o post <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu &gcirc;emo sonis kiel malgranda <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Abio</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_r;&c_e;&c_l;</trd>
  <trdgrp lng="bg">
    <trd>&c_s;&c_t;&c_r;&c_je;&c_l;&c_b;&c_a;</trd>,
    <trd>&c_i;&c_z;&c_s;&c_t;&c_r;&c_je;&c_l;</trd>
  </trdgrp>
  <trd lng="ca">tret</trd>
  <trdgrp lng="cs">
    <trd>r&aacute;na</trd>,
    <trd>v&yacute;st&rcaron;el</trd>
  </trdgrp>
  <trd lng="de">Schuss</trd>
  <trd lng="en">shot</trd>
  <trd lng="es">disparo</trd>
  <trdgrp lng="fr">
    <trd>coup de feu</trd>,
    <trd>tir</trd>
  </trdgrp>
  <trd lng="he">&yod;&resh;&yod;&yod;&he;</trd>
  <trd lng="hu">l&oacute;v&eacute;s</trd>
  <trd lng="id"><ind>tembak</ind>an</trd>
  <trd lng="nl">schot</trd>
  <trd lng="no">skudd</trd>
  <trd lng="pl">strza&lstroke;</trd>
  <trd lng="ro">&#537;ut</trd>
  <trd lng="ru">&c_v;&c_y;&c_s;&c_t;&c_r;&c_je;&c_l;</trd>
  <trd lng="sk">stre&lcaron;ba</trd>
  <trd lng="sv">skott</trd>
  <trd lng="uk">&c_z; &c_p;&c_je;&c_r;&c_sh;&c_o;&c_g;&c_o; &c_p;&c_o;&c_s;&c_t;&c_r;&c_ib;&c_l;&c_u;</trd>
</drv>

<drv mrk="paf.0ado">
  <kap><tld/>aro, <var><kap><ofc>*</ofc><tld/>ado
    <fnt><bib>F</bib>, <lok>Universala Vortaro,
    -ad'</lok></fnt></kap></var></kap>
  <snc mrk="paf.0ado.ARM">
    <dif>
      Ripetitaj <tld/>oj:
      <ekz>
        la <tld/>ado da&ubreve;ris tre longe
        <fnt><bib>F</bib>, <lok>Ekzercaro, &para;40</lok></fnt>;
      </ekz>
      <ekz>
        la afero fini&gcirc;is, kiel <ind>la <tld/>ado &ccirc;e Hornbergo</ind>, kaj ili devis
        foriri kun longa nazo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
        <trd lng="de">das <ind>Hornberg</ind>er Schie&szlig;en</trd>
      </ekz>
      <ekz>
        kia <tld/>aro! <klr>[&#8230;]</klr> unu &mdash; du &mdash; tri
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        saluta <tld/>aro.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_l;&c_p;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_mol;&c_b;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_l;&c_mol;&c_b;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_l;&c_ja;&c_n;&c_ib;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_s;&c_t;&c_r;&c_je;&c_l;&c_b;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_je;&c_l;&c_ja;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>r&agrave;fega</trd>,
    <trd>tiroteig</trd>,
    <trd>salva</trd>
  </trdgrp>
  <trd lng="cs">salva</trd>
  <trdgrp lng="de">
    <trd>Schie&szlig;en</trd>,
    <trd>Salve</trd>
  </trdgrp>
  <trd lng="en">shooting</trd>
  <trdgrp lng="fr">
    <trd>fusillade</trd>,
    <trd>rafale</trd>,
    <trd>salve<klr tip="ind"> (de tirs)</klr></trd>
  </trdgrp>
  <trd lng="he">&yod;&resh;&yod;</trd>
  <trd lng="hu">l&ouml;v&ouml;ld&ouml;z&eacute;s</trd>
  <trd lng="id">salvo</trd>
  <trd lng="nl">salvo</trd>
  <trd lng="no">salve</trd>
  <trdgrp lng="pl">
    <trd>salwa</trd>,
    <trd>palba</trd>,
    <trd>kanonada</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>salv&abreve;</trd>,
    <trd>foc</trd>,
    <trd>canonad&abreve;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_r;&c_je;&c_l;&c_mol;&c_b;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_l;&c_p;</trd>
  </trdgrp>
  <trd lng="sk">salva</trd>
  <trd lng="sv">skutande</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_r;&c_ib;&c_l;&c_mol;&c_b;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_ib;&c_l;&c_ja;&c_n;&c_n;&c_ja;</trd>,
    <trd><klr>(&c_b;&c_je;&c_z;&c_p;&c_je;&c_r;&c_je;&c_r;&c_v;&c_n;&c_a; &c_ib; &c_b;&c_je;&c_z;&c_l;&c_a;&c_d;&c_n;&c_a; &c_shch;&c_je;)</klr> &c_s;&c_t;&c_r;&c_ib;&c_l;&c_ja;&c_n;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="paf.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="paf.0ajxo.ARM">
    <dif>
      &Ccirc;io ajn, kion oni <tld/>as per <tld/>ilo:
      <ekz>la <tld/>a&jcirc;o eksplode atingis la celon
       <fnt>
         <aut>E. Rice Borroughs, trad. K. R. C. Sturmer</aut>
         <vrk>Princino de Marso</vrk>
         <lok>1938</lok>
       </fnt>;
      </ekz>
      <ekz>la lastan <tld/>a&jcirc;on mi el<tld/>is, kiel vi scias, al
      la
      korniko
        <fnt>
          <aut>B. N&ecaron;mcov&aacute;, trad. V. Tobek kaj K.
          Proch&aacute;zka</aut>
          <vrk>Avinjo</vrk>
          <lok>&ccirc;apitro 15a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="kugl.0o">kuglo</ref>,
      <ref cel="obus.0o">obuso</ref>,
      <ref cel="sag.0o">sago</ref>,
      <ref cel="sxton.0o">&scirc;tono</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_n;&c_a;&c_r;&c_a;&c_d;</trd>
  <trd lng="bg">&c_s;&c_n;&c_a;&c_r;&c_ja;&c_d;</trd>
  <trd lng="ca">projectil</trd>
  <trdgrp lng="en">
    <trd>projectile</trd>,
    <trd>ammunition</trd>
  </trdgrp>
  <trd lng="fr">projectile</trd>
  <trdgrp lng="de">
    <trd>Geschoss</trd>,
    <trd>Projektil</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&qof;&lamed;&yod;&ayin;</trd>,
    <trd>&tet;&yod;&lamed;</trd>
  </trdgrp>
  <trd lng="hu">l&ouml;ved&eacute;k</trd>
  <trd lng="id">proyektil</trd>
  <trd lng="nl">projectiel</trd>
  <trd lng="pl">pocisk</trd>
  <trd lng="ro">proiectil</trd>
  <trd lng="ru">&c_s;&c_n;&c_a;&c_r;&c_ja;&c_d;</trd>
  <trdgrp lng="sv">
    <trd>skott</trd>,
    <trd>projektil</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_n;&c_a;&c_r;&c_ja;&c_d;</trd>,
    <trd>&c_k;&c_u;&c_l;&c_ja;</trd>,
    <trd>&c_ja;&c_d;&c_r;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="paf.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="paf.0ejo.ARM">
    <dif>
      Loko, kie oni sin ekzercas <tld/>adon:
      <ekz>
        la <tld/>ejo finis oriente per kruta muro
        <fnt>
          <aut>H. Vallienne</aut>
          <vrk>Kastelo de prelongo</vrk>
          <lok>1907</lok>
        </fnt>;
      </ekz>
      <ekz>
        foje li estis en kermesa <tld/>ejo
        <fnt>
          <aut>J. Ha&scaron;ek, trad. V. V&aacute;&ncaron;a</aut>
          <vrk>La aventuroj de la brava soldato &Scirc;vejk dum la
          mondmilito</vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_ib;&c_r;</trd>
  <trd lng="bg">&c_s;&c_t;&c_r;&c_je;&c_l;&c_b;&c_i;&c_shch;&c_je;</trd>
  <trdgrp lng="ca">
    <trd>camp de tir</trd>,
    <trd>sala de tir</trd>
  </trdgrp>
  <trd lng="cs">st&rcaron;elnice</trd>
  <trd lng="de">Schie&szlig;stand</trd>
  <trdgrp lng="en">
    <trd>firing range</trd>,
    <trd>shooting range</trd>
  </trdgrp>
  <trd lng="fr">stand de tir</trd>
  <trd lng="he">&mem;&tet;&vav;&vav;&het;</trd>
  <trd lng="hu">l&odblac;t&eacute;r</trd>
  <trd lng="nl">schietstand</trd>
  <trd lng="pl">strzelnica</trd>
  <trd lng="ro">poligon de tragere</trd>
  <trd lng="ru">&c_t;&c_i;&c_r;</trd>
  <trd lng="sk">strelnica</trd>
  <trdgrp lng="sv">
    <trd>skjutf&auml;lt</trd>,
    <trd>skjutbana</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_i;&c_r;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_ib;&c_l;&c_mol;&c_b;&c_i;&c_shch;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="paf.0igxi">
  <kap><tld/>i&gcirc;i,
  <var><kap>sin <tld/>i</kap></var></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="paf.0igxi.KOMUNE">
    <dif>
      &Jcirc;eti&gcirc;i rapidege:
      <ekz>
        raketoj <tld/>i&gcirc;as alten;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li <tld/>is sin for <klr>(subite malaperis)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        docento de botaniko, kiu, dum klasekskurso en arbaro,
        ekmalaperas kaj trovi&gcirc;as
        morta pro <tld/>i&gcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2007/009132.php">Nova
          krimromano el A&ubreve;stralio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="paf.for0igxi">forpafi&gcirc;i, forpafi sin</ref>,
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_r;&c_e;&c_l;&c_ib;&c_c;&c_mol; &c_u; &c_s;&c_ja;&c_b;&c_je;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_r;&c_e;&c_l;&c_ib;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_i;&c_z;&c_s;&c_t;&c_r;&c_je;&c_l;&c_v;&c_a;&c_m;
  &c_s;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>st&rcaron;elit</trd>,
    <trd>st&rcaron;&iacute;let</trd>,
    <trd>vyst&rcaron;elit</trd>,
    <trd>zast&rcaron;elit</trd>
  </trdgrp>
  <trdgrp lng="en">
