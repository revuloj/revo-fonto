  <art mrk="$Id$">
    <kap>
      <rad>tek</rad>/o
    </kap>
    <drv mrk="tek.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">EKON</uzo>
      <snc>
 <dif>
   <ref tip="dif" cel="paper.0ujo.subbraka">Paperujo<sncref/></ref>,
   ofte kun tenilo a&ubreve; rimeno kaj seruro:
   <ekz>
     Nudokrudov forigis la sigelon de la pordo de la kabineto, prenis 
     el sia <tld/>o faldmezurilon kaj pa&scirc;is en la kabineton
     <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 9a</lok></fnt>..
   </ekz>
 </dif>
      </snc>
      <trdgrp lng="be">
 <trd>&c_t;&c_e;&c_ch;&c_k;&c_a;</trd>, 
 <trd>&c_p;&c_a;&c_p;&c_k;&c_a;</trd>, 
 <trd>&c_p;&c_a;&c_r;&c_t;&c_f;&c_e;&c_l;&c_mol;</trd>
      </trdgrp>  
      <trd lng="de">Aktentasche</trd>
      <trd lng="en">briefcase</trd>
      <trdgrp lng="hu">
        <trd>aktat&aacute;ska</trd>,
        <trd>iratt&aacute;rca</trd>
      </trdgrp>
      <trd lng="nl">aktentas</trd>
  <trdgrp lng="pt">
    <trd>pasta</trd>,
    <trd>maleta</trd>,
    <trd>valise</trd>,
  </trdgrp>
      <trdgrp lng="ru">
 <trd>&c_p;&c_a;&c_p;&c_k;&c_a;</trd>,
 <trd>&c_p;&c_o;&c_r;&c_t;&c_f;&c_je;&c_l;&c_mol;</trd>
      </trdgrp>
    </drv>
  </art>
