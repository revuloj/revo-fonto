<drv mrk="klopod.0o">
  <kap><tld/>o</kap>
  <snc mrk="klopod.0o.peno">
    <dif>
      Zorga kaj <ref cel="pen.0o">pena</ref> agado por realigi entreprenon kaj venki eblajn
      kontra&ubreve;a&jcirc;ojn:
      <ekz>
        ni kun granda <tld/>o apena&ubreve; povis savi la boaton
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 27:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estas en <tld/>oj de l' kapo &gcirc;is piedoj
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Bem&uuml;hung</trd>,
      <trd>Anstrengung</trd>,
      <trd>Bestrebung</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>igyekv&eacute;s</trd>,
      <trd>t&ouml;rekv&eacute;s</trd>
    </trdgrp>
  </snc>
  <snc mrk="klopod.0o.zorgo">
    <dif>
      Maltrankviliga pri<ref cel="zorg.0o">zorgo</ref>, maltrankviletigo:
      <ekz>
        mi havis multe da <tld/>oj kun la idoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili havis grandan deziron preni sur sin la <tld/>ojn de voja&gcirc;o eksterlanden
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        tute vane vi faras al vi multe da <tld/>oj pri la uzado
        de la artikolo
        <fnt>
          <bib>LR</bib>
          <lok>Dua Parto &ndash; Gramatiko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la duono de iliaj trezoroj savus la faraonon el la sen&ccirc;esaj <tld/>oj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[k]</klr>iom da respondeco, tiom da <tld/>oj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>ne faru al vi la <tld/>on</ind>, mi trovos mem la straton;
        <trd lng="de">mach dir nicht die <ind>M&uuml;he</ind></trd>
        <trdgrp lng="es">
          <trd>no te tomes la <ind>molestia</ind></trd>,
          <trd>no te <ind>molestes</ind></trd>
        </trdgrp>
        <trd lng="fr">ne vous en <ind>faites</ind> pas</trd>
      </ekz>
      <ekz>
        mankis al Petro <tld/>oj, li a&ccirc;etis al si domon
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        pli da havo, pli da <tld/>oj
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="hu">gond</trd>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_o;&c_p;&c_a;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>&uacute;sil&iacute;</trd>,
    <trd>lopota</trd>,
    <trd>n&aacute;maha</trd>,
    <trd>p&rcaron;i&ccaron;in&ecaron;n&iacute;</trd>,
    <trd>snaha</trd>,
    <trd>starost</trd>
  </trdgrp>
  <trd lng="de">M&uuml;he</trd>
  <trdgrp lng="es">
     <trd>af&aacute;n</trd>,
     <trd>esfuerzo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;marche</trd>,
    <trd>agitation</trd>,
    <trd>formalit&eacute;</trd>,
    <trd>soin<klr tip="ind"> (souci)</klr></trd>,
    <trd>souci</trd>,
    <trd>tracas</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5C3D;&#x529B;  <pr>&#x3058;&#x3093;&#x308A;&#x3087;&#x304F;</pr></trd>,
    <trd>&#x5954;&#x8D70;  <pr>&#x307B;&#x3093;&#x305D;&#x3046;</pr></trd>,
    <trd>&#x9AA8;&#x6298;&#x308A;  <pr>&#x307B;&#x306D;&#x304A;&#x308A;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>poging</trd>,
    <trd>moeite</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>baczenie</trd>,
    <trd>fatyga</trd>,
    <trd>staranie</trd>,
    <trd>zabieg</trd>
  </trdgrp>
  <trd lng="pt">esfor&ccedil;o</trd>
  <trdgrp lng="ro">
    <trd>aten&#x21B;ie</trd>,
    <trd>durere</trd>,
    <trd>efort</trd>
  </trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_h;&c_l;&c_o;&c_p;&c_o;&c_t;&c_y;</trd>,
    <trd>&c_z;&c_a;&c_b;&c_o;&c_t;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dravos&tcaron;</trd>,
    <trd>drav&aacute; t&uacute;&zcaron;ba</trd>,
    <trd>n&aacute;por</trd>,
    <trd>&uacute;silie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_k;&c_l;&c_o;&c_p;&c_ib;&c_t;</trd>,
    <trd>&c_k;&c_l;&c_o;&c_p;&c_o;&c_t;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_t;&c_u;&c_r;&c_b;&c_o;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_ib;&c_k;&c_l;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_r;&c_a;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x56F0;&#x96BE; <pr>k&#xF9;nnan</pr></trd>,
    <trd>&#x8270;&#x96BE; <pr>ji&#x101;nn&#xE1;n</pr></trd>
  </trdgrp>
</drv>


<drv mrk="klopod.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="klopod.0ema.KOMUNE">
    <dif>
      Kiu <tld/>as, penas, provadas por atingi celojn:
      <ekz>
        li estas honesta kaj <tld/>ema
        <fnt>
          <aut>B. N&ecaron;mcov&aacute;, trad. V. Tobek kaj K. Proch&aacute;zka</aut>,
          <vrk><url ref="https://eo.wikisource.org/wiki/Avineto/VII.">
          Avineto</url></vrk>,
          <lok>[sen dato]</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>emaj perdrikoj per impetaj ekflugoj gajigas
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 7 (105)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dilige.0a">diligenta</ref>,
      <ref cel="labor.0ema">laborema</ref>,
      <ref cel="zorg.0ema">zorgema</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_l;&c_a;&c_p;&c_a;&c_t;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_r;&c_a;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_d;&c_b;&c_a;&c_j;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>bem&uuml;ht</trd>,
    <trd>flei&szlig;ig</trd>,
    <trd>arbeitsam</trd>,
    <trd>f&uuml;rsorglich</trd>
  </trdgrp>
  <trd lng="fr">entreprenant</trd>
