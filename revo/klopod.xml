<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: klopod.xml,v 1.33 2023/12/03 09:58:51 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>klopod</rad>/i <fnt><bib>PV</bib></fnt> 
</kap>
<drv mrk="klopod.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="klopod.0i.KOMUNE">
    <dif>
      Uzi sian kapablon kaj energion por efektivigi ion per
      &ccirc;iuspecaj provoj:
      <ekz>
        &scirc;i iris, por <tld/>i &ccirc;e la re&gcirc;o pri sia domo kaj pri sia kampo
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 8:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum vi iradas kun via kontra&ubreve;ulo <klr>[&#x2026;]</klr>, survoje <tld/>u liberi&gcirc;i de li
        <fnt>
          <bib>NT</bib>
          <lok>Luko 12:58</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u veni balda&ubreve; al mi
        <fnt>
          <bib>NT</bib>
          <lok>II. Timoteo 4:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi neniel povis elirigi ilin, kiom ajn mi <tld/>is kaj penis, admonis kaj helpis
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>!
      </ekz>
      <ekz>
        &scirc;i komencis <tld/>i pri ordigo de la objektoj, elprenitaj el la paka&jcirc;etoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        neniu <tld/>is por instigi lin al la rompo de lia fra&ubreve;leco
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu senkorulo <tld/>as per siaj tutaj fortoj por min ruinigi
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sepa</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i pri &ccirc;ies favoro estas pleje malsa&gcirc;a laboro
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu <tld/>u nur en sia metio, tiam al la urbo mankos nenio
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pen.0i">peni</ref>,
      <ref cel="prov.0i.peni_sukcese_fari">provi<sncref/></ref>,
      <ref cel="streb.0i.KOMUNE">strebi</ref>
    </refgrp>
  </snc>
    <rim>
      <ref cel="cel.0i.por_atingi">Celi</ref>, <ref cel="okup.0igxi">okupi&gcirc;i</ref>, 
      klopodi, <ref cel="zorg.0i">zorgi</ref> estas sencoparencaj,
      sed <em>celi</em> insistas pri la deziro al la atingota&jcirc;o,
      <em>klopodi</em> pri la eksteraj agoj, pa&scirc;oj kaj provoj necesaj
      por sukcesi, <em>okupi&gcirc;i</em> pri la stre&ccirc;o de la atento kaj
      la absorbi&gcirc;o de la spirito, <em>zorgi</em> pri la maltrankvileta
      atento kaj la prirespondeco koncerne la farota&jcirc;on.
    </rim>
  <trdgrp lng="be">
    <trd>&c_k;&c_l;&c_a;&c_p;&c_a;&c_c;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_m;&c_je;&c_c;&c_mol; <ind>&c_k;&c_l;&c_o;&c_p;&c_a;&c_t;</ind></trd>,
    <trd>&c_u;&c_v;&c_ib;&c_h;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sich <ind>bem&uuml;hen</ind></trd>,
    <trd>sich <ind>M&uuml;he</ind> geben</trd>,
    <trd>bem&uuml;ht sein</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>undertake</trd>,
    <trd>try<klr> (undertake; take steps to do something)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
     <trd>esforzarse</trd>,
     <trd>tratar de</trd>,
     <trd>intentar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>agiter</ind><klr tip="ind"> (autour de qc ou
    qn)</klr></trd>,
    <trd>se <ind>battre</ind><klr tip="ind"> (faire des
    d&eacute;marches)</klr></trd>,
    <trd>se <ind>d&eacute;mener</ind><klr tip="ind"> (faire des
    d&eacute;marches)</klr></trd>,
    <trd>se <ind>pr&eacute;occuper</ind><klr tip="ind"> (s'occuper s&eacute;rieusement
    de qc)</klr></trd>,
    <trd>se <ind>remuer</ind><klr tip="ind"> (faire des
    d&eacute;marches)</klr></trd>,
    <trd>faire des <ind>d&eacute;marche</ind>s</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>igyekszik</trd>,
    <trd>t&ouml;rekszik</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5954;&#x8D70;&#x3059;&#x308B;  <pr>&#x307B;&#x3093;&#x305D;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5C3D;&#x529B;&#x3059;&#x308B;  <pr>&#x3058;&#x3093;&#x308A;&#x3087;&#x304F;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x4E16;&#x8A71;&#x3092;&#x713C;&#x304F;  <pr>&#x305B;&#x308F;&#x3092;&#x3084;&#x304F;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>pogen</trd>,
    <trd>trachten</trd>,
    <trd>streven</trd>,
    <trd>moeite doen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>baczy&cacute;</trd>,
    <trd>dba&cacute;</trd>,
    <trd>fatygowa&cacute; si&eogonek;</trd>,
    <trd>k&lstroke;opota&cacute; si&eogonek;</trd>,
    <trd>stara&cacute; si&eogonek;</trd>,
    <trd>zabiega&cacute;</trd>,
    <trd>zatroszczy&cacute; si&eogonek;</trd>
  </trdgrp>   
  <trd lng="pt">esfor&ccedil;ar-se</trd>
  <trdgrp lng="ro">
    <trd>s&abreve; aib&abreve; <ind>grij&abreve;</ind></trd>,
    <trd>ave&#x21B;i <ind>grij&abreve;</ind></trd>,
    <trd>&icirc;ncerca</trd>,
    <trd>s&abreve; se <ind>&icirc;ngrijeasc&abreve;</ind></trd>
  </trdgrp>   
  <trd lng="ru">&c_h;&c_l;&c_o;&c_p;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>dom&aacute;ha&tcaron; sa</trd>,
    <trd>presadzova&tcaron;</trd>,
    <trd>usilova&tcaron; sa</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dom&aacute;hat se</trd>,
    <trd>prosazovat</trd>,
    <trd>usilovat</trd>,
    <trd>usilovat o</trd>,
    <trd>zasazovat se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sich <ind>bem&uuml;hen</ind></trd>,
    <trd>sich <ind>M&uuml;he</ind> geben</trd>,
    <trd><ind>bem&uuml;ht</ind> sein</trd>,
    <trd>sich <ind>bem&uuml;hen</ind></trd>,
    <trd>sich <ind>M&uuml;he</ind> geben</trd>,
    <trd>bem&uuml;ht sein</trd>,
    <trd>sich <ind>anstrengen</ind></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_k;&c_l;&c_o;&c_p;&c_o;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_k;&c_l;&c_o;&c_p;&c_o;&c_t;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_ib;&c_k;&c_l;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_t;&c_u;&c_r;&c_b;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_r;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6323;&#x624E; <pr>zh&#x113;ngzh&#xE1;</pr></trd>,
    <trd>&#x5927;&#x8CBB;&#x5468;&#x7AE0; <pr>d&#xE0;f&#xE8;izh&#x14D;uzh&#x101;ng</pr></trd>
  </trdgrp>
</drv>


<drv mrk="klopod.0a">
  <kap><tld/>a</kap>
  <snc mrk="klopod.0a.klopodiga">
    <dif>
      Haviganta <tld/>ojn:
      <ekz>
        <tld/>a laboro;
      </ekz>
      <ekz>
        &ccirc;u ni povas akcepti servistinon kun infano? <klr>[&#x2026;]</klr> kun infano oni ne
        povas, tio estus tro multekosta kaj <tld/>a
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_a;&c_p;&c_o;&c_t;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>nam&aacute;hav&yacute;</trd>,
    <trd>n&aacute;ro&ccaron;n&yacute;</trd>,
    <trd>obt&iacute;&zcaron;n&yacute;</trd>,
    <trd>pracn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>m&uuml;hsam</trd>,
    <trd>anstrengend</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>inc&oacute;modo</trd>,
    <trd>molesto</trd>
  </trdgrp>
  <trd lng="fr">qui cause des <ind>tracas</ind></trd>
  <trd lng="hu">f&aacute;rads&aacute;gos</trd>
  <trd lng="ja">&#x9AA8;&#x306E;&#x6298;&#x308C;&#x308B;  <pr>&#x3053;&#x3064;&#x306E;&#x304A;&#x308C;&#x308B;</pr></trd>
