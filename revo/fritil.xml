<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>fritilari</rad>/o
</kap>
<drv mrk="fritil.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="liliac.0oj">liliacoj</ref>
      (<trd lng="la">Fritillaria</trd>),
      kies &ccirc;efa specio estas la t. n. imperiestra krono.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Kaiserkrone</trd>,
    <trd>Schachbrettblume</trd> k.a.
  </trdgrp>
  <trd lng="fr">fritillaire</trd>
  <trd lng="ru">&c_r;&c_ja;&c_b;&c_ch;&c_i;&c_k; <klr>(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="sv">klocklilja</trd>
</drv>
</art>
