<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fritil.xml,v 1.19 2023/08/31 13:11:31 revo Exp $">
<kap><rad>fritilari</rad>/o</kap>
<drv mrk="fritil.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="liliac.0oj">liliacoj</ref>
      (<trd lng="la">Fritillaria</trd>),
      kies &ccirc;efa specio estas la t. n. imperiestra krono:
      <ekz>
        Krasulfolia fritilario <klr>[&#x2026;]</klr> estas multjara, herba cepoplanto, kiu
        atingas kreskoalton de 6 &gcirc;is 20 cm, plej ofte estas kvar <klr>[&#x2026;]</klr>
        folioj po planto, la supra folioj estas liniformaj, la malsupraj ov- &gcirc;is
        lancetformaj, ekzistas unu &gcirc;is tri floroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=514070">Krasulfolia fritilario</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/b/b3/Kievitsbloem_(Fritillaria_meleagris)_02.JPG/360px-Kievitsbloem_(Fritillaria_meleagris)_02.JPG" prm="CC BY-SA 3.0">Meleagra a&ubreve; &Scirc;aka <tld/>o (Fritillaria meleagris)
        <fnt>
          <aut>Dominicus Johannes Bergsma, propra verko</aut>
          <lok><url ref="&WCW;/File:Kievitsbloem_(Fritillaria_meleagris)_02.JPG">&WCW;/File:Kievitsbloem_(Fritillaria_meleagris)_02.JPG</url></lok>
        </fnt>
    </bld>
    <bld lok="&WCU;/thumb/d/db/Fritillaria_imperialis_orange.JPG/360px-Fritillaria_imperialis_orange.JPG" prm="CC BY-SA 3.0">imperiestra krono (Fritillaria imperialis), Baden-Virtembergo
        <fnt>
          <aut>4028mdk09, propra verko</aut>
          <lok><url ref="&WCW;/File:Fritillaria_imperialis_orange.JPG">&WCW;/File:Fritillaria_imperialis_orange.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_b;&c_ch;&c_y;&c_k; <klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trdgrp lng="de">
    <trd>Kaiserkrone</trd>,
    <trd>Schachbrettblume</trd> k.a.
  </trdgrp>
  <trd lng="en">fritillary</trd>
  <trd lng="fr">fritillaire</trd>
  <trdgrp lng="hu">
    <trd>kock&aacute;sliliom</trd>,
    <trd>p&uacute;posliliom</trd> k.a.
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30D0;&#x30A4;&#x30E2;</trd>,
    <trd>&#x30E8;&#x30A6;&#x30E9;&#x30AF;&#x30E6;&#x30EA;</trd>
  </trdgrp>
  <trd lng="nl">fritillaria</trd>
  <trdgrp lng="pt">
    <trd>fritil&aacute;ria</trd>,
    <trd>coroa-imperial</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_ja;&c_b;&c_ch;&c_i;&c_k;<klr> (&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="sv">klocklilja</trd>
  <trd lng="uk">&c_r;&#x44F;&c_b;&c_ch;&c_i;&#x43A;</trd>
</drv>
</art>
