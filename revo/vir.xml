<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vir</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vir.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vir.0o.sekso">
    <dif>
      Homo a&ubreve; besto de la sekso organizita por demeti en aliseksan
      animalon la
      semon de ido (kontraste kun ino):
      <ekz>
          Dio kreis la homon <klr>[...]</klr>, en formo de
          <tld/>o kaj <tld/>ino li kreis ilin
          <fnt>
            <bib>MT</bib>
            <lok>Genezo 1:27</lok>
          </fnt>;
        </ekz>
        <ekz>
          Platono dankis la Diojn pro ke ili faris lin homo kaj ne
          besto,
          <tld/>o kaj ne <tld/>ino, greko kaj ne barbaro;
        </ekz>
        <ekz>
          pastrino en tre esceptaj okazoj povas fari&gcirc;i edzino, sed nur
          de <tld/>o de re&gcirc;a sango
          <fnt>
            <bib>Far2</bib>
            <lok>&Ccirc;apitro IX</lok>
          </fnt>.
        </ekz>
        <ekz>
          kvaza&ubreve; bovo kaj bovo<tld/>o
          <fnt>
            <bib>Far1</bib>
            <lok>&Ccirc;apitro XII</lok>
          </fnt>;
        </ekz>
        <ekz>
          <tld/>bovo-tirpovo
          <fnt>
            <bib>Monato</bib>
            <vrk><url
            ref='http://www.esperanto.be/fel/mon/007112.html'>Virbovo-tirpovo</url></vrk>
          </fnt>;
        </ekz>
        <ekz>
          la <tld/>&scirc;afo, kiun vi vidis kun la du kornoj
          <fnt>
            <bib>MT</bib>
            <lok>Daniel 8:20</lok>
          </fnt>;
        </ekz>
        <ekz>
          virbesto kaj ino el &ccirc;iu karno, eniris, kiel ordonis al ili
          Dio, kaj la Eternulo fermis post ili.  
          <fnt>
            <bib>MT</bib>
            <lok>Genezo 7:16</lok>
          </fnt>;
        </ekz>
        <ekz>
          &gcirc;i estas <tld/>seksulo, kaj la malbeleco sekve ne tiom multe
          malutilas
          <fnt>
            <bib>Fab2</bib>
            <lok>Malbela anasido</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_m;&c_u;&c_zh;&c_ch;&c_y;&c_n;&c_a;</trd>
      <trd lng="el">&alfa_ton;&ny;&delta;&rho;&alfa;&sigma_fina;</trd>
      <trd lng="es">hombre</trd>
      <trd lng="fr">homme</trd>
      <trd lng="hu">f&eacute;rfi</trd>
      <trdgrp lng="ku">
         <trd>mirov</trd>,
         <trd>mer&icirc;</trd>,
         <trd>peya</trd>,
         <trd>zilam</trd>
      </trdgrp>
      <trd lng="pl">m&eogonek;&zdot;czyzna</trd>
      <trd lng="pt">homem</trd>
      <trd lng="ru">&c_m;&c_u;&c_zh;&c_ch;&c_i;&c_n;&c_a;</trd>
      <trd lng="sk">mu&zcaron;</trd>
      <trdgrp lng="zh">
        <trd>&#30007;&#20154; [<ind>n&aacute;n r&eacute;n</ind>]</trd>
        <trd>&#30007;&#23376; [<ind>n&aacute;n z&#464;</ind>]</trd>
      </trdgrp>
    </snc>
    <snc mrk="vir.0o.matura">
      <dif>
        Matura&gcirc;a <tld/>seksa homo (kontraste kun infano):
        <ekz>
          li apena&ubreve; eniris la <tld/>econ;
        </ekz>
        <ekz>
          knabo jam <tld/>i&gcirc;anta;
        </ekz>
        <ekz>
          la plej certa rimedo veki <tld/>on el lia ama deliro estas
          &jcirc;eti malbenojn sur lian filon
          <fnt>
            <bib>Far2</bib>
            <lok>&Ccirc;apitro XIV</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_m;&c_u;&c_zh;&c_ch;&c_y;&c_n;&c_a;</trd>
      <trd lng="el">&alfa_ton;&ny;&delta;&rho;&alfa;&sigma_fina;</trd>
      <trd lng="es">hombre</trd>
      <trd lng="fr">homme<klr> (adulte)</klr></trd>
      <trd lng="hu">f&eacute;rfi</trd>
      <trd lng="pl">m&eogonek;&zdot;czyzna</trd>
      <trd lng="pt">homem</trd>
      <trd lng="ru">&c_m;&c_u;&c_zh;&c_ch;&c_i;&c_n;&c_a;</trd>
      <trd lng="sk">mu&zcaron;</trd>
      <trdgrp lng="zh">
        <trd>&#25104;&#24180;&#30007;&#23376; [<ind>ch&eacute;ng
        ni&aacute;n n&aacute;n r&eacute;n</ind>]</trd>
      </trdgrp>
    </snc>
    <trd lng="be">&c_s;&c_a;&c_m;&c_je;&c_c;</trd>
    <trd lng="el">&alfa;&rho;&sigma;&epsilon;&ny;&jota;&kappa;&omikron_ton;</trd>
    <trd lng="cs">mu&zcaron;</trd>
    <trd lng="de">Mann</trd>
    <trd lng="en">man</trd>
    <trd lng="es">macho</trd>
    <trd lng="fr">m&acirc;le<klr> (subst.)</klr></trd>
    <trd lng="fy">man</trd>
    <trd lng="he">&zayin;&kaf;&resh;<klr> (&bet;&ayin;&lamed;
    &het;&yod;&yod;&fmem;)</klr></trd>
    <trd lng="he">&#8503;&bet;&resh;</trd>
    <trdgrp lng="hu">
      <trd>h&iacute;m</trd>,
      <trd>kan</trd>,
    </trdgrp>
    <trd lng="io">viro</trd>
    <trd lng="nl">man</trd>
    <trd lng="pl">samiec</trd>
    <trd lng="pt">macho</trd>
    <trd lng="ru">&c_s;&c_a;&c_m;&c_je;&c_c;</trd>
    <trd lng="sk">samec</trd>
    <trdgrp lng="sk">
      <trd>chlap <klr>(samec)</klr></trd>,
      <trd>mu&zcaron;, chlap <klr>(samec)</klr></trd>
    </trdgrp>
    <trd lng="tp">mije</trd>
    <trdgrp lng="tr">
      <trd>erkek</trd>,
      <trd>adam</trd>
    </trdgrp>
    <trd lng="vo">man</trd>
</drv>

<drv mrk="vir.0a">
  <kap><tld/>a</kap>
  <snc mrk="vir.0a.virula">
    <dif>
      Karakterizanta la <tld/>ojn:
      <ekz>
        <tld/>a organo, birdo, infano, sekso.
      </ekz>
    </dif>
    <subsnc mrk="vir.0a.malina">
      <dif>
        Karakterizanta <tld/>homojn kontraste kun hominoj:
        <ekz>
          <tld/>a viza&gcirc;o;
        </ekz>
        <ekz>
          parenco per <tld/>a deveno;
        </ekz>
        <ekz>
          la <tld/>a potenco super <tld/>inoj;
        </ekz>
        <ekz>
          <tld/>a pronomo, sufikso
          <klr>(uzataj por montri <tld/>ojn)</klr>.
        </ekz>
      </dif>
      <trd lng="es">masculino</trd>
      <trd lng="hu">f&eacute;rfi-</trd>
      <trd lng="pl">m&eogonek;ski</trd>
      <trd lng="sk">mu&zcaron;sk&yacute;</trd>
      <trd lng="zh">&#30007;<klr> [n&aacute;n]</klr></trd>
    </subsnc>
    <subsnc mrk="vir.0a.brava">
      <dif>
        Karakterizanta <ref cel="vir.0o.matura"><tld/>ojn <sncref
        ref="vir.0o.matura"/></ref> kontraste kun hominoj a&ubreve;
        infanoj; kura&gcirc;a, brava:
        <ekz>
          <tld/>a a&gcirc;o, decido, ago;
        </ekz>
        <ekz>
          estu kura&gcirc;aj kaj <tld/>aj, ho
          Fili&scirc;toj
          <fnt>
            <bib>MT</bib>
            <lok>I. Samuel 4:9</lok>
          </fnt>!
        </ekz>
        <ekz>
          vi aspektas tiel <tld/>e maltima
          <fnt>
            <bib>Fab3</bib>
            <lok>Kolumo</lok>
          </fnt>!
        </ekz>
        <ekz>
          <tld/>e kaj nobele konduti.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="hero">heroa</ref>.
      </refgrp>
      <trd lng="hu">f&eacute;rfias</trd>
      <trd lng="pl">m&eogonek;&zdot;ny</trd>
      <trd lng="sk">chlapsk&yacute;</trd>
    </subsnc>
    <trd lng="es">masculino</trd>
    <trdgrp lng="hu">
      <trd>h&iacute;m-</trd>,
      <trd>kan-</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>m&eogonek;ski</trd>,
      <trd>samczy</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>mu&zcaron;sk&yacute;</trd>,
      <trd>sam&ccaron;&iacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="vir.0a.POE">
    <uzo tip="fak">POE</uzo>
    <dif>
      <klr>(p.p. <ref cel="rim.0o.versparto">rimo</ref>)</klr>
      Fini&gcirc;anta per akcenta silabo;
      <klr>(p.p. <ref cel="cezur.0o">cezuro</ref>)</klr>
      sekvanta akcentan silabon:
      <ekz>
        &leftquot;dolor'&rightquot;, &leftquot;kor'&rightquot;
        konsistigas <tld/>ajn rimojn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="in.0a.POE">ina</ref>.
    </refgrp>
    <trd lng="hu">h&iacute;m-</trd>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_zh;&c_ch;&c_y;&c_n;&c_s;&c_k;&c_ib;</trd>
  <trdgrp lng="cs">
    <trd>mu&zcaron;n&yacute;</trd>,
    <trd>mu&zcaron;sk&yacute;</trd>
  </trdgrp>
  <trd lng="de">m&auml;nnlich</trd>
  <trdgrp lng="el">
    <trd>&alfa;&rho;&sigma;&epsilon;&ny;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>,
    <trd>&alfa;&ny;&delta;&rho;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>male</trd>,
    <trd>manly</trd>,
    <trd>unmasculine</trd>
  </trdgrp>
  <trd lng="fr">masculin</trd>
  <trdgrp lng="he">
    <trd>&zayin;&kaf;&resh;&yod;</trd>,
    <trd>&#8503;&bet;&resh;&yod;</trd>
  </trdgrp>
  <trd lng="fy">manlik</trd>
  <trd lng="nl">mannelijk</trd>
  <trd lng="pt">masculino</trd>
  <trd lng="ru">&c_m;&c_u;&c_zh;&c_s;&c_k;&c_o;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>mu&zcaron;sk&yacute;, zmu&zcaron;il&yacute;</trd>,
    <trd>zmu&zcaron;il&yacute;</trd>
  </trdgrp>
  <trd lng="tp">mije</trd>
  <trd lng="vo">manik</trd>
</drv>

<drv mrk="vir.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="vir.0igxi.KOMUNE">
    <dif>
      Atingi la a&gcirc;on, surpreni la aspekton, la situacion de
      plena&gcirc;a
      <tld/>o:
      <ekz>
        la oficiroj, kiuj eliris en la militon preska&ubreve; infanoj,
        revenis,
        <tld/>i&gcirc;intaj de milita aero
        <fnt><bib>Ne&gcirc;aBlovado</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="matur.0igxi">maturi&gcirc;i</ref>,
      <ref cel="agx.plen0a.POL">plena&gcirc;a</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>atteindre l'&acirc;ge d'<ind>homme</ind></trd>,
    <trd>devenir un <ind>homme</ind></trd>
  </trdgrp>
  <trd lng="pl">m&eogonek;&zdot;nie&cacute;</trd>
</drv>

<drv mrk="vir.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="vir.0ilo.AMOR">
    <ref tip="dif" cel="penis.0o.ANA">peniso</ref>
      <ekz>
        elefanto demandas la kamelon:
        &mdash; Kial vi portas la mamojn surdorse?
        &mdash; Hm! Strange, ke tion demandas iu, kies <tld/>ilo pendas 
        surviza&gcirc;e. 
        <fnt>
          <aut>I. Szabo</aut>,
          <vrk><url
          ref="http://groups.google.com/group/humursako/browse_thread/thread/c3dc495d6048fece">
          Elefanto -- kamelo</url></vrk>,
          <lok>Humursako, 2010-09-15</lok>
        </fnt>.
      </ekz>
  </snc>
  <trdgrp lng="cs">
    <trd>mu&zcaron;stv&iacute;</trd>,
    <trd>&uacute;d</trd>
  </trdgrp>
  <trd lng="es">miembro viril</trd>
  <trd lng="fr">organe viril</trd>
  <trdgrp lng="sk">
    <trd>mu&zcaron;stvo</trd>,
    <trd>&uacute;d</trd>
  </trdgrp>
</drv>

<drv mrk="vir.0ina">
  <kap><tld/>ina</kap>
  <snc>
    <dif>
      Karakterizanta hominon, kontraste kun <tld/>homo:
      <ekz>
        la <tld/>ina
        frivoleco, babilemo, amemo;
      </ekz>
      <ekz>
        japana <tld/>ina koruso parkere kantis hungarajn melodiojn
        <fnt>
          <bib>Monato</bib>
          <vrk><url
          ref='http://www.esperanto.be/fel/mon/007166.html'>Korusa
          konkurso</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        feinoj, elfinoj, sova&gcirc;ulinoj kaj aliaj <tld/>inaj viva&jcirc;oj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Batalo je la Ebura Turo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_a;&c_n;&c_o;&c_ch;&c_y;</trd>,
    <trd>&c_zh;&c_a;&c_n;&c_o;&c_c;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="cs">&zcaron;ensk&yacute;</trd>
  <trd lng="de">weiblich</trd>
  <trdgrp lng="el">
    <trd>&theta;&eta;&lambda;&ypsilon;&kappa;&omikron_ton;&sigma_fina;</trd>,
    <trd>&gamma;&ypsilon;&ny;&alfa;&jota;&kappa;&epsilon;&jota_ton;&omikron;&sigma_fina;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>female</trd>,
    <trd>womanly</trd>,
    <trd>feminine</trd>
  </trdgrp>
  <trd lng="es">femenino</trd>
  <trd lng="fr">f&eacute;minin</trd>
  <trd lng="fy">froulik</trd>
  <trd lng="he">&nun;&shin;&yod;</trd>
  <trd lng="hu">n&odblac;i</trd>
  <trd lng="nl">vrouwelijk</trd>
  <trd lng="pl">&zdot;e&nacute;ski</trd>
  <trd lng="pt">feminino</trd>
  <trd lng="ru">&c_zh;&c_je;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">&zcaron;ensk&yacute;</trd>
  <trd lng="tp">meli</trd>
  <trd lng="vo">vomik</trd>
  <trd lng="zh">&#22899;<klr> [n&#474;]</klr></trd>
</drv>

<drv mrk="vir.0ino">
  <kap><tld/>ino</kap>
  <snc mrk="vir.0ino.homo">
    <dif>
      Homino, kontraste kun <tld/>homo:
      <ekz>
        adulta, aminda, bela <tld/>ino;
      </ekz>
      <ekz>
        sa&gcirc;a <tld/>ino konstruas sian domon, sed malsa&gcirc;a detruas &gcirc;in per
        siaj manoj  
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 14:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        kie diablo ne povas, tien <tld/>inon li &scirc;ovas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la eterna <tld/>ineco altiras nin supren.
      </ekz>
    </dif>
    <trd lng="fr">femme</trd>
    <trdgrp lng="zh">
      <trd>&#22899;&#20154; [<ind>n&#474; r&eacute;n</ind>]</trd>
      <trd>&#22899;&#23376; [<ind>n&#474; z&#464;</ind>]</trd>
    </trdgrp>
  </snc>
  <snc mrk="vir.0ino.maturagxa">
    <dif>
      Matura&gcirc;a homino, kontraste kun infano:
      <ekz>
        <tld/>ini&gcirc;anta knabino.
      </ekz>
    </dif>
    <trd lng="fr">femme<klr> (adulte)</klr></trd>
    <trdgrp lng="zh">
      <trd>&#25104;&#24180;&#22899;&#23376; [<ind>ch&eacute;ng
      ni&aacute;n n&#474; z&#464;</ind>]</trd>
      <trd>&#22919;&#22899; [<ind>f&ugrave; n&#474;</ind>]</trd>,
    </trdgrp>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_n;&c_ch;&c_y;&c_n;&c_a;</trd>
  <trd lng="cs">&zcaron;ena</trd>
  <trd lng="de">Frau</trd>
  <trd lng="el">&gamma;&ypsilon;&ny;&alfa;&jota_ton;&kappa;&alfa;</trd>
  <trd lng="en">woman</trd>
  <trd lng="es">mujer</trd>
  <trd lng="et">naine</trd>
  <trd lng="fy">frou</trd>
  <trd lng="he">&alef;&shin;&he;</trd>
  <trd lng="hu">n&odblac;</trd>
  <trd lng="io">muliero</trd>
  <trd lng="nl">vrouw</trd>
  <trd lng="pl">kobieta</trd>
  <trd lng="pt">mulher</trd>
  <trd lng="ru">&c_zh;&c_je;&c_n;&c_shch;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">&zcaron;ena</trd>
  <trd lng="tp">meli</trd>
  <trd lng="tr">kad&inodot;n</trd>
  <trd lng="vo">vom</trd>
</drv>

<drv mrk="vir.0inismo">
  <kap><tld/>inismo</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="vir.0inismo.MOR">
    <ref tip="dif" cel="femini.0o">feminismo</ref>
      <ekz>
        <tld/>inismo detruis multege da geedzaj paroj
        <fnt>
          <aut>Petro</aut>,
          <vrk>Ni ne faru an&gcirc;elojn el la homoj</vrk>,
          <lok>en: Vivprotokoloj, 2009</lok>
        </fnt>.
      </ekz>
  </snc>
  <trdgrp lng="cs">
    <trd>feminizmus</trd>,
    <trd>&zcaron;ensk&eacute; hnut&iacute;</trd>
  </trdgrp>
  <trd lng="de">Feminismus</trd>
  <trd lng="en">feminism</trd>
  <trd lng="es">feminismo</trd>
  <trd lng="fr">f&eacute;minisme</trd>
  <trd lng="fy">feminisme</trd>
  <trd lng="hu">feminizmus</trd>
  <trd lng="io">feminismo</trd>
  <trd lng="nl">feminisme</trd>
  <trd lng="pl">feminizm</trd>
  <trd lng="pt">feminismo</trd>
  <trd lng="ru">&c_f;&c_je;&c_m;&c_i;&c_n;&c_i;&c_z;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>feminizmus</trd>,
    <trd>&zcaron;ensk&eacute; hnutie</trd>
  </trdgrp>
</drv>

<drv mrk="vir.mal0a">
  <kap>mal<tld/>a</kap>
  <fnt><bib>WED</bib></fnt>
  <snc>
    <dif>
      Ne vira; ne karakterizita de, tipa de, a&ubreve; ta&ubreve;ga por <tld/>o.
    </dif>
  </snc>
  <trd lng="de">unm&auml;nnlich</trd>
  <trdgrp lng="en">
    <trd>unmanly</trd>,
    <trd>unmasculine</trd>
  </trdgrp>
  <trd lng="fy">&ucirc;nmanlik</trd>
  <trd lng="nl">onmannelijk</trd>
</drv>
</art>
