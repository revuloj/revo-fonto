<drv mrk="vir.0a">
  <kap><tld/>a</kap>
  <snc mrk="vir.0a.virula">
    <dif>
      Karakterizanta la <tld/>ojn:
      <ekz>
        <tld/>a organo, birdo, infano, sekso.
      </ekz>
    </dif>
    <subsnc mrk="vir.0a.malina">
      <dif>
        Karakterizanta <tld/>homojn kontraste kun hominoj:
        <ekz>
          en la komenco la Kreinto faris ilin <tld/>a kaj <tld/>ina
          <fnt>
            <bib>NT</bib>
            <lok>Mateo 19:4</lok>
          </fnt>;
        </ekz>
        <ekz>
          &scirc;i naskis filon, <tld/>an infanon
          <fnt>
            <bib>NT</bib>
            <lok>Apokalipso 12:5</lok>
          </fnt>;
        </ekz>
        <ekz>
          estu kura&gcirc;aj kaj <tld/>aj, ho
          Fili&scirc;toj
          <fnt>
            <bib>MT</bib>
            <lok>I. Samuel 4:9</lok>
          </fnt>!
        </ekz>
        <ekz>
          forta <tld/>a vo&ccirc;o ekkriis
          <fnt>
            <bib>Fab1</bib>
            <lok>Vojkamarado</lok>
          </fnt>;
        </ekz>
        <ekz>
          li lasis fari al &scirc;i <tld/>an vesta&jcirc;on, por ke &scirc;i povu akompanadi lin anka&ubreve; sur
          &ccirc;evalo
          <fnt>
            <bib>Fab1</bib>
            <lok>La virineto de maro</lok>
          </fnt>;
        </ekz>
        <ekz>
          <tld/>oj altaj, kun fiera mieno kaj <tld/>a energio en la viza&gcirc;o
          <fnt>
            <bib>Far2</bib>
            <lok>&Ccirc;apitro IV</lok>
          </fnt>;
        </ekz>
        <ekz>
          li e&ccirc; ne vidis la <tld/>inan silueton malanta&ubreve; la <tld/>a figuro
          <fnt>
            <bib>IK</bib>
          </fnt>;
        </ekz>
        <ekz>
          vi aspektas tiel <tld/>e maltima
          <fnt>
            <bib>Fab3</bib>
            <lok>Kolumo</lok>
          </fnt>!
        </ekz>
      </dif>
      <trd lng="es">masculino</trd>
      <trd lng="hu">f&eacute;rfi-</trd>
      <trd lng="pl">m&eogonek;ski</trd>
      <trd lng="sk">mu&zcaron;sk&yacute;</trd>
      <trd lng="zh">&#30007;<klr> [n&aacute;n]</klr></trd>
    </subsnc>
    <subsnc mrk="vir.0a.matura">
      <dif>
        Karakterizanta <ref cel="vir.0o.matura"><tld/>ojn<sncref ref="vir.0o.matura"/></ref>, t.e.
        maturan kontraste kun
        infanoj:
        <ekz>
          kiu naski&gcirc;as en via domo, mortos en la <tld/>a a&gcirc;o
          <fnt>
            <bib>MT</bib>
            <lok>I. Samuel 2:33</lok>
          </fnt>;
        </ekz>
        <ekz>
          <klr>[li]</klr> son&gcirc;is pri la tempo de sia juneco kaj de sia <tld/>a a&gcirc;o
          <fnt>
            <bib>Fab3</bib>
            <lok>Historio de la jaro</lok>
          </fnt>;
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="hero">heroa</ref>.
      </refgrp>
      <trd lng="hu">f&eacute;rfias</trd>
      <trd lng="pl">m&eogonek;&zdot;ny</trd>
      <trd lng="sk">chlapsk&yacute;</trd>
    </subsnc>
    <trd lng="es">masculino</trd>
    <trdgrp lng="hu">
      <trd>h&iacute;m-</trd>,
      <trd>kan-</trd>
    </trdgrp>
    <trd lng="id">jantan</trd>
    <trdgrp lng="pl">
      <trd>m&eogonek;ski</trd>,
      <trd>samczy</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>mu&zcaron;sk&yacute;</trd>,
      <trd>sam&ccaron;&iacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="vir.0a.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      Gramatika <ref cel="genr.0o.GRA">genro</ref>, kiu, kontraste kun
      la genroj ina kaj ne&ubreve;tra, marki&gcirc;as per la
      samaj artikoloj, pronomoj ktp. kiuj estas uzataj por <tld/>aj homoj:
      <ekz>
        <tld/>a pronomo, sufikso
        <klr>(uzataj por montri <tld/>ojn)</klr>;
      </ekz>
      <ekz>
        en la lingvo germana &leftquot;kapo&rightquot; havas <tld/>an sekson
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
    </dif>
    <trd lng="id">maskulin</trd>
  </snc>
  <snc mrk="vir.0a.POE">
    <uzo tip="fak">POE</uzo>
    <dif>
      <klr>(p.p. <ref cel="rim.0o.versparto">rimo</ref>)</klr>
      Fini&gcirc;anta per akcenta silabo;
      <klr>(p.p. <ref cel="cezur.0o">cezuro</ref>)</klr>
      sekvanta akcentan silabon:
      <ekz>
        &leftquot;dolor'&rightquot;, &leftquot;kor'&rightquot; konsistigas <tld/>ajn rimojn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="in.0a.POE">ina</ref>.
    </refgrp>
    <trd lng="hu">h&iacute;m-</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_u;&c_zh;&c_ch;&c_y;&c_n;&c_s;&c_k;&c_ib;</trd>,
    <trd>&c_m;&c_a;&c_s;&c_k;&c_u;&c_l;&c_ib;&c_n;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mu&zcaron;n&yacute;</trd>,
    <trd>mu&zcaron;sk&yacute;</trd>
  </trdgrp>
  <trd lng="de">m&auml;nnlich</trd>
  <trdgrp lng="el">
    <trd>&alfa;&rho;&sigma;&epsilon;&ny;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>,
    <trd>&alfa;&ny;&delta;&rho;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>male</trd>,
    <trd>manly</trd>,
    <trd>masculine</trd>
  </trdgrp>
  <trd lng="fr">masculin</trd>
  <trdgrp lng="he">
    <trd>&zayin;&kaf;&resh;&yod;</trd>,
    <trd>&#8503;&bet;&resh;&yod;</trd>
  </trdgrp>
  <trd lng="fy">manlik</trd>
  <trdgrp lng="ja">
    <trd>&#30007;&#x306E;  <pr>&#x304A;&#12392;&#12371;&#x306E;</pr></trd>,
    <trd>&#30007;&#x306E;&#12424;&#12358;&#x306A;  <pr>&#x304A;&#12392;&#12371;&#x306E;&#12424;&#12358;&#x306A;</pr></trd>
  </trdgrp>
  <trd lng="nl">mannelijk</trd>
  <trd lng="pt">masculino</trd>
  <trd lng="ru">&c_m;&c_u;&c_zh;&c_s;&c_k;&c_o;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>mu&zcaron;sk&yacute;, zmu&zcaron;il&yacute;</trd>,
    <trd>zmu&zcaron;il&yacute;</trd>
  </trdgrp>
  <trd lng="tok">mije</trd>
  <trd lng="uk">&c_ch;&#x43E;&#x43B;&#x43E;&c_v;&c_ib;&c_ch;&c_i;&c_j;</trd>
  <trd lng="vo">manik</trd>
</drv>

<drv mrk="vir.0eco">
  <kap><ofc>*</ofc> <tld/>eco</kap>
  <snc mrk="vir.0eco.eco">
    <dif>
      Eco karakterizanta <tld/>on:
      <ekz>
        tiun &ccirc;i baton li elportas kun tia sama <tld/>eco, kun kia li renkontadis kaj
        elportadis la marajn ventegojn
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        forto kaj komenco de mia <tld/>eco
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 49:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        en tiu formo de mensa primitiveco, en kiu regas <tld/>eco, decidan rolon ludas
        la genoj de la patro
       <fnt>
        <aut>Michel Duc Goninaz</aut>
        <vrk><url ref="&Monato;/2000/006947.html">Eks-Jugoslavio:
          kiu kiun mistifikas?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        neniu infano povus havi tian kruelan dan&gcirc;eran vivon kaj tamen atingi <tld/>econ
        kaj i&gcirc;i intelektulo
        <fnt>
          <bib>Monato</bib>
          <aut>Petro de Smedt</aut>
          <vrk><url ref="&Monato;/2010/009946.php">La verde farbita birdo</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_zh;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="de">M&auml;nnlichkeit</trd>
  <trd lng="en">virility</trd>
  <trd lng="fr">virilit&eacute;</trd>
  <trdgrp lng="id">
    <trd>ke<ind>jantan</ind>an</trd>,
    <trd>kele<ind>laki</ind>an</trd>,
