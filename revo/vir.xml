<drv mrk="vir.0ino">
  <kap><ofc>*</ofc><tld/>ino</kap>
  <snc mrk="vir.0ino.homo">
    <dif>
      Homino, kontraste kun <tld/>homo:
      <ekz>
        en formo de <tld/>o kaj <tld/>ino Li kreis ilin
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 1:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        en unu tago, kiam &scirc;i estis apud tiu fonto, venis al &scirc;i malri&ccirc;a
        <tld/>ino
        <fnt><bib>F</bib>, <lok>&FE; 15</lok></fnt>;
      </ekz>
      <ekz>
        sa&gcirc;a <tld/>ino konstruas sian domon, sed malsa&gcirc;a detruas &gcirc;in per
        siaj manoj  
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 14:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lango de <tld/>ino estas &scirc;ia glavo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kie diablo ne povas, tien <tld/>inon li &scirc;ovas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="fr">femme</trd>
    <trdgrp lng="zh">
      <trd>&#22899;&#20154; [<ind>n&#474; r&eacute;n</ind>]</trd>
      <trd>&#22899;&#23376; [<ind>n&#474; z&#464;</ind>]</trd>
    </trdgrp>
  </snc>
  <snc mrk="vir.0ino.maturagxa">
    <dif>
      Matura&gcirc;a homino, kontraste kun infano:
      <ekz>
        <nom>Esav</nom> levis siajn okulojn kaj ekvidis la <tld/>inojn kaj la infanojn
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 33:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>ino gravedi&gcirc;is, kaj naskis filon
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 2:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        jam <tld/>ino, ne knabino
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>inoj kaj knabinoj dancas unu kun alia
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">femme<klr> (adulte)</klr></trd>
    <trdgrp lng="zh">
      <trd>&#25104;&#24180;&#22899;&#23376; [<ind>ch&eacute;ng
      ni&aacute;n n&#474; z&#464;</ind>]</trd>
      <trd>&#22919;&#22899; [<ind>f&ugrave; n&#474;</ind>]</trd>,
    </trdgrp>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_n;&c_ch;&c_y;&c_n;&c_a;</trd>
  <trd lng="cs">&zcaron;ena</trd>
  <trd lng="de">Frau</trd>
  <trd lng="el">&gamma;&ypsilon;&ny;&alfa;&jota_ton;&kappa;&alfa;</trd>
  <trd lng="en">woman</trd>
  <trd lng="es">mujer</trd>
  <trd lng="et">naine</trd>
  <trd lng="fy">frou</trd>
  <trd lng="he">&alef;&shin;&he;</trd>
  <trd lng="hu">n&odblac;</trd>
  <trd lng="io">muliero</trd>
  <trd lng="nl">vrouw</trd>
  <trd lng="pl">kobieta</trd>
  <trd lng="pt">mulher</trd>
  <trd lng="ru">&c_zh;&c_je;&c_n;&c_shch;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">&zcaron;ena</trd>
  <trd lng="tp">meli</trd>
  <trd lng="tr">kad&inodot;n</trd>
  <trd lng="vo">vom</trd>
</drv>

<drv mrk="vir.0ineco">
  <kap><ofc>*</ofc> <tld/>ineco</kap>
  <snc mrk="vir.0ineco.eco">
    <dif>
      Eco karakterizanta <tld/>inon:
      <ekz>
        en ni montri&gcirc;os tuj la <tld/>ineco
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiu edzinigo al si celus detrui la <tld/>inecon de la <tld/>ino por edzi&gcirc;i al
        masklo
        <fnt>
          <bib>Monato</bib>
          <aut>Armando Zecchin</aut>
          <vrk><url ref="https://www.monato.be/2006/008972.php"
              >Maria Magdalena: &ccirc;u edzino a&ubreve; ne?</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Weiblichkeit</trd>
  <trd lng="en">virility</trd>
  <trd lng="fr">virilit&eacute;</trd>
</drv>  

<drv mrk="vir.0inismo">
  <kap><tld/>inismo</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="vir.0inismo.MOR">
    <dif>
      <ref tip="dif" cel="femini.0o">Feminismo</ref>:
      <ekz>
        <tld/>inismo detruis multege da geedzaj paroj
        <fnt>
          <aut>Petro</aut>,
          <vrk>Ni ne faru an&gcirc;elojn el la homoj</vrk>,
          <lok>en: Vivprotokoloj, 2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        mito de nacia unueco <klr>[&#8230;]</klr> estis drasta rimedo kontra&ubreve; klasbatalo,
        socialismo kaj <tld/>inismo
        <fnt>
          <vrk><url ref="https://libcom.org/taxonomy/term/2010/all/feed"
              >ANC</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>feminizmus</trd>,
    <trd>&zcaron;ensk&eacute; hnut&iacute;</trd>
  </trdgrp>
