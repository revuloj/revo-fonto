<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>musk</rad>/oj
</kap>
<drv mrk="musk.0oj">
  <kap><ofc>*</ofc><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Klaso de <ref tip="super" cel="musk.0oplantoj"><tld/>oplantoj</ref>
      (<trd lng="la">Musci</trd>), sen vera radiko kaj kun
      mallar&gcirc;aj folietoj, ofte dense kreskantaj sur
      malsekaj &scirc;tonoj, arboj, kc.
    </dif>
  </snc>
  <trd lng="de">Laubmoose</trd>
  <trd lng="hu">lombos <ind>moh&aacute;k</ind></trd>
  <trd lng="nl">mossen</trd>
  <trd lng="pt">musgos</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_i;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_je; <ind>&c_m;&c_h;&c_i;</ind><klr tip="ind"> (&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>,
    <trd>&c_l;&c_i;&c_s;&c_t;&c_o;&c_s;&c_t;&c_je;&c_b;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_je; <ind>&c_m;&c_h;&c_i;</ind> <klr tip="amb">(&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="musk.0oplantoj">
  <kap><tld/>oplantoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Divizio de la <ref tip="super" cel="plant.0oj">plantoj</ref>
      (<trd lng="la">Bryophyta</trd>), sporoplantoj
      sen veraj radikoj kaj angioj.
    </dif>
  </snc>
  <trd lng="de">Moose</trd>
  <trd lng="hu">moh&aacute;k</trd>
  <trd lng="nl">mossen</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_h;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;<klr tip="ind"> (&c_o;&c_t;&c_d;&c_je;&c_l; &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_j;)</klr></trd>,
    <trd>&c_m;&c_o;&c_h;&c_o;&c_v;&c_i;&c_d;&c_n;&c_y;&c_je; <klr tip="amb">(&c_o;&c_t;&c_d;&c_je;&c_l; &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_j;)</klr></trd>    
  </trdgrp>
</drv>
</art>
