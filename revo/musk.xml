<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>musk</rad>/oj
</kap>
<drv mrk="musk.0oj">
  <kap><ofc>*</ofc><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Klaso de <ref tip="super" cel="musk.0oplantoj"><tld/>oplantoj</ref>
      (<trd lng="la">Musci</trd>), sen vera radiko kaj kun
      mallar&gcirc;aj folietoj, ofte dense kreskantaj sur
      malsekaj &scirc;tonoj, arboj, kc.
    </dif>
  </snc>
  <trd lng="de">Laubmoose</trd>
</drv>
<drv mrk="musk.0oplantoj">
  <kap><tld/>oplantoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Grupo de <ref tip="super" cel="spor.0oplantoj">sporoplantoj</ref>
      (<trd lng="la">Bryophyta</trd>), sen veraj radikoj kaj angioj.
     </dif>
  </snc>
  <trd lng="de">Moose</trd>
</drv>
</art>
