<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: musk.xml,v 1.32 2024/01/26 19:05:54 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>musk</rad>/o
</kap>

<drv mrk="musk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="musk.0o.KOMUNE">
    <dif>
      Aro da senradikaj malgrandaj vegeta&jcirc;oj 
       ofte dense kreskantaj sur
      malsekaj &scirc;tonoj, arboj, kc:
      <ekz>
        kiel musko sur tegmentoj 
        <fnt><bib>MT</bib>, <lok>Jesaja 37:27</lok></fnt>;
      </ekz>
      <ekz>
         fre&scirc;e verde briletis la musko &ccirc;e la trunkoj de la arboj
         <fnt><bib>Fab3</bib>, <lok>Historio de la jaro</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_o;&c_h;</trd>
    <trd lng="cs">mech</trd>
    <trd lng="de">Moos</trd>
    <trd lng="en">moss</trd>
    <trd lng="fr">mousse</trd>
    <trd lng="id">lumut</trd>
    <trd lng="pl">mech</trd>
    <trd lng="sk">mach</trd>
    <trdgrp lng="zh">
    <trd>&#x85D3; <pr>xi&#x1CE;n</pr></trd>,
    <trd>&#x861A; <pr>xi&#x1CE;n</pr></trd>,
    <trd>&#x82D4; <pr>t&#xE1;i</pr></trd>,
    <trd>&#x82D4;&#x85D3; <pr>t&#xE1;ixi&#x1CE;n</pr></trd>,
    <trd>&#x82D4;&#x861A; <pr>t&#xE1;ixi&#x1CE;n</pr></trd>,
    <trd>&#x9752;&#x82D4; <pr>q&#x12B;ngt&#xE1;i</pr></trd>
  </trdgrp>
</snc>
  <trd lng="ja">&#x30B3;&#x30B1;</trd>
</drv>

<drv mrk="musk.0oj">
  <kap><tld/>oj</kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="divizi.0o.BOT" lst="voko:botanikaj_grupoj">Divizio</ref> de <ref tip="malprt" cel="plant.0oj">plantoj</ref>
      (<trd lng="la">Bryophyta</trd>);
      <ref tip="lst" cel="spor.0oplanto" lst="voko:sporoplantoj">sporoplantoj</ref>
      sen vera radiko kaj kun mallar&gcirc;aj folietoj.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_h;&c_a;&c_v;&c_a;&c_t;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_m;&c_malmol;&c_h;&c_o;&c_v;&c_je;</trd>
  <trd lng="cs">mechy <klr>(&ccaron;ele&dcaron;)</klr></trd>
  <trd lng="de">Laubmoose</trd>
  <trdgrp lng="en"><trd>mosses</trd>, <trd>bryophytes</trd></trdgrp>
  <trd lng="hu">lombos <ind>moh&aacute;k</ind></trd>
  <trd lng="ja">&#x861A;&#x985E;  <pr>&#x305B;&#x3093;&#x308B;&#x3044;</pr></trd>
  <trd lng="la">Musci</trd>
  <trd lng="nl">mossen</trd>
  <trd lng="pl">mchy</trd>
  <trd lng="pt">musgos</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_i;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_je; <ind>&c_m;&c_h;&c_i;</ind><klr tip="ind">
    (&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>,
    <trd>&c_l;&c_i;&c_s;&c_t;&c_o;&c_s;&c_t;&c_je;&c_b;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_je; <ind>&c_m;&c_h;&c_i;</ind> <klr tip="amb">(&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>
  </trdgrp>
  <trd lng="sk">machy</trd>
  <trd lng="uk">&#x43C;&#x43E;&c_h;&c_i;</trd>
</drv>

<drv mrk="musk.korno0oj">
  <kap>korno<tld/>oj</kap>
  <snc mrk="musk.korno0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="spor.0oplanto" lst="voko:sporoplantoj">Sporoplantoj</ref> el
      <ref tip="lst" cel="divizi.0o.BOT" lst="voko:botanikaj_grupoj">divizio</ref> de <ref tip="malprt" cel="plant.0oj">plantoj</ref>
      (<trd lng="la">Anthocerotophyta</trd>).
    </dif>
    <trd lng="be">&c_a;&c_n;&c_t;&c_a;&c_c;&c_e;&c_r;&c_o;&c_t;&c_a;&c_v;&c_y;&c_ja; &c_ib;&c_m;&c_h;&c_ib;</trd>
    <trd lng="de">Hornmoose</trd>
    <trd lng="en">hornworts</trd>
    <trd lng="pl">glewiki</trd>
    <trd lng="zh">&#x82F2;&#x8349; <pr>zh&#x1CE;c&#x1CE;o</pr></trd>
</snc>
</drv>

<drv mrk="musk.hepato0oj">
  <kap>hepato<tld/>oj<fnt>PIV2</fnt></kap>
  <snc mrk="musk.hepato0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="spor.0oplanto" lst="voko:sporoplantoj">Sporoplantoj</ref> el
      <ref tip="lst" cel="divizi.0o.BOT" lst="voko:botanikaj_grupoj">divizio</ref> de <ref tip="malprt" cel="plant.0oj">plantoj</ref>
      (<trd lng="la">Marchantiophyta</trd>).
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_ja;&c_ch;&c_o;&c_n;&c_a;&c_ch;&c_n;&c_y;&c_ja; &c_ib;&c_m;&c_h;&c_ib;</trd>,
      <trd>&c_p;&c_ja;&c_ch;&c_o;&c_n;&c_a;&c_ch;&c_n;&c_ib;&c_k;&c_ib;</trd>
    </trdgrp>
    <trd lng="de">Lebermoose</trd>
    <trd lng="en">liverworts</trd>
    <trd lng="pl">w&aogonek;trobowce</trd>
    <trdgrp lng="zh">
    <trd>&#x82D4;&#x7DB1; <pr>t&#xE1;ig&#x101;ng</pr></trd>,
    <trd>&#x82D4;&#x7EB2; <pr>t&#xE1;ig&#x101;ng</pr></trd>
  </trdgrp>
</snc>
</drv>


</art>
