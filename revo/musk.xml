<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>musk</rad>/oj
</kap>
<drv mrk="musk.0oj">
  <kap><ofc>*</ofc><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Klaso de sporoplantoj (<trd lng="la">Musci</trd>)
      sen vera radiko kaj mallar&gcirc;aj folietoj,
      ofte dense kreskanta sur malsekaj &scirc;tonoj, arboj, kc.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Laubmoose</trd>,
    <trd lng="de">Moose <klr tip="ind">(Laubmoose)</klr></trd>
  </trdgrp>
</drv>
</art>
