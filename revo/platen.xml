<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>platen</rad>/o
</kap>
<drv mrk="platen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <mlg kod="IUPAC">Pt</mlg>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
      lst="voko:&hcirc;emiaj_elementoj" val="78">elemento</ref> kun
      atomnumero 78, atompezo 195,09; grize
      blanka, peza, ne
      oksidebla <ref tip="super" cel="metal.0o">metalo</ref>, pli valora
      ol <ref tip="vid" cel="or.0o">oro</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ja;&c_t;&c_y;&c_n;&c_a;</trd>
  <trd lng="bg">&c_p;&c_l;&c_a;&c_t;&c_i;&c_n;&c_a;</trd>
  <trd lng="br">platin</trd>
  <trd lng="cs">platina</trd>
  <trd lng="de">Platin</trd>
  <trd lng="en">platinum</trd>
  <trd lng="es">platino</trd>
  <trd lng="fr">platine</trd>
  <trd lng="hu">platina</trd>
  <trd lng="id">platinum</trd>
  <trd lng="it">platino</trd>
  <trd lng="la">Platinum</trd>
  <trd lng="nl">platina</trd>
  <trd lng="pl">platyna</trd>
  <trd lng="pt">platina</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_t;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">platina</trd>
  <trd lng="sv">platina</trd>
</drv>
</art>
