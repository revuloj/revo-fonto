<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>platen</rad>/o
</kap>
<drv mrk="platen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 78, atompezo 195,09, simbolo Pt. Grize blanka, peza, ne
      oksidebla metalo, pli valora ol oro.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ja;&c_t;&c_y;&c_n;&c_a;</trd>
  <trd lng="de">Platin</trd>
  <trd lng="hu">platina</trd>
  <trd lng="nl">platina</trd>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_t;&c_i;&c_n;&c_a;</trd>
</drv>
</art>
