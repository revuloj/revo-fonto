<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>mit</rad>/o
</kap>
<drv mrk="mit.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">BELE</uzo>
    <dif>
      Tradicia fabela rakonto pri antikvaj dioj kaj praaj tempoj, ofte kun
      ka&scirc;ita alegoria signifo:
      <ekz>
        la <tld/>o de Prometeo.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tute fantazia imaga&jcirc;o
    </dif>
    <refgrp tip="sin">
      <ref cel="fabel.0o">fabelo</ref>,
      <ref cel="fantaz.0o">fantazio</ref>,
      <ref cel="rev">revo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Mythos</trd>
  <trd lng="en">myth</trd>
  <trd lng="es">mito</trd>
  <trd lng="fr">mythe</trd>
  <trd lng="hu">m&iacute;tosz</trd>
  <trd lng="nl">mythe</trd>
  <trd lng="ru">&c_m;&c_i;&c_f;</trd>
</drv>
</art>
