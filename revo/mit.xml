<vortaro>
<art mrk="$Id: mit.xml,v 1.39 2024/03/14 09:23:24 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>mit</rad>/o <fnt><bib>JED</bib></fnt>
</kap>

<drv mrk="mit.0o">
  <kap><tld/>o</kap>
  <snc mrk="mit.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Tradicia fabela <ref tip="lst" lst="voko:komunikoj" cel="rakont.0o">rakonto</ref>
      pri antikvaj dioj kaj praaj tempoj, ofte kun ka&scirc;ita alegoria signifo:
      <ekz>
        la <tld/>o pri Prometeo;
      </ekz>
      <ekz>
        <tld/>o el pratempo
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
          <lok>&leftquot;Monismo&rightquot;</lok>
        </fnt>;
      </ekz>
      <ekz>
        historia dokumento pri la <tld/>oj, <ref cel="rit.0o">rito</ref>j kaj militoj de nomada
        popolo, same
        interesa por sendiuloj kiel por malateistoj
        <fnt>
	<aut>Jorge Camacho</aut>
	<vrk><url ref="&Monato;/2000/006820.php">Plia libro biblia</url></vrk>
	<bib>Monato</bib>
	<lok>2000/08, p. 24</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="mitolo.0o.SCI">mitologio<sncref/></ref>
  </snc>
  <snc mrk="mit.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fantazia misinformo &gcirc;enerale kredata vera:
      <ekz>
        <ctl>Esperanto sen <tld/>oj</ctl>
        <fnt>
          <aut>Pejno S.</aut>
          <vrk>Faligas la fundamentojn de esperantismo</vrk>
          <bib>Monato</bib>
          <lok>1999/06, p. 27</lok>
        </fnt>;
      </ekz>
      <ekz>
        la leganto atendas novan historian eltrova&jcirc;on en temo tre diskutata, sed trovas
        nur malnovan <tld/>on, kiun la nunaj historiistoj ne plu subtenas
        <fnt>
        <aut>Michel Duc Goninaz</aut>
        <vrk><url ref="&Monato;/2000/006947.html">Eks-Jugoslavio:
          kiu kiun mistifikas?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 8</lok>
        </fnt>
      </ekz>
      <ekz>
        spite al la akceptitaj <tld/>oj (ne homkonformaj) pri kolektivo, popolo
        konsistas el individuoj
        <fnt>
	<aut>Gilbert Ledon</aut>
	<vrk><url ref="&Monato;/2001/007283.php">Putraj demokratioj</url></vrk>
	<bib>Monato</bib>
	<lok>2001/01, p. 5</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="fakt.0o">fakto</ref>
    <trd lng="de">moderne Legende</trd>
  </snc>
    <refgrp tip="vid">
      <ref cel="fabel.0o">fabelo</ref>,
      <ref cel="fam.0o.onidiroj">famo<sncref/></ref>,
      <ref cel="legend.0o">legendo</ref>,
      <ref cel="dir.oni0o">onidiro</ref>
    </refgrp>
  <trd lng="be">&c_m;&c_ib;&c_t;</trd>
  <trd lng="ca">mite</trd>
  <trdgrp lng="cs">
    <trd>b&aacute;je</trd>,
    <trd>m&yacute;tus</trd>,
    <trd>pov&ecaron;st</trd>
  </trdgrp>
  <trd lng="de">Mythos</trd>
  <trd lng="en">myth</trd>
  <trd lng="es">mito</trd>
  <trd lng="fr">mythe</trd>
  <trd lng="hu">m&iacute;tosz</trd>
  <trdgrp lng="ja">
    <trd>&#x795E;&#x8A71;  <pr>&#12375;&#12435;&#x308F;</pr></trd>,
    <trd>&#x4F5C;&#x308A;&#x8A71;  <pr>&#12388;&#x304F;&#x308A;&#12400;&#x306A;&#12375;</pr></trd>,
    <trd>&#26681;&#x62E0;&#x306E;&#x306A;&#12356;&#x793E;&#x4F1A;&#x901A;&#x5FF5; 
    <pr>&#12371;&#12435;&#x304D;&#12423;&#x306E;&#x306A;&#12356;&#12375;&#12419;&#x304B;&#12356;&#12388;&#12358;&#x306D;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">mythe</trd>
  <trd lng="pl">mit</trd>
  <trd lng="pt">mito</trd>
  <trd lng="ru">&c_m;&c_i;&c_f;</trd>
  <trdgrp lng="sk">
    <trd>b&aacute;j</trd>,
    <trd>m&yacute;tus</trd>,
    <trd>poves&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_ib;&c_f;</trd>,
    <trd>&#x43C;&c_ib;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x795E;&#x8A71; <pr>sh&#xE9;nhu&#xE0;</pr></trd>,
    <trd>&#x795E;&#x8BDD; <pr>sh&#xE9;nhu&#xE0;</pr></trd>,
    <trd>&#x8FF7;&#x601D; <pr>m&#xED;s&#x12B;</pr></trd>,
    <trd>&#x6C11;&#38291;&#25925;&#x4E8B; <pr>m&#xED;nji&amacron;ng&#xF9;sh&#xEC;</pr></trd>,
    <trd>&#x6C11;&#x95F4;&#25925;&#x4E8B; <pr>m&#xED;nji&amacron;ng&#xF9;sh&#xEC;</pr></trd>,
    <trd>&#x4F20;&#22855;&#x4EBA;&#29289; <pr>zhu&#xE0;nq&#xED;r&#xE9;nw&#xF9;</pr></trd>,
    <trd>&#x50B3;&#22855;&#x4EBA;&#29289; <pr>zhu&#xE0;nq&#xED;r&#xE9;nw&#xF9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mit.0a">
  <kap><tld/>a</kap>
  <snc mrk="mit.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Delonge tre fama, sed malfacile trovebla, spertebla:
      <ekz>
        <tld/>a trupo kun ege ritma kanto
        <fnt>
          <aut>JPVDD</aut>
          <vrk>&Scirc;paru monon</vrk>
          <bib>Monato</bib>
          <lok>1995:7, p. 27a</lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;la ses tagoj&rightquot; estas <klr>[&#8230;]</klr> <tld/>a kadro por transdoni la koncepton de
        la dia kreado
        <fnt>
          <bib>Monato</bib>
          <aut>Armando Zecchin</aut>
          <vrk><url ref="&Monato;/2009/009913.php">Akcepti kaj sciencon kaj
          Biblion</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        por multaj eksterlandanoj Litovio estas nekonata kaj fojfoje e&ccirc; <tld/>a lando,
        nekomprenata kaj misreprezentata
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2007/009195.php">Litovio, &ccirc;u Mitovio?</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj &leftquot;Fortoj&#x201D; &ndash; tondro, printempo, pluvo, suno, varmo, vento
        <fnt>
          <bib>Monato</bib>
          <aut>David Kelso</aut>
          <vrk><url ref="&Monato;/2009/009671.php">Kompreno en kunteksto
          ...</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;milito&rightquot;, vorto <tld/>a kaj magia, glora por la patriotoj, abomena por la
        pacifistoj
        <fnt>
        <aut>Michel Duc Goninaz</aut>
        <vrk><url ref="&Monato;/2000/006947.html">Eks-Jugoslavio:
          kiu kiun mistifikas?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 8</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="enigm.0a">enigma</ref>,
    <ref cel="mistik.0a">mistika</ref>
  </refgrp>
  <trd lng="be">&c_m;&c_ib;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">m&iacute;tic</trd>
  <trdgrp lng="cs">
    <trd>b&aacute;jn&yacute;</trd>,
    <trd>mytick&yacute;</trd>,
    <trd>smy&scaron;len&yacute;</trd>,
    <trd>vyb&aacute;jen&yacute;</trd>
  </trdgrp>
  <trd lng="de">mythisch</trd>
  <trd lng="en">mythical</trd>
  <trd lng="es">m&iacute;tico</trd>
  <trd lng="fr">mythique</trd>
  <trdgrp lng="ja">
    <trd>&#x795E;&#x8A71;&#x306E;  <pr>&#12375;&#12435;&#x308F;&#x306E;</pr></trd>,
    <trd>&#x795E;&#x8A71;&#30340;&#x306A;  <pr>&#12375;&#12435;&#x308F;&#12390;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x67B6;&#x7A7A;&#x306E;  <pr>&#x304B;&#x304F;&#12358;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="pl">mityczny</trd>
  <trd lng="ru">&c_m;&c_i;&c_f;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;
  </trd>
  <trdgrp lng="sk">
    <trd>b&aacute;jny</trd>,
    <trd>m&yacute;tick&yacute;</trd>,
    <trd>vyb&aacute;jen&yacute;</trd>
  </trdgrp>
  <trd lng="uk">&#x43C;&c_ib;&c_f;&c_ib;&c_ch;&#x43D;&c_i;&c_j;</trd>
</drv>

<drv mrk="mit.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="mit.0aro.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Tuto de
      <ref tip="vid" cel="mit.0o.MIT"><tld/>oj<sncref/></ref>
      de iu kulturo; <ref tip="sin" cel="mitolo.0o.mitaro">mitologio<sncref/></ref>:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[li]</klr> zorge studis la verajn faktojn kaj ne lasis sin influi&gcirc;i per la
        <tld/>aro, kiu akumuli&gcirc;is pri la iniciatinto de Esperanto
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2011/010152.php">Senmita biografio de
          Zamenhof</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_t;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="ca">mitologia</trd>
  <trdgrp lng="de">
    <trd>Mythensammlung</trd>,
    <trd>Mythologie</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>mythology</trd>,
    <trd>myths</trd>
  </trdgrp>
  <trd lng="es">mitolog&iacute;a</trd>
  <trd lng="fr">mythologie<klr> (collection de mythes)</klr></trd>
  <trd lng="pl">mitologia</trd>
  <trd lng="ru">&c_m;&c_i;&c_f;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="sk">mytol&oacute;gia</trd>
  <trdgrp lng="zh">
    <trd>&#x795E;&#x8A71; <pr>sh&#xE9;nhu&#xE0;</pr></trd>,
    <trd>&#x795E;&#x8BDD; <pr>sh&#xE9;nhu&#xE0;</pr></trd>,
    <trd>&#x795E;&#x8A71;&#x5B78; <pr>sh&#xE9;nhu&#xE0;xu&#xE9;</pr></trd>,
    <trd>&#x795E;&#x8BDD;&#x5B66; <pr>sh&#xE9;nhu&#xE0;xu&#xE9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mit.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="mit.sen0a.KOMUNE">
    <dif>
      Ne enhavanta <tld/>ojn, fantaziajn elpensa&jcirc;ojn,
      la&ubreve;eble <ref tip="vid" cel="objekt1.0a.KOMUNE">objektiva</ref>:
      <ekz>
        sen<tld/>a trakto de nia sintakso
        <fnt>
          <aut>W. Jansen</aut>,
          <vrk><url ref="http://edukado.net/biblioteko/dosierujo?iid=52">
          Naturaj vortordoj en Esperanto</url></vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>objektiv</trd>,
      <trd>n&uuml;chtern <klr>(objektiv)</klr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ja;&c_z;&c_mol; <ind>&c_m;&c_ib;&c_t;&c_a;&c_w;</ind></trd>,
    <trd>&c_a;&c_b;'&c_je;&c_k;&c_t;&c_y;&c_w;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="fr">sans mythe</trd>
</drv>
