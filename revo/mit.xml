<ref cel="fabel.0o">fabelo</ref>,
<ref cel="fantaz.0o">fantazio</ref>,
<ref cel="rev">revo</ref>.
</refgrp>
</snc>
<trd lng="fr">mythe</trd>
<trd lng="nl">mythe</trd>
</drv>
