<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gradus.xml,v 1.18 2023/10/15 18:38:19 revo Exp $">
<kap>
  <rad>gradus</rad>/o
</kap>
<drv mrk="gradus.0o">
  <kap><tld/>o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="gradus.0o.unuo">
    <uzo tip="fak">MAT</uzo>
    <mlg>gon</mlg>
    <dif>
      Komunuza <ref tip="lst" cel="unu.mezur0o" lst="voko:mezurunuoj">mezurunuo</ref> de
      <ref cel="angul.ebena0o">ebena angulo</ref>, egala al la
      200a parto de stre&ccirc;ita angulo; simb. gr:
      <ekz>
        <tld/>o egalas al 
        <frm>&pi;/200</frm>.
      </ekz>
    </dif>
    <ref tip="vid" cel="radian.0o">radianoj</ref>
    <trd lng="ca">grau centesimal</trd>
    <trdgrp lng="de"><trd>Gon</trd>, <trd>Neugrad</trd></trdgrp> 
    <trdgrp lng="en"><trd>grade</trd>, <trd>gon</trd></trdgrp> 
    <trd lng="fa">&f_g;&a_r;&a_A;&a_d;</trd>
    <trdgrp lng="fr"><trd>grade<klr tip="ind"> (unit&eacute; de
    mesure)</klr></trd>, <trd>gon</trd></trdgrp> 
    <trdgrp lng="hu"><trd>gon</trd>,
      <trd>&uacute;jfok</trd></trdgrp> 
    <trd lng="id">derajat</trd>
    <trd lng="nl">graad<klr tip="ind"> (400-delig)</klr></trd>
    <trdgrp lng="pl">
      <trd>grad</trd>,
      <trd>gradus</trd>
    </trdgrp> 
    <trd lng="pt">grado<klr tip="amb"> (unidade de medida)</klr></trd> 
    <trdgrp lng="ru"><trd>&c_g;&c_r;&c_a;&c_d;</trd>, <trd>&c_g;&c_o;&c_n;</trd></trdgrp>
  </snc>
  <snc mrk="gradus.0o.BELE">
    <uzo tip="fak">BELE</uzo>
    <dif>
      Latina <ref tip="super" cel="vort.0aro">vortaro</ref>, 
      montranta la prozodian valoron de &ccirc;iu silabo.
    </dif>
    <trd lng="ca">diccionari sil&middot;l&agrave;bic</trd>
    <trd lng="hu">Gradus ad Parnassum</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>grad</trd>,
    <trd>gr&#xE1;d</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30B0;&#x30E9;&#x30FC;&#x30C9;</trd>,
    <trd>&#x97FB;&#x5F8B;&#x8F9E;&#x5178;  <pr>&#x3044;&#x3093;&#x308A;&#x3064;&#x3058;&#x3066;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="sk">gr&#xE1;dus</trd>
  <trdgrp lng="uk">
    <trd>&#x491;&#x440;&#x430;&#x434;</trd>,
    <trd>&#x433;&#x430;&#x43D;</trd>
  </trdgrp>
</drv>
</art>

