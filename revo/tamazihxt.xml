<vortaro>
  <art mrk="$Id: tamazihxt.xml,v 1.3 2024/03/26 12:38:42 afido Exp $">
    <kap>
      <rad>tamazi&hcirc;t</rad>/o
      <fnt><bib>PIV2</bib></fnt>
    </kap>

<drv mrk="tamazihxt.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="dif" cel="berber1.0o">Berbero</ref>, parolanto de nearaba nordafrika lingvo:
      <ekz>
        <tld/>on oni ne konfuzos kun arabo kaj ne &ccirc;iujn ekvadoranojn oni konsideros
        hispanlingvaj
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos I Font</aut>
          <vrk><url ref="&Monato;/2004/008346.html"
              >Lingva bunto</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Berber</trd>,
      <trd>Tamazight</trd>
    </trdgrp>
    <trd lng="en">Berber</trd>
    <trd lng="fr">berb&egrave;re</trd>
    <trdgrp lng="pl">
      <trd>Berber</trd>,
      <trd>Amazygowie {pl}</trd>
    </trdgrp>
  </snc>
<trd lng="be">&c_t;&c_a;&c_m;&c_a;&c_z;&c_y;&c_h;&c_t;</trd>
</drv>
  </art>

