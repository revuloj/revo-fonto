        <trd>bien ra&iacute;z</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>fonds <klr tip="ind">(terrain)</klr></trd>,
        <trd>bien foncier</trd></trdgrp>
      <trdgrp lng="hu">
        <trd>f&ouml;ldbirtok</trd>,
        <trd>gazdas&aacute;g</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>gospodarstwo<klr tip="amb"> rolne</klr></trd>,
        <trd>zagroda</trd>,
        <trd>farma</trd>,
        <trd>maj&aogonek;tek<klr tip="amb"> ziemski</klr></trd>
      </trdgrp>
      <trd lng="pt">fazenda</trd>
      <trd lng="ro">ferm&abreve;</trd>
      <trdgrp lng="ru">
        <trd>&c_i;&c_m;&c_je;&c_n;&c_i;&c_je;</trd>,
        <trd>&c_p;&c_o;&c_m;&c_je;&c_s;&c_t;&c_mol;&c_je;</trd>
      </trdgrp>
      <trdgrp lng="zh">
    <trd>&#x519C;&#x573A; <pr>n&#xF3;ngch&#x1CE;ng</pr></trd>,
    <trd>&#x519C;&#x5E84; <pr>n&#xF3;ngzhu&#x101;ng</pr></trd>,
