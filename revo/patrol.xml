<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>patrol</rad>/o
</kap>
<drv mrk="patrol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Malgranda ta&ccirc;mento da soldatoj a&ubreve; policistoj garde rondiranta.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Patrouille</trd>,
    <trd>Streife</trd>
  </trdgrp>
  <trd lng="nl">patrouille</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_t;&c_r;&c_u;&c_l;&c_mol;</trd>,
    <trd>&c_d;&c_o;&c_z;&c_o;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="patrol.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Iri <tld/>e.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>patrouillieren</trd>,
    <trd>auf <ind>Streife</ind> sein</trd>
  </trdgrp>
  <trd lng="nl">patrouilleren</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_t;&c_r;&c_u;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol; <ind>&c_d;&c_o;&c_z;&c_o;&c_r;&c_o;&c_m;</ind></trd>
  </trdgrp>
</drv>
</art>
