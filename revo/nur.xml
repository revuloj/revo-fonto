  <trd lng="pt">mero</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_d;&c_i;&c_n; &c_t;&c_o;&c_l;&c_mol;&c_k;&c_o;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_o;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
