<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ba.xml,v 1.25 2024/11/29 10:33:16 revo Exp $">
<kap>
  <rad>ba</rad>
  <fnt> <bib>PV</bib></fnt>
</kap>
<drv mrk="ba.0">
  <kap><tld/></kap>
  <gra><vspec>ekkrio</vspec></gra>
  <snc mrk="ba.0.senzorga">
    <dif>
      Ekkrio esprimanta <ref cel="zorg.sen0a">senzorgeco</ref>n,
      <ref cel="indife.0eco">indiferenteco</ref>n:
      <ekz>
        mia kuzino sciis, sed <tld/>
        <fnt><bib>Marta</bib></fnt>!
      </ekz>
      <ekz>
        <tld/>, <tld/>! ni vidas, kian indon ili havas
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>! tio estas nur malplena, supra&jcirc;a maksimo de la hinda filozofo, la&ubreve; kiu li
        mem ne agas
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>, ne gravas
        <fnt><bib>SkandalJozef</bib>, <lok>plendoj de Jozefo
        arestita</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ej1.0">eh</ref>
    </refgrp>
    <trd lng="ca">bah!</trd>
    <trdgrp lng="de">
      <trd>pah!</trd>,
      <trd>papperlapapp</trd>
    </trdgrp>
    <trd lng="en">bah!</trd>
    <trd lng="fr">bah !</trd>
    <trdgrp lng="hu">
      <trd>&aacute;h!</trd>,
      <trd>ugyan!</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>bah</trd>,
      <trd>boh</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>ba!</trd>,
      <trd>e!</trd>
      <trd>e, tam!</trd>
    </trdgrp>
    <trd lng="pt">bah</trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_ju;!</trd>,
      <trd>&c_p;&c_o;&c_d;&c_u;&c_m;&c_a;&c_je;&c_sh;&c_mol;!</trd>
    </trdgrp>
  </snc>
  <snc mrk="ba.0.miro">
    <dif>
      Ekkrio esprimanta <ref cel="surpri.0o">surprizo</ref>n:
      <ekz>
        <tld/>! &ndash; subite blekis Azazello, kaj ekgapis al la &gcirc;ardenkrado, ien montrante
        per la fingro
        <fnt>
          <bib>MkM</bib>
          <lok>19</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ha.0">ha</ref>,
      <ref cel="ups.0">ups</ref>
    </refgrp>
    <trd lng="bg">&c_b;&c_r;&c_je;!</trd>
    <trd lng="ca">oh!</trd>
    <trdgrp lng="cs">
      <trd>bah!</trd>,
      <trd>ech!</trd>,
      <trd>no</trd>,
      <trd>no!</trd>,
      <trd>pch!</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>oh!</trd>,
      <trd>huch!</trd>
    </trdgrp>
    <trd lng="en">whoa!</trd>
    <trdgrp lng="fr">
      <trd>tiens !</trd>,
      <trd>t&eacute; !</trd>,
      <trd>v&eacute; !</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>ha!</trd>,
      <trd>nah&aacute;t!</trd>,
      <trd>nicsak!</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>o!</trd>,
      <trd>ojej!</trd>,
      <trd>&lstroke;a!</trd>
    </trdgrp>
    <trd lng="pt">bah</trd>
    <trd lng="ru">&c_b;&c_a;!</trd>
    <trdgrp lng="zh">
    <trd>&#21908; <pr>&#x14D;</pr></trd>,
    <trd>&#x55EC; <pr>he</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_b;&c_a;!</trd>
  <trdgrp lng="ja">
    <trd>&#12408;&#12387;&#xFF01;</trd>,
    <trd>&#12405;&#12435;&#xFF01;</trd>
  </trdgrp>
  <trd lng="sk">och!</trd>
</drv>
</art>
