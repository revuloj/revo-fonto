<drv mrk="ba.0">
  <kap><tld/></kap>
  <snc mrk="ba.0.miro">
    <dif>
      Ekkrio esprimanta surprizon.
    </dif>
    <trd lng="de">huch!</trd>
    <trd lng="bg">&c_b;&c_r;&c_je;!</trd>
    <trdgrp lng="fr">
      <trd>tiens!</trd>, <trd>t&eacute;! v&eacute;!</trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_a;!</trd>
  </snc>
  <snc mrk="ba.0.senzorga">
    <dif>
      Ekkrio esprimanta senzorgecon, indiferentecon.
    </dif>
    <trdgrp lng="de">
      <trd>pah!</trd>,
      <trd>papperlapapp!</trd>
    </trdgrp>
    <trd lng="fr">bah!</trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_ju;!</trd>,
      <trd>&c_p;&c_o;&c_d;&c_u;&c_m;&c_a;&c_je;&c_sh;&c_mol;!</trd>
    </trdgrp>
  </snc>
</drv>
</art>
