<art mrk="$Id: ba.xml,v 1.19 2015/07/04 08:37:31 revo Exp $">
<kap>
  <rad>ba</rad><fnt>Z</fnt>
</kap>
<drv mrk="ba.0">
  <kap><tld/></kap>
  <snc mrk="ba.0.senzorga">
    <dif>
      Ekkrio esprimanta senzorgecon, indiferentecon:
      <ekz>
        mia kuzino sciis, sed <tld/>
        <fnt><bib>Marta</bib></fnt>!
      </ekz>
      <ekz>
        <tld/>, ne gravas
        <fnt><bib>SkandalJozef</bib>, <lok>plendoj de Jozefo
        arestita</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="ca">bah!</trd>
    <trdgrp lng="de">
      <trd>pah</trd>,
      <trd>papperlapapp</trd>
    </trdgrp>
    <trd lng="en">bah!</trd>
    <trd lng="fr">bah !</trd>
    <trdgrp lng="hu">
      <trd>&aacute;h!</trd>,
      <trd>ugyan!</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>bah</trd>,
      <trd>boh</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>ba!</trd>,
      <trd>e!</trd>
      <trd>e, tam!</trd>
    </trdgrp>
    <trd lng="pt">bah</trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_ju;!</trd>,
      <trd>&c_p;&c_o;&c_d;&c_u;&c_m;&c_a;&c_je;&c_sh;&c_mol;!</trd>
    </trdgrp>
  </snc>
  <snc mrk="ba.0.miro">
    <dif>
      Ekkrio esprimanta surprizon.
    </dif>
    <trd lng="ca">oh!</trd>
    <trd lng="de">huch</trd>
    <trd lng="en">whoa!</trd>
    <trd lng="bg">&c_b;&c_r;&c_je;!</trd>
    <trdgrp lng="fr">
      <trd>tiens !</trd>,
      <trd>t&eacute; !</trd>,
      <trd>v&eacute; !</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>ha!</trd>,
      <trd>nah&aacute;t!</trd>,
      <trd>nicsak!</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>o!</trd>,
      <trd>ojej!</trd>,
      <trd>&lstroke;a!</trd>
    </trdgrp>
    <trd lng="pt">bah</trd>
    <trd lng="ru">&c_b;&c_a;!</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>bah!</trd>,
    <trd>ech!</trd>,
    <trd>no</trd>,
    <trd>no!</trd>,
    <trd>pch!</trd>
  </trdgrp>
  <trd lng="sk">och!</trd>
</drv>
</art>
