<kap>
    <rad>diabetolog</rad>/o
</kap>
<drv mrk="diabetolog.0o">
  <kap><tld/>o</kap>
  <snc mrk="diabetolog.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="endokrinolog.0o">Endokrinologo</ref>, kies
      fako estas <ref tip="vid" cel="diabetologi.0o">diabetologio</ref>:
      <ekz>
        <nom>Csaba L&odblac;rinczi</nom>: internista &ccirc;efkuracisto, <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=188841"
              >Listo de renomaj personoj de Bra&scirc;ovo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_b;&c_e;&c_t;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="cs">diabetolog</trd>
  <trd lng="de">Diabetologe</trd>
  <trd lng="en">diabetologist</trd>
  <trd lng="fr">diab&eacute;tologue</trd>
  <trd lng="sk">diabetol&oacute;g</trd>
</drv>
</art>

