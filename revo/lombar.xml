<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>lombard</rad>/o
</kap>
<drv mrk="lombar.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Monprunto kontra&ubreve; movebla garantia&jcirc;o. <tld/>a kredito.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_m;&c_b;&c_a;&c_r;&c_d;</trd>
  <trdgrp lng="de">
    <trd>Lombard</trd>,
    <trd>Lombardkredit</trd>
  </trdgrp>
  <trd lng="el">&epsilon;&ny;&epsilon_ton;&chi;&ypsilon;&rho;&omikron;</trd>
  <trdgrp lng="en">
    <trd>credit against pledge of chattels</trd>,
    <trd>pawn</trd>,
    <trd>hock</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>lombardhitel</trd>,
    <trd>k&eacute;ziz&aacute;loghitel</trd>
  </trdgrp> 
  <trd lng="nl">onderpand</trd> 
  <trd lng="pt">penhor</trd>
  <trd lng="ru">&c_l;&c_o;&c_m;&c_b;&c_a;&c_r;&c_d;</trd>
</drv>

<drv mrk="lombar.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>IKEV</bib></fnt>
  <snc>
    <dif>
      Monprunti kontra&ubreve; movebla garantia&jcirc;o en <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_k;&c_l;&c_a;&c_d;&c_v;&c_a;&c_c;&c_mol; &c_u; <ind>&c_l;&c_ja;&c_m;&c_b;&c_a;&c_r;&c_d;</ind></trd>
  <trd lng="de">lombardieren</trd>
  <trd lng="el">&kappa;&alfa;&tau;&alfa_ton;&sigma;&chi;&omega;</trd>
  <trdgrp lng="en">
    <trd>pawn</trd>,
    <trd>hock</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>elz&aacute;logos&iacute;t</trd>,
    <trd>z&aacute;logba ad</trd>
  </trdgrp>  
  <trdgrp lng="nl">
    <trd>belenen<klr tip="ind"> (tegen onderpand)</klr></trd>,
    <trd>verpanden</trd>
  </trdgrp>
  <trd lng="pt">penhorar</trd>
  <trd lng="ru">&c_z;&c_a;&c_k;&c_l;&c_a;&c_d;&c_y;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_v;&c_je;&c_shch;&c_i; &c_v; &c_l;&c_o;&c_m;&c_b;&c_a;&c_r;&c_d;)</klr></trd>
</drv>

<drv mrk="lombar.0ejo">
  <kap>
    <tld/>ejo
  </kap>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>IKEV</bib></fnt>
  <snc>
    <dif>
      Ejo, komunuma a&ubreve; privata, kie oni pruntedonas monon kontra&ubreve; movebla garantia&jcirc;o.
    </dif>
  </snc>
  <trd lng="en">pawnshop</trd>
  <trdgrp lng="nl">
    <trd>bank van lening</trd>,
    <trd>pandhuis</trd>,
    <trd>lommerd</trd>
  </trdgrp>
</drv>

<drv mrk="lombar.0isto">
  <kap>
    <tld/>isto
  </kap>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>IKEV</bib></fnt>
  <snc>
    <dif>
      Persono, kiu tenas <tld/>ejon.
    </dif>
  </snc>
  <trd lng="en">pawnbroker</trd>
  <trdgrp lng="nl">
    <trd>pandjesbaas</trd>,
    <trd>lommerdhouder</trd>
  </trdgrp>
</drv>

</art>
