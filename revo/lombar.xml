<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>lombard</rad>/o
</kap>
<drv mrk="lombar.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Monprunto kontra&ubreve; movebla garantia&jcirc;o.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Lombard</trd>,
    <trd>Lombardkredit</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_o;&c_m;&c_b;&c_a;&c_r;&c_d;</trd>
</drv>

<drv mrk="lombar.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Monprunti kontra&ubreve; movebla garantia&jcirc;o en <tld/>o.
    </dif>
  </snc>
  <trd lng="de">lombardieren</trd>
  <trd lng="nl">lenen<klr tip="ind"> (tegen onderpand)</klr></trd>
  <trd lng="ru">&c_z;&c_a;&c_k;&c_l;&c_a;&c_d;&c_y;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_v;&c_je;&c_shch;&c_i; &c_v; &c_l;&c_o;&c_m;&c_b;&c_a;&c_r;&c_d;)</klr></trd>
</drv>
</art>
