<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lombar.xml,v 1.33 2025/12/30 20:35:34 revo Exp $">
<kap>
  <rad>lombard</rad>/o <fnt><bib>PIV3</bib></fnt>
</kap>
<drv mrk="lombar.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="lombar.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
       Ano de <ref tip="vid" cel="lombar.L0oj"><tld/>oj</ref>,
       antikva &gcirc;ermana popolo.
    </dif>
    <trd lng="ca">longobard</trd>
    <trd lng="de">Langobarde</trd>
    <trd lng="en">Lombard <klr>(historical)</klr></trd>
    <trd lng="it">longobardo</trd>
    <trd lng="nl">Lombard</trd>
    <trd lng="pl">Lombard</trd>
    <trd lng="ro">Rezident lombard din Lombardia</trd>
  </snc>

  <snc mrk="lombar.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
       Lo&gcirc;anto de
       <ref tip="vid" cel="lombar.L0ujo">Lombardujo</ref>.
    </dif>
    <trd lng="ca">llombard</trd>
    <trd lng="en">Lombard <klr>(inhabitant of Lombardy)</klr></trd>
    <trd lng="it">lombardo</trd>
    <trd lng="nl">Lombardijer</trd>
    <trd lng="pl">Lombard</trd>
    <trd lng="ro">Rezident lombard din Lombardia</trd>
  </snc>
  <ref tip="hom" cel="lombard.0o">lombardo (kredito)</ref>
  <trdgrp lng="be">
    <trd>&c_l;&c_ja;&c_n;&c_g;&c_a;&c_b;&c_a;&c_r;&c_d;</trd>,
    <trd>&c_l;&c_ja;&c_m;&c_b;&c_a;&c_r;&c_d;</trd>
  </trdgrp>
  <trd lng="fr">Lombard</trd>
  <trd lng="ja">&#x8CEA;&#x5165;&#x308C;  <pr>&#x3057;&#x3061;&#x3044;&#x308C;</pr></trd>
</drv>

<drv mrk="lombar.L0oj">
  <kap>
    <tld/>oj,
    <var><kap>langobardoj</kap></var>
  </kap>

  <snc>
  <uzo tip="fak">HIS</uzo>
    <dif>
       Antikva <ref tip="vid" cel="gxerman.0o">&gcirc;ermana</ref> popolo, kiu
       migrante alvenis la landojn &ccirc;e la rivero <ref tip="vid" cel="elb.E0o.rivero">Elbo</ref> kaj
       poste Panonion,
       Norikon (en 548 p.K.) kaj fine, sub la estro de re&gcirc;o Alboino,
       <ref tip="vid" cel="ital.I0ujo">Italujon</ref> (568 p.K.); tiu
       popolo estas konata pro la karakterizaj longaj
       <ref tip="vid" cel="lanc.0o">lancoj</ref>:
      <ekz>
        la <tld/>oj lasis sian nomon al la okcidenta parto de norda Italio kaj al la nuntempa italia
        regiono Lombardio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18172">Lombardoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="en">Lombards</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ja;&c_n;&c_g;&c_a;&c_b;&c_a;&c_r;&c_d;&c_y;</trd>,
    <trd>&c_l;&c_ja;&c_m;&c_b;&c_a;&c_r;&c_d;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">longobards</trd>
  <trdgrp lng="de">
    <trd>Langobarden</trd>,
    <trd>Winniler</trd>
  </trdgrp>
  <trd lng="fr">Lombards</trd>
  <trd lng="it">Longobardi</trd>
  <trd lng="nl">Lombarden</trd>
  <trd lng="pl">Longobardowie</trd>
  <trd lng="ro">longobarzi</trd>
  <trd lng="uk">&c_l;&c_a;&c_n;&c_g;&c_o;&c_b;&c_a;&c_r;&c_d;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x4F26;&#x5DF4;&#x5E95;&#x4EBA; <pr>l&uacute;nb&amacron;d&#x1D0;r&eacute;n</pr></trd>,
    <trd>&#x502B;&#x5DF4;&#x5E95;&#x4EBA; <pr>l&uacute;nb&amacron;d&#x1D0;r&eacute;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lombar.L0ujo">
  <kap>
    <tld lit="L"/>ujo
  </kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
    <dif>
      Regiono de norda <ref tip="vid" cel="ital.I0ujo">Italujo</ref> kun
      &ccirc;efa urbo <ref tip="prt" cel="milan.M0o">Milano</ref>:
      <ekz>
        <tld lit="L"/>ujo trovi&gcirc;as inter Svislando norde,
        <klr>[...]</klr> Sudtirolo kaj Venetio oriente, Emilio-Romanjo
        sude
        kaj Piemonto okcidente,
        la nomo de la regiono devenas <klr>[...]</klr> de la &gcirc;ermana
        popolo de
        la langobardoj, kies &ccirc;efurbo estis Pavio
        <fnt>
          <bib>Viki</bib>,
          <vrk><url ref="&Viki;/Lombardujo">Lombardujo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="en">Lombardy</trd>
  </snc>
  <trd lng="be">&c_L;&c_ja;&c_m;&c_b;&c_a;&c_r;&c_d;&c_y;&c_ja;</trd>
  <trd lng="ca">Llombardia</trd>
  <trd lng="de">Lombardei</trd>
  <trd lng="fr">Lombardie</trd>
  <trd lng="it">Lombardia</trd>
  <trd lng="nl">Lombardije</trd>
  <trd lng="pl">Lombardia</trd>
  <trd lng="ro">Lombardia</trd>
  <trdgrp lng="zh">
    <trd>&#x4F26;&#x5DF4;&#x7B2C; <pr>l&uacute;nb&amacron;d&igrave;</pr></trd>,
    <trd>&#x502B;&#x5DF4;&#x7B2C; <pr>l&uacute;nb&amacron;d&igrave;</pr></trd>
  </trdgrp>
</drv>

</art>
