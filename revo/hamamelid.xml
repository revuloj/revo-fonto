<vortaro>
<art mrk="$Id$">
<kap>
<rad>hamamelid</rad>/o
</kap>
<drv mrk="hamamelid.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Hamamelido estas malrapide kreskanta arbedo, kiu floras a&ubreve;tune 
      aux vintre.
    </dif>
  </snc>
  <trd lng="de">Zaubernuss</trd>
  <trd lng="en">witch-hazel</trd>
  <trd lng="la">hamamelis</trd>
  <trdgrp lng="pt">
    <trd>hamam&#233;lis</trd>,
    <trd>amieiro-mosqueado <klr tip="amb">(Hamamelis virginiana)</klr></trd>,
    <trd>flor-de-inverno <klr tip="amb">(Hamamelis virginiana)</klr></trd>
  </trdgrp>
</drv>
</art>

