<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hamamelid</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="hamamelid.0o">
  <kap><tld/>o</kap>
  <snc mrk="hamamelid.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Falfolia, malrapide kreskanta arbedo, kiu floras a&ubreve;tune 
      a&ubreve; vintre, el Nordameriko a&ubreve; Orientazio,
      genro el familio hamamelidacoj (<trd lng="la">Hamamelis</trd>).
    </dif>
  </snc>
  <trd lng="de">Zaubernuss</trd>
  <trd lng="en">witch-hazel</trd>
  <trd lng="fr">hamam&eacute;lis</trd>
  <trdgrp lng="pt">
    <trd>hamam&eacute;lis</trd>,
    <trd>amieiro-mosqueado <klr tip="amb">(Hamamelis virginiana)</klr></trd>,
    <trd>flor-de-inverno <klr tip="amb">(Hamamelis virginiana)</klr></trd>
  </trdgrp>
</drv>
</art>
