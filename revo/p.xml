<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>*</ofc><rad>p</rad> <fnt><bib>PIV1</bib></fnt>
</kap>
<subart>
<drv mrk="p.0.konsonanto">
  <kap><tld/></kap>
    <uzo tip="fak">LIN</uzo>
    <uzo tip="fak">GRA</uzo>
    <uzo tip="fak">FON</uzo>
  <snc>    
    <dif>
      <ref tip="dif" cel="konson1.0o">Konsonanto</ref>, senvo&ccirc;a <ref
      tip="dif" cel="plosiv.0o">plosiva</ref> <ref tip="dif"
      cel="labial.0o">labialo</ref> [p].
    </dif>
  <trd lng="fr">p<klr> (consonne)</klr></trd>  
  </snc>
  <snc>
     <dif>
      La dudeka <ref tip="dif" cel="grafem.0o">grafemo</ref> de la
      Esperanta alfabeto, uzata i.a. kiel:
     </dif>
  <trd lng="fr">p<klr> (lettre)</klr></trd>
  <subsnc>
  <uzo tip="fak">KEM</uzo>
  <dif>
  (maj.) simbolo de fosforo: <ekz> <tld lit="P"/></ekz>;
  </dif>
  <trd lng="fr">phosphore</trd>
  </subsnc>
  <subsnc>
  <uzo tip="fak">FIZ</uzo>
  <dif>
  (maj.) simbolo de peta: <ekz> <tld lit="P"/></ekz>;
  </dif>
  <trd lng="fr">p&eacute;ta</trd>
  </subsnc>
  <subsnc>
  <uzo tip="fak">REL</uzo>
  <dif>
  (maj.) mallongigo de Patro: <ekz> <tld lit="P"/></ekz>;
  </dif>
  <trd lng="fr">P&egrave;re<klr> (Dieu le)</klr></trd>
  </subsnc>
  <subsnc>
  <uzo tip="fak">TIP</uzo>
  <dif>
  (min.) mallongigo de pa&gcirc;o: <ekz> <tld/>.</ekz>;
  </dif>
  <trd lng="fr">page<klr> (abr&eacute;viation)</klr></trd>
  </subsnc>
  <subsnc>
  <uzo tip="fak">FIZ</uzo>
  <dif>
  (min.) simbolo de piko: <ekz> <tld/></ekz>;
  </dif>
  <trd lng="fr">pico</trd>
  </subsnc>
  </snc>
  </drv>
</subart>

<subart>
<drv mrk="p.0.nomo">
  <kap><tld/>o</kap>
  <snc> 
    <dif>
      Nomo de tiuj <ref tip="dif" cel="fonem.0o">fonemo</ref> kaj
      grafemo.    
    </dif>
  <trd lng="fr">p</trd>
  </snc>
</drv>
</subart>
</art>

