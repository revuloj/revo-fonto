<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kirgiz</rad>/o
</kap>

<drv mrk="kirgiz.K0ujo">
  <kap>
    <tld lit="K"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="kirgiz.K0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <mlg>KG</mlg>
    <dif>
      &Scirc;tato en la centra Azio kun &ccirc;efurbo Bi&scirc;keko.
      Oficiala nomo: <em>Respubliko de Kirgizujo</em>. &Gcirc;is 1991
      <ref tip="vid" cel="sovet.S0unio">Sovetunia</ref> respubliko.
      &Gcirc;i limas kun <ref tip="vid"
      cel="cxin.CX0ujo">&Ccirc;inujo</ref>,
      <ref tip="vid" cel="kazahx.K0ujo.GEOG">Kaza&hcirc;ujo</ref>,
      <ref tip="vid" cel="tagxik.T0ujo.GEO">Ta&gcirc;ikujo</ref>
      kaj <ref tip="vid" cel="uzbek.U0ujo">Uzbekujo</ref>.
    </dif>
    <trd lng="bg">&c_K;&c_i;&c_r;&c_g;&c_i;&c_z;&c_s;&c_t;&c_a;&c_n;</trd>
    <trdgrp lng="ca">
      <trd>Kirguizistan</trd>,
      <trd>Rep&uacute;blica del Kirguizistan</trd>
    </trdgrp>
    <trd lng="de">Kirgisistan</trd>
    <trd lng="en">Kyrgyzstan</trd>
    <trd lng="hu">Kirgiziszt&aacute;n</trd>
    <trd lng="id">Kirgizstan</trd>
    <trd lng="it">Kirghistan</trd>
    <trdgrp lng="pl">
      <trd>Kirgistan</trd>,
      <trd>Republika <ind>Kirgistanu</ind></trd>
    </trdgrp>
    <trd lng="pt">Quirguist&atilde;o</trd>
  </snc>
</drv>
</art>

