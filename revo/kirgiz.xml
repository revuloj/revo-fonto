<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kirgiz</rad>/o
</kap>

<drv mrk="kirgiz.0ujo">
  <kap>
    <tld lit="K"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="kirgiz.0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <mlg>KG</mlg>
    <dif>
      &Scirc;tato en la centra Azio kun &ccirc;efurbo Bi&scirc;keko.
      Oficiala nomo: <em>Respubliko de Kirgizujo</em>. &Gcirc;is 1991
      <ref tip="vid" cel="sovet.S0unio">Sovetunia</ref> respubliko.
      &Gcirc;i limas kun <ref tip="vid"
      cel="cxin.Cx0ujo">&Ccirc;inujo</ref>,
      <ref tip="vid" cel="kazahx.0ujo.GEOG">Kaza&hcirc;ujo</ref>,
      <ref tip="vid" cel="tagxik.T0ujo.GEO">Ta&gcirc;ikujo</ref>
      kaj <ref tip="vid" cel="uzbek.0ujo">Uzbekujo</ref>.
    </dif>
    <trd lng="it">Kirghistan</trd>
    <trdgrp lng="pl">
      <trd>Kirgistan</trd>,
      <trd>Republika <ind>Kirgistanu</ind></trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="kirgiz.0io">
  <kap>
    <tld lit="K"/>io
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="kirgiz.0io.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <mlg>KG</mlg>
    <dif>
      <ref tip="dif" cel="kirgiz.0ujo.GEOG">Kirgizujo</ref>
    </dif>
    <trd lng="it">Kirghistan</trd>
    <trdgrp lng="pl">
      <trd>Kirgistan</trd>,
      <trd>Republika <ind>Kirgiska</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

