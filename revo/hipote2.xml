<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>hipotez</rad>/o
</kap>
<drv mrk="hipote2.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Nepruvita supozo, provizore akceptata por klarigi kaj komprenigi
      konstatitajn faktojn, sed kontrolota per pluaj eksperimentoj:
      <ekz>
        la ekzisto de <ref cel="eter.0o.FIZ">etero</ref> estis <tld/>o, kiu
        evidenti&gcirc;is mal&gcirc;usta;
      </ekz>
      <ekz>
        scienca <tld/>o plej ofte konsistas en la supozo de nekonata le&gcirc;o,
        de kiu oni devenigas eksperimentotajn konkludojn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="supoz">supozo</ref>,
      <ref cel="konjek.0o">konjekto</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Hypothese</trd>,
    <trd>Annahme</trd>,
    <trd>Vermutung</trd>
  </trdgrp>
  <trd lng="fr">hypoth&egrave;se</trd>
  <trdgrp lng="nl">
    <trd>hypothese</trd>,
    <trd>veronderstelling</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_i;&c_p;&c_o;&c_t;&c_je;&c_z;&c_a;</trd>
  <trd lng="sv">hypotes</trd>
</drv>

<drv mrk="hipote2.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Metode supozi.
    </dif>
  </snc>
  <trd lng="fr">&eacute;mettre des <ind>hypoth&egrave;ses</ind></trd>
  <trd lng="nl">veronderstellen</trd>
  <trd lng="ru">&c_v;&c_y;&c_d;&c_v;&c_i;&c_g;&c_a;&c_t;&c_mol; &c_g;&c_i;&c_p;&c_o;&c_t;&c_je;&c_z;&c_u;</trd>
</drv>
</art>
