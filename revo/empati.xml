<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>empati</rad>/o
</kap>
<drv mrk="empati.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="empati.0o.PSI">
    <uzo tip="fak">PSI</uzo>
    <ref tip="dif" cel="sent.kun0emo.PSI">kunsentemo</ref>
      <ekz>
        al tiu kiu posedas fantazion kaj <tld/>on tre bone eblas vidi 
        aferojn el la vidpunkto de aliaj homoj
        <fnt>
          <aut>M. Sandelin</aut>,
          <vrk><url ref="http://liberafolio.org/2005/Komentario/okuloj">
          &Ccirc;u eblas rigardi per du okuloj?</url></vrk>,
          <lok>libera folio, 2004-11-06</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estas funkcio ne nur ekkona, sed anka&ubreve; inklino
        mov-afekcia
        <fnt>
          <aut>-</aut>,
          <vrk><url
          ref="http://www.grafologiamorettiana.it/eo/apprendimento/elearning/lezioni/la_direzione_assiale">
          Psikologiaj kaj ne&ubreve;rologiaj bazoj pri empatio</url></vrk>,
          <lok>[vidita en 2012]</lok>
        </fnt>.
      </ekz>
  </snc>
</drv>
</art>

