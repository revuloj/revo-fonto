<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>&ccirc;ikan</rad>/i
</kap>
<drv mrk="cxikan.0i">
<kap><ofc>*</ofc><tld/>i</kap>
<gra><vspec>tr</vspec></gra>
<snc num="1">
<dif>
Fari al iu malpravajn kontra&ubreve;a&jcirc;ojn, malhelpojn, malagrabla&jcirc;ojn.  
</dif>
<refgrp tip="vid">
<ref cel="cxagre.0i">&ccirc;agreni</ref>,
<ref cel="turmen.0i">turmenti</ref>.
</refgrp>
</snc>
<snc num="2">
<dif>
Tro subtile kritika&ccirc;i, pedante disputadi. 
</dif>
<refgrp tip="sin">
disfendi <ref cel="har.0o">harojn</ref>,
<ref cel="fend.har0i">harfendi</ref>.
</refgrp>
<adm>
  Enkonduki "harfendi" sub "haro". <aut>MB</aut>
</adm>
</snc>
<trdgrp lng="cs">
  <trd>&scaron;ikanovat</trd>,
  <trd>t&yacute;rat</trd>
</trdgrp>
<trd lng="en">quibble</trd>
<trdgrp lng="fr">
  <trd>chicaner</trd>,
  <trd>ergoter</trd>,
  <trd>chipoter</trd></trdgrp> 
<trdgrp lng="ru">
  <trd>&c_p;&c_r;&c_i;&c_d;&c_i;&c_r;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
  <trd>&c_k;&c_r;&c_ju;&c_ch;&c_k;&c_o;&c_t;&c_v;&c_o;&c_r;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</trdgrp>
</drv>
</art>
