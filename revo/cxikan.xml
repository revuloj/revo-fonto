<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cxikan.xml,v 1.21 2021/05/29 14:32:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&ccirc;ikan</rad>/i
</kap>
<drv mrk="cxikan.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="cxikan.0i.gxeni">
    <dif>
      Fari al iu malpravajn kontra&ubreve;a&jcirc;ojn, malhelpojn, 
      malagrabla&jcirc;ojn:
      <ekz>&ccirc;u foje vi vidis, ke iu <tld/>as kontra&ubreve; ni?
      <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 52a, p.
      177a</lok></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxagre.0i">&ccirc;agreni</ref>,
      <ref cel="turmen.0i">turmenti</ref>.
    </refgrp>
    <trdgrp lng="de">
       <trd>schikanieren</trd>,
       <trd>mobben</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>hacer novatadas</trd>,
      <trd>abusar</trd>
      <trd>jorobar</trd>,
      <trd>fastidiar</trd>,
      <trd>putear<klr> (fastidiar)</klr></trd>,
    </trdgrp>
    <trdgrp lng="hu">
      <trd>zaklat</trd>,
      <trd>bosszant</trd>,
      <trd>piszk&aacute;l</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>infastidire</trd>,
      <trd>tormentare</trd>,
      <trd>vessare</trd>
    </trdgrp>
    <trd lng="pl">szykanowa&cacute;</trd>
  </snc>
  <snc mrk="cxikan.0i.harfendi">
    <dif>
      Tro subtile kritika&ccirc;i, pedante disputadi.
    </dif>
    <refgrp tip="sin">
      disfendi <ref cel="har.0o">harojn</ref>,
      <ref cel="fend.har0i">harfendi</ref>.
    </refgrp>
    <adm>
      Enkonduki "harfendi" sub "haro". <aut>MB</aut>
    </adm>
    <trdgrp lng="es">
      <trd>ser tiquismiquis</trd>,
      <trd>ser quisquilloso</trd>
    </trdgrp>
    <trd lng="hu">okvetetlenkedik</trd>
    <trdgrp lng="it">
      <trd>cavillare</trd>,
      <trd>pignoleggiare</trd>,
      <trd>sottilizzare</trd>
    </trdgrp>
  </snc>
  <trd lng="bo">&#xF66;&#xFA1;&#xF44;&#xF0B;&#xF54;&#xF7C;&#xF0B;&#xF56;&#xFB1;&#xF7A;&#xF51;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>&scaron;ikanovat</trd>,
    <trd>t&yacute;rat</trd>
  </trdgrp>
  <trd lng="en">quibble</trd>
  <trdgrp lng="fr">
    <trd>chicaner</trd>,
    <trd>ergoter</trd>,
    <trd>chipoter</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&#x5DC;&#x5D4;&#x5D8;&#x5E8;&#x5D9;&#x5D3;</trd>,
    <trd>&#x5DC;&#x5D4;&#x5EA;&#x5E2;&#x5DC;&#x5DC;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>atazanar</trd>,
    <trd>chicanar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_d;&c_i;&c_r;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>
&c_k;&c_r;&c_ju;&c_ch;&c_k;&c_o;&c_t;&c_v;&c_o;&c_r;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;
    </trd>
  </trdgrp>
  <trd lng="sk">&scaron;ikanova&tcaron;</trd>
</drv>

<drv mrk="cxikan.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="cxikan.0ema.KOMUNE">
    <dif>
      Havanta inklinon por <tld/>i:
      <ekz>
        male, kun la instruistino ili estas <klr>[&#x2026;] </klr><tld/>emaj
        kaj avaraj
        <fnt>
          <aut>A. Fournier, trad. R. Bernard</aut>,
          <vrk>La Granda Meaulnes</vrk>,
          <lok>p. 148</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd>abus&oacute;n</trd>,
    <trd>quisquilloso</trd>,
    <trd>tiquismiquis</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>chicaneur</trd>,
    <trd>chicanier</trd>
  </trdgrp>
</drv>
</art>
