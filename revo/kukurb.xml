<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>kukurb</rad>/o
</kap>
<drv mrk="kukurb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="kukurb.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Cucurbita</trd>) kun grandaj sukplenaj
      man&gcirc;eblaj bulformaj fruktoj.
    </dif>
  </snc>
  <trd lng="de">K&uuml;rbis</trd>
  <trd lng="fr">courge</trd>
  <trd lng="tr">kabak</trd>
</drv>
<drv mrk="kukurb.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Cucurbitaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">K&uuml;rbisgew&auml;chse</trd>
</drv>
</art>
