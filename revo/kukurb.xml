<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>kukurb</rad>/o
</kap>
<drv mrk="kukurb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="kukurb1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Cucurbita</trd>) kun grandaj sukplenaj
      man&gcirc;eblaj bulformaj fruktoj.
    </dif>
  </snc>
  <trd lng="de">K&uuml;rbis</trd>
  <trd lng="fr">courge</trd>
  <trd lng="tr">kabak</trd>
  <trd lng="nl">pompoen</trd>
</drv>

</art>
