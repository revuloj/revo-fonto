<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kukurb.xml,v 1.36 2023/09/01 15:46:46 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kukurb</rad>/o <fnt><bib>Far1</bib></fnt>
</kap>

<drv mrk="kukurb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kukurb.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Liana vegeta&jcirc;o kun man&gcirc;eblaj fruktoj;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref> el familio
      <ref tip="malprt" cel="kukurb1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Cucurbita</trd>):
      <ekz>
        miaj du infanoj &ccirc;i-jare plantis unuafoje en sia vivo <tld/>ojn,
        kaj ili kun granda atento sekvas ilian evoluon
        <fnt>
          <bib>Monato</bib>
          <aut>Guido van Damme</aut>
          <vrk><url ref="&Monato;/2004/008167.html">Pli
          interese estas kreskigi kukurbojn</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="kukurb.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="frukt.0o.BOT">Frukto<sncref/></ref> de
      <tld/>oplanto
      granda, bulforma, sukplena:
      <ekz>
        vi estas kiel homo, kiu, ser&ccirc;ante <tld/>on sur figarbo, ne vidas
        la figon
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        nun fanfarone li levis la nazon, nazon imponan en form' de
        <tld/>'
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga
          Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la festo koincidas kun la maturi&gcirc;o de <tld/>oj, kiujn oni
        diversmaniere
        uzas: rostitaj a&ubreve; bakitaj por man&gcirc;i, a&ubreve; malplenigitaj kaj de
        infanoj
        &ccirc;izitaj en la formo de teruraspekta kapo, en kiun oni starigas
        kandelon
        (vd <ref tip="vid" cel="haloven.0o">Haloveno</ref>)
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2005/008488.php">Kuk-urbo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_r;&c_b;&c_u;&c_z;</trd>,
    <trd>&c_k;&c_a;&c_b;&c_a;&c_ch;&c_o;&c_k; <klr tip="amb">(&c_g;&c_a;&c_r;&c_o;&c_d;&c_n;&c_ib;&c_n;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="bg">&c_m;&c_i;&c_k;&c_v;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>d&yacute;n&ecaron;</trd>,
    <trd>turek</trd>,
    <trd>tykev</trd>
  </trdgrp>
  <trd lng="de">K&uuml;rbis</trd>
  <trdgrp lng="en">
    <trd>squash</trd>,
    <trd>pumpkin</trd>
  </trdgrp>
  <trd lng="es">calabaza</trd>
  <trd lng="fr">courge</trd>
  <trd lng="hu">t&ouml;k</trd>
  <trd lng="id">labu</trd>
  <trd lng="ja">&#x30AB;&#x30DC;&#x30C1;&#x30E3;</trd>
  <trd lng="nl">pompoen</trd>
  <trd lng="pl">kabaczek</trd>
  <trd lng="ro">dovleac</trd>
  <trd lng="ru">&c_t;&c_y;&c_k;&c_v;&c_a;</trd>
  <trd lng="sk">tekvica jedl&aacute;</trd>
  <trd lng="tr">kabak</trd>
  <trd lng="uk">&c_g;&c_a;&c_r;&c_b;&c_u;&c_z;</trd>
</drv>

<drv mrk="kukurb.0eto">
  <kap>
    <tld/>eto,
    <var><kap>itala <tld/>o</kap></var>,
    <var><kap>italia <tld/>o</kap></var>
  </kap>
  <snc mrk="kukurb.0eto.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Kultivoformo de <tld/>o populara
      en la mediteranea regiono
      (<trd lng="la">Cucurbita pepo pepo</trd>).
    </dif>
  </snc>
  <snc mrk="kukurb.0eto.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Frukto de <tld/>etoplanto plej ofte verda, longforma,
      kiun oni man&gcirc;as nematura:
      <ekz>
        telero da <tld/>etoj je petroselo
        <fnt>
          <bib>Monato</bib>
          <aut>Sen Rodin</aut>
          <vrk><url ref="&Monato;/2006/008905.php">Negro i&gcirc;is
          nigrulo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>eto a&ubreve; italia kukurbo <klr>[&#x2026;]</klr> aspektas simile al
        kukumo,
        krom la frukto, oni anka&ubreve; man&gcirc;as la floron
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=128320">Kukurbeto</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_u;&c_k;&c_ib;&c_n;&c_ib;</trd>
  <trd lng="bg">&c_t;&c_i;&c_k;&c_v;&c_i;&c_ch;&c_k;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Zucchini</trd>,
      <trd>Zucchetti</trd>
    </trdgrp>
  <trd lng="en">zucchini</trd>
  <trd lng="fr">courgette</trd>
  <trd lng="id">zukini</trd>
  <trd lng="ja">&#x30BA;&#x30C3;&#x30AD;&#x30FC;&#x30CB;</trd>
  <trd lng="nl">courgette</trd>
  <trd lng="pl">cukinia</trd>
  <trd lng="ro">dovlecel</trd>
  <trd lng="uk">&#x43A;&c_a;&c_b;&c_a;&c_ch;&#x43E;&#x43A;</trd>
</drv>

</art>
