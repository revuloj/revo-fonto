<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kukurb</rad>/o
</kap>

<drv mrk="kukurb.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="kukurb1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Cucurbita</trd>) kun grandaj sukplenaj
      man&gcirc;eblaj bulformaj fruktoj.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_b;&c_u;&c_z;</trd>
  <trd lng="bg">&c_m;&c_i;&c_k;&c_v;&c_a;</trd>
  <trd lng="de">K&uuml;rbis</trd>
  <trdgrp lng="en">
    <trd>squash</trd>,
    <trd>pumpkin</trd>
  </trdgrp>
  <trd lng="es">calabaza</trd> 
  <trd lng="fr">courge</trd>
  <trd lng="hu">t&ouml;k</trd>
  <trd lng="nl">pompoen</trd>
  <trd lng="pl">kabaczek</trd>
  <trd lng="ru">&c_t;&c_y;&c_k;&c_v;&c_a;</trd>
  <trd lng="tr">kabak</trd>
</drv>

<drv mrk="kukurb.0eto">
  <kap>
    <tld/>eto, <var><kap>itala <tld/>o</kap></var>, <var><kap>italia
    <tld/>o
  </kap></var></kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Kultivoformo de <tld/>o (<trd lng="la">Cucurbita pepo</trd>) tre
      kultivata en la mediteranea regiono kun verda longforma frukto, 
      ofte man&gcirc;ata juna.
    </dif>
  </snc>
  <trd lng="bg">&c_t;&c_i;&c_k;&c_v;&c_i;&c_ch;&c_k;&c_a;</trd>
  <trd lng="de">Zucchino</trd>
  <trd lng="en">zucchini</trd>
  <trd lng="nl">courgette</trd>
  <trd lng="pl">cukinia</trd>
</drv>

</art>
