<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>silk</rad>/o
</kap>
<drv mrk="silk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Delikata kaj brila fadeno sekreciita de iu speco de ra&ubreve;po.
    </dif>
    <refgrp tip="vid">
      <ref cel="kokon.0o">kokono</ref>, 
      <ref cel="bombik.0o">bombikso</ref>.
    </refgrp>
  </snc>
  <snc>
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Teksa&jcirc;o farita el tiaj fadenoj:
      <ekz>
        vivi en <tld/>o kaj veluro, en &gcirc;ojo kaj plezuro
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
      <ref cel="atlas.0o.TEKS">atlaso</ref>.
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_sh;&c_o;&c_w;&c_k;</trd>, 
    <trd>&c_ja;&c_d;&c_v;&c_a;&c_b;</trd>
  </trdgrp>
  <trd lng="de">Seide</trd>
  <trd lng="es">seda</trd>
  <trd lng="hu">selyem</trd>  
  <trd lng="nl">zijde</trd>
  <trd lng="ru">&c_sh;&c_jo;&c_l;&c_k;</trd>
</drv>

<drv mrk="silk.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Farita el <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_a;&c_w;&c_k;&c_o;&c_v;&c_y;</trd>, 
    <trd>&c_ja;&c_d;&c_v;&c_a;&c_b;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">seiden</trd>
  <trd lng="hu">selyem-</trd>  
  <trd lng="nl">zijden</trd>
  <trd lng="ru">&c_sh;&c_jo;&c_l;&c_k;&c_o;&c_v;&c_y;&c_j;</trd>
</drv>

<drv mrk="silk.0eca">
  <kap><tld/>eca</kap>
  <snc>
    <dif>
      Havanta kelkajn ecojn, la aspekton de <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_a;&c_w;&c_k;&c_a;&c_v;&c_ib;&c_s;&c_t;&c_y;</trd>,
    <trd>&c_sh;&c_o;&c_w;&c_k;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">seidig</trd>
  <trd lng="es">sedoso</trd>
  <trd lng="hu">selymes</trd>  
  <trd lng="nl">zijdeachtig</trd>
  <trd lng="ru">&c_sh;&c_je;&c_l;&c_k;&c_o;&c_v;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>
</drv>
</art>
