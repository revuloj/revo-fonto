<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: silk.xml,v 1.25 2023/03/08 20:28:31 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>silk</rad>/o
</kap>
<drv mrk="silk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Delikata kaj brila fadeno sekreciita de iu speco de ra&ubreve;po:
      <ekz>
        ili starigis du teksilojn <klr>[&#8230;]</klr> kaj postuladis la plej delikatan <tld/>on kaj la plej
        bonan oron
        <fnt>
          <bib>Fab1</bib>
          <lok>La novaj vestoj de la re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i montris la&ccirc;on plektitan el multkolora <tld/>o
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;uoj broditaj per <tld/>o 
        <fnt>
          <bib>ElektFab</bib>
          <lok>Cindrulino</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kokon.0o">kokono</ref>,
      <ref cel="bombik.0o">bombikso</ref>
    </refgrp>
  </snc>
  <snc>
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="teks.0ajxo.TEKS">Teksa&jcirc;o</ref> farita el tiaj fadenoj:
      <ekz>
        viaj vestoj estis el bisino, <tld/>o, kaj brodita&jcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 16:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia frako estas el <tld/>o, sed estas ne eble diri precize, kian koloron &gcirc;i havas
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        amasoj da &scirc;tofoj disvolvataj kaj kunvolvataj kun sen&ccirc;esa susurado de la <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        robo el batik-desegna <tld/>o
        <fnt>
          <bib>ChM</bib>
          <lok>10</lok>
        </fnt>;
      </ekz>
      <ekz>
        vivi en <tld/>o kaj veluro, en &gcirc;ojo kaj plezuro
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>  
    <refgrp tip="vid">
      <ref cel="atlas1.0o.TEKS">atlaso</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_o;&c_w;&c_k;</trd>,
    <trd>&c_ja;&c_d;&c_v;&c_a;&c_b;</trd>
  </trdgrp>
  <trd lng="de">Seide</trd>
  <trd lng="en">silk</trd>
  <trd lng="es">seda</trd>
  <trd lng="fr">soie</trd>
  <trd lng="hu">selyem</trd>
  <trd lng="nl">zijde</trd>
  <trd lng="pl">jedwab</trd>
  <trd lng="pt">seda</trd>
  <trd lng="ru">&c_sh;&c_jo;&c_l;&c_k;</trd>
  <trd lng="sk">hodv&aacute;b</trd>
  <trd lng="cs">hedv&aacute;b&iacute;</trd>
  <trd lng="uk">&c_sh;&c_o;&c_v;&c_k;</trd>
</drv>

<drv mrk="silk.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Farita el <tld/>o:
      <ekz>
        mi kovris vin per <tld/>a kovrotuko
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 16:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i prenis sian grandan oran tondilon, distran&ccirc;is grandan pecon da <tld/>a &scirc;tofo
        kaj kudris malgrandan beletan saketon
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> enligis &gcirc;in en sian <tld/>an po&scirc;tukon
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        sofoj kun <tld/>a tego
        <fnt>
          <bib>Fab2</bib>
          <lok>Abio</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni povis rigardi en la plej belegajn salonojn, kie pendis multekostaj <tld/>aj
        kurtenoj kaj tapi&scirc;oj
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la butikisto prenis siajn valorajn paperojn kaj la servistino sian silkan
        mantilon, kiun &scirc;iaj rimedoj permesis al &scirc;i a&ccirc;eti
        <fnt>
          <bib>Fab3</bib>
          <lok>Koboldo &ccirc;e la butikisto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_a;&c_w;&c_k;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_ja;&c_d;&c_v;&c_a;&c_b;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">seiden</trd>
  <trdgrp lng="fr">
    <trd>en soie</trd>,
    <trd>de soie</trd>
  </trdgrp>
  <trd lng="hu">selyem-</trd>
  <trd lng="nl">zijden</trd>
  <trd lng="pl">jedwabny</trd>
  <trd lng="ru">&c_sh;&c_jo;&c_l;&c_k;&c_o;&c_v;&c_y;&c_j;</trd>
  <trd lng="uk">&c_sh;&c_o;&c_v;&c_k;&c_o;&c_v;&c_i;&c_j;</trd>
</drv>

<drv mrk="silk.0eca">
  <kap><tld/>eca</kap>
  <snc>
    <dif>
      Havanta kelkajn ecojn, la aspekton de <tld/>o:
      <ekz>
        <tld/>ecaj nigraj haroj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;arma vesto el <tld/>eca kaj bele surbrodita &scirc;tofo
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bril.0a">brila</ref>,
      <ref cel="glace.0a">glacea</ref>,
      <ref cel="muar.0a">muara</ref>,
      <ref cel="ond.0obrila">ondobrila</ref>,
      <ref cel="saten.0o">satena</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_a;&c_w;&c_k;&c_a;&c_v;&c_ib;&c_s;&c_t;&c_y;</trd>,
    <trd>&c_sh;&c_o;&c_w;&c_k;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">seidig</trd>
  <trd lng="es">sedoso</trd>
  <trd lng="fr">soyeux</trd>
  <trd lng="hu">selymes</trd>
  <trd lng="nl">zijdeachtig</trd>
  <trd lng="pl">jedwabisty</trd>
  <trd lng="ru">&c_sh;&c_je;&c_l;&c_k;&c_o;&c_v;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>
  <trd lng="uk">&c_sh;&c_o;&c_v;&c_k;&c_o;&c_v;&c_i;&c_s;&c_t;&c_i;&c_j;</trd>
</drv>
</art>
