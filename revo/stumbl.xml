<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>stumbl</rad>/i
</kap>
<drv mrk="stumbl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <refgrp tip="dif">
      <ref cel="fal.0eti">faleti, duonfali</ref>.
    </refgrp>
  </snc>
  <trd lng="de">stolpern</trd>
  <trdgrp lng="en">
    <trd>stumble</trd>,
    <trd>trip</trd>,
    <trd>misstep</trd>,
    <trd>miss one's step</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>tr&eacute;bucher</trd>,
    <trd>broncher<klr tip="ind"> (tr&eacute;bucher)</klr></trd>
  </trdgrp>
  <trd lng="nl">struikelen</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_o;&c_t;&c_k;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_o;&c_s;&c_t;&c_u;&c_p;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="stumbl.0o">
  <kap><tld/>o</kap>
  <snc mrk="stumbl.0o.KOMUNE">
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <snc mrk="stumbl.0o.licenco">
    <uzo tip="fak">POE</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      Metrika <ref tip="super" cel="licenc1.0o">licenco</ref>, pro kiu oni
      toleras anstata&ubreve;igon de tri <ref cel="jamb.0o">jamboj</ref>
      per du <ref cel="anapes.0o">anapestoj</ref>, a&ubreve; de tri
      <ref cel="troke.0o">trokeoj</ref> per du
      <ref cel="amfibr.0o">amfibra&hcirc;oj</ref>:
      <ekz>
        <ctl>&iacute;ru, &eacute;ble h&eacute;jmon pli t&aacute;&ubreve;gan
        vi tr&oacute;vos</ctl><fnt>K</fnt>
        <klr>(en <ref cel="troke.n0o">kvintrokeo</ref>)</klr>;
      </ekz>
      <ekz>
        &ccirc;u ne &scirc;ajnus pli natura skandi <klr>[tiun verson]</klr>
        per <tld/>o?
        <fnt><aut>&K;, Waringhien, Bernard</aut><vrk>Parnasa Gvidlibro;
        Pisa, 1984</vrk><lok>p. 24</lok></fnt>.
      </ekz>
    </dif>
  </snc>
</drv>
</art>
