<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: badahxoz.xml,v 1.6 2024/04/20 21:20:31 revo Exp $">
<kap>
  <rad>Bada&hcirc;oz</rad>/o <fnt><vrk>Pekoteko</vrk> <lok>1994</lok></fnt>
</kap>
<drv mrk="badahxoz.0o">
  <kap><tld/>o</kap>
  <snc mrk="badahxoz.0o.GEOG_provinco">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="super" cel="provin.0o">Provinco</ref> de <ref
      tip="malprt" cel="hispan.H0ujo">Hispanujo</ref> en la a&ubreve;tonoma
      regiono Ekstremaduro.
    </dif>
  </snc>
  <snc mrk="badahxoz.0o.GEOG_urbo">
    <dif>
      &Ccirc;ef<ref tip="lst" lst="voko:urboj" cel="urb.0o">urbo</ref> de la samnoma hispana
      provinco:
      <ekz>
        en a&ubreve;gusto en <tld/>o avera&gcirc;e la temperaturo estas 25,9 gradoj
        celsiaj kaj la pluvo nur aperas malpli ol unu tago kutime
        <fnt>
          <lok><url ref="http://ijk2018.tejo.org/la-urbo/">IJK 2018:
          Bada&hcirc;ozo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_B;&c_a;&c_d;&c_a;&c_h;&c_o;&c_s;</trd>
  <trd lng="ca">Badajoz</trd>
  <trd lng="de">Badajoz</trd>
  <trd lng="en">Badajoz</trd>
  <trd lng="es">Badajoz</trd>
  <trd lng="eu">Badajoz</trd>
  <trd lng="fr">Badajoz</trd>
  <trd lng="gl">Badaxoz</trd>
  <trd lng="nl">Badajoz</trd>
  <trd lng="pt">Badajoz</trd>
  <trd lng="ru">&c_B;&c_a;&c_d;&c_a;&c_h;&c_o;&c_s;</trd>
</drv>
</art>

