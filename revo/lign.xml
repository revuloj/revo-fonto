<drv mrk="lign.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Malmola <ref cel="substa.0o.FIZ">substanco</ref>,
      el kiu konsistas radikoj, trunko kaj bran&ccirc;oj de
      <ref tip="malprt" cel="arb.0o.BOT">arbo</ref>j,
      uzata diverscele kiel <ref tip="super" cel="materi1.0o.substanco">materialo</ref>:
      <ekz>
        la <tld/>isto vendas <tld/>on, kaj la <tld/>a&jcirc;isto faras tablojn, se&gcirc;ojn kaj
        aliajn <tld/>ajn objektojn
        <fnt>
          <bib>F</bib>
          <lok>&FE; 37</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estas bona brula materialo
        <fnt>
          <bib>F</bib>
          <lok>&FE; 31</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni iros en la arbaron haki <tld/>on
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ha&ccirc;jo kaj Grenjo</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili faru keston el akacia <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 25:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        faru al vi arkeon el <tld/>o gofera
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 6:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        parfuma <tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 18:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        li portis bastonegon el mahagona <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <klr>[&#x2026;]</klr> havis sur sia dorso 
        faskon da <ind>brulo<tld/>o</ind>, kiun &scirc;i estis
        kolektinta en la arbaro
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
        <trd lng="ca">llenya</trd>
        <trd lng="de">Brennholz</trd>
        <trd lng="fa">&a_h;&f_y;&a_z;&a_m;</trd>
        <trd lng="hu">t&uuml;zifa</trd>
      </ekz>
      <ekz>
        la tuta koridoro estis prilumita per <ind>putra <tld/>o</ind>
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
        <trd lng="de"><ind>morsch</ind>es Holz</trd>
      </ekz>
      <ekz>
        <klr>[li]</klr> montris se&gcirc;ojn, tablojn <klr>[&#x2026;]</klr> ornamitajn per ri&ccirc;aj
        desegna&jcirc;oj, inkrustitajn per diverskolora <tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estas flami&gcirc;ema kiel rezina <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        fendita <tld/>o facile ekbrulas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne el &ccirc;iu <tld/>o oni faras violonon
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxarpe.0i">&ccirc;arpenti</ref>,
      <ref cel="hak.0i.trancxi">haki<sncref/></ref>,
      <ref cel="rabot.0i">raboti</ref>,
      <ref cel="seg.0i">segi</ref>,
      <ref cel="karb.ligno0o">lignokarbo</ref>,
      <ref cel="lignit.0o">lignito</ref>
    </refgrp>
  </snc>
  <snc mrk="lign.0o.peco">
    <dif>
      Peco da tia materialo:
      <ekz>
        de la &scirc;ipo ne restis e&ccirc; unu <tld/>eto
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i prenis ankora&ubreve; el malanta&ubreve; la forno faskon da <tld/>etoj kaj ekbruligis sur
        la kameno gajan fajron
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Iftah</nom> kun tor&ccirc;o brulanta aliras / Kaj fajron al <tld/>oj aldonas, / La <tld/>oj
        ne volas kaj kontra&ubreve; sin metas, / Sed <nom>Iftah</nom> obstinas kaj pajlon en&jcirc;etas
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la popolo &scirc;in kondamnis al forbruligo sur <tld/>aro
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la mezo de la halo videblis granda hejtilo, eble anta&ubreve; nelonge plenigita per
        <tld/>oj, bruligitaj, kaj nun la hejtilo bruegis kiel vaporlokomotivo
        <fnt>
          <bib>Monato</bib>
          <aut>Julian Modest</aut>
          <vrk><url ref="&Monato;/2013/010962.php">La iluziisto</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        li kuntiri&gcirc;is, &ccirc;ar la planko<tld/>oj knaris
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>En la Lando de la Noktuloj</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxind.0o">&scirc;indo</ref>,
      <ref cel="sxtip.0o.KOMUNE">&scirc;tipo<sncref/></ref>,
      <ref cel="tabul.0o">tabulo</ref>,
      <ref cel="trab.0o">trabo</ref>
    </refgrp>
    <trd lng="de">Holzscheit</trd>
  </snc>
   <trdgrp lng="be">
     <trd>&c_d;&c_r;&c_e;&c_v;&c_a; <klr>
     (&c_m;&c_a;&c_t;&c_a;&c_r;'&c_ja;&c_l;)</klr></trd>,
     <trd>&c_d;&c_r;&c_a;&c_w;&c_n;&c_ib;&c_n;&c_a;</trd>
   </trdgrp>
   <trd lng="bg">&c_d;&c_malmol;&c_r;&c_v;&c_o;</trd>
   <trd lng="bo">&#xF64;&#xF72;&#xF44;&#xF0B;</trd>
  <trd lng="ca">fusta</trd>
   <trd lng="cs">d&rcaron;evo</trd>
   <trd lng="de">Holz</trd>
   <trd lng="el">&xi;&ypsilon_ton;&lambda;&omikron;</trd>
   <trd lng="en">wood</trd>
   <trdgrp lng="es">
     <trd>madera</trd>,
     <trd>le&ntilde;o</trd>
   </trdgrp>
   <trd lng="fa">&f_ch;&a_w;&a_b;</trd>
   <trd lng="fr">bois</trd>
   <trd lng="hu">fa</trd>   
   <trd lng="id">kayu</trd>
   <trdgrp lng="ja">
    <trd>&#x6728;  <pr>&#x3082;&#x304F;</pr></trd>,
    <trd>&#x6728;&#x6750;  <pr>&#x3082;&#x304F;&#x3056;&#x3044;</pr></trd>,
    <trd>&#x6750;&#x6728;  <pr>&#x3056;&#x3044;&#x3082;&#x304F;</pr></trd>,
    <trd>&#x6728;&#x8CEA;  <pr>&#x3082;&#x304F;&#x3057;&#x3064;</pr></trd>
  </trdgrp>
  <trd lng="nl">hout</trd>
   <trd lng="pl">drewno</trd>
   <trdgrp lng="pt">
     <trd>madeira</trd>,
     <trd>lenha</trd>
   </trdgrp>
   <trd lng="ro">lemn</trd>
   <trdgrp lng="ru">
     <trd>&c_d;&c_r;&c_je;&c_v;&c_je;&c_s;&c_i;&c_n;&c_a;</trd>,
     <trd>&c_d;&c_je;&c_r;&c_je;&c_v;&c_o; <klr tip="amb">
     (&c_m;&c_a;&c_t;&c_je;&c_r;&c_i;&c_a;&c_l;)</klr></trd>
   </trdgrp>
  <trd lng="tok">kasi</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_je;&c_r;&c_je;&c_v;&c_i;&#x43D;&c_a;</trd>,
    <trd>&c_d;&c_je;&c_r;&c_je;&c_v;&#x43E;</trd>,
    <trd>&#x43B;&c_ib;&c_s;&#x43E;&#x43C;&c_a;&c_t;&c_je;&c_r;&c_ib;&c_a;&#x43B;</trd>,
    <trd>&#x43F;&c_i;&#x43B;&#x43E;&#x43C;&c_a;&c_t;&c_je;&c_r;&c_ib;&c_a;&#x43B;</trd>,
    <trd>&c_b;&c_r;&c_u;&c_s;</trd>,
    <trd>&c_b;&c_a;&#x43B;&#x43A;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6728;&#x6750; <pr>m&#xF9;c&#xE1;i</pr></trd>,
    <trd>&#x6728;&#x5934; <pr>m&#xF9;tou</pr></trd>,
    <trd>&#x6728;&#x982D; <pr>m&#xF9;tou</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lign.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      El <tld/>o:
      <ekz>
        &ccirc;iun <tld/>an vazon oni lavu per akvo
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 15:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        faru al vi <tld/>an keston
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 10:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        vidu, mi lo&gcirc;as en domo cedro<tld/>a
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 7:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        la skribisto <nom>Ezra</nom> staris sur <tld/>a ambono
        <fnt>
          <bib>MT</bib>
          <lok>Ne&hcirc;emja 8:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a altaro havis la alton de tri ulnoj
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 41:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        li pritran&ccirc;is grandan <tld/>an krucon, por meti &gcirc;in sur la tombon de sia patro
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i metis sur siajn piedojn <tld/>ajn pantoflojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tisto de porkoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;armaj <tld/>aj pupoj kun vitraj okuloj
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_r;&c_a;&c_w;&c_l;&c_ja;&c_n;&c_y;</trd>,
    <trd>&c_d;&c_r;&c_a;&c_w;&c_n;&c_ja;&c_n;&c_y;</trd>,
    <trd>&c_d;&c_r;&c_a;&c_w;&c_n;&c_ib;&c_n;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">h&ouml;lzern</trd>
  <trd lng="en">wooden</trd>
  <trd lng="fr">en bois</trd>
  <trd lng="ja">&#x6728;&#x88FD;&#x306E;  <pr>&#x3082;&#x304F;&#x305B;&#x3044;&#x306E;</pr></trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_je;&c_r;&c_je;&c_v;&#x2019;&#x44F;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_d;&c_je;&c_r;&c_je;&c_v;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_z; &c_d;&c_je;&c_r;&c_je;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6728;&#x5236; <pr>m&#xF9;zh&#xEC;</pr></trd>,
    <trd>&#x6728;&#x88FD; <pr>m&#xF9;zh&#xEC;</pr></trd>,
    <trd>&#x6728;&#x8CEA; <pr>m&#xF9;zh&#xEC;</pr></trd>,
    <trd>&#x6728;&#x8D28; <pr>m&#xF9;zh&#xEC;</pr></trd>
  </trdgrp>
</drv>


<drv mrk="lign.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc>
    <dif>
      Objekto, konstrua&jcirc;o, framo el <tld/>o:
      <ekz>
        lin oni mortigis, pendigante lin sur <tld/>a&jcirc;o
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 10:39</lok>
        </fnt>;
      </ekz>
      <ekz>
        la meblaj kovriloj estis el blua atlaso, kaj la <tld/>a&jcirc;o de la se&gcirc;oj estis
        origita
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> artistoj <klr>[&#x2026;]</klr> ekspoziciis interalie ceramika&jcirc;ojn kaj
        <tld/>a&jcirc;ojn
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2011/010405.php">Bruas la mezepoko</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;oforo asertas, ke li transportis <tld/>a&jcirc;on kaj ke pri ka&scirc;itaj cigaredoj li
        nenion sciis
       <fnt>
	<aut>Laimius Stra&zcaron;nickas</aut>
	<vrk><url ref="&Monato;/2000/006830.php">Kontrabanditaj cigaredoj invadas Brition</url></vrk>
	<bib>Monato</bib>
	<lok>2000/04, p. 13</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_a;&c_w;&c_l;&c_ja;&c_n;&c_y; &c_v;&c_y;&c_r;&c_a;&c_b;</trd>
  <trd lng="ja">&#x6728;&#x88FD;&#x54C1;  <pr>&#x3082;&#x304F;&#x305B;&#x3044;&#x3072;&#x3093;</pr></trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_je;&c_r;&c_je;&c_v;&#x2019;&#x44F;&#x43D;&c_i;&c_j; &c_v;&c_i;&c_r;&c_ib;&c_b;</trd>,
    <trd>&c_v;&c_i;&c_r;&c_ib;&c_b; &c_z; &c_d;&c_je;&c_r;&c_je;&c_v;&c_a;</trd>
  </trdgrp>
</drv>


<drv mrk="lign.0ero">
  <kap>
    <tld/>ero
  </kap>
  <snc>
    <dif>
      Malgranda a&ubreve; ne tro granda peco de ligno:
      <ekz>
        lasu min eltiri la <tld/>ereton el via okulo
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 7:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel fajro, kiu bruligas <tld/>erojn, kiel akvo, kiu bolas de fajro
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 64:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la litbenkoj trovi&gcirc;is <tld/>eroj, bonegaj por ku&scirc;i sur ili
        <fnt>
          <bib>VRA</bib>
        </fnt>;
      </ekz>
      <ekz>
        la junaj knabinoj gaje &jcirc;etas pezajn <tld/>erojn dum volontula komsomola
        ripoztaglaboro
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>hakado de ligno donas <tld/>erojn</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
        <trd lng="de">wo gehobelt wird, da fallen Sp&auml;ne</trd>
      </ekz>
      <ekz>
        <ind>en fremda okulo ni vidas <tld/>eron, en nia ni trabon ne vidas</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
        <trd lng="de">man sieht den <ind>Splitter</ind> im fremden Auge, im eignen den Balken
        nicht</trd>
        <trd lng="id">bintang di langit dapat dibilang, tetapi arang di muka tidak sadar</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="ken.0o">keno</ref>,
       <ref cel="rabot.0ajxo">rabota&jcirc;o</ref>,
       <ref cel="split.0o.ligna">splito</ref>,
       <ref cel="tig.0o">tigo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_e;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_c;&c_jo;&c_s;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Span</trd>,
    <trd>Splitter</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6728;&#x304F;&#x305A;  <pr>&#x304D;&#x304F;&#x305A;</pr></trd>,
    <trd>&#x6728;&#x3063;&#x7AEF;  <pr>&#x3053;&#x3063;&#x3071;</pr></trd>
  </trdgrp>
  <trd lng="uk">&c_t;&c_r;&c_ib;&c_s;&#x43A;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x788E;&#x7247; <pr>su&#xEC;pi&#xE0;n</pr></trd>,
    <trd>&#x7D30;&#x8584;&#x7247; <pr>x&#xEC;b&#xF3;pi&#xE0;n</pr></trd>,
    <trd>&#x7EC6;&#x8584;&#x7247; <pr>x&#xEC;b&#xF3;pi&#xE0;n</pr></trd>,
    <trd>&#x524A;&#x5C51; <pr>xi&#x101;oxi&#xE8;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lign.0ajxisto">
  <kap>
    <tld/>a&jcirc;isto
  </kap>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:metioj" cel="meti.0isto">Metiisto</ref>, kiu prilaboras <tld/>ajn
      objektojn:
      <ekz>
        restis &ccirc;e la patrino du miaj pli junaj fratoj, el kiuj unu lernoservas &ccirc;e
        <tld/>a&jcirc;isto, kaj la dua vizitas lernejon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la sekvinta tago la <tld/>a&jcirc;isto pagis por la nokto, nenion rimarkinte prenis
        la tablon kaj foriris
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tablo, la orazeno kaj bastono el sako</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi petas fajron, tornilon kaj <tld/>a&jcirc;istan stablon kun tran&ccirc;ilo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fabelo pri iu, kiu migris por ekkoni timon</lok>
        </fnt>;
      </ekz>
      <ekz>
        post ili venis la granda korporacio de <tld/>a&jcirc;istoj, kiu montris se&gcirc;ojn,
        tablojn, kanapojn, portilojn kaj veturilojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>.
      </ekz>
     </dif>
    <refgrp tip="vid">
      <ref cel="barel.0isto">barelisto</ref>,
      <ref cel="cxarpe.0isto">&ccirc;arpentisto</ref>,
      <ref cel="mebl.0isto">meblisto</ref>
    </refgrp>
   </snc>
   <trdgrp lng="be">
     <trd>&c_d;&c_r;&c_e;&c_v;&c_a;&c_a;&c_p;&c_r;&c_a;&c_c;&c_o;&c_w;&c_sh;&c_ch;&c_y;&c_k;</trd>,
     <trd>&c_c;&c_ja;&c_s;&c_mol;&c_l;&c_ja;&c_r;</trd>,
     <trd>&c_s;&c_t;&c_a;&c_l;&c_ja;&c_r;</trd>
   </trdgrp>
   <trd lng="bg">&c_d;&c_malmol;&c_r;&c_v;&c_o;&c_d;&c_je;&c_l;&c_je;&c_c; &c_m;&c_je;&c_b;&c_je;&c_l;&c_i;&c_s;&c_t;</trd>
   <trd lng="ca">fuster</trd>
   <trd lng="de">Schreiner</trd>
   <trd lng="el">&xi;&ypsilon;&lambda;&omikron;&ypsilon;&rho;&gamma;&omikron_ton;&sigma_fina;</trd>
   <trd lng="es">carpintero</trd>
