<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lign</rad>/o
</kap>
<drv mrk="lign.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Malmola substanco el kiu konsistas radikoj, trunko kaj bran&ccirc;oj de
      arboj:
      <ekz>
        brul<tld/>o;
      </ekz>
      <ekz>
        seg<tld/>o;
      </ekz>
      <ekz>
        konstru<tld/>o;
      </ekz>
      <ekz>
        <tld/>a benko, &scirc;uo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_r;&c_e;&c_v;&c_a; <klr>(&c_m;&c_a;&c_t;&c_a;&c_r;'&c_ja;&c_l;)</klr></trd>,
    <trd>&c_d;&c_r;&c_a;&c_w;&c_n;&c_ib;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">d&rcaron;evo</trd>
  <trd lng="de">Holz</trd>
  <trd lng="el">&xi;&ypsilon_ton;&lambda;&omikron;</trd>
  <trd lng="en">wood</trd>
  <trdgrp lng="es">
    <trd>madera</trd>,
    <trd>le&ntilde;o</trd>
  </trdgrp>
  <trd lng="fr">bois</trd>
  <trd lng="nl">hout</trd>
  <trdgrp lng="pt">
    <trd>madeira</trd>,
    <trd>lenha</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_r;&c_je;&c_v;&c_je;&c_s;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_d;&c_je;&c_r;&c_je;&c_v;&c_o; <klr tip="amb">(&c_m;&c_a;&c_t;&c_je;&c_r;&c_i;&c_a;&c_l;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="lign.0ajxisto">
  <kap>
    <tld/>a&jcirc;isto
  </kap>
  <snc>
    <dif>
      Metiisto, kiu prilaboras <tld/>ajn objektojn.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_r;&c_e;&c_v;&c_a;&c_a;&c_p;&c_r;&c_a;&c_c;&c_o;&c_w;&c_sh;&c_ch;&c_y;&c_k;</trd>,
    <trd>&c_c;&c_ja;&c_s;&c_mol;&c_l;&c_ja;&c_r;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_l;&c_ja;&c_r;</trd>
  </trdgrp>
  <trd lng="de">Schreiner</trd>
  <trd lng="el">&xi;&ypsilon;&lambda;&omikron;&ypsilon;&rho;&gamma;&omikron_ton;&sigma_fina;</trd>
  <trd lng="es">carpintero</trd>
  <trd lng="fr">menuisier</trd>
  <trd lng="nl">schrijnwerker</trd>
  <trd lng="pt">carpinteiro</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;&c_o;&c_b;&c_d;&c_je;&c_l;&c_o;&c_ch;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;&c_o;&c_b;&c_r;&c_a;&c_b;&c_o;&c_t;&c_ch;&c_i;&c_k;</trd>,
    <trd>&c_p;&c_l;&c_o;&c_t;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_l;&c_ja;&c_r;</trd>
  </trdgrp>
</drv>
</art>
