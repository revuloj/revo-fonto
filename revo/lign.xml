<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lign</rad>/o
</kap>
<drv mrk="lign.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Malmola substanco el kiu konsistas radikoj, trunko kaj bran&ccirc;oj de
      arboj:
      <ekz>
        brul<tld/>o;
      </ekz>
      <ekz>
        seg<tld/>o;
      </ekz>
      <ekz>
        konstru<tld/>o;
      </ekz>
      <ekz>
        <tld/>a benko, &scirc;uo.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">d&rcaron;evo</trd>
  <trd lng="en">wood</trd>
  <trdgrp lng="es">
    <trd>madera</trd>,
    <trd>le&ntilde;o</trd>
  </trdgrp>
  <trd lng="nl">hout</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_r;&c_je;&c_v;&c_je;&c_s;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_d;&c_je;&c_r;&c_je;&c_v;&c_o; <klr tip="amb">(&c_m;&c_a;&c_t;&c_je;&c_r;&c_i;&c_a;&c_l;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="lign.0ajxisto">
  <kap>
    <tld/>a&jcirc;isto
  </kap>
  <snc>
    <dif>
      Metiisto, kiu prilaboras <tld/>ajn objektojn.
    </dif>
  </snc>
  <trd lng="es">carpintero</trd>
  <trd lng="nl">schrijnwerker</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;&c_o;&c_b;&c_d;&c_je;&c_l;&c_o;&c_ch;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;&c_o;&c_b;&c_r;&c_a;&c_b;&c_o;&c_t;&c_ch;&c_i;&c_k;</trd>,
    <trd>&c_p;&c_l;&c_o;&c_t;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_l;&c_ja;&c_r;</trd>
  </trdgrp>
</drv>
</art>
