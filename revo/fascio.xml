<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>fasciol</rad>/o
</kap>
<drv mrk="fascio.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la ordo <ref tip="super" cel="tremat.0oj">trematodoj</ref>
      (<trd lng="la">Fasciola</trd>), parazita platvermo, kiu vivas en la
      hepato de brutoj kaj homoj.
    </dif>
  </snc>
  <trd lng="de">Leberegel</trd>
  <trdgrp lng="fr">
    <trd>douve<klr tip="ind"> (ver)</klr></trd>,
    <trd>fasciole</trd></trdgrp> 
  <trd lng="hu">m&aacute;jm&eacute;tely</trd>
  <trd lng="nl">leverbot</trd>
  <trdgrp lng="pt">
    <trd>fasc&#237;ola</trd>,
    <trd>barata-do-f&#237;gado</trd>,
    <trd>baratinha-do-f&#237;gado</trd>,
    <trd>d&#250;via</trd>,
    <trd>saguaip&#233;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_l;&c_o;&c_s;&c_k;&c_i;&c_j; &c_g;&c_l;&c_i;&c_s;&c_t;</trd>
</drv>
</art>
