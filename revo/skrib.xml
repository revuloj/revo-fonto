<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>skrib</rad>/i
</kap>
<drv mrk="skrib.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Figuri vortojn per konvenciaj desegnoj a&ubreve; literoj:
      <ekz>
        li <tld/>as kiel kokino <klr>(tute nelegeble)</klr>;
      </ekz>
      <ekz>
        <tld/>i literojn, nulojn;
      </ekz>
      <ekz>
        li scipovas nek legi, nek <tld/>i;
      </ekz>
      <ekz>
        la tablo, sur kiu oni <tld/>as;
      </ekz>
      <ekz>
        viaj leteroj estas <tld/>itaj tute nelegeble<fnt>Z</fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>schreiben</trd>
  </trdgrp>
  <snc>
    <dif>
      Esprimi pensojn per tiaj rimedoj:
      <ekz>
        <tld/>i leteron<fnt>B</fnt>, romanon<fnt>B</fnt>, en gazeto;
      </ekz>
      <ekz>
        verse <tld/>ita rakonto;
      </ekz>
      <ekz>
        altaron, sur kiu estis <tld/>ite: Al Dio Nekonata;
      </ekz>
      <ekz>
        kiel estas <tld/>ite <klr>(en la Biblio)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sur &scirc;ia viza&jcirc;o estas <tld/>ita (montri&gcirc;as, evidenti&gcirc;as) mildeco;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Mi <tld/>os Mian le&gcirc;on sur ilia koro<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>schreiben</trd>,
      <trd>verfassen</trd>,
      <trd>niederlegen</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Registri, decidi:
      <ekz>
        Vi <tld/>as kontra&ubreve; mi maldol&ccirc;a&jcirc;on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        respondis Pilato: kion mi <tld/>is, tion mi <tld/>is
        <klr>(ne&scirc;an&gcirc;eble)</klr>;
      </ekz>
      <ekz>
        ve al tiuj, kiuj <tld/>as maljustajn verdiktojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        estis <tld/>ite <klr>(de la fatalo)</klr>.
      </ekz>
    </dif>
    <trdgrp lng="de">
     <trd>schreiben</trd>,
     <trd>verfassen</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Sciigi al iu per <tld/>a&jcirc;o:
      <ekz>
        mi <tld/>os al li, ke mi deturnas de li mian manon, ke li
        neniam venu anta&ubreve; miaj okuloj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
     <trd>schreiben</trd>,
     <trd>schriftlich <ind>kund tun</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="skrib.0i.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Sendi datumon el pli alta nivelo de la memorhierarkio al malpli
      alta (periferia), ekz-e el la &ccirc;efmemoro sur diskon.
    </dif>
    <trdgrp lng="de">
     <trd>schreiben</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">ps&aacute;t</trd>
  <trd lng="en">write</trd>
  <trd lng="fr">&eacute;crire</trd>
  <trd lng="nl">schrijven</trd>
  <trd lng="ru">&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="skrib.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i.
    </dif>
    <trd lng="de">Schreiben</trd>
  </snc>
  <snc>
    <dif>
      Maniero <tld/>i.
    </dif>
  </snc>
  <trd lng="de">Schreibweise</trd>
  <snc>
    <dif>
      Io <tld/>ita:
      <ekz>
        diron oni neas, <tld/>o ne pereas<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">REL</uzo>
        la <ind>Sankta <tld lit="S"/>o</ind><fnt>Z</fnt> <klr>(la Biblio)</klr>.
        <trd lng="fr"><ind>&Eacute;criture</ind> Sainte</trd>
        <trd lng="ru">&c_s;&c_v;&c_ja;&c_shch;&c_je;&c_n;&c_n;&c_o;&c_je; &c_p;&c_i;&c_s;&c_a;&c_n;&c_i;&c_je;</trd>
      </ekz>
    </dif>
    <trd lng="de">Schrift</trd>
    <trdgrp lng="ru">
      <trd>&c_n;&c_a;&c_p;&c_i;&c_s;&c_a;&c_n;&c_n;&c_o;&c_je; <klr>(&c_v; &c_z;&c_n;&c_a;&c_ch;. &c_s;&c_u;&c_shch;.)</klr></trd>
      <trd>&c_z;&c_a;&c_p;&c_i;&c_s;&c_mol;</trd>,
      <trd>&c_n;&c_a;&c_d;&c_p;&c_i;&c_s;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>&eacute;criture</trd>,
    <trd>&eacute;crit<klr tip="ind"> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="nl">schrift</trd>
  <trd lng="ru">&c_p;&c_i;&c_s;&c_a;&c_n;&c_i;&c_je;</trd>
</drv>
<drv mrk="skrib.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="S"/>ita, per<tld/>a:
      <ekz>
        <tld/>a literaturo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ni faris la kontrakton ne <tld/>e sed parole<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">schriftlich</trd>
  <trd lng="fr">&eacute;crit<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="nl">schriftelijk</trd>
  <trd lng="ru">&c_p;&c_i;&c_s;&c_mol;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>
<drv mrk="skrib.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <trdgrp lng="de">
     <trd>Schreiben</trd>,
     <trd>Geschriebenes</trd>
  </trdgrp>
</drv>
<drv mrk="skrib.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      = skribo&ccirc;ambro.
    </dif>
  </snc>
  <snc>
    <dif>
      = skriboficejo.
    </dif>
  </snc>
</drv>
<drv mrk="skrib.0eraro">
  <kap><tld/>eraro</kap>
  <trdgrp lng="de">
     <trd>Schreibfehler</trd>
  </trdgrp>
</drv>
<drv mrk="skrib.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Homo, kies profesio estas <tld/>i a&ubreve; trans<tld/>i.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Schreiber</trd>,
     <trd>Berufsschreiber</trd>,
     <trd>Schriftsteller</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>scribe</trd>,
    <trd>copiste</trd>,
    <trd>&eacute;crivain public</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_i;&c_s;&c_je;&c_c;</trd>,
    <trd>&c_p;&c_i;&c_s;&c_a;&c_r;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="skrib.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      &Ccirc;io, kio servas por <tld/>i.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Stift</trd>,
     <trd>Schreibger&auml;t</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>crayon</trd>,
    <trd>stylo</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>pen</trd>,
    <trd>potlood</trd>,
    <trd>schrijfbenodigdheid</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_r;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o; &c_p;&c_i;&c_s;&c_mol;&c_m;&c_a;</trd>
</drv>
<drv mrk="skrib.0ilaro">
  <kap><tld/>ilaro</kap>
  <ekz>
    <tld/>ilaro konsistas el inkujo, sablujo, plumoj, krajono
    kaj inksorbilo<fnt>Z</fnt>.
  </ekz>
  <trdgrp lng="de">
     <trd>Schreibzeug</trd>
  </trdgrp>
</drv>
<drv mrk="skrib.0maniero">
  <kap><tld/>maniero</kap>
  <trdgrp lng="de">
     <trd>Schreibweise</trd>
  </trdgrp>
</drv>
<drv mrk="skrib.0masxino">
  <kap><tld/>ma&scirc;ino</kap>
  <trdgrp lng="de">
     <trd>Schreibmaschine</trd>
  </trdgrp>
</drv>
<drv mrk="skrib.0ocxambro">
  <kap><tld/>o&ccirc;ambro</kap>
  <trdgrp lng="de">
     <trd>Schreibzimmer</trd>,
     <trd>Schreibstube</trd>
  </trdgrp>
</drv>
<drv mrk="skrib.0oficejo">
  <kap><tld/>oficejo</kap>
  <trdgrp lng="de">
     <trd>Schreibb&uuml;ro</trd>
  </trdgrp>
</drv>
<drv mrk="skrib.0tablo">
  <kap><tld/>tablo</kap>
  <trdgrp lng="de">
     <trd>Schreibtisch</trd>
  </trdgrp>
</drv>
<drv mrk="skrib.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="skrib.cxirkaux0i.KOMUNE">
    <dif>
      Marki limojn &ccirc;ika&ubreve; io; enfermi en limojn:
      <ekz>
        &ccirc;irka&ubreve;<tld/>i temon, ser&ccirc;on.
      </ekz>
    </dif>
  </snc>
  <snc mrk="skrib.cxirkaux0i.MAT">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="klr">(fakula &jcirc;argono)</uzo>
    <dif>
      Trovi <ref cel="figur.geometria0o">geometrian figuron</ref>
      desegneblan &ccirc;irka&ubreve; dua, tiel ke la dua estu
      <ref cel="skrib.en0ita">en<tld/>ita</ref> en la unua:
      <ekz>
        &ccirc;ika&ubreve;<tld/>i kubon &ccirc;irka&ubreve; sfero.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">umschreiben</trd>
  <trd lng="fr">circonscrire</trd>
  <trd lng="ru">&c_o;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="skrib.cxirkaux0ita">
  <kap>&ccirc;irka&ubreve;<tld/>ita</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(p.p.
      <ref cel="figur.geometria0o">geometria figuro</ref> rilate
      al alia)</klr>
      Tia, ke la dua figuro estas <ref cel="skrib.en0ita">en<tld/>ita</ref>
      en la unua.
    </dif>
  </snc>
  <trd lng="de">umschrieben</trd>
  <trd lng="fr">circonscrit<klr tip="ind"> (en p. d'une figure dans une autre)</klr></trd>
  <trd lng="ru">&c_o;&c_p;&c_i;&c_s;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>
<drv mrk="skrib.en0i">
  <kap>en<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="skrib.en0i.KOMUNE">
    <dif>
      <tld lit="S"/>e noti ion inter sama&jcirc;oj (
      <ref tip="vid" cel="regist.0i">registri</ref>):
      <ekz>
        tiun &ccirc;i sumon mi en<tld/>is en vian kalkulon<fnt>Z</fnt>,
        en vian krediton<fnt>Z</fnt>, en vian &scirc;uldon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        en Via libro estis en<tld/>itaj &ccirc;iuj tagoj
        detruitaj<fnt>Z</fnt> <klr>(pasintaj)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        glore en<tld/>i sian nomon en la historion<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gravur.0i">gravuri</ref>,
      <ref cel="stamp.0i">stampi</ref>,
      <ref cel="desegn.0i">desegni</ref>,
      <ref cel="redakt.0i">redakti</ref>,
      <ref cel="formul.0i">formuli</ref>.
    </refgrp>
    <trdgrp lng="de">
     <trd>einschreiben</trd>,
     <trd>eintragen</trd>
    </trdgrp>
    <trd lng="nl">bijschrijven</trd>
  </snc>
  <snc mrk="skrib.en0i.MAT">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="klr">(fakula &jcirc;argono)</uzo>
    <dif>
      Trovi la plej grandan
      <ref cel="figur.geometria0o">geometrian figuron</ref> de unu tipo,
      kiun eblas desegni ene de figuro de dua tipo:
      <ekz>
        en<tld/>i plurlateron en cirklo(n), sferon en piramido(n).
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="skrib.cxirkaux0i">&ccirc;ikra&ubreve;<tld/>i</ref>.
    </refgrp>
    <rim>
      Por doni precizan difinon necesas konsideri la du apartajn
      tipojn de figuro. Vd
      <refgrp tip="vid">
        <ref cel="skrib.en0ita">en<tld/>ita</ref>.
      </refgrp>
    </rim>
    <trdgrp lng="de">
     <trd>einbeschreiben</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">inscrire</trd>
  <trd lng="ru">&c_v;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="skrib.en0ita">
  <kap>en<tld/>ita</kap>
  <snc mrk="skrib.en0ita.encirklosfero">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <klr>(p.p. <ref cel="later.plur0o">plurlatero</ref> rilate al
      <ref cel="cirkl.0o.linio">cirklo</ref> a&ubreve;
      p.p. <ref cel="edr.plur0o">pluredro</ref> rilate al
      <ref cel="sfer.0o.surfaco">sfero</ref>)</klr>
      Tia, ke &ccirc;iuj &gcirc;iaj verticoj situas sur la cirklo, a&ubreve;
      sur la sfero:
      <ekz>
        se ekzistas cirklo, en kiu plurlatero estas en<tld/>ita,
        la plurlateron oni kvalifikas <ind>en<tld/>ebla</ind>;
        <trd lng="fr">inscriptible<klr tip="ind">
        (en p. d'un polygone ou poly&egrave;dre)</klr></trd>
      </ekz>
      <ekz>
        &ccirc;iuj verticoj de en<tld/>ebla pluredro situas egaldistance
        al certa punkto, kiu estas la centro de sfero, en kiu &gcirc;i estas
        en<tld/>ita.
      </ekz>
    </dif>
  </snc>
  <snc mrk="skrib.en0ita.enplurlateropluredro">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <klr>(p.p. <ref cel="cirkl.0o.linio">cirklo</ref> rilate al
      <ref cel="later.plur0o">plurlatero</ref> a&ubreve; p.p.
      <ref cel="sfer.0o.surfaco">sfero</ref> rilate al
      <ref cel="edr.plur0o">pluredro</ref>)</klr>
      Tia, ke &gcirc;i tan&gcirc;as &ccirc;iujn laterojn de la plurlatero,
      a&ubreve; &ccirc;iujn facojn de la pluredro:
      <ekz>
        por &ccirc;iu triangulo kaj por &ccirc;iu regula plurlatero
        ekzistas en<tld/>ita cirklo;
      </ekz>
      <ekz>
        por &ccirc;iu kvaredro kaj por &ccirc;iu regula pluredro
        ekzistas en<tld/>ita sfero.
      </ekz>
    </dif>
  </snc>
  <snc mrk="skrib.en0ita.enkonusocilindro">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <klr>(p.p. <ref cel="sfer.0o.surfaco">sfero</ref> rilate al
      <ref cel="konus.0o.surfaco">konuso</ref> a&ubreve;
      <ref cel="cilind.0o.surfaco">cilindro</ref>)</klr>
      Tia, ke &gcirc;i tan&gcirc;as &ccirc;iujn
      <ref cel="nask.0anto.rekto">naskantojn</ref> de la dua figuro.
    </dif>
  </snc>
    <trdgrp lng="de">
     <trd>einbeschrieben</trd>
    </trdgrp>
  <trd lng="fr">inscrit<klr tip="ind"> (en p. d'une figure dans une autre)</klr></trd>
  <trd lng="ru">&c_v;&c_p;&c_i;&c_s;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>
<drv mrk="skrib.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Distinge kaj vive prezenti ion per vortoj, tiamaniere, ke la
      a&ubreve;danto a&ubreve; leganto povas &gcirc;in sufi&ccirc;e precize 
      imagi.
    </dif>
    <refgrp tip="vid">
      <ref cel="pentr.0i">pentri</ref>,
      <ref cel="desegn.0i">desegni</ref>,
      <ref cel="skiz.0i">skizi</ref>,
      <ref cel="rakont.0i">rakonti</ref>.
    </refgrp>
  </snc>
    <trdgrp lng="de">
     <trd>beschreiben</trd>
    </trdgrp>
  <trd lng="fr">d&eacute;crire</trd>
  <trdgrp lng="nl">
    <trd>beschrijven</trd>,
    <trd>omschrijven</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="skrib.sub0i">
  <kap>sub<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="S"/>i sian nomon sub io por montri sian a&ubreve;torecon
      a&ubreve; sian aprobon.
    </dif>
    <refgrp tip="vid">
      <ref cel="paraf.0o">parafo</ref>.
    </refgrp>
  </snc>
    <trdgrp lng="de">
     <trd>unterschreiben</trd>,
     <trd>unterzeichnen</trd>
    </trdgrp>
  <trdgrp lng="fr">
    <trd>signer<klr tip="ind"> (par ex. un contrat)</klr></trd>,
    <trd>souscrire</trd>
  </trdgrp>
  <trd lng="nl">ondertekenen</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="skrib.sub0o">
  <kap>sub<tld/>o</kap>
  <snc>
    <trdgrp lng="de">
     <trd>Unterschrift</trd>,
     <trd>Signatur</trd>
    </trdgrp>
  </snc>
</drv>
<drv mrk="skrib.sur0o">
  <kap>sur<tld/>o</kap>
  <snc>
    <trdgrp lng="de">
     <trd>Aufschrift</trd>
    </trdgrp>
  </snc>
</drv>
<drv mrk="skrib.trans0i">
  <kap>trans<tld/>i</kap>
  <snc>
    <trdgrp lng="de">
     <trd>&uuml;bertragen</trd>,
     <trd>umschreiben</trd>
    </trdgrp>
  </snc>
  <snc>
    <ekz>
        Transskribi la &ccirc;inan en la latinan skribon.
    </ekz>
    <trdgrp lng="de">
     <trd>transskribieren</trd>,
    </trdgrp>
  </snc>
</drv>

</art>
