<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gana</rad>/o
</kap>

<drv mrk="gana.0o">
  <kap>
    <tld lit="G"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="gana.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la nordokcidenta <ref tip="malprt"
      cel="afrik.A0o">Afriko</ref>
      kun &ccirc;efurbo Akro.
      Teritorio de Respubliko Ganao.
      ISO-3166 kodo: GH.
      &Gcirc;i limas kun
      <ref tip="vid" cel="burkin.0o.GEOG">Burkino</ref>,
      <ref tip="vid" cel="ebur.0bordo.GEOG">Ebur-Bordo</ref>,
      <ref tip="vid" cel="togo.0olando.GEOG">Togolando</ref>
      kaj <ref tip="vid" cel="atlant.0o.GEOG">Atlantiko</ref> (Gvinea 
      Golfo).
    </dif>
    <trdgrp lng="pl">
      <trd>Ghana</trd>
      <trd>Republika <ind>Ghany</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

