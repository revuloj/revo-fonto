<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: stenotajp.xml,v 1.5 2023/10/17 21:24:41 revo Exp $">
<kap>
  <rad>stenotajp</rad>/i <fnt><vrk><url
  ref="http://www.de.majstro.com/vortaroj/esperanto-hispana/stenotajpi">Multlingva
  vortaro</url></vrk></fnt>
</kap>
<drv mrk="stenotajp.0i">
  <kap><tld/>i</kap>
  <snc mrk="stenotajp.0i.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Skribi, <ref tip="super" cel="stenog.0i.TIP">stenografi</ref>
      per <tld/>ilo;
      <ref tip="sin" cel="stenot.0i.TIP">stenotipi</ref>:
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_e;&c_n;&c_a;&c_t;&c_y;&c_p;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">escriure amb <ind>estenot&iacute;pia</ind></trd>
  <trd lng="en">stenotype</trd>
  <trd lng="fr">st&eacute;notyper</trd>
  <trdgrp lng="hu">
    <trd>gyorsg&eacute;p&iacute;r</trd>,
    <trd>gyors&iacute;r&oacute;g&eacute;ppel &iacute;r</trd>
  </trdgrp>
  <trd lng="pl">pisa&cacute; na maszynie  <ind>stenograficznej</ind></trd>
</drv>

<drv mrk="stenotajp.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="stenotajp.0ilo.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Stenografma&scirc;ino;
      <ref tip="sin" cel="stenot.0o.TIP">stenotipo</ref>:
      <ekz>
        ili starigas ekspedicion, kun filmaparato,
        magnetofonoj, <tld/>iloj kaj tiel plu
        <fnt>
          <aut>G. Waringhien</aut>,
          <vrk>Lingvo kaj Vivo</vrk>,
          <lok>1959</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_e;&c_n;&c_a;&c_t;&c_y;&c_p;</trd>
  <trd lng="ca">estenot&iacute;pia<klr> (m&agrave;quina)</klr></trd>
  <trd lng="de">Kurzschriftmaschine</trd>
  <trd lng="en">stenotype</trd>
  <trd lng="fr">st&eacute;notype</trd>
  <trd lng="hu">gyors&iacute;r&oacute;g&eacute;p</trd>
  <trd lng="nl">stenomachine</trd>
  <trd lng="pl">maszyna  <ind>stenograficzna</ind></trd>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_n;&c_o;&c_t;&c_i;&c_p;</trd>
</drv>

<drv mrk="stenotajp.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="stenotajp.0isto.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Homo profesie <tld/>anta;
      <ref tip="sin" cel="stenot.0isto.TIP">stenotipisto</ref>:
      <ekz>
        mi estas &ccirc;irka&ubreve;<klr>a</klr>ta de <tld/>istinoj stultaj!
        <fnt>
          <aut>trad. senlime</aut>,
          <vrk><url ref="http://www.senlime.be/Kino/Diktator.htm">
          La Granda Diktatoro</url></vrk>,
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_e;&c_n;&c_a;&c_t;&c_y;&c_p;&c_ib;&c_s;&c_t;</trd>
  <trd lng="ca">estenotipista</trd>
  <trd lng="en">stenotypist</trd>
  <trd lng="fr">st&eacute;notypiste</trd>
  <trdgrp lng="hu">
    <trd>gyorsg&eacute;p&iacute;r&oacute;</trd>,
    <trd>g&eacute;pi gyors&iacute;r&oacute;</trd>
  </trdgrp>
  <trd lng="nl">stenotypist</trd>
  <trd lng="pl">stenotypista</trd>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_n;&c_o;&c_t;&c_i;&c_p;&c_i;&c_s;&c_t;</trd>
</drv>
</art>

