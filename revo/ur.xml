<art mrk="$Id: ur.xml,v 1.25 2024/03/27 01:21:58 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>ur</rad>/o
</kap>
<drv mrk="ur.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
       Sova&gcirc;a e&ubreve;ropa bovo, el la familio <ref tip="super" cel="boved.0oj">bovedoj</ref> (<trd
       lng="la">Bos
       primigenius</trd>)
       malaperinta en la 18a jarcento:
       <ekz>
         arbaregoj, kie kuras &gcirc;is nun sova&gcirc;aj <tld/>bovoj
         <fnt><bib>VivZam</bib> <lok>&ccirc;apitro 1a, p. 18a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
  <trdgrp lng="ja">
    <trd>&#x30AA;&#x30FC;&#x30ED;&#x30C3;&#x30AF;&#x30B9;</trd>,
    <trd>&#x539F;&#x725B;  <pr>&#12370;&#12435;&#x304E;&#12421;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="lat">Bos urus</trd>
  <trd lng="be">&c_z;&c_u;&c_b;&c_a;&c_r;</trd>
  <trd lng="ca">ur</trd>
  <trd lng="cs">pratur</trd>
  <trdgrp lng="de">
     <trd>Ur</trd>,
     <trd>Auerochse</trd>
   </trdgrp>
  <trd lng="en">aurochs</trd>
  <trd lng="es">uro</trd>
  <trd lng="fi">alkuh&auml;rk&auml;</trd>
  <trd lng="fr">aurochs</trd>
  <trd lng="hu">&odblac;stulok</trd>
  <trd lng="it">uro</trd>
  <trd lng="nl">oeros</trd>
  <trd lng="pl">tur</trd>
  <trd lng="pt">auroque</trd>
  <trd lng="ru">&c_z;&c_u;&c_b;&c_r;</trd>
  <trd lng="sk">zubor eur&oacute;psky</trd>
  <trd lng="uk">&c_z;&c_u;&c_b;&c_r;</trd>
</drv>
</art>
