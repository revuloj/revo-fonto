<drv mrk="hauxt.0ero">
  <kap><tld/>ero</kap>
  <snc mrk="hauxt.0ero.ANA">
    <dif>
      Skvameto, kiu elfalas el la <tld/>o, plej ofte el la kap<tld/>o:
      <ekz>
        veston oni konstante malpurigas kaj kadukigas <klr>[&#8230;]</klr> per falintaj
        <tld/>eroj, per &scirc;vito, per frotado
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=8320"
              >Vesto</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_sh;&c_u;&c_t;&c_a;</trd>
  <trd lng="br">kenn<klr tip="ind"> (kroc'hen)</klr></trd>
  <trd lng="de"><klr tip="ind">(Haut-)</klr>Schuppe</trd>
  <trdgrp lng="en">
    <trd>skin flake</trd>,
    <trd>dandruff <klr tip="ind">(using -j)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
   <trd>descamaci&oacute;n<klr tip="amb"> (de la piel)</klr></trd>, 
   <trd>caspa</trd> 
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&f_p;&a_w;&a_s;&a_t;&a_h;</trd>,
    <trd>&a_sh;&a_w;&a_r;&a_h;&a_hamza_sure; &a_s;&a_r;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>squame</trd>,
    <trd>pellicule<klr tip="ind"> (squame)</klr></trd>
  </trdgrp> 
  <trd lng="hu">korpa<klr tip="ind">(b&odblac;r-)</klr></trd>
  <trdgrp lng="it">
    <trd>scaglia<klr> (di pelle)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#30382;&#33178;&#29255; [<ind>&#12402;&#12405;&#12408;&#12435;</ind>]</trd>,
    <trd>&#12405;&#12369;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>huidschilfer</trd>,
    <trd>schilfer<klr tip="ind"> (op huid)</klr></trd>,
    <trd>roos<klr tip="ind"> (op hoofd)</klr></trd>
  </trdgrp>
  <trd lng="pl">&lstroke;upie&zdot;</trd>
  <trdgrp lng="pt">
   <trd>descama&ccedil;&atilde;o<klr tip="amb"> (de pele)</klr></trd>, 
   <trd>caspa</trd> 
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_h;&c_o;&c_t;&c_mol;</trd>
</drv>

<drv mrk="hauxt.sen0igi">
  <kap>sen<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="hauxt.sen0igi.KOMUNE">
    <dif>
      Senigi je la <tld/>o:
      <ekz>
        se vi min denuncus, mi lasus sen<tld/>igi min viva
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekdua</lok>
        </fnt>;
      </ekz>
      <ekz>
        se &scirc;i venos al Alma-Ato, mi unge sen<tld/>igos vian viza&gcirc;on, mi mordos vian
        &scirc;ultron &gcirc;issange
        <fnt>
          <bib>CKv</bib>
          <lok>7</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ni]</klr> ne estas entrepreno por sen<tld/>igi bestojn
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2006/008917.php"
              >Sigloj</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        senlanigu, sed ne sen<tld/>igu
        <klr>(uzu, sed ne tro uzu)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_d;&c_y;&c_m;&c_a;&c_c;&c_mol; <ind>&c_s;&c_k;&c_u;&c_r;&c_u;</ind></trd>,
    <trd>&c_s;&c_d;&c_y;&c_m;&c_a;&c_c;&c_mol; <ind>&c_s;&c_k;&c_a;&c_l;&c_mol;&c_p;</ind></trd>,
    <trd>&c_s;&c_k;&c_a;&c_l;&c_mol;&c_p;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">digroc'hena&ntilde;</trd>
  <trdgrp lng="de">
    <trd>enth&auml;uten</trd>,
    <trd>skalpieren <klr>(kap<tld/>on)</klr></trd>
  </trdgrp>
  <trd lng="en">skin</trd>
  <trdgrp lng="es">
   <trd>pelar</trd>, 
   <trd>despellejar</trd> 
  </trdgrp>
  <trd lng="fa">&f_p;&a_w;&a_s;&a_t; &f_k;&a_n;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;pecer</trd>,
    <trd>d&eacute;piauter</trd>,
    <trd>d&eacute;pouiller<klr tip="ind"> (d&eacute;pecer)</klr></trd>,
    <trd>&eacute;corcher</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megny&uacute;z</trd>,
    <trd>megskalpol</trd>
  </trdgrp>
  <trd lng="io">senpeligar</trd>
  <trdgrp lng="it">
    <trd>spellare</trd>,
    <trd>scuoiare</trd>,
    <trd>scotennare</trd>,
    <trd>scorticare</trd>,
    <trd>pelare</trd>,
    <trd>sbucciare</trd>
  </trdgrp>
  <trd lng="ja">&#30382;&#12434;&#21093;&#12368;
  [<ind>&#12399;&#12431;&#12434;&#12399;&#12368;</ind>]</trd>
  <trd lng="ru">&c_s;&c_n;&c_ja;&c_t;&c_mol; &c_k;&c_o;&c_zh;&c_u;</trd>
  <trdgrp lng="nl">
    <trd>villen</trd>,
    <trd>scalperen</trd>
  </trdgrp>
  <trd lng="pl">skalpowa&cacute;</trd>
  <trdgrp lng="pt">
   <trd>pelar</trd>, 
   <trd>tirar a <ind>pele</ind></trd> 
  </trdgrp>
  <trd lng="sv">fl&aring;</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_d;&c_i;&c_r;&c_a;&c_t;&c_i; &c_sh;&c_k;&c_ib;&c_r;&c_u;</trd>,
    <trd>&c_z;&c_n;&c_ib;&c_m;&c_a;&c_t;&c_i; &c_sh;&c_k;&c_u;&c_r;&c_u;</trd>,
    <trd>&c_b;&c_ib;&c_l;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="hauxt.anser0o">
  <kap>anser<tld/>o</kap>
  <snc mrk="hauxt.anser0o.FRAZ">
    <uzo tip="stl">FRAZ</uzo>
    <dif>
      Hirti&gcirc;o de la sur<tld/>aj haretoj,
      kiu vidigas veruketojn similajn al
      tiuj de senplumigita birdo:
      <ekz>
        li eksentis na&ubreve;zon tian, ke li ekhavis anser<tld/>on
        <fnt>
          <aut>Hevesi L., trad. Vizi L.</aut>,
          <vrk><url ref="https://sites.google.com/site/laaventurojdeandreojelky">
          La aventuroj de Andreo Jelky</url></vrk>,
          <lok>[2010]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">G&auml;nsehaut</trd>
  <trd lng="en">goose bumps</trd>
  <trd lng="es">piel de gallina</trd>
  <trd lng="fa">&a_m;&a_w; &a_sh;&a_q;&f_y;</trd>
  <trd lng="fr">chair de poule</trd>
  <trd lng="it">pelle d'oca</trd>
  <trd lng="ja">&#40165;&#32908; [<ind>&#12392;&#12426;&#12399;&#12384;</ind>]</trd>
  <trd lng="nl">kippenvel</trd>
  <trd lng="sv">g&aring;shud</trd>
</drv>
