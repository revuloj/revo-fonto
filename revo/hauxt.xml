   <trd>caspa</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_h;&c_o;&c_t;&c_mol;</trd>
</drv>

<drv mrk="hauxt.sen0igi">
  <kap>sen<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="hauxt.sen0igi.KOMUNE">
    <dif>
      Senigi je la <tld/>o:
      <ekz>
        se vi min denuncus, mi lasus sen<tld/>igi min viva
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekdua</lok>
        </fnt>;
      </ekz><ekz>
        se &scirc;i venos al Alma-Ato, mi unge sen<tld/>igos vian viza&gcirc;on, mi mordos vian
        &scirc;ultron &gcirc;issange
        <fnt><bib>CKv</bib> <lok>7</lok></fnt>;
      </ekz><ekz>
        <klr>[ni]</klr> ne estas entrepreno por sen<tld/>igi bestojn
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2006/008917.php">Sigloj</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        senlanigu, sed ne sen<tld/>igu
        <klr>(uzu, sed ne tro uzu)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_d;&c_y;&c_m;&c_a;&c_c;&c_mol; <ind>&c_s;&c_k;&c_u;&c_r;&c_u;</ind></trd>,
    <trd>&c_s;&c_d;&c_y;&c_m;&c_a;&c_c;&c_mol; <ind>&c_s;&c_k;&c_a;&c_l;&c_mol;&c_p;</ind></trd>,
    <trd>&c_s;&c_k;&c_a;&c_l;&c_mol;&c_p;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">digroc'hena&ntilde;</trd>
  <trdgrp lng="de">
    <trd>enth&auml;uten</trd>,
    <trd>skalpieren <klr>(kap<tld/>on)</klr></trd>
  </trdgrp>
  <trd lng="en">skin</trd>
  <trdgrp lng="es">
   <trd>pelar</trd>,
   <trd>despellejar</trd>
  </trdgrp>
  <trd lng="fa">&f_p;&a_w;&a_s;&a_t; &f_k;&a_n;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;pecer</trd>,
    <trd>d&eacute;piauter</trd>,
    <trd>d&eacute;pouiller<klr tip="ind"> (d&eacute;pecer)</klr></trd>,
    <trd>&eacute;corcher</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megny&uacute;z</trd>,
    <trd>megskalpol</trd>
  </trdgrp>
  <trd lng="io">senpeligar</trd>
  <trdgrp lng="it">
    <trd>spellare</trd>,
    <trd>scuoiare</trd>,
    <trd>scotennare</trd>,
    <trd>scorticare</trd>,
    <trd>pelare</trd>,
    <trd>sbucciare</trd>
  </trdgrp>
  <trd lng="ja">&#x76AE;&#12434;&#21093;&#12368;
  <pr>&#x306F;&#x308F;&#12434;&#x306F;&#12368;</pr></trd>
  <trd lng="ru">&c_s;&c_n;&c_ja;&c_t;&c_mol; &c_k;&c_o;&c_zh;&c_u;</trd>
  <trdgrp lng="nl">
    <trd>villen</trd>,
    <trd>scalperen</trd>
  </trdgrp>
  <trd lng="pl">skalpowa&cacute;</trd>
  <trdgrp lng="pt">
   <trd>pelar</trd>,
   <trd>tirar a <ind>pele</ind></trd>
  </trdgrp>
  <trd lng="sv">fl&aring;</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_d;&c_i;&c_r;&c_a;&c_t;&c_i; &c_sh;&c_k;&c_ib;&c_r;&c_u;</trd>,
    <trd>&c_z;&c_n;&c_ib;&c_m;&c_a;&c_t;&c_i; &c_sh;&c_k;&c_u;&c_r;&c_u;</trd>,
    <trd>&c_b;&c_ib;&c_l;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x53BB;&#x76AE; <pr>q&#xF9;p&#xED;</pr></trd>,
    <trd>&#x525D;&#x76AE; <pr>b&#x14D;p&#xED;</pr></trd>
  </trdgrp>
