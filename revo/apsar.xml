<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: apsar.xml,v 1.4 2023/05/06 15:22:44 revo Exp $">
<kap>
  <rad>apsar</rad>/o
</kap>
<drv mrk="apsar.0o">
  <kap><tld/>o</kap>
  <snc mrk="apsar.0o.HIN">
    <uzo tip="fak">HIN</uzo>
    <dif>
      Iu el duondiinoj ofte figurataj kiel dancantinoj a&ubreve; muzikantinoj, 
      parulinoj de <ref tip="vid" cel="gandarv.0o.HIN">gandarvoj</ref>:
      <ekz>
        la tieaj freskoj <klr>...</klr> prezentas budaojn, bodisatvojn, 
        bik&scirc;uojn, flugajn <tld/>ojn
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;altebena&jcirc;o Ali&rightquot;, 
        2006-11-06</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_s;&c_a;&c_r;&c_a;</trd>
  <trd lng="ca">apsara</trd>
  <trd lng="de">Apsara</trd>
  <trdgrp lng="en">
    <trd>apsara</trd>,
    <trd>apsaras</trd>
  </trdgrp>
  <trd lng="fr">apsara</trd>
</drv>
</art>

