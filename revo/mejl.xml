  </snc>
  <trd lng="de">Meile</trd>
  <trd lng="en">mile</trd>
  <trd lng="nl">mijl</trd>
  <trd lng="ru">&c_m;&c_i;&c_l;&c_ja;</trd>
  <trd lng="sv">mil</trd>
</drv>
</art>
