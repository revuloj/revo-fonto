<art mrk="$Id: elevat.xml,v 1.16 2021/03/18 18:00:05 revo Exp $">
<kap>
  <rad>elevator</rad>/o
  <fnt><bib>ZR</bib></fnt>
</kap>
<drv mrk="elevat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>  
  <snc>
    <dif>
      <ref tip="super" cel="aparat.0o.TEK">Aparato</ref> por suprenlevi &scirc;ar&gcirc;ojn, varojn, e&ccirc;
      &scirc;ipojn:
      <ekz>
        oni ofte renkontas minejojn kun la grandaj cisternoformaj <tld/>oj
        <fnt>
          <bib>MD02</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o <klr>[servas]</klr> por suprenlevi pezajn &scirc;ar&gcirc;ojn
        <fnt>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Lifto">Lifto - Vikipedio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="argan.0o">argano</ref>,
      <ref cel="kapsta.0o">kapstano</ref>,
      <ref cel="lift.sxip0o">&scirc;iplifto</ref>,
      <ref cel="vincx.0o">vin&ccirc;o</ref>
    </refgrp>
    <bld lok="&WCU;/thumb/2/22/Elevator5_k19.JPG/448px-Elevator5_k19.JPG" prm="CC BY-SA 3.0"><tld/>o por mal&scirc;argado de &scirc;ipoj
        <fnt>
          <aut>Ursula Horn, propra verko</aut>
          <lok><url ref="&WCW;/File:Elevator5_k19.JPG">https://commons.wikimedia.org/wiki/File:Elevator5_k19.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_v;&c_a;&c_t;&c_a;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>elev&aacute;tor</trd>,
    <trd>v&yacute;tah na obil&iacute;</trd>,
    <trd>zdvi&zcaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hebewerk</trd>,
    <trd>Elevator</trd>
  </trdgrp>
  <trd lng="en">elevator</trd>
  <trd lng="es">elevador</trd>
  <trd lng="fr">&eacute;l&eacute;vateur</trd>
  <trd lng="hu">elev&aacute;tor
      <klr>(emel&odblac;szerkezet)</klr></trd>
  <trd lng="pl">elewator</trd>
  <trd lng="pt">elevador <klr tip="amb">(para cargas)</klr></trd>
  <trd lng="ru">&c_e;&c_l;&c_je;&c_v;&c_a;&c_t;&c_o;&c_r;
      <klr>(&c_p;&c_o;&c_d;&c_malmol;&c_jo;&c_m;&c_n;&c_o;&c_je;
      &c_u;&c_s;&c_t;&c_r;&c_o;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trdgrp lng="sk">
    <trd>zdv&iacute;hac&iacute; stroj</trd>,
    <trd>zdv&iacute;ha&ccaron;</trd>
  </trdgrp>
</drv>
