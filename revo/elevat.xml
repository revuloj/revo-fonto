<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>elevator</rad>/o
      <fnt>Z</fnt>
    </kap>
    <drv mrk="elevat.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">TEK</uzo>
      <fnt><bib>ZR</bib></fnt>
      <snc>
        <dif>
          Aparato por suprenlevi &scirc;ar&gcirc;ojn, varojn, e&ccirc; &scirc;ipojn:
          <ekz><tld/>o konsistas principe el senfina &ccirc;eno.</ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="argan.0o">argano</ref>,
          <ref cel="kapsta.0o">kapstano</ref>,
          <ref cel="vincx.0o">vin&ccirc;o</ref>.
        </refgrp>
      </snc>
      <trd lng="be">&c_e;&c_l;&c_je;&c_v;&c_a;&c_t;&c_a;&c_r;</trd>
      <trd lng="de">Hebewerk</trd>
      <trd lng="en">elevator</trd>
      <trd lng="es">elevador</trd>
      <trd lng="fr">&eacute;l&eacute;vateur</trd>
      <trd lng="hu">elev&aacute;tor <klr>(emel&odblac;szerkezet)</klr></trd>
      <trd lng="ru">&c_e;&c_l;&c_je;&c_v;&c_a;&c_t;&c_o;&c_r; <klr>(&c_p;&c_o;&c_d;&c_malmol;&c_jo;&c_m;&c_n;&c_o;&c_je; &c_u;&c_s;&c_t;&c_r;&c_o;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
    </drv>
  </art>
