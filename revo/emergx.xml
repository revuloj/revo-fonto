<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: emergx.xml,v 1.16 2023/11/07 20:34:10 revo Exp $">
<kap>
<rad>emer&gcirc;</rad>/i
</kap>
<drv mrk="emergx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="ir.el0i">Eliri</ref> el iu medio, trairinte &gcirc;in:
      <ekz>
        kiam radio <tld/>as el iu medio, &gcirc;i estas nomata 
        <ind><tld/>anta</ind> radio;
        <trdgrp lng="de">
           <trd>auftretend</trd>,
           <trd>austretend</trd>,
           <trd>auftauchend</trd>,
           <trd>sich <ind>erhebend</ind></trd>,
           <trd>emporkommend</trd>,
           <trd>zum <ind>Vorschein kommend</ind></trd>,
           <trd>in <ind>Erscheinung tretend</ind></trd>
        </trdgrp>
        <trd lng="fr">&eacute;mergent<klr tip="ind"> (phys.)</klr></trd>
        <trdgrp lng="nl">
          <trd>oprijzend</trd>,
          <trd>bovenkomend</trd>,
          <trd>opdoemend</trd>,
          <trd>opdagend</trd>,
          <trd>opduikend</trd>,
          <trd>&uacute;&iacute;tstekend</trd>,
          <trd>aan de <ind>oppervlakte</ind> komend</trd>
        </trdgrp>
      </ekz>
      <ekz>
        la <ind><tld/>a</ind> ebeno estas ebeno normala je la <tld/>a
        faco de la medio, kaj kiu entenas la <tld/>antan radion;
        <trd lng="fr">d'<ind>&eacute;mergence</ind></trd>
      </ekz>
      <ekz>
        la <tld/>a angulo estas formata de la <tld/>anta radio kun la
        normalo  &ccirc;e la <tld/>a punkto;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[kredeble]</klr> tio emer&gcirc;is &ndash;, ke <nom>Zamenhof</nom> opiniis, ke la diversaj
        &leftquot;rakontoj&rightquot; de la religioj &leftquot;povas ne esti fantaziaj&rightquot; (estas lia esprimo)
        <fnt>
          <bib>Monato</bib>
          <aut>Armando Zecchin</aut>
          <vrk><url ref="https://www.monato.be/2010/010077.php">Homarane</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aper.0i">aperi</ref>,
      <ref cel="merg.el0igxi">elmergi&gcirc;i</ref>,
      <ref cel="manife.sin0i">manifesti&gcirc;i</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&#x2019;&c_ja;&c_w;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_w;&c_s;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>auftreten</trd>,
     <trd>austreten</trd>,
     <trd>erscheinen</trd>,
     <trd>austreten</trd>,
     <trd>sich <ind>erheben</ind></trd>,
     <trd>emporkommen</trd>,
     <trd>zum <ind>Vorschein kommen</ind></trd>,
     <trd>in <ind>Erscheinung treten</ind></trd>
  </trdgrp>
  <trd lng="es">emerger</trd>
  <trd lng="fr">&eacute;merger<klr tip="ind"> (phys.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x6D6E;&#x304B;&#x3073;&#x51FA;&#x308B;  <pr>&#x3046;&#x304B;&#x3073;&#x3067;&#x308B;</pr></trd>,
    <trd>&#x73FE;&#x308C;&#x51FA;&#x308B;  <pr>&#x3042;&#x3089;&#x308F;&#x308C;&#x3067;&#x308B;</pr></trd>,
    <trd>&#x5C04;&#x51FA;&#x3059;&#x308B;  <pr>&#x3044;&#x3067;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>oprijzen</trd>,
    <trd>bovenkomen</trd>,
    <trd>opdoemen</trd>,
    <trd>opdagen</trd>,
    <trd>opduiken</trd>,
    <trd>uitsteken</trd>,
    <trd>aan de <ind>oppervlakte</ind> komen</trd>
  </trdgrp>
  <trdgrp lng="pl">
     <trd><ind>wynurza&cacute;</ind> si&eogonek;</trd>,
     <trd><ind>pojawia&cacute;</ind> si&eogonek;</trd>,
     <trd>wychodzi&cacute;</trd>
  </trdgrp>
  <trd lng="pt">emergir</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_r;&c_i;&c_n;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_p;&c_l;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_a;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_z;&#x2019;&c_ja;&c_v;&c_l;&c_ja;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_i;&c_s;&c_t;&c_u;&c_p;&c_a;&c_t;&c_i; (&c_z; &c_v;&c_o;&c_d;&c_i;)</trd>,
    <trd>&c_v;&c_i;&c_h;&c_o;&c_d;&c_i;&c_t;&c_i; (&c_z; &c_s;&c_je;&c_r;&c_je;&c_d;&c_o;&c_v;&c_i;&c_shch;&c_a;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x51FA;&#x73B0; <pr>ch&#x16B;xi&#xE0;n</pr></trd>,
    <trd>&#x663E;&#x5F97; <pr>xi&#x1CE;nde</pr></trd>,
    <trd>&#x4F3C;&#x4E4E; <pr>s&#xEC;h&#x16B;</pr></trd>
  </trdgrp>
</drv>
</art>

