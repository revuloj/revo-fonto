<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>loz</rad>/a <fnt><bib>PIV3</bib></fnt>
</kap>

<drv mrk="loz.0a">
  <kap><tld/>a</kap>
  <snc mrk="loz.0a.KOMUNE">
    <dif>
      Malstrikta, malstre&ccirc;ita, malkompakta:
    </dif>
      <ekz>
      <tld/>a bluzo, pantalono; <tld/>e ligita &scirc;nuro.
      </ekz>
  <trd lng="fr">rel&acirc;ch&eacute;</trd>
  <trd lng="nl">los</trd>
  </snc>
  
  <snc mrk="loz.0a.KOMERCO">
    <dif>
      Ne enpakita:
    </dif>
      <ekz>
      <tld/>a kargo.
      </ekz>
  <trd lng="fr">en <ind>vrac</ind></trd>
  <trd lng="nl">bulk</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>uvolněný</trd>,
    <trd>volný</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>uvoľnený</trd>,
    <trd>voľný</trd>
  </trdgrp>
</drv>
</art>

