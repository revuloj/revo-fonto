<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: loz.xml,v 1.12 2022/05/01 12:04:34 revo Exp $">
<kap>
    <rad>loz</rad>/a <fnt><bib>PIV3</bib></fnt>
</kap>

<drv mrk="loz.0a">
  <kap><tld/>a</kap>
  <snc mrk="loz.0a.KOMUNE">
    <dif>
      <ref tip="dif" cel="strikt.mal0a">Malstrikta</ref>, 
      malstre&ccirc;ita, <ref cel="kompak.mal0a">malkompakta</ref>:
      <ekz>
        li metis la piedojn en karese <tld/>ajn pantoflojn
        <fnt>
          <bib>MMa</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi le&gcirc;ere malstre&ccirc;is la korpon <klr>[&#x2026;]</klr>, por ke la
        malfruaj sunradioj skizu la allogajn konturojn de via figuro tra la <tld/>a
        vualvesta&jcirc;o, kiu nur akcentis la suban nudecon
        <fnt>
          <bib>SkandalJozef</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        estas malfacile teni la ekvilibron sur la <tld/>a &scirc;nuro de politikaj
        intertraktadoj
        <fnt>
          <bib>Monato</bib>
          <aut>Guido Van Damme</aut>
          <vrk><url ref="&Monato;/2007/009157.php">Lando disfalos</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi faru <klr>[nur]</klr> <tld/>ajn 
        <klr>(<ref cel="koher.ne0a">nekohera</ref>jn)</klr> rimarkojn
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 2 (64)</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>loose</trd>,
      <trd>slack</trd>
    </trdgrp>
    <trd lng="fr">rel&acirc;ch&eacute;</trd>
    <trd lng="nl">los</trd>
  </snc>  
  <snc mrk="loz.0a.nepakita">
    <dif>
      Ne enpakita:
      <ekz>
        <tld/>a kargo;
      </ekz>
      <ekz>
        fari teon el <tld/>a miksa&jcirc;o anstata&ubreve; per for&jcirc;etebla tesaketo
        <fnt>
          <bib>Kon11</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="detal.0a.EKON">detala<sncref/></ref>,
      <ref cel="detal.po0a">podetala</ref>,
      <ref cel="dis.0a.KOMUNE">disa<sncref/></ref>
    </refgrp>
    <trdgrp lng="en">
      <trd>loose</trd>,
      <trd>unrestrained</trd>
    </trdgrp>
    <trd lng="fr">en <ind>vrac</ind></trd>
    <trd lng="nl">bulk</trd>
  </snc>
  <trd lng="bo">&t_la;&t_hata;&t_shabchu;&t_ga;&t_sa;&t_tsheg;&t_ra;&t_shabchu;&t_tsheg;&t_ga;&t_ta;&t_naro;&t_nga;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>uvoln&ecaron;n&yacute;</trd>,
    <trd>voln&yacute;</trd>
  </trdgrp>
  <trd lng="de">lose</trd>
  <trd lng="pl">lu&zacute;ny</trd>
  <trdgrp lng="sk">
    <trd>uvo&lcaron;nen&yacute;</trd>,
    <trd>vo&lcaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x456;&#x43B;&#x44C;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x448;&#x438;&#x440;&#x43E;&#x43A;&#x438;&#x439;</trd>,
    <trd>&#x43F;&#x443;&#x445;&#x43A;&#x438;&#x439;</trd>,
    <trd>&#x43D;&#x435;&#x443;&#x43F;&#x430;&#x43A;&#x43E;&#x432;&#x430;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>
</art>

