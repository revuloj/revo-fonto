<kap>
  <rad>partikul</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="partik.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">GRA</uzo>
    <dif>
      Nedeklinaciebla vorteto; ekz. <ekz>ajn, ja.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>neohebn&eacute; slovo</trd>,
    <trd>&ccaron;&aacute;stice <klr>(ne, nur, ja, la ...)</klr></trd>
  </trdgrp>
  <trd lng="de">Partikel</trd>
  <trdgrp lng="en">
    <trd><klr>grammatical </klr>particle</trd>
  </trdgrp>
  <trd lng="es">part&iacute;cula</trd>
  <trd lng="fr">particule</trd>
  <trd lng="hu">partikula</trd>
  <trdgrp lng="ja">
    <trd>&#x52A9;&#x8F9E;  <pr>&#x3058;&#x3087;&#x3058;</pr></trd>,
    <trd>&#x4E0D;&#x5909;&#x5316;&#x8A5E;  <pr>&#x3075;&#x3078;&#x3093;&#x304B;&#x3057;</pr></trd>
  </trdgrp>
  <trd lng="nl">partikel</trd>
  <trd lng="pl">partyku&lstroke;a</trd>
  <trd lng="pt">partic&iacute;pio</trd>
  <trd lng="ru">&c_ch;&c_a;&c_s;&c_t;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">nesamostatn&aacute; &ccaron;astica</trd>
  <trdgrp lng="zh">
    <trd>&#x52A9;&#x8A5E; <pr>zh&#xF9;c&#xED;</pr></trd>,
    <trd>&#x52A9;&#x8BCD; <pr>zh&#xF9;c&#xED;</pr></trd>
  </trdgrp>
</drv>
</art>
