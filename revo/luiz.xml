<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: luiz.xml,v 1.3 2023/03/28 10:32:03 afido Exp $">
    <kap>
      <rad>Luiz</rad>/o
    </kap>

<drv mrk="luiz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc mrk="luiz.0o.nomo">
    <dif>
      Hispana-franca <ref tip="lst" lst="voko:personaj_nomoj" cel="nom.persona0o.GEN">nom</ref>formo
      
      de <ref tip="sin" cel="ludovik.0o">Ludoviko</ref>,
      uzata por virinoj kaj viroj:
      <ekz>
        re&gcirc;o <tld/>o &ccirc;iusemajne sendis la ekzekutiston
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a Parko <klr>[&#8230;]</klr> en la germana urbo <nom>Mannheim</nom> <klr>[estas
        nomita]</klr> la&ubreve; la princino <nom>Luise Marie Elisabeth</nom> de Prusio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=281804">Luiza Parko Mannheim</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_L;&c_u;&c_ib;&c_s;</trd>
    <trdgrp lng="de">
      <trd>Luis</trd>,
      <trd>Luise</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>Luis</trd>,
      <trd>Luise</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>Louis</trd>,
      <trd>Louise</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="uk">
    <trd>&#x41B;&c_u;&c_ji;&c_s;</trd>,
    <trd>&#x41B;&#x44E;&c_d;&#x43E;&c_v;&c_ib;&#x43A;</trd>
  </trdgrp>
</drv>

  </art>

