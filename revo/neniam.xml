<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>neniam</rad>
</kap>
<drv mrk="neniam.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Adverbo signifanta &leftquot;en neniu ajn tempo&rightquot;:
      <ekz>
        mi <tld/> fumis;
      </ekz>
      <ekz>
        oni devas <tld/> mensogi;
      </ekz>
      <ekz>
        <tld/> malfeli&ccirc;o alvenas sola;
      </ekz>
      <ekz>
        mi <tld/> a&ubreve;dis tion;
      </ekz>
      <ekz>
        ni <tld/> plu revidis lin;
      </ekz>
      <ekz>
        la tempo pasinta <tld/> revenos<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi <tld/> povos havi plu konfidon al &scirc;i;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>en la tago de (la) sankta <tld lit="N"/>o</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="fr">&agrave; la <ind>saint-glinglin</ind></trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="jxauxd">&jcirc;audo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_ib;&c_k;&c_o;&c_l;&c_ib;</trd>
  <trdgrp lng="de">
    <trd>nie</trd>,
    <trd>niemals</trd>
  </trdgrp>
  <trd lng="el">&pi;&omikron_ton;&tau;&epsilon;</trd>
  <trdgrp lng="en">
    <trd>never</trd>,
    <trd>at no <ind>time</ind></trd>,
    <trd>not <ind>ever</ind></trd>,
  </trdgrp>
  <trd lng="es">nunca</trd>
  <trd lng="fr">jamais</trd>
  <trdgrp lng="hu">
    <trd>soha</trd>,
    <trd>semikor</trd>
  </trdgrp>
  <trd lng="nl">nooit</trd>
  <trd lng="pt">nunca</trd>
  <trd lng="ru">&c_n;&c_i;&c_k;&c_o;&c_g;&c_d;&c_a;</trd>
  <trd lng="sv">aldrig</trd>
  <trd lng="vo">nevelo</trd>
</drv>
</art>
