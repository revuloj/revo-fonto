<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: iluminat.xml,v 1.4 2023/11/19 10:22:12 revo Exp $">
<kap>
    <rad>Iluminat</rad>/oj <fnt><bib>Viki</bib></fnt>
</kap>
<drv mrk="iluminat.0o">
  <kap><tld/>oj</kap>
  <snc mrk="iluminat.0o.SNC">
    <uzo tip="fak">POL</uzo>
    <dif>
      Nomo, de pluraj sekretaj societaj grupoj, kaj
      historiaj kaj modernaj kaj realaj kaj fikciaj:
      <ekz>
        la Viena ordeno de <tld lit="i"/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=273684">Zur wahren Eintracht</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        Johann Adam Weishaupt estis germana filozofo kaj fondinto de la Ordeno de
        <tld/>oj, sekreta societo kun originoj en Bavario
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=326613">Adam Weishaupt</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="framas.0o">framasono</ref>
  </snc>
  <trd lng="be">&c_ib;&c_l;&c_ju;&c_m;&c_ib;&c_n;&c_a;&c_t;&c_y;</trd>
  <trd lng="de">Illuminaten</trd>
  <trd lng="fr">Illuminati</trd>
  <trdgrp lng="zh">
    <trd>&#x5149;&#x660E;&#x5E2E; <pr>gu&#x101;ngm&#xED;ngb&#x101;ng</pr></trd>,
    <trd>&#x5149;&#x7167;&#x6D3E; <pr>gu&#x101;ngzh&#xE0;op&#xE0;i</pr></trd>
  </trdgrp>
</drv>
</art>

