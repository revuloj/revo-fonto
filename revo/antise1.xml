<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>antiseps</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="antise1.0o">
  <kap><tld/>o</kap>
  <snc mrk="antise1.0o.MED">
  <uzo tip="fak">MED</uzo>
    <dif>
      Procedo, per kiu oni detruas en kirurgio la infektantajn mikrobojn
      per kemiaj substancoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="aseps.0o">asepso</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_t;&c_y;&c_s;&c_e;&c_p;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="en">antisepsis</trd>
  <trd lng="es">antisepsia</trd>
  <trd lng="fr">antisepsie</trd>
  <trd lng="hu">antiszepszis</trd>  
  <trd lng="ru">&c_a;&c_n;&c_t;&c_i;&c_s;&c_je;&c_p;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">antisepsa</trd>
</drv>

<drv mrk="antise1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="antise1.0i.MED">
    <dif>
      Fari <tld/>on.
    </dif>
    <refgrp tip="vid">
      <ref cel="desinf.0i">desinfekti</ref>
    </refgrp>
  </snc>
  <trd lng="de">desinfizieren</trd>
  <trd lng="en">disinfect</trd>
  <trdgrp lng="hu">
    <trd>fert&odblac;tlen&iacute;t</trd>,
    <trd>dezinfici&aacute;l</trd>
  </trdgrp>
</drv>

<drv mrk="antise1.0a">
  <kap><tld/>a</kap>
  <snc mrk="antise1.0a.MED">
    <dif>
      Per <tld/>o detruanta la infektajn mikrobojn, kontra&ubreve;mikroba,
      senmikrobiga:
      <ekz>
        mi estis trabanita en <tld/>a &scirc;prucado
        <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;apitro 2a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_t;&c_y;&c_s;&c_e;&c_p;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="de">antiseptisch</trd>
  <trd lng="en">antiseptic</trd>
  <trd lng="es">antis&eacute;ptico</trd>
  <trd lng="fr">antiseptique</trd>
  <trdgrp lng="hu">
    <trd>fert&odblac;tlen&iacute;t&odblac;</trd>,
    <trd>cs&iacute;r&aacute;tlan&iacute;t&oacute;</trd>,
    <trd>antiszeptikus</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_n;&c_t;&c_i;&c_s;&c_je;&c_p;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">antiseptick&yacute;</trd>
</drv>
</art>
