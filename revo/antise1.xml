<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>antiseps</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="antise1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Procedo, per kiu oni detruas en kirurgio la infektantajn mikrobojn
      per kemiaj substancoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="aseps.0o">asepso</ref>.
    </refgrp>
  </snc>
  <trd lng="es">antisepsia</trd>
  <trd lng="fr">antisepsie</trd>
  <trd lng="ru">&c_a;&c_n;&c_t;&c_i;&c_s;&c_je;&c_p;&c_t;&c_i;&c_k;&c_a;</trd>
</drv>

<drv mrk="antise1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on.
    </dif>
    <refgrp tip="vid">
      <ref cel="desinf.0i">desinfekti</ref>.
    </refgrp>
  </snc>
</drv>

<drv mrk="antise1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Bazita sur la <tld/>o.
    </dif>
  </snc>
  <snc>
    <dif>
      Kapabla detrui la infektantajn mikrobojn, kontra&ubreve;mikroba,
      senmikrobiga.
    </dif>
  </snc>
  <trd lng="es">antis&eacute;ptico</trd>
  <trd lng="fr">antiseptique</trd>
  <trd lng="ru">&c_a;&c_n;&c_t;&c_i;&c_s;&c_je;&c_p;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
