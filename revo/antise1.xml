<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>antiseps</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="antise1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Procedo, per kiu oni detruas en kirurgio la  infektantajn mikrobojn per
      kemiaj substancoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="aseps.0o">asepso</ref>.
    </refgrp>
  </snc>
  <trd lng="fr">antisepsie</trd>
  <trd
  lng="ru">&#1072;&#1085;&#1090;&#1080;&#1089;&#1077;&#1087;&#1090;&#1080;&#1082;&#1072;</trd>
</drv>
<drv mrk="antise1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on.
    </dif>
    <refgrp tip="vid">
      <ref cel="desinf.0i">desinfekti</ref>.
    </refgrp>
  </snc>
</drv>
<drv mrk="antise1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Bazita sur la <tld/>o.
    </dif>
  </snc>
  <snc>
    <dif>
      Kapabla detrui la  infektantajn mikrobojn, kontra&ubreve;mikroba,
      senmikrobiga.
    </dif>
  </snc>
  <trd lng="fr">antiseptique</trd>
  <trd
  lng="ru">&#1072;&#1085;&#1090;&#1080;&#1089;&#1077;&#1087;&#1090;&#1080;&#1095;&#1077;&#1089;&#1082;&#1080;&#1081;</trd>
</drv>
</art>
