<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hil.xml,v 1.4 2023/11/16 09:24:30 revo Exp $">
<kap>
  <rad>hil</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="hil.0o">
  <kap><tld/>o</kap>
  <snc mrk="hil.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Verda raneto kun &gcirc;enerale orkoloraj okuloj
      kaj kupaj piedoj, per kiuj &gcirc;i facile grimpas sur kreska&jcirc;oj;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref>
      el familio hiledoj vivanta en Nordameriko,
      E&ubreve;ropo, Nordafriko kaj Azio (<trd lng="la">Hyla</trd>):
      <ekz>
        por protekti <tld/>ajn ovojn de rabofi&scirc;oj oni konstruas specialajn barilojn,
        kie ili restas &gcirc;is apero de <tld/>idoj
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2012/010875.php"
              >Pereon de specioj malhelpas homoj</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        post la kanto de la <tld lit="H"/>o balda&ubreve; ekpluvas
        <fnt>
          <aut>J. Dorosmai, trad. J. Dorosmai jun. kaj J. Horvath</aut>
          <vrk>Fabloj kaj Aforismoj</vrk>
          <lok>2002</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/4/41/BennyTrapp_Hyla_intermedia_Italien.jpg/640px-BennyTrapp_Hyla_intermedia_Italien.jpg"
      prm="CC BY-SA 3.0"
      ><tld/>o (Hyla intermedia)
        <fnt>
          <aut>Benny Trapp, propra verko</aut>
          <lok><url ref="&WCW;/File:BennyTrapp_Hyla_intermedia_Italien.jpg"
              >https://commons.wikimedia.org/wiki/File:BennyTrapp_Hyla_intermedia_Italien.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="fr">rainette</trd>
  </snc>
  <trd lng="de">Laubfrosch</trd>
  <trd lng="ja">&#12450;&#12510;&#12460;&#12456;&#12523;</trd>
</drv>
</art>

