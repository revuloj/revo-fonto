<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bambu.xml,v 1.33 2024/09/01 02:03:31 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bambu</rad>/o <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="bambu.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bambu.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Granda tropika <ref tip="super" cel="kan.0o.BOT">kano</ref>,
      el kies lignaj tigoj oni faras meblojn, konstruojn;
      <ref tip="lst" cel="genr.0o" lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Bambusa</trd>):
      <ekz>
        la <tld/>aj kanoj estis rompitaj sur iliaj &scirc;ultroj
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pintoj de iliaj <tld/>aj lancoj brilis en la suno kiel
        flametoj
        <fnt>
          <aut>H. Sienkewicz, trad. L. Zamenhof</aut>,
          <vrk>Quo vadis?</vrk>,
          <lok>1933</lok>
        </fnt>;
      </ekz>
      <ekz>
        la man&gcirc;a&jcirc;o estis &leftquot;modesta&rightquot; &ndash; fungoj, <klr>[&#8230;]</klr> anasaj kaj kokaj ovoj,
        kuiritaj en salita akvo, <tld/>o kaj kompreneble rizo
        <fnt>
          <bib>Kon11</bib>
          <lok>&rightquot;Teo kaj amo&#8221;: festivalaj notoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/8/8b/Bamboo_Richelieu.jpg/640px-Bamboo_Richelieu.jpg"
      prm="CC BY-SA 3.0"
      ><tld/>o
        <fnt>
          <aut>I, Manfred Heyde, propra verko</aut>
          <lok><url ref="&WCW;/File:Bamboo_Richelieu.jpg"
              >https://commons.wikimedia.org/wiki/File:Bamboo_Richelieu.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="zh">&#31681; <pr>hu&aacute;ng</pr></trd>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_m;&c_b;&c_u;&c_k;</trd>
  <trd lng="bg">&c_b;&c_a;&c_m;&c_b;&c_u;&c_k;</trd>
  <trd lng="br">bambouz</trd>
  <trd lng="ca">bamb&uacute;</trd>
  <trd lng="cs">bambus</trd>
  <trd lng="de">Bambus</trd>
  <trd lng="en">bamboo</trd>
  <trd lng="es">bamb&uacute;</trd>
  <trd lng="fr">bambou</trd>
  <trd lng="he">&bet;&#1468;&#1464;&mem;&bet;&#1468;&vav;&#1468;&qof;</trd>
  <trd lng="hu">bambusz</trd>
  <trd lng="id">bambu</trd>
  <trd lng="ja">&#12479;&#12465;</trd>
  <trd lng="nl">bamboe</trd>
  <trd lng="pl">bambus</trd>
  <trd lng="pt">bambu</trd>
  <trd lng="ro">bambus</trd>
  <trd lng="ru">&c_b;&c_a;&c_m;&c_b;&c_u;&c_k;</trd>
  <trd lng="sk">bambus</trd>
  <trd lng="sv">bambu</trd>
  <trd lng="uk">&c_b;&c_a;&c_m;&c_b;&c_u;&c_k;</trd>
</drv>
</art>
