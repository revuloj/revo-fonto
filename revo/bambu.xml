<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>bambu</rad>/o
</kap>
<drv mrk="bambu.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       Genro el familio <ref tip="super" cel="po1.0acoj">poacoj</ref>
       (<trd lng="la">Bambusa</trd>), grandaj tropikaj herboj, el kies
       lignaj tigoj oni faras meblojn, konstruojn.
     </dif>
   </snc>
   <trd lng="de">Bambus</trd>
   <trd lng="fr">bambou</trd>
   <trd lng="nl">bamboe</trd>
   <trd lng="ru">&c_b;&c_a;&c_m;&c_b;&c_u;&c_k;</trd>
</drv>
</art>
