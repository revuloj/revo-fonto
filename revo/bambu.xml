<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>bambu</rad>/o
</kap>
<drv mrk="bambu.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="po.0acoj">poacoj</ref>
      (<trd lng="la">Bambusa</trd>), grandaj tropikaj herboj, el kies
      lignaj tigoj oni faras meblojn, konstruojn.  
    </dif>
  </snc>
  <trd lng="de">Bambus</trd>
  <trd lng="nl">bamboe</trd>
</drv>
</art>
