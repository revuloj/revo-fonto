<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kimr.xml,v 1.7 2022/03/21 17:51:58 revo Exp $">
<kap>
  <rad>kimr</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kimr.0o">
  <kap><tld/>o</kap>
  <snc mrk="kimr.0o.ETN">
    <dif>
      Ano de <ref tip="lst" cel="popol.0o.etno" lst="voko:popolanoj">popolo</ref> origine 
      kelta-lingva, vivanta en <tld lit="K"/>ujo:
      <ekz>
        Maximus <klr>...</klr> la <tld/>oj ankora&ubreve; memoras 
        kiel Macsen Wledig
        <fnt>
          <aut>D. Harlow</aut>,
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Recenzoj/ys.html">
          [recenzo de] La Re&gcirc;o de Ys</url></vrk>,
          <lok>Berklia Ligilo, 1987-05</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_l;&c_ib;&c_je;&c_c;</trd>
  <trd lng="cs">Walesan</trd>
  <trd lng="en">Welshman</trd>
  <trd lng="fr">Gallois</trd>
  <trd lng="pl">Walijczyk</trd>     
  <trd lng="ro">Galezi {pl}</trd>
  <trd lng="sk">Walesan</trd>
  <trd lng="uk">&#x432;&#x430;&#x43B;&#x43B;&#x456;&#x454;&#x446;&#x44C;</trd>
</drv>

<drv mrk="kimr.0ujo">
  <kap><tld lit="K"/>ujo</kap>
  <snc mrk="kimr.0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Okcidenta regiono de
      <ref tip="malprt" cel="brit.B0ujo.GEOG">Britujo</ref>,
      unu el la naciaj divida&jcirc;oj de tiu regno;
      <ref tip="sin" cel="kimri.0o.GEOG">Kimrio</ref>:
      <ekz>
        li estis patro de Kolur, kiun K&aacute;ri poste mortigis 
        en <tld lit="K"/>ujo
        <fnt>
          <aut>-, trad. B. Ragnarsson</aut>,
          <vrk>Sagao de Njal</vrk>,
          <lok>2002</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_U;&c_e;&c_l;&c_mol;&c_s;</trd>
  <trd lng="en">Wales</trd>
  <trdgrp lng="fr">
    <trd>Galles</trd>,
    <trd>pays de Galles</trd>
  </trdgrp>
  <trd lng="pl">Walia</trd>     
  <trd lng="ro">&#x21A;ara Galilor</trd>
</drv>

<drv mrk="kimr.novasud0ujo">
  <kap>Nova Sud<tld/>ujo,
  <var><kap>Novsud<tld/>ujo</kap></var></kap>
  <snc mrk="kimr.novasud0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Sudorienta sub&scirc;tato de
      <ref tip="malprt" cel="auxstr1.A0o.sxtato">A&ubreve;stralio</ref>;
      <ref tip="sin" cel="kimri.novsud0o.GEOG">Novsudkimrio</ref>:
      <ekz>
        lo&gcirc;antojn de Nova Sud-<tld lit="K"/>ujo <klr>...</klr>
        oni ne povas nomi <tld lit="K"/>oj
        <fnt>
          <aut>B. Wennergren</aut>,
          <vrk>[PMEG] Plena Manlibro de Esperanta Gramatiko</vrk>,
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_N;&c_o;&c_v;&c_y; &c_P;&c_a;&c_w;&c_d;&c_n;&c_jo;&c_v;&c_y; &c_W;&c_e;&c_l;&c_mol;&c_s;</trd>
  <trd lng="en">New South Wales</trd>
  <trd lng="fr">Nouvelle Galles du sud</trd>
  <trd lng="pl">Nowa Po&lstroke;udniowa Walia</trd>     
  <trd lng="ro">Noul Wales de Sud</trd>
</drv>

</art>

