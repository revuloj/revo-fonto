<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>fatras</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="fatras.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="fatras.0o.malordo">
    <dif>
      <ref tip="super" cel="ord.mal0o">Malorda</ref> amaso de &ccirc;iaj a&jcirc;oj:
      <ekz>
        kelkaj veturiloj &scirc;ar&gcirc;itaj per &ccirc;iaspeca <tld/>o
        <fnt><bib>SkandalJozef</bib>, <lok>korpogvardia
        veterano</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pelmel.0o">pelmelo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>bric-&agrave;-brac</trd>,
    <trd>fatras</trd>
  </trdgrp>
</drv>
</art>

