<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: karbid.xml,v 1.3 2022/05/03 20:38:46 afido Exp $">
    <kap>
      <rad>karbid</rad>/o
    </kap>

<drv mrk="karbid.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="kombin.0ajxo.KEM">kombina&jcirc;o</ref> de <ref tip="prt"
      cel="karbon.0o">karbono</ref>
      kun alia elemento:
      <ekz>
        <nom>Alexandr Sommer Bat&ecaron;k</nom> <klr>[&#8230;]</klr> precizigis la atompezon de cerio, okupi&gcirc;is
        pri <tld/>oj, likvigado de oksigeno
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=252326">Alexandr Sommer Bat&ecaron;k</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la granda milito, de kiu la junulo memoras nur nutra&jcirc;kartojn, <tld/>lampojn
        <klr>(gaslampo funkcianta per kalcia <tld/>o)</klr>,
        gigantajn rubrikojn en la gazetoj kaj senfinajn diskutadojn de la plena&gcirc;uloj
        <fnt>
          <vrk><url ref="https://tekstaro.com/t?nomo=al-torento">Al Torento</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Karbid</trd>
    <trd lng="en">carbid</trd>
    <trd lng="fr">carbure</trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_b;&c_ib;&c_d;</trd>
  <trd lng="uk">&#x43A;&c_a;&c_r;&c_b;&c_ib;&c_d;</trd>
</drv>

  </art>

