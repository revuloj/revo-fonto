<kap>
<ofc>2</ofc>
<rad>dekagram</rad>/o
</kap>
<drv mrk="dekagr.0o">
  <kap><tld/>o</kap>
  <mlg kod="SI">dag</mlg>
  <snc>
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de maso, egala
      al dek <ref tip="prt" cel="gram.0o">gramoj</ref>:
      <ekz>
        ilia nutra&jcirc;bezono estas minimuma, se kalkuli specialajn grajna&jcirc;ojn por &ccirc;in&ccirc;iloj,
        nur 2-3 <tld/>oj por unu tago
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17760">&Ccirc;in&ccirc;ilo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_k;&c_a;&c_g;&c_r;&c_a;&c_m;</trd>
  <trd lng="ca">decagram</trd>
  <trdgrp lng="cs">
    <trd>deka</trd>,
    <trd>dekagram</trd>,
    <trd>deko</trd>
  </trdgrp>
  <trd lng="de">Dekagramm</trd>
  <trd lng="en">decagram</trd>
  <trd lng="fr">d&eacute;cagramme</trd>
  <trd lng="hu">dekagramm</trd>
  <trd lng="ja">&#x30C7;&#x30AB;&#x30B0;&#x30E9;&#x30E0;</trd>
  <trd lng="nl">dekagram</trd>
  <trd lng="pl">dekagram</trd>
  <trd lng="pt">decagrama</trd>
  <trd lng="ru">&c_d;&c_je;&c_k;&c_a;&c_g;&c_r;&c_a;&c_m;&c_m;</trd>
  <trd lng="sk">dekagram</trd>
  <trd lng="uk">&#x434;&#x435;&#x43A;&#x430;&#x433;&#x440;&#x430;&#x43C;</trd>
  <trd lng="zh">&#x5341;&#x514B; <pr>sh&#xED;k&#xE8;</pr></trd>
</drv>
