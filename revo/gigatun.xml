<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: gigatun.xml,v 1.8 2023/10/15 15:22:39 revo Exp $">
<kap>
  <rad>gigatun</rad>/o
</kap>
<drv mrk="gigatun.0o">
  <kap><tld/>o</kap>

  <snc mrk="gigatun.0o.pezo">
    <mlg>Gt</mlg>
    <dif>
      Miliardo da <ref tip="prt" cel="tun.0o.pezunuo">tunoj</ref>:
      <ekz>
        bruligo de fosilia brula&jcirc;o produktas &ccirc;irka&ubreve; 6,3 miliardojn de metrikaj tunoj
        (=&#8239;6,3 gigatunoj) de karbondioksido &ccirc;iujare
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=221704"
              >Fosilia brula&jcirc;o</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="gigatun.0o.TNT">
    <mlg>GT</mlg>
    <uzo tip="fak">MIL</uzo>
    <uzo tip="fak">GEOL</uzo>
    <uzo tip="fak">FIZ</uzo>
    <uzo tip="fak">AST</uzo>
    <dif>
      Praktika <ref tip="lst" cel="unu.mezur0o"
      lst="voko:mezurunuoj">mezurunuo</ref> kiu
      respondas al la <ref cel="energi.0o.FIZ">energio</ref>
      ellasata de unu miliardoj tunoj da TNT dum eksplodo:
      <ekz>
        la unuoj estas kilotunoj (kT), megatunoj (MT) kaj gigatunoj (GT) <klr>[&#8230;]</klr>
        la plej forta hidrogena bombo iam elprovita (de Sovetio en 1961) havis forton de
        56 MT da TNT
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=131877"
              >TNT-ekvivalento</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
       <ref cel="megatun.0o">megatuno</ref>,
       <ref cel="kilotun.0o">kilotuno</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_g;&c_a;&c_t;&c_o;&c_n;&c_a;</trd>
  <trd lng="de">Gigatonne</trd>
  <trd lng="en">gigaton</trd>
  <trd lng="hu">gigatonna</trd>
  <trd lng="id">gigaton</trd>
  <trd lng="it">gigatone</trd>
  <trd lng="pl">gigatona</trd>
</drv>
</art>

