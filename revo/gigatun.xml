  <art mrk="$Id$">
  <kap>
    <rad>gigatun</rad>/o
  </kap>
  <drv mrk="gigatun.0o">
    <kap><tld/>o</kap>
    <mlg>GT</mlg>
    <snc>
      <uzo tip="fak">MIL</uzo>
      <uzo tip="fak">GEOL</uzo>
      <uzo tip="fak">FIZ</uzo>
      <uzo tip="fak">AST</uzo>
      <dif>
      Praktika <ref tip="lst" cel="unu.mezur0o"
      lst="voko:mezurunuoj">mezurunuo</ref> kiu
respondas al la <ref cel="energi.0o.FIZ">energio</ref> ellasata de 1 000
000 000
tunoj da TNT dum eksplodo.
      </dif>
      </snc>
      <trd lng="de">Gigatonne</trd>
      <trd lng="en">gigaton</trd>
      <trd lng="hu">gigatonna</trd>
      <trd lng="id">gigaton</trd>
      <trd lng="it">gigatone</trd>
      <trd lng="pl">gigatona</trd>
  </drv>
</art>

