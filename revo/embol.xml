<art mrk="$Id: embol.xml,v 1.18 2022/03/17 13:47:18 revo Exp $">
<kap>
  <rad>embol</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="embol.0o">
  <kap><tld/>o</kap>
  <snc mrk="embol.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Ensanga koagula&jcirc;o ka&ubreve;zanta
      <ref tip="vid" cel="emboli.0o.MED">embolion</ref>:
      <ekz>
        pulma embolio a&ubreve; pulma embolozo estas embolio (bara&jcirc;o) de pulma arterio <klr>[&#x2026;]</klr>, la
        bara&jcirc;o preska&ubreve; &ccirc;iam konsistas el <tld/>o, kvankam sub tre specialaj kondi&ccirc;oj aliaj embolioj
        povas okazi (ekz. per aero)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=88163">Pulma embolio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_m;&c_b;&c_o;&c_l;</trd>
  <trd lng="bg">&c_je;&c_m;&c_b;&c_o;&c_l;</trd>
  <trdgrp lng="cs">
    <trd>embolus</trd>,
    <trd>vmetek</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Blutgerinnsel</trd>,
     <trd>Embolus</trd>
  </trdgrp>
  <trd lng="en">embolus</trd>
  <trd lng="fr">embole</trd>
  <trd lng="it">embolo</trd>
  <trdgrp lng="pl">
    <trd>skrzep krwi</trd>,
    <trd>embolus</trd>
  </trdgrp>
  <trd lng="pt">&ecirc;mbolo</trd>
  <trd lng="ru">&c_e;&c_m;&c_b;&c_o;&c_l;</trd>
  <trd lng="sk">krvn&aacute; zrazenina</trd>
  <trd lng="uk">&#x435;&#x43C;&#x431;&#x43E;&#x43B;</trd>
</drv>
</art>
