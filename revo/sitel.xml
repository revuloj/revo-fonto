<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sitel.xml,v 1.24 2025/05/18 10:46:44 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sitel</rad>/o
<fnt><bib>UV</bib></fnt></kap>

<drv mrk="sitel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sitel.0o.ujo">
    <dif>
      Rondforma ligna, plasta a&ubreve; metala <ref tip="super" cel="uj.0o">ujo</ref> havanta tenilon kaj
      destinita por &ccirc;erpi, porti a&ubreve; teni flua&jcirc;on:
      <ekz>
        plenigu kvar <tld/>ojn per akvo, kaj oni ver&scirc;u tion sur la bruloferon
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 18:34</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i apena&ubreve; povas treni sin mem, kaj devas tamen porti ankora&ubreve; la <tld/>on de la
        pumpilo
        <fnt>
          <bib>Fab2</bib>
          <lok>Malgranda Tuk</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu <tld/>o &scirc;vebis apud la alia, dum la levma&scirc;ino knaris kaj la <tld/>oj
        dancadis en la aero
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>fali de <tld/>o en barelon</ind>
        <klr>(de malbona stato en ankora&ubreve; pli malbonan)</klr>;
        <trd lng="hu">cs&ouml;b&ouml;rb&odblac;l v&ouml;d&ouml;rbe esik</trd>
      </ekz>
      <ekz>
        edzino pli elver&scirc;as per funelo, ol edzo enver&scirc;as per <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="sitel.0o.enhavo">
    <dif>
      Enhavo de <tld/>o, rigardata kiel <ref tip="lst" lst="voko:mezurunuoj" cel="mezur.0o.enhavo">mezuro</ref>:
      <ekz>
        <tld/>o da glacia akvo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> ordonis kapti <tld/>on da gugonoj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fabelo pri iu, kiu migris por ekkoni timon</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis sen&scirc;eliginta <tld/>on da terpomoj
        <fnt>
          <bib>IdO</bib>
          <lok>Kvara Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="bat1.0o">bato</ref>
    <refgrp tip="vid">
      <ref cel="kuv.0o">kuvo</ref>,
      <ref cel="krucx.0o">kru&ccirc;o</ref>,
      <ref cel="tin.0o">tino</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_d;&c_r;&c_o;</trd>
  <trdgrp lng="bg">
    <trd>&c_k;&c_o;&c_f;&c_a;</trd>,
    <trd>&c_v;&c_je;&c_d;&c_r;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>. v&ecaron;dro</trd>,
    <trd>d&zcaron;ber</trd>,
    <trd>kbel&iacute;k</trd>,
    <trd>k&yacute;bl</trd>,
    <trd>okov</trd>,
    <trd>putna</trd>,
    <trd>&zcaron;ejdl&iacute;k</trd>
  </trdgrp>
  <trd lng="de">Eimer</trd>
  <trdgrp lng="en">
    <trd>bucket</trd>,
    <trd>pail</trd>
  </trdgrp>
  <trd lng="fa">&a_s;&a_T;&a_l;</trd>
  <trd lng="fr">seau</trd>
  <trd lng="hu">v&ouml;d&ouml;r</trd>
  <trd lng="id">ember</trd>
  <trdgrp lng="ja">
    <trd>&#12496;&#12465;&#12484;  <pr>&#12400;&#12369;&#12388;</pr></trd>,
    <trd>&#25163;&#26742;  <pr>&#12390;&#12362;&#12369;</pr></trd>,
    <trd>&#12496;&#12465;&#12484;&#19968;&#26479;  <pr>&#12400;&#12369;&#12388;&#12356;&#12387;&#12401;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">emmer</trd>
  <trd lng="pl">wiadro</trd>
  <trd lng="ru">&c_v;&c_je;&c_d;&c_r;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>okov <klr>(na &ccaron;erpanie)</klr></trd>,
    <trd>put&ncaron;a</trd>,
    <trd>vedro</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_r;&c_o;</trd>,
    <trd>&c_c;&c_je;&c_b;&c_je;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_o;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#26742; <pr>t&#466;ng</pr></trd>,
    <trd>&#31602; <pr>sh&amacron;o</pr></trd>,
    <trd>&#27700;&#26742; <pr>shu&#464;t&#466;ng</pr></trd>,
    <trd>&#27700;&#31602; <pr>shu&#464;sh&amacron;o</pr></trd>,
    <trd>&#25552;&#26742; <pr>t&iacute;t&#466;ng</pr></trd>
  </trdgrp>
</drv>
</art>
