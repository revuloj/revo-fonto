<art mrk="$Id: sitel.xml,v 1.22 2024/03/24 20:21:58 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sitel</rad>/o
</kap>

<drv mrk="sitel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Rondforma ligna, plasta a&ubreve; metala vazo havanta tenilon kaj
      destinita
      por &ccirc;erpi, porti a&ubreve; teni flua&jcirc;on:
      <ekz>
        pluvis kvaza&ubreve; per <tld/>oj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>fali de <tld/>o en barelon</ind><fnt>Z</fnt>
        <klr>(de malbona stato en ankora&ubreve; pli malbonan.
        <ref tip="sin" cel="fal.de0i">defali</ref>
        <refgrp tip="vid">
          <ref cel="pluv.0o">pluvo</ref>,
          <ref cel="flam.0o">flamo</ref>
        </refgrp>)</klr>.
        <trd lng="hu">cs&ouml;b&ouml;rb&odblac;l v&ouml;d&ouml;rbe esik</trd>
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Enhavo de <tld/>o, rigardata kiel mezuro.
    </dif>
    <refgrp tip="sin">
      <ref cel="kuv.0o">kuvo</ref>,
      <ref cel="krucx.0o">kru&ccirc;o</ref>,
      <ref cel="tin.0o">tino</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_d;&c_r;&c_o;</trd>
  <trdgrp lng="bg">
    <trd>&c_k;&c_o;&c_f;&c_a;</trd>,
    <trd>&c_v;&c_je;&c_d;&c_r;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>. v&#x11B;dro</trd>,
    <trd>d&#x17E;ber</trd>,
    <trd>kbel&#xED;k</trd>,
    <trd>k&#xFD;bl</trd>,
    <trd>okov</trd>,
    <trd>putna</trd>,
    <trd>&#x17E;ejdl&#xED;k</trd>
  </trdgrp>
  <trd lng="de">Eimer</trd>
  <trdgrp lng="en">
    <trd>bucket</trd>,
    <trd>pail</trd>
  </trdgrp>
  <trd lng="fa">&a_s;&a_T;&a_l;</trd>
  <trd lng="fr">seau</trd>
  <trd lng="hu">v&ouml;d&ouml;r</trd>
  <trd lng="id">ember</trd>
  <trdgrp lng="ja">
    <trd>&#x30D0;&#x30B1;&#x30C4;  <pr>&#x3070;&#x3051;&#x3064;</pr></trd>,
    <trd>&#x624B;&#x6876;  <pr>&#x3066;&#x304A;&#x3051;</pr></trd>,
    <trd>&#x30D0;&#x30B1;&#x30C4;&#x4E00;&#x676F;  <pr>&#x3070;&#x3051;&#x3064;&#x3044;&#x3063;&#x3071;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">emmer</trd>
  <trd lng="pl">wiadro</trd>
  <trd lng="ru">&c_v;&c_je;&c_d;&c_r;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>okov <klr>(na &#x10D;erpanie)</klr></trd>,
    <trd>put&ncaron;a</trd>,
    <trd>vedro</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_r;&#x43E;</trd>,
    <trd>&c_c;&c_je;&c_b;&c_je;&c_r;&#x43A;&c_a;</trd>,
    <trd>&#x43A;&#x43E;&#x43D;&#x43E;&c_v;&#x43A;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6876; <pr>t&#x1D2;ng</pr></trd>,
    <trd>&#x7B72; <pr>sh&#x101;o</pr></trd>,
    <trd>&#x6C34;&#x6876; <pr>shu&#x1D0;t&#x1D2;ng</pr></trd>,
    <trd>&#x6C34;&#x7B72; <pr>shu&#x1D0;sh&#x101;o</pr></trd>,
    <trd>&#x63D0;&#x6876; <pr>t&#xED;t&#x1D2;ng</pr></trd>
  </trdgrp>
</drv>
</art>
