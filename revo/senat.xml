<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: senat.xml,v 1.28 2025/10/14 16:00:22 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>senat</rad>/o
<fnt><bib>JED</bib></fnt></kap>
<drv mrk="senat.0o">
  <kap><tld/>o</kap>
  <snc mrk="senat.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Plej alta le&gcirc;ofara anaro a&ubreve; supera &ccirc;ambro en
      parlamento:
      <ekz>
        le&gcirc;propono devas trapasi plurajn &scirc;tupojn: plej grava
        estas
        tiu, ke
        la proponon devas akcepti kaj &Ccirc;ambro de Deputitoj kaj <tld lit="S"/>o<fnt><aut>Guido Van
        Damme</aut>: <vrk>Puno ne sen
        peno</vrk>, <bib>Monato</bib><lok>jaro 2002a, numero 4a, p.
        15a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="senat.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      En <ref tip="vid" cel="romi.0o">Romio</ref> reganta komitato el
      nobeloj.
    </dif>
    <refgrp tip="vid">
      <ref cel="patric">patriciaro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_n;&c_a;&c_t;</trd>
  <trd lng="cs">sen&aacute;t</trd>
  <trd lng="de">Senat</trd>
  <trd lng="en">senate</trd>
  <trd lng="es">senado</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_n;&a_A;</trd>
    <trd>&a_m;&a_j;&a_l;&a_s; &a_s;&a_n;&a_A;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s&eacute;nat</trd>
  </trdgrp>
  <trd lng="hu">szen&aacute;tus</trd>
  <trd lng="it">senato</trd>
  <trdgrp lng="ja">
    <trd>&#19978;&#38498;  <pr>&#12376;&#12423;&#12358;&#12356;&#12435;</pr></trd>,
    <trd>&#21442;&#35696;&#38498;  <pr>&#12373;&#12435;&#12366;&#12356;&#12435;</pr></trd>,
    <trd>&#20803;&#32769;&#38498;  <pr>&#12370;&#12435;&#12429;&#12358;&#12356;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">senaat</trd>
  <trd lng="pl">senat</trd>
  <trd lng="pt">senado</trd>
  <trd lng="ru">&c_s;&c_je;&c_n;&c_a;&c_t;</trd>
  <trd lng="sk">sen&aacute;t</trd>
  <trd lng="uk">&c_s;&c_je;&c_n;&c_a;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#21442;&#35758;&#38498; <pr>c&#257;ny&igrave;yu&agrave;n</pr></trd>,
    <trd>&#21443;&#35696;&#38498; <pr>c&#257;ny&igrave;yu&agrave;n</pr></trd>,
    <trd>&#20803;&#32769;&#38498; <pr>yu&aacute;nl&#462;oyu&agrave;n</pr></trd>,
    <trd>&#19978;&#35696;&#38498; <pr>Sh&agrave;ngy&igrave;yu&agrave;n</pr></trd>,
    <trd>&#19978;&#35758;&#38498; <pr>Sh&agrave;ngy&igrave;yu&agrave;n</pr></trd>,
    <trd>&#19978;&#35696;&#38498; <pr>sh&agrave;ngy&igrave;yu&agrave;n</pr></trd>,
    <trd>&#19978;&#35758;&#38498; <pr>sh&agrave;ngy&igrave;yu&agrave;n</pr></trd>,
    <trd>&#19978;&#38498; <pr>sh&agrave;ngyu&agrave;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="senat.0ano">
  <kap><tld/>ano</kap>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Ano de <tld/>o:
      <ekz>
        parlamentanoj kaj <tld/>anoj kuni&gcirc;is poste<fnt><aut>Betty
        Refior</aut>: <vrk>Terorismo (5)</vrk>,
        <bib>Monato</bib><lok>jaro
        2001a, numero 12a, p. 4a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="patric1.0o">patricio</ref>,
      <ref cel="lord.0o">lordo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_n;&c_a;&c_t;&c_a;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>sen&aacute;tor</trd>,
    <trd>&ccaron;len sen&aacute;tu</trd>
  </trdgrp>
  <trd lng="de">Senator</trd>
  <trd lng="en">senator</trd>
  <trd lng="es">senador</trd>
  <trd lng="fa">&a_s;&a_n;&a_A;&a_t;&a_w;&a_r;</trd>
  <trdgrp lng="fr">
    <trd>s&eacute;nateur</trd>
  </trdgrp>
  <trd lng="hu">szen&aacute;tor</trd>
  <trd lng="it">senatore</trd>
  <trdgrp lng="ja">
    <trd>&#19978;&#38498;&#35696;&#21729;  <pr>&#12376;&#12423;&#12358;&#12356;&#12435;&#12366;&#12356;&#12435;</pr></trd>,
    <trd>&#21442;&#35696;&#38498;&#35696;&#21729;  <pr>&#12373;&#12435;&#12366;&#12356;&#12435;&#12366;&#12356;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">senator</trd>
  <trd lng="pl">senator</trd>
  <trd lng="pt">senador</trd>
  <trd lng="ru">&c_s;&c_je;&c_n;&c_a;&c_t;&c_o;&c_r;</trd>
  <trd lng="sk">sen&aacute;tor</trd>
  <trd lng="uk">&c_s;&c_je;&c_n;&c_a;&c_t;&c_o;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#21442;&#35758;&#21592; <pr>c&#257;ny&igrave;yu&aacute;n</pr></trd>,
    <trd>&#21443;&#35696;&#21729; <pr>c&#257;ny&igrave;yu&aacute;n</pr></trd>,
    <trd>&#19978;&#35696;&#38498;&#35696;&#21729; <pr>sh&agrave;ngy&igrave;yu&agrave;ny&igrave;yu&aacute;n</pr></trd>,
    <trd>&#19978;&#35758;&#38498;&#35758;&#21592; <pr>sh&agrave;ngy&igrave;yu&agrave;ny&igrave;yu&aacute;n</pr></trd>,
    <trd>&#20803;&#32769;&#38498;&#35696;&#21729; <pr>yu&aacute;nl&#462;oyu&agrave;ny&igrave;yu&aacute;n</pr></trd>,
    <trd>&#20803;&#32769;&#38498;&#35758;&#21592; <pr>yu&aacute;nl&#462;oyu&agrave;ny&igrave;yu&aacute;n</pr></trd>,
    <trd>&#29702;&#20107; <pr>l&#464;sh&igrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="senat.0estro">
  <kap><tld/>estro</kap>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      <tld lit="S"/>ano, kiu gvidas internajn debatojn, ekstere
      reprezentas la <tld/>on, a&ubreve; iel simile funkcias depende je
      la
      loka
      kutimo:
      <ekz>
        ambicia <tld/>estro, iama ministro kaj guberniestro de la
        &scirc;tato
        Para<fnt><aut>Gilbert R. Ledon</aut>: <vrk>Esenco de
        korupto</vrk>, <bib>Monato</bib><lok>jaro 2002a, numero 1a, p.
        8a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_p;&c_ib;&c_k;&c_je;&c_r; &c_s;&c_e;&c_n;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_r;&c_sh;&c_y;&c_n;&c_ja; &c_s;&c_e;&c_n;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_sh;&c_a;&c_l;&c_a;&c_k; &c_s;&c_e;&c_n;&c_a;&c_t;&c_a; <klr>(&c_u;
    &c_P;&c_o;&c_l;&c_mol;&c_sh;&c_ch;&c_y;)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>senate president</trd>,
    <trd>speaker of a senate</trd>, <trd>marshal of a senate</trd>,
    <trd>chairman of a senate</trd>
  </trdgrp>
  <trd lng="fa">&a_r;&a_y_hamza;&f_y;&a_s; &a_s;&a_n;&a_A;</trd>
  <trdgrp lng="fr">
    <trd>pr&eacute;sident du <ind>s&eacute;nat</ind></trd>,
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szen&aacute;tus eln&ouml;ke</trd>
  </trdgrp>
  <trd lng="it">presidente del <ind>senato</ind></trd>
  <trd lng="pl">marsza&lstroke;ek <ind>senatu</ind></trd>
  <trd lng="pt">presidente do senado</trd>
</drv>
</art>
