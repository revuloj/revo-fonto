<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>totem</rad>/o
</kap>
<drv mrk="totem.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Viva a&ubreve; malviva esta&jcirc;o, &gcirc;enerale besto 
a&ubreve; vegeta&jcirc;o konsiderata de iu
      grupo (<ref cel="klan.0o.ETN">klano</ref>) de primitivuloj kiel 
praulo kaj protektanto de la
      grupo, al kiu &gcirc;i servas kiel insigno kaj kolektiva nomo.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_t;&c_e;&c_m;</trd>
  <trdgrp lng="de">
    <trd>Totem</trd>,
    <trd>Stammeszeichen</trd>
  </trdgrp>
  <trd lng="en">totem</trd>
  <trd lng="es">t&oacute;tem</trd>
  <trd lng="hu">totem</trd>
  <trd lng="nl">totem</trd>
  <trd lng="ru">&c_t;&c_o;&c_t;&c_je;&c_m;</trd>
</drv>

<drv mrk="totem.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      Soci-religia sistemo bazita sur parenceco kaj ligo inter iu <tld/>o
      kaj iu homa grupo.
    </dif>
    <refgrp tip="sin">
      <ref cel="tabu.0o">tabuo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_t;&c_e;&c_m;&c_ib;&c_z;&c_m;</trd>
  <trdgrp lng="de">
    <trd>Totemismus</trd>,
    <trd>Stammesglaube</trd>
  </trdgrp>
  <trd lng="en">totemism</trd>
  <trd lng="es">totemismo</trd>
  <trd lng="hu">totemizmus</trd>
  <trd lng="nl">totemisme</trd>
  <trd lng="ru">&c_t;&c_o;&c_t;&c_je;&c_m;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
