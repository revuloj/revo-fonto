<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>stratigrafi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="stratigrafi.0o">
  <kap><tld/>o</kap>
  <snc mrk="stratigrafi.0o.KAL">
    <uzo tip="fak">ARKE</uzo>
    <uzo tip="fak">GEOL</uzo>
    <uzo tip="fak">KAL</uzo>
    <dif>
      Studo de tertavoloj, de ilia konsisto,
      esti&gcirc;o, la&ubreve;tempa evoluo;
      rimedo de <ref tip="vid" cel="kronol.0o.SCI">kronologio</ref>:
      <ekz>
        la scienco de <tld/>o multe progresis
        &ccirc;ar geologoj komencis uzi fosiliojn
        por identigi stratumarojn
        <fnt>
          <bib>OriginoDeSpecioj</bib>,
          <lok>Notoj pri la Traduka&jcirc;o</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">estratigrafia</trd>
  <trd lng="de">Stratigraphie</trd>
  <trd lng="fr">stratigraphie</trd>
  <trd lng="sk">stratigrafia</trd>
</drv>
</art>

