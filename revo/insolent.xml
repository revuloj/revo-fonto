<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>insolent</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="insolent.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">POE</uzo>
  <snc>
    <ref tip="dif" cel="impert.0a">impertinenta</ref>
  </snc>
  <trdgrp lng="fr">
    <trd>effront&eacute;</trd>,
    <trd>impertinent</trd>,
    <trd>insolent</trd>
  </trdgrp>
  <trd lng="fr">insolent</trd>
</drv>

<drv mrk="insolent.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">POE</uzo>
  <snc>
    <dif>
      Impertinento:
      <ekz>
        venu &ccirc;i tien vidi kian pagon vi ricevos por via stulta
        <tld/>o<fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 3a, p.
        41a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>effronterie</trd>,
    <trd>impertinence</trd>,
    <trd>insolence</trd>
  </trdgrp>
</drv>
</art>

