<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: insolent.xml,v 1.11 2022/03/21 15:27:49 revo Exp $">
<kap>
   <rad>insolent</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="insolent.0a">
   <kap><tld/>a</kap>
   <uzo tip="stl">RAR</uzo>
   <snc>
     <ref tip="dif" cel="impert.0a">impertinenta</ref>
   </snc>
  <trd lng="be">&c_n;&c_a;&c_h;&c_a;&c_b;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>drz&#xFD;</trd>,
    <trd>nestoudn&#xFD;</trd>,
    <trd>opov&#xE1;&#x17E;liv&#xFD;</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>insolent</trd>,
     <trd>arrogant</trd>,
     <trd>haughty</trd>,
     <trd>lordly</trd>,
     <trd>overbearing</trd>,
     <trd>supercillious</trd>,
     <trd>lofty</trd>,
     <trd>overweening</trd>,
     <trd>proud</trd>,
     <trd>prideful</trd>
   </trdgrp>
  <trdgrp lng="fr">
     <trd>effront&eacute;</trd>,
     <trd>impertinent</trd>,
     <trd>insolent</trd>
   </trdgrp>
  <trdgrp lng="hu">
     <trd>szemtelen</trd>,
     <trd>arc&aacute;tlan</trd>
   </trdgrp>
  <trdgrp lng="pt">
    <trd>impertinente<klr tip="ind"> (adj.)</klr></trd>, 
    <trd>insolente<klr tip="ind"> (adj.)</klr></trd> 
   </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_d;&c_je;&c_r;&c_z;&c_k;&c_i;&c_j;</trd>,
     <trd>&c_n;&c_a;&c_g;&c_l;&c_y;&c_j;</trd>,
     <trd>&c_n;&c_a;&c_h;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>drz&#xFD;</trd>,
    <trd>vyz&#xFD;vav&#xFD;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x437;&#x443;&#x445;&#x432;&#x430;&#x43B;&#x438;&#x439;</trd>,
    <trd>&#x43D;&#x430;&#x445;&#x430;&#x431;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x431;&#x435;&#x437;&#x446;&#x435;&#x440;&#x435;&#x43C;&#x43E;&#x43D;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="insolent.0o">
   <kap><tld/>o</kap>
   <uzo tip="stl">RAR</uzo>
   <snc>
     <dif>
       Impertinento:
       <ekz>
         venu &ccirc;i tien vidi kian pagon vi ricevos por via stulta
         <tld/>o<fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 3a, p.
         41a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
  <trdgrp lng="be">
     <trd>&c_n;&c_a;&c_h;&c_a;&c_b;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
     <trd>&c_n;&c_a;&c_h;&c_a;&c_b;&c_s;&c_t;&c_v;&c_a;</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>drzost</trd>,
    <trd>insolence</trd>,
    <trd>nestoudnost</trd>
  </trdgrp>
  <trdgrp lng="fr">
     <trd>effronterie</trd>,
     <trd>impertinence</trd>,
     <trd>insolence</trd>
   </trdgrp>
  <trdgrp lng="hu">
     <trd>szemtelens&eacute;g</trd>,
     <trd>arc&aacute;tlans&aacute;g</trd>
   </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_d;&c_je;&c_r;&c_z;&c_o;&c_s;&c_t;&c_mol;</trd>,
     <trd>&c_n;&c_a;&c_g;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>,
     <trd>&c_n;&c_a;&c_h;&c_a;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>drzos&#x165;</trd>,
    <trd>nehanebnos&#x165;</trd>
  </trdgrp>
</drv>
</art>
