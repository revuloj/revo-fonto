<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: insolent.xml,v 1.12 2023/11/19 09:22:21 revo Exp $">
<kap>
   <rad>insolent</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="insolent.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="impert.0a">Impertinenta</ref>:
      <ekz>
        <nom>Em&egrave;ric</nom> demandis La Majstron pri <klr>[&#8230;]</klr> a&hcirc;ila kalkano de la <tld/>a
        madridano
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 6</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la paca tempo iu politika ofendo, iu <tld/>a inter&scirc;tata kontrakto
        <klr>[&#8230;]</klr> iritas la homojn multe pli ol la malka&scirc;a batalo
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#8470;10</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="arog.0i">aroga</ref>,
      <ref cel="modest.mal0a">malmodesta</ref>,
      <ref cel="hont.sen0a">senhonta</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_h;&c_a;&c_b;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>drz&yacute;</trd>,
    <trd>nestoudn&yacute;</trd>,
    <trd>opov&aacute;&zcaron;liv&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ungeb&uuml;hrlich</trd>,
    <trd>anma&szlig;end</trd>,
    <trd>unversch&auml;mt</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>insolent</trd>,
     <trd>arrogant</trd>,
     <trd>haughty</trd>,
     <trd>lordly</trd>,
     <trd>overbearing</trd>,
     <trd>supercillious</trd>,
     <trd>lofty</trd>,
     <trd>overweening</trd>,
     <trd>proud</trd>,
     <trd>prideful</trd>
   </trdgrp>
  <trdgrp lng="fr">
     <trd>effront&eacute;</trd>,
     <trd>impertinent</trd>,
     <trd>insolent</trd>
   </trdgrp>
  <trdgrp lng="hu">
     <trd>szemtelen</trd>,
     <trd>arc&aacute;tlan</trd>
   </trdgrp>
  <trdgrp lng="pt">
    <trd>impertinente<klr tip="ind"> (adj.)</klr></trd>,
    <trd>insolente<klr tip="ind"> (adj.)</klr></trd>
   </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_d;&c_je;&c_r;&c_z;&c_k;&c_i;&c_j;</trd>,
     <trd>&c_n;&c_a;&c_g;&c_l;&c_y;&c_j;</trd>,
     <trd>&c_n;&c_a;&c_h;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>drz&yacute;</trd>,
    <trd>vyz&yacute;vav&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_u;&c_h;&c_v;&c_a;&c_l;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_a;&c_h;&c_a;&c_b;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_c;&c_je;&c_r;&c_je;&c_m;&c_o;&c_n;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="insolent.0o">
   <kap><tld/>o</kap>
   <uzo tip="stl">RAR</uzo>
   <snc>
     <dif>
       Impertinento:
       <ekz>
         venu &ccirc;i tien vidi kian pagon vi ricevos por via stulta
         <tld/>o<fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 3a, p.
         41a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
  <trdgrp lng="be">
     <trd>&c_n;&c_a;&c_h;&c_a;&c_b;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
     <trd>&c_n;&c_a;&c_h;&c_a;&c_b;&c_s;&c_t;&c_v;&c_a;</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>drzost</trd>,
    <trd>insolence</trd>,
    <trd>nestoudnost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Anma&szlig;ung</trd>,
    <trd>Unversch&auml;mtheit</trd>
  </trdgrp>
  <trdgrp lng="fr">
     <trd>effronterie</trd>,
     <trd>impertinence</trd>,
     <trd>insolence</trd>
   </trdgrp>
  <trdgrp lng="hu">
     <trd>szemtelens&eacute;g</trd>,
     <trd>arc&aacute;tlans&aacute;g</trd>
   </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_d;&c_je;&c_r;&c_z;&c_o;&c_s;&c_t;&c_mol;</trd>,
     <trd>&c_n;&c_a;&c_g;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>,
     <trd>&c_n;&c_a;&c_h;&c_a;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>drzos&tcaron;</trd>,
    <trd>nehanebnos&tcaron;</trd>
  </trdgrp>
</drv>
</art>
