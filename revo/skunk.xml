<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: skunk.xml,v 1.6 2019/10/19 16:10:23 revo Exp $">
<kap>
  <rad>skunk</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="skunk.0o">
  <kap><tld/>o</kap>
  <snc mrk="skunk.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="mefit.0o.ZOO">Mefito</ref>:
      <ekz>
        en sova&gcirc;a naturo tiuj estas vulpoj, lupoj, &scirc;akaloj, procionoj, <tld/>oj,
        vespertoj kaj aliaj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="https://www.monato.be/2008/009550.php"
              >Batalo kontra&ubreve; mortiga malsano</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        kio okazos, se mi pafmortigos tiun <tld/>on
        <fnt>
          <aut>V. Z&iacute;ka, trad. P. Toma&scaron;ovsk&yacute;</aut>,
          <vrk><url
          ref="http://i-espero.info/files/elibroj/eo%20-%20zika,%20vlada%20-%20justa%20chess.pdf">
          Justa Chess</url></vrk>,
          <lok>1990</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_u;&c_n;&c_s;</trd>
  <trd lng="bg">&c_s;&c_k;&c_u;&c_n;&c_k;&c_s;</trd>
  <trd lng="ca">mofeta</trd>
  <trd lng="cs">skunk</trd>
  <trdgrp lng="de">
    <trd>Stinktier</trd>,
    <trd>Skunk</trd>
  </trdgrp>
  <trd lng="en">skunk</trd>
  <trd lng="fr">moufette</trd>
  <trdgrp lng="hu">
    <trd>b&udblac;zborz</trd>,
    <trd>szkunk</trd>
  </trdgrp>
  <trd lng="nl">stinkdier</trd>
  <trdgrp lng="pt">
    <trd>jaritataca</trd>,
    <trd>cangamb&aacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_u;&c_n;&c_s;</trd>,
    <trd>&c_v;&c_o;&c_n;&c_ju;&c_ch;&c_k;&c_a; <klr tip="amb">(&c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_o;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="sk">skunk</trd>
</drv>
</art>

