<art mrk="$Id: kolik.xml,v 1.19 2023/10/18 17:23:24 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>kolik</rad>/o
</kap>

<drv mrk="kolik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Intesta doloro.
    </dif>
    <trdgrp lng="en">
      <trd>colic</trd>,
      <trd>gripes</trd>,
      <trd>intestinal <klr>cramps</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_je;&c_z;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Interna spasma doloro:
      <ekz>
        hepata, rena <tld/>o.
      </ekz>
    </dif>
  </snc>
  <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>
  <trdgrp lng="be">
    <trd>&c_k;&c_o;&c_l;&c_ib;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_e;&c_z;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_a;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">c&ograve;lic</trd>
  <trd lng="de">Kolik</trd>
  <trd lng="es">c&oacute;lico</trd>
  <trd lng="fr">colique<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="id">kolik</trd>
  <trdgrp lng="ja">
    <trd>&#x759D;&#x75DB;  <pr>&#x305B;&#x3093;&#x3064;&#x3046;</pr></trd>,
    <trd>&#x5DEE;&#x3057;&#x8FBC;&#x307F;  <pr>&#x3055;&#x3057;&#x3053;&#x307F;</pr></trd>,
    <trd>&#x8179;&#x75DB;  <pr>&#x3075;&#x304F;&#x3064;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">koliek</trd>
  <trd lng="pl">kolka</trd>     
  <trd lng="ro">colic&abreve;</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">kolika</trd>
  <trd lng="cs">kolika <klr>(z&aacute;chvat)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&#x43B;&#x44C;&#x43A;&#x430;</trd>,
    <trd>&#x440;&#x456;&#x437;&#x44C;</trd>
  </trdgrp>
</drv>
</art>
