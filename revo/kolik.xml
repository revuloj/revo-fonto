<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kolik</rad>/o
</kap>
<drv mrk="kolik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc num="1">
    <dif>
      Intesta doloro. 
    </dif>
    <trdgrp lng="en">
      <trd>colic</trd>,
      <trd>gripes</trd>,
      <trd>intestinal <klr>cramps</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_je;&c_z;&c_mol;</trd>
  </snc>
  <snc num="2">
    <dif>
      Interna spasma doloro: <ekz>hepata, rena <tld/>o.</ekz>
    </dif>
  </snc>
  <trd lng="nl">koliek</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_i;&c_k;&c_a;</trd>
</drv>
</art>
