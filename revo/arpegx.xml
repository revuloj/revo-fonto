<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: arpegx.xml,v 1.26 2025/08/19 10:00:22 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>arpe&gcirc;</rad>/o
<fnt><bib>JED</bib></fnt></kap>
<drv mrk="arpegx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="ornam.melodi0o">Melodiornamo</ref>
      uzata en klasika e&ubreve;ropa muziko,
      kiu komencas la tonojn de akordo ne samtempe sed prokrastete:
      <ekz>
        Bruis &ccirc;e la pord' ordone! / Pliinsistas jam la bru', / Kvaza&ubreve; dum koler'
        <tld/>oj, Kun minaco de detru'
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        duparta varia&jcirc;o precipe konsistanta el gamaj linioj kaj <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=32931">Goldberg-variacioj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_p;&c_e;&c_d;&c_zh;&c_y;&c_jo;</trd>
  <trd lng="bg">&c_a;&c_r;&c_p;&c_je;&c_zh;&c_i;&c_o;</trd>
  <trd lng="ca">arpegi</trd>
  <trd lng="de">Arpeggio</trd>
  <trd lng="en">arpeggio</trd>
  <trd lng="es">arpegio</trd>
  <trd lng="fr">arp&egrave;ge</trd>
  <trd lng="he">&alef;&qof;&vav;&resh;&dalet; &shin;&bet;&vav;&resh;</trd>
  <trdgrp lng="hu">
    <trd>t&ouml;rt <ind>hangzat</ind></trd>,
    <trd>arpeggio</trd>
  </trdgrp>
  <trd lng="it">arpeggio</trd>
  <trd lng="ja">&#12450;&#12523;&#12506;&#12472;&#12458;</trd>
  <trd lng="nl">arpeggio</trd>
  <trd lng="ru">&c_a;&c_r;&c_p;&c_je;&c_d;&c_zh;&c_i;&c_o;</trd>
  <trd lng="uk">&c_a;&c_r;&c_p;&c_je;&c_d;&c_zh;&c_ib;&c_o;</trd>
</drv>

<drv mrk="arpegx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>ojn.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_c;&c_mol;
  <ind>&c_a;&c_r;&c_p;&c_e;&c_d;&c_zh;&c_y;&c_jo;</ind></trd>
  <trdgrp lng="ca">
    <trd>fer <ind>arpegi</ind>s</trd>,
    <trd>arpegiar</trd>
  </trdgrp>
  <trd lng="en">sing arpeggios</trd>
  <trd lng="es">arpegiar</trd>
  <trdgrp lng="fr">
    <trd>arp&eacute;ger<klr> (intr.)</klr></trd>
  </trdgrp>
  <trd lng="it">arpeggiare</trd>
  <trd lng="ja">&#12450;&#12523;&#12506;&#12472;&#12458;&#12391;&#28436;&#22863;&#12377;&#12427; 
  <pr>&#12450;&#12523;&#12506;&#12472;&#12458;&#12391;&#12360;&#12435;&#12381;&#12358;&#12377;&#12427;</pr></trd>
</drv>
</art>
