<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>arpe&gcirc;</rad>/o
</kap>
<drv mrk="arpegx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Akordo, kies sonojn oni ne a&ubreve;digas samtempe, sed unu post la alia.
    </dif>
  </snc>
  <trd lng="de">Arpeggio</trd>
  <trd lng="fr">arp&egrave;ge</trd>
  <trd lng="ru">&c_a;&c_r;&c_p;&c_je;&c_d;&c_zh;&c_i;&c_o;</trd>
</drv>

<drv mrk="arpegx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Fari <tld/>ojn.
    </dif>
  </snc>
</drv>
</art>
