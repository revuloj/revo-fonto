<drv mrk="kolagen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZL</uzo>
  <snc>
    <dif>
      Iu el struturaj <ref tip="super" cel="protein.0o">proteino</ref>j &ccirc;e bestoj formante
      inter&ccirc;elajn fibrojn,
      trovi&gcirc;antaj i.a. en tendenoj, kartilago, suba ha&ubreve;ttavolo:
      <ekz>
        en la pulmoj, la triagrada fumado &scirc;ajnas respondeci pri ekscesa produktado de
        <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011291.php"
              >Kie fumo levi&gcirc;as, tie fajro (kaj malsanoj) trovi&gcirc;as</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_l;&c_a;&c_g;&c_je;&c_n;</trd>
    <trd lng="de">Kollagen</trd>
    <trd lng="en">collagen</trd>
    <trd lng="fr">collag&egrave;ne</trd>
    <trd lng="ru">&c_k;&c_o;&c_l;&c_l;&c_a;&c_g;&c_je;&c_n;</trd>
  </snc>
</drv>

  </art>

