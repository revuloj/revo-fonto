<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: epifenomen.xml,v 1.5 2022/11/17 12:58:49 revo Exp $">
<kap>
  <rad>epifenomen</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="epifenomen.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="epifenomen.0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Malgrava apera&jcirc;o apud la precipa, celata,
      koncerna apera&jcirc;o:
      <ekz>
        tiu obskura karaktero tamen prezentas nur 
        akcesora&jcirc;on, <tld/>on
        <fnt>
          <aut>D. Kessous</aut>,
          <vrk><url ref="http://www.satesperanto.org/Rauxmismo-kaj-postmodernismo.html">
          Ra&ubreve;mismo kaj postmodernismo</url></vrk>,
          <lok>[vidita en 2013]</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="efik.krom0o.SCI">kromefiko</ref>,
      <ref cel="efik.post0o">postefiko</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_p;&c_ib;&c_f;&c_e;&c_n;&c_o;&c_m;&c_e;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>epifenomen</trd>,
    <trd>soud&#x11B;j</trd>,
    <trd>vedlej&scaron;&#xED; d&#x11B;j</trd>
  </trdgrp>
  <trd lng="en">epiphenomenon</trd>
  <trd lng="fr">&eacute;piph&eacute;nom&egrave;ne</trd>
  <trd lng="sk">ved&#x13E;aj&scaron;&#xED; dej</trd>
  <trd lng="uk">&c_je;&#x43F;&c_ib;&c_f;&c_je;&#x43D;&#x43E;&#x43C;&c_je;&#x43D;</trd>
</drv>
</art>

