  <trd lng="ca">democr&agrave;cia</trd>
  <trd lng="cs">demokracie</trd>
  <trd lng="de">Demokratie</trd>
  <trd lng="en">democracy</trd>
  <trd lng="he">&#x5D3;&#x5B5;&#x5DE;&#x5D5;&#x5B9;&#x5E7;&#x5E8;&#x5B8;&#x5D8;&#x5B4;&#x5D9;&#x5B8;&#x5D4;</trd>
