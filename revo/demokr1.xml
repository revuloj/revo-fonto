<drv mrk="demokr1.0o">
  <kap><tld/>o</kap>
  <snc mrk="demokr1.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Politika <ref tip="super" cel="regxim.0o.POL">re&gcirc;imo</ref>,
      en kiu <ref cel="suvere.0o">suvereno</ref> estas,
      jen pere, jen senpere la popolo, kaj en kiu fortas
      la sendistinge egaligaj tendencoj:
      <ekz>
        parlamenta <tld/>o
        <fnt>
          <bib>FSp</bib>
          <lok>Dua Parto: Svisa Sperto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a respubliko
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        inter la jaroj 470 kaj 460 la urboj-&scirc;tatoj de Sicilio sukcesis forigi la
        tiranecajn re&gcirc;imojn kaj establis <tld/>on
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi devis anta&ubreve;e rakonti al li, kio estas &ccirc;e ni la &scirc;tatorganizo, kio la <tld/>o,
        kion signifas la diktaturo, parlamentismo, la elektado, nomado, ktp
        <fnt>
          <bib>VaK</bib>
          <lok>Kvina &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        en politikaj <tld/>oj malobservo de elementa socia disciplino kondukas al detruo
        de demokrataj institucioj kaj establo de totaleca diktaturo
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        vera <tld/>o postulas, ke &ccirc;iuj tenantoj de publikaj oficoj
        elekti&gcirc;u de la komunumo
        <fnt>
        <aut>W. H. Simcock</aut>
          <vrk>&Ccirc;u en Britio?</vrk>
          <bib>Monato</bib>
          <lok>1997:4, p. 9a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="respub.0o">respubliko</ref>
    <ref tip="vid" cel="demokr.0o">demokrato</ref>
    <trdgrp lng="de">
      <trd>Demokratie</trd>,
      <trd>Volksherrschaft</trd>
    </trdgrp>
    <trd lng="es">democracia</trd>
    <trd lng="fr">d&eacute;mocratie</trd>
    <trdgrp lng="hu">
      <trd>demokr&aacute;cia</trd>,
      <trd>n&eacute;puralom</trd>
    </trdgrp>
    <trd lng="pl">demokracja</trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_je;&c_m;&c_o;&c_k;&c_r;&c_a;&c_t;&c_i;&c_ja;</trd>,
      <trd>&c_n;&c_a;&c_r;&c_o;&c_d;&c_o;&c_v;&c_l;&c_a;&c_s;&c_t;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
<!--
  <snc>
    <dif>
      Demokrataro.
    </dif>
    <adm>
      &Ccirc;i tiu senco estas dubinda. Eble &gcirc;i celas tiujn
      partiojn, kiuj metafore nomas sin "D~o".
      Se ni devas distingi subsencojn,
      &scirc;ajnas al mi, ke ni povus diri, ke la vorto apliki&gcirc;as
      al filozofipolitika sistemo, al politika re&gcirc;imo kaj al
      &scirc;tato.
      <aut>MB</aut>
    </adm>
  </snc>
-->
  <trd lng="be">&c_d;&c_e;&c_m;&c_a;&c_k;&c_r;&c_a;&c_t;&c_y;&c_ja;</trd>
  <trd lng="bo">&t_da;&t_ma;&t_nga;&t_sa;&t_tsheg;&t_ga;&t_tsa;&t_naro;&t_-a;&t_gigu;&t_tsheg;&t_ra;&t_gigu;&t_nga;&t_tsheg;&t_la;&t_shabchu;&t_ga;&t_sa;&t_tsheg;</trd>
  <trd lng="ca">democr&agrave;cia</trd>
  <trd lng="cs">demokracie</trd>
  <trd lng="de">Demokratie</trd>
  <trd lng="en">democracy</trd>
  <trd lng="he">&dalet;&#1461;&mem;&vav;&#1465;&qof;&resh;&#1464;&tet;&#1460;&yod;&#1464;&he;</trd>
  <trdgrp lng="ja">
    <trd>&#27665;&#20027;&#25919;&#27835;  <pr>&#12415;&#12435;&#12375;&#12421;&#12379;&#12356;&#12376;</pr></trd>,
    <trd>&#27665;&#20027;&#25919;&#20307;  <pr>&#12415;&#12435;&#12375;&#12421;&#12379;&#12356;&#12383;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">democratie</trd>
  <trd lng="it">democrazia</trd>
  <trd lng="pt">democracia</trd>
  <trd lng="sk">demokracia</trd>
  <trd lng="sv">demokrati</trd>
  <trd lng="uk">&c_d;&c_je;&c_m;&c_o;&c_k;&c_r;&c_a;&c_t;&c_ib;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#27665;&#20027;&#25919;&#27835; <pr>m&iacute;nzh&#468;zh&egrave;ngzh&igrave;</pr></trd>,
    <trd>&#27665;&#20027; <pr>m&iacute;nzh&#468;</pr></trd>,
    <trd>&#27665;&#20027;&#20027;&#20041; <pr>m&iacute;nzh&#468;zh&#468;y&igrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="demokr1.0a">
  <kap><tld/>a</kap>
  <snc mrk="demokr1.0a.POL">
    <dif>
      Konforma al <tld/>o, rilata al <tld/>o:
      <ekz>
        <tld/>aj landoj
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a debato
        <fnt>
          <bib>Kon11</bib>
          <lok>Scienco</lok>
        </fnt>;
      </ekz>
      <ekz>
        la rolo de Esperanto kiel <tld/>a komunikilo
        <fnt>
        <aut>J. G. Champlin</aut>
        <vrk>Kiel pentri demokration?</vrk>
        <bib>Monato</bib>
        <lok>2001:12, p. 20a</lok></fnt>
      </ekz>.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_m;&c_a;&c_k;&c_r;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">democr&agrave;tic</trd>
  <trd lng="cs">demokratick&yacute;</trd>
  <trd lng="de">demokratisch</trd>
  <trd lng="en">democratic</trd>
  <trd lng="fr">d&eacute;mocratique</trd>
  <trd lng="he">&dalet;&#1461;&mem;&vav;&#1465;&qof;&resh;&#1464;&tet;&#1460;&yod;</trd>
  <trd lng="hu">demokratikus</trd>
  <trd lng="it">democratico</trd>
  <trdgrp lng="ja">
    <trd>&#27665;&#20027;&#30340;&#12394;  <pr>&#12415;&#12435;&#12375;&#12421;&#12390;&#12365;&#12394;</pr></trd>,
    <trd>&#27665;&#20027;&#25919;&#27835;&#12398;  <pr>&#12415;&#12435;&#12375;&#12421;&#12379;&#12356;&#12376;&#12398;</pr></trd>
  </trdgrp>
  <trd lng="nl">democratisch</trd>
  <trd lng="pl">demokratyczny</trd>
  <trd lng="ru">&c_d;&c_je;&c_m;&c_o;&c_k;&c_r;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">demokratick&yacute;</trd>
  <trd lng="uk">&c_d;&c_je;&c_m;&c_o;&c_k;&c_r;&c_a;&c_t;&c_i;&c_ch;&c_n;&c_i;&c_j;</trd>
</drv>

<drv mrk="demokr1.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="demokr1.0igi.POL">
    <dif>
      Igi <tld/>o, igi <tld/>a, submeti al rekta a&ubreve; malrekta regado de la popolo:
      <ekz>
        generalo <nom>Lansana Conte</nom>, kiu kaptis la
        potencon dum &scirc;tatrenverso en 1984,
        ekde tiam <klr>[&#8230;]</klr> rezistis provojn
        <tld/>igi la landon
        <fnt>
          <aut>P. H. Oguinye, pg</aut>
          <vrk><url ref="&Monato;/2009/009726.php">
          Solidaras soldatoj sed &ccirc;u por demokratio?</url></vrk>
          <bib>Monato</bib>
          <lok>2009:03, p. 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        post la malvenko <klr>[&#8230;]</klr> usonanoj provis <tld/>igi Japanion
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="&Monato;/2006/008568.php"
              >Februaraj festoj</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        se ni volas <tld/>igi la sciencajn sciojn, la &ccirc;efa baro transpasenda estas la
        lingva
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;u Scienco per Esperanto? &Ccirc;u Esperanto per Scienco?</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_m;&c_a;&c_k;&c_r;&c_a;&c_t;&c_y;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">demokratisieren</trd>
  <trd lng="fr">d&eacute;mocratiser</trd>
  <trd lng="ja">&#27665;&#20027;&#21270;&#12377;&#12427;  <pr>&#12415;&#12435;&#12375;&#12421;&#12363;&#12377;&#12427;</pr></trd>
</drv>

<drv mrk="demokr1.0igo">
  <kap><tld/>igo</kap>
  <snc mrk="demokr1.0igo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      &Scirc;an&gcirc;o de regado al <tld/>o a&ubreve; al plia <tld/>eco:
      <ekz>
        tio grave grandigis la potencialan spektantaron
        de <nom>Do&jcirc;d</nom>, kaj estis unu el la tre malmultaj
        atingoj de <tld/>igo dum la kvar jaroj de
        <nom>Medvedev</nom> en Kremlo
        <fnt>
          <aut>K. Kniivil&auml;</aut>
          <vrk>Homoj de Putin</vrk>
          <lok>2016</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_m;&c_a;&c_k;&c_r;&c_a;&c_t;&c_y;&c_z;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trd lng="de">Demokratisierung</trd>
  <trd lng="fr">d&eacute;mocratisation</trd>
  <trd lng="he">&dalet;&mem;&vav;&qof;&resh;&tet;&yod;&zayin;&tsadi;&yod;&he;</trd>
  <trd lng="ja">&#27665;&#20027;&#21270;  <pr>&#12415;&#12435;&#12375;&#12421;&#12363;</pr></trd>
</drv>

<drv mrk="demokr1.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc mrk="demokr1.0igxo.evoluo">
    <dif>
      Evoluo de re&gcirc;imo a&ubreve; institucio al <tld/>a funkciado:
      <ekz>
        pri orienta E&ubreve;ropo ni ne parolu, &ccirc;ar tie la
        <tld/>i&gcirc;o ankora&ubreve; da&ubreve;ras
        <fnt>
        <aut>S. Maul</aut>
        <vrk>Alarm-signalo el Italio</vrk>
        <bib>Monato</bib>
        <lok>1993:8, p. 5a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_m;&c_a;&c_k;&c_r;&c_a;&c_t;&c_y;&c_z;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trd lng="ca">democratitzaci&oacute;</trd>
  <trd lng="de">Demokratisierung</trd>
  <trd lng="en">democratization</trd>
  <trd lng="fr">d&eacute;mocratisation</trd>
  <trd lng="hu">demokratiz&aacute;l&oacute;d&aacute;s</trd>
  <trd lng="nl">democratisering</trd>
  <trd lng="it">democratizzazione</trd>
  <trd lng="pl">demokratyzacja</trd>
  <trd lng="ru">&c_d;&c_je;&c_m;&c_o;&c_k;&c_r;&c_a;&c_t;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#27665;&#20027;&#25913;&#38761; <pr>m&iacute;nzh&#468;g&#462;ig&eacute;</pr></trd>,
    <trd>&#27665;&#20027;&#21270; <pr>m&iacute;nzh&#468;hu&agrave;</pr></trd>
  </trdgrp>
</drv>
