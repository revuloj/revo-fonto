<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>okcident</rad>/o
</kap>
<drv mrk="okcide.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Parto de la horizonto, kie la suno &scirc;ajnas subiri.
    </dif>
    <trd lng="hr">zapad</trd>
    <trd lng="de">Westen</trd>
  </snc>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Terparto, regiono direktata al la <tld/>o:
      <ekz>
        disvasti&gcirc;i <tld/>en
        kaj orienten<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Westen</trd>,
      <trd>Okzident</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_h;&c_a;&c_d;</trd>
  <trd lng="cs">z&aacute;pad</trd>
  <trd lng="el">&delta;&ypsilon_ton;&sigma;&eta;</trd>
  <trd lng="en">west</trd>
  <trdgrp lng="es">
    <trd>oeste</trd>,
    <trd>occidente</trd>
  </trdgrp>
  <trd lng="hu">nyugat</trd>
  <trd lng="nl">westen</trd>
  <trd lng="pt">ocidente</trd>
  <trd lng="ru">&c_z;&c_a;&c_p;&c_a;&c_d;</trd>
  <trd lng="sv">v&auml;st</trd>
  <trd lng="vo">ves&uuml;d</trd>
  <trdgrp lng="zh">
    <trd>&#35199;<klr> [x&#299;]</klr></trd>,
    <trd>&#35199;&#26041;<klr> [x&#299;f&#257;ng]</klr></trd>
  </trdgrp>
</drv>
</art>
