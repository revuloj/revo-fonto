<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kel.xml,v 1.28 2022/05/01 08:09:24 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kel</rad>/o
</kap>
<drv mrk="kel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Subtera <ref tip="super" cel="cxambr.0o.doma">&ccirc;ambro</ref>, 
      ordinare uzata por konservi provizojn,
      karbon, ktp:
      <ekz>
        lia kuirejo kaj <tld/>o estas bone provizitaj
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la malalta <tld/>o lo&gcirc;is malri&ccirc;a, malsana knabo
        <fnt>
          <bib>Fab2</bib>
          <lok>An&gcirc;elo</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu fripono <klr>[&#8230;]</klr> rompis la pordon de la najbara <tld/>o, kie estis
        vino, renversis kelke da multvaloraj kru&ccirc;oj kaj tiel ebrii&gcirc;is
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia patro &ccirc;eestis &ccirc;e lia entombi&gcirc;o: li vidis lian &ccirc;erkon metitan en la funebran
        <tld/>on
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekoka</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li estis fiera de sia <tld/>o
        <klr>(t.e. ties enhavo da vino...)</klr>;
      </ekz>
      <ekz>
        <ind>li havas nek &ccirc;elon nek <tld/>on</ind>
        <klr>(li estas senhejmulo)</klr>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="fr">il est &agrave; la <ind>rue</ind></trd>
      </ekz>
      <ekz>
        malfermita <tld/>o tentas al &scirc;telo
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_k;&c_l;&c_je;&c_p;</trd>,
      <trd>&c_p;&c_a;&c_d;&c_v;&c_a;&c_l;</trd>,
      <trd>&c_s;&c_u;&c_t;&c_a;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trd lng="br">kav</trd>
    <trd lng="de">Keller</trd>
    <trd lng="en">cellar</trd>
    <trdgrp lng="es">
      <trd>bodega</trd>,
      <trd>despensa</trd>,
      <trd>almac&eacute;n</trd>
    </trdgrp>
    <trd lng="fa">&a_z;&f_y;&a_r;&a_z;&a_m;&f_y;&a_n;</trd>
    <trdgrp lng="pt">
      <trd>adega</trd>,
      <trd>despensa</trd>,
      <trd>frasqueira</trd>,
      <trd>garrafeira</trd>
    </trdgrp>
    <trd lng="pl">piwnica</trd>     
    <trd lng="ro">beci</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_d;&c_v;&c_a;&c_l;</trd>,
      <trd>&c_p;&c_o;&c_g;&c_r;&c_je;&c_b;</trd>
    </trdgrp>
  </snc>
  <trd lng="bo">&t_a;&t_naro;&t_ga;&t_tsheg;&t_kha;&t_nga;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>sklep</trd>,
    <trd>suter&eacute;n</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cave<klr tip="ind"> (subst. f&eacute;m.)</klr></trd>,
    <trd>sous-sol</trd>
  </trdgrp>
  <trd lng="hu">pince</trd>
  <trd lng="nl">kelder</trd>
  <trd lng="pl">piwnica</trd>     
  <trd lng="ro">beci</trd>
  <trd lng="sk">pivnica</trd>
  <trdgrp lng="uk">
    <trd>&c_l;&c_mol;&c_o;&c_h;</trd>,
    <trd>&c_p;&c_o;&c_g;&c_r;&c_ib;&c_b;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_v;&c_a;&c_l;</trd>
  </trdgrp>
</drv>

<drv mrk="kel.0isto">
  <kap><tld/>isto,
    <var><kap><tld/>estro</kap></var>
  </kap>
  <snc>
    <dif>
      Servisto, kiu zorgas speciale pri la vinprovizoj:
      <ekz>
        en la &ccirc;efa korto komencis kolekti&gcirc;i la amaso: kuiristoj, &ccirc;evalistoj, <tld/>istoj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio, kion elpensis mia <tld/>isto, ne estas biero
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam ili venis al la bareloj, la <tld/>estro volis doni al &gcirc;i vinon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvar kelistoj penis <klr>[&#8230;]</klr> sensoifigi la soifantajn dancantojn
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">paotr ar c'hav</trd>
  <trd lng="cs">sklepn&iacute;k</trd>
  <trd lng="de">Kellermeister</trd>
  <trd lng="en">cellarman</trd>
  <trd lng="es">bodeguero</trd>
  <trdgrp lng="fr">
    <trd>caviste</trd>,
    <trd>sommelier</trd>,
    <trd>pr&eacute;pos&eacute; aux vins</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>pincemester</trd>,
    <trd>pincemunk&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">bottelier</trd>
  <trd lng="pl">sommelier</trd>     
  <trd lng="ro">sommelier</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_p;&c_r;&c_a;&c_v;&c_l;&c_ja;&c_ju;&c_shch;&c_i;&c_j; &c_v;&c_i;&c_n;&c_n;&c_y;&c_m; &c_p;&c_o;&c_g;&c_r;&c_je;&c_b;&c_o;&c_m;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_je;&c_d;&c_u;&c_ju;&c_shch;&c_i;&c_j; &c_v;&c_i;&c_n;&c_a;&c_m;&c_i;</trd>,
    <trd>&c_s;&c_o;&c_m;&c_je;&c_l;&c_mol;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">ved&uacute;ci pivnice</trd>
  <trdgrp lng="uk">
    <trd>&c_o;&c_f;&c_ib;&c_c;&c_ib;&c_a;&c_n;&c_t; (&c_n;&c_a; &c_r;&c_o;&c_z;&c_d;&c_a;&c_ch;&c_ib; &c_v;&c_i;&c_n;&c_a;)</trd>,
    <trd>&c_p;&c_r;&c_a;&c_c;&c_ib;&c_v;&c_n;&c_i;&c_k; &c_r;&c_je;&c_s;&c_t;&c_o;&c_r;&c_a;&c_n;&c_u;</trd>,
    <trd>&c_shch;&c_o; &c_v;&c_ib;&c_d;&c_a;&c_jeu; &c_a;&c_l;&c_k;&c_o;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_i;&c_m;&c_i; &c_n;&c_a;&c_p;&c_o;&c_ja;&c_m;&c_i;</trd>,
    <trd>&c_ib;&c_s;&c_t;. &c_v;&c_i;&c_n;&c_o;&c_ch;&c_je;&c_r;&c_p;&c_ib;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="kel.vin0o">
  <kap>
    vin<tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="K"/>o en kiu oni stokas vinon, foje anka&ubreve; servante gastojn 
      (vd <ref tip="super" cel="vin.0ejo">vinejo</ref>):
      <ekz>
        la vin<tld/>oj de <nom>Tartessos</nom>
        <fnt>
          <bib>SatirRak</bib>
          <lok>Pythagoras</lok>
        </fnt>;
      </ekz>
      <ekz>
        posttagmeze oni povis viziti &Ccirc;efepiskopajn Vin<tld/>ojn
        <fnt>
          <bib>LOdE</bib>
          <aut>Ljubomir Trifon&ccirc;ovski</aut>
          <vrk><url ref="https://tekstaro.com/t?nomo=ondo-de-esperanto">La 55a Kongreso de IKUE</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        veturante per&scirc;ipe oni vidos vila&gcirc;ojn, monumentojn, vin<tld/>ojn kaj restoraciojn
        <fnt>
          <bib>Monato</bib>
          <aut>Marc Gassion</aut>
          <vrk><url ref="&Monato;/2003/007934.html">Aude</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Weinkeller</trd>
  <trd lng="en">wine cellar</trd>
  <trd lng="fr">cave &agrave; vin</trd>
  <trd lng="uk">&c_v;&c_i;&c_n;&c_n;&c_i;&c_j; &c_p;&c_o;&c_g;&c_r;&c_ib;&c_b;</trd>
</drv>


</art>
