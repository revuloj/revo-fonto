      <trd>by&cacute; w <ind>s&aogonek;siedztwie</ind></trd>
    </trdgrp>
  <trdgrp lng="ru">
      <trd>&c_s;&c_o;&c_s;&c_je;&c_d;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_b;&c_y;&c_t;&c_mol; <ind>&c_r;&c_ja;&c_d;&c_o;&c_m;</ind>
      </trd>
    </trdgrp>
  <trd lng="sk">susedi&tcaron;</trd>
  </drv>

  <drv mrk="apud.0eco">
  <kap><tld/>o, <var><kap><tld/>eco</kap></var></kap>
  <snc mrk="apud.0eco.KOMUNE">
    <dif>
      Proksima &ccirc;eesto:
      <ekz>
        refoje li regustumis la tiklan plezuron de &scirc;ia <tld/>o
        <fnt><aut>J. Francis</aut>, <vrk>Misio sen Alveno</vrk>,
        <lok>1982</lok></fnt>;
      </ekz>
      <ekz>
        tiu amindumo aliformi&gcirc;is en &ccirc;iutagajn geedzajn kuni&gcirc;ojn, kiujn pruvis la
        <tld/>eco de du apartamentoj
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekunua</lok>
        </fnt>;
      </ekz>
      <ekz>
        galantoj kaj kandeloj mildigis la litan <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="https://www.monato.be/2013/011130.php">Morto de elstara irlanda poeto kaj
          homaramanto</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#21608;&#x56F4; <pr>zh&#x14D;uw&#xE9;i</pr></trd>,
    <trd>&#21608;&#x570D; <pr>zh&#x14D;uw&#xE9;i</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_l;&c_ib;&c_z;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_u;&c_s;&c_je;&c_d;&c_z;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>N&auml;he</trd>,
     <trd>Beisein</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>proximity</trd>,
    <trd>closeness</trd>,
    <trd>vicinity</trd>,
    <trd>adjacency</trd>,
    <trd>adjacence</trd>,
    <trd>presence</trd>
  </trdgrp>
  <trdgrp lng="fr">
