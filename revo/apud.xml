<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>apud</rad></kap>
<drv mrk="apud.0">
   <kap><ofc>*</ofc><tld/></kap>
   <subdrv>
     <dif>
       Prepozicio esprimanta la proksimecon, la pli-malpli
       grandan najbarecon:
       <ekz>
         <tld/> propra domo &scirc;telisto ne &scirc;telas
         <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
       </ekz>
       <ekz>
         <tld/> plena man&gcirc;otablo &ccirc;iu estas tre afabla
         <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
       </ekz>
       <ekz>
         la batalo <tld/> Solferino.
       </ekz>
     </dif>
   </subdrv>
   <subdrv>
     <dif>
       Memstara vortero kun tiu signifo:
       <ekz>
         li lo&gcirc;as en la <tld/>a domo;
       </ekz>
       <ekz>
         la &ccirc;apelo staras sur la tablo, k la gantoj
         ku&scirc;as <tld/>e.
       </ekz>
     </dif>
   </subdrv>
   <subdrv>
     <dif>
       Prefikso ku tiu signifo:
       <ekz>
         li <tld/>estas al sia patro;
       </ekz>
       <ekz>
         <tld/>meti.
       </ekz>
       <refgrp tip="sin">
         <ref cel="cxe">&ccirc;e</ref>,
         <ref cel="en">en</ref>.
       </refgrp>
     </dif>
   </subdrv>
   <trdgrp lng="cs">
     <trd>vedle</trd>,
     <trd>u</trd>
   </trdgrp>
   <trdgrp lng="de"><trd>neben</trd>, <trd>bei</trd></trdgrp>
   <trdgrp lng="fr">
     <trd>pr&egrave;s de</trd>,
     <trd>&agrave; c&ocirc;t&eacute; de</trd></trdgrp>
   <trdgrp lng="it">
     <trd>presso</trd>,
     <trd>vicino</trd>
   </trdgrp>
   <trd lng="la">apud</trd>
   <trdgrp lng="ru">
     <trd>&#1086;&#1082;&#1086;&#1083;&#1086;</trd>,
     <trd>&#1074;&#1086;&#1079;&#1083;&#1077;</trd>,
     <trd>&#1074;&#1073;&#1083;&#1080;&#1079;&#1080;</trd>,
     <trd>&#1073;&#1083;&#1080;&#1079;</trd>
   </trdgrp>
</drv>
</art>
