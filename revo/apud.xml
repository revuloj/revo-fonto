    <trd>voisinage<klr> (proximit&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="he">&qof;&#x5B4;&resh;&bet;&#x5B8;&he;</trd>
  <trdgrp lng="ja">
    <trd>&#x8FD1;&#12373;  <pr>&#12385;&#x304B;&#12373;</pr></trd>,
