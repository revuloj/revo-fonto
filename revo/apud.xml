<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: apud.xml,v 1.66 2024/01/26 09:24:27 revo Exp $">
<kap>
  <ofc>*</ofc> <rad>apud</rad>
  <fnt> <bib>PV</bib></fnt>
</kap>
<subart>
  <dif>
    Prepozicio esprimanta:
  </dif>
  <drv mrk="apud.0">
    <kap><ofc>*</ofc><tld/></kap>
    <snc>
      <dif>
        proksimecon, pli-malpli grandan najbarecon:
        <ekz>
          li staris tutan horon <tld/> la fenestro
          <fnt><bib>F</bib><lok>&FE; 26</lok></fnt>;
          <trd lng="pl">sta&lstroke; ca&lstroke;&aogonek;
          godzin&eogonek; <ind>przy</ind> oknie</trd>
        </ekz>
        <ekz>
          la gefian&ccirc;oj staris <tld/> la altaro
          <fnt>
            <bib>F</bib>
            <lok>&FE; 36</lok>
          </fnt>;
        </ekz>
        <ekz>
          promenante <tld/> la reloj de fervojo
          <fnt>
            <bib>F</bib>
            <lok>&FE; 39</lok>
          </fnt>;
        </ekz>
        <ekz>
          jen estas puto sur la kampo, kaj tri gregoj da &scirc;afoj ku&scirc;as <tld/> &gcirc;i
          <fnt>
            <bib>MT</bib>
            <lok>Genezo 29:2</lok>
          </fnt>;
        </ekz>
        <ekz>
          vi partoprenis kun mia frato en la batalo <tld/> Prago
          <fnt>
            <bib>Rabistoj</bib>
            <lok>Akto Dua</lok>
          </fnt>;
        </ekz>
        <ekz>
          <uzo tip="stl">FIG</uzo>
          &ccirc;u ne estas por vi pli konsolinde senti, ke li trovi&gcirc;as <tld/> vi, vidi lin
          &ccirc;iutage
          <fnt>
            <bib>ChL</bib>
            <lok>&Ccirc;apitro Oka</lok>
          </fnt>;
        </ekz>
        <ekz>
          <tld/> propra domo &scirc;telisto ne &scirc;telas
          <fnt><bib>PrV</bib></fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="cxe.0">&ccirc;e</ref>,
        <ref cel="cxirka.0">&ccirc;irka&ubreve;</ref>,
        <ref cel="proksi.0ede">proksime de</ref>
      </refgrp>
      <rim>
        Pri uzo de <em><tld/></em> kun akuzativo,
        vd <ref cel="apud.al0">al <tld/>, <tld/> ...-n</ref>.
      </rim>
    </snc>
  <trdgrp lng="be">
      <trd>&c_l;&c_ja;</trd>,
      <trd>&c_k;&c_a;&c_l;&c_ja;</trd>,
      <trd>&c_p;&c_o;&c_b;&c_a;&c_ch; &c_z;</trd>,
      <trd>&c_p;&c_a;&c_b;&c_l;&c_ib;&c_z;&c_u; &c_z;</trd>
    </trdgrp>
  <trd lng="bo">&amp;t_-a;&t_kha;&t_rata;&t_gigu;&t_sa;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;i</trd>,
    <trd>u</trd>,
    <trd>vedle</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>neben</trd>, <trd>bei</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>next to</trd>,
    <trd>alongside</trd>,
    <trd>beside</trd>
  </trdgrp>
  <trd lng="es">junto a</trd>
  <trdgrp lng="fr">
      <trd>pr&egrave;s de</trd>,
      <trd>&agrave; c&ocirc;t&eacute; de</trd>
    </trdgrp>
  <trd lng="he">&ayin;&lamed; &yod;&dalet;</trd>
  <trd lng="hu">mellett</trd>
  <trdgrp lng="it">
      <trd>presso</trd>,
      <trd>vicino</trd>
    </trdgrp>
  <trd lng="ja">&#xFF5E;<ind>&#x306E;&#x305D;&#12400;&#x306B;</ind></trd>
  <trd lng="la">apud</trd>
  <trdgrp lng="nl">
      <trd>bij</trd>,
      <trd>nabij</trd>
    </trdgrp>
  <trdgrp lng="no">
      <trd>ved</trd>,
      <trd>i <ind>n&aelig;rheten</ind> av</trd>
    </trdgrp>
  <trdgrp lng="pl">
      <trd>przy</trd>,
      <trd>obok</trd>,
      <trd>u</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>ao lado de</trd>,
      <trd>pr&oacute;ximo a</trd>
    </trdgrp>
  <trdgrp lng="ru">
      <trd>&c_o;&c_k;&c_o;&c_l;&c_o;</trd>,
      <trd>&c_v;&c_o;&c_z;&c_l;&c_je;</trd>,
      <trd>&c_v;&c_b;&c_l;&c_i;&c_z;&c_i;</trd>,
      <trd>&c_b;&c_l;&c_i;&c_z;</trd>,
      <trd>&c_r;&c_ja;&c_d;&c_o;&c_m; &c_s;</trd>
    </trdgrp>
  <trdgrp lng="sk">
    <trd>pri</trd>,
    <trd>ved&lcaron;a</trd>
  </trdgrp>
  <trd lng="sw">karibu na</trd>
  <trdgrp lng="th">
      <trd>&#xE02;&#xE49;&#xE32;&#xE07;</trd>,
      <trd>&#xE02;&#xE49;&#xE32;&#xE07; &#xE46;</trd>,
      <trd>&#xE16;&#xE31;&#xE14;&#xE08;&#xE32;&#xE01;</trd>
    </trdgrp>
    <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_i;&c_j;&c_m;.&nbsp;&c_k;&c_o;&c_l;&c_o;</trd>,
    <trd>&c_b;&c_ib;&c_l;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_ja;&c_d;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_u;&c_ch;</trd>,
    <trd>&c_p;&c_o;&c_b;&c_ib;&c_l;&c_ja;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_j;</trd>
  </trdgrp>
  <trd lng="vo">neb</trd>
</drv>

  <drv mrk="apud.mal0">
    <kap>mal<tld/></kap>
    <uzo tip="stl">RAR</uzo>
    <snc mrk="apud.mal0.KOMUNE">
      <dif>
        En ioma <ref cel="distan.0o.SPA">distanco</ref> de;
        <ref tip="vid" cel="proksi.mal0ede.KOMUNE">malproksime
        de</ref>:
        <ekz>
          mi estis mal<tld/> la vojkuro de &scirc;ipoj
          <fnt>
            <bib>Rob</bib>
            <lok>Parto II</lok>
          </fnt>;
        </ekz>
        <ekz>
          mal<tld/> mi, ne parolu
          <fnt>
            <aut>J. Benavente, trad. V. Inglada</aut>,
            <vrk><url ref="http://www.delbarrio.eu/servanto.htm">
            La servanto de Don Juan</url></vrk>,
            <lok>en: Hispanaj dramoj, 1911</lok>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="for.0.malproksimeco">for</ref>
      </refgrp>
      <trdgrp lng="zh">
    <trd>&#x504F;&#x8FDC; <pr>pi&amacron;nyu&#x1CE;n</pr></trd>,
    <trd>&#x504F;&#36960; <pr>pi&amacron;nyu&#x1CE;n</pr></trd>,
    <trd>&#x504F;&#x50FB;&#30340; <pr>pi&amacron;np&#xEC;de</pr></trd>
  </trdgrp>
</snc>
    <trdgrp lng="be">
      <trd>&c_v;&c_o;&c_d;&c_d;&c_a;&c_l;&c_mol;</trd>,
      <trd>&c_n;&c_a; <ind>&c_a;&c_d;&c_l;&c_je;&c_g;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_ib;</ind></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>fern</trd>,
      <trd><ind>fern</ind> von</trd>,
      <trd><ind>entfernt</ind> von</trd>,
      <trd><ind>abseits</ind> von</trd>
    </trdgrp>
    <trd lng="en">away from</trd>
    <trdgrp lng="fr">
      <trd>&agrave; quelque distance de</trd>,
      <trd>loin de</trd>
    </trdgrp>
    <trd lng="nl">ver van</trd>
    <trd lng="no">langt fra</trd>
  </drv>

  <drv mrk="apud.al0">
    <kap>al <tld/>, <var><kap><tld/> ...-n</kap></var></kap>
    <snc>
      <dif>
        <ref cel="al.0">Al</ref> loko situanta
        <ref cel="apud.0"><tld/></ref>:
        <ekz>
          <tld/> liajn ostojn metu miajn ostojn
          <fnt><bib>MT</bib><lok>&Re1; 13:31</lok></fnt>.
        </ekz>
      </dif>
      <rim>
        Pri la prepoziciaj kuna&jcirc;oj kun <em>al</em>,
        vidu rimarkon sub
        <ref cel="cxe.al0.kunajxo_kun_al">al &ccirc;e</ref>.
      </rim>
    </snc>
    <trdgrp lng="be">
      <trd>&c_k;&c_a;&c_l;&c_ja;</trd>,
      <trd>&c_p;&c_o;&c_b;&c_a;&c_ch;</trd>,
      <trd>&c_b;&c_l;&c_ib;&c_z;&c_k;&c_a; &c_d;&c_a;</trd>
    </trdgrp>
    <trd lng="de">neben</trd>
    <trdgrp lng="en">
      <trd>to beside</trd>,
      <trd>to next to</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>pr&egrave;s de<klr tip="ind"> (avec mouvement vers)</klr>
      </trd>,
      <trd>&agrave; c&ocirc;t&eacute; de<klr tip="ind">
      (avec mouvement vers)</klr></trd>
    </trdgrp>
    <trd lng="nl">bij<klr tip="amb"> (richting)</klr></trd>
    <trd lng="no">ved<klr tip="amb"> (retning)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_o;&c_z;&c_l;&c_je;<klr tip="ind">
      (&c_k;&c_u;&c_d;&c_a;)</klr></trd>,
      <trd>&c_o;&c_k;&c_o;&c_l;&c_o;<klr tip="ind">
      (&c_k;&c_u;&c_d;&c_a;)</klr></trd>,
      <trd>&c_r;&c_ja;&c_d;&c_o;&c_m; &c_s;<klr tip="amb">
      (&c_k;&c_u;&c_d;&c_a;)</klr></trd>
    </trdgrp>
  </drv>

  <drv mrk="apud.de0">
    <kap>de <tld/></kap>
    <snc>
      <dif>
        <ref cel="de.0">De</ref>
        <sncref ref="de.0.spaco"/>,<sncref ref="de.0.origino"/>
        loko situanta
        <ref cel="apud.0"><tld/></ref>:
        <ekz>
          eliris de <tld/> li &ccirc;iuj
          <fnt><bib>MT</bib><lok>&Jug; 3:19</lok></fnt>.
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="de">
      <trd>von <ind>neben</ind></trd>,
      <trd>von <ind>bei</ind></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>from beside</trd>,
      <trd>from next to</trd>
    </trdgrp>
  </drv>

<drv mrk="apud.unu_0_alia">
  <kap>
    unu <tld/> alia
  </kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc>
    <dif>
      <ref tip="sin" cel="flank.0o_cxe_0o">Flanko &ccirc;e flanko</ref> &ccirc;e flanko,
      orientita samdirekte:
      <ekz>
        la du homoj mar&scirc;is silente unu <tld/> alia
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dektria</lok>
        </fnt>;
      </ekz>
      <ekz>
        en du vicoj, unu <tld/> alia, ku&scirc;is kvar grandegaj kvadratoj, kunmetitaj el ru&gcirc;a
        fazeolo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        la plej malsamaj popoloj staras unu <tld/> alia ne kiel fremduloj, ne kiel
        konkurantoj, sed kiel fratoj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista en Boulogne sur Mer en la 5a de a&ubreve;gusto
          1905</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la flankaj galerioj, unu <tld/> la alia, havis la longon de tridek tri futoj
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 41:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        homoj, kiuj restis ankora&ubreve; vivaj, faladis amase unu <tld/> aliaj
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LVI</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_z;&c_ib;&c_n; &c_k;&c_a;&c_l;&c_ja; &c_a;&c_d;&c_n;&c_a;&c_g;&c_o;</trd>
  <trd lng="de">nebeneinander</trd>
  <trd lng="en">side by side</trd>
  <trd lng="fr">cote &agrave; cote</trd>
</drv>

</subart>

<subart>
  <dif>
    Memstara vortero kun tiu signifo:
  </dif>
<drv mrk="apud.0a">
  <kap><tld/>a</kap>
  <snc mrk="apud.0a.KOMUNE">
    <dif>
      Estanta proksime:
      <ekz>
        la muljuna viro iris en la <tld/>an &ccirc;ambron
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li lo&gcirc;as en la <ind><tld/>a</ind> domo;
        <trd lng="ru">&c_s;&c_o;&c_s;&c_je;&c_d;&c_n;&c_i;&c_j;</trd>
      </ekz>
      <ekz>
        la &ccirc;apelo staras sur la tablo, kaj la gantoj
        ku&scirc;as <ind><tld/>e</ind>;
        <trd lng="ru">&c_r;&c_ja;&c_d;&c_o;&c_m;</trd>
      </ekz>
      <ekz>
        li sidis &ccirc;e <tld/>a tablo
        <fnt>
          <aut>Konstanten Krysakov</aut>
          <vrk><url ref="&Monato;/2000/006805.php">KOTA AKVO
         </url></vrk>
         <bib>Monato</bib>
         <lok>2000/03, p. 28</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni iru en la <tld/>an &ccirc;ambron
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Sinjorino Ajuolo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_u;&c_s;&c_je;&c_d;&c_n;&c_ib;</trd>,
      <trd>&c_b;&c_l;&c_ib;&c_z;&c_k;&c_ib;</trd>,
      <trd>&c_s;&c_u;&c_m;&c_je;&c_zh;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Nachbar...</trd>,
      <trd>benachbart</trd>,
      <trd>angrenzend</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>next to</trd>,
      <trd>adjacent</trd>
    </trdgrp>
    <trd lng="es">contiguo</trd>
    <trdgrp lng="fr">
      <trd>tout <ind>proche</ind></trd>,
      <trd>voisin<klr tip="ind"> (adj.)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>mellette l&eacute;v&odblac;</trd>,
      <trd>szomsz&eacute;dos</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>nabij</trd>,
      <trd>naburig</trd>
    </trdgrp>
    <trd lng="no">like <ind>ved</ind></trd>
    <trdgrp lng="pl">
      <trd>przyleg&lstroke;y</trd>,
      <trd>s&aogonek;siedni</trd>,
      <trd>bliski</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_o;&c_s;&c_je;&c_d;&c_n;&c_i;&c_j;</trd>,
      <trd>&c_b;&c_l;&c_i;&c_zh;&c_n;&c_i;&c_j;</trd>,
      <trd>
      &c_n;&c_a;&c_h;&c_o;&c_d;&c_ja;&c_shch;&c_i;&c_j;&c_s;&c_ja;
      <ind>&c_r;&c_ja;&c_d;&c_o;&c_m;</ind>
      </trd>,
      <trd>&c_s;&c_m;&c_je;&c_zh;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
    <trd lng="zh">&#x4EA4;&#x754C;&#22320; <pr>ji&amacron;oji&#xE8;de</pr></trd>
</snc>
  <snc mrk="apud.0a.angulo">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 27</lok></fnt>
    <dif>
      <klr>(p.p. du <ref cel="angul.0o.figuro">anguloj</ref>)</klr>
      <ref cel="later.sam0a.angulo">Samlateraj</ref> kaj
      <ref cel="suplem.0a.angulo">suplementaj</ref>:
      <ekz>
        se du <ref cel="kruc.0igxi.MAT">kruci&gcirc;antaj</ref> rektoj
        difinas du egalajn <tld/>ajn angulojn, tiam ili estas ortaj.
      </ekz>
    </dif>
    <rim>
      Kvankam tiu &ccirc;i termino aperas jam &ccirc;e <nom>Bricard</nom>,
      &gcirc;ia tiama senco estas nur <ctl>samlatera</ctl>. La saman
      sencon ni retrovas en
      <fnt><bib>PIV1</bib><lok>angulo</lok></fnt>,
      nerekte konfirmitan en la geometria plato
      <fnt><bib>PIV1</bib><lok>plato: XVIII, 13-d</lok></fnt>,
      kiu tekstas <ctl>apudaj suplementaj</ctl>.
      Tamen la terminologio jam &scirc;an&gcirc;i&gcirc;is, kiel
      konfirmas <fnt><bib>MatVort</bib></fnt> kaj
      <fnt><bib>PIV2</bib><lok>angulo</lok></fnt>, kiujn
      ni sekvis por la difinoj de <ctl>samlatera</ctl> kaj
      <ctl><tld/>a</ctl>.
    </rim>
    <trd lng="be">&c_p;&c_r;&c_y;&c_l;&c_je;&c_g;&c_l;&c_y;
    <klr tip="amb">(&c_v;&c_u;&c_g;&c_a;&c_l;)</klr></trd>
    <trd lng="de">Neben-<klr tip="amb">(winkel)</klr></trd>
    <trdgrp lng="en">
      <trd>adjacent</trd>,
      <trd>supplementary<klr tip="ind"> (angle)</klr></trd>
    </trdgrp>
    <trd lng="fr"><klr tip="ind">(angle) </klr>
    adjacent suppl&eacute;mentaire</trd>
    <trd lng="pl"><klr tip="ind">(k&aogonek;t) </klr>
    przyleg&lstroke;y</trd>
    <trd lng="ru">&c_s;&c_m;&c_je;&c_zh;&c_n;&c_y;&c_j;
    <klr tip="ind"> (&c_u;&c_g;&c_o;&c_l;)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>p&rcaron;idru&zcaron;en&yacute;</trd>,
    <trd>p&rcaron;ilehl&yacute;</trd>,
    <trd>sousedn&iacute;</trd>,
    <trd>stoj&iacute;c&iacute; vedle</trd>,
    <trd>vedlej&scaron;&iacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>adjacent</trd>,
    <trd>adjoining</trd>,
    <trd>abutting</trd>,
    <trd>alongside</trd>
  </trdgrp>
  <trd lng="he">&samekh;&mem;&vav;&fkaf;</trd>
  <trd lng="ja">&#x304B;&#x305F;&#x308F;&#12425;&#x306E;</trd>
  <trdgrp lng="sk">
    <trd>pri&lcaron;ahl&yacute;</trd>,
    <trd>ved&lcaron;aj&scaron;&iacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_o;&c_j;</trd>,
    <trd>&c_shch;&c_o; &c_z;&c_n;&c_a;&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja; &c_p;&c_o;&c_r;&c_u;&c_ch;/&c_p;&c_o;&c_b;&c_l;&c_i;&c_z;&c_u;</trd>,
    <trd>&c_b;&c_l;&c_i;&c_zh;&c_n;&c_ib;&c_j;</trd>,
    <trd>&c_s;&c_u;&c_s;&c_ib;&c_d;&c_n;&c_ib;&c_j;</trd>
  </trdgrp>
</drv>

  <drv mrk="apud.0i">
    <kap><tld/>i</kap>
    <gra><vspec>x</vspec></gra>
    <snc>
      <dif>
        Esti <tld/>, <tld/>e, <tld/>a:
        <ekz>
          eble milfoje sentis ni sangbolon, dum en ardglua ku&scirc;'
          <tld/>is ni
          <fnt><aut>P. Peneter</aut>, <vrk>Sekretaj
          Sonetoj</vrk>, <lok>XLIX</lok></fnt>;
        </ekz>
        <ekz>
          <nom>Margarita</nom> sentis la proksimecon de akvo kaj komprenis, ke la
          celo <tld/>as
          <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 21a,
          p. 212a</lok></fnt>.
        </ekz>
      </dif>
    </snc>
  <trdgrp lng="be">
      <trd>&c_s;&c_u;&c_s;&c_je;&c_d;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_n;&c_a;&c_h;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_c;&c_a;
      <ind>&c_p;&c_o;&c_b;&c_a;&c_ch;</ind></trd>
    </trdgrp>
  <trdgrp lng="cs">
    <trd>b&yacute;t v bl&iacute;zkosti</trd>,
    <trd>b&yacute;t v sousedstv&iacute;</trd>,
    <trd>sousedit</trd>
  </trdgrp>
  <trd lng="de">angrenzen</trd>
  <trd lng="en">be beside</trd>
    <trd lng="es">estar junto a</trd>
  <trdgrp lng="fr">
      <trd>&ecirc;tre <ind>proche(s)</ind></trd>,
      <trd>&ecirc;tre <ind>voisin(s)</ind></trd>,
      <trd>se <ind>toucher</ind><klr tip="ind"> (&ecirc;tre voisins)
      </klr></trd>
    </trdgrp>
  <trd lng="hu">mellette van</trd>
  <trdgrp lng="pl">
