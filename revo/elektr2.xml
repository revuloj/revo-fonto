<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>elektriz</rad>/i
    </kap>
    <drv mrk="elektr2.0i">
      <kap><tld/>i</kap>
      <uzo tip="fak">ELE</uzo>
      <snc>
        <ref tip="dif" cel="elektr.0umi">elektr(um)i.</ref>
      </snc>
      <trd
      lng="be">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
      <trd lng="de">elektrisieren</trd>
      <trd lng="es">electrizar</trd>
      <trd lng="fr">&eacute;lectriser</trd>
      <trdgrp lng="pl">
        <trd>elektryzowa&cacute;</trd>,
        <trd>naelektryzowa&cacute;</trd>
      </trdgrp>
      <trd lng="pt">eletrificar</trd>
      <trd
      lng="ru">&c_n;&c_a;&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </drv>
  </art>
