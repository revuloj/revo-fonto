<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>akordion</rad>/o
</kap>
<drv mrk="akordi.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Portebla <ref tip="super" cel="instru2.blov0o">blovinstrumento</ref>, kun klavaro, en kiu oni estigas la sonon
      alterne tirante kaj pu&scirc;ante balgon.
      <ref tip="sin" cel="harmon2.tir0o">Tirharmoniko</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Harmonika</trd>,
     <trd>Ziehharmonika</trd>,
     <trd>Schifferklavier</trd>,
     <trd>Akkordeon</trd>
  </trdgrp>
  <trd lng="en">accordion</trd>
  <trd lng="fr">accord&eacute;on</trd>
  <trd lng="nl">accordeon</trd>
  <trd lng="pt">acorde&atilde;o</trd>
  <trd lng="ru">&c_g;&c_a;&c_r;&c_m;&c_o;&c_n;&c_mol;</trd>
  <trd lng="sv">dragspel</trd>
</drv>
</art>
