<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>akordion</rad>/o
</kap>
<drv mrk="akordi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Portebla <ref tip="super" cel="instru2.blov0o">blovinstrumento</ref>,
      kun klavaro, en kiu oni estigas la sonon alterne tirante kaj
      pu&scirc;ante balgon.
    </dif>
    <ref tip="sin" cel="harmon2.tir0o">tirharmoniko.</ref>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_a;&c_r;&c_d;&c_e;&c_o;&c_n;</trd>
  <trdgrp lng="de">
    <trd>Harmonika</trd>,
    <trd>Ziehharmonika</trd>,
    <trd>Schifferklavier</trd>,
    <trd>Akkordeon</trd>
  </trdgrp>
  <trd lng="en">accordion</trd>
  <trd lng="es">acerde&oacute;n</trd>
  <trd lng="fr">accord&eacute;on</trd>
  <trd lng="hu">tang&oacute;harmonika</trd>
  <trd lng="nl">accordeon</trd>
  <trd lng="pt">acorde&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_r;&c_m;&c_o;&c_n;&c_mol;</trd>,
    <trd>&c_a;&c_k;&c_k;&c_o;&c_r;&c_d;&c_je;&c_o;&c_n;</trd>
  </trdgrp>
  <trd lng="sk">akorde&oacute;n</trd>
  <trd lng="sv">dragspel</trd>
</drv>
</art>
