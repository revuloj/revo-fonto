<art mrk="$Id: korife.xml,v 1.20 2023/12/03 09:58:58 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>korife</rad>/o
  <fnt>Z</fnt>
</kap>

<drv mrk="korife.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Estro de &hcirc;oro en antikva komedio a&ubreve; tragedio.
    </dif>
    <trd lng="en">coryphaeus</trd>
    <trd lng="hu">karvezet&odblac; <klr tip="ind">(dr&aacute;m&aacute;ban)</klr></trd>
    <trd lng="nl">coryfee</trd>
    <trd lng="pt">corifeu</trd>
    <trd lng="ru">&c_k;&c_o;&c_r;&c_i;&c_f;&c_je;&c_j;</trd>
  </snc>
  <snc>
    <dif>
      Estro de kanta a&ubreve; danca &hcirc;oro en moderna opero.
    </dif>
    <trd lng="en">chorus leader</trd>
    <trdgrp lng="hu">
      <trd>k&oacute;rusvezet&odblac;</trd>,
      <trd>balettvezet&odblac;</trd>
    </trdgrp>
    <trd lng="nl">coryfee</trd>
    <trd lng="pt">corifeu</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <uzo tip="klr">(ofte ironie)</uzo>
    <dif>
      Plej grava, plej fama persono; &ccirc;efrolulo, &ccirc;efa instiganto:
      <ekz>
        la <tld/>o de l' opozicio;
      </ekz>
      <ekz>
        al la gazeto nun ne hezitas sendi siajn kontribua&jcirc;ojn William
        Auld,
        Georgo Kama&ccirc;o, Blazio Vaha kaj aliaj <tld/>oj de la nuna
        e-kulturo
        <fnt>
        <aut>A. Fjodorov</aut>,
        <vrk>Sur la kultura vojo</vrk>,
        <lok><url ref="http://www.eventoj.hu/arkivo/eve-043.htm">Eventoj,
        1993-11, numero 43a</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">leader</trd>
    <trdgrp lng="hu">
      <trd>hangad&oacute;</trd>,
      <trd>vez&eacute;regy&eacute;nis&eacute;g</trd>,
      <trd>korifeus</trd>
    </trdgrp>
    <trd lng="nl">uitblinker</trd>
    <trdgrp lng="pt">
      <trd>corifeu</trd>,
      <trd>l&iacute;der</trd>,
      <trd>chefe</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_r;&c_i;&c_f;&c_je;&c_j;</trd>,
      <trd>&c_v;&c_y;&c_d;&c_a;&c_ju;&c_shch;&c_i;&c_j;&c_s;&c_ja; &c_d;&c_je;&c_ja;&c_t;&c_je;&c_l;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_y;&c_f;&c_e;&c_j;</trd>
  <trdgrp lng="cs">
    <trd>koryfej</trd>,
    <trd>vedouc&iacute; ch&oacute;ru v &rcaron;eck&eacute;m dramatu</trd>,
    <trd>v&yacute;zna&ccaron;n&yacute; odborn&iacute;k</trd>,
    <trd>v&yacute;zna&ccaron;n&yacute; v&ecaron;dec</trd>
  </trdgrp>
  <trd lng="de">Koryph&auml;e</trd>
  <trd lng="es">corifeo</trd>
  <trd lng="fr">coryph&eacute;e</trd>
  <trdgrp lng="ja">
    <trd>&#x5408;&#x5531;&#x968A;&#x9577;  <pr>&#x304C;&#x3063;&#x3057;&#x3087;&#x3046;&#x305F;&#x3044;&#x3061;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x30B3;&#x30EA;&#x30D5;&#x30A7;</trd>,
    <trd>&#x5531;&#x9053;&#x8005;  <pr>&#x3057;&#x3087;&#x3046;&#x3069;&#x3046;&#x3057;&#x3083;</pr></trd>,
    <trd>&#x7D71;&#x7387;&#x8005;  <pr>&#x3068;&#x3046;&#x305D;&#x3064;&#x3057;&#x3083;</pr></trd>
  </trdgrp>
  <trd lng="pl">koryfeusz</trd>
  <trd lng="ro">coryphee</trd>
  <trdgrp lng="sk">
    <trd>koryfej</trd>,
    <trd>v&yacute;zna&ccaron;n&yacute; odborn&iacute;k</trd>,
    <trd>v&yacute;zna&ccaron;n&yacute; vedec</trd>
  </trdgrp>
  <trd lng="zh">&#x4E66;&#x5723; <pr>sh&#x16B;sh&#xE8;ng</pr></trd>
</drv>
