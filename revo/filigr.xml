<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: filigr.xml,v 1.25 2025/10/29 20:33:16 revo Exp $">
<kap>
  <rad>filigran</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="filigr.0o">
  <kap><tld/>o</kap>
  <snc mrk="filigr.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
       <ref cel="delika.0a">Delikata</ref> a&jcirc;ura
       <ref tip="super" cel="teks.0ajxo.TEKS">teksa&jcirc;o</ref> el oraj
       a&ubreve; ar&gcirc;entaj fadenoj:
       <ekz>
         korbo el ora <tld/>o;
       </ekz>
       <ekz>
        &scirc;iaj brovoj <klr>[&#x2026;]</klr> similis al fajnaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <vrk>de Julian Modest</vrk>
        </fnt>;
      </ekz>
       <ekz>
         <uzo tip="stl">FIG</uzo>
         la lunaj radioj brilis tra la <tld/>o <klr>(reto)</klr> de
         la bran&ccirc;aro
         <fnt><bib>PV</bib></fnt>;
       </ekz>
       <ekz>
         <uzo tip="stl">FIG</uzo>
         la <tld/>oj <klr>(a&jcirc;uraj skulpta&jcirc;oj)</klr> de la
         pre&gcirc;eja turo;
       </ekz>
       <ekz>
        juna knabino, unu el tiuj <tld/>aj
        <klr>(<ref cel="gracil.0a">gracila</ref>j)</klr>
        Japaninoj, personigantaj la &ccirc;armon kaj dol&ccirc;econ
        <fnt>
          <bib>FdO</bib>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/7/75/Portuguese_filigree_intricate_work.jpg/320px-Portuguese_filigree_intricate_work.jpg"
      prm="CC BY-SA 4.0"
      >ora <tld/>o
        <fnt>
          <aut>Ss.analuisa, propra verko</aut>
          <lok><url ref="&WCW;/File:Portuguese_filigree_intricate_work.jpg"
              >https://commons.wikimedia.org/wiki/File:Portuguese_filigree_intricate_work.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="de">
      <trd>Filigran</trd>,
      <trd>Filigranarbeit</trd>
    </trdgrp>
    <trd lng="en">filigree</trd>
    <trdgrp lng="hu">
      <trd>filigr&aacute;n</trd>,
      <trd>filigr&aacute;nmunka</trd>
    </trdgrp>
    <trd lng="nl">filigrane</trd>
    <trd lng="pl">filigran</trd>
    <trd lng="zh">&#x7D30;&#x7DDA;&#x7D30;&#x5DE5; <pr>x&igrave;xi&agrave;nx&igrave;g&omacron;ng</pr></trd>
  </snc>
   <snc mrk="filigr.0o.TIP">
     <uzo tip="fak">TIP</uzo>
     <dif>
       Travidebla desegno, <ref tip="super" cel="mark.0o.signo">marko</ref>
       en <ref tip="malprt" cel="paper.0o">papero</ref>:
       <ekz>
         <tld/>o de bankbileto.
       </ekz>
     </dif>
     <rim>
       La&ubreve; <fnt><bib>BL</bib></fnt>: (ekzemple en bankbileto)  prefere diru <ke>travidebla
       marko</ke>.
     </rim>
     <trd lng="be">&c_v;&c_a;&c_d;&c_z;&c_ja;&c_n;&c_y; &c_z;&c_n;&c_a;&c_k;</trd>
     <trd lng="de">Wasserzeichen</trd>
     <trd lng="en">watermark</trd>
     <trd lng="nl">watermerk</trd>
     <trdgrp lng="hu">
       <trd>v&iacute;zjel</trd>,
       <trd>v&iacute;zjegy</trd>
     </trdgrp>
     <trd lng="pl">znak wodny</trd>
     <trd lng="ru">&c_v;&c_o;&c_d;&c_ja;&c_n;&c_o;&c_j; &c_z;&c_n;&c_a;&c_k;</trd>
   </snc>
  <trd lng="be">&c_f;&c_ib;&c_l;&c_ib;&c_g;&c_r;&c_a;&c_n;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>drobn&yacute; um&ecaron;leck&yacute; p&rcaron;edm&ecaron;t</trd>,
    <trd>ozdoba</trd>
  </trdgrp>
  <trd lng="fr">filigrane</trd>
  <trdgrp lng="ja">
    <trd>&#x7DDA;&#x6761;&#x7D30;&#x5DE5;  <pr>&#x305B;&#x3093;&#x3058;&#x3087;&#x3046;&#x3055;&#x3044;&#x304F;</pr></trd>,
    <trd>&#x30D5;&#x30A3;&#x30EA;&#x30B0;&#x30EA;&#x30FC;</trd>,
    <trd>&#x3059;&#x304B;&#x3057;&#x6A21;&#x69D8;  <pr>&#x3059;&#x304B;&#x3057;&#x3082;&#x3088;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="pt">filigrana</trd>
  <trd lng="ru">&c_f;&c_i;&c_l;&c_i;&c_g;&c_r;&c_a;&c_n;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>drobn&yacute; umeleck&yacute; predmet</trd>,
    <trd>filigr&aacute;n</trd>,
    <trd>ozdoba</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_f;&c_ib;&c_l;&c_ib;&c_gu;&c_r;&c_a;&c_n;&c_mol;</trd>,
    <trd>&c_v;&c_o;&c_d;&c_ja;&c_n;&c_i;&c_j; &c_z;&c_n;&c_a;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="filigr.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc mrk="filigr.0i.TEK">
     <dif>
       <tld lit="F"/>e prilabori, formi <tld/>on:
       <ekz>
         kadro <tld/>ita la&ubreve; la ma&ubreve;ra stilo;
       </ekz>
       <ekz>
        <uzo tip="stl">FIG</uzo>
        febla vento norda al la proksima fjordo disfadene <tld/>as
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#x2116;6</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tio estis ege misa <tld/>ado, mi provis banda&gcirc;i &scirc;in per aranea&jcirc;o, sed la najado
        &gcirc;emis: &leftquot;ve, &gcirc;i tran&ccirc;doloras kiel &scirc;nuroj!&rightquot; Mi volis firmigi la rompitan
        krureton per petalo de pomfloro, kaj &scirc;i ekploris, &leftquot;a&hcirc;, &gcirc;i premdoloras kiel
        &scirc;tono!&rightquot;
        <fnt>
          <aut>Josef &Ccaron;apek, trad. Josef Vondrou&scaron;ek</aut>
          <vrk><url ref="https://eduinf.waw.pl/esp/lern/librejo/0090/0010.php"
              >Granda Doktora Fabelo</url></vrk>
          <lok>Na&ubreve; Fabeloj, 10</lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_f;&c_ib;&c_l;&c_ib;&c_g;&c_r;&c_a;&c_n;&c_mol;</ind></trd>
   <trd lng="en">filigree</trd>
   <trd lng="fr">filigraner</trd>
   <trd lng="hu">filigr&aacute;nnal d&iacute;sz&iacute;t</trd>
  <trdgrp lng="ja">
    <trd>&#x7DDA;&#x6761;&#x7D30;&#x5DE5;&#x3092;&#x65BD;&#x3059;  <pr>&#x305B;&#x3093;&#x3058;&#x3087;&#x3046;&#x3055;&#x3044;&#x304F;&#x3092;&#x307B;&#x3069;&#x3053;&#x3059;</pr></trd>,
    <trd>&#x3059;&#x304B;&#x3057;&#x6A21;&#x69D8;&#x3092;&#x5165;&#x308C;&#x308B;
    <pr>&#x3059;&#x304B;&#x3057;&#x3082;&#x3088;&#x3046;&#x3092;&#x3044;&#x308C;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="pl">obrobi&cacute; filigranowo</trd>
  <trd lng="uk">&c_f;&c_ib;&c_l;&c_ib;&c_gu;&c_r;&c_a;&c_n;&c_i;&c_t;&c_i;</trd>
</drv>
</art>
