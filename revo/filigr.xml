<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>filigran</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="filigr.0o">
   <kap><tld/>o</kap>
   <snc num="1">
     <uzo tip="fak">BELA</uzo>
     <dif>
       Delikata a&jcirc;ura teksa&jcirc;o el oraj a&ubreve; ar&gcirc;entaj 
fadenoj:
       <ekz>
         korbo el ora <tld/>o;
       </ekz>
       <ekz>
         <uzo tip="stl">FIG</uzo>
         la lunaj radioj brilis tra la <tld/>o <klr>(reto)</klr> de
         la bran&ccirc;aro<fnt>B</fnt>;
       </ekz>
       <ekz>
         <uzo tip="stl">FIG</uzo>
         la <tld/>oj <klr>(a&jcirc;uraj skulpta&jcirc;oj)</klr> de la
         pre&gcirc;eja turo.
       </ekz>
     </dif>
   <trdgrp lng="de">
      <trd>Filigran</trd>,
      <trd>Filigranarbeit</trd></trdgrp>
   </snc>
   <snc num="2">
     <uzo tip="fak">TEK</uzo>
     <dif>
       Travidebla desegno, marko en papero:
       <ekz><tld/>o de bankbileto.</ekz>
     </dif>
     <trd lng="de">Wasserzeichen</trd>
     <trd lng="ru">&c_v;&c_o;&c_d;&c_ja;&c_n;&c_o;&c_j; 
&c_z;&c_n;&c_a;&c_k;</trd>
   </snc>
   <trd lng="fr">filigrane</trd>
   <trd lng="ru">&c_f;&c_i;&c_l;&c_i;&c_g;&c_r;&c_a;&c_n;&c_mol;</trd>
</drv>
<drv mrk="filigr.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc>
     <dif>
       <tld lit="F"/>e prilabori, formi <tld/>on:
       <ekz>kadro <tld/>ita la&ubreve; la ma&ubreve;ra stilo.</ekz>
     </dif>
   </snc>
   <trd lng="fr">filigraner</trd>
</drv>
</art>
