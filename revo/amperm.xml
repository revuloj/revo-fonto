<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: amperm.xml,v 1.26 2023/10/12 18:05:09 revo Exp $">
<kap>
  <rad>ampermetr</rad>/o
</kap>
<drv mrk="amperm.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ELE</uzo>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="aparat.0o.TEK">Aparato</ref>
      por mezuri la intensecon de <ref cel="kurent.0o">kurento</ref>
      <klr>(vd <ref cel="amper.a0o">ampero</ref>)</klr>:
      <ekz>
        <tld/>o estas <klr>[&#x2026;]</klr> &ccirc;iam serie konektita je la
        mezurpunkto, sian 
        nomon &gcirc;i ricevis pro la franca matematikisto kaj fizikisto
        <nom>Andr&eacute; Marie Amp&egrave;re</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=387">Ampermetro</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        tiuj kurentmezuriloj estas nomataj <tld/>oj
        <fnt>
          <aut>Walter Bernard</aut>
          <vrk><url ref="&STEB;fiziko/fiziklibro/1_fiziklibro.pdf">Fiziko - Baza Kurso, Unua Parto</url></vrk>
          <lok>p.76</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_p;&c_e;&c_r;&c_m;&c_e;&c_t;&c_a;&c_r;</trd>
  <trd lng="bg">&c_a;&c_m;&c_p;&c_je;&c_r;&c_m;&c_je;&c_t;&c_malmol;&c_r;</trd>
  <trd lng="ca">amper&iacute;metre</trd>
  <trd lng="cs">amp&eacute;rmetr</trd>
  <trdgrp lng="de">
    <trd>Strommessger&auml;t</trd>,
    <trd>Amperemeter</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ampmeter</trd>,
    <trd>ammeter</trd>
  </trdgrp>
  <trd lng="es">amper&iacute;metro</trd>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_m;&f_p;&a_r;&a_s;&a_n;&a_j;</trd>,
    <trd>&a_A_madda;&a_m;&f_p;&a_r;&a_m;&a_t;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">amp&egrave;rem&egrave;tre</trd>
  <trd lng="he">&mem;&#x5B7;&dalet; &zayin;&#x5B6;&resh;&#x5B6;&fmem;</trd>
  <trd lng="hu">amperm&eacute;r&odblac;</trd>
  <trd lng="ja">&#x96FB;&#x6D41;&#x8A08;  <pr>&#x3067;&#x3093;&#x308A;&#x3085;&#x3046;&#x3051;&#x3044;</pr></trd>
  <trd lng="nl">amp&egrave;remeter</trd>
  <trdgrp lng="pl">
    <trd>amperomierz</trd>
    <trd>amperometr<klr> (rzadko)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_m;&c_p;&c_je;&c_r;&c_m;&c_je;&c_t;&c_r;</trd>
  <trd lng="sk">amp&eacute;rmeter</trd>
  <trd lng="uk">&c_a;&c_m;&c_p;&c_je;&c_r;&c_m;&c_je;&c_t;&c_r;</trd>
</drv>
</art>
