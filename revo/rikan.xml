<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>rikan</rad>/i
</kap>
<drv mrk="rikan.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Duonridi aŭ pro stulteco aŭ pro malica moko.
    </dif>
  </snc>
  <trd lng="be">выскаляцца</trd>
  <trd lng="de">grinsen</trd>
  <trdgrp lng="en">
    <trd>sneer</trd>,
    <trd>laugh derisively</trd>
  </trdgrp>
  <trd lng="hu">vigyorog</trd>
  <trd lng="nl">grijnzen</trd>
  <trdgrp lng="pt">
    <trd>caçoar</trd>,
    <trd>escarnecer</trd>
  </trdgrp>
  <trd lng="ru">зубоскалить</trd>
</drv>
</art>
