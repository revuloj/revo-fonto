<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: rikan.xml,v 1.17 2018/11/20 17:10:19 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>rikan</rad>/i
</kap>
<drv mrk="rikan.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Duon<ref tip="super" cel="rid.0i.priridi">ridi</ref>
      a&ubreve; pro stulteco a&ubreve; pro malica moko:
      <ekz>
        en la mezo sur la &scirc;ranko staris skulptita tuta viro, li havis ridindan aspekton,
        kaj li mem efektive ridis, a&ubreve; pli &gcirc;uste li <tld/>is, &ccirc;ar oni neniel povis
        tion nomi ridado
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tistino kaj kamentubisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ha, ha, ha! <tld/>is &ccirc;iuj anasoj, sed la cikonio &scirc;ajnigis, ke &gcirc;i tion tute ne
        a&ubreve;das
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;iuj floroj estis alligitaj al ostoj de homoj, kaj en la florpotoj staris kapoj
        de mortintoj kaj <tld/>is
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_s;&c_k;&c_a;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="bg">
    <trd>&c_h;&c_i;&c_l;&c_ja; &c_s;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_h;&c_i;&c_l;&c_v;&c_a;&c_m; &c_s;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>posm&iacute;vat se</trd>,
    <trd>po&scaron;klebovat se</trd>,
    <trd>&scaron;klebit se zlomysln&ecaron;</trd>
  </trdgrp>
  <trd lng="de">grinsen</trd>
  <trdgrp lng="en">
    <trd>sneer</trd>,
    <trd>laugh derisively</trd>
  </trdgrp>
  <trd lng="fr">ricaner</trd>
  <trd lng="hu">vigyorog</trd>
  <trd lng="nl">grijnzen</trd>
  <trdgrp lng="pt">
    <trd>ca&ccedil;oar</trd>,
    <trd>escarnecer</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_u;&c_b;&c_o;&c_s;&c_k;&c_a;&c_l;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>u&scaron;keri&tcaron; sa</trd>,
    <trd>&scaron;k&lcaron;abi&tcaron; sa</trd>
  </trdgrp>
</drv>
</art>
