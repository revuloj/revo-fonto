<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: dag.xml,v 1.13 2025/09/26 06:40:17 revo Exp $">
<kap>
  <rad>dag</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>

<drv mrk="dag.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="glav.0o.MIL">Glavo</ref> kun lar&gcirc;a,
      mallonga
      klingo, por persona sindefendo:
      <ekz>
        &scirc;i portis &ccirc;e la zono, ne glavon, sed gemo-garnitan
        <tld/>on
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 49a, p.
        669a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="ponard.0o">ponardo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_n;&c_zh;&c_a;&c_l;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_c;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_k;&c_a;&c_m;&c_a;</trd>
  <trd lng="ca">daga</trd>
  <trd lng="cs">d&yacute;ka</trd>
  <trd lng="de">Dolch</trd>
  <trd lng="en">dagger</trd>
  <trd lng="es">daga</trd>
  <trd lng="fr">dague<klr> (arme)</klr></trd>
  <trd lng="nl">dolk</trd>
  <trdgrp lng="pl">
    <trd>daga</trd>,
    <trd>deka</trd>
  </trdgrp>
  <trd lng="pt">adaga</trd>
  <trd lng="zh">&#x5315;&#x9996; <pr>b&#x1D0; sh&#x1D2;u</pr></trd>
</drv>
</art>

