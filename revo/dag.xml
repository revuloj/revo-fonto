<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>dag</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="dag.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="glav.0o.MIL">Glavo</ref> kun lar&gcirc;a, mallonga
      klingo, por persona sindefendo:
      <ekz>
        &scirc;i portis &ccirc;e la zono, ne glavon, sed gemo-garnitan <tld/>on
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 49a, p.
        669a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_n;&c_zh;&c_a;&c_l;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_c;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">daga</trd>
  <trd lng="cs">d&yacute;ka</trd>
  <trd lng="es">daga</trd>
  <trd lng="fr">dague<klr> (arme)</klr></trd>
  <trd lng="pt">adaga </trd>
</drv>
</art>

