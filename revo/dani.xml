<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dani.xml,v 1.20 2022/04/26 17:11:47 revo Exp $">
<kap>
  <rad>Dani</rad>/o
</kap>
<drv mrk="dani.D0o">
  <kap><tld/>o</kap>
    <snc mrk="dani.D0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="dif" cel="dan.D0ujo">Danujo</ref>:
      <ekz>
        <tld lit="D"/>o estas lando situanta en Nord-E&ubreve;ropo.
        <fnt><bib>Viki</bib>, <lok><url ref="&Viki;/Danio">Danio</url></lok></fnt>;
      </ekz>
      <ekz>
        el Britio, <tld/>o, Germanio kaj Svedio estis forpelitaj litovoj, kies vizoj ne
        plu validis kaj kiuj kontra&ubreve;le&gcirc;e laboris
        <fnt>
          <bib>Monato</bib>
          <aut>Last</aut>
          <vrk><url ref="&Steloj;monato2/006829.html">&Gcirc;is nerevido</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la kromvojo tra <tld/>o ka&ubreve;zas iom da prokrasto
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2003/008057.html">Po&scirc;to kaj kosto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_D;&c_a;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_D;&c_a;&c_n;&c_i;&c_ja;</trd>
  <trdgrp lng="ca">
    <trd>Dinamarca</trd>,
    <trd>Regne de Dinamarca</trd>
  </trdgrp>
  <trd lng="cs">D&aacute;nsko</trd>
  <trd lng="de">D&auml;nemark</trd>
  <trd lng="en">Denmark</trd>
  <trd lng="es">Dinamarca</trd>
  <trd lng="fa">&a_d;&a_A;&a_n;&a_m;&a_A;&a_r;&f_k;</trd>
  <trd lng="fr">Danemark</trd>
  <trd lng="he">&#x5D3;&#x5E0;&#x5DE;&#x5E8;&#x5E7;</trd>
  <trd lng="hu">D&aacute;nia</trd>
  <trd lng="id">Denmark</trd>
  <trd lng="nl">Denemarken</trd>
  <trd lng="no">Danmark</trd>
  <trd lng="pl">Dania</trd>
  <trd lng="sk">D&aacute;nsko</trd>
  <trd lng="uk">&#x414;&#x430;&#x43D;&#x456;&#x44F;</trd>
</drv>
</art>

