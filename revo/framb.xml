<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>framb</rad>/o
</kap>
<drv mrk="framb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Bongusta man&gcirc;ebla bero de <tld/>ujo. 
    </dif>
    <trd lng="de">Himbeere</trd>
    <trd lng="nl">framboos</trd>
    <trd lng="pl">malina</trd>
  </snc>
</drv>
<drv mrk="framb.0ujo">
  <kap><tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="rubus.0o">rubuso</ref>
      (<trd lng="la">Rubus idaeus</trd>) kun ru&gcirc;aj
      fruktoj.
    </dif>
  </snc>
  <bld lok="../bld/framb.jpg">floroj</bld>
  <trd lng="de">Himbeere</trd>
  <trd lng="nl">framboos</trd>
</drv>
</art>
