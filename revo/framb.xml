<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: framb.xml,v 1.35 2023/11/10 06:08:00 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>framb</rad>/o
</kap>

<drv mrk="framb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="framb.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Ru&gcirc;a, bongusta man&gcirc;ebla frukto de <tld/>ujo.
    </dif>
    <ref tip="super" cel="frukt.plur0o">plurfrukto</ref>
    <bld lok="../bld/framb2.jpg">
      <tld/>o
    </bld>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_ib;&c_n;&c_a; <klr>(&c_ja;&c_g;&c_a;&c_d;&c_a;)</klr></trd>
  <trd lng="bg">&c_m;&c_a;&c_l;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>malina</trd>,
    <trd>malina <klr>(plod)</klr></trd>
  </trdgrp>
  <trd lng="de">Himbeere</trd>
  <trd lng="en">raspberry</trd>
  <trd lng="es">frambuesa</trd>
  <trd lng="fr">framboise</trd>
  <trd lng="hu">m&aacute;lna</trd>
  <trdgrp lng="ja">
    <trd>&#x30E9;&#x30BA;&#x30D9;&#x30EA;&#x30FC;</trd>,
    <trd>&#x30D5;&#x30E9;&#x30F3;&#x30DC;&#x30EF;&#x30FC;&#x30BA;</trd>,
    <trd>&#x30AD;&#x30A4;&#x30C1;&#x30B4;</trd>
  </trdgrp>
  <trd lng="nl">framboos</trd>
  <trd lng="no">bringeb&aelig;r</trd>
  <trd lng="pl">malina</trd>
  <trd lng="pt">framboesa</trd>
  <trd lng="ru">&c_m;&c_a;&c_l;&c_i;&c_n;&c_a; <klr>(&c_ja;&c_g;&c_o;&c_d;&c_a;)</klr></trd>
  <trd lng="sk">malina <klr>(plod)</klr></trd>
  <trd lng="uk">&#x43C;&#x430;&#x43B;&#x438;&#x43D;&#x430; (&#x44F;&#x433;&#x43E;&#x434;&#x430;)</trd>
  <trdgrp lng="zh">
    <trd>&#x8986;&#x76C6;&#x5B50; <pr>f&#xF9;p&#xE9;nz&#x1D0;</pr></trd>,
    <trd>&#x6811;&#x8393; <pr>sh&#xF9;m&#xE9;i</pr></trd>,
    <trd>&#x6728;&#x8393; <pr>m&#xF9;m&#xE9;i</pr></trd>,
    <trd>&#x9911;&#x9911;&#x982D; <pr>b&#x14D;bot&#xF3;u</pr></trd>
  </trdgrp>
</drv>

<drv mrk="framb.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="framb.0ujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:botanikaj_specioj">Specio</ref> de <ref tip="super" cel="rubus.0o.genro">rubuso</ref>
      kun ru&gcirc;aj plurfruktoj (<trd lng="la">Rubus idaeus</trd>):
      <ekz>
        inter verdaj <tld/>ujoj staris alta svelta fra&ubreve;lino
        <fnt>
        <aut>Ivan Turgenev, trad. I. Novikova k.a.</aut>,
        <vrk><url ref="&LOdE;Rn-turg1.htm">La unua
        amo</url></vrk>,
        <lok>Jekaterinburg: Sezonoj, 1993</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/framb.jpg">
    floroj
  </bld>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_l;&c_ib;&c_n;&c_a; <klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>,
    <trd>&c_m;&c_a;&c_l;&c_ib;&c_n;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_m;&c_a;&c_l;&c_ib;&c_n;&c_a;&c_v;&c_y; &c_k;&c_u;&c_s;&c_t;</trd>
  </trdgrp>
  <trd lng="bg">&c_m;&c_a;&c_l;&c_i;&c_n;&c_a;</trd>
  <trd lng="cs">malin&iacute;k</trd>
  <trd lng="de">Himbeerstrauch</trd>
  <trd lng="en">raspberry bush</trd>
  <trd lng="fr">framboisier</trd>
  <trd lng="hu">m&aacute;lnabokor</trd>
  <trdgrp lng="ja">
    <trd>&#x30E9;&#x30BA;&#x30D9;&#x30EA;&#x30FC;</trd>,
    <trd>&#x30E8;&#x30FC;&#x30ED;&#x30C3;&#x30D1;&#x30AD;&#x30A4;&#x30C1;&#x30B4;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>framboos</trd>,
    <trd>frambozenstruik</trd>
  </trdgrp>
  <trdgrp lng="no">
    <trd>bringeb&aelig;r</trd>,
    <trd>bringeb&aelig;rbusk</trd>
  </trdgrp>
  <trd lng="pl">malina</trd>
  <trd lng="pt">framboeseira</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_l;&c_i;&c_n;&c_a; <klr>(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>,
    <trd>&c_m;&c_a;&c_l;&c_i;&c_n;&c_o;&c_v;&c_y;&c_j; &c_k;&c_u;&c_s;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">ostru&#x17E;ina malinov&#xE1;</trd>
  <trdgrp lng="cs">
    <trd>malin&#xED;k obecn&#xFD;</trd>,
    <trd>ostru&#x17E;in&#xED;k malinov&#xFD;</trd>
  </trdgrp>
  <trd lng="uk">&#x43C;&#x430;&#x43B;&#x438;&#x43D;&#x430; (&#x43A;&#x443;&#x449;)</trd>
  <trd lng="zh">&#x8986;&#x76C6;&#x5B57; <pr>f&#xF9;p&#xE9;nz&#xEC;</pr></trd>
</drv>
</art>
