<art mrk="$Id: geod.xml,v 1.20 2025/12/31 22:34:07 revo Exp $">
<kap>
  <rad>geod</rad>/o
<fnt><bib>PVEF</bib></fnt></kap>
<drv mrk="geod.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Minerala, kavoforma maso, sfereca, interne kovrita
      de <ref tip="prt" cel="krista.0o.MIN">kristaloj</ref>:
      <ekz>
        agato estas amorfa, tavole kolorigita formo de kalcedono, demeti&gcirc;inta en
        <tld/>ojn, la&ubreve; paralelaj tavoloj, prezentante variajn, ordinare diafanajn
        kolora&jcirc;ojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=53869">Agato</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gang.0o">gango</ref>.
    </refgrp>
  <trd lng="en">geode</trd>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_sh;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trd lng="ca">geoda</trd>
  <trdgrp lng="cs">
    <trd>druza</trd>,
    <trd>geoda</trd>,
    <trd>orlovec</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Geode</trd>,
     <trd>Druse</trd></trdgrp>
  <trd lng="fr">g&eacute;ode</trd>
  <trdgrp lng="hu">
    <trd>geoda</trd>,
    <trd>druza</trd>
  </trdgrp>
  <trd lng="ja">&#x6676;&#x6D1E;&#x77F3;  <pr>&#x3057;&#x3087;&#x3046;&#x3069;&#x3046;&#x305B;&#x304D;</pr></trd>
  <trd lng="nl">geode</trd>
  <trd lng="pl">geoda</trd>
  <trd lng="pt">geode</trd>
  <trd lng="ru">&c_zh;&c_je;&c_o;&c_d;&c_a;</trd>
  <trd lng="sk">geoda</trd>
  <trdgrp lng="zh">
    <trd>&#x5F02;&#x8D28;&#x6676;&#x7C07; <pr>y&igrave;zh&igrave;j&imacron;ngc&ugrave;</pr></trd>,
    <trd>&#x6676;&#x6D1E; <pr>j&imacron;ngd&ograve;ng</pr></trd>
  </trdgrp>
</drv>
</art>
