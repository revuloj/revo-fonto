<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: geod.xml,v 1.13 2018/12/20 16:10:16 revo Exp $">
<kap>
  <rad>geod</rad>/o
</kap>
<drv mrk="geod.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Minerala, kavoforma maso, sfereca, interne kovrita
      de <ref tip="prt" cel="krista.0o.MIN">kristaloj</ref>:
      <ekz>
        agato estas amorfa, tavole kolorigita formo de kalcedono, demeti&gcirc;inta en
        <tld/>ojn, la&ubreve; paralelaj tavoloj, prezentante variajn, ordinare diafanajn
        kolora&jcirc;ojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=53869"
              >Agato</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gang.0o">gango</ref>.
    </refgrp>
  </snc>
  <trd lng="ca">geoda</trd>
  <trdgrp lng="cs">
    <trd>druza</trd>,
    <trd>geoda</trd>,
    <trd>orlovec</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Geode</trd>,
     <trd>Druse</trd></trdgrp>
  <trd lng="fr">g&eacute;ode</trd>
  <trdgrp lng="hu">
    <trd>geoda</trd>,
    <trd>druza</trd>
  </trdgrp>
  <trd lng="nl">geode</trd>
  <trd lng="pt">geode</trd>
  <trd lng="ru">&c_zh;&c_je;&c_o;&c_d;&c_a;</trd>
  <trd lng="sk">geoda</trd>
</drv>
</art>
