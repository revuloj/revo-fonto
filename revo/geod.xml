<art mrk="$Id: geod.xml,v 1.17 2024/05/08 07:25:04 revo Exp $">
<kap>
  <rad>geod</rad>/o
</kap>
<drv mrk="geod.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Minerala, kavoforma maso, sfereca, interne kovrita
      de <ref tip="prt" cel="krista.0o.MIN">kristaloj</ref>:
      <ekz>
        agato estas amorfa, tavole kolorigita formo de kalcedono, demeti&gcirc;inta en
        <tld/>ojn, la&ubreve; paralelaj tavoloj, prezentante variajn, ordinare diafanajn
        kolora&jcirc;ojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=53869">Agato</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gang.0o">gango</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_sh;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trd lng="ca">geoda</trd>
  <trdgrp lng="cs">
    <trd>druza</trd>,
    <trd>geoda</trd>,
    <trd>orlovec</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Geode</trd>,
     <trd>Druse</trd></trdgrp>
  <trd lng="fr">g&eacute;ode</trd>
  <trdgrp lng="hu">
    <trd>geoda</trd>,
    <trd>druza</trd>
  </trdgrp>
  <trd lng="ja">&#26230;&#x6D1E;&#x77F3;  <pr>&#12375;&#12423;&#12358;&#12393;&#12358;&#x305B;&#x304D;</pr></trd>
  <trd lng="nl">geode</trd>
  <trd lng="pt">geode</trd>
  <trd lng="ru">&c_zh;&c_je;&c_o;&c_d;&c_a;</trd>
  <trd lng="sk">geoda</trd>
  <trdgrp lng="zh">
    <trd>&#x5F02;&#x8D28;&#26230;&#x7C07; <pr>y&#xEC;zh&#xEC;j&#x12B;ngc&#xF9;</pr></trd>,
    <trd>&#26230;&#x6D1E; <pr>j&#x12B;ngd&#xF2;ng</pr></trd>
  </trdgrp>
</drv>
</art>
