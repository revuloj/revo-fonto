<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: regol.xml,v 1.16 2023/09/03 07:53:10 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>regol</rad>/o
</kap>
<drv mrk="regol.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio <ref tip="malprt" cel="silvie.0oj">silviedoj</ref>
      (<trd lng="la">Regulus</trd>), tre malgranda insektman&gcirc;anta 
      <ref tip="lst" lst="voko:birdoj" cel="bird.0o.ZOO">birdeto</ref>,
      ofte kun ora strio sur la kapo:
      <ekz>
        Madejra <tld/>o estas tre eta paseroforma birdo, inter 8 kaj 8,5&#x202F;cm longa
        <klr>[&#x2026;]</klr> sur la insulo Madejro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=229261">Madejra regolo</url></lok>
        </fnt>.
      </ekz>
     </dif>
    <bld lok="&WCU;/thumb/2/2c/Regulus_satrapa_28258.JPG/180px-Regulus_satrapa_28258.JPG" prm="CC BY 2.5"><tld/>o (Regulus satrapa)
        <fnt>
          <aut>Walter Siegmund, propra verko</aut>
          <lok><url ref="&WCW;/File:Regulus_satrapa_28258.JPG">&WCW;/File:Regulus_satrapa_28258.JPG</url></lok>
        </fnt>
    </bld>
   </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_a;&c_l;&c_jo;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>kr&aacute;licek</trd>,
    <trd>kr&aacute;l&iacute;&ccaron;ek</trd>
  </trdgrp>
  <trd lng="de">Goldh&auml;hnchen</trd>
  <trd lng="en">goldcrest</trd>
  <trd lng="fr">roitelet</trd>
  <trd lng="hu">kir&aacute;lyka</trd>
  <trd lng="ja">&#x30AD;&#x30AF;&#x30A4;&#x30BF;&#x30C0;&#x30AD;</trd>
  <trd lng="nl">goudhaantje</trd>
  <trd lng="pl">mysikr&oacute;lik</trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_o;&c_l;&c_jo;&c_k;</trd>
  <trd lng="sk">orie&scaron;ok</trd>
  <trd lng="uk">&#x437;&#x43E;&#x43B;&#x43E;&#x442;&#x43E;&#x43C;&#x443;&#x448;&#x43A;&#x430;</trd>
</drv>
</art>
