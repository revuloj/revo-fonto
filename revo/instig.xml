<drv mrk="instig.0o">
  <kap><tld/>o</kap>
  <snc mrk="instig.0o.KOMUNE">
    <dif>
      Ago <tld/>i:
      <ekz>
        estas feli&ccirc;o <klr>[...]</klr> havi jam en la sango
        la <tld/>on progresadi en &ccirc;io brava
        <fnt><bib>Fab3</bib><lok>&Ccirc;io sur sian &gcirc;ustan
        lokon!</lok></fnt>;
      </ekz>
      <ekz>
        <ind>sub <tld/>o de</ind> la instinkto de memkonservo
        &scirc;i kuris for de la homoj
        <fnt><bib>Marta</bib></fnt>;
        <trdgrp lng="fr">
          <trd>&agrave; l'<ind>instigation</ind> de</trd>,
          <trd>sur l'<ind>instance</ind> de</trd>
        </trdgrp>
      </ekz>
      <ekz>
        Amtelaj iris viziti Zalmunan <ind>la&ubreve; <tld/>o de</ind>
        sia servistino Nupta
        <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 3</lok></fnt>.
        <trdgrp lng="fr">
          <trd>&agrave; l'<ind>instigation</ind> de</trd>,
          <trd>sur l'<ind>instance</ind> de</trd>
        </trdgrp>
      </ekz>
    </dif>
  <trdgrp lng="en">
    <trd>encouragement</trd>,
    <trd>incitement</trd>,
    <trd>motivation</trd>,
    <trd>instigation</trd>
  </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_a;&c_h;&c_v;&c_o;&c_ch;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_s;&c_t;&c_y;&c_m;&c_u;&c_l;&c_ja;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="bg">
