<drv mrk="instig.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="instig.0ilo.KOMUNE">
    <dif>
      &Ccirc;io, kio <tld/>as:
      <ekz>
        <klr>[ilia ar&gcirc;ento kaj ilia oro]</klr> estis
        <tld/>ilo por iliaj malbonagoj
        <fnt><bib>MT</bib><lok>&Jeh; 7:19</lok></fnt>;
      </ekz>
      <ekz>
        la ani&gcirc;o al SAT povos esti <tld/>ilo <klr>[lerni la
        lingvon]</klr>
        <fnt><bib>Lanti</bib>, <lok>Je la lumo de la sperto</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_r;&c_o;&c_d;&c_a;&c_k; <ind>&c_z;&c_a;&c_a;&c_h;&c_v;&c_o;&c_ch;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ja;</ind></trd>,
    <trd>&c_s;&c_t;&c_y;&c_m;&c_u;&c_l;</trd>
  </trdgrp>
  <trd lng="en">means of encouragement, instigation</trd>
  <trdgrp lng="fr">
    <trd>incitation<klr tip="ind"> (ce qui incite)</klr></trd>,
    <trd>aiguillon<klr tip="ind"> (ce qui incite)</klr></trd>,
    <trd>encouragement<klr tip="ind"> (ce qui incite)</klr></trd>,
    <trd>stimulant<klr tip="ind"> (ce qui incite)</klr></trd>.
  </trdgrp>
  <trdgrp lng="pl">
    <trd>stymulator</trd>,
    <trd>stymulant</trd>
  </trdgrp>
  <trd lng="uk">&c_z;&c_b;&c_u;&c_d;&c_l;&c_i;&c_v;&c_i;&c_j; &c_z;&c_a;&c_s;&c_ib;&c_b;</trd>
  <trd lng="zh">&#35480;&#22240; <pr>y&ograve;uy&imacron;n</pr></trd>
</drv>

<drv mrk="instig.mal0i">
  <kap>mal<tld/>i</kap>
  <snc mrk="instig.mal0i.KOMUNE">
    <dif>
      <tld lit="I"/>i ne fari, <tld/>i al la malo:
      <ekz>
        tio ne mal<tld/>is &scirc;in fari&gcirc;i amikino
        de la plej humila persono en la regno
