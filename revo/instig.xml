    <trd>&c_t;&c_o;&c_l;&c_k;&c_a;&c_t;&c_mol; <klr>(&c_n;&c_a;
    &c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
