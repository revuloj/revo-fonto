<art mrk="$Id: homer.xml,v 1.17 2023/11/15 17:08:01 revo Exp $">
<kap>
  <rad>Homer</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="homer.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Eposisto helena (probable el la 8a jarcento a. K.) konsiderata
      kiel a&ubreve;toro de <ref tip="vid" cel="iliad.0o">Iliado</ref> kaj <ref tip="vid" cel="odise.0ado.BELE">Odiseado</ref>:
      <ekz>
        <tld/>o prikantas en sia eposo la templon de Atena
        <fnt><bib>IntArkeol</bib>, <lok>p. 19a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_G;&c_a;&c_m;&c_e;&c_r;</trd>
  <trd lng="ca">Homer</trd>
  <trd lng="cs">Hom&#xE9;r</trd>
  <trd lng="de">Homer</trd>
  <trd lng="en">Homer</trd>
  <trd lng="es">Homero</trd>
  <trd lng="fr">Hom&egrave;re</trd>
  <trd lng="hu">Hom&eacute;rosz</trd>
  <trd lng="it">Omero</trd>
  <trd lng="ja">&#x30DB;&#x30E1;&#x30ED;&#x30B9;</trd>
  <trd lng="pl">Homer</trd>
  <trd lng="pt">Homero</trd>
  <trd lng="ru">&c_G;&c_o;&c_m;&c_je;&c_r;</trd>
  <trd lng="sk">Hom&#xE9;r</trd>
  <trd lng="uk">&#x413;&#x43E;&#x43C;&#x435;&#x440; (&#x434;&#x430;&#x432;&#x43D;&#x44C;&#x43E;&#x433;&#x440;&#x435;&#x446;&#x44C;&#x43A;&#x438;&#x439; &#x43F;&#x43E;&#x435;&#x442;)</trd>
  <trd lng="zh">&#x8377;&#x99AC; <pr>H&#xE9;m&#x1CE;</pr></trd>
</drv>
</art>

