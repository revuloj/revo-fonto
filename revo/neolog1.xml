<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: neolog1.xml,v 1.9 2025/04/13 20:27:47 revo Exp $">
<kap>
  <rad>neolog</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="neolog1.0o">
  <kap><tld/>o</kap>
  <snc mrk="neolog1.0o.ESP">
    <uzo tip="fak">ESP</uzo>
    <dif>
      Uzanto, subtenanto de
      <ref tip="vid" cel="neolog.0o.malnova">neologismoj<sncref/></ref>:
      <ekz>
        sufi&ccirc;as pilorii <tld/>ojn por akiri pli da vo&ccirc;oj
        <fnt>
         <aut>G. Neves</aut>,
         <vrk>La oldulo kaj l'amaro</vrk>,
         <lok><url ref="http://www.esperanto-gacond.ch/pdf/SES_informas_2006_3.pdf">
         Svisa Esperanto-Societo Informas, 2006:3</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">n&eacute;ologue</trd>
  </snc>
  <rim mrk="neolog1.0o.RIM_ETIM">
    Retroderiva&jcirc;o el <em>neologismo</em>.
  </rim>
  <trdgrp lng="be">
    <trd>&c_n;&c_e;&c_o;&c_l;&c_ja;&c_g;</trd>,
    <trd>&c_v;&c_y;&c_n;&c_a;&c_h;&c_o;&c_d;&c_n;&c_ib;&c_k; &c_n;&c_o;&c_v;&c_y;&c_h; &c_s;&c_l;&c_o;&c_v;&c_a;&c_w;</trd>
  </trdgrp>
  <trd lng="cs">neolog</trd>
  <trd lng="en">neologizer</trd>
  <trd lng="pl">neolog</trd>
  <trd lng="ro">neolog</trd>
  <trd lng="sk">neol&oacute;g</trd>
  <trd lng="uk">&c_t;&c_v;&c_o;&c_r;&c_je;&c_c;&c_mol; &c_n;&c_je;&c_o;&c_l;&c_o;&c_g;&c_ib;&c_z;&c_m;&c_ib;&c_v;</trd>
</drv>
</art>

