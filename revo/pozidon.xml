<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pozidon.xml,v 1.17 2020/09/07 20:10:34 revo Exp $">
<kap>
  <rad>Pozidon</rad>/o <fnt><bib>PIV1</bib></fnt>,
  <var><kap><rad var="sej">Posejdon</rad>/o <fnt><bib>FK</bib></fnt></kap></var>
</kap>

<drv mrk="pozidon.0o">
  <kap><tld/>o, <var><kap><tld var="sej"/>o</kap></var></kap>
  <snc mrk="pozidon.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Helena <ref tip="super" cel="di.0o.politeisma">dio</ref>
      de maroj:
      <ekz>
        kateni minacis lin &ccirc;iuj aliaj: Hero kaj <tld var="sej"/>o kaj anka&ubreve; Palas-Ateno
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        de <tld var="sej"/>o portas kvieton al la kor', ke la submarajn vojojn tapi&scirc;e kovras
        moskoj
        <fnt>
          <aut>N. Hohlov</aut>
          <vrk><url
          ref="http://literaturo.org/Harlow-Don/Esperanto/Literaturo/Poezio/hohlov/lesboso.html"
              >&Ccirc;e Lesboso</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la maro, mu&gcirc;anta <tld var="sej"/>o, elvaporis obtuzajn bulojn, malsekajn &scirc;vebojn
        <fnt>
          <aut>Baldur Ragnarsson, afi&scirc;is Nicolino Rossi</aut>
          <vrk><url
          ref="https://www.facebook.com/Gruppo-Esperantista-Napoletano-Harmonio-kaj-progreso-496320003888420/posts"
              >La amo kaj l'amebo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi trovis templon de <tld/>o, kiel indikis la surskribo, kaj 
        post nelonge anka&ubreve; multajn tombojn kaj steleojn
        <fnt>
          <aut>Lukiano, trad. G. Berveling</aut>,
          <vrk><url ref="http://www.esperanto.gr/lit/els/rakontoj.pdf">veraj rakontoj</url>. &#8208;
          2005</vrk>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; la ekveturo ili oferis solene al <tld/>o kaj al &ccirc;iuj maraj dioj
        <fnt>
          <aut>Eduard Peti&scirc;ka, trad. Dorothea kaj Hans-Georg Kaiser</aut>
          <vrk><url
          ref="https://cezartradukoj.blogspot.com/2017/05/malnovaj-grekaj-mitoj-jazono-kaj-medea.html#!"
              >Malnovaj grekaj mitoj: Jazono kaj Medea</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="neptun.0o.mita">Neptuno</ref>
  </snc>
  <trd lng="be">&c_P;&c_a;&c_s;&c_e;&c_j;&c_d;&c_o;&c_n;</trd>
  <trd lng="bg">&c_P;&c_o;&c_s;&c_je;&c_j;&c_d;&c_o;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>Poseidon</trd>,
    <trd>Poseid&oacute;n</trd>
  </trdgrp>
  <trd lng="de">Poseidon</trd>
  <trd lng="el">&Pi;&omikron;&sigma;&epsilon;&jota;&delta;&omega_acute;&ny;&alfa;&sigma_fina;</trd>
  <trd lng="en">Poseidon</trd>
  <trd lng="es">Poseid&oacute;n</trd>
  <trd lng="fr">Pos&eacute;idon</trd>
  <trd lng="nl">Poseidon</trd>
  <trd lng="pl">Posejdon</trd>
  <trdgrp lng="pt">
    <trd>Poseidon</trd>,
    <trd>Pos&iacute;don</trd>
  </trdgrp>
  <trd lng="ru">&c_P;&c_o;&c_s;&c_je;&c_j;&c_d;&c_o;&c_n;</trd>
  <trd lng="sk">Poseid&oacute;n</trd>
</drv>
</art>
