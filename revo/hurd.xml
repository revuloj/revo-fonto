<art mrk="$Id$">
<kap>
  <rad>hurd</rad>/o
</kap>
<drv mrk="hurd.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      A&jcirc;ura plata plekta&jcirc;o el salikaj bran&ccirc;oj a&ubreve;
      metalfadenoj por sekigi froma&gcirc;ojn, fruktojn
