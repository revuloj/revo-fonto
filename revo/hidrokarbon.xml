<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: hidrokarbon.xml,v 1.3 2022/04/30 08:42:41 afido Exp $">
    <kap>
      <rad>hidrokarbon</rad>/o
    </kap>

<drv mrk="hidrokarbon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc mrk="hidrokarbon.0o.KEM">
    <dif>
      <ref tip="super" cel="substa.0o.FIZ">Substanco</ref> konsistanta nur el 
      <ref tip="prt" cel="hidrog.0o">hidrogeno</ref> kaj <ref tip="prt" cel="karbon.0o">karbono</ref>, 
      el kiuj multajn oni trovas en <ref tip="malprt" cel="naft.0o">nafto</ref>
      kaj <ref tip="malprt" cel="gas.ter0o">tergaso</ref>:
      <ekz>
        landoj kies grava rolo dependas nur de la produktado de <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Lenio Marobin</aut>
          <vrk><url ref="&Monato;/2013/011023.php">Frakasado igos Usonon energio-sendependa</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        a&ubreve;tonoma submara veturilo, kies elektronikaj nazoj kaj oreloj sonorigos alarmon,
        kiam estas rimarkitaj elver&scirc;oj abruptaj de <tld/>oj en la maron
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2012/010460.php">Subakva spiono</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="hidrok.0o">hidrokarbido</ref>
    <trd lng="be">&c_v;&c_u;&c_g;&c_l;&c_je;&c_v;&c_a;&c_d;&c_a;&c_r;&c_o;&c_d;</trd>
    <trd lng="ca">hidrocarbur</trd>
    <trd lng="de">Kohlenwasserstoff</trd>
    <trd lng="el">&ypsilon;&delta;&rho;&alfa;&ny;&theta;&rho;&alfa;&kappa;&jota;&kappa;&omikron_ton; &epsilon;&xi;&ypsilon_ton;</trd>
    <trd lng="en">hydrocarbon</trd>
    <trd lng="es">hidrocarburo</trd>
    <trd lng="fa">&a_h;&f_y;&a_d;&a_r;&a_w;&f_k;&a_r;&a_b;&a_n;</trd>
    <trd lng="fr">hydrocarbure</trd>
    <trd lng="hu">sz&eacute;nhidrog&eacute;n</trd>
    <trd lng="it">idrocarburo</trd>
    <trd lng="nl">koolwaterstofverbinding</trd>
    <trd lng="pl">w&eogonek;glowod&oacute;r</trd>
    <trd lng="pt">hidrocarboneto</trd>
    <trd lng="ru">&c_u;&c_g;&c_l;&c_je;&c_v;&c_o;&c_d;&c_o;&c_r;&c_o;&c_d;</trd>  </snc>
  <trd lng="uk">&#x432;&#x443;&#x433;&#x43B;&#x435;&#x432;&#x43E;&#x434;&#x435;&#x43D;&#x44C;</trd>
</drv>

  </art>

