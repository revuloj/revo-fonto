<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kripto.xml,v 1.16 2023/12/03 09:59:01 revo Exp $">
<kap>
  <rad>kriptogam</rad>/o
</kap>
<drv mrk="kripto.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="dif" cel="spor.0oplanto">Sporoplanto</ref>:
      <ekz>
        diplomlaboro estis pri <tld/>oj de Malsupra A&ubreve;strio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=278250">Friedrich Welwitsch</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la fungoj kaj mukofungoj estas aldonitaj al la <tld/>oj, kvankam &ccirc;i-kaze ne
        temas pri plantoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=20958">Kriptogamoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="fanero.0o">fanerogamo</ref>
  </snc>
  <adm>
    Necesas singularigi iujn tradukojn...
  </adm>
  <trd lng="be">&c_b;&c_ja;&c_s;&c_k;&c_v;&c_je;&c_t;&c_k;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_y;)</klr></trd>
  <trdgrp lng="de">
     <trd>Kryptogame</trd>,
     <trd>Sporenpflanze</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cryptogam</trd>,
    <trd>spore plant</trd>, <trd>lower plant</trd>
  </trdgrp>
  <trd lng="fr">cryptogame</trd>
  <trdgrp lng="hu">
    <trd>sp&oacute;r&aacute;sok</trd>,
    <trd>kriptog&aacute;mok</trd>,
    <trd>rejtett ivar&uacute;ak</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>sporenplanten</trd>,
    <trd>cryptogamen</trd>
  </trdgrp>
  <trd lng="pl">skrytop&lstroke;ciowe<klr> ro&sacute;liny</klr></trd>
  <trd lng="pt">cript&oacute;gamas</trd>
  <trd lng="ru">
  &c_t;&c_a;&c_j;&c_n;&c_o;&c_b;&c_r;&c_a;&c_ch;&c_n;&c_y;&c_je;</trd>
  <trdgrp lng="zh">
    <trd>&#x9690;&#x82B1;&#x690D;&#x7269; <pr>y&#x1D0;nhu&#x101;zh&#xED;w&#xF9;</pr></trd>,
    <trd>&#x96B1;&#x82B1;&#x690D;&#x7269; <pr>y&#x1D0;nhu&#x101;zh&#xED;w&#xF9;</pr></trd>
  </trdgrp>
</drv>
</art>
