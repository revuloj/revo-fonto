<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>megalit</rad>/o
</kap>
<drv mrk="megali.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">ARKE</uzo>
    <snc>
      <dif>
        Ŝtonega monumento el prahistoriaj tempoj:
        <ekz>
          la <tld/>oj, vicoj de starantaj ŝtonoj, estis starigitaj
ĉirkaŭ la
          jaro 2000 antaŭ Kristo<fnt><aut>Josette Ducloyer</aut>:
          <vrk>Bretonio</vrk>, <bib>Monato</bib><lok>jaro 2000a, numero 2a,
          p. 18a</lok></fnt>.
        </ekz>
      </dif>
      <refgrp tip="sub">
        <ref cel="menhir.0o">menhiro</ref>,
        <ref cel="dolmen.0o">dolmeno</ref>.
      </refgrp>
    </snc>

    <trd lng="be">мэгаліт</trd>
    <trd lng="br">meurvaen</trd>
    <trd lng="de">Megalith</trd>
    <trd lng="fr">mégalithe</trd>
    <trd lng="hu">megalit</trd>
    <trd lng="nl">megaliet</trd>
    <trd lng="pt">megálito</trd>
    <trd lng="ru">мегалит</trd>
</drv>
</art>
