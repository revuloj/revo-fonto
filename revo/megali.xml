<art mrk="$Id: megali.xml,v 1.25 2023/11/21 21:23:27 revo Exp $">
<kap>
  <rad>megalit</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="megali.0o">
  <kap><tld/>o</kap>
  <snc mrk="megali.0o.ARKE">
    <uzo tip="fak">ARKE</uzo>
    <dif>
      &Scirc;tonega <ref tip="super" cel="monume.0o.ARKI">monumento</ref>,
      kiajn oni konstruis ekde neolitiko (&ccirc;. 5000 a.K. &gcirc;is 800 a.K.):
      <ekz>
        la <tld/>oj, vicoj de starantaj &scirc;tonoj, estis starigitaj
        &ccirc;irka&ubreve; la jaro 2000 anta&ubreve; Kristo
        <fnt><aut>J. Ducloyer</aut>
          <vrk>Bretonio</vrk>
          <bib>Monato</bib>
          <lok>2000:2, p. 18a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="menhir.0o">menhiro</ref>,
      <ref cel="dolmen.0o">dolmeno</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_g;&c_a;&c_l;&c_ib;&c_t;</trd>
  <trd lng="bg">&c_m;&c_je;&c_g;&c_a;&c_l;&c_i;&c_t;</trd>
  <trd lng="br">meurvaen</trd>
  <trd lng="ca">meg&agrave;lit</trd>
  <trd lng="cs">megalit</trd>
  <trd lng="da">megalit</trd>
  <trd lng="de">Megalith</trd>
  <trd lng="en">megalith</trd>
  <trd lng="fr">m&eacute;galithe</trd>
  <trd lng="hu">megalit</trd>
  <trd lng="ja">&#24040;&#30707;  <pr>&#12365;&#12423;&#12379;&#12365;</pr></trd>
  <trd lng="nl">megaliet</trd>
  <trd lng="pl">megalit</trd>
  <trd lng="pt">meg&aacute;lito</trd>
  <trd lng="ru">&c_m;&c_je;&c_g;&c_a;&c_l;&c_i;&c_t;</trd>
  <trd lng="sk">megalit</trd>
  <trd lng="uk">&c_m;&c_je;&c_g;&c_a;&c_l;&c_ib;&c_t;</trd>
</drv>
</art>
