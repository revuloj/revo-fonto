<vortaro>
<art mrk="$Id$">
<kap>
     <rad>megalit</rad>/o
</kap>
<drv mrk="megali.0o">
     <kap><tld/>o</kap>
     <uzo tip="fak">ARKE</uzo>
     <snc>
       <dif>
         &Scirc;tonega monumento el prahistoriaj tempoj:
       <ekz>
         la <tld/>oj, vicoj de starantaj &scirc;tonoj, estis starigitaj
         &ccirc;irka&ubreve; la
         jaro 2000 anta&ubreve; Kristo<fnt><aut>Josette Ducloyer</aut>:
         <vrk>Bretonio</vrk>, <bib>Monato</bib><lok>jaro 2000a, numero
         2a,
         p. 18a</lok></fnt>.
         </ekz>
       </dif>
       <refgrp tip="sub">
         <ref cel="menhir.0o">menhiro</ref>,
         <ref cel="dolmen.0o">dolmeno</ref>.
       </refgrp>
     </snc>
   <trd lng="be">&c_m;&c_e;&c_g;&c_a;&c_l;&c_ib;&c_t;</trd>
   <trd lng="br">meurvaen</trd>
   <trd lng="de">Megalith</trd>
   <trd lng="fr">m&eacute;galithe</trd>
   <trd lng="hu">megalit</trd>
   <trd lng="nl">megaliet</trd>
   <trd lng="pl">megalit</trd>
   <trd lng="pt">meg&aacute;lito</trd>
   <trd lng="ru">&c_m;&c_je;&c_g;&c_a;&c_l;&c_i;&c_t;</trd>
</drv>
</art>
