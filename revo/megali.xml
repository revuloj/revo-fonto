<drv mrk="megali.0o">
  <kap><tld/>o</kap>
  <snc mrk="megali.0o.ARKE">
    <uzo tip="fak">ARKE</uzo>
    <dif>
      &Scirc;tonega monumento el prahistoriaj tempoj:
      <ekz>
        la <tld/>oj, vicoj de starantaj &scirc;tonoj, estis starigitaj
        &ccirc;irka&ubreve; la jaro 2000 anta&ubreve; Kristo
        <fnt><aut>J. Ducloyer</aut>
          <vrk>Bretonio</vrk>
          <bib>Monato</bib>
          <lok>2000:2, p. 18a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="menhir.0o">menhiro</ref>,
      <ref cel="dolmen.0o">dolmeno</ref>.
    </refgrp>
