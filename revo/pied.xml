<drv mrk="pied.0e_de">
   <kap><tld/>e de</kap>
   <snc mrk="pied.0e_de.iu">
     <dif>
       &Ccirc;e la <tld/>oj de iu.
     </dif>
     <trd lng="ca">als <ind>peus</ind> de</trd>
     <trd lng="de">zu <ind>F&uuml;&szlig;en</ind> <klr tip="ind">(von)</klr></trd>
     <trd lng="es">a los <ind>pies</ind> de</trd>
     <trd lng="fr">aux <ind>pieds</ind> de</trd>
     <trd lng="id">di <ind>kaki</ind></trd>
     <trd lng="nl">aan de <ind>voeten</ind></trd>
     <trd lng="pl">u st&oacute;p</trd>
     <trd lng="ro">la picioare</trd>
     <trd lng="ru">&c_v; <ind>&c_n;&c_o;&c_g;&c_a;&c_h;</ind> &c_u;<klr>
        (&c_k;&c_o;&c_g;&c_o;-&c_l;.)</klr></trd>
     <trd lng="sv">vid <ind>f&ouml;tterna</ind></trd>
   </snc>
   <snc mrk="pied.0e_de.FIG">
     <uzo tip="stl">FIG</uzo>
     <dif>
       Malsupre de io:
      <ekz>
        &jcirc;uro elparolita <tld/>e de l&#8217; altaro
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        tenante ankora&ubreve; en sia krispi&gcirc;anta mano la leteron kolektitan <tld/>e de l&#8217;
        apogse&gcirc;o
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;evalo <klr>[&#8230;]</klr> sin frapegis kontra&ubreve; arbon, <tld/>e de kiu &gcirc;i falegis
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        li fine haltis por iom spiri <tld/>e de la monto
        <fnt>
          <bib>ChB</bib>
        </fnt>;
      </ekz>
      <ekz>
        ducento da apogantoj kunvenis <tld/>e de la <nom>Atomiumo</nom>
        <fnt>
          <aut>pp</aut>
          <vrk><url ref="&Monato;/2005/008593.php">Blua Atomiumo pro forrabita
          &jcirc;urnalistino</url></vrk>
          <bib>Monato</bib>
          <lok>2005/04, p. 17</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la monon]</klr> mi petas spezi al la konto <tld/>e de mia anoncilo
        <fnt>
          <aut>Pejno Simono</aut>
          <vrk><url ref="&Monato;/2001/pdf/monato200198.pdf#page=25">La profeta
          kurso</url></vrk>
           <bib>Monato</bib>
          <lok>2001/08, p. 25</lok>
        </fnt>.
      </ekz>
     </dif>
     <trd lng="br">ouzh <ind>troad</ind></trd>
     <trd lng="ca">al <ind>peu</ind> de</trd>
     <trd lng="de">am <ind>Fu&szlig;</ind> <klr tip="ind">(von)</klr></trd>
     <trd lng="es">al <ind>pie</ind> de</trd>
     <trd lng="fr">au <ind>pied</ind> de</trd>
     <trd lng="id"><ind>di</ind> bawah</trd>
     <trd lng="nl">aan de <ind>voet</ind></trd>
     <trd lng="pl">u do&lstroke;u</trd>
     <trd lng="ro">&icirc;n partea de jos</trd>
     <trd lng="ru">&c_u;  <ind>&c_p;&c_o;&c_d;&c_n;&c_o;&c_zh;&c_i;&c_ja;</ind></trd>
   </snc>
   <trdgrp lng="en">
     <trd>at the foot of</trd>,
     <trd>below</trd>
   </trdgrp>
   <trd lng="it">ai <ind>piedi</ind> di</trd>
  <trd lng="zh">&#x9E93; <pr>l&#xF9;</pr></trd>
</drv>
