  <trd lng="et">suu</trd>
  <trd lng="eu">oinez</trd>
  <trd lng="fr">pied</trd>
  <trd lng="fy">foet</trd>
  <trd lng="gl">p&#xE9;</trd>
  <trd lng="gu">&#xAAA;&#xA97;</trd>
  <trd lng="ha">kafa</trd>
