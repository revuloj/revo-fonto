  <trd lng="hu">l&aacute;b</trd>
  <trd lng="hy">&#x578;&#x57F;&#x584;&#x578;&#x57E;</trd>
  <trd lng="is">f&#xF3;tur</trd>
  <trd lng="it">piede</trd>
  <trd lng="ja">&#x8DB3;</trd>
  <trd lng="jw">mlaku</trd>
  <trd lng="ka">&#x10E4;&#x10D4;&#x10EE;&#x10D8;&#x10D7;</trd>
  <trd lng="km">&#x1787;&#x17BE;&#x1784;</trd>
