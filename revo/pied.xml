  <trdgrp lng="pl">
    <trd>depta&cacute;</trd>,
    <trd>tupa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>c&abreve;lca</trd>,
    <trd>pa&#537;i de step</trd>
  </trdgrp>
</drv>

