<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>pied</rad>/o
</kap>
<drv mrk="pied.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Artikigita parto &ccirc;e ekstremo de la homa a&ubreve; besta kruro,
      servanta por stari kaj mar&scirc;i:
      <ekz>
        mi sidas sur se&gcirc;o k tenas
        la <tld/>ojn sur benketo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &scirc;i &jcirc;etis
        sin al la <tld/>oj de sia edzo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>bati, <tld/>frapi, <tld/>premi
        (<ref tip="sin" cel="tret.0i">treti</ref>);
      </ekz>
      <ekz>
        <tld/>a <klr>(kie ku&scirc;as la
        <tld/>oj)</klr> parto de lito<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kurba<tld/>a ulo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="krur.0o">kruro</ref>
    <ref tip="vid" cel="tut.0a">tuta</ref>
    <trd lng="tr">ayak</trd>
  </snc>
  <snc num="2">
    <uzo tip="klr">(f)</uzo>
    <dif>
      Parto sur kiu meblo staras:
      <ekz>
        tablo kun kvar <tld/>oj.
      </ekz>
    </dif>
    <trd lng="tr">ayak</trd>
  </snc>
  <snc num="3">
    <uzo tip="klr">(f)</uzo>
    <dif>
      Malsupra parto de io:
      <ekz>
        <tld/>o de roko, de pa&gcirc;o;
      </ekz>
      <ekz>
        <tld/>noto;
      </ekz>
      <ekz>
        anta&ubreve; &ccirc;io ni devas penadi, ke nia afero
        stari&gcirc;u sur fortaj <tld/>oj <klr>(bazo)</klr> en la
        mondo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="tr">ayak</trd>
  </snc>
  <snc num="4">
    <uzo tip="klr">(f)</uzo>
    <dif>
      Versero 
    </dif>
    <ref tip="sin" cel="membr.0o">membro 3</ref>
  </snc>
  <trd lng="cs">noha <klr tip="ind">(od kotn&iacute;ku dol&ugrave;)</klr></trd>
  <trd lng="de">Fu&szlig;</trd>
  <trd lng="en">foot</trd>
</drv>
<drv mrk="pied.0e">
  <kap><tld/>e</kap>
  <snc num="1">
    <dif>
      Per la <tld/>oj:
      <ekz>
        iri <tld/>e;
      </ekz>
      <ekz>
        malfeli&ccirc;o venas rajde, foriras <tld/>e<fnt>Z</fnt>.
      </ekz> 
    </dif>
    <trd lng="de">zu <ind>Fu&szlig;</ind></trd>
  </snc>
  <snc num="2">
    <dif>
      &Ccirc;e la <tld/>oj de iu. 
    </dif>
    <trd lng="de">
      zu <ind>F&uuml;&szlig;en</ind> <klr tip="ind">(von)</klr>
    </trd>
  </snc>
  <snc num="3">
    <uzo tip="klr">(f)</uzo>
    <dif>
      Malsupre de io:
      <ekz>
        <tld/>e de la pa&gcirc;o trovi&gcirc;as noto.
      </ekz>
    </dif>
    <trd lng="de">am <ind>Fu&szlig;</ind> <klr tip="ind">(von)</klr></trd>
  </snc>
</drv>
<drv mrk="pied.0ingo">
  <kap><tld/>ingo</kap>
  <snc>
    <dif>
      Speco de ringo; kiu pendas de la selo k en kiun oni metas
      ekstrema&jcirc;ojn de la <tld/>oj por sur&ccirc;evali&gcirc;i k rajdi. 
    </dif>
  </snc>
  <trd lng="de">Steigb&uuml;gel</trd>
</drv>
<drv mrk="pied.tri0o">
  <kap>tri<tld/>o</kap>
  <snc>
    <dif>
      Tablo a&ubreve; se&gcirc;o kun tri <tld/>oj.  
    </dif>
  </snc>
  <trd lng="de">Dreibein</trd>
</drv>
<drv mrk="pied.kapo0uloj">
  <kap>kapo<tld/>uloj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <ref tip="dif" cel="cefalo.0oj">cefalopodoj</ref>
  </snc>
</drv>
</art>
