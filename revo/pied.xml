<drv mrk="pied.0a">
  <kap>
    <tld/>a
  </kap>
  <snc mrk="pied.0a.rilata">
    <dif>
      Rilata al <tld/>oj:
      <ekz>
        tri<tld/>aj kaj unu<tld/>aj tabloj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        dik<tld/>a tablo
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>;
      </ekz>
      <ekz>
         <tld/>a <klr>(kie ku&scirc;as la
         <tld/>oj)</klr> parto de la lito
         <fnt><bib>Fab4</bib><lok>Filino de la mar&ccirc;a re&gcirc;o</lok></fnt>;
      </ekz>
      <ekz>
        miaj tagoj estis pli rapid<tld/>aj ol kuristo
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 9:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>rapid<tld/>a</ind> kiel gazelo
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 2:18</lok>
        </fnt>;
        <trd lng="de">schnellf&uuml;&szlig;ig</trd>
      </ekz>
      <ekz>
         <ind>lama<tld/>a</ind> artisto
         <fnt>
           <bib>FK</bib>
         </fnt>.
         <trd lng="de">fu&szlig;lahm</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>foot</trd>,
    <trd>paw</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8DB3;&#x306E;  <pr>&#12354;&#12375;&#x306E;</pr></trd>,
    <trd>&#x4E0B;&#x90E8;&#x306E;  <pr>&#x304B;&#12406;&#x306E;</pr></trd>
  </trdgrp>
