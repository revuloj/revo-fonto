     <trd lng="tr">ayak</trd>
  </snc>
  <snc mrk="pied.0o.FIG">
     <uzo tip="stl">FIG</uzo>
     <dif>
       Malsupra parto de io, bazo, <ref tip="sin" cel="fundam.0o">fundamento</ref>:
      <ekz>
        <klr>[tion]</klr> vi ekscios nur en la temploj, &ccirc;e la <tld/>o de la altaroj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la <tld/>oj de la monumentoj <klr>[&#8230;]</klr> pa&scirc;is gardistoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la <tld/>o de l' monto
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
          <lok>Tro Homa Sorto de iu Ne&gcirc;homo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la <tld/>oj de Everesto
        <fnt>
        <aut>E. Georgiev</aut>,
        <vrk><url ref="&Monato;/2000/007116.html">Sir Edmund Hillary: Vivo
        dedi&ccirc;e al
        la &scirc;erpoj</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/10, p. 18-19</lok>
        </fnt>;
      </ekz>
      <ekz>
        li reatingis la <tld/>on de la arbokoloso
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Pramaljuna Morlino</lok>
        </fnt>;
      </ekz>
      <ekz>
        estas mistajpo <klr>[&#8230;]</klr> <tld/>e de la lasta pa&gcirc;o
        <fnt>
          <aut>Pejno Simono</aut>
          <vrk><url ref="&Monato;/2001/pdf/monato200198.pdf#page=25">La profeta
          kurso</url></vrk>
           <bib>Monato</bib>
          <lok>2001/08, p. 25</lok>
        </fnt>;
      </ekz>
      <ekz>
         <ref tip="vid" cel="not.pied0o">piednoto</ref>.
       </ekz>
     </dif>
