<drv mrk="band.0acxo">
  <kap><tld/>a&ccirc;o</kap>
  <snc mrk="band.0acxo.bando">
    <dif>
      <ref tip="dif" cel="band.0o.bandacxo"><tld lit="B"/>o
      <sncref/></ref>:
      <ekz>
        defendi sian organizon kontra&ubreve; fa&scirc;ista <tld/>a&ccirc;o
        <fnt><bib>Lanti</bib>, <lok>&ccirc;u unue SAT-anoj?</lok></fnt>;
      </ekz>
      <ekz>
        la plej terura brulego de tiu urbo, komencita kaj da&ubreve;rigita de
        la perfido kaj malico de papa <tld/>a&ccirc;o 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=10618">Granda
          incendio de Londono</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>trepa</trd>,
      <trd>c&agrave;fila</trd>,
      <trd>horda</trd>
    </trdgrp>
    <trd lng="de">
      Bande
    </trd>
    <trdgrp lng="en">
      <trd>gang</trd>,
      <trd>band</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>banda</trd>,
      <trd>szajka</trd>,
      <trd>klika</trd>,
      <trd>gang</trd>,
      <trd>ha&lstroke;astra</trd>,
      <trd>zgraja</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>banda</trd>, 
      <trd>cricca</trd>, 
      <trd>gang</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="band.0ano">
  <kap><tld/>ano</kap>
  <snc mrk="band.0ano.KOMUNE">
    <dif>
      Unuopa ulo el <ref tip="malprt" cel="band.0o.KOMUNE"><tld/>o</ref>:
    </dif>
    <subsnc mrk="band.0ano.MUZ">
      <ekz>
        &leftquot;Povus esti simple&rightquot; estas la unua vera
        rokmuzika disko
        <klr>...</klr> verkita kaj aran&gcirc;ita de la <tld/>anoj mem
        <fnt>
        <aut>Bertilo Wennergren</aut>,
        <vrk>Nova albumo de Persone</vrk>,
        <lok>soc.culture.esperanto, 1996-08-12</lok>
        </fnt>.
      </ekz>
      <trd lng="en">band member</trd>
      <trd lng="pl">cz&lstroke;onek <ind>kapeli</ind><klr>
      (pot.)</klr></trd>
    </subsnc>
    <subsnc mrk="band.0ano.kunularo">
      <ekz>
        ribelemaj <tld/>anoj <klr>...</klr> asertis, ke nenia estro
        havas la rajton tiel senigi la kunularon je tiel bela
        kapta&jcirc;o
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 9a,
        &leftquot;aventura voja&gcirc;o&rightquot;</lok></fnt>.
      </ekz>
      <refgrp tip="vid">
        <ref cel="bandit.0o">bandito</ref>
      </refgrp>
      <trd lng="en">gang member</trd>
      <trdgrp lng="pl">
        <trd>cz&lstroke;onek <ind>bandy</ind></trd>,
        <trd>cz&lstroke;onek <ind>paczki</ind></trd>
      </trdgrp>
    </subsnc>
  </snc>
  <trd lng="ca">membre</trd>
  <trdgrp lng="cs">
    <trd>bandita</trd>,
    <trd>&ccaron;len bandy</trd>,
    <trd>&ccaron;len tlupy</trd>
  </trdgrp>
  <trd lng="fr">membre d'une bande</trd>
  <trd lng="it">membro d'una banda</trd>
  <trd lng="sk">bandita</trd>
  <trdgrp lng="uk">
    <trd>&#x431;&#x430;&#x43D;&#x434;&#x438;&#x442;&#xA0;(&#x447;&#x43B;&#x435;&#x43D; &#x437;&#x433;&#x440;&#x430;&#x457;</trd>,
    <trd>&#x431;&#x430;&#x43D;&#x434;&#x438;)</trd>
  </trdgrp>
</drv>

<drv mrk="band.jxaz0o">
  <kap>&jcirc;az<tld/>o,
    <var><kap>&gcirc;azbando <fnt><bib>PV</bib></fnt></kap></var>
  </kap>
  <snc mrk="band.jxaz0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="grup.muzik0o.MUZ">Muzikgrupo</ref> ludanta
      &jcirc;azmuzikon: eta <tld/>o el unu blovisto, unu drumisto, unu
      kontrabasisto kaj &gcirc;is 5 homoj, a&ubreve; granda <tld/>o 
      &gcirc;is 20 a&ubreve; 25-homa:
      <ekz>
        aperis &jcirc;az<tld/>o kaj salonorkestroj, kiuj muzikis fokstroton
        [...] kaj sambojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=22999">Sambo
          (muziko)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la pianon oni &scirc;losis, la &jcirc;az<tld/>o disiris
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 5a, p. 52a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_zh;&c_a;&c_z;&c_a;&c_v;&c_y;
    &c_g;&c_u;&c_r;&c_t;</trd>,
    <trd>&c_d;&c_zh;&c_a;&c_z;-&c_b;&c_e;&c_n;&c_d;</trd>
  </trdgrp>
  <trd lng="br">strollad jazz</trd>
  <trd lng="ca">orquestra de jazz</trd>
  <trdgrp lng="de">
    <trd>Jazzband</trd>,
    <trd>Jazzkapelle</trd>
  </trdgrp>
  <trd lng="en">jazz band</trd>
  <trd lng="es">grupo de <ind>jazz</ind></trd>
  <trdgrp lng="fr">
    <trd>jazz-band</trd>,
    <trd>orchestre de jazz</trd>
  </trdgrp>
  <trd lng="hu">dzsessz-zenekar</trd>
  <trdgrp lng="pl">
    <trd>jazz-band</trd>,
    <trd>big-band</trd>,
    <trd>zesp&oacute;&lstroke; jazzowy</trd>,
    <trd>kapela jazzowa <klr> (pot.)</klr></trd>
  </trdgrp>
  <trd lng="pt">banda de jazz</trd>
  <trd lng="ru">
&c_d;&c_zh;&c_a;&c_z;-&c_o;&c_r;&c_k;&c_je;&c_s;&c_t;&c_r;
  </trd>
  <trd lng="it">gruppo <ind>jazz</ind></trd>
  <trdgrp lng="uk">
    <trd>&#x434;&#x436;&#x430;&#x437;-&#x431;&#x430;&#x43D;&#x434;</trd>,
    <trd>&#x434;&#x436;&#x430;&#x437;-&#x431;&#x435;&#x43D;&#x434;</trd>
  </trdgrp>
</drv>
