<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>band</rad>/o
</kap>
<drv mrk="band.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="band.0o.KOMUNE">
    <dif>
      Eta <ref tip="super" cel="grup.0o.personoj">grupo</ref> facile
      movi&gcirc;anta:
    </dif>
    <subsnc mrk="band.0o.irantaro">
      <dif>
        Irantaro:
        <ekz>
          <tld/>o da junuloj;
        </ekz>
        <ekz>
          <tld/>o da migrantaj birdoj<fnt>B</fnt>.
        </ekz>
      </dif>
      <trdgrp lng="be">
        <trd>&c_g;&c_r;&c_u;&c_p;&c_a;</trd>,
        <trd>&c_g;&c_u;&c_r;&c_t;</trd>
      </trdgrp>
      <trdgrp lng="br">
        <trd>bagad</trd>,
        <trd>bandenn</trd>
      </trdgrp>
      <trd lng="ca">colla</trd>
      <trd lng="de">Gruppe</trd>
      <trdgrp lng="en">
        <trd>band</trd>,
        <trd>posse</trd>
      </trdgrp>
      <trd lng="hu">csapat</trd>
      <trd lng="nl">troep</trd>
      <trdgrp lng="pl">
        <trd>grupa</trd>,
        <trd>paczka<klr> (pot.)</klr></trd>,
        <trd>dru&zdot;yna<klr> (harcerzy starszych)</klr></trd>
      </trdgrp>
      <trd lng="ru">&c_g;&c_r;&c_u;&c_p;&c_p;&c_a;</trd>
      <trdgrp lng="it">
        <trd>gruppo</trd>, 
        <trd>banda</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="band.0o.MUZ">
      <uzo tip="fak">MUZ</uzo>
      <dif>
        Malgranda <ref tip="super" cel="grup.muzik0o.MUZ">muzikgrupo
        </ref>
        kuniranta, kunlunda:
        <ekz>
          nova esperanta rok<tld/>o
          <fnt>
          <aut>Manuel Pancorbo Castro</aut>,
          <vrk>Al la barikadoj! (JoMo kaj Liberecanoj)</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 1999a, numero 7a, p. 24a</lok>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="trup.0o.TEA">trupo <sncref/></ref>
      </refgrp>
      <trdgrp lng="be">
        <trd>&c_a;&c_r;&c_k;&c_je;&c_s;&c_t;&c_a;&c_r;</trd>,
        <trd>&c_a;&c_n;&c_s;&c_a;&c_m;&c_b;&c_l;&c_mol;</trd>,
        <trd>&c_g;&c_r;&c_u;&c_p;&c_a;</trd>,
        <trd>&c_g;&c_u;&c_r;&c_t;</trd>
      </trdgrp>
      <trdgrp lng="br">
        <trd>laz<klr tip="ind"> (-kana&ntilde;, -seni&ntilde;)</klr>
        </trd>,
        <trd>strollad<klr tip="ind"> (sonerezh)</klr></trd>,
        <trd>bagad<klr tip="ind"> (sonerien)</klr></trd>
      </trdgrp>
      <trd lng="ca">grup</trd>
      <trdgrp lng="de">
        <trd>Band</trd>,
        <trd>Kapelle</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>band</trd>
      </trdgrp>
      <trd lng="hu">zenei <ind>egy&uuml;ttes</ind></trd>
      <trdgrp lng="nl">
        <trd>band<klr tip="ind"> (muziekgroep)</klr></trd>,
        <trd>muziekgroep</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>kapela<klr> (pot.)</klr></trd>
      </trdgrp>
     <trdgrp lng="it">
        <trd>gruppo</trd>, 
        <trd>banda</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="band.0o.bandacxo">
      <dif>
        Pluraj uloj ordinare obeantaj unu &ccirc;efulon, minacaj,
        misfaraj, 
        <ref tip="super" cel="grup.0acxo.KOMUNE">grupa&ccirc;o</ref>:
        <ekz>
          tutaj <tld/>oj da rabistoj
          <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 18a</lok></fnt>;
        </ekz>
        <ekz>
          sa&gcirc;aj homoj <klr>...</klr> foriru el tiu &ccirc;i krima 
          <tld/>o
          <fnt>
          <aut>G. Squarcina</aut>,
          <vrk>esperantigita Osamo</vrk>,
          <lok>soc.culture.esperanto, 2002-11-13</lok>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="sin">
        <ref cel="band.0acxo.bando"><tld/>a&ccirc;o</ref>
      </refgrp>
      <trd lng="be">&c_b;&c_a;&c_n;&c_d;&c_a;</trd>
      <trd lng="br">bandennad</trd>
      <trdgrp lng="ca">
        <trd>ramat</trd>,
        <trd>ardat</trd>
      </trdgrp>
      <trd lng="de">Bande</trd>
      <trdgrp lng="en">
        <trd>band</trd>,
        <trd>posse</trd>
      </trdgrp>
      <trd lng="hu">banda</trd>
      <trdgrp lng="pl">
        <trd>banda</trd>,
        <trd>szajka</trd>,
        <trd>klika</trd>,
        <trd>gang</trd>,
        <trd>ha&lstroke;astra</trd>,
        <trd>zgraja</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_b;&c_a;&c_n;&c_d;&c_a;</trd>,
        <trd>&c_sh;&c_a;&c_j;&c_k;&c_a;</trd>,
        <trd>&c_k;&c_l;&c_i;&c_k;&c_a;</trd>,
        <trd>&c_s;&c_b;&c_o;&c_r;&c_i;&c_shch;&c_je;</trd>
      </trdgrp>
      <trdgrp lng="it">
        <trd>banda</trd>, 
        <trd>cricca</trd>, 
        <trd>gang</trd>,
        <trd>squadra d'azione<klr> (se fascista)</klr></trd>
      </trdgrp>
    </subsnc>
  </snc>
  <trd lng="ca">banda</trd>
  <trd lng="de">Gruppe</trd>
  <trd lng="es">banda</trd>
  <trd lng="fr">bande <klr tip="amb"> (groupe de personnes,
  d'animaux)</klr></trd>
  <trd lng="nl">bende</trd>
  <trd lng="sk">tlupa</trd>
</drv>

<drv mrk="band.0acxo">
  <kap><tld/>a&ccirc;o</kap>
  <snc mrk="band.0acxo.bando">
    <ref tip="dif" cel="band.0o.bandacxo"><tld/>o <sncref/></ref>
    <ekz>
      defendi sian organizon kontra&ubreve; fa&scirc;ista
      <tld/>a&ccirc;o
      <fnt><bib>Lanti</bib>, <lok>&ccirc;u unue SAT-anoj?</lok></fnt>.
    </ekz>
      <trdgrp lng="ca">
        <trd>trepa</trd>,
        <trd>c&agrave;fila</trd>,
        <trd>horda</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>banda</trd>,
        <trd>szajka</trd>,
        <trd>klika</trd>,
        <trd>gang</trd>,
        <trd>ha&lstroke;astra</trd>,
        <trd>zgraja</trd>
      </trdgrp>
     <trdgrp lng="it">
        <trd>banda</trd>, 
        <trd>cricca</trd>, 
        <trd>gang</trd>
      </trdgrp>
  </snc>
</drv>

<drv mrk="band.0ano">
  <kap><tld/>ano</kap>
  <snc mrk="band.0ano.KOMUNE">
    <dif>
      Unuopa ulo el <ref tip="malprt"
      cel="band.0o.KOMUNE"><tld/>o</ref>:
    </dif>
    <subsnc mrk="band.0ano.MUZ">
      <ekz>
        &leftquot;Povus esti simple&rightquot; estas la unua vera
        rokmuzika disko
        <klr>...</klr> verkita kaj aran&gcirc;ita de la <tld/>anoj mem
        <fnt>
        <aut>Bertilo Wennergren</aut>,
        <vrk>Nova albumo de Persone</vrk>,
        <lok>soc.culture.esperanto, 1996-08-12</lok>
        </fnt>.
      </ekz>
      <trd lng="pl">cz&lstroke;onek <ind>kapeli</ind><klr>
      (pot.)</klr></trd>
    </subsnc>
    <subsnc mrk="band.0ano.kunularo">
      <ekz>
        ribelemaj <tld/>anoj <klr>...</klr> asertis, ke nenia estro
        havas la rajton tiel senigi la kunularon je tiel bela
        kapta&jcirc;o
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 9a,
        &leftquot;aventura voja&gcirc;o&rightquot;</lok></fnt>.
      </ekz>
      <refgrp tip="vid">
        <ref cel="bandit.0o">bandito</ref>
      </refgrp>
      <trdgrp lng="pl">
        <trd>cz&lstroke;onek <ind>bandy</ind></trd>,
        <trd>cz&lstroke;onek <ind>paczki</ind></trd>
      </trdgrp>
    </subsnc>
  </snc>
  <trd lng="ca">membre</trd>
  <trd lng="fr">membre d'une bande</trd>
  <trd lng="it">membro d'una banda</trd>
  <trd lng="sk">bandita</trd>
</drv>

<drv mrk="band.jxaz0o">
  <kap>&jcirc;az<tld/>o</kap>
  <snc mrk="band.jxaz0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="grup.muzik0o.MUZ">Muzikgrupo</ref> ludanta
      &jcirc;azmuzikon: eta <tld/>o el unu blovisto, unu drumisto, unu
      kontrabasisto kaj &gcirc;is 5 homoj, a&ubreve; granda <tld/>o 
      &gcirc;is 20 a&ubreve; 25-homa:
      <ekz>
        la pianon oni &scirc;losis, la &jcirc;az<tld/>o disiris
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 5a, p. 52a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="gxazba.0o">&gcirc;azbando</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_zh;&c_a;&c_z;&c_a;&c_v;&c_y;
    &c_g;&c_u;&c_r;&c_t;</trd>,
    <trd>&c_d;&c_zh;&c_a;&c_z;-&c_b;&c_e;&c_n;&c_d;</trd>
  </trdgrp>
  <trd lng="br">strollad jazz</trd>
  <trd lng="ca">orquestra de jazz</trd>
  <trdgrp lng="de">
    <trd>Jazzband</trd>,
    <trd>Jazzkapelle</trd>
  </trdgrp>
  <trd lng="es">grupo de <ind>jazz</ind></trd>
  <trdgrp lng="fr">
    <trd>jazz-band</trd>,
    <trd>orchestre de jazz</trd>
  </trdgrp>
  <trd lng="hu">dzsessz-zenekar</trd>
  <trdgrp lng="pl">
    <trd>jazz-band</trd>,
    <trd>big-band</trd>,
    <trd>zesp&oacute;&lstroke; jazzowy</trd>,
    <trd>kapela jazzowa <klr> (pot.)</klr></trd>
  </trdgrp>
  <trd lng="pt">banda de jazz</trd>
  <trd lng="ru">
&c_d;&c_zh;&c_a;&c_z;-&c_o;&c_r;&c_k;&c_je;&c_s;&c_t;&c_r;
  </trd>
  <trd lng="it">gruppo <ind>jazz</ind></trd>
</drv>
</art>
