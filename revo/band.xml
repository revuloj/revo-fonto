<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>band</rad>/o
</kap>
<drv mrk="band.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="band.0o.irantaro">
    <dif>
      Irantaro:
      <ekz>
        <tld/>o da junuloj;
      </ekz>
      <ekz>
        <tld/>o da migrantaj birdoj<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_g;&c_r;&c_u;&c_p;&c_a;</trd>,
      <trd>&c_g;&c_u;&c_r;&c_t;</trd>
    </trdgrp>
    <trdgrp lng="br">
      <trd>bagad</trd>,
      <trd>bandenn</trd>
    </trdgrp>
    <trd lng="de">Gruppe</trd>
    <trdgrp lng="en">
      <trd>band</trd>,
      <trd>posse</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>bande<klr tip="ind"> (troupe)</klr></trd>,
      <trd>troupe</trd>
    </trdgrp>
    <trd lng="hu">csapat</trd>
    <trd lng="nl">troep</trd>
    <trd lng="ru">&c_g;&c_r;&c_u;&c_p;&c_p;&c_a;</trd>
  </snc>
  <snc mrk="band.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Grupo da kunlundantaj muzikistoj.
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_r;&c_k;&c_je;&c_s;&c_t;&c_a;&c_r;</trd>,
      <trd>&c_a;&c_n;&c_s;&c_a;&c_m;&c_b;&c_l;&c_mol;</trd>,
      <trd>&c_g;&c_r;&c_u;&c_p;&c_a;</trd>,
      <trd>&c_g;&c_u;&c_r;&c_t;</trd>
    </trdgrp>
    <trdgrp lng="br">
      <trd>laz<klr tip="ind"> (-kana&ntilde;, -seni&ntilde;)</klr></trd>,
      <trd>strollad<klr tip="ind"> (sonerezh)</klr></trd>,
      <trd>bagad<klr tip="ind"> (sonerien)</klr></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Band</trd>,
      <trd>Kapelle</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>band</trd>
    </trdgrp>
    <trd lng="fr">groupe<klr tip="ind"> (musical)</klr></trd>
    <trd lng="hu">zenei <ind>egy&uuml;ttes</ind></trd>
    <trdgrp lng="nl">
      <trd>band<klr tip="ind"> (muziekgroep)</klr></trd>,
      <trd>muziekgroep</trd>
    </trdgrp>
  </snc>
  <snc mrk="band.0o.kunularo">
    <dif>
      Aro da individuoj obeantaj unu &ccirc;efon, ordinare kun mal&scirc;ata
      nuanco:
      <ekz>
        tutaj <tld/>oj da rabistoj
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 18</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="grup.0o">grupo</ref>,
      <ref cel="hord.0o">hordo</ref>,
      <ref cel="trup.0o">trupo</ref>.
    </refgrp>
    <trd lng="be">&c_b;&c_a;&c_n;&c_d;&c_a;</trd>
    <trd lng="br">bandennad</trd>
    <trd lng="de">Bande</trd>
    <trdgrp lng="en">
      <trd>band</trd>,
      <trd>posse</trd>
    </trdgrp>
    <trd lng="fr">bande<klr tip="ind"> (de voyous)</klr></trd>
    <trd lng="hu">banda</trd>
    <trdgrp lng="ru">
      <trd>&c_b;&c_a;&c_n;&c_d;&c_a;</trd>,
      <trd>&c_sh;&c_a;&c_j;&c_k;&c_a;</trd>,
      <trd>&c_k;&c_l;&c_i;&c_k;&c_a;</trd>,
      <trd>&c_s;&c_b;&c_o;&c_r;&c_i;&c_shch;&c_je;</trd>
    </trdgrp>
  </snc>
  <trd lng="es">banda</trd>
  <trd lng="nl">bende</trd>
</drv>

<drv mrk="band.jxaz0o">
  <kap>&jcirc;az<tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Muzikgrupo ludanta &jcirc;azan muzikon, kiel eta <tld/>o el minimume unu
      blovinstrumentisto, unu drumisto, unu kontrabasisto, kaj &gcirc;is 5 
homoj,
      a&ubreve; kiel granda <tld/>o nombranta &gcirc;is 20 a&ubreve; 25 homoj.
    </dif>
    <refgrp tip="sin">
      <ref cel="gxazba.0o">&gcirc;azbando</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_zh;&c_a;&c_z;&c_a;&c_v;&c_y; &c_g;&c_u;&c_r;&c_t;</trd>,
    <trd>&c_d;&c_zh;&c_a;&c_z;-&c_b;&c_e;&c_n;&c_d;</trd>
  </trdgrp>
  <trd lng="br">strollad jazz</trd>
  <trdgrp lng="de">
    <trd>Jazzband</trd>,
    <trd>Jazzkapelle</trd>
  </trdgrp>
  <trd lng="es">grupo de <ind>jazz</ind></trd>
  <trdgrp lng="fr">
    <trd>jazz-band</trd>,
    <trd>orchestre de jazz</trd>
  </trdgrp>
  <trd lng="hu">dzsessz-zenekar</trd>
  <trd lng="pt">banda de jazz</trd>
  <trd
  lng="ru">&c_d;&c_zh;&c_a;&c_z;-&c_o;&c_r;&c_k;&c_je;&c_s;&c_t;&c_r;</trd>
</drv>
</art>
