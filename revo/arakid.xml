<vortaro>
<art mrk="$Id$">
<kap><rad>arakid</rad>/o</kap>
<drv mrk="arakid.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Arachis</trd>), kies oleri&ccirc;aj fruktoj
      maturi&gcirc;as en la tero.
    </dif>
  </snc>
  <trd lng="de">Erdnu&szlig;</trd>
  <trd lng="es">cacahuate</trd>
  <trdgrp lng="fr">
    <trd>arachide</trd>,
    <trd>cacahu&egrave;te</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>amerikaimogyor&oacute;</trd>,
    <trd>b&uacute;rmogyor&oacute;</trd>,
    <trd>f&ouml;ldimogyor&oacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_a;&c_h;&c_i;&c_s;</trd>,
    <trd>&c_z;&c_je;&c_m;&c_l;&c_ja;&c_n;&c_o;&c_j;
&c_o;&c_r;&c_je;&c_h;</trd>
  </trdgrp>
  <trd lng="tr">yer f&inodot;st&inodot;&gbreve;&inodot;</trd>
</drv>
</art>
