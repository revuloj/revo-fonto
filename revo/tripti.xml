<art mrk="$Id: tripti.xml,v 1.25 2025/04/08 14:12:32 revo Exp $">
<kap>
  <rad>triptik</rad>/o
<fnt><bib>PVEF</bib></fnt></kap>
<drv mrk="tripti.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc mrk="tripti.0o.BELA">
    <dif>
      <ref tip="super" cel="pentr.0ajxo">Pentra&jcirc;o</ref>
      sur tri tabuloj, tia ke la du flankaj tabuloj povas
      kunfermi&gcirc;i super la centra tabulo.
      <ekz>
        en la pre&gcirc;ejo estas belegaj pentra&jcirc;oj de <nom>David Tenier</nom>,
        resta&jcirc;oj de <tld/>o, kies &ccirc;efa centra parto perdi&gcirc;is,
        kiuj prezentas la patronon kaj patroninon de la urbo,
        Sankta Hilduardo kaj Sankta Kristiana
        <fnt>
          <bib>Monato</bib>
          <aut>Petro De Smet'</aut>
          <vrk><url ref="&Monato;/2008/009516.php">&Ccirc;u &scirc;tormo en tetaso a&ubreve; en ambono?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="diptik.0o.BELA">diptiko</ref>,
      <ref cel="altar.0o">altaro</ref>
    </refgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>tripanela arta&#309;o</ke>.
  </rim>

  <trd lng="be">&c_t;&c_r;&c_y;&c_p;&c_t;&c_y;&c_h;</trd>
  <trd lng="ca">tr&iacute;ptic</trd>
  <trd lng="cs">triptych</trd>
  <trdgrp lng="de">
    <trd>Triptychon</trd>,
    <trd>dreiteiliges <ind>Altarbild</ind></trd>
  </trdgrp>
  <trd lng="en">triptych</trd>
  <trd lng="fr">triptyque</trd>
  <trd lng="hu">triptichon</trd>
  <trd lng="it">trittico</trd>
  <trdgrp lng="ja">
    <trd>&#19977;&#36899;&#31085;&#22727;&#30011;  <pr>&#12415;&#12387;&#12428;&#12435;&#12373;&#12356;&#12384;&#12435;&#12364;</pr></trd>,
    <trd>&#12488;&#12522;&#12503;&#12486;&#12451;&#12463;</trd>
  </trdgrp>
  <trd lng="nl">triptiek</trd>
  <trd lng="pl">tryptyk</trd>
  <trd lng="pt">tr&iacute;ptico</trd>
  <trd lng="ru">&c_t;&c_r;&c_i;&c_p;&c_t;&c_i;&c_h;</trd>
  <trdgrp lng="sk">
    <trd>triptych</trd>,
    <trd>trojdielny obraz</trd>
  </trdgrp>
  <trd lng="uk">&#1090;&#1088;&#1080;&#1087;&#1090;&#1080;&#1093;</trd>
</drv>
</art>
