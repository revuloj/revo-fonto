<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: detekt.xml,v 1.25 2023/11/04 13:16:27 revo Exp $">
<kap>
<rad>detekt</rad>/i
</kap>
<drv mrk="detekt.0i">
  <kap><tld/>i</kap>
  <snc mrk="detekt.0i.trovi">
    <gra><vspec>tr</vspec></gra>
    <dif>
      Pruvi la &ccirc;eeston de io fizika, pli malpli ka&scirc;ita, <ref tip="super" cel="trov.0i.elsercxi">trovi</ref>
      per sensiloj k.a. te&hcirc;nikaj rimedoj:
      <ekz>
        <tld/>i elektromagnetajn ondojn per <tld/>ilo
        <klr>(vd <refgrp tip="vid">
          <ref cel="modul1.0i.TEL">malmodulacii</ref>,
          <ref cel="rektif.0i.ELE">rektifi</ref></refgrp>)</klr>;
      </ekz>
      <ekz>
        ne <tld/>i&gcirc;is radioj
        <fnt>
          <bib>Monato</bib>
          <aut>Yazaki Yoko</aut>
          <vrk><url ref="&Monato;/2015/011442.php">Fuku&scirc;ima kvar
           jarojn post la granda tertremo</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        spiriton neniu vidas, neniu aparato <tld/>as
        <fnt>
          <aut>Michel Marko</aut>
            <vrk><url ref="&Monato;/2000/007051.php">La stranga grupo</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 29</lok>
          </fnt>;
      </ekz>
      <ekz>
        li <tld/>as nenion &dash; e&ccirc; ne estas certe, &ccirc;u estas io <tld/>inda
        <fnt>
          <aut>Donald Broadribb</aut>
            <vrk><url ref="&Monato;/2000/007079.php"/>Inter la dek plej bonaj romanoj</vrk>
            <bib>Monato</bib>
            <lok>2000/11, p. 26</lok>
          </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="lokali.0i">lokalizi</ref>,
       <ref cel="sens.0i">sensi</ref>,
       <ref cel="sond.0i.TEK">sondi<sncref/></ref>,
       <ref cel="ekzame.0i.esplori">ekzameni<sncref/></ref>,
       <ref cel="esplor.0i">esplori</ref>,
       <ref cel="indik.0ajxo">indika&jcirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_t;&c_e;&c_k;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_d;&c_je;&c_t;&c_je;&c_k;&c_t;&c_i;&c_r;&c_a;&c_m;</trd>
  <trd lng="de">orten</trd>
  <trd lng="en">detect</trd>
  <trd lng="es">detectar</trd>
  <trd lng="fr">d&eacute;tecter<klr tip="ind"> (ondes)</klr></trd>
  <trd lng="he">&lamed;&zayin;&he;&vav;&tav;</trd>
  <trdgrp lng="hu">
    <trd>detekt&aacute;l<klr tip="ind"> (kimutat)</klr></trd>,
    <trd>kimutat<klr tip="ind"> (detekt&aacute;l)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x691C;&#x6CE2;&#x3059;&#x308B; <pr>&#x3051;&#x3093;&#x3071;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x691C;&#x51FA;&#x3059;&#x308B;  <pr>&#x3051;&#x3093;&#x3057;&#x3085;&#x3064;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">detecteren</trd>
  <trd lng="pl">wykrywa&cacute;</trd>
  <trd lng="pt">detectar</trd>
  <trd lng="ru">&c_d;&c_je;&c_t;&c_je;&c_k;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">dok&aacute;za&tcaron; pr&iacute;tomnos&tcaron; el.v&lacute;n</trd>
  <trdgrp lng="cs">
    <trd>detegovat</trd>,
    <trd>detekovat</trd>,
    <trd>detektovat</trd>,
    <trd>zji&scaron;&tcaron;ovat el. vlny</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_je;&c_t;&c_je;&#x43A;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&#x43F;&c_r;&#x44F;&#x43C;&#x43B;&#x44F;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_ib;&c_j;&c_s;&#x44C;&#x43A;</trd>
  </trdgrp>
  <trd lng="zh">&#x5B9A;&#x4F4D; <pr>d&#xEC;ngw&#xE8;i</pr></trd>
</drv>
<drv mrk="detekt.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">ELE</uzo>
  <snc mrk="detekt.0ilo.aparato">
    <dif>
      <ref tip="super" cel="aparat.0o.TEK">Aparato</ref> por pruvi la &ccirc;eeston
      de io fizika, ekz-e elektromagnetaj ondoj,
      radioaktiveco ks:
      <ekz>
        metal<tld/>ilo
        <fnt>
          <bib>Monato</bib>
          <aut>Rob Ebenau</aut>
          <vrk><url ref="&Monato;/2005/008528.php">Kiel protekti kontra&ubreve; perdo kaj
          &scirc;telo?</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ie en la &scirc;ipo trovi&gcirc;as kameraoj, &scirc;prucigiloj, fum- kaj fajro-<tld/>iloj
        <fnt>
          <bib>Monato</bib>
          <aut>Christian Bertin</aut>
          <vrk><url ref="&Monato;/2004/008255.html">Re&gcirc;ino de la maroj</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>iloj malkovras fotonojn, retroradiatajn de la korpo
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2013/011032.php">Skanado &gcirc;ena kaj
          kancerogena</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        por mezuri radiadan dozon &ccirc;irka&ubreve; si oni povas anka&ubreve; uzi porteblan <tld/>ilon
        <fnt>
          <bib>Monato</bib>
          <aut>Yazaki Yoko</aut>
          <vrk><url ref="&Monato;/2015/011442.php">Fuku&scirc;ima kvar jarojn post la granda
          tertremo</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="indukt.il0o">induktilo</ref>,
      <ref cel="mezur.0ilo">mezurilo</ref>,
      <ref cel="sond.0ilo">sondilo</ref>
    </refgrp>
    <ref tip="prt" cel="sens.0ilo.TEK">sensilo<sncref/></ref>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_t;&c_e;&c_k;&c_t;&c_a;&c_r;</trd>
  <trd lng="bg">&c_d;&c_je;&c_t;&c_je;&c_k;&c_t;&c_o;&c_r;</trd>
  <trd lng="cs">&ccaron;idlo</trd>
  <trd lng="de">Detektor</trd>
  <trd lng="en">detector</trd>
  <trd lng="es">detector</trd>
  <trd lng="fr">d&eacute;tecteur<klr tip="ind"> (d'ondes)</klr></trd>
  <trd lng="he">&gimel;&#x5B7;&lamed;&#x5B7;&alef;&#x5B4;&yod;</trd>
  <trd lng="hu">detektor</trd>
  <trdgrp lng="ja">
    <trd>&#x691C;&#x6CE2;&#x5668;  <pr>&#x3051;&#x3093;&#x3071;&#x304D;</pr></trd>,
    <trd>&#x691C;&#x51FA;&#x5668;  <pr>&#x3051;&#x3093;&#x3057;&#x3085;&#x3064;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="nl">detector</trd>
  <trdgrp lng="pl">
    <trd>detektor</trd>,
    <trd>wykrywacz</trd>
  </trdgrp>
  <trd lng="pt">detector</trd>
  <trd lng="ru">&c_d;&c_je;&c_t;&c_je;&c_k;&c_t;&c_o;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>detektor</trd>,
    <trd>&ccaron;idlo</trd>
  </trdgrp>
  <trd lng="uk">&c_d;&c_je;&c_t;&c_je;&#x43A;&c_t;&#x43E;&c_r;</trd>
  <trd lng="zh">&#x68C0;&#x6D4B;&#x5668; <pr>ji&#x1CE;nc&#xE8;q&#xEC;</pr></trd>
</drv>
