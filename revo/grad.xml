<drv mrk="grad.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="grad.0aro.KOMUNE">
    <dif>
      Sistemo de mezuraj <tld/>oj;
      <ref tip="vid" cel="skal.0o.absoluta">skalo<sncref/></ref>:
      <ekz>
        kiam mi vizitis la lernejon, ni ricevis atestojn
        la&ubreve; kvingrada <tld/>aro
        <fnt>
          <aut>L. Ganman</aut>,
          <vrk><url ref="http://linusganman.se/y3p.htm">
          Kvadrata kufa skribo</url></vrk>,
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">d&ecaron;len&yacute; kruh</trd>
  <trdgrp lng="es">
    <trd>escala<klr> (de medida)</klr></trd>,
    <trd>graduaci&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;chelle<klr> (de mesure)</klr></trd>,
    <trd>graduation</trd>
  </trdgrp>
  <trd lng="id">skala</trd>
  <trd lng="sk">delen&yacute; kruh</trd>
  <trd lng="uk">&c_p;&c_o;&c_d;&c_ib;&c_l;&c_k;&c_i;</trd>
</drv>

<drv mrk="grad.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      <ref tip="super" cel="distri.0i">Distribui</ref> la&ubreve; la <tld/>oj de io:
      <ekz>
        <tld/>igi la ekzercojn en lernolibro;
      </ekz>
      <ekz>
        la ju&gcirc;isto <tld/>igis siajn kulpigrimedojn, li konservis por la fino sian plej
        bonan argumenton
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        li pli &scirc;atis <tld/>igi la dozojn kaj nur veki sian sciemon, ne intervenante tro
        peze
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekkvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        provu anstata&ubreve; &leftquot;fama&rightquot; la malpli absolutan &leftquot;konata&rightquot;, <klr>[&#8230;]</klr> tiel eblas
        <tld/>igi la famon
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="https://www.monato.be/2013/010996.php"
              >Fama? Ne, dankon.</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="doz.0i">dozi</ref>,
      <ref cel="nuanc.0i">nuanci</ref>,
      <ref cel="klas.0igi">klasigi</ref>,
      <ref cel="vic.0igi">vicigi</ref>
    </refgrp>
    <trdgrp lng="hu">
      <trd>fokoz</trd>,
      <trd>fokozatokra oszt</trd>
    </trdgrp>
    <trd lng="nl">graderen</trd>
    <trd lng="pl">stopniowa&cacute;</trd>
    <trd lng="ru">
    &c_r;&c_a;&c_s;&c_p;&c_o;&c_l;&c_a;&c_g;&c_a;&c_t;&c_mol;
    &c_v; &c_p;&c_o;&c_r;&c_ja;&c_d;&c_k;&c_je; 
    &c_u;&c_v;&c_je;&c_l;&c_i;&c_ch;&c_je;&c_n;&c_i;&c_ja;<klr>
   (&c_n;&c_a;&c_p;&c_r;. &c_t;&c_r;&c_u;&c_d;&c_n;&c_o;&c_s;&c_t;&c_i;)
    </klr></trd>
  </snc>
  <snc>
    <dif>
      <ref tip="super" cel="divid.0i.partigi">Dividi</ref> per <tld/>oj, marki <tld/>ojn sur io:
      <ekz>
        <tld/>igi termometron.
      </ekz>
    </dif>
    <trd lng="hu">fokokra oszt</trd>
    <trd lng="nl">gradueren<klr tip="ind"> (schaalverdeling)</klr></trd>
    <trdgrp lng="pl">
      <trd>cechowa&cacute;</trd>,
      <trd>miarowa&cacute;</trd>
    </trdgrp>
    <trd lng="ru">
    &c_g;&c_r;&c_a;&c_d;&c_u;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>gradovat</trd>,
    <trd>odstup&ncaron;ovat</trd>,
    <trd>stup&ncaron;ovat</trd>,
    <trd>zesilovat</trd>,
    <trd>zv&ecaron;t&scaron;ovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>abstufen</trd>,
    <trd>gradieren</trd>,
    <trd>einteilen</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>graduar</trd>,
