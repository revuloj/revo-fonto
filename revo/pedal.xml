<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>pedal</rad>/o
</kap>
<drv mrk="pedal.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Peco de me&hcirc;anismo, kiun oni movas per piedo:
      <ekz>
        <tld/>o de piano, orgeno, harpo, bicikleto.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_d;&c_a;&c_l;&c_mol;</trd>
  <trd lng="ca">pedal</trd>
  <trd lng="de">Pedal</trd>
  <trd lng="en">pedal</trd>
  <trd lng="es">pedal</trd>
  <trd lng="hu">ped&aacute;l</trd>  
  <trdgrp lng="nl">
    <trd>pedaal</trd>,
    <trd>trapper</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_d;&c_a;&c_l;&c_mol;</trd>
  <trd lng="sv">pedal</trd>
</drv>

<drv mrk="pedal.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Movi <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_e;&c_d;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_c;&c_ib;&c_s;&c_n;&c_u;&c_c;&c_mol; &c_n;&c_a; <ind>&c_p;&c_e;&c_d;&c_a;&c_l;&c_mol;</ind></trd>
  </trdgrp>
  <trd lng="de">in die <ind>Pedale</ind> treten</trd>
  <trd lng="en">pedal</trd>
  <trd lng="hu">ped&aacute;loz(ik)</trd>  
  <trd lng="nl">trappen</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_d;&c_a;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_v;&c_i;&c_t;&c_mol; &c_n;&c_a; &c_p;&c_je;&c_d;&c_a;&c_l;&c_mol;</trd>,
    <trd>&c_k;&c_r;&c_u;&c_t;&c_i;&c_t;&c_mol; &c_p;&c_je;&c_d;&c_a;&c_l;&c_i;</trd>
  </trdgrp>
</drv>
</art>
