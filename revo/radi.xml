  <snc mrk="radi.0o.radiuso">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="stl">ARK</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 28</lok></fnt>
    <dif>
      <ref tip="dif" cel="radius.0o.cirklo">Radiuso</ref>:
      <ekz>
        armeo estas nenie en almena&ubreve; trimejla <tld/>o
        <klr>(en cirklo kun trimejla <tld/>o)</klr>
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        la rigardo &ccirc;irka&ubreve;prenis en kelkmejla <tld/>o preska&ubreve; la tutan
        provincon
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la sfero &ccirc;iela havanta <frm><k>I</k></frm> kiel centron, <frm><k>C sek D</k></frm> kiel
        <tld/>on, ni konsideru <klr>[&#x2026;]</klr>
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_d;&c_y;&c_ju;&c_s;</trd>
    <trd lng="bg">&c_r;&c_a;&c_d;&c_i;&c_u;&c_s;</trd>
