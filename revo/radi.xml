<drv mrk="radi.0i">
  <kap><tld/>i</kap>
  <snc>
    <gra><vspec>x</vspec></gra>
    <dif>
      Eligi kaj dissendi <tld/>ojn de lumo a&ubreve; varmo:
      <ekz>
        la suno <tld/>is oblikve;
      </ekz>
      <ekz>
        en la varmaj landoj la suno <tld/>as <klr>(<tld/>e dissendas)</klr> alian
        varmegon ol &ccirc;e ni
        <fnt>
          <bib>Fab2</bib>
          <lok>La ombro</lok>
        </fnt>;
      </ekz>
      <ekz>
        milionoj da steloj eklumis, milionoj da diamantaj lampoj radiis
        <fnt>
          <bib>Fab2</bib>
          <lok>Sonorilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>anta blua stelo
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
<!-- radioaktivaj?
      <ekz>
        <ind><tld/>antaj substancoj</ind>
        <trd lng="hu">sug&aacute;rz&oacute; anyagok</trd>
      </ekz>
-->
      <ekz>
        <uzo tip="stl">FIG</uzo>
        lernejo estas fajrejo dis<tld/>anta la spiritan vivon;
      </ekz>
      <ekz>
        lumo, kiu povus pri<tld/>i la problemon
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvin pladoj preparitaj el fungoj havantaj 600 Bq da cezio-137 <tld/>as
        sur homa korpo preska&ubreve; samforte kiel unu aviadilflugo tra Atlantiko
        <fnt>
          <bib>Monato</bib>
          <aut>Last</aut>
          <vrk><url ref="https://www.monato.be/2004/008448.html">Fungoj
           litova eksportvaro</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        tiaj &ccirc;i skaniloj retro<tld/>as fotonojn
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2013/011032.php">Skanado &gcirc;ena kaj kancerogena</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_p;&c_r;&c_a;&c_m;&c_je;&c_n;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_m;&c_ja;&c_n;&c_ib;&c_c;&c_c;&c_a;</trd>
      <trd>&c_z;&c_mol;&c_z;&c_ja;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">strahlen</trd>
    <trd lng="es">irradiar</trd>
    <trd lng="fr">rayonner</trd>
    <trd lng="nl">uitstralen</trd>
    <trd lng="pl">promieniowa&cacute;</trd>
    <trdgrp lng="ru">
      <trd>&c_i;&c_z;&c_l;&c_u;&c_ch;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_i;&c_ja;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Disiri de iu punkto discelante kvaza&ubreve; <tld/>oj de rado:
      <ekz>
        pavoj kun etenditaj <tld/>antaj vostoj
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
         el tiuj &scirc;tonaj muroj dis<tld/>as tutmondaj konfliktoj
        <fnt>
          <bib>Monato</bib>
          <aut>Gilbert Ledon</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007175.html">Muroj dumiljara solidas</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        preska&ubreve; &ccirc;iuj fervojaj kaj &scirc;oseaj komunikiloj el<tld/>as el Dublino
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2012/010649.php">De kelta tigro al orienta tigro</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;io liapluma el<tld/>as simpation
        <fnt>
          <bib>Monato</bib>
          <aut>Antonio Val&eacute;n</aut>
          <vrk><url ref="&Monato;/2005/008625.php">Klero kaj klaro delektos la leganton</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ili el<tld/>as originalecon, modestan noblecon, da&ubreve;remon, estetikan kaj materian valoron, foje e&ccirc; humuron
        <fnt>
          <bib>Monato</bib>
          <aut>Rita R&ouml;ssler-Buckel</aut>
          <vrk><url ref="&Monato;/2007/009174.php">Metio kaj formo tradicio kaj progreso</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>ausstrahlen</trd>
    </trdgrp>
    <trd lng="be">&c_v;&c_y;&c_h;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol; <klr>(&c_ja;&c_k; &c_p;&c_r;&c_a;&c_m;&c_je;&c_n;&c_mol;)</klr></trd>
    <trd lng="fr">rayonner</trd>
    <trd lng="pl">rozchodzi&cacute; si&eogonek; <klr>(promieni&sacute;cie)</klr></trd>
    <trd lng="ru">&c_i;&c_s;&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      &Gcirc;oje a&ubreve; pompe brili:
      <ekz>
        &scirc;iaj okuloj <tld/>is
        <fnt>
          <bib>Fab4</bib>
          <lok>Ne&gcirc;ulo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>anta rideto
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>anta de feli&ccirc;o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia viza&gcirc;o <tld/>is pro &gcirc;ojo;
      </ekz>
      <ekz>
        el lia persono <tld/>as majesteco;
      </ekz>
      <ekz>
         kolorri&ccirc;a, <tld/>anta altaro
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        okuloj, <tld/>antaj per la lasta brilo de la vivo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la milkolora beleco de la floroj <tld/>as en la &gcirc;ardenoj;
      </ekz>
      <ekz>
        li lum<tld/>is de &gcirc;ojo
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_mol;&c_z;&c_ja;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_m;&c_ja;&c_n;&c_je;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">strahlen</trd>
    <trd lng="fr">rayonner</trd>
    <trdgrp lng="hu">
      <trd>sug&aacute;roz</trd>,
      <trd>kisug&aacute;roz</trd>
    </trdgrp>
    <trd lng="nl">stralen</trd>
    <trdgrp lng="pl">
      <trd>b&lstroke;yszcze&cacute;</trd>,
      <trd>brylowa&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_i;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_l;&c_u;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>

