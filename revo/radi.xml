  <snc mrk="radi.0o.brila">
    <uzo tip="stl">FIG</uzo>
    <dif>
      &Gcirc;ojbrila aspekto:
      <ekz>
        super la viza&gcirc;o de la patrino kuris <tld/>o da
        feli&ccirc;o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        nenia <tld/>o de amo, boneco, malsevereco
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>o da espero</ind>;
        <trd lng="hu">rem&eacute;nysug&aacute;r</trd>
      </ekz>
      <ekz>
        <ind><tld/>luma</ind> estonto.
        <trd lng="es">radiante</trd>
        <trd lng="fr">radieux</trd>
        <trd lng="hu">ragyog&oacute;</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_r;&c_a;&c_m;&c_je;&c_n;&c_mol;</trd>
    <trd lng="bg">&c_l;&c_malmol;&c_ch;</trd>
